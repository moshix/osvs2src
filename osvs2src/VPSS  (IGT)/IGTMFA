         BNE   @RF00096                                         02S0096 00259000
*                         DO;       /* BOUNDARY PROBLEM              */ 00260000
*                           APCWRCN=APCCX011;/* ERROR NUMBER FOR NOT    00261000
*                                      A WORD BOUNDARY - YARRV       */ 00262000
         MVI   APCWRCN(APCWPTR),X'0B'                           02S0098 00263000
*                           APEMMSGN=APCMX011;/* MESSAGE NUMBER FOR     00264000
*                                      NOT A WORD BOUNDARY           */ 00265000
         MVC   APEMMSGN(4,@04),@CF01358                         02S0099 00266000
*                         END;      /* END OF YARRV BOUNDARY ERROR      00267000
*                                      SEGMENT                       */ 00268000
*                       ELSE        /* YARRV ERROR WAS RANGE (YARRV     00269000
*                                      POINTER LESS THAN BSAD)       */ 00270000
*                         DO;       /* RANGE ERROR                   */ 00271000
         B     @RC00096                                         02S0101 00272000
@RF00096 DS    0H                                               02S0102 00273000
*                           APCWRCN=APCCX007;/* ERROR NUMBER FOR OUT    00274000
*                                      OF RANGE - YARRV              */ 00275000
         MVI   APCWRCN(APCWPTR),X'07'                           02S0102 00276000
*                           APEMMSGN=APCMX007;/* MESSAGE NUMBER FOR     00277000
*                                      OUT OF RANGE                  */ 00278000
         MVC   APEMMSGN+112(4,APCWPTR),@CF00628                 02S0103 00279000
*                         END;      /* END OF YARRV RANGE ERROR 02S0104 00280000
*                                      SEGMENT                       */ 00281000
*                     END;          /* END OF SEGMENT                */ 00282000
*                 END;              /* END OF SEGMENT                */ 00283000
*               ELSE                /* ICTY INVALID                  */ 00284000
*                 DO;               /* ICTY ERROR PROCESSING         */ 00285000
         B     @RC00078                                         02S0107 00286000
@RF00078 DS    0H                                               02S0108 00287000
*                   ERRFLAG=ON;     /* FLAG ERROR TO MAIN SEGMENT    */ 00288000
         LA    ERRFLAG,1                                        02S0108 00289000
*                   APCWRCN=APCCX012;/* ERROR NUMBER FOR OUT OF RANGE   00290000
*                                      - ICTY                        */ 00291000
         MVI   APCWRCN(APCWPTR),X'0C'                           02S0109 00292000
*                   APEMMSGN=APCMX012;/* MESSAGE NUMBER FOR OUT OF      00293000
*                                      RANGE                         */ 00294000
         LA    @04,112                                          02S0110 00295000
         ALR   @04,APCWPTR                                      02S0110 00296000
         MVC   APEMMSGN(4,@04),@CF00628                         02S0110 00297000
*                   APEMVALN=APCMK018;/* KEYWORD NUMBER FOR ICTY     */ 00298000
         MVC   APEMVALN(4,@04),@CF01070                         02S0111 00299000
*                 END;              /* END OF ICTY ERROR PROCESSING  */ 00300000
*             END;                  /* END OF SEGMENT                */ 00301000
*           ELSE                    /* IVAL1 INVALID                 */ 00302000
*             DO;                   /* IVAL1 ERROR PROCESSING        */ 00303000
         B     @RC00074                                         02S0114 00304000
@RF00074 DS    0H                                               02S0115 00305000
*               ERRFLAG=ON;         /* FLAG ERROR TO MAIN SEGMENT    */ 00306000
         LA    ERRFLAG,1                                        02S0115 00307000
*               APCWRCN=APCCX001;   /* ERROR NUMBER FOR OUT OF RANGE    00308000
*                                      - IVAL1                       */ 00309000
         MVI   APCWRCN(APCWPTR),X'01'                           02S0116 00310000
*               APEMMSGN=APCMX001;  /* MESSAGE NUMBER FOR OUT OF        00311000
*                                      RANGE                         */ 00312000
         LA    @04,112                                          02S0117 00313000
         ALR   @04,APCWPTR                                      02S0117 00314000
         MVC   APEMMSGN(4,@04),@CF00628                         02S0117 00315000
*               APEMVALN=APCMK038;  /* KEYWORD NUMBER - IVAL1        */ 00316000
         MVC   APEMVALN(4,@04),@CF01148                         02S0118 00317000
*             END;                  /* END OF IVAL1 ERROR PROCESSING */ 00318000
*         END;                      /* END OF SEGMENT - CIT BUILD FOR   00319000
*                                      XMVS/XMVX                     */ 00320000
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */ 00321000
         B     CE0001@                                          02S0121 00322000
*       END NDEXBULK;               /* END OF SUBCASE                */ 00323000
*NDEXNDEX:                                                      02S0123 00324000
*       DO;                         /* SUBCASE INDEX - INDEX    02S0123 00325000
*                                      OPERATION                     */ 00326000
NDEXNDEX DS    0H                                               02S0124 00327000
*/**************************************************************     */ 00328000
*/*                                                                  */ 00329000
*/* THIS CASE BUILDS A CIT ENTRY FOR INDEX - INDEX OPERATIONS        */ 00330000
*/* OF THE FOLLOWING FORM:                                           */ 00331000
*/*                                                                  */ 00332000
*/*      CALL VPSS(IUNIT,IFUNC,IVAL1,IVAL2)                          */ 00333000
*/*                                                                  */ 00334000
*/*      WHERE:                                                      */ 00335000
*/*                                                                  */ 00336000
*/*        IUNIT - UNIT REFERENCE NUMBER                             */ 00337000
*/*        IFUNC - FUNCTION MNEMONIC AS FOLLOWS-                     */ 00338000
*/*                  'XMV ' - MOVE IVAL2 TO IVAL1                    */ 00339000
*/*                  'XAD ' - ADD IVAL2 TO IVAL1                     */ 00340000
*/*                  'XSB ' - SUBTRACT IVAL2 FROM IVAL1              */ 00341000
*/*                  'XML ' - MULTIPLY IVAL2 TIMES IVAL1             */ 00342000
*/*                  'XDV ' - DIVIDE IVAL2 INTO IVAL1                */ 00343000
*/*        IVAL1 - INDEX ID                                          */ 00344000
*/*        IVAL2 - INDEX ID                                          */ 00345000
*/*    NOTE : RESULT ALWAYS PLACED IN IVAL1.                         */ 00346000
*/*                                                                  */ 00347000
*/* THE CIT FORMAT FOR INDEX - INDEX OPERATIONS IS:                  */ 00348000
*/*                                                                  */ 00349000
*/*        <-------------- 4 BYTES ----------------->                */ 00350000
*/*        ------------------------------------------                */ 00351000
*/*        |   OPERATION ID    |ENTRY LENGTH(HWORDS)|                */ 00352000
*/*        ------------------------------------------                */ 00353000
*/*        |   FIRST INDEX ID  | SECOND INDEX ID    |                */ 00354000
*/*        ------------------------------------------                */ 00355000
*/*                                                                  */ 00356000
*/**************************************************************     */ 00357000
*                                                               02S0124 00358000
*         DO;                                                   02S0124 00359000
*           IF(APINXI1>=1)&(APINXI1<=15) THEN/* TEST IF IVAL1 IS A      00360000
*                                      VALID INDEX ID                */ 00361000
         LA    @04,1                                            02S0125 00362000
         L     @15,APCWAPIN(,APCWPTR)                           02S0125 00363000
         L     @01,APINAXI1(,@15)                               02S0125 00364000
         L     @14,APINXI1(,@01)                                02S0125 00365000
         CR    @14,@04                                          02S0125 00366000
         BL    @RF00125                                         02S0125 00367000
         LA    @11,15                                           02S0125 00368000
         CR    @14,@11                                          02S0125 00369000
         BH    @RF00125                                         02S0125 00370000
*             IF(APINXI2>=1)&(APINXI2<=15) THEN/* YES - TEST IF IVAL2   00371000
*                                      IS A VALID INDEX ID           */ 00372000
         L     @01,APINAXI2(,@15)                               02S0126 00373000
         L     @15,APINXI2(,@01)                                02S0126 00374000
         CR    @15,@04                                          02S0126 00375000
         BL    @RF00126                                         02S0126 00376000
         CR    @15,@11                                          02S0126 00377000
         BH    @RF00126                                         02S0126 00378000
*               DO;                 /* YES - PUT INDEX IDS INTO THE     00379000
*                                      CIT ENTRY                     */ 00380000
*                 APCIXID1=APINXI1; /* FIRST INDEX ID IS IVAL1       */ 00381000
         LA    @04,CITENTRY                                     02S0128 00382000
         STH   @14,APCIXID1(,@04)                               02S0128 00383000
*                 APCIXID2=APINXI2; /* SECOND INDEX ID IS IVAL2      */ 00384000
         STH   @15,APCIXID2(,@04)                               02S0129 00385000
*               END;                                            02S0130 00386000
*             ELSE                  /* IVAL2 INVALID                 */ 00387000
*               DO;                 /* SET ERROR INDICATORS FOR OUT     00388000
*                                      OF RANGE - IVAL2 MSG.         */ 00389000
         B     @RC00126                                         02S0131 00390000
@RF00126 DS    0H                                               02S0132 00391000
*                 ERRFLAG=ON;       /* SET ERROR FLAG                */ 00392000
         LA    ERRFLAG,1                                        02S0132 00393000
*                 APCWRCN=APCCX002; /* SET ERROR NUMBER              */ 00394000
         MVI   APCWRCN(APCWPTR),X'02'                           02S0133 00395000
*                 APEMMSGN=APCMX002;/* SET MESSAGE NUMBER            */ 00396000
         LA    @04,112                                          02S0134 00397000
         ALR   @04,APCWPTR                                      02S0134 00398000
         MVC   APEMMSGN(4,@04),@CF00628                         02S0134 00399000
*                 APEMVALN=APCMK039;/* SET KEYWORD NUMBER FOR IVAL2  */ 00400000
         MVC   APEMVALN(4,@04),@CF01150                         02S0135 00401000
*               END;                                            02S0136 00402000
*           ELSE                    /* IVAL1 INVALID                 */ 00403000
*             DO;                   /* SET ERROR INDICATORS FOR OUT     00404000
*                                      OF RANGE - IVAL1              */ 00405000
         B     @RC00125                                         02S0137 00406000
@RF00125 DS    0H                                               02S0138 00407000
*               ERRFLAG=ON;         /* FLAG THE ERROR                */ 00408000
         LA    ERRFLAG,1                                        02S0138 00409000
*               APCWRCN=APCCX001;   /* SET ERROR NUMBER              */ 00410000
         MVI   APCWRCN(APCWPTR),X'01'                           02S0139 00411000
*               APEMMSGN=APCMX001;  /* SET MESSAGE NUMBER            */ 00412000
         LA    @04,112                                          02S0140 00413000
         ALR   @04,APCWPTR                                      02S0140 00414000
         MVC   APEMMSGN(4,@04),@CF00628                         02S0140 00415000
*               APEMVALN=APCMK038;  /* SET KEYWORD VALUE FOR IVAL1   */ 00416000
         MVC   APEMVALN(4,@04),@CF01148                         02S0141 00417000
*             END;                                              02S0142 00418000
*         END;                                                  02S0143 00419000
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */ 00420000
         B     CE0001@                                          02S0144 00421000
*       END NDEXNDEX;               /* END OF SUBCASE                */ 00422000
*NDEXIMMD:                                                      02S0146 00423000
*       DO;                         /* SUBCASE INDEX-IMMEDIATE  02S0146 00424000
*                                      OPERATION                     */ 00425000
NDEXIMMD DS    0H                                               02S0147 00426000
*/************************************************************       */ 00427000
*/*                                                                  */ 00428000
*/* THIS CASE BUILDS A CIT ENTRY FOR INDEX - IMMEDIATE               */ 00429000
*/* LOGIC OPERATIONS OF THE FOLLOWING FORM:                          */ 00430000
*/*                                                                  */ 00431000
*/*      CALL VPSS(IUNIT,IFUNC,IVAL1,IVAL2)                          */ 00432000
*/*                                                                  */ 00433000
*/*      WHERE:                                                      */ 00434000
*/*                                                                  */ 00435000
*/*        IUNIT - UNIT REFERENCE NUMBER                             */ 00436000
*/*        IFUNC - FUNCTION MNEMONIC AS FOLLOWS -                    */ 00437000
*/*                  'XMVI' - MOVE IVAL2 TO IVAL1                    */ 00438000
*/*                  'XADI' - ADD IVAL2 TO IVAL1                     */ 00439000
*/*                  'XSBI' - SUBTRACT IVAL2 FROM IVAL1              */ 00440000
*/*                  'XMLI' - MULTIPLY IVAL2 TIMES IVAL1             */ 00441000
*/*                  'XDVI' - DIVIDE IVAL2 INTO IVAL1                */ 00442000
*/*                  'XDIR' - DIVIDE IVAL1 INTO IVAL2                */ 00443000
*/*        IVAL1 - INDEX ID                                          */ 00444000
*/*        IVAL2 - IMMEDIATE DATA                                    */ 00445000
*/*     NOTE: RESULT ALWAYS PLACED IN IVAL1.                         */ 00446000
*/*                                                                  */ 00447000
*/* THE CIT FORMAT FOR INDEX - IMMEDIATE OPERATIONS IS:              */ 00448000
*/*                                                                  */ 00449000
*/*        <------------- 4 BYTES ------------------>                */ 00450000
*/*        ------------------------------------------                */ 00451000
*/*        |   OPERATION ID    |ENTRY LENGTH(HWORDS)|                */ 00452000
*/*        ------------------------------------------                */ 00453000
*/*        |   FIRST INDEX ID  |     UNUSED         |                */ 00454000
*/*        ------------------------------------------                */ 00455000
*/*        |             IMMEDIATE DATA             |                */ 00456000
*/*        ------------------------------------------                */ 00457000
*/*                                                                  */ 00458000
*/************************************************************       */ 00459000
*                                                               02S0147 00460000
*         DO;                                                   02S0147 00461000
*           IF(APINXI1>=1)&(APINXI1<=15) THEN/* TEST IF IVAL1 IS A      00462000
*                                      VALID INDEX ID                */ 00463000
         L     @04,APCWAPIN(,APCWPTR)                           02S0148 00464000
         L     @01,APINAXI1(,@04)                               02S0148 00465000
         L     @15,APINXI1(,@01)                                02S0148 00466000
         C     @15,@CF00061                                     02S0148 00467000
         BL    @RF00148                                         02S0148 00468000
         C     @15,@CF00084                                     02S0148 00469000
         BH    @RF00148                                         02S0148 00470000
*             DO;                   /* YES - PUT INDEX ID INTO THE      00471000
*                                      CIT ENTRY                     */ 00472000
*               APCIXID1=APINXI1;   /* FIRST INDEX ID IS IVAL1       */ 00473000
         LA    @14,CITENTRY                                     02S0150 00474000
         STH   @15,APCIXID1(,@14)                               02S0150 00475000
*               APCIXIMM=APINXI2;   /* IMMEDIATE DATA IS IVAL2       */ 00476000
         L     @04,APINAXI2(,@04)                               02S0151 00477000
         L     @04,APINXI2(,@04)                                02S0151 00478000
         ST    @04,APCIXIMM(,@14)                               02S0151 00479000
*             END;                                              02S0152 00480000
*           ELSE                    /* IVAL1 INVALID                 */ 00481000
*             DO;                   /* SET ERROR INDICATORS FOR OUT     00482000
*                                      OF RANGE - IVAL1              */ 00483000
         B     @RC00148                                         02S0153 00484000
@RF00148 DS    0H                                               02S0154 00485000
*               ERRFLAG=ON;         /* FLAG THE ERROR                */ 00486000
         LA    ERRFLAG,1                                        02S0154 00487000
*               APCWRCN=APCCX001;   /* SET ERROR NUMBER              */ 00488000
         MVI   APCWRCN(APCWPTR),X'01'                           02S0155 00489000
*               APEMMSGN=APCMX001;  /* SET MESSAGE NUMBER            */ 00490000
         LA    @04,112                                          02S0156 00491000
         ALR   @04,APCWPTR                                      02S0156 00492000
         MVC   APEMMSGN(4,@04),@CF00628                         02S0156 00493000
*               APEMVALN=APCMK038;  /* SET KEYWORD VALUE FOR IVAL1   */ 00494000
         MVC   APEMVALN(4,@04),@CF01148                         02S0157 00495000
*             END;                                              02S0158 00496000
*         END;                                                  02S0159 00497000
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */ 00498000
         B     CE0001@                                          02S0160 00499000
*       END NDEXIMMD;               /* END OF SUBCASE                */ 00500000
*CDBRANCH:                                                      02S0162 00501000
*       DO;                         /* SUBCASE                       */ 00502000
CDBRANCH DS    0H                                               02S0163 00503000
*/************************************************************       */ 00504000
*/*                                                                  */ 00505000
*/* THIS CASE BUILDS A CIT ENTRY FOR CONDITIONAL BRANCH              */ 00506000
*/* LOGIC OPERATIONS. THE CALL FORMAT FOR A CONDITIONAL              */ 00507000
*/* BRANCH IS:                                                       */ 00508000
*/*                                                                  */ 00509000
*/*     CALL VPSS(IUNIT,IFUNC,IVAL1,IVAL2,LABEL,COND)                */ 00510000
*/*                                                                  */ 00511000
*/*     WHERE:                                                       */ 00512000
*/*                                                                  */ 00513000
*/*       IUNIT - UNIT REFERENCE NUMBER                              */ 00514000
*/*       IFUNC - FUNCTION MNEMONIC AS FOLLOWS -                     */ 00515000
*/*                 'XC  ' - COMPARE INDEX : INDEX                   */ 00516000
*/*                 'XCI ' - COMPARE INDEX : IMMEDIATE DATA          */ 00517000
*/*                 'XDC ' - DECREMENT AND COMPARE                   */ 00518000
*/*                          INDEX : INDEX                           */ 00519000
*/*                 'XDCI' - DECREMENT AND COMPARE                   */ 00520000
*/*                          INDEX : IMMEDIATE                       */ 00521000
*/*                 'XCF ' - FLOATING-POINT COMPARE                  */ 00522000
*/*                          INDEX : INDEX                           */ 00523000
*/*                 'XCFI' - FLOATING-POINT COMPARE                  */ 00524000
*/*                          INDEX : IMMEDIATE                       */ 00525000
*/*       IVAL1 - INDEX ID                                           */ 00526000
*/*       IVAL2 - INDEX ID IF 'XC  ','XDC ' OR 'XCF '                */ 00527000
*/*               IMMEDIATE DATA IF 'XCI ','XDCI' OR 'XCFI'          */ 00528000
*/*       LABEL - BRANCH TO LOCATION - DEFINED BY 'XID '             */ 00529000
*/*       COND  - CONDITION FOR WHICH BRANCH IS TO BE TAKEN          */ 00530000
*/*               IF TRUE. COND MAY BE ONE OF THE FOLLOWING -        */ 00531000
*/*                 'EQ  ' - EQUAL                                   */ 00532000
*/*                 'GT  ' - GREATER THAN                            */ 00533000
*/*                 'GE  ' - GREATER THAN OR EQUAL                   */ 00534000
*/*                 'NE  ' - NOT EQUAL                               */ 00535000
*/*                 'LT  ' - LESS THAN                               */ 00536000
*/*                 'LE  ' - LESS THAN OR EQUAL                      */ 00537000
*/*                                                                  */ 00538000
*/* THE CIT FORMAT FOR CONDITIONAL BRANCH OPERATIONS IS:             */ 00539000
*/*                                                                  */ 00540000
*/*      <--------------- 4 BYTES ---------------->                  */ 00541000
*/*      ------------------------------------------                  */ 00542000
*/*      |   OPERATION ID    |ENTRY LENGTH(HWORDS)|                  */ 00543000
*/*      ------------------------------------------                  */ 00544000
*/*      |   FIRST INDEX ID  |2ND INDEX ID/UNUSED |                  */ 00545000
*/*      ------------------------------------------                  */ 00546000
*/*      |    IMMEDIATE DATA OR UNUSED            |                  */ 00547000
*/*      ------------------------------------------                  */ 00548000
*/*      |  CONDITION MASK   |REL CIT OFFSET-HWORD|                  */ 00549000
*/*      ------------------------------------------                  */ 00550000
*/*                                                                  */ 00551000
*/*   CONDITION MASK MAY BE:                                         */ 00552000
*/*     X'0008' - EQUAL                                              */ 00553000
*/*     X'FFF7' - NOT EQUAL                                          */ 00554000
*/*     X'0002' - (FIRST) INDEX LOW                                  */ 00555000
*/*     X'FFFD' - (FIRST) INDEX NOT LOW                              */ 00556000
*/*     X'FFF5' - (FIRST) INDEX HIGH                                 */ 00557000
*/*     X'000A' - (FIRST) INDEX NOT HIGH                             */ 00558000
*/*     X'0000' - NEVER BRANCH                                       */ 00559000
*/*     X'FFFF' - UNCONDITIONAL BRANCH                               */ 00560000
*/*                                                                  */ 00561000
*/************************************************************       */ 00562000
*                                                               02S0163 00563000
*         DO;                                                   02S0163 00564000
*           IF(APINXI1>=1)&(APINXI1<=15) THEN/* TEST IF IVAL1 IS A      00565000
*                                      VALID INDEX ID                */ 00566000
         LA    @04,1                                            02S0164 00567000
         L     @15,APCWAPIN(,APCWPTR)                           02S0164 00568000
         L     @01,APINAXI1(,@15)                               02S0164 00569000
         L     @14,APINXI1(,@01)                                02S0164 00570000
         CR    @14,@04                                          02S0164 00571000
         BL    @RF00164                                         02S0164 00572000
         LA    @11,15                                           02S0164 00573000
         CR    @14,@11                                          02S0164 00574000
         BH    @RF00164                                         02S0164 00575000
*             DO;                                               02S0165 00576000
*               APCIXID1=APINXI1;   /* YES - STORE IN CIT            */ 00577000
         LA    @10,CITENTRY                                     02S0166 00578000
         STH   @14,APCIXID1(,@10)                               02S0166 00579000
*               IF APALLI2X=ON THEN /* TEST IF IVAL2 SHOULD BE AN       00580000
*                                      INDEX ID                      */ 00581000
         L     @01,APCWAPAL(,APCWPTR)                           02S0167 00582000
         TM    APALLI2X(@01),B'10000000'                        02S0167 00583000
         BNO   @RF00167                                         02S0167 00584000
*                 DO;               /* YES                           */ 00585000
*                   IF(APINXI2>=1)&(APINXI2<=15) THEN/* TEST IF IVAL2   00586000
*                                      IS A VALID INDEX ID           */ 00587000
         L     @01,APINAXI2(,@15)                               02S0169 00588000
         L     @15,APINXI2(,@01)                                02S0169 00589000
         CR    @15,@04                                          02S0169 00590000
         BL    @RF00169                                         02S0169 00591000
         CR    @15,@11                                          02S0169 00592000
         BH    @RF00169                                         02S0169 00593000
*                     APCIXID2=APINXI2;/* YES - STORE IN CIT         */ 00594000
         STH   @15,APCIXID2(,@10)                               02S0170 00595000
*                   ELSE                                        02S0171 00596000
*                     DO;           /* NO - SET ERROR INDICATORS     */ 00597000
         B     @RC00169                                         02S0171 00598000
@RF00169 DS    0H                                               02S0172 00599000
*                       ERRFLAG=ON; /* SET ERROR FLAG                */ 00600000
         LA    ERRFLAG,1                                        02S0172 00601000
*                       APCWRCN=APCCX002;/* SET ERROR NUMBER         */ 00602000
         MVI   APCWRCN(APCWPTR),X'02'                           02S0173 00603000
*                       APEMMSGN=APCMX002;/* SET MESSAGE NUMBER FOR     00604000
*                                      OUT OF RANGE                  */ 00605000
         LA    @04,112                                          02S0174 00606000
         ALR   @04,APCWPTR                                      02S0174 00607000
         MVC   APEMMSGN(4,@04),@CF00628                         02S0174 00608000
*                       APEMVALN=APCMK039;/* SET KEYWORD NUMBER FOR     00609000
*                                      IVAL2                         */ 00610000
         MVC   APEMVALN(4,@04),@CF01150                         02S0175 00611000
*                     END;                                      02S0176 00612000
*                 END;                                          02S0177 00613000
*               ELSE                /* IVAL2 IS IMMEDIATE DATA       */ 00614000
*                 APCIXIMM=APINXI2; /* STORE INTO CIT                */ 00615000
*                                                               02S0178 00616000
         B     @RC00167                                         02S0178 00617000
@RF00167 LA    @04,CITENTRY                                     02S0178 00618000
         L     @01,APCWAPIN(,APCWPTR)                           02S0178 00619000
         L     @01,APINAXI2(,@01)                               02S0178 00620000
         L     @15,APINXI2(,@01)                                02S0178 00621000
         ST    @15,APCIXIMM(,@04)                               02S0178 00622000
*               /*****************************************************/ 00623000
*               /*                                                   */ 00624000
*               /* DETERMINE THE BRANCH CONDITION REQUESTED BY THE   */ 00625000
*               /* COND PARAMETER AND SET THE CORRECT CONDITION MASK */ 00626000
*               /* IN THE CIT ENTRY                                  */ 00627000
*               /*                                                   */ 00628000
*               /*****************************************************/ 00629000
*                                                               02S0179 00630000
*               IF APINXCD='EQ  ' THEN/* EQUAL?                      */ 00631000
@RC00167 L     @04,APCWAPIN(,APCWPTR)                           02S0179 00632000
         L     @04,APINAXCD(,@04)                               02S0179 00633000
         CLC   APINXCD(4,@04),@CC01867                          02S0179 00634000
         BNE   @RF00179                                         02S0179 00635000
*                 APCIXCC=APCIXCEQ; /* STORE EQ CONDITION MASK       */ 00636000
         LA    @04,CITENTRY                                     02S0180 00637000
         MVC   APCIXCC(2,@04),@CB00456                          02S0180 00638000
*               IF APINXCD='NE  ' THEN/* NOT EQUAL?                  */ 00639000
@RF00179 L     @04,APCWAPIN(,APCWPTR)                           02S0181 00640000
         L     @04,APINAXCD(,@04)                               02S0181 00641000
         CLC   APINXCD(4,@04),@CC01868                          02S0181 00642000
         BNE   @RF00181                                         02S0181 00643000
*                 APCIXCC=APCIXCNE; /* STORE NE CONDITION MASK       */ 00644000
         LA    @04,CITENTRY                                     02S0182 00645000
         MVC   APCIXCC(2,@04),@CB00458                          02S0182 00646000
*               IF APINXCD='LT  ' THEN/* LESS THAN?                  */ 00647000
@RF00181 L     @04,APCWAPIN(,APCWPTR)                           02S0183 00648000
         L     @04,APINAXCD(,@04)                               02S0183 00649000
         CLC   APINXCD(4,@04),@CC01869                          02S0183 00650000
         BNE   @RF00183                                         02S0183 00651000
*                 APCIXCC=APCIXCLT; /* STORE LT CONDITION MASK       */ 00652000
         LA    @04,CITENTRY                                     02S0184 00653000
         MVC   APCIXCC(2,@04),@CB00460                          02S0184 00654000
*               IF APINXCD='LE  ' THEN/* LESS THAN OR EQUAL?         */ 00655000
@RF00183 L     @04,APCWAPIN(,APCWPTR)                           02S0185 00656000
         L     @04,APINAXCD(,@04)                               02S0185 00657000
         CLC   APINXCD(4,@04),@CC01870                          02S0185 00658000
         BNE   @RF00185                                         02S0185 00659000
*                 APCIXCC=APCIXCLE; /* STORE LE CONDITION MASK       */ 00660000
         LA    @04,CITENTRY                                     02S0186 00661000
         MVC   APCIXCC(2,@04),@CB00464                          02S0186 00662000
*               IF APINXCD='GT  ' THEN/* GREATER THAN?               */ 00663000
@RF00185 L     @04,APCWAPIN(,APCWPTR)                           02S0187 00664000
         L     @04,APINAXCD(,@04)                               02S0187 00665000
         CLC   APINXCD(4,@04),@CC01871                          02S0187 00666000
         BNE   @RF00187                                         02S0187 00667000
*                 APCIXCC=APCIXCGT; /* STORE GT CONDITION MASK       */ 00668000
         LA    @04,CITENTRY                                     02S0188 00669000
         MVC   APCIXCC(2,@04),@CB00466                          02S0188 00670000
*               IF APINXCD='GE  ' THEN/* GREATER THAN OR EQUAL?      */ 00671000
@RF00187 L     @04,APCWAPIN(,APCWPTR)                           02S0189 00672000
         L     @04,APINAXCD(,@04)                               02S0189 00673000
         CLC   APINXCD(4,@04),@CC01872                          02S0189 00674000
         BNE   @RF00189                                         02S0189 00675000
*                 APCIXCC=APCIXCGE; /* STORE GE CONDITION MASK       */ 00676000
*                                                               02S0190 00677000
         LA    @04,CITENTRY                                     02S0190 00678000
         MVC   APCIXCC(2,@04),@CB00462                          02S0190 00679000
*               /*****************************************************/ 00680000
*               /*                                                   */ 00681000
*               /* TEST IF THE COND PARAMETER WAS VALID. IF NOT SET  */ 00682000
*               /* UP ERROR INDICATORS.                              */ 00683000
*               /*                                                   */ 00684000
*               /*****************************************************/ 00685000
*                                                               02S0191 00686000
*               IF APCIXCC=0 THEN   /* TEST IF COND VALID            */ 00687000
@RF00189 LA    @04,CITENTRY                                     02S0191 00688000
         LH    @04,APCIXCC(,@04)                                02S0191 00689000
         N     @04,@CF01966                                     02S0191 00690000
         LTR   @04,@04                                          02S0191 00691000
         BNZ   @RF00191                                         02S0191 00692000
*                 DO;               /* NO - SET ERROR INDICATORS     */ 00693000
*                   ERRFLAG=ON;     /* INDICATE ERROR                */ 00694000
         LA    ERRFLAG,1                                        02S0193 00695000
*                   APCWRCN=APCCX006;/* SET ERROR NUMBER             */ 00696000
         MVI   APCWRCN(APCWPTR),X'06'                           02S0194 00697000
*                   APEMMSGN=APCMX006;/* SET MESSAGE NUMBER FOR 02S0195 00698000
*                                      INVALID VALUE                 */ 00699000
         LA    @04,112                                          02S0195 00700000
         ALR   @04,APCWPTR                                      02S0195 00701000
         MVC   APEMMSGN(4,@04),@CF01059                         02S0195 00702000
*                   APEMVALN=APCMK037;/* SET KEYWORD NUMBER FOR COND */ 00703000
         MVC   APEMVALN(4,@04),@CF01146                         02S0196 00704000
*                 END;                                          02S0197 00705000
*               ELSE                /* COND OKAY - CONTINUE          */ 00706000
*                                                               02S0198 00707000
*                 /***************************************************/ 00708000
*                 /*                                                 */ 00709000
*                 /* SEARCH THE LABEL TABLE TO SEE IF THIS LABEL HAS */ 00710000
*                 /* BEEN DEFINED. IF SO, GET THE BRANCH TO OFFSET   */ 00711000
*                 /* AND INSERT IT INTO THE CIT ENTRY.               */ 00712000
*                 /*                                                 */ 00713000
*                 /***************************************************/ 00714000
*                                                               02S0198 00715000
*                 DO;                                           02S0198 00716000
         B     @RC00191                                         02S0198 00717000
@RF00191 DS    0H                                               02S0199 00718000
*                   LOOPCTRL=OFF;   /* INITIALIZE LOOP CONTROL FLAG     00719000
*                                      TO OFF                        */ 00720000
         SLR   @11,@11                                          02S0199 00721000
         LR    LOOPCTRL,@11                                     02S0199 00722000
*                   GOODONE=OFF;    /* INITIALIZE FLAG               */ 00723000
         LR    GOODONE,@11                                      02S0200 00724000
*                   DO I=1 TO APLBNUMB WHILE LOOPCTRL=OFF;/* LOOP       00725000
*                                      THRU APLB LOOKING FOR A  02S0201 00726000
*                                      DEFINITION OF THIS LABEL      */ 00727000
         LA    I,1                                              02S0201 00728000
         B     @DE00201                                         02S0201 00729000
@DL00201 LTR   LOOPCTRL,LOOPCTRL                                02S0201 00730000
         BNZ   @DC00201                                         02S0201 00731000
*                     IF(APLBGOOD(I)=ON)&(APLBDEF(I)=ON)&(APLBLBL(I)=   00732000
*                         APINXLB) THEN/* TEST IF LABEL DEFINED      */ 00733000
         LR    @11,I                                            02S0202 00734000
         SLA   @11,3                                            02S0202 00735000
         L     @04,APCWACB(,APCWPTR)                            02S0202 00736000
         L     @04,ACBAMWAP(,@04)                               02S0202 00737000
         L     @04,APAXAPLB(,@04)                               02S0202 00738000
         ST    @11,@TF00001                                     02S0202 00739000
         ALR   @11,@04                                          02S0202 00740000
         TM    APLBGOOD-8(@11),B'11000000'                      02S0202 00741000
         BNO   @RF00202                                         02S0202 00742000
         L     @11,APCWAPIN(,APCWPTR)                           02S0202 00743000
         L     @11,APINAXLB(,@11)                               02S0202 00744000
         LR    @01,@04                                          02S0202 00745000
         AL    @01,@TF00001                                     02S0202 00746000
         CLC   APLBLBL-8(4,@01),APINXLB(@11)                    02S0202 00747000
         BNE   @RF00202                                         02S0202 00748000
*                       DO;         /* YES - GET CIT OFFSET          */ 00749000
*                         APCIXBRO=APLBLOC(I);/* INSERT INTO CIT     */ 00750000
         LA    @11,CITENTRY                                     02S0204 00751000
         L     @01,@TF00001                                     02S0204 00752000
         LH    @04,APLBLOC-8(@01,@04)                           02S0204 00753000
         STH   @04,APCIXBRO(,@11)                               02S0204 00754000
*                         LOOPCTRL=ON;/* END SCAN                    */ 00755000
         LA    @11,1                                            02S0205 00756000
         LR    LOOPCTRL,@11                                     02S0205 00757000
*                         GOODONE=ON;/* MISSION ACCOMPLISHED         */ 00758000
         LR    GOODONE,@11                                      02S0206 00759000
*                       END;                                    02S0207 00760000
*                     ELSE                                      02S0208 00761000
*                       ;                                       02S0208 00762000
@RF00202 DS    0H                                               02S0209 00763000
*                   END;                                        02S0209 00764000
*                                                               02S0209 00765000
         AL    I,@CF00061                                       02S0209 00766000
@DE00201 L     @11,APCWACB(,APCWPTR)                            02S0209 00767000
         L     @11,ACBAMWAP(,@11)                               02S0209 00768000
         L     @11,APAXAPLB(,@11)                               02S0209 00769000
         C     I,APLBNUMB(,@11)                                 02S0209 00770000
         BNH   @DL00201                                         02S0209 00771000
@DC00201 DS    0H                                               02S0210 00772000
*                   /*************************************************/ 00773000
*                   /*                                               */ 00774000
*                   /* IF THE LABEL WAS NOT DEFINED, INSERT A        */ 00775000
*                   /* REFERENCE ENTRY INTO THE LABEL TABLE SO THE   */ 00776000
*                   /* BRANCH CAN BE RESOLVED WHEN DEFINED VIA XID.  */ 00777000
*                   /*                                               */ 00778000
*                   /*************************************************/ 00779000
*                                                               02S0210 00780000
*                   IF GOODONE=OFF THEN/* TEST IF LABEL WAS DEFINED  */ 00781000
         SLR   @11,@11                                          02S0210 00782000
         CR    GOODONE,@11                                      02S0210 00783000
         BNE   @RF00210                                         02S0210 00784000
*                     DO;           /* NO                            */ 00785000
*                       LOOPCTRL=OFF;/* INITIALIZE FLAG              */ 00786000
         LR    LOOPCTRL,@11                                     02S0212 00787000
*                       DO I=1 TO APLBNUMB WHILE LOOPCTRL=OFF;/* FIND   00788000
*                                      AN UNUSED ENTRY AND USE IT    */ 00789000
         LA    I,1                                              02S0213 00790000
         B     @DE00213                                         02S0213 00791000
@DL00213 LTR   LOOPCTRL,LOOPCTRL                                02S0213 00792000
         BNZ   @DC00213                                         02S0213 00793000
*                         IF APLBGOOD(I)=OFF THEN/* TEST IF ENTRY IN    00794000
*                                      USE                           */ 00795000
