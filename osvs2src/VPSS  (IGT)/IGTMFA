         AL    @14,@TF00001                                     02S0253 00980000
         CLC   APLBLBL-8(4,@14),APINDLB(@04)                    02S0253 00981000
         BNE   @RF00253                                         02S0253 00982000
*               DO;                 /* YES                           */ 00983000
*                 APCIXBRO=APLBLOC(I);/* INSERT CIT OFFSET INTO CIT     00984000
*                                      ENTRY                         */ 00985000
