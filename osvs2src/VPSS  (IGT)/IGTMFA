*/************************************************************       */ 01116000
*/*                                                                  */ 01117000
*/* THIS CASE BUILDS A CIT ENTRY FOR THE COUNT LOOP                  */ 01118000
*/* OPERATION. THE CALL FORMAT FOR A COUNT LOOP IS:                  */ 01119000
*/*                                                                  */ 01120000
*/*      CALL VPSS(IUNIT,'XBCT',IVAL1,LABEL)                         */ 01121000
*/*                                                                  */ 01122000
*/*      WHERE:                                                      */ 01123000
*/*                                                                  */ 01124000
*/*        IUNIT - UNIT REFERENCE NUMBER                             */ 01125000
*/*        XBCT  - FUNCTION MNEMONIC                                 */ 01126000
*/*        LABEL - BRANCH TO LOCATION - DEFINED BY 'XID '            */ 01127000
*/*        IVAL1 - INDEX ID                                          */ 01128000
*/*  NOTE: THE XBCT IS EQUIVALENT TO THE FOLLOWING CALL-             */ 01129000
*/*                                                                  */ 01130000
*/*      CALL VPSS(IUNIT,'XDCI','GT  ',IVAL1,0,LABEL)                */ 01131000
*/*                                                                  */ 01132000
*/* THE CIT FORMAT FOR A COUNT LOOP OPERATION IS:                    */ 01133000
*/*                                                                  */ 01134000
*/*       <------------- 4 BYTES ------------------>                 */ 01135000
*/*       ------------------------------------------                 */ 01136000
*/*       |   OPERATION ID    |ENTRY LENGTH(HWORDS)|                 */ 01137000
*/*       ------------------------------------------                 */ 01138000
*/*       |    INDEX ID       |     UNUSED         |                 */ 01139000
*/*       ------------------------------------------                 */ 01140000
*/*       |                 ZERO                   |                 */ 01141000
*/*       ------------------------------------------                 */ 01142000
*/*       |  CONDITION MASK   |REL CIT OFFSET-HWORD|                 */ 01143000
*/*       ------------------------------------------                 */ 01144000
*/*                                                                  */ 01145000
*/*       CONDITION MASK IS:                                         */ 01146000
*/*         X'FFF5' - GREATER THAN                                   */ 01147000
*/*                                                                  */ 01148000
*/************************************************************       */ 01149000
*                                                               02S0290 01150000
*         DO;                                                   02S0290 01151000
*           IF(APINBIV<1)|(APINBIV>15) THEN/* TEST IF IVAL1 IS A        01152000
*                                      VALID INDEX ID                */ 01153000
         L     @04,APCWAPIN(,APCWPTR)                           02S0291 01154000
         L     @04,APINABIV(,@04)                               02S0291 01155000
         L     @04,APINBIV(,@04)                                02S0291 01156000
         LTR   @04,@04                                          02S0291 01157000
         BNP   @RT00291                                         02S0291 01158000
         C     @04,@CF00084                                     02S0291 01159000
         BNH   @RF00291                                         02S0291 01160000
@RT00291 DS    0H                                               02S0292 01161000
*             DO;                   /* NO - SET ERROR INDICATORS     */ 01162000
*               APCWRCN=APCCX001;   /* SET ERROR NUMBER              */ 01163000
         MVI   APCWRCN(APCWPTR),X'01'                           02S0293 01164000
*               ERRFLAG=ON;         /* SET ERROR FLAG                */ 01165000
         LA    ERRFLAG,1                                        02S0294 01166000
*               APEMMSGN=APCMX001;  /* SET MESSAGE NUMBER FOR OUT OF    01167000
*                                      RANGE                         */ 01168000
         LA    @04,112                                          02S0295 01169000
         ALR   @04,APCWPTR                                      02S0295 01170000
         MVC   APEMMSGN(4,@04),@CF00628                         02S0295 01171000
*               APEMVALN=APCMK038;  /* SET KEYWORD NUMBER FOR IVAL1  */ 01172000
         MVC   APEMVALN(4,@04),@CF01148                         02S0296 01173000
*             END;                                              02S0297 01174000
*           ELSE                                                02S0298 01175000
*             DO;                                               02S0298 01176000
         B     @RC00291                                         02S0298 01177000
@RF00291 DS    0H                                               02S0299 01178000
*               APCIXID1=APINBIV;   /* IVAL1 TO CIT                  */ 01179000
*                                                               02S0299 01180000
         LA    @11,CITENTRY                                     02S0299 01181000
         L     @04,APCWAPIN(,APCWPTR)                           02S0299 01182000
         L     @04,APINABIV(,@04)                               02S0299 01183000
         L     @04,APINBIV(,@04)                                02S0299 01184000
         STH   @04,APCIXID1(,@11)                               02S0299 01185000
*               /*****************************************************/ 01186000
*               /*                                                   */ 01187000
*               /* INSERT A CONDITION MASK OF GREATER THAN           */ 01188000
*               /*                                                   */ 01189000
*               /*****************************************************/ 01190000
*                                                               02S0300 01191000
*               APCIXCC=APCIXCGT;   /* GT MASK TO CIT                */ 01192000
*                                                               02S0300 01193000
         MVC   APCIXCC(2,@11),@CB00466                          02S0300 01194000
*               /*****************************************************/ 01195000
*               /*                                                   */ 01196000
*               /* SEARCH THE LABEL TABLE TO SEE IF THE LABEL HAS    */ 01197000
*               /* BEEN DEFINED. IF SO, GET THE BRANCH TO OFFSET AND */ 01198000
*               /* INSERT IT INTO THE CIT.                           */ 01199000
*               /*                                                   */ 01200000
*               /*****************************************************/ 01201000
*                                                               02S0301 01202000
*               LOOPCTRL=OFF;       /* INITIALIZE LOOP CONTROL FLAG     01203000
*                                      TO OFF                        */ 01204000
         SLR   @11,@11                                          02S0301 01205000
         LR    LOOPCTRL,@11                                     02S0301 01206000
*               GOODONE=OFF;        /* INITIALIZE FLAG               */ 01207000
         LR    GOODONE,@11                                      02S0302 01208000
*               DO I=1 TO APLBNUMB WHILE LOOPCTRL=OFF;/* LOOP THRU      01209000
*                                      APLB LOOKING FOR A DEFINITION    01210000
*                                      OF THIS LABEL                 */ 01211000
         LA    I,1                                              02S0303 01212000
         B     @DE00303                                         02S0303 01213000
@DL00303 LTR   LOOPCTRL,LOOPCTRL                                02S0303 01214000
         BNZ   @DC00303                                         02S0303 01215000
*                 IF(APLBGOOD(I)=ON)&(APLBDEF(I)=ON)&(APLBLBL(I)=       01216000
*                     APINBLB) THEN /* SEE IF LABEL HAS BEEN DEFINED */ 01217000
         LR    @11,I                                            02S0304 01218000
         SLA   @11,3                                            02S0304 01219000
         L     @04,APCWACB(,APCWPTR)                            02S0304 01220000
         L     @04,ACBAMWAP(,@04)                               02S0304 01221000
         L     @04,APAXAPLB(,@04)                               02S0304 01222000
         ST    @11,@TF00001                                     02S0304 01223000
         ALR   @11,@04                                          02S0304 01224000
         TM    APLBGOOD-8(@11),B'11000000'                      02S0304 01225000
         BNO   @RF00304                                         02S0304 01226000
         L     @11,APCWAPIN(,APCWPTR)                           02S0304 01227000
         L     @11,APINABLB(,@11)                               02S0304 01228000
         LR    @01,@04                                          02S0304 01229000
         AL    @01,@TF00001                                     02S0304 01230000
         CLC   APLBLBL-8(4,@01),APINBLB(@11)                    02S0304 01231000
         BNE   @RF00304                                         02S0304 01232000
*                   DO;             /* YES                           */ 01233000
*                     APCIXBRO=APLBLOC(I);/* INSERT CIT OFFSET INTO     01234000
*                                      CIT ENTRY                     */ 01235000
         LA    @11,CITENTRY                                     02S0306 01236000
         L     @01,@TF00001                                     02S0306 01237000
         LH    @04,APLBLOC-8(@01,@04)                           02S0306 01238000
         STH   @04,APCIXBRO(,@11)                               02S0306 01239000
*                     LOOPCTRL=ON;  /* END SEARCH                    */ 01240000
         LA    @11,1                                            02S0307 01241000
         LR    LOOPCTRL,@11                                     02S0307 01242000
*                     GOODONE=ON;   /* INDICATE SUCCESS              */ 01243000
         LR    GOODONE,@11                                      02S0308 01244000
*                   END;                                        02S0309 01245000
*                 ELSE                                          02S0310 01246000
*                   ;                                           02S0310 01247000
@RF00304 DS    0H                                               02S0311 01248000
*               END;                                            02S0311 01249000
*                                                               02S0311 01250000
         AL    I,@CF00061                                       02S0311 01251000
@DE00303 L     @11,APCWACB(,APCWPTR)                            02S0311 01252000
         L     @11,ACBAMWAP(,@11)                               02S0311 01253000
         L     @11,APAXAPLB(,@11)                               02S0311 01254000
         C     I,APLBNUMB(,@11)                                 02S0311 01255000
         BNH   @DL00303                                         02S0311 01256000
@DC00303 DS    0H                                               02S0312 01257000
*               /*****************************************************/ 01258000
*               /*                                                   */ 01259000
*               /* IF LABEL WAS NOT DEFINED MAKE A REFERENCE ENTRY IN*/ 01260000
*               /* THE LABEL TABLE                                   */ 01261000
*               /*                                                   */ 01262000
*               /*****************************************************/ 01263000
*                                                               02S0312 01264000
*               IF GOODONE=OFF THEN /* TEST IF LABEL WAS DEFINED     */ 01265000
         SLR   @11,@11                                          02S0312 01266000
         CR    GOODONE,@11                                      02S0312 01267000
         BNE   @RF00312                                         02S0312 01268000
*                 DO;               /* NO                            */ 01269000
*                   LOOPCTRL=OFF;   /* SET FLAG                      */ 01270000
         LR    LOOPCTRL,@11                                     02S0314 01271000
*                   DO I=1 TO APLBNUMB WHILE LOOPCTRL=OFF;/* SEARCH     01272000
*                                      FOR AN UNUSED ENTRY           */ 01273000
         LA    I,1                                              02S0315 01274000
         B     @DE00315                                         02S0315 01275000
@DL00315 LTR   LOOPCTRL,LOOPCTRL                                02S0315 01276000
         BNZ   @DC00315                                         02S0315 01277000
*                     IF APLBGOOD(I)=OFF THEN/* TEST IN USE FLAG     */ 01278000
         LR    @11,I                                            02S0316 01279000
         SLA   @11,3                                            02S0316 01280000
         L     @04,APCWACB(,APCWPTR)                            02S0316 01281000
         L     @04,ACBAMWAP(,@04)                               02S0316 01282000
         L     @15,APAXAPLB(,@04)                               02S0316 01283000
         ST    @11,@TF00001                                     02S0316 01284000
         ALR   @11,@15                                          02S0316 01285000
         TM    APLBGOOD-8(@11),B'10000000'                      02S0316 01286000
         BNZ   @RF00316                                         02S0316 01287000
*                       DO;         /* UNUSED - USE IT               */ 01288000
*                         APLBGOOD(I)=ON;/* MARK AS IN USE           */ 01289000
         LR    @11,@15                                          02S0318 01290000
         AL    @11,@TF00001                                     02S0318 01291000
         OI    APLBGOOD-8(@11),B'10000000'                      02S0318 01292000
*                         APLBLBL(I)=APINBLB;/* INSERT LABEL         */ 01293000
         LR    @11,@15                                          02S0319 01294000
         AL    @11,@TF00001                                     02S0319 01295000
         L     @01,APCWAPIN(,APCWPTR)                           02S0319 01296000
         L     @01,APINABLB(,@01)                               02S0319 01297000
         MVC   APLBLBL-8(4,@11),APINBLB(@01)                    02S0319 01298000
*                         APLBDEF(I)=OFF;/* MARK AS REFERENCE        */ 01299000
         LR    @11,@15                                          02S0320 01300000
         AL    @11,@TF00001                                     02S0320 01301000
         NI    APLBDEF-8(@11),B'10111111'                       02S0320 01302000
*                         APLBLOC(I)=(APAXCIN-APAXCIS)/2;/* FILL IN     01303000
*                                      CIT OFFSET OF THIS ENTRY      */ 01304000
         L     @00,APAXCIN(,@04)                                02S0321 01305000
         SL    @00,APAXCIS(,@04)                                02S0321 01306000
         SRDA  @00,32                                           02S0321 01307000
         D     @00,@CF00064                                     02S0321 01308000
         L     @11,@TF00001                                     02S0321 01309000
         STH   @01,APLBLOC-8(@11,@15)                           02S0321 01310000
*                         LOOPCTRL=ON;/* END LOOP                    */ 01311000
         LA    @11,1                                            02S0322 01312000
         LR    LOOPCTRL,@11                                     02S0322 01313000
*                         GOODONE=ON;/* INDICATE SUCCESS             */ 01314000
         LR    GOODONE,@11                                      02S0323 01315000
*                       END;                                    02S0324 01316000
*                     ELSE                                      02S0325 01317000
*                       ;                                       02S0325 01318000
@RF00316 DS    0H                                               02S0326 01319000
*                   END;                                        02S0326 01320000
         AL    I,@CF00061                                       02S0326 01321000
@DE00315 L     @11,APCWACB(,APCWPTR)                            02S0326 01322000
         L     @11,ACBAMWAP(,@11)                               02S0326 01323000
         L     @11,APAXAPLB(,@11)                               02S0326 01324000
         C     I,APLBNUMB(,@11)                                 02S0326 01325000
         BNH   @DL00315                                         02S0326 01326000
@DC00315 DS    0H                                               02S0327 01327000
*                 END;                                          02S0327 01328000
*               ELSE                                            02S0328 01329000
*                 ;                                             02S0328 01330000
@RF00312 DS    0H                                               02S0329 01331000
*               IF GOODONE=OFF THEN                             02S0329 01332000
*                                                               02S0329 01333000
         LTR   GOODONE,GOODONE                                  02S0329 01334000
         BNZ   @RF00329                                         02S0329 01335000
*                 /***************************************************/ 01336000
*                 /*                                                 */ 01337000
*                 /* IF LABEL NOT DEFINED AND UNABLE TO PLACE A      */ 01338000
*                 /* REFERENCE ENTRY INTO THE LABEL TABLE, SET ERROR */ 01339000
*                 /* INDICATORS                                      */ 01340000
*                 /*                                                 */ 01341000
*                 /***************************************************/ 01342000
*                                                               02S0330 01343000
*                 DO;               /* SET ERROR INDICATORS          */ 01344000
*                   ERRFLAG=ON;     /* SET ERROR FLAG                */ 01345000
         LA    ERRFLAG,1                                        02S0331 01346000
*                   APCWRCN=APCCX004;/* SET ERROR NUMBER             */ 01347000
         MVI   APCWRCN(APCWPTR),X'04'                           02S0332 01348000
*                   APEMMSGN=APCMX004;/* SET MESSAGE NUMBER FOR TABLE   01349000
*                                      OVERFLOW                      */ 01350000
         LA    @04,112                                          02S0333 01351000
         ALR   @04,APCWPTR                                      02S0333 01352000
         MVC   APEMMSGN(4,@04),@CF01150                         02S0333 01353000
*                   APEMVALN=APCMK051;/* KEYWORD NUMBER FOR APLB     */ 01354000
         MVC   APEMVALN(4,@04),@CF01173                         02S0334 01355000
*                 END;                                          02S0335 01356000
*               ELSE                                            02S0336 01357000
*                 ;                                             02S0336 01358000
@RF00329 DS    0H                                               02S0337 01359000
*             END;                                              02S0337 01360000
*         END;                                                  02S0338 01361000
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */ 01362000
         B     CE0001@                                          02S0339 01363000
*       END XBCT;                   /* END OF SUBCASE                */ 01364000
*XID:                                                           02S0341 01365000
*       DO;                         /* SUBCASE                       */ 01366000
XID      DS    0H                                               02S0342 01367000
*/************************************************************       */ 01368000
*/*                                                                  */ 01369000
*/* THIS CASE BUILDS A LABEL TABLE ENTRY FOR A LABEL                 */ 01370000
*/* DEFINITION CALL('XID '). THE CALL FORMAT FOR A LABEL             */ 01371000
*/* DEFINITION IS:                                                   */ 01372000
*/*                                                                  */ 01373000
*/*     CALL VPSS(IUNIT,'XID ',LABEL)                                */ 01374000
*/*                                                                  */ 01375000
*/*     WHERE:                                                       */ 01376000
*/*                                                                  */ 01377000
*/*       IUNIT - UNIT REFERENCE NUMBER                              */ 01378000
*/*       XID   - FUNCTION MNEMONIC                                  */ 01379000
*/*       LABEL - LABEL TO DEFINE THIS OFFSET INTO THE CIT           */ 01380000
*/* NOTE: XID MERELY DEFINES A LOCATION IN THE CIT WHICH             */ 01381000
*/*       CAN BE REFERENCED BY BRANCH INSTRUCTIONS. NO CIT           */ 01382000
*/*       ENTRY IS CONSTRUCTED FOR THIS CALL.                        */ 01383000
*/*                                                                  */ 01384000
*/************************************************************       */ 01385000
*                                                               02S0342 01386000
*         DO;                                                   02S0342 01387000
*                                                               02S0342 01388000
*           /*********************************************************/ 01389000
*           /*                                                       */ 01390000
*           /* FIRST VERIFY THAT THIS LABEL IS NOT A DUPLICATE BY    */ 01391000
*           /* SCANNING THE LABEL TABLE                              */ 01392000
*           /*                                                       */ 01393000
*           /*********************************************************/ 01394000
*                                                               02S0343 01395000
*           DO I=1 TO APLBNUMB WHILE ERRFLAG=OFF;               02S0343 01396000
         LA    I,1                                              02S0343 01397000
         B     @DE00343                                         02S0343 01398000
@DL00343 LTR   ERRFLAG,ERRFLAG                                  02S0343 01399000
         BNZ   @DC00343                                         02S0343 01400000
*             IF(APLBGOOD(I)=ON)&(APLBDEF(I)=ON)&(APLBLBL(I)=APINDLB)   01401000
*               THEN                /* THE LABEL IS A DUPLICATE IF      01402000
*                                      THE APLB ENTRY IS : VALID,A      01403000
*                                      DEFINITION ENTRY AND THE 02S0344 01404000
*                                      LABELS ARE THE SAME           */ 01405000
         LR    @04,I                                            02S0344 01406000
         SLA   @04,3                                            02S0344 01407000
         L     @03,APCWACB(,APCWPTR)                            02S0344 01408000
         L     @03,ACBAMWAP(,@03)                               02S0344 01409000
         L     @03,APAXAPLB(,@03)                               02S0344 01410000
         ST    @04,@TF00001                                     02S0344 01411000
         ALR   @04,@03                                          02S0344 01412000
         TM    APLBGOOD-8(@04),B'11000000'                      02S0344 01413000
         BNO   @RF00344                                         02S0344 01414000
         L     @04,APCWAPIN(,APCWPTR)                           02S0344 01415000
         L     @04,APINADLB(,@04)                               02S0344 01416000
         AL    @03,@TF00001                                     02S0344 01417000
         CLC   APLBLBL-8(4,@03),APINDLB(@04)                    02S0344 01418000
         BNE   @RF00344                                         02S0344 01419000
*               DO;                 /* SET ERROR INDICATORS FOR 02S0345 01420000
*                                      DUPLICATE LABEL               */ 01421000
*                 ERRFLAG=ON;       /* INDICATE ERROR DETECTED       */ 01422000
         LA    ERRFLAG,1                                        02S0346 01423000
*                 APCWRCN=APCCX003; /* SET ERROR NUMBER              */ 01424000
         MVI   APCWRCN(APCWPTR),X'03'                           02S0347 01425000
*                 APEMMSGN=APCMX003;/* SET MESSAGE NUMBER FOR   02S0348 01426000
*                                      DUPLICATE LABEL               */ 01427000
         LA    @03,112                                          02S0348 01428000
         ALR   @03,APCWPTR                                      02S0348 01429000
         MVC   APEMMSGN(4,@03),@CF01377                         02S0348 01430000
*                 APEMVALN=-1;      /* INDICATE ACTUAL USER PARA-       01431000
*                                      METER TO BE DISPLAYED         */ 01432000
         MVC   APEMVALN(4,@03),@CF01876                         02S0349 01433000
*                 APEMVALP=APINDLB; /* SHOW LABEL IN ERROR           */ 01434000
         MVC   APEMVALP(4,@03),APINDLB(@04)                     02S0350 01435000
*               END;                                            02S0351 01436000
*             ELSE                                              02S0352 01437000
*               ;                                               02S0352 01438000
@RF00344 DS    0H                                               02S0353 01439000
*           END;                                                02S0353 01440000
*                                                               02S0353 01441000
         AL    I,@CF00061                                       02S0353 01442000
@DE00343 L     @04,APCWACB(,APCWPTR)                            02S0353 01443000
         L     @04,ACBAMWAP(,@04)                               02S0353 01444000
         L     @04,APAXAPLB(,@04)                               02S0353 01445000
         C     I,APLBNUMB(,@04)                                 02S0353 01446000
         BNH   @DL00343                                         02S0353 01447000
@DC00343 DS    0H                                               02S0354 01448000
*           /*********************************************************/ 01449000
*           /*                                                       */ 01450000
*           /* IF THE LABEL IS NOT A DUPLICATE, PLACE A LABEL        */ 01451000
*           /* DEFINITION ENTRY INTO THE LABEL TABLE AND RESOLVE ALL */ 01452000
*           /* FORWARD DEFINITIONS TO THE LABEL                      */ 01453000
*           /*                                                       */ 01454000
*           /*********************************************************/ 01455000
*                                                               02S0354 01456000
*           IF ERRFLAG=OFF THEN     /* TEST IF LABEL WAS FOUND TO BE    01457000
*                                      A DUPLICATE                   */ 01458000
         SLR   @04,@04                                          02S0354 01459000
         CR    ERRFLAG,@04                                      02S0354 01460000
         BNE   @RF00354                                         02S0354 01461000
*             DO;                   /* NO - PLACE A DEFINITION ENTRY    01462000
*                                      IN THE LABEL TABLE            */ 01463000
*               LOOPCTRL=OFF;       /* INITIALIZE LOOP CONTROL FLAG     01464000
*                                      TO OFF                        */ 01465000
         LR    LOOPCTRL,@04                                     02S0356 01466000
*               GOODONE=OFF;        /* SET FLAG                      */ 01467000
         LR    GOODONE,@04                                      02S0357 01468000
*               DO I=1 TO APLBNUMB WHILE LOOPCTRL=OFF;/* LOOP FOR A     01469000
*                                      FREE ENTRY                    */ 01470000
         LA    I,1                                              02S0358 01471000
         B     @DE00358                                         02S0358 01472000
@DL00358 LTR   LOOPCTRL,LOOPCTRL                                02S0358 01473000
         BNZ   @DC00358                                         02S0358 01474000
*                 IF(APLBGOOD(I)=OFF) THEN/* TEST IF ENTRY IS FREE   */ 01475000
         LR    @04,I                                            02S0359 01476000
         SLA   @04,3                                            02S0359 01477000
         L     @01,APCWACB(,APCWPTR)                            02S0359 01478000
         L     @15,ACBAMWAP(,@01)                               02S0359 01479000
         L     @14,APAXAPLB(,@15)                               02S0359 01480000
         ST    @04,@TF00001                                     02S0359 01481000
         ALR   @04,@14                                          02S0359 01482000
         TM    APLBGOOD-8(@04),B'10000000'                      02S0359 01483000
         BNZ   @RF00359                                         02S0359 01484000
*                   DO;             /* YES - MAKE ENTRY              */ 01485000
*                     APLBGOOD(I)=ON;/* FLAG ENTRY VALID             */ 01486000
*                     APLBDEF(I)=ON;/* MARK AS DEFINITION ENTRY      */ 01487000
         LR    @04,@14                                          02S0362 01488000
         AL    @04,@TF00001                                     02S0362 01489000
         OI    APLBGOOD-8(@04),B'11000000'                      02S0362 01490000
*                     APLBLBL(I)=APINDLB;/* INSERT THE LABEL         */ 01491000
         LR    @04,@14                                          02S0363 01492000
         AL    @04,@TF00001                                     02S0363 01493000
         L     @01,APCWAPIN(,APCWPTR)                           02S0363 01494000
         L     @01,APINADLB(,@01)                               02S0363 01495000
         MVC   APLBLBL-8(4,@04),APINDLB(@01)                    02S0363 01496000
*                     LOOPCTRL=ON;  /* END THE SEARCH                */ 01497000
         LA    @04,1                                            02S0364 01498000
         LR    LOOPCTRL,@04                                     02S0364 01499000
*                     APLBLOC(I)=(APAXCIN-APAXCIS)/2;/* CIT HALFWORD    01500000
*                                      OFFSET OF THIS LABEL          */ 01501000
         L     @00,APAXCIN(,@15)                                02S0365 01502000
         SL    @00,APAXCIS(,@15)                                02S0365 01503000
         SRDA  @00,32                                           02S0365 01504000
         D     @00,@CF00064                                     02S0365 01505000
         L     @15,@TF00001                                     02S0365 01506000
         STH   @01,APLBLOC-8(@15,@14)                           02S0365 01507000
*                     TEMPLOC=APLBLOC(I);                       02S0366 01508000
         LR    TEMPLOC,@01                                      02S0366 01509000
*                     GOODONE=ON;   /* INDICATE SUCCESS              */ 01510000
         LR    GOODONE,@04                                      02S0367 01511000
*                   END;                                        02S0368 01512000
*                 ELSE                                          02S0369 01513000
*                   ;                                           02S0369 01514000
@RF00359 DS    0H                                               02S0370 01515000
*               END;                                            02S0370 01516000
         AL    I,@CF00061                                       02S0370 01517000
@DE00358 L     @04,APCWACB(,APCWPTR)                            02S0370 01518000
         L     @04,ACBAMWAP(,@04)                               02S0370 01519000
         L     @04,APAXAPLB(,@04)                               02S0370 01520000
         C     I,APLBNUMB(,@04)                                 02S0370 01521000
         BNH   @DL00358                                         02S0370 01522000
@DC00358 DS    0H                                               02S0371 01523000
*             END;                                              02S0371 01524000
*           ELSE                                                02S0372 01525000
*             ;                                                 02S0372 01526000
*                                                               02S0372 01527000
@RF00354 DS    0H                                               02S0373 01528000
*           /*********************************************************/ 01529000
*           /*                                                       */ 01530000
*           /* SET ERROR INDICATORS IF NO ROOM IN LABEL TABLE        */ 01531000
*           /*                                                       */ 01532000
*           /*********************************************************/ 01533000
*                                                               02S0373 01534000
*           IF ERRFLAG=OFF THEN     /* TEST IF DUPLICATE             */ 01535000
         SLR   @04,@04                                          02S0373 01536000
         CR    ERRFLAG,@04                                      02S0373 01537000
         BNE   @RF00373                                         02S0373 01538000
*             DO;                   /* NO                            */ 01539000
*               IF GOODONE=OFF THEN /* TEST IF NO ROOM IN TABLE      */ 01540000
         CR    GOODONE,@04                                      02S0375 01541000
         BNE   @RF00375                                         02S0375 01542000
*                 DO;               /* YES - SET ERROR INDICATORS    */ 01543000
*                   ERRFLAG=ON;     /* INDICATE PROBLEM              */ 01544000
         LA    ERRFLAG,1                                        02S0377 01545000
*                   APCWRCN=APCCX004;/* SET ERROR NUMBER             */ 01546000
         MVI   APCWRCN(APCWPTR),X'04'                           02S0378 01547000
*                   APEMMSGN=APCMX004;/* SET MESSAGE NUMBER FOR TABLE   01548000
*                                      OVERFLOW                      */ 01549000
         LA    @04,112                                          02S0379 01550000
         ALR   @04,APCWPTR                                      02S0379 01551000
         MVC   APEMMSGN(4,@04),@CF01150                         02S0379 01552000
*                   APEMVALN=APCMK051;/* SET KEYWORD NUMBER FOR APLB */ 01553000
         MVC   APEMVALN(4,@04),@CF01173                         02S0380 01554000
*                 END;                                          02S0381 01555000
*               ELSE                                            02S0382 01556000
*                                                               02S0382 01557000
*                 /***************************************************/ 01558000
*                 /*                                                 */ 01559000
*                 /* IF ALL IS OKAY TO THIS POINT, SCAN THE LABEL    */ 01560000
*                 /* TABLE TO RESOLVE REFERENCES TO THIS LABEL AND   */ 01561000
*                 /* REMOVE THE REFERENCE ENTRIES.                   */ 01562000
*                 /*                                                 */ 01563000
*                 /***************************************************/ 01564000
*                                                               02S0382 01565000
*                 DO I=1 TO APLBNUMB;                           02S0382 01566000
         B     @RC00375                                         02S0382 01567000
@RF00375 LA    I,1                                              02S0382 01568000
         B     @DE00382                                         02S0382 01569000
@DL00382 DS    0H                                               02S0383 01570000
*                   IF(APLBGOOD(I)=ON)&(APLBDEF(I)=OFF)&(APLBLBL(I)=    01571000
*                       APINDLB) THEN/* SEARCH FOR REFERENCES TO THIS   01572000
*                                      LABEL                         */ 01573000
         LR    @10,I                                            02S0383 01574000
         SLA   @10,3                                            02S0383 01575000
