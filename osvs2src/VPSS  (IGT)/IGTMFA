         BH    @RF00078                                         02S0078 00188000
         CR    @10,@15                                          02S0078 00189000
         BL    @RF00078                                         02S0078 00190000
*                 DO;               /* ICTY VALID - CONTINUE         */ 00191000
*                   APCIXBSA=APINAXBS;/* MOVE POINTER TO BULK STORE     00192000
*                                      ADDRESS TO CIT WORKAREA       */ 00193000
         L     @04,APINAXBS(,@04)                               02S0080 00194000
         ST    @04,APCIXBSA(,@11)                               02S0080 00195000
*                   CALL APCVZMIF(APCIXBSA,RSOLVERR);/* RESOLVE IT   */ 00196000
         LA    @04,APCIXBSA(,@11)                               02S0081 00197000
         ST    @04,@AL00001                                     02S0081 00198000
         LA    @04,RSOLVERR                                     02S0081 00199000
         ST    @04,@AL00001+4                                   02S0081 00200000
         L     @15,APCTZMIF(,APCTPTR)                           02S0081 00201000
         LA    @01,@AL00001                                     02S0081 00202000
         BALR  @14,@15                                          02S0081 00203000
*                   IF RSOLVERR=0 THEN/* TEST FOR ERROR ON CALL      */ 00204000
         SLR   @04,@04                                          02S0082 00205000
         C     @04,RSOLVERR                                     02S0082 00206000
         BNE   @RF00082                                         02S0082 00207000
*                     DO;           /* NO ERROR - CONTINUE           */ 00208000
*                       IF(APINXBX>=0)&(APINXBX<=15) THEN/* YARRVX      00209000
*                                      VALIDITY TEST                 */ 00210000
         L     @01,APCWAPIN(,APCWPTR)                           02S0084 00211000
         L     @01,APINAXBX(,@01)                               02S0084 00212000
         L     @15,APINXBX(,@01)                                02S0084 00213000
         CR    @15,@04                                          02S0084 00214000
         BL    @RF00084                                         02S0084 00215000
