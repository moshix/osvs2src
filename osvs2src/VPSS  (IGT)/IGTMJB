TMJB     TITLE 'IGTMJB --- GENERATE AN ACB                             *00001000
                        '                                               00002000
IGTMJB   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTMJB    77.179'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         MVC   @PC00001(4),0(@01)                               01S0001 00022000
*                                                               01S0041 00023000
*   /*****************************************************************/ 00024000
*   /*                                                               */ 00025000
*   /* GENERATE A NEW ACB                                            */ 00026000
*   /*                                                               */ 00027000
*   /*****************************************************************/ 00028000
*                                                               01S0041 00029000
*   DO;                                                         01S0041 00030000
*     RFY                                                       01S0042 00031000
*       APCWPTR RSTD;                                           01S0042 00032000
*     APCWPTR=ADDR(DAPCW);          /* NEW POINTER FOR APCW          */ 00033000
         L     APCWPTR,@PC00001                                 01S0043 00034000
*     RFY                                                       01S0044 00035000
*       APCW BASED(APCWPTR);                                    01S0044 00036000
*     RFY                                                       01S0045 00037000
*       APEM BASED(ADDR(APCWEMSG)); /* NEW POINTER FOR APEM          */ 00038000
*     RFY                                                       01S0046 00039000
*       APCT BASED(APCTPTR);        /* NEW POINTER FOR APCT          */ 00040000
*     RFY                                                       01S0047 00041000
*       APIN BASED(APCWAPIN);       /* NEW POINTER FOR APIN          */ 00042000
*     RFY                                                       01S0048 00043000
*       APUT BASED(APCWAPUT);       /* NEW POINTER FOR APUT          */ 00044000
*     RFY                                                       01S0049 00045000
*       APAT BASED(APUTAPAT);       /* NEW POINTER FOR APAT          */ 00046000
*                                                               01S0049 00047000
*     /***************************************************************/ 00048000
*     /*                                                             */ 00049000
*     /* CONSTRUCT A NEW ACB WITH CALL EXTENSION AND EXIT LIST. THE  */ 00050000
*     /* THREE BLOCKS ARE IN CONTIGUOUS STORAGE, AND ONE GETMAIN IS  */ 00051000
*     /* USED. IF THE USE OF CONTIGUOUS STORAGE IS CEASED, THEN AN   */ 00052000
*     /* APPROPRIATE CHANGE SHOULD BE MADE TO THE FREE ACB ROUTINE   */ 00053000
*     /* (IGTMJG) ALSO.                                              */ 00054000
*     /*                                                             */ 00055000
*     /***************************************************************/ 00056000
*                                                               01S0050 00057000
*     RFY                                                       01S0050 00058000
*       R0F RSTD;                   /* PREPARE FOR GETMAIN INPUT     */ 00059000
*     R0F=(LENGTH(IFGACB)+7)/8*8    /* ALL THREE BLOCKS TO BE FORMED */ 00060000
*         +(APCXLN+7)/8*8           /* AND ENOUGH SPACE OBTAINED     */ 00061000
*         +(40+7)/8*8;              /* FOR EACH TO HAVE DWORD BDY.   */ 00062000
         LA    R0F,160                                          01S0051 00063000
*     GENERATE(GETMAIN R,LV=(0))                                01S0052 00064000
*     REFS(R0F);                    /* OBTAIN REQUIRED SPACE         */ 00065000
         GETMAIN R,LV=(0)                                               00066000
*     RFY                                                       01S0053 00067000
*       R1P RSTD;                   /* SECURE ADDR OF NEW ACB BLOCK  */ 00068000
*     RFY                                                       01S0054 00069000
*       R0F UNRSTD;                 /* LET REG 0 GO BACK NOW         */ 00070000
*     APCWACB=R1P;                  /* SET NEW ACB ADDRESS IN APCW   */ 00071000
         ST    R1P,APCWACB(,APCWPTR)                            01S0055 00072000
*     APATACB(APINUNIT)=R1P;        /* SET NEW ACB ADDRESS IN APAT   */ 00073000
         L     @09,APCWAPIN(,APCWPTR)                           01S0056 00074000
         L     @09,APINAUNT(,@09)                               01S0056 00075000
         L     @15,APINUNIT(,@09)                               01S0056 00076000
         SLA   @15,2                                            01S0056 00077000
         L     @14,APCWAPUT(,APCWPTR)                           01S0056 00078000
         L     @14,APUTAPAT(,@14)                               01S0056 00079000
         ST    R1P,APATACB-4(@15,@14)                           01S0056 00080000
*     RFY                                                       01S0057 00081000
*       IFGACB BASED(APCWACB);      /* NEW ACB BASED ON POINTER      */ 00082000
*     RFY                                                       01S0058 00083000
*       R1P UNRSTD;                                             01S0058 00084000
*                                                               01S0058 00085000
*     /***************************************************************/ 00086000
*     /*                                                             */ 00087000
*     /* INITIALIZE THE ACB                                          */ 00088000
*     /*                                                             */ 00089000
*     /***************************************************************/ 00090000
*                                                               01S0059 00091000
*     IFGACB=MJBACB;                /* MOVE IN INITIALIZATION VALUES */ 00092000
         L     @15,APCWACB(,APCWPTR)                            01S0059 00093000
         MVC   IFGACB(76,@15),MJBACB                            01S0059 00094000
*     ACBDORGA=TRUE;                /* SET ACB INDICATOR BIT ON      */ 00095000
         OI    ACBDORGA(@15),B'00001000'                        01S0060 00096000
*     ACBAMAX=APCWACB+(LENGTH(IFGACB)+7)/8*8;/* APCX IS AFTER ACB    */ 00097000
         LA    @14,80                                           01S0061 00098000
         ALR   @14,@15                                          01S0061 00099000
         ST    @14,ACBAMAX(,@15)                                01S0061 00100000
*     ACBEXLST=ACBAMAX+(APCXLN+7)/8*8;/* EXLST IS AT END OF APCX     */ 00101000
         AL    @14,@CF00362                                     01S0062 00102000
         ST    @14,ACBEXLST(,@15)                               01S0062 00103000
*     ACBMSGLN=APEMLN;              /* MSG LENGTH DEFINED IN APEM    */ 00104000
         MVC   ACBMSGLN(2,@15),@CH00468                         01S0063 00105000
*     ACBLENG=(LENGTH(IFGACB)+7)/8*8/* ACBLENG INCLUDES LENGTHS OF   */ 00106000
*         +(APCXLN+7)/8*8;          /* ACB AND ALSO APCX             */ 00107000
         MVC   ACBLENG(2,@15),@CH00452                          01S0064 00108000
*     CALL APCVZMMA(APINUNIT,ACBDDNM);/* SET (FTNNF001) DDNAME IN ACB*/ 00109000
*                                                               01S0065 00110000
         ST    @09,@AL00001                                     01S0065 00111000
         LA    @09,ACBDDNM(,@15)                                01S0065 00112000
         ST    @09,@AL00001+4                                   01S0065 00113000
         L     @15,APCTZMMA(,APCTPTR)                           01S0065 00114000
         LA    @01,@AL00001                                     01S0065 00115000
         BALR  @14,@15                                          01S0065 00116000
*     /***************************************************************/ 00117000
*     /*                                                             */ 00118000
*     /* INITIALIZE THE APCX                                         */ 00119000
*     /*                                                             */ 00120000
*     /***************************************************************/ 00121000
*                                                               01S0066 00122000
*     RFY                                                       01S0066 00123000
*       APCX BASED(ACBAMAX);        /* SPECIFY POINTER FOR APCX      */ 00124000
*     APCX=MJBAPCX;                 /* MOVE IN INITIALIZATION VALUES */ 00125000
         L     @09,APCWACB(,APCWPTR)                            01S0067 00126000
         L     @15,ACBAMAX(,@09)                                01S0067 00127000
         MVI   APCX+36(@15),C' '                                01S0067 00128000
         MVC   APCX+37(3,@15),APCX+36(@15)                      01S0067 00129000
         MVC   APCX(36,@15),MJBAPCX                             01S0067 00130000
*     APCXAPUT=APCWAPUT;            /* SAVE APUT PTR FOR FUTURE USE  */ 00131000
*                                                               01S0068 00132000
         L     @14,APCWAPUT(,APCWPTR)                           01S0068 00133000
         ST    @14,APCXAPUT(,@15)                               01S0068 00134000
*     /***************************************************************/ 00135000
*     /*                                                             */ 00136000
*     /* INITIALIZE THE EXIT LIST                                    */ 00137000
*     /*                                                             */ 00138000
*     /***************************************************************/ 00139000
*                                                               01S0069 00140000
*     RFY                                                       01S0069 00141000
*       IFGEXLST BASED(ACBEXLST);   /* SPECIFY POINTER FOR EXLST     */ 00142000
*     IFGEXLST=MJBEXLST;            /* MOVE IN INITIALIZATION VALUES */ 00143000
*                                                               01S0070 00144000
         L     @09,ACBEXLST(,@09)                               01S0070 00145000
         MVC   IFGEXLST(40,@09),MJBEXLST                        01S0070 00146000
*     /***************************************************************/ 00147000
*     /*                                                             */ 00148000
*     /* NOW EXAMINE THE USER'S INPUT PARAMETERS                     */ 00149000
*     /*                                                             */ 00150000
*     /***************************************************************/ 00151000
*                                                               01S0071 00152000
*     CALL APCVZMJH(DAPCW);         /* MJH PROCESSES PARAM. VALUES   */ 00153000
         L     @09,@PC00001                                     01S0071 00154000
         ST    @09,@AL00001                                     01S0071 00155000
         L     @15,APCTZMJH(,APCTPTR)                           01S0071 00156000
         LA    @01,@AL00001                                     01S0071 00157000
         BALR  @14,@15                                          01S0071 00158000
*     RETURN;                       /* MJH SETS NECESSARY ERROR CODES*/ 00159000
@EL00001 L     @13,4(,@13)                                      01S0072 00160000
@EF00001 L     @00,@SIZDATD                                     01S0072 00161000
         LR    @01,@06                                          01S0072 00162000
         IGTYFSV  R,LV=(0),A=(1)                                        00163000
@ER00001 LM    @14,@12,12(@13)                                  01S0072 00164000
         BR    @14                                              01S0072 00165000
*   END;                            /* END OF APCVZMJB PROCESSING    */ 00166000
*                                   /* IGTXEPLG                      */ 00167000
*                                                               01S0074 00168000
*   /*****************************************************************/ 00169000
