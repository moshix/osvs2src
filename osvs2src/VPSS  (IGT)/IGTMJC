*     RFY                                                       01S0062 00050000
*       IFGACB BASED(APCWACB);      /* TAKE HOLD OF THE ACB          */ 00051000
*     RFY                                                       01S0063 00052000
*       APCX BASED(ACBAMAX);        /* TAKE HOLD OF THE APCX         */ 00053000
*     APCWZAM=BITZERO;              /* INITIALLY, NO ERROR PREVAILS  */ 00054000
         NI    APCWZAM(APCWPTR),B'11111110'                     01S0064 00055000
*     DEVWORD0='00000000'X;         /* INIT. 1ST WORD OF OUTPUT AREA */ 00056000
         MVC   DEVWORD0(4),@CB01799                             01S0065 00057000
*     DEVWORD1='00000000'X;         /* INIT. 2ND WORD OF OUTPUT AREA */ 00058000
         MVC   DEVWORD1(4),@CB01799                             01S0066 00059000
*     RFY                                                       01S0067 00060000
*       R2P RSTD;                   /* SECURE REG 2 FOR DEVTYPE CALL */ 00061000
*     R2P=ADDR(ACBDDNM);            /* OBTAIN ADDRESS OF DESIRED DDNM*/ 00062000
         L     R2P,APCWACB(,APCWPTR)                            01S0068 00063000
         LA    R2P,ACBDDNM(,R2P)                                01S0068 00064000
*     GENERATE(DEVTYPE (2),DEVAREA)                             01S0069 00065000
*     SETS(R15P);                   /* CALL ON DEVTYPE               */ 00066000
