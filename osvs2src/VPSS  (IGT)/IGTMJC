         LA    @03,RQBLK                                        02S0078 00102000
         ST    @03,S99RBPTR(,@09)                               02S0078 00103000
*         S99RBPND=BITONE;          /* SET BIT FOR LAST PTR TO RB'S  */ 00104000
         OI    S99RBPND(@09),B'10000000'                        02S0079 00105000
