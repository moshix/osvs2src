         LA    @09,TUPTRS                                       02S0086 00127000
         ST    @09,S99TXTPP(,@03)                               02S0086 00128000
*         S99RSV01=ZERO;            /* PROVIDE A FULLWORD OF ZEROES  */ 00129000
         ST    @14,S99RSV01(,@03)                               02S0087 00130000
*         S99FLAG2=RQBFLAG2;        /* FLAGS2 DEFINED IN RQBFLAG2       00131000
*                                      PREPARE FOR BUILDING POINTER     00132000
*                                      LIST AND TEXT UNITS.          */ 00133000
*                                                               02S0088 00134000
         MVC   S99FLAG2(4,@03),RQBFLAG2                         02S0088 00135000
*         /***********************************************************/ 00136000
*         /*                                                         */ 00137000
*         /* 1ST TEXT UNIT IS FOR DDNAME                             */ 00138000
