*             /*******************************************************/ 00303000
*             /*                                                     */ 00304000
*             /* ERROR CONDITION DETECTED BY DYNALLOC                */ 00305000
*             /*                                                     */ 00306000
*             /*******************************************************/ 00307000
*                                                               02S0139 00308000
*             DYNAR15C=R15F;        /* SAVE RETURN CODE FOR IGTMJI   */ 00309000
         ST    R15F,DYNAR15C                                    02S0139 00310000
*             RFY                                               02S0140 00311000
*               R15F UNRSTD;                                    02S0140 00312000
*             APCWZAM=BITONE;       /* INDICATE ERROR                */ 00313000
         OI    APCWZAM(APCWPTR),B'00000001'                     02S0141 00314000
