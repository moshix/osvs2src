*                 /*                                                 */ 00371000
*                 /* ACB WAS NOT OPENED                              */ 00372000
*                 /*                                                 */ 00373000
*                 /***************************************************/ 00374000
*                                                               03S0155 00375000
*                 APCWZAM=BITONE;   /* INDICATE ERROR                */ 00376000
         OI    APCWZAM(APCWPTR),B'00000001'                     03S0155 00377000
*                 MACRONAM='OPEN';  /* IDENTIFY OPEN AS MACRO RETURN */ 00378000
         MVC   MACRONAM(4),@CC01829                             03S0156 00379000
*                 FBYTE0=BITZERO;   /* LEAD BYTE IS ZERO             */ 00380000
