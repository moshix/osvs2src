*         /*                                                         */ 00113000
*         /***********************************************************/ 00114000
*                                                               02S0081 00115000
*         S99RBLN=20;               /* REQ.BLOCK LENGTH IS 20 BYTES  */ 00116000
         MVI   S99RBLN(@03),X'14'                               02S0081 00117000
*         S99VERB=S99VRBAL;         /* VERB CODE IS FOR DSNAME ALLOC.*/ 00118000
         MVI   S99VERB(@03),X'01'                               02S0082 00119000
*         S99FLAG1=RQBFLAG1;        /* FLAGS1 DEFINED IN RQBFLAG1    */ 00120000
         MVC   S99FLAG1(2,@03),RQBFLAG1                         02S0083 00121000
*         S99ERROR=ZERO;            /* ERROR REASON INITIALLY ZERO   */ 00122000
         STH   @14,S99ERROR(,@03)                               02S0084 00123000
