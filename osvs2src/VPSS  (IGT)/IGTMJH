*                       EXLSYNS=BITONE;/* INDICATE INPUT VALUE GIVEN */ 00382000
*                       EXLSYNA=BITONE;/* INDICATE EXIT ENTRY ACTIVE */ 00383000
*                       EXLSYNK=BITZERO;/* INDICATE ENTRY NAME EMPTY */ 00384000
         OI    EXLSYNS(@09),B'11000000'                         02S0153 00385000
