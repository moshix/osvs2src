TMJH     TITLE 'IGTMJH --- PROCESS USER INPUTS FOR GENACB AND MODACB   *00001000
                        '                                               00002000
IGTMJH   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTMJH    77.231'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         MVC   @PC00001(4),0(@01)                               01S0001 00022000
*/*                                                                  */ 00023000
*/* END OF APSV                                                      */ 00024000
*/*                                                                  */ 00025000
*                                                               01S0040 00026000
*                                                               01S0040 00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* PROCESS USER'S ACB INPUT PARAMETERS                           */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                               01S0040 00033000
*   DO;                                                         01S0040 00034000
*     RFY                                                       01S0041 00035000
*       APCWPTR RSTD;                                           01S0041 00036000
*     APCWPTR=ADDR(DAPCW);          /* NEW POINTER FOR APCW          */ 00037000
         L     APCWPTR,@PC00001                                 01S0042 00038000
*     RFY                                                       01S0043 00039000
*       APCW BASED(APCWPTR);                                    01S0043 00040000
*     RFY                                                       01S0044 00041000
*       APCT BASED(APCTPTR);        /* GRAB HOLD OF APCT             */ 00042000
*     RFY                                                       01S0045 00043000
*       IFGACB BASED(APCWACB);      /* GRAB HOLD OF ACB              */ 00044000
*     RFY                                                       01S0046 00045000
*       APCX BASED(ACBAMAX);        /* GRAB HOLD OF APCX             */ 00046000
*     RFY                                                       01S0047 00047000
*       IFGEXLST BASED(ACBEXLST);   /* GRAB HOLD OF EXLST            */ 00048000
*     RFY                                                       01S0048 00049000
*       APEM BASED(ADDR(APCWEMSG)); /* GRAB HOLD OF APEM             */ 00050000
*     PARAMPTR=APCWAPIN;            /* INITIALIZE MOVING APIN PTR    */ 00051000
         L     PARAMPTR,APCWAPIN(,APCWPTR)                      01S0049 00052000
*     RFY                                                       01S0050 00053000
*       APIN BASED(PARAMPTR);       /* GRAB HOLD OF APIN             */ 00054000
*     IF APINAP3E=BITZERO THEN      /* SEE IF IERROR IS LAST PARAM.  */ 00055000
         TM    APINAP3E(PARAMPTR),B'10000000'                   01S0051 00056000
         BNZ   @RF00051                                         01S0051 00057000
*       DO;                         /* IF NOT, LOOK FOR MORE PARAM.S */ 00058000
*         ENDATKYW=FALSE;           /* INDICATES LIST END AT KEYWORD */ 00059000
         NI    ENDATKYW,B'01111111'                             01S0053 00060000
*         ENDATKYV=FALSE;           /* INDICATES LIST END AT KEYVALUE*/ 00061000
         NI    ENDATKYV,B'01111111'                             01S0054 00062000
*         DO WHILE(ENDATKYW=FALSE&ENDATKYV=FALSE);              01S0055 00063000
*                                                               01S0055 00064000
         B     @DE00055                                         01S0055 00065000
@DL00055 DS    0H                                               01S0056 00066000
*           /*********************************************************/ 00067000
*           /*                                                       */ 00068000
*           /* EXAMINE PAIRS OF PARAMETERS, UNTIL LIST END IS        */ 00069000
*           /* ENCOUNTERED                                           */ 00070000
*           /*                                                       */ 00071000
*           /*********************************************************/ 00072000
*                                                               01S0056 00073000
*           KYWMATCH=FALSE;         /* INITIALIZE MATCH INDICATOR    */ 00074000
         NI    KYWMATCH,B'01111111'                             01S0056 00075000
*           DO INDEX=1 TO KYWCOUNT BY 1 WHILE(KYWMATCH=FALSE);/* SCAN   00076000
*                                      ALL POSSIBLE KEYWORDS FOR A      00077000
*                                      KEYWORD MATCH                 */ 00078000
         LA    INDEX,1                                          01S0057 00079000
@DL00057 TM    KYWMATCH,B'10000000'                             01S0057 00080000
         BNZ   @DC00057                                         01S0057 00081000
*             IF APINKYWD=KYWDCHAR(INDEX) THEN                  01S0058 00082000
         L     @09,APINAKYW(,PARAMPTR)                          01S0058 00083000
         LR    @05,INDEX                                        01S0058 00084000
         SLA   @05,3                                            01S0058 00085000
         LA    @01,KYWDCHAR-8(@05)                              01S0058 00086000
         CLC   APINKYWD(4,@09),0(@01)                           01S0058 00087000
         BNE   @RF00058                                         01S0058 00088000
*               DO;                 /* A KEYWORD MATCH IS OBTAINED   */ 00089000
*                 KYWMATCH=TRUE;    /* SET MATCH INDICATOR           */ 00090000
         OI    KYWMATCH,B'10000000'                             01S0060 00091000
*                 KYWINDEX=INDEX;   /* PRESERVE INDEX FOR CASES BELOW*/ 00092000
         LR    KYWINDEX,INDEX                                   01S0061 00093000
*               END;                                            01S0062 00094000
*             ELSE                                              01S0063 00095000
*               ;                                               01S0063 00096000
@RF00058 DS    0H                                               01S0064 00097000
*           END;                                                01S0064 00098000
         AL    INDEX,@CF00040                                   01S0064 00099000
         C     INDEX,@CF00057                                   01S0064 00100000
         BNH   @DL00057                                         01S0064 00101000
@DC00057 DS    0H                                               01S0065 00102000
*           IF KYWMATCH=TRUE THEN                               01S0065 00103000
         TM    KYWMATCH,B'10000000'                             01S0065 00104000
         BNO   @RF00065                                         01S0065 00105000
*             DO;                                               01S0066 00106000
*                                                               01S0066 00107000
*               /*****************************************************/ 00108000
*               /*                                                   */ 00109000
*               /* A KEYWORD MATCH HAVING BEEN OBTAINED, PLACE INPUT */ 00110000
*               /* VALUE (OR NULL VALUE IF NONE WERE INPUT) INTO     */ 00111000
*               /* APPROPRIATE CONTROL BLOCK.                        */ 00112000
*               /*                                                   */ 00113000
*               /*****************************************************/ 00114000
*                                                               01S0067 00115000
*               IF WSIGNBIT=BITONE THEN                         01S0067 00116000
         TM    WSIGNBIT(PARAMPTR),B'10000000'                   01S0067 00117000
         BNO   @RF00067                                         01S0067 00118000
*                 ENDATKYW=TRUE;    /* KEYWORD IS FINAL PARAMETER,      00119000
*                                      AND NULL VALUE IS USED.       */ 00120000
         OI    ENDATKYW,B'10000000'                             01S0068 00121000
*               ELSE                                            01S0069 00122000
*                 IF VSIGNBIT=BITONE THEN                       01S0069 00123000
         B     @RC00067                                         01S0069 00124000
@RF00067 TM    VSIGNBIT(PARAMPTR),B'10000000'                   01S0069 00125000
         BNO   @RF00069                                         01S0069 00126000
*                   ENDATKYV=TRUE;  /* VALUE IS FINAL PARAMETER      */ 00127000
         OI    ENDATKYV,B'10000000'                             01S0070 00128000
*                 ELSE                                          01S0071 00129000
*                   ;               /* PARAMETER STRING CONTINUES    */ 00130000
@RF00069 DS    0H                                               01S0072 00131000
*               ERRORNUM=ZERO;      /* INTERNAL ERROR INDICATOR      */ 00132000
@RC00067 SLR   ERRORNUM,ERRORNUM                                02S0072 00133000
*KEYWORD:                                                       02S0073 00134000
*               DO;                 /* KEYWORD:CASE(KYWINDEX)NAMES(RE   00135000
*                                      G,STRN,OWN,LERR,PERR,INTV,EMSG   00136000
*                                      ,RBA,RCDE,BSAD,NULL)          */ 00137000
KEYWORD  DS    0H                                               02S0074 00138000
*                 DCL                                           02S0074 00139000
*                   CL0001@ LABEL BASED(CP0001@(KYWINDEX)) VALUERANGE(  00140000
*                       REG,STRN,OWN,LERR,PERR,INTV,EMSG,RBA,RCDE,BSAD, 00141000
*                       NULL);      /* BRANCH TARGET                 */ 00142000
*                 DCL                                           02S0075 00143000
*                   CP0001@(11) PTR(31) INIT(ADDR(REG),ADDR(STRN),ADDR( 00144000
*                       OWN),ADDR(LERR),ADDR(PERR),ADDR(INTV),ADDR(EMSG 00145000
*                       ),ADDR(RBA),ADDR(RCDE),ADDR(BSAD),ADDR(NULL));  00146000
*                 GOTO CL0001@;     /* BRANCH TO SELECTED CASE NTH      00147000
*                                      NAME SHOULD = KYWDCHAR(N)     */ 00148000
         LR    @09,KYWINDEX                                     02S0076 00149000
         SLA   @09,2                                            02S0076 00150000
         L     @09,CP0001@-4(@09)                               02S0076 00151000
         BR    @09                                              02S0076 00152000
*REG:                                                           02S0077 00153000
*                 DO;               /* SUBCASE                       */ 00154000
REG      DS    0H                                               02S0078 00155000
*                   IF ACBOPEN=TRUE THEN/* NO REG MODIFY ON OPEN ACB */ 00156000
         L     @09,APCWACB(,APCWPTR)                            02S0078 00157000
         TM    ACBOPEN(@09),B'00010000'                         02S0078 00158000
         BNO   @RF00078                                         02S0078 00159000
*                     DO;                                       02S0079 00160000
*                       APEMMSGN=APCMG007;/* SPECIFY MESSAGE NO.        00161000
*                                      INDEX                         */ 00162000
         MVC   APEMMSGN+112(4,APCWPTR),@CF00105                 02S0080 00163000
*                       APCWRCN=APCCG007;/* PROVIDE ERROR RETURN        00164000
*                                      NUMBER                        */ 00165000
         MVI   APCWRCN(APCWPTR),X'07'                           02S0081 00166000
*                       ERRORNUM=1; /* TO DENOTE ERROR ON REG PARM.  */ 00167000
         LA    ERRORNUM,1                                       02S0082 00168000
*                     END;                                      02S0083 00169000
*                   ELSE                                        02S0084 00170000
*                     IF ENDATKYW=TRUE THEN/* RESET PARAMETER TO NULL   00171000
*                                      VALUE                         */ 00172000
         B     @RC00078                                         02S0084 00173000
@RF00078 TM    ENDATKYW,B'10000000'                             02S0084 00174000
         BNO   @RF00084                                         02S0084 00175000
*                       APCXREG=ZERO;                           02S0085 00176000
         L     @09,APCWACB(,APCWPTR)                            02S0085 00177000
         L     @09,ACBAMAX(,@09)                                02S0085 00178000
         SLR   @05,@05                                          02S0085 00179000
         ST    @05,APCXREG(,@09)                                02S0085 00180000
*                     ELSE                                      02S0086 00181000
*                       APCXREG=APINKYVF;/* USE INPUT VALUE          */ 00182000
         B     @RC00084                                         02S0086 00183000
@RF00084 L     @09,APCWACB(,APCWPTR)                            02S0086 00184000
         L     @09,ACBAMAX(,@09)                                02S0086 00185000
         L     @05,APINAKYV(,PARAMPTR)                          02S0086 00186000
         L     @05,APINKYVF(,@05)                               02S0086 00187000
         ST    @05,APCXREG(,@09)                                02S0086 00188000
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */ 00189000
         B     CE0001@                                          02S0087 00190000
*                 END REG;          /* END OF SUBCASE                */ 00191000
*STRN:                                                          02S0089 00192000
*                 DO;               /* SUBCASE                       */ 00193000
STRN     DS    0H                                               02S0090 00194000
*                   IF ACBOPEN=TRUE THEN/* NO STRN MODIFY ON AN OPEN    00195000
*                                      ACB                           */ 00196000
         L     @09,APCWACB(,APCWPTR)                            02S0090 00197000
         TM    ACBOPEN(@09),B'00010000'                         02S0090 00198000
         BNO   @RF00090                                         02S0090 00199000
*                     DO;                                       02S0091 00200000
*                       APEMMSGN=APCMG008;/* SPECIFY MESSAGE NO.        00201000
*                                      INDEX                         */ 00202000
         MVC   APEMMSGN+112(4,APCWPTR),@CF00105                 02S0092 00203000
*                       APCWRCN=APCCG008;/* PROVIDE ERROR RETURN        00204000
*                                      NUMBER                        */ 00205000
         MVI   APCWRCN(APCWPTR),X'08'                           02S0093 00206000
*                       ERRORNUM=2; /* TO DENOTE ERROR ON STRN PARM. */ 00207000
         LA    ERRORNUM,2                                       02S0094 00208000
*                     END;                                      02S0095 00209000
*                   ELSE                                        02S0096 00210000
*                     IF ENDATKYW=TRUE THEN/* RESET PARAMETER TO NULL   00211000
*                                      VALUE                         */ 00212000
         B     @RC00090                                         02S0096 00213000
@RF00090 TM    ENDATKYW,B'10000000'                             02S0096 00214000
         BNO   @RF00096                                         02S0096 00215000
*                       ACBSTRNO=APCTSTDF;                      02S0097 00216000
         L     @09,APCWACB(,APCWPTR)                            02S0097 00217000
         L     @05,APCTSTDF(,APCTPTR)                           02S0097 00218000
         STC   @05,ACBSTRNO(,@09)                               02S0097 00219000
*                     ELSE                                      02S0098 00220000
*                       IF(APINKYVF<1|APINKYVF>255) THEN        02S0098 00221000
         B     @RC00096                                         02S0098 00222000
@RF00096 L     @09,APINAKYV(,PARAMPTR)                          02S0098 00223000
         L     @09,APINKYVF(,@09)                               02S0098 00224000
         LTR   @09,@09                                          02S0098 00225000
         BNP   @RT00098                                         02S0098 00226000
         C     @09,@CF01344                                     02S0098 00227000
         BNH   @RF00098                                         02S0098 00228000
@RT00098 DS    0H                                               02S0099 00229000
*                         DO;       /* STRN VALUE IS OUT OF RANGE    */ 00230000
*                           APEMMSGN=APCMG003;/* SPECIFY ERROR  02S0100 00231000
*                                      MESSAGE INDEX                 */ 00232000
         MVC   APEMMSGN+112(4,APCWPTR),@CF00092                 02S0100 00233000
*                           APCWRCN=APCCG003;/* SPECIFY ERROR RETURN    00234000
*                                      CODE                          */ 00235000
         MVI   APCWRCN(APCWPTR),X'03'                           02S0101 00236000
*                           ERRORNUM=2;/* TO DENOTE ERROR ON STRN       00237000
*                                      PARM.                         */ 00238000
         LA    ERRORNUM,2                                       02S0102 00239000
*                         END;                                  02S0103 00240000
*                       ELSE                                    02S0104 00241000
*                         ACBSTRNO=APINKYVF;/* USE INPUT VALUE       */ 00242000
         B     @RC00098                                         02S0104 00243000
@RF00098 L     @09,APCWACB(,APCWPTR)                            02S0104 00244000
         L     @05,APINAKYV(,PARAMPTR)                          02S0104 00245000
         L     @05,APINKYVF(,@05)                               02S0104 00246000
         STC   @05,ACBSTRNO(,@09)                               02S0104 00247000
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */ 00248000
         B     CE0001@                                          02S0105 00249000
*                 END STRN;         /* END OF SUBCASE                */ 00250000
*OWN:                                                           02S0107 00251000
*                 DO;               /* SUBCASE                       */ 00252000
OWN      DS    0H                                               02S0108 00253000
*                   IF ACBOPEN=TRUE THEN/* NO OWN MODIFY ON AN OPEN     00254000
*                                      ACB                           */ 00255000
         L     @09,APCWACB(,APCWPTR)                            02S0108 00256000
         TM    ACBOPEN(@09),B'00010000'                         02S0108 00257000
         BNO   @RF00108                                         02S0108 00258000
*                     DO;                                       02S0109 00259000
*                       APEMMSGN=APCMG002;/* SPECIFY MESSAGE NO.        00260000
*                                      INDEX                         */ 00261000
         MVC   APEMMSGN+112(4,APCWPTR),@CF00105                 02S0110 00262000
*                       APCWRCN=APCCG002;/* PROVIDE ERROR RETURN        00263000
*                                      NUMBER                        */ 00264000
         MVI   APCWRCN(APCWPTR),X'02'                           02S0111 00265000
*                       ERRORNUM=3; /* TO DENOTE ERROR ON OWN PARM.  */ 00266000
         LA    ERRORNUM,3                                       02S0112 00267000
*                     END;                                      02S0113 00268000
*                   ELSE                                        02S0114 00269000
*                     IF ENDATKYW=TRUE THEN/* RESET PARAMETER TO NULL   00270000
*                                      VALUE                         */ 00271000
         B     @RC00108                                         02S0114 00272000
@RF00108 TM    ENDATKYW,B'10000000'                             02S0114 00273000
         BNO   @RF00114                                         02S0114 00274000
*                       APCXOWN=BITZERO;/* '0'B REPRESENTS OWN='SHR' */ 00275000
         L     @09,APCWACB(,APCWPTR)                            02S0115 00276000
         L     @09,ACBAMAX(,@09)                                02S0115 00277000
         NI    APCXOWN(@09),B'01111111'                         02S0115 00278000
*                     ELSE                                      02S0116 00279000
*                       IF APINKYVL='SHR ' THEN/* SET PARAMETER TO      00280000
*                                      INPUT VALUE                   */ 00281000
         B     @RC00114                                         02S0116 00282000
@RF00114 L     @09,APINAKYV(,PARAMPTR)                          02S0116 00283000
         CLC   APINKYVL(4,@09),@CC01600                         02S0116 00284000
         BNE   @RF00116                                         02S0116 00285000
*                         APCXOWN=BITZERO;                      02S0117 00286000
         L     @09,APCWACB(,APCWPTR)                            02S0117 00287000
         L     @09,ACBAMAX(,@09)                                02S0117 00288000
         NI    APCXOWN(@09),B'01111111'                         02S0117 00289000
*                       ELSE                                    02S0118 00290000
*                         IF APINKYVL='EXC ' THEN/* ALSO SET TO INPUT   00291000
*                                      VALUE                         */ 00292000
         B     @RC00116                                         02S0118 00293000
@RF00116 L     @09,APINAKYV(,PARAMPTR)                          02S0118 00294000
         CLC   APINKYVL(4,@09),@CC01601                         02S0118 00295000
         BNE   @RF00118                                         02S0118 00296000
*                           APCXOWN=BITONE;                     02S0119 00297000
         L     @09,APCWACB(,APCWPTR)                            02S0119 00298000
         L     @09,ACBAMAX(,@09)                                02S0119 00299000
         OI    APCXOWN(@09),B'10000000'                         02S0119 00300000
*                         ELSE                                  02S0120 00301000
*                           DO;     /* OWN PARAMETER IS INVALID      */ 00302000
         B     @RC00118                                         02S0120 00303000
@RF00118 DS    0H                                               02S0121 00304000
*                             APEMMSGN=APCMG004;/* SPECIFY ERROR        00305000
*                                      MESSAGE INDEX                 */ 00306000
         MVC   APEMMSGN+112(4,APCWPTR),@CF00057                 02S0121 00307000
*                             APCWRCN=APCCG004;/* SPECIFY ERROR 02S0122 00308000
*                                      RETURN CODE                   */ 00309000
         MVI   APCWRCN(APCWPTR),X'04'                           02S0122 00310000
*                             ERRORNUM=3;/* DENOTE ERROR AS FOR OWN     00311000
*                                      PARM.                         */ 00312000
         LA    ERRORNUM,3                                       02S0123 00313000
*                           END;                                02S0124 00314000
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */ 00315000
         B     CE0001@                                          02S0125 00316000
*                 END OWN;          /* END OF SUBCASE                */ 00317000
*LERR:                                                          02S0127 00318000
*                 DO;               /* SUBCASE                       */ 00319000
LERR     DS    0H                                               02S0128 00320000
*                   IF ENDATKYW=TRUE THEN                       02S0128 00321000
         TM    ENDATKYW,B'10000000'                             02S0128 00322000
         BNO   @RF00128                                         02S0128 00323000
*                     DO;           /* RESET PARAMETER TO NULL       */ 00324000
*                       EXLLERF=BITZERO;                        02S0130 00325000
         L     @09,APCWACB(,APCWPTR)                            02S0130 00326000
         L     @09,ACBEXLST(,@09)                               02S0130 00327000
         MVI   EXLLERF(@09),B'00000000'                         02S0130 00328000
*                       EXLLERP=ZERO;                           02S0131 00329000
         SLR   @05,@05                                          02S0131 00330000
         STCM  @05,15,EXLLERP(@09)                              02S0131 00331000
*                     END;                                      02S0132 00332000
*                   ELSE                                        02S0133 00333000
*                     DO;                                       02S0133 00334000
         B     @RC00128                                         02S0133 00335000
@RF00128 DS    0H                                               02S0134 00336000
*                       EXLLERP=APINKYVP;/* SET PARAMETER TO INPUT      00337000
*                                      VALUE                         */ 00338000
         L     @09,APCWACB(,APCWPTR)                            02S0134 00339000
         L     @09,ACBEXLST(,@09)                               02S0134 00340000
         L     @05,APINAKYV(,PARAMPTR)                          02S0134 00341000
         MVC   EXLLERP(4,@09),APINKYVP(@05)                     02S0134 00342000
*                       EXLLERS=BITONE;/* INDICATE INPUT VALUE GIVEN */ 00343000
*                       EXLLERA=BITONE;/* INDICATE EXIT ENTRY ACTIVE */ 00344000
*                       EXLLERK=BITZERO;/* INDICATE ENTRY NAME EMPTY */ 00345000
         OI    EXLLERS(@09),B'11000000'                         02S0137 00346000
         NI    EXLLERK(@09),B'11011111'                         02S0137 00347000
*                       RFY                                     02S0138 00348000
*                         PARMSIGN BASED(ADDR(EXLLERP));        02S0138 00349000
*                       PSIGNBIT=BITZERO;/* ENSURE THAT SIGNBIT IS      00350000
*                                      OFF                           */ 00351000
         NI    PSIGNBIT+16(@09),B'01111111'                     02S0139 00352000
*                     END;                                      02S0140 00353000
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */ 00354000
         B     CE0001@                                          02S0141 00355000
*                 END LERR;         /* END OF SUBCASE                */ 00356000
*PERR:                                                          02S0143 00357000
*                 DO;               /* SUBCASE                       */ 00358000
PERR     DS    0H                                               02S0144 00359000
*                   IF ENDATKYW=TRUE THEN                       02S0144 00360000
