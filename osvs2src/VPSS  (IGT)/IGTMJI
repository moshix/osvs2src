         BNE   @RF00093                                         01S0093 00328000
*               DO;                                             01S0094 00329000
*                 APCWRCN=APCCS101; /* SELECT RETURN CODE FOR USER   */ 00330000
         MVI   APCWRCN(APCWPTR),X'65'                           01S0095 00331000
*                 APCWRCT=APCCTLOG; /* DESIGNATE AS LOGIC ERROR      */ 00332000
         MVI   APCWRCT(APCWPTR),X'08'                           01S0096 00333000
*                 APEMMSGN=APCMS101;/* SELECT MESSAGE INDEX          */ 00334000
         MVC   APEMMSGN+112(4,APCWPTR),@CF00601                 01S0097 00335000
*               END;                                            01S0098 00336000
*             ELSE                  /* RTRN CODE TO IGTMJC MUST BE 12*/ 00337000
*               DO;                                             01S0099 00338000
         B     @RC00093                                         01S0099 00339000
@RF00093 DS    0H                                               01S0100 00340000
*                 APCWRCN=APCCS102; /* SELECT RETURN CODE FOR USER   */ 00341000
         MVI   APCWRCN(APCWPTR),X'66'                           01S0100 00342000
*                 APCWRCT=APCCTLOG; /* DESIGNATE AS LOGIC ERROR      */ 00343000
         MVI   APCWRCT(APCWPTR),X'08'                           01S0101 00344000
*                 APEMMSGN=APCMS102;/* SELECT MESSAGE INDEX          */ 00345000
         MVC   APEMMSGN+112(4,APCWPTR),@CF00168                 01S0102 00346000
*               END;                                            01S0103 00347000
*           RFY                                                 01S0104 00348000
*             R3F RSTD;             /* BITWHACKING WILL OCCUR IN R3F */ 00349000
@RC00093 DS    0H                                               01S0104 00350000
@RC00087 DS    0H                                               01S0105 00351000
*           IF DYNAR15C^=8 THEN     /* IF ERROR WAS NOT INSTALLATION */ 00352000
         L     @09,@PC00001+4                                   01S0105 00353000
         CLC   DYNAR15C(4,@09),@CF00052                         01S0105 00354000
         BE    @RF00105                                         01S0105 00355000
*             DO INDEX=1 TO 4;      /* THEN PASS ON ERROR & INFO CODE*/ 00356000
         LA    INDEX,1                                          01S0106 00357000
@DL00106 DS    0H                                               01S0107 00358000
*               R3F=ZERO;           /* CLEAR REG BEFORE INSERT CHAR  */ 00359000
         SLR   R3F,R3F                                          01S0107 00360000
*               R3F=CODES(INDEX:INDEX);/* SELECT NEXT CODES CHAR.    */ 00361000
         L     @09,@PC00001+4                                   01S0108 00362000
         SLR   R3F,R3F                                          01S0108 00363000
         IC    R3F,CODES-1(INDEX,@09)                           01S0108 00364000
*               SRA(R3F,4);         /* SHIFT OUT 4 LOW ORDER BITS    */ 00365000
         SRA   R3F,4                                            01S0109 00366000
*               IF R3F>9 THEN       /* IF 4 HIGH ORDER BITS ARE > 9  */ 00367000
         LA    @09,9                                            01S0110 00368000
         CR    R3F,@09                                          01S0110 00369000
         BNH   @RF00110                                         01S0110 00370000
*                 R3F=(R3F-9)|'000000C0'X;/* MAKE DIGIT PRINTABLE    */ 00371000
         LCR   @09,@09                                          01S0111 00372000
         ALR   @09,R3F                                          01S0111 00373000
         O     @09,@CF01268                                     01S0111 00374000
         LR    R3F,@09                                          01S0111 00375000
*               ELSE                /* ELSE BITS IN DECIMAL RANGE 0-9*/ 00376000
*                 R3F=R3F|'000000F0'X;/* MAKE DEC DIGIT PRINTABLE    */ 00377000
         B     @RC00110                                         01S0112 00378000
@RF00110 O     R3F,@CF01269                                     01S0112 00379000
*               APEMVALU(2*INDEX-1:2*INDEX-1)=R3F;/* READY TO PRINT  */ 00380000
@RC00110 LR    @09,INDEX                                        01S0113 00381000
         ALR   @09,@09                                          01S0113 00382000
         STC   R3F,APEMVALU+110(@09,APCWPTR)                    01S0113 00383000
*               R3F=CODES(INDEX:INDEX);/* LOAD IN THE CHAR AGAIN     */ 00384000
         L     @09,@PC00001+4                                   01S0114 00385000
         SLR   R3F,R3F                                          01S0114 00386000
         IC    R3F,CODES-1(INDEX,@09)                           01S0114 00387000
*               R3F=R3F&'0000000F'X;/* AND OUT ALL THE HIGH BITS     */ 00388000
         N     R3F,@CF01260                                     01S0115 00389000
*               IF R3F>9 THEN       /* IF 4 HIGH ORDER BITS ARE > 9  */ 00390000
         LA    @09,9                                            01S0116 00391000
         CR    R3F,@09                                          01S0116 00392000
         BNH   @RF00116                                         01S0116 00393000
