         BNE   @RF00087                                         01S0087 00314000
*             DO;                                               01S0088 00315000
*               APCWRCN=APCCS100;   /* SELECT RETURN CODE FOR USER   */ 00316000
         MVI   APCWRCN(APCWPTR),X'64'                           01S0089 00317000
*               APCWRCT=APCCTLOG;   /* DESIGNATE AS LOGIC ERROR      */ 00318000
