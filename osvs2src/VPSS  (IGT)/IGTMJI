*         CP0001@(3) PTR(31) INIT(ADDR(OPEN),ADDR(CLSE),ADDR(DYNA));    00085000
*       GOTO CL0001@;               /* BRANCH TO SELECTED CASE BRANCH   00086000
*                                      TO APPROPRIATE TABLE          */ 00087000
         LR    @09,CASEVRBL                                     01S0036 00088000
         SLA   @09,2                                            01S0036 00089000
         L     @09,CP0001@-4(@09)                               01S0036 00090000
         BR    @09                                              01S0036 00091000
*OPEN:                                                          01S0037 00092000
*       DO;                         /* SUBCASE FOLLOWING CASE IS FOR    00093000
*                                      OPEN                          */ 00094000
*                                                               01S0037 00095000
OPEN     DS    0H                                               01S0038 00096000
*         /***********************************************************/ 00097000
*         /*                                                         */ 00098000
*         /* SEARCH OPEN TABLE AND SET VALUES.                       */ 00099000
*         /*                                                         */ 00100000
*         /***********************************************************/ 00101000
*                                                               01S0038 00102000
*         DO;                                                   01S0038 00103000
*           DO INDEX=1 TO OPENTBSZ BY 1 WHILE(ANSRINDX=ZERO);   01S0039 00104000
*                                                               01S0039 00105000
         LA    INDEX,1                                          01S0039 00106000
@DL00039 LTR   ANSRINDX,ANSRINDX                                01S0039 00107000
         BNZ   @DC00039                                         01S0039 00108000
*             /*******************************************************/ 00109000
*             /*                                                     */ 00110000
*             /* COMMENCE SEQUENTIAL SEARCH                          */ 00111000
*             /*                                                     */ 00112000
*             /*******************************************************/ 00113000
*                                                               01S0040 00114000
*             IF FBYTE1=OPENCODE(INDEX) THEN/* IF MATCH FOUND        */ 00115000
