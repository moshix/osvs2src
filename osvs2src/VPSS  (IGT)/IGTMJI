*               FULLWORD=FULLWORD|'0000000F'X;/* TURN SIGN TO EBCD   */ 00176000
         OC    FULLWORD(4),@CB01260                             01S0053 00177000
*               UNPK(APEMVALP,FULLWORD);/* UNPACK DEC CODE TO EBCDIC */ 00178000
         UNPK  APEMVALP(4,@03),FULLWORD(4)                      01S0054 00179000
*               APEMVALN=-1;        /* SIGNAL AN ERRONEOUS ERROR CODE*/ 00180000
         MVC   APEMVALN(4,@03),@CF01262                         01S0055 00181000
*             END;                                              01S0056 00182000
*           ELSE                    /* OTHERWISE, TABLE MATCH FOUND  */ 00183000
*             APEMVALN=OPENVALN(ANSRINDX);/* RETURN KEYWORD INDEX    */ 00184000
         B     @RC00050                                         01S0057 00185000
@RF00050 LR    @09,ANSRINDX                                     01S0057 00186000
         MH    @09,@CH00140                                     01S0057 00187000
         SLR   @03,@03                                          01S0057 00188000
         IC    @03,OPENVALN-6(@09)                              01S0057 00189000
         ST    @03,APEMVALN+112(,APCWPTR)                       01S0057 00190000
*         END;                                                  01S0058 00191000
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */ 00192000
         B     CE0001@                                          01S0059 00193000
*       END OPEN;                   /* END OF SUBCASE                */ 00194000
*CLSE:                                                          01S0061 00195000
