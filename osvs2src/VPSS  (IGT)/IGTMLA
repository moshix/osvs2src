TMLA     TITLE 'IGTMLA --- PROCESS INITIAL BUILD REQUEST               *00001000
                        '                                               00002000
IGTMLA   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTMLA    77.093'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @07,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@07                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         IGTYGSV  R,LV=(0)                                              00015000
         LR    @06,@01                                          01S0001 00016000
         USING @DATD,@06                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @06,8(,@13)                                      01S0001 00020000
         LR    @13,@06                                          01S0001 00021000
         MVC   @PC00001(4),0(@01)                               01S0001 00022000
*/*                                                                  */ 00023000
*/* END OF APSV                                                      */ 00024000
*/*                                                                  */ 00025000
*                                                               01S0038 00026000
*                                                               01S0038 00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* SET POINTERS FOR CONTROL BLOCKS NEEDED                        */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                               01S0038 00033000
*   DO;                                                         01S0038 00034000
*     RFY                                                       01S0039 00035000
*       APCWPTR RSTD;               /* NEW POINTER FOR APCW          */ 00036000
*     APCWPTR=ADDR(DAPCW);                                      01S0040 00037000
         L     APCWPTR,@PC00001                                 01S0040 00038000
*     RFY                                                       01S0041 00039000
*       APCW BASED(APCWPTR);                                    01S0041 00040000
*     RFY                                                       01S0042 00041000
*       APEM BASED(ADDR(APCWEMSG)); /* GET APEM                      */ 00042000
*     RFY                                                       01S0043 00043000
*       APIN BASED(APCWAPIN);       /* GET APIN                      */ 00044000
*     RFY                                                       01S0044 00045000
*       IFGACB BASED(APCWACB);      /* GET ACB                       */ 00046000
*     RFY                                                       01S0045 00047000
*       APAX BASED(ACBAMWAP);       /* GET APAX                      */ 00048000
*     RFY                                                       01S0046 00049000
*       APLB BASED(APAXAPLB);       /* GET APLB                      */ 00050000
*   END;                                                        01S0047 00051000
*                                                               01S0047 00052000
*   /*****************************************************************/ 00053000
*   /*                                                               */ 00054000
*   /* ZERO CCW AND CIT HEADER WORK AREAS                            */ 00055000
*   /*                                                               */ 00056000
*   /*****************************************************************/ 00057000
*                                                               01S0048 00058000
*   DO;                                                         01S0048 00059000
*     CCWENTRY=CCWENTRY&&CCWENTRY;  /* ZERO CCW WORK AREA            */ 00060000
         XC    CCWENTRY(8),CCWENTRY                             01S0049 00061000
*     CITHDR=CITHDR&&CITHDR;        /* ZERO CIT HEADER               */ 00062000
         XC    CITHDR(4),CITHDR                                 01S0050 00063000
*   END;                                                        01S0051 00064000
*                                                               01S0051 00065000
*   /*****************************************************************/ 00066000
*   /*                                                               */ 00067000
*   /* INITIALIZE CIT AND CCW POINTERS - THE USER CCW TABLE START    */ 00068000
*   /* ADDRESS(ACCW) IS SAVED IN APAX. THE ADDRESS IS THEN CHECKED TO*/ 00069000
*   /* ASSURE THAT IT IS ON A DOUBLEWORD BOUNDARY. IF NOT, AN ERROR  */ 00070000
*   /* MESSAGE IS SET UP (NOT DWORD BDY - ACCW) AND A RETURN IS MADE */ 00071000
*   /* TO THE CALLING PROGRAM. OTHERWISE, THE CCW NEXT ENTRY ADDRESS */ 00072000
*   /* IS SET TO THE START ADDRESS. THE CCW END ADDRESS IS THEN      */ 00073000
*   /* COMPUTED USING THE CCW TABLE LENGTH (ICCW). NEXT, THE CIT     */ 00074000
*   /* START, NEXT AND END ADDRESSES ARE SIMILARLY SAVED IN APAX. THE*/ 00075000
*   /* ADDRESS OF THE LAST CIT ENTRY AND NUMBER OF WORDS IN THE      */ 00076000
*   /* CURRENT CCW DATA CHAIN ARE INITIALIZED TO ZERO.               */ 00077000
*   /*                                                               */ 00078000
*   /*****************************************************************/ 00079000
*                                                               01S0052 00080000
*   DO;                                                         01S0052 00081000
*     APAXCCWS=APINAACW;            /* SET CCW START ADDRESS         */ 00082000
         L     @09,APCWACB(,APCWPTR)                            01S0053 00083000
         L     @09,ACBAMWAP(,@09)                               01S0053 00084000
         L     @01,APCWAPIN(,APCWPTR)                           01S0053 00085000
         L     @15,APINAACW(,@01)                               01S0053 00086000
         ST    @15,APAXCCWS(,@09)                               01S0053 00087000
*     IF(APAXCCWS&EVENBITS)^=0 THEN /* TEST IF CCW TABLE IS ON A        00088000
*                                      DWORD BOUNDARY                */ 00089000
         N     @15,@CF00065                                     01S0054 00090000
         LTR   @15,@15                                          01S0054 00091000
         BZ    @RF00054                                         01S0054 00092000
*       DO;                         /* NO - SET UP ERROR INDICATORS  */ 00093000
*         APCWR15C=APCCRERR;        /* PROVIDE R15 RETURN CODE       */ 00094000
         MVC   APCWR15C(4,APCWPTR),@CF00040                     01S0056 00095000
*         APCWRCT=APCCTLOG;         /* ERROR TYPE - LOGICAL          */ 00096000
         MVI   APCWRCT(APCWPTR),X'08'                           01S0057 00097000
*         APCWRCC=APCCB;            /* COMPONENT ID                  */ 00098000
         MVI   APCWRCC(APCWPTR),X'04'                           01S0058 00099000
*         APCWRCN=APCCB003;         /* ERROR NUMBER                  */ 00100000
         MVI   APCWRCN(APCWPTR),X'03'                           01S0059 00101000
*         APCWZAM=ON;               /* SET FLAG TO INDICATE ERROR       00102000
*                                      DETECTED                      */ 00103000
         OI    APCWZAM(APCWPTR),B'00000001'                     01S0060 00104000
*         APEMMSGN=APCMB003;        /* MESSAGE NUMBER FOR NOT DWORD     00105000
*                                      BDY                           */ 00106000
         LA    @09,112                                          01S0061 00107000
         ALR   @09,APCWPTR                                      01S0061 00108000
         MVC   APEMMSGN(4,@09),@CF00583                         01S0061 00109000
*         APEMVALN=APCMK043;        /* KEYWORD NUMBER FOR ACCW       */ 00110000
         MVC   APEMVALN(4,@09),@CF00338                         01S0062 00111000
*         RETURN;                   /* RETURN TO CALLER              */ 00112000
@EL00001 L     @13,4(,@13)                                      01S0063 00113000
@EF00001 L     @00,@SIZDATD                                     01S0063 00114000
         LR    @01,@06                                          01S0063 00115000
         IGTYFSV  R,LV=(0),A=(1)                                        00116000
@ER00001 LM    @14,@12,12(@13)                                  01S0063 00117000
         BR    @14                                              01S0063 00118000
*       END;                                                    01S0064 00119000
*     ELSE                                                      01S0065 00120000
*       ;                           /* NO ERROR                      */ 00121000
@RF00054 DS    0H                                               01S0066 00122000
*     APAXCCWN=APAXCCWS;            /* SET CCW NEXT ENTRY ADDR       */ 00123000
         L     @09,APCWACB(,APCWPTR)                            01S0066 00124000
         L     @09,ACBAMWAP(,@09)                               01S0066 00125000
         L     @15,APAXCCWS(,@09)                               01S0066 00126000
         ST    @15,APAXCCWN(,@09)                               01S0066 00127000
*     APAXCCWE=APAXCCWS+(4*APINICW)-1;/* SET CCW END ADDRESS         */ 00128000
         L     @14,APCWAPIN(,APCWPTR)                           01S0067 00129000
         L     @01,APINAICW(,@14)                               01S0067 00130000
         L     @11,APINICW(,@01)                                01S0067 00131000
         SLA   @11,2                                            01S0067 00132000
         ALR   @15,@11                                          01S0067 00133000
         BCTR  @15,0                                            01S0067 00134000
         ST    @15,APAXCCWE(,@09)                               01S0067 00135000
*     APAXCIS=APINAACI;             /* SET CIT START ADDRESS         */ 00136000
         L     @15,APINAACI(,@14)                               01S0068 00137000
         ST    @15,APAXCIS(,@09)                                01S0068 00138000
*     APAXCIN=APAXCIS;              /* SET CIT NEXT ENTRY ADDR       */ 00139000
         ST    @15,APAXCIN(,@09)                                01S0069 00140000
*     APAXCIE=APAXCIS+(4*APINICI)-1;/* SET CIT END ADDRESS           */ 00141000
         L     @01,APINAICI(,@14)                               01S0070 00142000
         L     @14,APINICI(,@01)                                01S0070 00143000
         SLA   @14,2                                            01S0070 00144000
         ALR   @15,@14                                          01S0070 00145000
         BCTR  @15,0                                            01S0070 00146000
         ST    @15,APAXCIE(,@09)                                01S0070 00147000
*     APAXLCIT=APAXLCIT&&APAXLCIT;  /* SET ADDRESS OF LAST CIT  01S0071 00148000
*                                      INITIALLY TO ZERO             */ 00149000
         L     @15,APAXLCIT(,@09)                               01S0071 00150000
         LR    @14,@15                                          01S0071 00151000
         XR    @14,@15                                          01S0071 00152000
         ST    @14,APAXLCIT(,@09)                               01S0071 00153000
*     APAXIOCT=APAXIOCT&&APAXIOCT;  /* INITIALIZE NUMBER OF WORDS IN    00154000
*                                      CURRENT CCW CHAIN TO ZERO     */ 00155000
         L     @15,APAXIOCT(,@09)                               01S0072 00156000
         LR    @14,@15                                          01S0072 00157000
         XR    @14,@15                                          01S0072 00158000
         ST    @14,APAXIOCT(,@09)                               01S0072 00159000
*   END;                                                        01S0073 00160000
*                                                               01S0073 00161000
*   /*****************************************************************/ 00162000
*   /*                                                               */ 00163000
*   /* CONSTRUCT PROCESS CCW IN WORK AREA - THE PROCESS CCW ALERTS   */ 00164000
*   /* THE 3838 TO GET READY. MISSING FIELDS IN THE CCW WILL BE      */ 00165000
*   /* FILLED IN AT EXC OR EXCW TIME. IT IS CONSTRUCTED HERE SINCE   */ 00166000
*   /* SPACE MUST BE AVAILABLE AT THE HEAD OF THE CCW CHAIN.         */ 00167000
*   /*                                                               */ 00168000
*   /*****************************************************************/ 00169000
*                                                               01S0074 00170000
*   DO;                                                         01S0074 00171000
*     CMDCODE=PROCCCW;              /* SET CCW CODE TO PROCESS       */ 00172000
         MVI   CMDCODE,X'0F'                                    01S0075 00173000
*     CCFLAG=ON;                    /* SET COMMAND CHAINING          */ 00174000
         OI    CCFLAG,B'01000000'                               01S0076 00175000
*     BYTCOUNT=8;                   /* SET BYTE TRANSFER COUNT       */ 00176000
         MVC   BYTCOUNT(2),@CH00040                             01S0077 00177000
*   END;                                                        01S0078 00178000
*                                                               01S0078 00179000
*   /*****************************************************************/ 00180000
*   /*                                                               */ 00181000
*   /* MOVE PROCESS CCW TO CCW TABLE                                 */ 00182000
*   /*                                                               */ 00183000
*   /*****************************************************************/ 00184000
*                                                               01S0079 00185000
*   DO;                                                         01S0079 00186000
*     CALL APCVZMIB(CCWENTRY,ERRORBIT);/* CALL MOVE CCW              */ 00187000
         LA    @09,CCWENTRY                                     01S0080 00188000
         ST    @09,@AL00001                                     01S0080 00189000
         LA    @09,ERRORBIT                                     01S0080 00190000
         ST    @09,@AL00001+4                                   01S0080 00191000
         L     @15,APCTZMIB(,APCTPTR)                           01S0080 00192000
         LA    @01,@AL00001                                     01S0080 00193000
         BALR  @14,@15                                          01S0080 00194000
*     IF ERRORBIT=ON THEN           /* CCW TABLE FULL?               */ 00195000
         TM    ERRORBIT,B'10000000'                             01S0081 00196000
         BO    @RT00081                                         01S0081 00197000
*       RETURN;                     /* YES - RETURN                  */ 00198000
*     ELSE                                                      01S0083 00199000
*       ;                                                       01S0083 00200000
*     CCWENTRY=CCWENTRY&&CCWENTRY;  /* ZERO WORK AREA                */ 00201000
         XC    CCWENTRY(8),CCWENTRY                             01S0084 00202000
*   END;                                                        01S0085 00203000
*                                                               01S0085 00204000
*   /*****************************************************************/ 00205000
*   /*                                                               */ 00206000
*   /* CONSTRUCT WRITE CCW IN WORK AREA - THIS CCW WRITES THE CIT TO */ 00207000
*   /* THE 3838. THE LENGTH FIELD WILL BE FILLED IN WITH THE ACTUAL  */ 00208000
*   /* LENGTH OF THE CIT AT THE TIME OF THE XLTE CALL.               */ 00209000
*   /*                                                               */ 00210000
*   /*****************************************************************/ 00211000
*                                                               01S0086 00212000
*   DO;                                                         01S0086 00213000
*     CMDCODE=WRITECCW;             /* WRITE CCW                     */ 00214000
         MVI   CMDCODE,X'01'                                    01S0087 00215000
*     DATADDR=APAXCIS;              /* START ADDR OF CIT             */ 00216000
         L     @09,APCWACB(,APCWPTR)                            01S0088 00217000
         L     @09,ACBAMWAP(,@09)                               01S0088 00218000
         MVC   DATADDR(3),APAXCIS+1(@09)                        01S0088 00219000
*     CCFLAG=ON;                    /* SPECIFY COMMAND CHAINING      */ 00220000
         OI    CCFLAG,B'01000000'                               01S0089 00221000
*   END;                                                        01S0090 00222000
*                                                               01S0090 00223000
*   /*****************************************************************/ 00224000
*   /*                                                               */ 00225000
*   /* MOVE CCW ENTRY TO CCW TABLE                                   */ 00226000
*   /*                                                               */ 00227000
*   /*****************************************************************/ 00228000
*                                                               01S0091 00229000
*   CALL APCVZMIB(CCWENTRY,ERRORBIT);/* CALL MOVE CCW                */ 00230000
         LA    @09,CCWENTRY                                     01S0091 00231000
         ST    @09,@AL00001                                     01S0091 00232000
         LA    @09,ERRORBIT                                     01S0091 00233000
         ST    @09,@AL00001+4                                   01S0091 00234000
         L     @15,APCTZMIB(,APCTPTR)                           01S0091 00235000
         LA    @01,@AL00001                                     01S0091 00236000
         BALR  @14,@15                                          01S0091 00237000
*   IF ERRORBIT=ON THEN             /* CCW TABLE FULL?               */ 00238000
         TM    ERRORBIT,B'10000000'                             01S0092 00239000
         BO    @RT00092                                         01S0092 00240000
*     RETURN;                       /* YES - RETURN                  */ 00241000
*   ELSE                                                        01S0094 00242000
*     ;                                                         01S0094 00243000
*                                                               01S0094 00244000
*   /*****************************************************************/ 00245000
*   /*                                                               */ 00246000
*   /* MOVE CIT HEADER TO CIT                                        */ 00247000
*   /*                                                               */ 00248000
*   /*****************************************************************/ 00249000
*                                                               01S0095 00250000
*   DO;                                                         01S0095 00251000
*     CITLNGTH=1;                   /* HEADER LENGTH IN WORDS        */ 00252000
         MVC   CITLNGTH(4),@CF00047                             01S0096 00253000
*     CALL APCVZMIC(CITHDR,CITLNGTH,ERRORBIT);/* CALL MOVE CIT       */ 00254000
         LA    @09,CITHDR                                       01S0097 00255000
         ST    @09,@AL00001                                     01S0097 00256000
         LA    @09,CITLNGTH                                     01S0097 00257000
         ST    @09,@AL00001+4                                   01S0097 00258000
         LA    @09,ERRORBIT                                     01S0097 00259000
         ST    @09,@AL00001+8                                   01S0097 00260000
         L     @15,APCTZMIC(,APCTPTR)                           01S0097 00261000
         LA    @01,@AL00001                                     01S0097 00262000
         BALR  @14,@15                                          01S0097 00263000
*     IF ERRORBIT=ON THEN           /* CIT FULL?                     */ 00264000
         TM    ERRORBIT,B'10000000'                             01S0098 00265000
         BO    @RT00098                                         01S0098 00266000
*       RETURN;                     /* YES - RETURN                  */ 00267000
*     ELSE                                                      01S0100 00268000
*       ;                                                       01S0100 00269000
*   END;                                                        01S0101 00270000
*                                                               01S0101 00271000
*   /*****************************************************************/ 00272000
*   /*                                                               */ 00273000
*   /* INITIALIZE THE LABEL TABLE TO ALL ZEROES.                     */ 00274000
*   /*                                                               */ 00275000
*   /*****************************************************************/ 00276000
*                                                               01S0102 00277000
*   DO I=APLBNUMB TO 1 BY-1;        /* APLBNUMB CONTAINS THE NUMBER     00278000
*                                      OF ENTRIES IN APLB            */ 00279000
         L     @09,APCWACB(,APCWPTR)                            01S0102 00280000
         L     @09,ACBAMWAP(,@09)                               01S0102 00281000
         L     @09,APAXAPLB(,@09)                               01S0102 00282000
         L     I,APLBNUMB(,@09)                                 01S0102 00283000
         B     @DE00102                                         01S0102 00284000
@DL00102 DS    0H                                               01S0103 00285000
*     APLBENTY(I)=APLBENTY(I)&&APLBENTY(I);                     01S0103 00286000
         LR    @09,I                                            01S0103 00287000
         SLA   @09,3                                            01S0103 00288000
         L     @03,APCWACB(,APCWPTR)                            01S0103 00289000
         L     @03,ACBAMWAP(,@03)                               01S0103 00290000
         L     @03,APAXAPLB(,@03)                               01S0103 00291000
         ALR   @03,@09                                          01S0103 00292000
         XC    APLBENTY-8(8,@03),APLBENTY-8(@03)                01S0103 00293000
*   END;                                                        01S0104 00294000
*                                                               01S0104 00295000
         BCTR  I,0                                              01S0104 00296000
@DE00102 LTR   I,I                                              01S0104 00297000
         BP    @DL00102                                         01S0104 00298000
*   /*****************************************************************/ 00299000
*   /*                                                               */ 00300000
*   /* SAVE THE ISTATE FOR LATER USE BY APRL                         */ 00301000
*   /*                                                               */ 00302000
*   /*****************************************************************/ 00303000
