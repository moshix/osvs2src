TUAG     TITLE 'IGTUAG -- SUPERVISOR STATE STORAGE MANAGEMENT INTERFACE*00001000
               E        '                                               00002000
IGTUAG   CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IGTUAG    77.093'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @06,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@06                                      01S0001 00013000
*   RAPSVPTR=APSVPTR;               /* KEEP SAVEAREA PTR (R13 USED)  */ 00014000
         LR    RAPSVPTR,APSVPTR                                 01S0025 00015000
*   RAPGFPTR=R1P;                   /* POINT TO INPUT PARMS          */ 00016000
         LR    RAPGFPTR,R1P                                     01S0026 00017000
*   RFY                                                         01S0027 00018000
*    (R1P,                                                      01S0027 00019000
*     APSVPTR,                                                  01S0027 00020000
*     APCTPTR) UNRSTD;              /* FINISHED WITH THESE REGS      */ 00021000
*   RFY                                                         01S0028 00022000
*     APSV BASED(RAPSVPTR);         /* TELL COMPILER OF              */ 00023000
*   RFY                                                         01S0029 00024000
*     APGF BASED(RAPGFPTR);         /* NEW POINTERS                  */ 00025000
*                                                               01S0029 00026000
*   /*****************************************************************/ 00027000
*   /*                                                               */ 00028000
*   /* SAVE INPUT PSW KEY & SWITCH TO KEY ZERO                       */ 00029000
*   /*                                                               */ 00030000
*   /*****************************************************************/ 00031000
*                                                               01S0030 00032000
*   RFY                             /* REQUIRED REGISTERS            */ 00033000
*    (R2F,                          /* SET TO INPUT KEY BY MODESET   */ 00034000
*     RINKEY) RSTD;                 /* HOLD INPUT KEY THROUGHOUT PGM */ 00035000
*   GEN SETS(R2F) NOREFS;                                       01S0031 00036000
*                                   /*                                  00037000
         MODESET EXTKEY=ZERO,       GO TO KEY 0                        *00038000
               SAVEKEY=(2)          SAVE INPUT KEY IN REG 2             00039000
*   RINKEY=R2F;                     /* SAVE INPUT KEY                */ 00040000
*   RFY                                                         01S0033 00041000
*    (R2F) UNRSTD;                  /* FINISHED WITH R2F             */ 00042000
*                                                               01S0033 00043000
*   /*****************************************************************/ 00044000
*   /*                                                               */ 00045000
*   /* OBTAIN THE LOCAL LOCK                                         */ 00046000
*   /*                                                               */ 00047000
*   /*****************************************************************/ 00048000
*                                                               01S0034 00049000
*   DO;                             /* START OF CODE                 */ 00050000
*                                                               02S0034 00051000
*     /***************************************************************/ 00052000
*     /*                                                             */ 00053000
*     /* IF THE LOCAL LOCK IS NOT OWNED ON ENTRY, OBTAIN IT NOW. SET */ 00054000
*     /* APGFSLCK=0 SO THE EXIT PROCEDURE WILL FREE IT.              */ 00055000
*     /*                                                             */ 00056000
*     /***************************************************************/ 00057000
*                                                               02S0035 00058000
*     APGFSLCK=BON;                 /* SHOW LOCAL LOCK OWNED ON ENTRY*/ 00059000
         OI    APGFSLCK(RAPGFPTR),B'00001000'                   02S0035 00060000
*     GEN NOSETS REFS(PSAHLHI) FLOWS(GETLOCK1) SEQFLOW;         02S0036 00061000
*                                   /*                                  00062000
         SETLOCK TEST,TYPE=LOCAL,  SEE IF LOCAL LOCK OWNED ON ENTRY    *00063000
               BRANCH=(HELD,GETLOCK1), * IF OWNED, SKIP OBTAIN AND     *00064000
               RELATED=APGFSLCK;   * SET APGFSLCK=1.                    00065000
*     DO;                           /* GET THE LOCAL LOCK            */ 00066000
*       APGFSLCK=BOFF;              /* FREE LOCAL LOCK ON EXIT       */ 00067000
         NI    APGFSLCK(RAPGFPTR),B'11110111'                   02S0038 00068000
*       GEN REFS(FLC,PSALITA) SETS(R11F,R12F,R13F,R14F);        02S0039 00069000
*                                      /*                               00070000
         SETLOCK OBTAIN,           ACQUIRE LOCAL LOCK                  *00071000
               TYPE=LOCAL,         * SUSPEND UNTIL LOCK                *00072000
               MODE=UNCOND,        * IS OBTAINED                       *00073000
               RELATED=APGFSLCK    * APGFSLCK=0                         00074000
*     END;                          /* LOCAL LOCK OBTAINED           */ 00075000
*GETLOCK1:                          /* LOCAL LOCK OWNED ON ENTRY     */ 00076000
*   END;                            /* END OF CODE -- LOCK OWNED     */ 00077000
GETLOCK1 DS    0H                                               02S0042 00078000
*                                                               01S0042 00079000
*   /*****************************************************************/ 00080000
*   /*                                                               */ 00081000
*   /* INITIALIZE GETMAIN/FREEMAIN REGISTERS                         */ 00082000
*   /*                                                               */ 00083000
*   /*****************************************************************/ 00084000
*                                                               01S0042 00085000
*   DO;                             /* SET GETMAIN/FREEMAIN REGS     */ 00086000
*                                                               02S0042 00087000
*     /***************************************************************/ 00088000
*     /*                                                             */ 00089000
*     /* PLACED REQUIRED GETMAIN/FREEMAIN VALUES IN REGISTERS        */ 00090000
*     /*                                                             */ 00091000
*     /***************************************************************/ 00092000
*                                                               02S0043 00093000
*     RFY                           /* NEEDED REGISTERS              */ 00094000
*      (RTCBPTR,                    /* TCB                           */ 00095000
*       RASCBPTR,                   /* ASCB                          */ 00096000
*       RKEY,                       /* STORAGE KEY                   */ 00097000
*       RSP,                        /* STORAGE SUBPOOL               */ 00098000
*       RLV,                        /* STORAGE LENGTH                */ 00099000
*       RADDR,                      /* PTR TO STORAGE                */ 00100000
*       RCODE) RSTD;                /* HOLD PGM RETURN CODE          */ 00101000
*     RSP=APGFXSP;                  /* STORAGE SUBPOOL               */ 00102000
         SLR   RSP,RSP                                          02S0044 00103000
         IC    RSP,APGFXSP(,RAPGFPTR)                           02S0044 00104000
*     IF APGFXSP=CSP000 THEN        /* IF WANT SUBPOOL 0 (USER AREA) */ 00105000
         CLI   APGFXSP(RAPGFPTR),0                              02S0045 00106000
         BNE   @RF00045                                         02S0045 00107000
*       RSP=CSP250;                 /* MUST USE 250 SINCE IN KEY 0.  */ 00108000
         LA    RSP,250                                          02S0046 00109000
*     ELSE                          /* IF NOT WANTING SUBPOOL 0,     */ 00110000
*       IF APGFXSP=CSP252 THEN      /* IF WANT SUBPOOL 252 (PROTECT) */ 00111000
         B     @RC00045                                         02S0047 00112000
@RF00045 CLI   APGFXSP(RAPGFPTR),252                            02S0047 00113000
         BNE   @RF00047                                         02S0047 00114000
*         RSP=CSP000;               /* MUST USE 0 SINCE IN KEY 0.    */ 00115000
         SLR   RSP,RSP                                          02S0048 00116000
*     RLV=APGFXLN;                  /* STORAGE LENGTH                */ 00117000
@RF00047 DS    0H                                               02S0049 00118000
@RC00045 L     RLV,APGFXLN(,RAPGFPTR)                           02S0049 00119000
*     IF APGFSINL=BON THEN          /* IF INDIRECT LENGTH SPECIFIED, */ 00120000
         TM    APGFSINL(RAPGFPTR),B'01000000'                   02S0050 00121000
         BNO   @RF00050                                         02S0050 00122000
*       RLV=RLV->I031F;             /* LOAD REQUIRED LENGTH          */ 00123000
         L     RLV,I031F(,RLV)                                  02S0051 00124000
*     RLV=RLV&'00FFFFFF'X;          /* CLEAR SUBPOOL (IF ANY)        */ 00125000
@RF00050 N     RLV,@CF01496                                     02S0052 00126000
*     RASCBPTR=PSAAOLD;             /* CURRENT ASCB ADDRESS          */ 00127000
         L     RASCBPTR,PSAAOLD                                 02S0053 00128000
*     RTCBPTR=PSATOLD;              /* CURRENT TCB ADDRESS           */ 00129000
         L     RTCBPTR,PSATOLD                                  02S0054 00130000
*     IF APGFSTCB=BON THEN          /* IF CURRENT TCB NOT TO BE USED */ 00131000
         TM    APGFSTCB(RAPGFPTR),B'00000100'                   02S0055 00132000
         BNO   @RF00055                                         02S0055 00133000
*       RTCBPTR=APGFTCB;            /* USE PARAMETER LIST VALUE.     */ 00134000
         L     RTCBPTR,APGFTCB(,RAPGFPTR)                       02S0056 00135000
*     IF APGFXKEY=255 THEN          /* IF USER KEY REQUESTED, USE    */ 00136000
@RF00055 CLI   APGFXKEY(RAPGFPTR),255                           02S0057 00137000
         BNE   @RF00057                                         02S0057 00138000
*       RKEY=RTCBPTR->TCBPKF;       /* KEY FROM TCB.                 */ 00139000
         SLR   RKEY,RKEY                                        02S0058 00140000
         IC    RKEY,TCBPKF(,RTCBPTR)                            02S0058 00141000
*     ELSE                          /* ELSE PUT GIVEN KEY IN BIT     */ 00142000
*       RKEY=APGFXKEY*16;           /* POSITIONS 24-27.              */ 00143000
         B     @RC00057                                         02S0059 00144000
@RF00057 SLR   RKEY,RKEY                                        02S0059 00145000
         IC    RKEY,APGFXKEY(,RAPGFPTR)                         02S0059 00146000
         SLA   RKEY,4                                           02S0059 00147000
*   END;                            /* END OF CODE                   */ 00148000
@RC00057 DS    0H                                               02S0061 00149000
         EJECT                                                          00150000
*                                                               01S0061 00151000
*   /*****************************************************************/ 00152000
*   /*                                                               */ 00153000
*   /* ISSUE EITHER GETMAIN OR FREEMAIN BRANCH ENTRY                 */ 00154000
*   /*                                                               */ 00155000
*   /*****************************************************************/ 00156000
*                                                               01S0061 00157000
*   IF APGFSFRE=BOFF THEN           /* IF GETMAIN REQUESTED          */ 00158000
         TM    APGFSFRE(RAPGFPTR),B'00000001'                   01S0061 00159000
         BNZ   @RF00061                                         01S0061 00160000
*     DO;                           /* GETMAIN THE STORAGE           */ 00161000
*                                                               02S0062 00162000
*       /*************************************************************/ 00163000
*       /*                                                           */ 00164000
*       /* GETMAIN REQUESTED. ISSUE (UN)CONDITIONAL GETMAIN (BRANCH  */ 00165000
*       /* ENTRY) FOR THE STORAGE. IF NOT OBTAINED, SET RCODE TO SHOW*/ 00166000
*       /* THE FAILURE. OTHERWISE, AFTER SAVING THE ADDRESS OF THE   */ 00167000
