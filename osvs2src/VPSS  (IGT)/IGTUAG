*               RFY                                             02S0095 00313000
*                (R0P,                                          02S0095 00314000
*                 R14P,                                         02S0095 00315000
*                 R1F,                                          02S0095 00316000
*                 R15F) UNRSTD;     /* DONE WITH REGS.               */ 00317000
*             END;                  /* STORAGE INITIALIZED           */ 00318000
*         END;                      /* STORAGE ACQUIRED              */ 00319000
*     END;                          /* GETMAIN PROCESSED             */ 00320000
*   ELSE                            /* ELSE IS FREEMAIN              */ 00321000
         EJECT                                                          00322000
*     DO;                           /* ISSUE FREEMAIN REQUEST        */ 00323000
*                                                               02S0099 00324000
         B     @RC00061                                         02S0099 00325000
@RF00061 DS    0H                                               02S0100 00326000
*       /*************************************************************/ 00327000
*       /*                                                           */ 00328000
*       /* FREEMAIN REQUESTED. ISSUE (UN)CONDITIONAL FREEMAIN (BRANCH*/ 00329000
*       /* ENTRY) FOR THE STORAGE. SET RCODE TO INDICATE IF THE      */ 00330000
*       /* FREEMAIN WORKED (0 OR ^0).                                */ 00331000
*       /*                                                           */ 00332000
*       /*************************************************************/ 00333000
*                                                               02S0100 00334000
*       SPKA(RINKEY);               /* INPUT KEY                     */ 00335000
         SPKA  0(RINKEY)                                        02S0100 00336000
*       IF APGFSINA=BON THEN        /* FIND ADDRESS OF STORAGE       */ 00337000
         TM    APGFSINA(RAPGFPTR),B'10000000'                   02S0101 00338000
         BNO   @RF00101                                         02S0101 00339000
*         DO;                       /* TO FREEMAIN. CONTAINED        */ 00340000
*           RADDR=APGFXADR->I031P;  /* IN EITHER APGFXADR OR         */ 00341000
         L     @03,APGFXADR(,RAPGFPTR)                          02S0103 00342000
