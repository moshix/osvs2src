*           /*********************************************************/ 00284000
*           /*                                                       */ 00285000
*           /* IF REQUESTED, INITIALIZE ACQUIRED STORAGE TO APGFINIT */ 00286000
*           /*                                                       */ 00287000
*           /*********************************************************/ 00288000
*                                                               02S0086 00289000
*           IF APGFSCLR=BON THEN    /* IF CLEARING OF OBTAINED AREA  */ 00290000
         TM    APGFSCLR(RAPGFPTR),B'00100000'                   02S0086 00291000
         BNO   @RF00086                                         02S0086 00292000
*             DO;                   /* WANTED, THEN USE MVCL.        */ 00293000
*               RFY                                             02S0088 00294000
*                (R0P,                                          02S0088 00295000
*                 R14P,                                         02S0088 00296000
*                 R1F,                                          02S0088 00297000
*                 R15F) RSTD;       /* INITIALIZE                    */ 00298000
*               R0P=RADDR;          /* THE OBTAINED                  */ 00299000
