*         IF APGFSUNC=BON THEN      /* IF UNCONDITIONAL GETMAIN      */ 00200000
         TM    APGFSUNC(RAPGFPTR),B'00000010'                   02S0070 00201000
         BNO   @RF00070                                         02S0070 00202000
*           GEN SETS(R0F,R1F,R14F,R15F,R3F,R7P) REFS(CVTPTR,CVTMAP,     00203000
*               CVTCRMN,RKEY,RSP,RLV);                          02S0071 00204000
*                                   /*                                  00205000
         GETMAIN RU,               UNCONDITIONAL GETMAIN               *00206000
