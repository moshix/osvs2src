         BNE   @DL01628                                            1630 04372000
*             VMTNXT=MWAPTRA->VMTNXT;/* DECHAIN VMT                  */ 04373000
         L     @11,VMTNXT(,@11)                                    1631 04374000
         ST    @11,VMTNXT(,MWAPTRB)                                1631 04375000
*             RFY                                                  1632 04376000
*               IDAVMT BASED(MWAPTRA);                             1632 04377000
*             MWAPTRA=VMTNXT;       /* PT TO NEXT VMT                */ 04378000
         ST    @11,MWAPTRA                                         1633 04379000
*           END;                                                   1634 04380000
*         IF OPWFLGS1^=0 THEN       /* IF SYSTEM DATA SET            */ 04381000
@RC01620 CLI   OPWFLGS1(RCORE),0                                   1635 04382000
         BE    @RF01635                                            1635 04383000
*           MWAGFSP=CATPOOL;        /* THEN SET SYSTEM SUBPOOL       */ 04384000
         MVI   MWAGFSP,X'E7'                                       1636 04385000
*         ELSE                      /* NO, NOT CAT           @Z30SSDP*/ 04386000
*           IF ACBGSR=ON|OPWCBIC=ON THEN/* IF GSR OR CONTROL BLKS IN    04387000
*                                      COMMON                @ZA16012*/ 04388000
