@EF00015 DS    0H                                                  1277 03127000
@ER00015 BR    @14                                                 1277 03128000
*PROBDT:                                                           1278 03129000
*                                                                  1278 03130000
*/********************************************************************/ 03131000
*/*                                                                  */ 03132000
*/*   FUNCTION: THIS ROUTINE IS THE INTERFACE BETWEEN THIS           */ 03133000
*/*             MODULE AND IDA0192P.  IT IS CALLED IN THE EVENT      */ 03134000
*/*             AN ERROR OR WARNING CONDITION.                       */ 03135000
*/*                                                                  */ 03136000
*/*   INPUTS:   1. PROBLEM DETERMINATION PROBLEM CODE (PPCODE).      */ 03137000
*/*             2. PROBLEM DETERMINATION FUNCTION CODE (PPFUNC).     */ 03138000
*/*                                                                  */ 03139000
*/*   OUTPUTS:  1. PROBLEM DETERMINATION PARAMETER LIST IS           */ 03140000
*/*                   COMPLETED.                                     */ 03141000
*/*             2. THE ACBERFLG IS DETERMINED AND SET                */ 03142000
*/*             3. THE CALL TO IDA0192P IS MADE                      */ 03143000
*/*             4. RETURN TO CALLER IF WARNING CONDITION, OR ...     */ 03144000
*/*             5. BRANCH TO LABEL EXIT IF ERROR CONDITION           */ 03145000
*/*                                                                  */ 03146000
*/********************************************************************/ 03147000
*                                                                  1278 03148000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1278 03149000
PROBDT   DS    0H                                                  1279 03150000
*   RFY                                                            1279 03151000
*     RET RSTD;                                                    1279 03152000
*   MWASAVEE=RET;                   /* SAVE RETURN ADDRESS           */ 03153000
         ST    RET,MWASAVEE                                        1280 03154000
*   RFY                                                            1281 03155000
*     RET UNRSTD;                                                  1281 03156000
*   RACB=OPWUACB;                   /* PT TO USER ACB                */ 03157000
         L     RACB,OPWUACB(,RCORE)                                1282 03158000
*   ACBERFLG=OPWERMAP(PDPCODE);     /* SET ERROR FLAG IN USER ACB    */ 03159000
         L     @11,OPWCOMWA(,RCORE)                                1283 03160000
         SLR   @07,@07                                             1283 03161000
         IC    @07,PDPCODE+280(,@11)                               1283 03162000
         ALR   @07,RCORE                                           1283 03163000
         MVC   ACBERFLG(1,RACB),OPWERMAP-1(@07)                    1283 03164000
*   RESPECIFY                                                      1284 03165000
*     R1 RESTRICTED;                                               1284 03166000
*   R1=OPWCOMWA;                    /* PT TO FORECORE                */ 03167000
         LR    R1,@11                                              1285 03168000
*   RESPECIFY                                                      1286 03169000
*     R1 UNRESTRICTED;                                             1286 03170000
*   RSTATRTN=IDAR192P;              /* INDIC 192P CONTROL    @Y30DSDP*/ 03171000
         MVC   RSTATRTN(4,@11),@CC05329                            1287 03172000
*   CALL IDA0192P;                  /* CALL PROBLEM DETERMINATION    */ 03173000
         L     @15,@CV05701                                        1288 03174000
         BALR  @14,@15                                             1288 03175000
*   RSTATRTN=IDAR192A;              /* INDIC 192A CONTROL    @Y30DSDP*/ 03176000
         L     @11,OPWCOMWA(,RCORE)                                1289 03177000
         MVC   RSTATRTN(4,@11),@CC05315                            1289 03178000
*   IF ACBERFLG>=OPERR128&          /* IF FATAL ERROR OCCURRED       */ 03179000
*       MWAEXIT=OFF THEN            /* NOT ALREADY EXITING           */ 03180000
         CLI   ACBERFLG(RACB),128                                  1290 03181000
         BL    @RF01290                                            1290 03182000
         TM    MWAEXIT,B'00000010'                                 1290 03183000
         BNZ   @RF01290                                            1290 03184000
*     DO;                           /* THEN TERMINATE OPEN           */ 03185000
*       MWAOFAIL=ON;                /* IND OPEN FAILURE              */ 03186000
         OI    MWAOFAIL,B'00010000'                                1292 03187000
*       RETURN TO TERM192A;         /* EXIT MODULE IMMEDIATELY       */ 03188000
         LA    @14,TERM192A                                        1293 03189000
@EL00016 DS    0H                                                  1293 03190000
@EF00016 DS    0H                                                  1293 03191000
@ER00016 BR    @14                                                 1293 03192000
*     END;                                                         1294 03193000
*   RFY                                                            1295 03194000
*     RET RSTD;                                                    1295 03195000
