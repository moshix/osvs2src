         CH    MWAJ,VMTNOVOL(,@05)                                 1616 04336000
         BNE   @RF01616                                            1616 04337000
*       DO;                         /* THEN DECHAIN AND FREE VMT     */ 04338000
*         MWAGFADR=MWAPTRA;         /* PT TO VMT TO FREE             */ 04339000
         ST    @05,MWAGFADR                                        1618 04340000
*         MWAGFLEN=VMTLEN;          /* INIT LENGTH OF VMT            */ 04341000
