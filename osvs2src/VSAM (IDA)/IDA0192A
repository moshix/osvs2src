         TITLE 'IDA0192A - VS2-3 VSAM OPEN STRING                      *00001000
                        '                                               00002000
IDA0192A CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192A  78.188'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @12,4095(,@03)                                      0001 00012000
         LA    @10,4095(,@12)                                      0001 00013000
         USING @PSTART,@03                                         0001 00014000
         USING @PSTART+4095,@12                                    0001 00015000
         USING @PSTART+8190,@10                                    0001 00016000
JSCBDISP EQU 188                                                        00017000
JSCBAOS1 EQU  72                                                        00018000
JSCBAOS2 EQU 192                                                        00019000
RBPRFXLN EQU 64                                                         00020000
RBPRFLNA EQU 32                                                         00021000
PRBLEN   EQU 136                                                        00022000
SIRBLEN  EQU 200                                                        00023000
TIRBLEN  EQU 136                                                        00024000
IRBLEN   EQU 128                                                        00025000
SVRBLEN  EQU 224                                                        00026000
*   RSTATRTN=IDAR192A;              /* INDIC 192A CONTROL    @Y30DSDP*/ 00027000
         MVC   RSTATRTN(4,RCORE),@CC05315                          0338 00028000
*   CALL INIT192A;                  /* MODULE INITIALIZATION         */ 00029000
         BAL   @14,INIT192A                                        0339 00030000
*   RFY                                                            0340 00031000
*     FORCORE BASED(OPWCOMWA);      /*                       @Z30SSDP*/ 00032000
*   GEN(USING @DATD,R13)                                           0341 00033000
*   REFS(R13) NOSETS;               /* ADDRESSABILITY                */ 00034000
         USING @DATD,R13                                                00035000
*   CALL IDA0192F;                  /* OPEN CLUSTERS IN SPHERE       */ 00036000
         L     @15,@CV05653                                        0342 00037000
         BALR  @14,@15                                             0342 00038000
*   RSTATRTN=IDAR192A;              /* INDIC 192A CONTROL    @Y30DSDP*/ 00039000
         L     @11,OPWCOMWA(,RCORE)                                0343 00040000
         MVC   RSTATRTN(4,@11),@CC05315                            0343 00041000
*   RFY                                                            0344 00042000
*     RETCODE RSTD;                                                0344 00043000
*   IF RETCODE>4 THEN               /* IF OPEN FAILURE               */ 00044000
         C     RETCODE,@CF00096                                    0345 00045000
         BNH   @RF00345                                            0345 00046000
*     DO;                           /* THEN TERMINATE OPEN           */ 00047000
*       RFY                                                        0347 00048000
*         RETCODE UNRSTD;                                          0347 00049000
*       MWAOFAIL=ON;                /* IND OPEN FAILURE              */ 00050000
         OI    MWAOFAIL,B'00010000'                                0348 00051000
*       GOTO TERM192A;              /* TERMINATE OPEN                */ 00052000
         B     TERM192A                                            0349 00053000
*     END;                                                         0350 00054000
*   CALL BLDDDEB;                   /* BUILD DUMMY DEB FOR ACB       */ 00055000
@RF00345 BAL   @14,BLDDDEB                                         0351 00056000
*TERM192A:                          /* TERMINATION PROCESSING        */ 00057000
*/********************************************************************/ 00058000
*/*                                                                  */ 00059000
*/*   FUNCTION: TERMINATE OPEN PROCESSING.  IF MODULE WORK AREA      */ 00060000
*/*             DOES NOT EXIST, THEN RESTORE ACB AND EXIT.  IF OPW   */ 00061000
*/*             EXISTS, THEN (1) IF CATALOG OPEN, CALL IDA0192C FOR  */ 00062000
*/*             DUMMY LOCATE, (2) CALL UPSMF TO WRITE SMF RECORDS IF */ 00063000
*/*             REQUIRED, (3) IF OPEN FAILED, CALL CLNUP, (4) USE    */ 00064000
*/*             SSL TO UPDATE POINTERS, (5) CALL DEQBUSY TO DEQ      */ 00065000
*/*             BUSY ENQ'S, (6) IF CHECKPONT/RESTART, CALL           */ 00066000
*/*             JFCACBMG TO DO JFCB TO ACB MERGE, (7) CALL FREELSTS  */ 00067000
*/*             TO FREE SAVE LISTS, (8) RESET OPEN INDICATORS IN     */ 00068000
*/*             VMT'S, (9) INITIALIZE ADDRESS OF INTERFACE ROUTINE   */ 00069000
*/*             IN USER ACB, (10) CALL IDA0192M TO RELEASE EXCESS    */ 00070000
*/*             SPACE IN STORAGE BLOCKS.  FREE MODULE WORK AREA,     */ 00071000
*/*             RESTORE REGISTERS, AND RETURN TO IFG0192A.           */ 00072000
*/*                                                                  */ 00073000
*/*   INPUTS:   1. IF MWA EXISTS RCORE NOT EQUAL TO ZERO. IF MWA     */ 00074000
*/*                DOES NOT EXIST, RCORE EWUALS ZERO.                */ 00075000
*/*             2. IF OPW EXISTS, OPWUACB POINTS TO USER ACB, ELSE   */ 00076000
*/*                RACB POINTS TO USER ACB.                          */ 00077000
*/*                                                                  */ 00078000
*/*   OUTPUTS:  1. OPEN TERMINATED                                   */ 00079000
*/*                                                                  */ 00080000
*/********************************************************************/ 00081000
*                                                                  0352 00082000
*   /*****************************************************************/ 00083000
*   /*                                                               */ 00084000
*   /* TERMINATE OPEN PROCESSING                                     */ 00085000
*   /*                                                               */ 00086000
*   /*****************************************************************/ 00087000
*                                                                  0352 00088000
*   IF RCORE=0 THEN                 /* IF MODULE WORK AREA DOES NOT     00089000
*                                      EXIST                         */ 00090000
TERM192A LTR   RCORE,RCORE                                         0352 00091000
         BNZ   @RF00352                                            0352 00092000
*     DO;                           /* THEN RESTORE ACB              */ 00093000
*       RFY                                                        0354 00094000
*         R1 RSTD;                                                 0354 00095000
*       RFY                                                        0355 00096000
*         FORCORE BASED(RCORE);                                    0355 00097000
*       RCORE=R1;                   /* RESTORE PTR TO COMMON WA      */ 00098000
         LR    RCORE,R1                                            0356 00099000
*       RFY                                                        0357 00100000
*         R1 UNRSTD;                                               0357 00101000
*       ACBDDNM=PDPTIOT->TIOEDDNM;  /* RESTORE ACB DD NAME           */ 00102000
         L     @11,PDPTIOT+280(,RCORE)                             0358 00103000
         MVC   ACBDDNM(8,RACB),TIOEDDNM(@11)                       0358 00104000
*       ACBEXFG='1'B;               /* RESET LOCK BIT                */ 00105000
*       ACBIOSFG=OFF;               /* RESET BUSY BIT                */ 00106000
         OI    ACBEXFG(RACB),B'00000010'                           0360 00107000
         NI    ACBIOSFG(RACB),B'11111110'                          0360 00108000
*       RFY                                                        0361 00109000
*         FORCORE BASED(OPWCOMWA);                                 0361 00110000
*     END;                                                         0362 00111000
*   ELSE                                                           0363 00112000
*     DO;                           /* ELSE MODULE WORK AREA EXISTS  */ 00113000
         B     @RC00352                                            0363 00114000
@RF00352 DS    0H                                                  0364 00115000
*       IF MWAOPWRK=ON THEN         /* IF OPW EXISTS                 */ 00116000
         TM    MWAOPWRK,B'10000000'                                0364 00117000
         BNO   @RF00364                                            0364 00118000
*         DO;                       /* THEN PERFORM NORMAL         0365 00119000
*                                      TERMINATION                   */ 00120000
*           RACB=OPWUACB;           /* POINT TO USER ACB             */ 00121000
         L     RACB,OPWUACB(,RCORE)                                0366 00122000
*           PDPFUNC=PDF95;          /* SET FUNCTION CODE             */ 00123000
         L     @11,OPWCOMWA(,RCORE)                                0367 00124000
         MVI   PDPFUNC+280(@11),X'5F'                              0367 00125000
*           MWAEXIT=ON;             /* TERMINATION IN CONTROL        */ 00126000
         OI    MWAEXIT,B'00000010'                                 0368 00127000
*           IF MWARSMAC=ON THEN     /* IF ACBMACRF NEEDS TO BE     0369 00128000
*                                      RESTORED                      */ 00129000
         TM    MWARSMAC,B'00100000'                                0369 00130000
         BNO   @RF00369                                            0369 00131000
*             ACBMACRF=MWAMACRF;    /* THEN RESTORE ACBMACRF         */ 00132000
         MVC   ACBMACRF(2,RACB),MWAMACRF                           0370 00133000
*           IF OPWCAT=ON|OPWSCRA=ON|OPWUCRA=ON THEN/* IF CATLG, SCRA    00134000
*                                      OR UCRA                       */ 00135000
@RF00369 TM    OPWCAT(RCORE),B'11000000'                           0371 00136000
         BNZ   @RT00371                                            0371 00137000
         TM    OPWUCRA(RCORE),B'10000000'                          0371 00138000
         BNO   @RF00371                                            0371 00139000
@RT00371 DS    0H                                                  0372 00140000
*             DO;                   /* THEN DO DUMMY LOCATE          */ 00141000
*               IF MWAOFAIL=OFF THEN/* IF NO ERRORS                  */ 00142000
         TM    MWAOFAIL,B'00010000'                                0373 00143000
         BNZ   @RF00373                                            0373 00144000
*                 ACBOPEN=ON;       /* THEN IND CATLG OPENED         */ 00145000
         OI    ACBOPEN(RACB),B'00010000'                           0374 00146000
*               MWACTID=DUM;        /* IND DUMMY LOCATE REQUEST      */ 00147000
@RF00373 MVI   MWACTID,X'1C'                                       0375 00148000
*               MWACTSFI=ADDR(IDASFI);/* PT TO SFI AREA              */ 00149000
         LA    @11,MWASFI                                          0376 00150000
         STCM  @11,7,MWACTSFI                                      0376 00151000
*               IDASFI=IDASFI&&IDASFI;/* ZERO SFI AREA               */ 00152000
         XC    IDASFI(12,@11),IDASFI(@11)                          0377 00153000
*               RFY                                                0378 00154000
*                (R1,                                              0378 00155000
*                 R5) RSTD;                                        0378 00156000
*               ACBINRTN=BIBINTRF;  /* INIT ADDR OF INTERFACE ROUTINE   00157000
*                                      IN ACB                        */ 00158000
         L     @11,OPWBIB(,RCORE)                                  0379 00159000
         L     @11,BIBINTRF(,@11)                                  0379 00160000
         ST    @11,ACBINRTN(,RACB)                                 0379 00161000
*               R5=ADDR(IDAAMB)-ADDR(AMBCACB)+ADDR(OPWUACB);/*     0380 00162000
*                                      SIMULATE AMB FOR DUMMY LOCATE    00163000
*                                      LOAD OF CATLG ACB ADDRESS     */ 00164000
         LA    R5,OPWUACB(,RCORE)                                  0380 00165000
         AL    R5,@CF05876                                         0380 00166000
*               R1=ADDR(MWACATPL);  /* PT TO CATLG INTRF PARM LIST   */ 00167000
         LA    R1,MWACATPL                                         0381 00168000
*               RSTATRTN=IDAR192C;  /* INDIC 192C CONTROL    @Y30DSDP*/ 00169000
         L     @11,OPWCOMWA(,RCORE)                                0382 00170000
