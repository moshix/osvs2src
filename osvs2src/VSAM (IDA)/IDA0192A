*                                                                  0576 00820000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0576 00821000
FREELSTS DS    0H                                                  0577 00822000
*   RFY                                                            0577 00823000
*     RET RSTD;                                                    0577 00824000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 00825000
         ST    RET,MWASAVEA                                        0578 00826000
*   RFY                                                            0579 00827000
*     RET UNRSTD;                                                  0579 00828000
*   MWAI=1;                         /* SET DUMMYPTR INDEX    @Z30SSRL*/ 00829000
         MVC   MWAI(4),@CF00054                                    0580 00830000
*   DO WHILE(BIBCSL^=0);            /* FREE CSL CHAIN        @Z30SSDP*/ 00831000
         B     @DE00581                                            0581 00832000
@DL00581 DS    0H                                                  0582 00833000
*     MWAGFADR=BIBCSL;              /* INIT ADDRESS OF CSL   @Z30SSDP*/ 00834000
         L     @11,OPWBIB(,RCORE)                                  0582 00835000
         L     @05,BIBCSL(,@11)                                    0582 00836000
         ST    @05,MWAGFADR                                        0582 00837000
*     OPWCSL=BIBCSL;                /* UPDATE CSL PTR        @Z30SSDP*/ 00838000
         ST    @05,OPWCSL(,RCORE)                                  0583 00839000
*     MWAGFLEN=SVELENTH;            /* INIT LENGTH OF CSL    @Z30SSDP*/ 00840000
         L     @05,MWAI                                            0584 00841000
         SLA   @05,2                                               0584 00842000
         L     @05,DUMMYPTR-4(@05,RCORE)                           0584 00843000
         MVC   MWAGFLEN(3),SVELENTH(@05)                           0584 00844000
*     MWAGFSP=SVESUBPL;             /* INIT SUBPOOL OF CSL   @Z30SSDP*/ 00845000
         IC    @15,SVESUBPL(,@05)                                  0585 00846000
         STC   @15,MWAGFSP                                         0585 00847000
*     SVEID='00'X;                  /* ZERO EBCDIC ID        @Z30SSDP*/ 00848000
         XC    SVEID+1(7,@05),SVEID+1(@05)                         0586 00849000
         MVI   SVEID(@05),X'00'                                    0586 00850000
*     BIBCSL=SVENXPTR;              /* PT TO NEXT CSL        @Z30SSDP*/ 00851000
         L     @05,SVENXPTR(,@05)                                  0587 00852000
         ST    @05,BIBCSL(,@11)                                    0587 00853000
*     CALL FREECORE;                /* FREE CSL IF IT EXISTS @Z30SSDP*/ 00854000
         BAL   @14,FREECORE                                        0588 00855000
*   END;                            /*                       @Z30SSDP*/ 00856000
@DE00581 SLR   @11,@11                                             0589 00857000
         L     @05,OPWBIB(,RCORE)                                  0589 00858000
         C     @11,BIBCSL(,@05)                                    0589 00859000
         BNE   @DL00581                                            0589 00860000
*   OPWCSL=0;                       /* ZERO CSL PTR          @Z30SSDP*/ 00861000
         ST    @11,OPWCSL(,RCORE)                                  0590 00862000
*   DO MWAI=2 TO LENGTH(OPWSAVE)/FWORDLEN;/* FREE SAVE LISTS @Z30SSDP*/ 00863000
         LA    @11,2                                               0591 00864000
         ST    @11,MWAI                                            0591 00865000
@DL00591 DS    0H                                                  0592 00866000
*     DO WHILE(DUMMYPTR(MWAI)^=0);  /* FREE SAVE LIST CHAIN          */ 00867000
         B     @DE00592                                            0592 00868000
@DL00592 DS    0H                                                  0593 00869000
*       MWAGFADR=DUMMYPTR(MWAI);    /* INIT ADDRESS OF SAVE LIST     */ 00870000
         L     @11,MWAI                                            0593 00871000
         SLA   @11,2                                               0593 00872000
         L     @05,DUMMYPTR-4(@11,RCORE)                           0593 00873000
         ST    @05,MWAGFADR                                        0593 00874000
*       MWAGFLEN=SVELENTH;          /* INIT LENGTH OF SAVE LIST      */ 00875000
         MVC   MWAGFLEN(3),SVELENTH(@05)                           0594 00876000
*       MWAGFSP=SVESUBPL;           /* INIT SUBPOOL OF SAVE LIST     */ 00877000
         IC    @15,SVESUBPL(,@05)                                  0595 00878000
         STC   @15,MWAGFSP                                         0595 00879000
*       SVEID='00'X;                /* ZERO EBCDIC ID                */ 00880000
         XC    SVEID+1(7,@05),SVEID+1(@05)                         0596 00881000
         MVI   SVEID(@05),X'00'                                    0596 00882000
*       DUMMYPTR(MWAI)=SVENXPTR;    /* PT TO NEXT SAVE LIST          */ 00883000
         L     @05,SVENXPTR(,@05)                                  0597 00884000
         ST    @05,DUMMYPTR-4(@11,RCORE)                           0597 00885000
*       CALL FREECORE;              /* FREE SAVE LIST IF IT EXISTS   */ 00886000
         BAL   @14,FREECORE                                        0598 00887000
*     END;                                                         0599 00888000
@DE00592 L     @11,MWAI                                            0599 00889000
         LR    @05,@11                                             0599 00890000
         SLA   @05,2                                               0599 00891000
         L     @05,DUMMYPTR-4(@05,RCORE)                           0599 00892000
         LTR   @05,@05                                             0599 00893000
         BNZ   @DL00592                                            0599 00894000
*   END;                                                           0600 00895000
         AL    @11,@CF00054                                        0600 00896000
         ST    @11,MWAI                                            0600 00897000
         C     @11,@CF00146                                        0600 00898000
         BNH   @DL00591                                            0600 00899000
*   RFY                                                            0601 00900000
*     RET RSTD;                                                    0601 00901000
*   RET=MWASAVEA;                   /* RESTORE RETURN ADDRESS        */ 00902000
         L     RET,MWASAVEA                                        0602 00903000
*   RFY                                                            0603 00904000
*     RET UNRSTD;                                                  0603 00905000
*   END FREELSTS;                                                  0604 00906000
@EL00003 DS    0H                                                  0604 00907000
@EF00003 DS    0H                                                  0604 00908000
@ER00003 BR    @14                                                 0604 00909000
*INIT192A:                                                         0605 00910000
*                                                                  0605 00911000
*/********************************************************************/ 00912000
*/*                                                                  */ 00913000
*/*   FUNCTION: VSAM OPEN INITIALIZATION.  BUILDS MODULE WORK AREA,  */ 00914000
*/*             ACB WORK AREA(OPW), AND SAVE LISTS.  ISSUES ENQ BUSY */ 00915000
*/*             FOR EACH DATA SET BEING PROCESSED.  ESTABLISHES      */ 00916000
*/*             MODULE ADDRESSABILITY.                               */ 00917000
*/*                                                                  */ 00918000
*/*   INPUTS:   SAME AS MODULE                                       */ 00919000
*/*                                                                  */ 00920000
*/*   OUTPUTS:  1. PTR TO MODULE WORK AREA AND SAVE AREA IN REG 13.  */ 00921000
*/*             2. PTR TO ACB WORK AREA IN REGISTER 4.               */ 00922000
*/*             3. PTR'S TO ESL, DSL, CSL AND SSL IN OPW.            */ 00923000
*/*                                                                  */ 00924000
*/********************************************************************/ 00925000
*                                                                  0605 00926000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0605 00927000
INIT192A DS    0H                                                  0606 00928000
*   RFY                                                            0606 00929000
*     FORCORE BASED(RCORE);                                        0606 00930000
*   RFY                                                            0607 00931000
*     TCB BASED(PSATOLD);                                          0607 00932000
*                                                                  0607 00933000
*   /*****************************************************************/ 00934000
*   /*                                                               */ 00935000
*   /* INITIALIZE PROB DET PARAMETER LIST                            */ 00936000
*   /*                                                               */ 00937000
*   /*****************************************************************/ 00938000
*                                                                  0608 00939000
*   GEN(LA RACB,0(RACB));           /* ZERO OPTION BYTE IN ACB ADDR  */ 00940000
         LA RACB,0(RACB)                                                00941000
*   IDAPDPRM=IDAPDPRM&&IDAPDPRM;    /* ZERO PROB DET PARM LIST       */ 00942000
         LA    @11,280                                             0609 00943000
         ALR   @11,RCORE                                           0609 00944000
         XC    IDAPDPRM(40,@11),IDAPDPRM(@11)                      0609 00945000
*   PDPCALL=PDPOPEN;                /* CALLER IS OPEN                */ 00946000
         MVI   PDPCALL(@11),X'00'                                  0610 00947000
*   PDPFUNC=PDF81;                  /* MOD INIT IN CONTROL           */ 00948000
         MVI   PDPFUNC(@11),X'51'                                  0611 00949000
*   PDPACB=RACB;                    /* PT TO USER ACB                */ 00950000
         ST    RACB,PDPACB(,@11)                                   0612 00951000
*   PDPTIOT=DXTIOTAD;               /* SET UP TIOT PTR       @Y30DSDP*/ 00952000
         L     @05,DXTIOTAD(,RCORE)                                0613 00953000
         ST    @05,PDPTIOT(,@11)                                   0613 00954000
*   PDPCLSNM=ADDR(JFCBDSNM);        /* CLN PT'S TO DSN ON DD STMNT   */ 00955000
         LA    @05,100                                             0614 00956000
         ALR   @05,RCORE                                           0614 00957000
         ST    @05,PDPCLSNM(,@11)                                  0614 00958000
*   PDPDSN=0;                       /* SET DATA SET NAME TO ZERO     */ 00959000
         SLR   @05,@05                                             0615 00960000
         ST    @05,PDPDSN(,@11)                                    0615 00961000
*   PDPDSTYP=PDPDDNM;               /* SET DATA SET TYPE TO DD NAME  */ 00962000
         MVI   PDPDSTYP(@11),X'01'                                 0616 00963000
*   IF ACBERFLG^=0&ACBERFLG^=OPERR188 THEN/* IF ERROR FROM COMMON  0617 00964000
*                                      OPEN                          */ 00965000
         CLI   ACBERFLG(RACB),0                                    0617 00966000
         BE    @RF00617                                            0617 00967000
         CLI   ACBERFLG(RACB),188                                  0617 00968000
         BE    @RF00617                                            0617 00969000
*     DO;                                                          0618 00970000
*       RFY                                                        0619 00971000
*         R1 RSTD;                                                 0619 00972000
*       R1=RCORE;                   /* SAVE PTR TO COMMON WORK AREA  */ 00973000
         LR    R1,RCORE                                            0620 00974000
*       RCORE=0;                    /* IND NO MOD WORK AREA OR OPW   */ 00975000
         SLR   RCORE,RCORE                                         0621 00976000
*       RETURN TO TERM192A;         /* RESTORE ACB AND EXIT MODULE   */ 00977000
         LA    @14,TERM192A                                        0622 00978000
@EL00004 DS    0H                                                  0622 00979000
@EF00004 DS    0H                                                  0622 00980000
@ER00004 BR    @14                                                 0622 00981000
*       RFY                                                        0623 00982000
*         R1 UNRSTD;                                               0623 00983000
*     END;                                                         0624 00984000
*   RFY                                                            0625 00985000
*     R6 RSTD;                                                     0625 00986000
@RF00617 DS    0H                                                  0626 00987000
*   RFY                                                            0626 00988000
*     RET RSTD;                                                    0626 00989000
*   R6=RET;                         /* SAVE RETURN ADDRESS           */ 00990000
         LR    R6,RET                                              0627 00991000
*   RFY                                                            0628 00992000
*     RET UNRSTD;                                                  0628 00993000
*                                                                  0628 00994000
*/********************************************************************/ 00995000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 00996000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 00997000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 00998000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 00999000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 01000000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 01001000
*/********************************************************************/ 01002000
*                                                                  0629 01003000
*   GEN;                                                           0629 01004000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       01005000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   01006000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      01007000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     01008000
*                                       IF NO ERROR WAS ENCOUNTERED     01009000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    01010000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 01011000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           01012000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     01013000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  01014000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    01015000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           01016000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            01017000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    01018000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   01019000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        01020000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        01021000
         B     GMN00200                 BYPASS SETTING ERROR            01022000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 01023000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    01024000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      01025000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          01026000
*   RFY                                                            0630 01027000
*     RETCODE RSTD;                                                0630 01028000
*   IF RETCODE=0 THEN               /* IF GETMAIN ERROR              */ 01029000
         LTR   RETCODE,RETCODE                                     0631 01030000
         BNZ   @RF00631                                            0631 01031000
*     DO;                           /* THEN CALL PROB DET            */ 01032000
*       RFY                                                        0633 01033000
*         RETCODE UNRSTD;                                          0633 01034000
*       ACBERFLG=OPERR136;          /* SET ACB ERROR FLAG            */ 01035000
         MVI   ACBERFLG(RACB),X'88'                                0634 01036000
*       PDPCODE=PP20;               /* SET PROB DET ERROR CODE       */ 01037000
         MVI   PDPCODE+280(RCORE),X'14'                            0635 01038000
*       RFY                                                        0636 01039000
*         R1 RSTD;                                                 0636 01040000
*       R1=RCORE;                   /* PT TO COMMON WORK AREA        */ 01041000
         LR    R1,RCORE                                            0637 01042000
*       RSTATRTN=IDAR192P;          /* INDIC 192P CONTROL    @Y30DSDP*/ 01043000
         MVC   RSTATRTN(4,RCORE),@CC05329                          0638 01044000
*       CALL IDA0192P;              /* WRITE PROB DET MESSAGE        */ 01045000
         L     @15,@CV05701                                        0639 01046000
         BALR  @14,@15                                             0639 01047000
*       RSTATRTN=IDAR192A;          /* INDIC 192A CONTROL    @Y30DSDP*/ 01048000
         MVC   RSTATRTN(4,RCORE),@CC05315                          0640 01049000
*       RCORE=0;                    /* IND NO MOD OR ACB WORK AREA   */ 01050000
         SLR   RCORE,RCORE                                         0641 01051000
*       RETURN TO TERM192A;         /* RESTORE ACB AND EXIT MODULE   */ 01052000
         LA    @14,TERM192A                                        0642 01053000
         BR    @14                                                 0642 01054000
*       RFY                                                        0643 01055000
*         R1 UNRSTD;                                               0643 01056000
*     END;                                                         0644 01057000
*                                                                  0644 01058000
*   /*****************************************************************/ 01059000
*   /*                                                               */ 01060000
*   /* ESTABLISH MODULE ADDRESSABILITY                               */ 01061000
*   /*                                                               */ 01062000
*   /*****************************************************************/ 01063000
*                                                                  0645 01064000
*   GEN(ST R13,4(R15))                                             0645 01065000
*   REFS(R13,R15) NOSETS;           /* SAVE LAST SAVE AREA PTR       */ 01066000
@RF00631 DS    0H                                                  0645 01067000
         ST R13,4(R15)                                                  01068000
*   RFY                                                            0646 01069000
*     R15 RSTD;                                                    0646 01070000
*   R13=R15;                        /* SET BASE FOR MOD WORK AREA    */ 01071000
         LR    R13,R15                                             0647 01072000
*   RFY                                                            0648 01073000
*     R15 UNRSTD;                                                  0648 01074000
*   GEN(USING @DATD,R13)                                           0649 01075000
*   REFS(R13) NOSETS;                                              0649 01076000
         USING @DATD,R13                                                01077000
*   MWATCB=ADDR(TCB);               /* INIT TCB PTR IN MWA           */ 01078000
         L     @11,PSATOLD                                         0650 01079000
         ST    @11,MWATCB                                          0650 01080000
*   RFY                                                            0651 01081000
*     TCB BASED(MWATCB);            /* ADDRESSABILITY TO TCB         */ 01082000
*   MWASAVEA=R6;                    /* SAVE RETURN ADDRESS           */ 01083000
         ST    R6,MWASAVEA                                         0652 01084000
*   RFY                                                            0653 01085000
*     R6 UNRSTD;                                                   0653 01086000
*                                                                  0653 01087000
*   /*****************************************************************/ 01088000
*   /*                                                               */ 01089000
*   /* BUILD ACB WORK AREA                                           */ 01090000
*   /*                                                               */ 01091000
*   /*****************************************************************/ 01092000
*                                                                  0654 01093000
*   MWAGFSP=WKPOOL;                 /* SET SUBPOOL                   */ 01094000
         MVI   MWAGFSP,X'FC'                                       0654 01095000
*   MWAGFLEN=LENGTH(IDAOPWRK)+MAXNO*LENGTH(OPWUPAIX);/* CALC LENGTH  */ 01096000
         MVC   MWAGFLEN(3),@CF05879+1                              0655 01097000
*   CALL GETCORE;                   /* OBTAIN STORAGE FOR OPW        */ 01098000
         BAL   @14,GETCORE                                         0656 01099000
*   RFY                                                            0657 01100000
*     R9 RSTD;                                                     0657 01101000
*   R9=RCORE;                       /* SAVE PTR TO COMMON WORK AREA  */ 01102000
         LR    R9,RCORE                                            0658 01103000
*   RFY                                                            0659 01104000
*     IDAOPWRK BASED(RCORE);        /* SET BASE FOR ACB WORK AREA    */ 01105000
*   RCORE=MWAGFADR;                                                0660 01106000
         L     RCORE,MWAGFADR                                      0660 01107000
*   MWAOPWRK=ON;                    /* IND ACB WORK AREA EXISTS      */ 01108000
         OI    MWAOPWRK,B'10000000'                                0661 01109000
*   OPWSUBPL=WKPOOL;                /* SET SUBPOOL IN ACB WORK       */ 01110000
         MVI   OPWSUBPL(RCORE),X'FC'                               0662 01111000
*   OPWLENTH=MWAGFLEN;              /* SET LENGTH IN ACB WORK        */ 01112000
         MVC   OPWLENTH(3,RCORE),MWAGFLEN                          0663 01113000
*   OPWID='IDAOPWRK';               /* SET EBCDIC ID IN OPW          */ 01114000
         MVC   OPWID(8,RCORE),@CC05705                             0664 01115000
*   OPWCOMWA=R9;                    /* SET BASE FOR COMMON WORK AREA */ 01116000
         ST    R9,OPWCOMWA(,RCORE)                                 0665 01117000
*   RFY                                                            0666 01118000
*     R9 UNRSTD;                                                   0666 01119000
*   RFY                                                            0667 01120000
*     FORCORE BASED(OPWCOMWA);                                     0667 01121000
*   OPWCTCB=ADDR(TCB);              /* INIT PTR TO CURRENT TCB       */ 01122000
         L     @11,MWATCB                                          0668 01123000
         ST    @11,OPWCTCB(,RCORE)                                 0668 01124000
*   OPWJSTCB=TCBJSTCB;              /* PTR TO JOB STEP TCB   @Y30DSDP*/ 01125000
         L     @11,TCBJSTCB(,@11)                                  0669 01126000
         ST    @11,OPWJSTCB(,RCORE)                                0669 01127000
*   OPWCURPT=ADDR(OPWBSECL);        /* SET CURR CLUSTER TO BASE      */ 01128000
         LA    @05,OPWBSECL(,RCORE)                                0670 01129000
         ST    @05,OPWCURPT(,RCORE)                                0670 01130000
*   MWATCB=OPWJSTCB;                /* PT TO JOB STEP TCB            */ 01131000
         ST    @11,MWATCB                                          0671 01132000
*   OPWUACB=RACB;                   /* INIT PTR TO USER ACB          */ 01133000
         ST    RACB,OPWUACB(,RCORE)                                0672 01134000
*   OPWTIOT=DXTIOTAD;               /* PT TO TIOT ENTRY      @Y30DSDP*/ 01135000
         L     @11,OPWCOMWA(,RCORE)                                0673 01136000
         L     @05,DXTIOTAD(,@11)                                  0673 01137000
         ST    @05,OPWTIOT(,RCORE)                                 0673 01138000
*   OPWERMAP=ERRMAP;                /* LD PPCODE TO ACBERFLG MAP     */ 01139000
*                                                                  0674 01140000
         MVC   OPWERMAP(256,RCORE),ERRMAP                          0674 01141000
*   /*****************************************************************/ 01142000
*   /*                                                               */ 01143000
*   /* INITIALIZE ACB FIELDS TO ZERO                                 */ 01144000
*   /*                                                               */ 01145000
*   /*****************************************************************/ 01146000
*                                                                  0675 01147000
*   ACBAMBL=0;                                                     0675 01148000
         SLR   @05,@05                                             0675 01149000
         ST    @05,ACBAMBL(,RACB)                                  0675 01150000
*   ACBDEB=0;                                                      0676 01151000
         STCM  @05,7,ACBDEB(RACB)                                  0676 01152000
*   ACBINRTN=0;                                                    0677 01153000
         ST    @05,ACBINRTN(,RACB)                                 0677 01154000
*   IF JFCBDSNM(1:9)='NULLFILE ' THEN/* IF DUMMY DATA SET            */ 01155000
