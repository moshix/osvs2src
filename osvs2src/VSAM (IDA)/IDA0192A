*GETCORE:                                                          1217 02944000
*                                                                  1217 02945000
*/********************************************************************/ 02946000
*/*                                                                  */ 02947000
*/*   FUNCTION: THIS ROUTINE GETS VIRTUAL STORAGE.                   */ 02948000
*/*                                                                  */ 02949000
*/*   INPUTS:   1. SUBPOOL REQUIRED IN MWAGFSP.                      */ 02950000
*/*             2. LENGTH OF CORE REQUIRED IN MWAGFLEN.              */ 02951000
*/*                                                                  */ 02952000
*/*   OUTPUTS:  1. GETMAIN IS ISSUED.                                */ 02953000
*/*             2. CORE ALLOCATED IS ZEROED.                         */ 02954000
*/*                                                                  */ 02955000
*/********************************************************************/ 02956000
*                                                                  1217 02957000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1217 02958000
GETCORE  DS    0H                                                  1218 02959000
*   RFY                                                            1218 02960000
*     RET RSTD;                                                    1218 02961000
*   MWASAVEE=RET;                   /* SAVE RETURN ADDRESS           */ 02962000
         ST    RET,MWASAVEE                                        1219 02963000
*   RFY                                                            1220 02964000
*     RET UNRSTD;                                                  1220 02965000
*   RFY                                                            1221 02966000
*    (R0,                                                          1221 02967000
*     R1,                                                          1221 02968000
*     R6,                                                          1221 02969000
*     R7,                                                          1221 02970000
*     R8,                                                          1221 02971000
*     R9) RSTD;                                                    1221 02972000
*   STM(R6,R9,MWAGFSVE);            /* SAVE WORK REGISTERS           */ 02973000
         STM   R6,R9,MWAGFSVE                                      1222 02974000
*   MWAGFLEN=(MWAGFLEN+SEVEN)&NEGEIGHT;/* ROUND LENGTH UP TO NEXT  1223 02975000
*                                      DOUBLE WORD MULTIPLE          */ 02976000
         LA    @11,7                                               1223 02977000
         L     @05,MWAGFLEN-1                                      1223 02978000
         LA    @05,0(,@05)                                         1223 02979000
         ALR   @11,@05                                             1223 02980000
         N     @11,@CF05547                                        1223 02981000
         STCM  @11,7,MWAGFLEN                                      1223 02982000
*   R6=MWAGFSP;                     /* LOAD SUBPOOL                  */ 02983000
         SLR   R6,R6                                               1224 02984000
         IC    R6,MWAGFSP                                          1224 02985000
*   R7=MWAGFLEN;                    /* LOAD BLOCK LENGTH             */ 02986000
         LR    R7,@11                                              1225 02987000
*   GEN(GETMAIN RC,LV=(R7),SP=(R6))                                1226 02988000
*   SETS(RETCODE);                  /* GET BLOCK             @Y30DSDP*/ 02989000
         GETMAIN RC,LV=(R7),SP=(R6)                                     02990000
*   RFY                                                            1227 02991000
*     RETCODE RSTD;                                                1227 02992000
*   IF RETCODE=0 THEN               /* WAS GETMAIN SUCCESSFUL        */ 02993000
         LTR   RETCODE,RETCODE                                     1228 02994000
         BNZ   @RF01228                                            1228 02995000
*     DO;                           /* YES - ZERO CORE BLOCK         */ 02996000
*       MWAGFADR=R1;                /* PTR TO GOTTEN CORE    @Y30DSDP*/ 02997000
         ST    R1,MWAGFADR                                         1230 02998000
*       R6=MWAGFADR;                /* POINT TO CORE BLOCK           */ 02999000
         L     @14,MWAGFADR                                        1231 03000000
         LR    R6,@14                                              1231 03001000
*       R7=MWAGFLEN;                /* LOAD BLOCK LENGTH             */ 03002000
         L     R7,MWAGFLEN-1                                       1232 03003000
         LA    R7,0(,R7)                                           1232 03004000
*       R9=0;                       /* ZERO PAD CHARACTER            */ 03005000
         SLR   R9,R9                                               1233 03006000
*       MVCL(R6,R8);                /* ZERO CORE BLOCK               */ 03007000
         MVCL  R6,R8                                               1234 03008000
*     END;                                                         1235 03009000
*   LM(R6,R9,MWAGFSVE);             /* RESTORE WORK REGISTERS        */ 03010000
@RF01228 LM    R6,R9,MWAGFSVE                                      1236 03011000
*   IF RETCODE^=0 THEN              /* IF GETMAIN FAILED             */ 03012000
         LTR   RETCODE,RETCODE                                     1237 03013000
         BZ    @RF01237                                            1237 03014000
*     DO;                           /* THEN PROCESS ERROR            */ 03015000
*       IF MWAOPWRK=ON THEN         /* IF OPW EXISTS                 */ 03016000
         TM    MWAOPWRK,B'10000000'                                1239 03017000
         BNO   @RF01239                                            1239 03018000
*         DO;                       /* THEN DO NORMAL PROB DET       */ 03019000
*           PDPCODE=PP20;           /* IND GETMAIN FAILURE           */ 03020000
         L     @14,OPWCOMWA(,RCORE)                                1241 03021000
         MVI   PDPCODE+280(@14),X'14'                              1241 03022000
*           CALL PROBDT;            /* WRITE PROB DET MSG            */ 03023000
         BAL   @14,PROBDT                                          1242 03024000
*         END;                                                     1243 03025000
*       ELSE                        /* ELSE OPW DOES NOT EXIST       */ 03026000
*         DO;                       /* TERMINATE OPEN                */ 03027000
         B     @RC01239                                            1244 03028000
@RF01239 DS    0H                                                  1245 03029000
*           RESPECIFY                                              1245 03030000
*             FORCORE BASED(RCORE); /*                       @ZA33666*/ 03031000
*           PDPCODE=PP20;           /* IND GETMAIN FAILURE   @ZA33666*/ 03032000
         MVI   PDPCODE+280(RCORE),X'14'                            1246 03033000
*           RESPECIFY                                              1247 03034000
*             FORCORE BASED(OPWCOMWA);/*                     @ZA33666*/ 03035000
*           ACBERFLG=IDAERMAP(PP20);/* SET ACB ERROR CODE            */ 03036000
         IC    @14,IDAERMAP+19                                     1248 03037000
         STC   @14,ACBERFLG(,RACB)                                 1248 03038000
*           R1=RCORE;               /* PT TO FORECORE                */ 03039000
         LR    R1,RCORE                                            1249 03040000
*           RSTATRTN=IDAR192P;      /* INDIC 192P CONTROL    @Y30DSDP*/ 03041000
         L     @14,OPWCOMWA(,RCORE)                                1250 03042000
         MVC   RSTATRTN(4,@14),@CC05329                            1250 03043000
*           CALL IDA0192P;          /* WRITE PROB DET MSG            */ 03044000
         L     @15,@CV05701                                        1251 03045000
