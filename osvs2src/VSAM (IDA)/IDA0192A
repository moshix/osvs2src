*           PDPSFI=ADDR(IDASFI);    /* PT TO SFI AREA                */ 04558000
         LA    @11,MWASFI                                          1690 04559000
         ST    @11,PDPSFI(,@14)                                    1690 04560000
*           CALL PROBDT;            /* PROCESS ERROR                 */ 04561000
         BAL   @14,PROBDT                                          1691 04562000
*           PDPSFI=0;               /* ZERO PTR TO SFI AREA          */ 04563000
         LA    @14,280                                             1692 04564000
         AL    @14,OPWCOMWA(,RCORE)                                1692 04565000
         SLR   @11,@11                                             1692 04566000
