         BE    @RF00404                                            0404 00259000
*             CALL UPSMF;           /* UPDATE SMF DATA SET           */ 00260000
         BAL   @14,UPSMF                                           0405 00261000
*           IF MWAOFAIL=ON THEN     /* IF OPEN FAILURE               */ 00262000
@RF00404 TM    MWAOFAIL,B'00010000'                                0406 00263000
         BNO   @RF00406                                            0406 00264000
*             CALL CLNUP;           /* CLEANUP PROCESSING            */ 00265000
         BAL   @14,CLNUP                                           0407 00266000
*           ELSE                    /* NOT OPEN FAILURE              */ 00267000
*             DO;                                                  0408 00268000
         B     @RC00406                                            0408 00269000
