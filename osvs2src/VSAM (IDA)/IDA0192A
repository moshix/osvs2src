         STCM  @11,7,MWAGFLEN                                      1619 04343000
*         IF BIBVMT=MWAPTRA THEN    /* IF VMT IS FIRST ON CHAIN      */ 04344000
         L     @11,OPWBIB(,RCORE)                                  1620 04345000
         C     @05,BIBVMT(,@11)                                    1620 04346000
         BNE   @RF01620                                            1620 04347000
*           DO;                     /* THEN DECHAIN VMT              */ 04348000
*             BIBVMT=VMTNXT;        /* DECHAIN VMT                   */ 04349000
         L     @05,VMTNXT(,@05)                                    1622 04350000
         ST    @05,BIBVMT(,@11)                                    1622 04351000
*             MWAPTRA=BIBVMT;       /* PT TO NEXT VMT                */ 04352000
         ST    @05,MWAPTRA                                         1623 04353000
*           END;                                                   1624 04354000
*         ELSE                      /* ELSE FIND PREVIOUS VMT        */ 04355000
*           DO;                                                    1625 04356000
         B     @RC01620                                            1625 04357000
@RF01620 DS    0H                                                  1626 04358000
*             RFY                                                  1626 04359000
*               IDAVMT BASED(MWAPTRB);                             1626 04360000
*             MWAPTRB=BIBVMT;       /* PT TO FIRST VMT               */ 04361000
         L     @11,OPWBIB(,RCORE)                                  1627 04362000
         L     MWAPTRB,BIBVMT(,@11)                                1627 04363000
*             DO WHILE(VMTNXT^=MWAPTRA);/* FIND PREVIOUS VMT         */ 04364000
         B     @DE01628                                            1628 04365000
@DL01628 DS    0H                                                  1629 04366000
*               MWAPTRB=VMTNXT;     /* PT TO NEXT VMT                */ 04367000
         L     MWAPTRB,VMTNXT(,MWAPTRB)                            1629 04368000
*             END;                                                 1630 04369000
@DE01628 L     @11,MWAPTRA                                         1630 04370000
