*JFCACBMG:                                                         1341 03340000
*                                                                  1341 03341000
*/********************************************************************/ 03342000
*/*                                                                  */ 03343000
*/*   FUNCTION: THIS ROUTINE DOES A JFCB TO ACB MERGE.               */ 03344000
*/*                                                                  */ 03345000
*/*   INPUTS:   1. CHECKPOINT/RESTART INDICATOR IN JFCB (JFCAMCRO).  */ 03346000
*/*                                                                  */ 03347000
*/*   OUTPUTS:  1. CHECKPOINT/RESTART INDICATORS IN ACB.             */ 03348000
*/*                                                                  */ 03349000
*/********************************************************************/ 03350000
*                                                                  1341 03351000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1341 03352000
JFCACBMG DS    0H                                                  1342 03353000
*   RFY                                                            1342 03354000
*     RET RSTD;                                                    1342 03355000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 03356000
         ST    RET,MWASAVEB                                        1343 03357000
*   RFY                                                            1344 03358000
*     RET UNRSTD;                                                  1344 03359000
*                                                                  1344 03360000
*   /*****************************************************************/ 03361000
*   /*                                                               */ 03362000
*   /* MERGE USERS JFCB CHECKPOINT/RESTART INDICATORS INTO ACB       */ 03363000
*   /*                                                               */ 03364000
*   /*****************************************************************/ 03365000
*                                                                  1345 03366000
*   IF JFCAMCRO(CRJFCBYT)=RCK THEN  /* SHOULD C/R MAKE TESTS         */ 03367000
         L     @11,OPWCOMWA(,RCORE)                                1345 03368000
         CLI   JFCAMCRO+100(@11),128                               1345 03369000
         BNE   @RF01345                                            1345 03370000
*     DO;                           /* YES                           */ 03371000
*       ACBCRNCK=OFF;               /* SET "NO CHECK FOR MODIFY" OFF */ 03372000
*       ACBCRNRE=OFF;               /* SET "NO DATA ERASE/REPOS" OFF */ 03373000
         NI    ACBCRNCK(RACB),B'00111111'                          1348 03374000
*     END;                                                         1349 03375000
*   ELSE                            /* IF NOT RCK                    */ 03376000
*     IF JFCAMCRO(CRJFCBYT)=NRC THEN/* SHOULD C/R MAKE TESTS         */ 03377000
         B     @RC01345                                            1350 03378000
@RF01345 L     @11,OPWCOMWA(,RCORE)                                1350 03379000
         CLI   JFCAMCRO+100(@11),16                                1350 03380000
         BNE   @RF01350                                            1350 03381000
*       DO;                         /* NO                            */ 03382000
*         ACBCRNCK=ON;              /* SET "NO CHECK FOR MODIFY" ON  */ 03383000
*         ACBCRNRE=ON;              /* SET "NO DATA ERASE/REPOS" ON  */ 03384000
         OI    ACBCRNCK(RACB),B'11000000'                          1353 03385000
*       END;                                                       1354 03386000
*     ELSE                          /* IF NOT NRC                    */ 03387000
*       IF JFCAMCRO(CRJFCBYT)=NCK THEN/* NO CHECK FOR MODIFY         */ 03388000
         B     @RC01350                                            1355 03389000
@RF01350 L     @11,OPWCOMWA(,RCORE)                                1355 03390000
         CLI   JFCAMCRO+100(@11),64                                1355 03391000
         BNE   @RF01355                                            1355 03392000
*         DO;                                                      1356 03393000
*           ACBCRNCK=ON;            /* SET "NO CHECK FOR MODIFY" ON  */ 03394000
*           ACBCRNRE=OFF;           /* SET "NO DATA ERASE/REPOS" OFF */ 03395000
         OI    ACBCRNCK(RACB),B'10000000'                          1358 03396000
         NI    ACBCRNRE(RACB),B'10111111'                          1358 03397000
*         END;                                                     1359 03398000
*       ELSE                        /* IF NOT NCK                    */ 03399000
*         IF JFCAMCRO(CRJFCBYT)=NRE THEN/* NO DATA ERASE/REPOS       */ 03400000
         B     @RC01355                                            1360 03401000
@RF01355 L     @11,OPWCOMWA(,RCORE)                                1360 03402000
         CLI   JFCAMCRO+100(@11),32                                1360 03403000
         BNE   @RF01360                                            1360 03404000
*           DO;                                                    1361 03405000
*             ACBCRNCK=OFF;         /* SET "NO CHECK FOR MODIFY" OFF */ 03406000
