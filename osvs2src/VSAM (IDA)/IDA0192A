@EL00026 DS    0H                                                  1592 04231000
@EF00026 DS    0H                                                  1592 04232000
@ER00026 BR    @14                                                 1592 04233000
*VMTDELET:                                                         1593 04234000
*                                                                  1593 04235000
*/********************************************************************/ 04236000
*/*                                                                  */ 04237000
*/*   FUNCTION: CALLED BY CLNUP TO DECREMENT UCB USE COUNT AND ZERO  */ 04238000
*/*             ENTRY FOR EACH VMT ENTRY ADDED THIS OPEN.  IF ALL    */ 04239000
*/*             THE ENTRIES IN A PARTICULAR VMT WERE ADDED DURING    */ 04240000
*/*             THIS OPEN, REMOVE VMT FROM CHAIN AND FREE IT.        */ 04241000
*/*                                                                  */ 04242000
*/*   INPUTS:   1. PTR TO FIRST VMT IN BIBVMT.                       */ 04243000
*/*             2. PTR TO BIB IN OPWBIB.                             */ 04244000
*/*                                                                  */ 04245000
*/*   OUTPUTS:  1. VMT ENTRIES ADDED FOR THIS OPEN ARE CLEANED UP.   */ 04246000
*/*                                                                  */ 04247000
*/********************************************************************/ 04248000
*                                                                  1593 04249000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1593 04250000
VMTDELET DS    0H                                                  1594 04251000
*   RFY                                                            1594 04252000
*     RET RSTD;                                                    1594 04253000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 04254000
         ST    RET,MWASAVEB                                        1595 04255000
*   RFY                                                            1596 04256000
*     RET UNRSTD;                                                  1596 04257000
*   RFY                                                            1597 04258000
*     IDAVMT BASED(MWAPTRA);                                       1597 04259000
*   RFY                                                            1598 04260000
*     UCBOB BASED(VMTUCB(MWAI));                                   1598 04261000
*   MWAPTRA=BIBVMT;                 /* PT TO FIRST VMT               */ 04262000
         L     @11,OPWBIB(,RCORE)                                  1599 04263000
         L     @11,BIBVMT(,@11)                                    1599 04264000
         ST    @11,MWAPTRA                                         1599 04265000
*   DO WHILE(MWAPTRA^=0);           /* SCAN VMT'S                    */ 04266000
         B     @DE01600                                            1600 04267000
@DL01600 DS    0H                                                  1601 04268000
*     MWAJ=0;                       /* ZERO VMT OPEN COUNT           */ 04269000
         SLR   MWAJ,MWAJ                                           1601 04270000
*     DO MWAI=1 TO VMTNOVOL;        /* SCAN ENTRIES IN VMT           */ 04271000
         LA    @05,1                                               1602 04272000
         B     @DE01602                                            1602 04273000
@DL01602 DS    0H                                                  1603 04274000
*       IF VMTOPEN(MWAI)=ON THEN    /* IF ENTRY FOR THIS OPEN        */ 04275000
         L     @15,MWAPTRA                                         1603 04276000
         SLA   @05,4                                               1603 04277000
         ALR   @05,@15                                             1603 04278000
         TM    VMTOPEN-16(@05),B'10000000'                         1603 04279000
         BNO   @RF01603                                            1603 04280000
*         DO;                       /* THEN DECR UCB USE COUNT AND 1604 04281000
*                                      REMOVE VMT ENTRY FOR VOL      */ 04282000
*           IF OPWSCRA=OFF&OPWUCRA=OFF THEN/* IF NOT SCRA OR UCRA    */ 04283000
         TM    OPWSCRA(RCORE),B'01000000'                          1605 04284000
         BNZ   @RF01605                                            1605 04285000
         TM    OPWUCRA(RCORE),B'10000000'                          1605 04286000
         BNZ   @RF01605                                            1605 04287000
*             DO;                   /* THEN DECR UCB USE COUNT       */ 04288000
*               RFY                                                1607 04289000
*                (R6,                                              1607 04290000
*                 R7,                                              1607 04291000
*                 R8,                                              1607 04292000
*                 R9) RSTD;                                        1607 04293000
*               R9=VMTUCB(MWAI);    /* LD ADDR OF UCB                */ 04294000
         L     @05,MWAI                                            1608 04295000
         SLA   @05,4                                               1608 04296000
         L     R9,VMTUCB-16(@05,@15)                               1608 04297000
*               GEN SETS(R6,R7,R8,UCBDMCT);                        1609 04298000
*                                      /* DECREMENT UCB USE COUNT */    04299000
L2       L     R6,UCBDMCT-3(R9)        LOAD FWORD WITH UCB USE COUNT    04300000
         SR    R7,R7                   ZERO REGISTER 7                  04301000
         IC    R7,UCBDMCT(R9)          GET UCB USE COUNT                04302000
         BCTR  R7,0                    DECREMENT USE COUNT BY ONE       04303000
         L     R8,UCBDMCT-3(R9)        GET FWORD WITH UCB USE COUNT     04304000
         SRL   R8,8                    SHIFT OUT OLD USE COUNT          04305000
         SLL   R8,8                    SHIFT BACK TO ORIGINAL POSITION  04306000
         OR    R7,R8                   LOGICALLY OR IN NEW USE COUNT    04307000
         CS    R6,R7,UCBDMCT-3(R9)     COMPARE AND SWAP                 04308000
         BC    7,L2                    IF UNSUCCESSFUL TRY AGAIN        04309000
*               RFY                                                1610 04310000
*                (R6,                                              1610 04311000
*                 R7,                                              1610 04312000
*                 R8,                                              1610 04313000
*                 R9) UNRSTD;                                      1610 04314000
*             END;                                                 1611 04315000
*           VMTVOL(MWAI)=VMTVOL(MWAI)&&VMTVOL(MWAI);/* ZERO VOLUME 1612 04316000
*                                      ENTRY                         */ 04317000
@RF01605 L     @05,MWAI                                            1612 04318000
         SLA   @05,4                                               1612 04319000
         L     @01,MWAPTRA                                         1612 04320000
         ALR   @01,@05                                             1612 04321000
         XC    VMTVOL-16(16,@01),VMTVOL-16(@01)                    1612 04322000
*           MWAJ=MWAJ+1;            /* INCR OPEN COUNT               */ 04323000
         AL    MWAJ,@CF00054                                       1613 04324000
*         END;                                                     1614 04325000
*     END;                                                         1615 04326000
@RF01603 LA    @05,1                                               1615 04327000
         AL    @05,MWAI                                            1615 04328000
@DE01602 ST    @05,MWAI                                            1615 04329000
         L     @01,MWAPTRA                                         1615 04330000
         CH    @05,VMTNOVOL(,@01)                                  1615 04331000
         BNH   @DL01602                                            1615 04332000
*     IF MWAJ=VMTNOVOL THEN         /* IF ALL VMT ENTRIES ADDED THIS    04333000
*                                      OPEN                          */ 04334000
