*/********************************************************************/ 00760000
*/*                                                                  */ 00761000
*/*   FUNCTION: CALLED BY TERM192A TO DEQ ENQ BUSY FOR EACH DATA     */ 00762000
*/*             SET IN SPHERE AND FREE ENQ PARAMETER LIST.           */ 00763000
*/*                                                                  */ 00764000
*/*   INPUTS:   1. PTR TO ENQ PARAMETER LIST IN MWAENQPT.            */ 00765000
*/*             2. LENGTH OF ENQ PARAMETER LIST IN MWAENQLN.         */ 00766000
*/*                                                                  */ 00767000
*/*   OUTPUTS:  1. ENQ PARAMETER LIST FREED.                         */ 00768000
*/*                                                                  */ 00769000
*/********************************************************************/ 00770000
*                                                                  0560 00771000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0560 00772000
DEQBUSY  DS    0H                                                  0561 00773000
*   RFY                                                            0561 00774000
*     RET RSTD;                                                    0561 00775000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 00776000
         ST    RET,MWASAVEA                                        0562 00777000
*   RFY                                                            0563 00778000
*     RET UNRSTD;                                                  0563 00779000
*   RFY                                                            0564 00780000
*     R1 RSTD;                                                     0564 00781000
*   R1=ADDR(ENQENTRY(1));           /* PT TO ENQ PARAMETER LIST      */ 00782000
         L     R1,MWAENQPT                                         0565 00783000
         LA    R1,ENQENTRY(,R1)                                    0565 00784000
*   GEN(DEQ (,,,),MF=(E,(R1)));     /* DEQ BUSY                      */ 00785000
         DEQ (,,,),MF=(E,(R1))                                          00786000
*   RFY                                                            0567 00787000
*     R1 UNRSTD;                                                   0567 00788000
*   MWAGFADR=MWAENQPT;              /* INIT ADDR OF ENQ PARM LIST    */ 00789000
         L     @11,MWAENQPT                                        0568 00790000
         ST    @11,MWAGFADR                                        0568 00791000
*   MWAGFSP=WKPOOL;                 /* INIT SUBPOOL OF ENQ PARM LIST */ 00792000
         MVI   MWAGFSP,X'FC'                                       0569 00793000
*   MWAGFLEN=MWAENQLN;              /* INIT LENGTH OF ENQ PARM LIST  */ 00794000
         MVC   MWAGFLEN(3),MWAENQLN+1                              0570 00795000
*   CALL FREECORE;                  /* FREE ENQ PARAMETER LIST       */ 00796000
         BAL   @14,FREECORE                                        0571 00797000
*   RFY                                                            0572 00798000
*     RET RSTD;                                                    0572 00799000
