*                   PDPSFC=0;       /* ZERO SUB=FUNCTION CODE        */ 00223000
         MVI   PDPSFC(@11),X'00'                                   0399 00224000
*                 END;                                             0400 00225000
*               ACBOPEN=OFF;        /* RESET OPEN INDR IN CATLG ACB  */ 00226000
@RF00386 NI    ACBOPEN(RACB),B'11101111'                           0401 00227000
*               RFY                                                0402 00228000
*                (R1,                                              0402 00229000
*                 R5) UNRSTD;                                      0402 00230000
*             END;                                                 0403 00231000
*           IF OPWDUMMY=OFF&        /* IF NOT DUMMY DATA SET         */ 00232000
*               CVTSMCA^=0&         /* AND SMCA EXISTS               */ 00233000
*               SMCADSA=ON&         /* AND DS INFO REQUIRED          */ 00234000
*               (SMCAUSER=ON&SMCAMAN=ON)&/* AND SMF RECORDING        */ 00235000
*               TCBTCTB^=0&         /* AND TCT EXIST AND     @ZA29395*/ 00236000
*               OPWCACB^=0&OPWCACB->ACBUAPTR^=0 THEN/* CAT ACB EXIST    00237000
*                                                            @ZA07208*/ 00238000
@RF00371 TM    OPWDUMMY(RCORE),B'00010000'                         0404 00239000
