         LA    @14,MWASFI                                          1681 04534000
         XC    IDASFI(12,@14),IDASFI(@14)                          1681 04535000
*       R5=AMBLIX;                  /* PT TO DATA AMB                */ 04536000
         L     R5,AMBLIX(,MWAXAMBL)                                1682 04537000
