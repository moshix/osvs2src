*               CALL IDA0192C;      /* DO DUMMY LOCATE               */ 00172000
         L     @15,@CV05659                                        0383 00173000
         BALR  @14,@15                                             0383 00174000
*               RFY                                                0384 00175000
*                 RETCODE RSTD;                                    0384 00176000
*               RSTATRTN=IDAR192A;  /* INDIC 192A CONTROL    @Y30DSDP*/ 00177000
         L     @14,OPWCOMWA(,RCORE)                                0385 00178000
         MVC   RSTATRTN(4,@14),@CC05315                            0385 00179000
*               IF RETCODE>0 THEN   /* IF ERROR FROM CATALOG         */ 00180000
         SLR   @11,@11                                             0386 00181000
         CR    RETCODE,@11                                         0386 00182000
         BNH   @RF00386                                            0386 00183000
*                 DO;               /* THEN WRITE ERROR MESSAGE      */ 00184000
*                   MWAPDPRM=RETCODE;/* LD RETCODE                   */ 00185000
         ST    RETCODE,MWAPDPRM                                    0388 00186000
*                   RFY                                            0389 00187000
*                     RETCODE UNRSTD;                              0389 00188000
*                   PDPCODE=MWAPCODE;/* LOAD PROBLEM CODE            */ 00189000
         AL    @14,@CF05875                                        0390 00190000
         IC    @00,MWAPCODE                                        0390 00191000
         STC   @00,PDPCODE(,@14)                                   0390 00192000
*                   PDPFUNC=MWAPFUNC;/* LOAD FUNCTION CODE           */ 00193000
         IC    @00,MWAPFUNC                                        0391 00194000
         STC   @00,PDPFUNC(,@14)                                   0391 00195000
*                   PDPSFC=MWAPSFC; /* LOAD SUB-FUNCTION CODE        */ 00196000
         IC    @00,MWAPSFC                                         0392 00197000
         STC   @00,PDPSFC(,@14)                                    0392 00198000
*                   IF PDPSFC=0&    /* IF NO SUB-FUNCTION CODE       */ 00199000
*                       SFIRETCD^=0 THEN/* AND SFI INITIALIZED       */ 00200000
         CLI   PDPSFC(@14),0                                       0393 00201000
         BNE   @RF00393                                            0393 00202000
         LA    @15,MWASFI                                          0393 00203000
         C     @11,SFIRETCD(,@15)                                  0393 00204000
         BE    @RF00393                                            0393 00205000
*                     PDPSFI=ADDR(IDASFI);/* THEN PT TO SFI AREA     */ 00206000
         ST    @15,PDPSFI(,@14)                                    0394 00207000
*                   ELSE            /* IF NOT                        */ 00208000
*                     PDPSFI=0;     /* IND NO SFI                    */ 00209000
         B     @RC00393                                            0395 00210000
@RF00393 L     @11,OPWCOMWA(,RCORE)                                0395 00211000
         SLR   @00,@00                                             0395 00212000
         ST    @00,PDPSFI+280(,@11)                                0395 00213000
*                   MWAOFAIL=ON;    /* SET ERROR INDICATOR           */ 00214000
@RC00393 OI    MWAOFAIL,B'00010000'                                0396 00215000
*                   CALL PROBDT;    /* PROCESS ERROR                 */ 00216000
         BAL   @14,PROBDT                                          0397 00217000
*                   PDPSFI=0;       /* ZERO PTR TO SFI AREA          */ 00218000
         LA    @11,280                                             0398 00219000
