@EF00027 DS    0H                                                  1649 04429000
@ER00027 BR    @14                                                 1649 04430000
*PUTOPENI:                                                         1650 04431000
*                                                                  1650 04432000
*/********************************************************************/ 04433000
*/*                                                                  */ 04434000
*/*   FUNCTION: CALLED BY CLNUP TO RESET THE OPEN INDICATOR IN THE   */ 04435000
*/*             CATALOG FOR EACH DATA SET SUCCESSFULLY OPENED PRIOR  */ 04436000
*/*             TO THE FAILURE.  INITIALIZES PARAMETER LIST AND      */ 04437000
*/*             CALLS IDA0192C TO RESET OPEN INDICATOR.              */ 04438000
*/*                                                                  */ 04439000
*/*   INPUTS:   1. PTR TO CURRENT CLUSTER IN OPWCURPT.               */ 04440000
*/*             2. PTR TO CURRENT AMBL IN MWAXAMBL.                  */ 04441000
*/*                                                                  */ 04442000
*/*   OUTPUTS:  1. OPEN INDICATOR IS RESET IN CATALOG.               */ 04443000
*/*                                                                  */ 04444000
*/********************************************************************/ 04445000
*                                                                  1650 04446000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1650 04447000
PUTOPENI DS    0H                                                  1651 04448000
*   RFY                                                            1651 04449000
*     RET RSTD;                                                    1651 04450000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 04451000
         ST    RET,MWASAVEB                                        1652 04452000
*   RFY                                                            1653 04453000
*     RET UNRSTD;                                                  1653 04454000
*   MWACTID=OPO;                    /* IND RESET OPEN INDICATOR      */ 04455000
         MVI   MWACTID,X'0C'                                       1654 04456000
*   MWACTSFI=ADDR(IDASFI);          /* PT TO SFI AREA                */ 04457000
         LA    @11,MWASFI                                          1655 04458000
         STCM  @11,7,MWACTSFI                                      1655 04459000
*   RESPECIFY                                                      1656 04460000
*    (R1,                                                          1656 04461000
*     R5,                                                          1656 04462000
*     RETCODE) RESTRICTED;                                         1656 04463000
*   R1=ADDR(MWACATPL);              /* PT TO CATALOG PARM LIST       */ 04464000
         LA    R1,MWACATPL                                         1657 04465000
*   IF OPWDOPEN=ON THEN             /* OPEN INDR IN CATLG ON FOR DATA*/ 04466000
         L     @14,OPWCURPT(,RCORE)                                1658 04467000
         TM    OPWDOPEN(@14),B'10000000'                           1658 04468000
         BNO   @RF01658                                            1658 04469000
*     DO;                                                          1659 04470000
*       IDASFI=IDASFI&&IDASFI;      /* ZERO SFI                      */ 04471000
         XC    IDASFI(12,@11),IDASFI(@11)                          1660 04472000
*       R5=AMBLDTA;                 /* PT TO DATA AMB                */ 04473000
         L     R5,AMBLDTA(,MWAXAMBL)                               1661 04474000
*       RSTATRTN=IDAR192C;          /* INDIC 192C CONTROL    @Y30DSDP*/ 04475000
         L     @14,OPWCOMWA(,RCORE)                                1662 04476000
         MVC   RSTATRTN(4,@14),@CC05319                            1662 04477000
*       CALL IDA0192C;              /* CALL CATLG INTRF TO INIT OPW  */ 04478000
         L     @15,@CV05659                                        1663 04479000
         BALR  @14,@15                                             1663 04480000
*       RSTATRTN=IDAR192A;          /* INDIC 192A CONTROL    @Y30DSDP*/ 04481000
         L     @14,OPWCOMWA(,RCORE)                                1664 04482000
         MVC   RSTATRTN(4,@14),@CC05315                            1664 04483000
*       IF RETCODE>0 THEN           /* IF ERROR FROM CATALOG         */ 04484000
         SLR   @11,@11                                             1665 04485000
         CR    RETCODE,@11                                         1665 04486000
         BNH   @RF01665                                            1665 04487000
*         DO;                       /* THEN WRITE ERROR MESSAGE      */ 04488000
*           MWAPDPRM=RETCODE;       /* LD RETCODE                    */ 04489000
         ST    RETCODE,MWAPDPRM                                    1667 04490000
*           PDPCODE=MWAPCODE;       /* LOAD PROBLEM CODE             */ 04491000
         AL    @14,@CF05875                                        1668 04492000
         IC    @07,MWAPCODE                                        1668 04493000
         STC   @07,PDPCODE(,@14)                                   1668 04494000
*           PDPFUNC=MWAPFUNC;       /* LOAD FUNCTION CODE            */ 04495000
         IC    @07,MWAPFUNC                                        1669 04496000
         STC   @07,PDPFUNC(,@14)                                   1669 04497000
*           PDPSFC=MWAPSFC;         /* LOAD SUB-FUNCTION CODE        */ 04498000
         IC    @07,MWAPSFC                                         1670 04499000
         STC   @07,PDPSFC(,@14)                                    1670 04500000
*           IF PDPSFC=0&            /* IF NO SUB-FUNCTION CODE       */ 04501000
*               SFIRETCD^=0 THEN    /* AND SFI INITIALIZED           */ 04502000
         CLI   PDPSFC(@14),0                                       1671 04503000
         BNE   @RF01671                                            1671 04504000
         LA    @07,MWASFI                                          1671 04505000
         C     @11,SFIRETCD(,@07)                                  1671 04506000
         BE    @RF01671                                            1671 04507000
*             PDPSFI=ADDR(IDASFI);  /* THEN PT TO SFI AREA           */ 04508000
         ST    @07,PDPSFI(,@14)                                    1672 04509000
*           ELSE                    /* IF NOT                        */ 04510000
*             PDPSFI=0;             /* IND NO SFI                    */ 04511000
         B     @RC01671                                            1673 04512000
@RF01671 L     @14,OPWCOMWA(,RCORE)                                1673 04513000
         SLR   @11,@11                                             1673 04514000
         ST    @11,PDPSFI+280(,@14)                                1673 04515000
*           CALL PROBDT;            /* PROCESS ERROR                 */ 04516000
@RC01671 BAL   @14,PROBDT                                          1674 04517000
*           PDPSFI=0;               /* ZERO PTR TO SFI AREA          */ 04518000
         LA    @14,280                                             1675 04519000
         AL    @14,OPWCOMWA(,RCORE)                                1675 04520000
         SLR   @11,@11                                             1675 04521000
         ST    @11,PDPSFI(,@14)                                    1675 04522000
*           PDPSFC=0;               /* ZERO SUB=FUNCTION CODE        */ 04523000
         MVI   PDPSFC(@14),X'00'                                   1676 04524000
*         END;                                                     1677 04525000
*     END;                                                         1678 04526000
