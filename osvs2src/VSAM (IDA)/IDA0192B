         LR    @01,MWABUFND                                        0687 01110000
         MR    @00,MWACISZD                                        0687 01111000
         AL    @01,MWABFSPI                                        0687 01112000
         LR    MWABUFSP,@01                                        0687 01113000
*           IF MWABUFSP>MWABFSP THEN/* CURRENT BFR SPACE STILL BIG   */ 01114000
         CR    MWABUFSP,@10                                        0688 01115000
         BNH   @RF00688                                            0688 01116000
*             MWABUFNI=MWARQIBF;    /* REDUCE TO MINIMUM             */ 01117000
         L     MWABUFNI,MWARQIBF                                   0689 01118000
*         END;                                                     0690 01119000
@RF00688 DS    0H                                                  0691 01120000
*     END;                                                         0691 01121000
@RF00684 DS    0H                                                  0692 01122000
*   RESPECIFY                                                      0692 01123000
*     RET RESTRICTED;                                              0692 01124000
@RC00671 DS    0H                                                  0693 01125000
*   RET=MWASAVEB;                   /* LOAD RETURN ADDRESS           */ 01126000
         L     RET,MWASAVEB                                        0693 01127000
*   RESPECIFY                                                      0694 01128000
*     RET UNRESTRICTED;                                            0694 01129000
*   END TRIMMER;                                                   0695 01130000
@EL00005 DS    0H                                                  0695 01131000
@EF00005 DS    0H                                                  0695 01132000
@ER00005 BR    @14                                                 0695 01133000
*BEEFER:                                                           0696 01134000
*                                                                  0696 01135000
*/********************************************************************/ 01136000
*/*                                                                  */ 01137000
*/*   FUNCTION -  THIS ROUTINE INCREASES THE NUMBER OF DATA AND      */ 01138000
*/*               INDEX BUFFERS SUCH THAT THE BFSPACE PARAMETER      */ 01139000
*/*               WILL BE SATISFIED                                  */ 01140000
*/*   INPUT -     1. PROCESSING MODE FROM ACB                        */ 01141000
*/*               2. MINIMUM REQD DATA BUFFERS (MWARQDBF)            */ 01142000
*/*               3. MINIMUM REQD INDEX BUFFERS (MWARQIBF)           */ 01143000
*/*               4. CURRENT NUMBER OF DATA BUFFERS (MWABUFND)       */ 01144000
*/*               5. CURRENT NUMBER OF INDEX BUFFERS (MWABUFNI)      */ 01145000
*/*               6. DATA BUFFER SIZE (MWACISZD)                     */ 01146000
*/*               7. INDEX BUFFER SIZE (MWACISZI)                    */ 01147000
*/*               8. CURRENT MWABFSP REQD FOR MWABUFND AND MWABUFNI  */ 01148000
*/*   OUTPUT -    1. MWABUFND,MWABUFNI AND MWABUFSP WILL BE INCREASED*/ 01149000
*/*                                                                  */ 01150000
*/********************************************************************/ 01151000
*                                                                  0696 01152000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0696 01153000
BEEFER   DS    0H                                                  0697 01154000
*   RESPECIFY                                                      0697 01155000
*     RET RESTRICTED;                                              0697 01156000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 01157000
         ST    RET,MWASAVEB                                        0698 01158000
*   RESPECIFY                                                      0699 01159000
*     RET UNRESTRICTED;                                            0699 01160000
*   IF(ACBSEQ=OFF&ACBSKP=OFF) THEN  /* IF DIRECT PROCESSING          */ 01161000
         TM    ACBSEQ(RACB),B'00010000'                            0700 01162000
         BNZ   @RF00700                                            0700 01163000
         TM    ACBSKP(RACB),B'00010000'                            0700 01164000
         BNZ   @RF00700                                            0700 01165000
*     DO;                           /* INCREASE INDEX BUFFERS        */ 01166000
*       IF MWACISZI^=ZERO THEN      /* IF INDEX BUFFER SIZE NOT ZERO */ 01167000
         LTR   MWACISZI,MWACISZI                                   0702 01168000
         BZ    @RF00702                                            0702 01169000
*         DO;                                                      0703 01170000
*           MWABUFNI=MWABUFNI+(MWABFSP-MWABUFSP)/MWACISZI;/* SET   0704 01171000
*                                      INDEX BUFFER NUMBER           */ 01172000
         L     @00,MWABFSP                                         0704 01173000
         SLR   @00,MWABUFSP                                        0704 01174000
         SRDA  @00,32                                              0704 01175000
         DR    @00,MWACISZI                                        0704 01176000
         ALR   MWABUFNI,@01                                        0704 01177000
*           MWABUFSP=MWACISZI*MWABUFNI+MWABFSPD;/* UPDATE CURRENT  0705 01178000
*                                      BUFFER SPACE                  */ 01179000
         LR    @01,MWABUFNI                                        0705 01180000
         MR    @00,MWACISZI                                        0705 01181000
         AL    @01,MWABFSPD                                        0705 01182000
         LR    MWABUFSP,@01                                        0705 01183000
*         END;                                                     0706 01184000
*       ELSE                        /* IF INDEX BUFFER SIZE IS ZERO  */ 01185000
*         MWABUFND=MWABUFND+(MWABFSP-MWABUFSP)/MWACISZD;/* SET DATA     01186000
*                                      BUFFER NUMBER                 */ 01187000
         B     @RC00702                                            0707 01188000
@RF00702 L     @00,MWABFSP                                         0707 01189000
         SLR   @00,MWABUFSP                                        0707 01190000
         SRDA  @00,32                                              0707 01191000
         DR    @00,MWACISZD                                        0707 01192000
         ALR   MWABUFND,@01                                        0707 01193000
*     END;                                                         0708 01194000
*   ELSE                            /* IF NOT DIRECT PROCESSING      */ 01195000
*     DO;                                                          0709 01196000
         B     @RC00700                                            0709 01197000
@RF00700 DS    0H                                                  0710 01198000
*       IF MWACISZI<=MWABFSP-MWABUFSP THEN/* IS THERE ROOM FOR 1   0710 01199000
*                                      INDEX BUFFERS                 */ 01200000
         L     @15,MWABFSP                                         0710 01201000
         LR    @14,@15                                             0710 01202000
         SLR   @14,MWABUFSP                                        0710 01203000
         CR    MWACISZI,@14                                        0710 01204000
         BH    @RF00710                                            0710 01205000
*         DO;                                                      0711 01206000
*           MWABUFNI=MWABUFNI+ONE;  /* INCREASE INDEX BUFFER NUMBER  */ 01207000
         AL    MWABUFNI,@CF00059                                   0712 01208000
*           MWABUFSP=MWABUFSP+MWACISZI;/* UPDATE CURRENT BUFFER SPACE*/ 01209000
         ALR   MWABUFSP,MWACISZI                                   0713 01210000
*           MWABUFND=MWABUFND+(MWABFSP-MWABUFSP)/MWACISZD;/*       0714 01211000
*                                      REMAINING SPACE USED FOR DATA    01212000
*                                      BUFFERS                       */ 01213000
         LR    @00,@15                                             0714 01214000
         SLR   @00,MWABUFSP                                        0714 01215000
         SRDA  @00,32                                              0714 01216000
         DR    @00,MWACISZD                                        0714 01217000
         ALR   MWABUFND,@01                                        0714 01218000
*           MWABUFSP=(MWACISZD*MWABUFND)+(MWACISZI*MWABUFNI);/*    0715 01219000
*                                      UPDATE BUFFER SPACE           */ 01220000
         LR    @01,MWABUFND                                        0715 01221000
         MR    @00,MWACISZD                                        0715 01222000
         LR    @11,MWABUFNI                                        0715 01223000
         MR    @10,MWACISZI                                        0715 01224000
         ALR   @01,@11                                             0715 01225000
         LR    MWABUFSP,@01                                        0715 01226000
*           IF MWACISZI^=ZERO THEN  /* INDEX CI SIZE NOT ZERO        */ 01227000
         LTR   MWACISZI,MWACISZI                                   0716 01228000
         BZ    @RF00716                                            0716 01229000
*             MWABUFNI=MWABUFNI+(MWABFSP-MWABUFSP)/MWACISZI;/*     0717 01230000
*                                      REMAINING SPACE USED FOR INDEX   01231000
*                                      BUFFERS                       */ 01232000
         LR    @00,@15                                             0717 01233000
         SLR   @00,MWABUFSP                                        0717 01234000
         SRDA  @00,32                                              0717 01235000
         DR    @00,MWACISZI                                        0717 01236000
         ALR   MWABUFNI,@01                                        0717 01237000
*         END;                                                     0718 01238000
@RF00716 DS    0H                                                  0719 01239000
*     END;                                                         0719 01240000
@RF00710 DS    0H                                                  0720 01241000
*   RESPECIFY                                                      0720 01242000
*     RET RESTRICTED;                                              0720 01243000
@RC00700 DS    0H                                                  0721 01244000
*   RET=MWASAVEB;                   /* LOAD RETURN ADDRESS           */ 01245000
         L     RET,MWASAVEB                                        0721 01246000
*   RESPECIFY                                                      0722 01247000
*     RET UNRESTRICTED;                                            0722 01248000
*   END BEEFER;                                                    0723 01249000
@EL00006 DS    0H                                                  0723 01250000
@EF00006 DS    0H                                                  0723 01251000
@ER00006 BR    @14                                                 0723 01252000
*VIRTPROC:                                                         0724 01253000
*                                                                  0724 01254000
*/********************************************************************/ 01255000
*/*                                                                  */ 01256000
*/*       01 FUNCTION - ACQUIRE VIRTUAL DEVICES.                     */ 01257000
*/*       01 INPUT -    1. PARAMETER LIST IN MWAVIRT.                */ 01258000
*/*       01 OUTPUT -   1. REQUEST TO IDA0192D COMPLETED.            */ 01259000
*/*                                                                  */ 01260000
*/********************************************************************/ 01261000
*                                                                  0724 01262000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0724 01263000
VIRTPROC DS    0H                                                  0725 01264000
*   RESPECIFY                                                      0725 01265000
*    (R1,                                                          0725 01266000
*     RET,                                                         0725 01267000
*     RETCODE) RESTRICTED;                                         0725 01268000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 01269000
         ST    RET,MWASAVEA                                        0726 01270000
*   RESPECIFY                                                      0727 01271000
*     RET UNRESTRICTED;                                            0727 01272000
*   VIRTACB=RACB;                   /* ADDRESS OF USER ACB           */ 01273000
         LA    @14,MWAVIRT                                         0728 01274000
         ST    RACB,VIRTACB(,@14)                                  0728 01275000
*   VIRTTIOE=OPWTIOT;               /* POINT TO TIOT ENTRY   @XM02122*/ 01276000
         L     @05,OPWTIOT(,RCORE)                                 0729 01277000
         ST    @05,VIRTTIOE(,@14)                                  0729 01278000
*   VIRTACQ=ON;                     /* INDICATE ACQUIRE              */ 01279000
*   VIRTBACB=ON;                    /* INDICATE CALL BY ACB          */ 01280000
         OI    VIRTACQ(@14),B'10010000'                            0731 01281000
*   R1=ADDR(IDAVIRT);               /* POINT TO PARAMETER LIST       */ 01282000
         LR    R1,@14                                              0732 01283000
*   CALL IDA0192D;                  /* ACQUIRE VIRTUAL DEVICE        */ 01284000
         L     @15,@CV05384                                        0733 01285000
         BALR  @14,@15                                             0733 01286000
*   RSTATRTN=IDAR192B;              /* INDIC 192B IN CONTROL @Z30SSDP*/ 01287000
         L     @14,OPWCOMWA(,RCORE)                                0734 01288000
         MVC   RSTATRTN(4,@14),@CC04253                            0734 01289000
*   IF RETCODE^=ZERO THEN           /* ERROR IN IDA0192D             */ 01290000
         LTR   RETCODE,RETCODE                                     0735 01291000
         BZ    @RF00735                                            0735 01292000
*     DO;                                                          0736 01293000
*       MWAPDPRM=RETCODE;           /* SAVE RETURN CODE              */ 01294000
         ST    RETCODE,MWAPDPRM                                    0737 01295000
*       CALL PROBDT2B;              /* PROBLEM DETERMINATION         */ 01296000
         BAL   @14,PROBDT2B                                        0738 01297000
*     END;                                                         0739 01298000
*   RESPECIFY                                                      0740 01299000
*     RET RESTRICTED;                                              0740 01300000
@RF00735 DS    0H                                                  0741 01301000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 01302000
         L     RET,MWASAVEA                                        0741 01303000
*   RESPECIFY                                                      0742 01304000
*    (R1,                                                          0742 01305000
*     RET,                                                         0742 01306000
*     RETCODE) UNRESTRICTED;                                       0742 01307000
*   END VIRTPROC;                                                  0743 01308000
@EL00007 DS    0H                                                  0743 01309000
@EF00007 DS    0H                                                  0743 01310000
@ER00007 BR    @14                                                 0743 01311000
*PRFMT:                                                            0744 01312000
*                                                                  0744 01313000
*/********************************************************************/ 01314000
*/*                                                                  */ 01315000
*/*    01 FUNCTION - THE PURPOSE OF THIS ROUTINE IS TO INTERFACE     */ 01316000
*/*                  WITH RECORD MANAGEMENT TO DO PREFORMATTING AND  */ 01317000
*/*                  WITH IDA0192C UPDATE THE CATALOG TO INDICATE    */ 01318000
*/*                  THE PREFORMATTING                               */ 01319000
*/*    01 INPUT -    1. ADDRESS OF AMB IN MWAAMBPT                   */ 01320000
*/*                  2. SPEED BIT AMBSPEED                           */ 01321000
*/*                  3. ARDPRFMT BIT ON INDICATES PREFORMATTING REQD */ 01322000
*/*                  4. HIGH RBA ADDRESS                             */ 01323000
*/*    01 OUTPUT -   1. EXIT TO RECORD MANAGEMENT TO PREFORMAT       */ 01324000
*/*                  2. CALL IDA0192C TO INDICATE IN THE CATALOG     */ 01325000
*/*                     THAT THE DATA SET HAS BEEN PREFORMATTED      */ 01326000
*/*                                                                  */ 01327000
*/********************************************************************/ 01328000
*                                                                  0744 01329000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0744 01330000
PRFMT    DS    0H                                                  0745 01331000
*   RESPECIFY                                                      0745 01332000
*     RET RESTRICTED;                                              0745 01333000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 01334000
         ST    RET,MWASAVEA                                        0746 01335000
*   RESPECIFY                                                      0747 01336000
*     RET UNRESTRICTED;                                            0747 01337000
*   MWAAMBPT=AMBLDTA;               /* SET AMB POINTER               */ 01338000
         L     @05,ACBAMBL(,RACB)                                  0748 01339000
         L     @05,AMBLDTA(,@05)                                   0748 01340000
         ST    @05,MWAAMBPT                                        0748 01341000
*   MWAPFUNC=PDF63;                 /* SET FUNCTION CODE             */ 01342000
         MVI   MWAPFUNC,X'3F'                                      0749 01343000
*   OPWWRKPT=OPWDTWRK;              /* POINT TO DATA AMB WORK AREA   */ 01344000
         L     @05,OPWDTWRK(,RCORE)                                0750 01345000
         ST    @05,OPWWRKPT(,RCORE)                                0750 01346000
*   MWABYTES=((LENGTH(IFGRPL)+(FWORDLEN-ONE))&(-FWORDLEN))+LENGTH( 0751 01347000
*       MWAPFTSA);                  /* BUILD DUMMY RPL AND REGISTER     01348000
*                                      SAVE AREA                     */ 01349000
         MVC   MWABYTES(3),@CF04586+1                              0751 01350000
*   IF OPWCBIC=ON THEN              /* IF CBIC PROCESSING    @ZA16012*/ 01351000
         TM    OPWCBIC(RCORE),B'10000000'                          0752 01352000
         BNO   @RF00752                                            0752 01353000
*     DO;                           /* SET UP FOR GETCORE    @ZA16012*/ 01354000
*       MWAKEY=DXUKEY;              /* SET CALLER'S KEY      @ZA16012*/ 01355000
         L     @05,OPWCOMWA(,RCORE)                                0754 01356000
         MVC   MWAKEY(1),DXUKEY(@05)                               0754 01357000
*       MWASPOOL=GSRSPOOL;          /* SET CSA SUBPOOL       @ZA16012*/ 01358000
         MVI   MWASPOOL,X'F1'                                      0755 01359000
*     END;                          /* END SET UP            @ZA16012*/ 01360000
*   ELSE                            /* NOT CBIC              @ZA16012*/ 01361000
*     MWASPOOL=USERPOOL;            /* UNPROTECTED SUBPOOL FOR     0757 01362000
*                                      PROBLEM PROGRAM STATE         */ 01363000
         B     @RC00752                                            0757 01364000
@RF00752 MVI   MWASPOOL,X'FA'                                      0757 01365000
*   CALL GETCORE;                   /* BUILD DUMMY RPL               */ 01366000
@RC00752 BAL   @14,GETCORE                                         0758 01367000
*   MWARPLPT=MWADDRES;              /* POINT TO DUMMY RPL            */ 01368000
         L     @05,MWADDRES                                        0759 01369000
         ST    @05,MWARPLPT                                        0759 01370000
*   RPLDACB=RACB;                   /* INITIALIZE RPL ACB POINTER    */ 01371000
         ST    RACB,RPLDACB(,@05)                                  0760 01372000
*   RPLPLHPT=ADDR(IDAPLH);          /* POINT RPL TO PLH ENTRY        */ 01373000
         LA    @15,16                                              0761 01374000
         L     @14,MWAAMBPT                                        0761 01375000
         AL    @15,AMBPH(,@14)                                     0761 01376000
         ST    @15,RPLPLHPT(,@05)                                  0761 01377000
*   MWAPLHSV=PLHARDB;               /* SAVE ARDB POINTER             */ 01378000
         L     @05,PLHARDB(,@15)                                   0762 01379000
         ST    @05,MWAPLHSV                                        0762 01380000
*   DO WHILE MWAAMBPT^=ZERO;        /* PROCESS BOTH AMB'S            */ 01381000
         B     @DE00763                                            0763 01382000
@DL00763 DS    0H                                                  0764 01383000
*     MWARDBPT=AMDPARDB;            /* INITIALIZE POINTER TO 1ST ARDB*/ 01384000
         L     @05,MWAAMBPT                                        0764 01385000
         L     @01,AMBDSB(,@05)                                    0764 01386000
         L     @15,AMDPARDB(,@01)                                  0764 01387000
         ST    @15,MWARDBPT                                        0764 01388000
*     IF AMBSPEED=OFF THEN          /* IF NOT SPEED OPTION           */ 01389000
         TM    AMBSPEED(@05),B'00001000'                           0765 01390000
         BNZ   @RF00765                                            0765 01391000
*       DO WHILE MWARDBPT^=ZERO;    /* PROCESS ALL ARDBS             */ 01392000
         B     @DE00766                                            0766 01393000
@DL00766 DS    0H                                                  0767 01394000
*         IF ARDPRFMT=ON THEN       /* EXTENT NEED TO BE PREFORMATTED*/ 01395000
         L     @05,MWARDBPT                                        0767 01396000
         TM    ARDPRFMT(@05),B'01000000'                           0767 01397000
         BNO   @RF00767                                            0767 01398000
*           DO;                                                    0768 01399000
*             RPLDDDD=ARDHRBA;      /* INITIALIZE RBA WHERE FORMAT 0769 01400000
*                                      BEGINS                        */ 01401000
         L     @15,ARDHRBA(,@05)                                   0769 01402000
         L     @14,MWARPLPT                                        0769 01403000
         ST    @15,RPLDDDD(,@14)                                   0769 01404000
*             PLHARDB=ADDR(IDAARDB);/* INITIALIZE PLH ARDB           */ 01405000
         L     @01,MWAAMBPT                                        0770 01406000
         L     @01,AMBPH(,@01)                                     0770 01407000
         ST    @05,PLHARDB+16(,@01)                                0770 01408000
*             RESPECIFY                                            0771 01409000
*              (R0,                                                0771 01410000
*               R1,                                                0771 01411000
*               R6,                                                0771 01412000
*               R7) RESTRICTED;                                    0771 01413000
*             MWAPFMTS=ON;          /* SET LOOP BIT          @Z30SSDP*/ 01414000
         OI    MWAPFMTS,B'10000000'                                0772 01415000
*             DO WHILE(MWAPFMTS=ON);/* PREFMT PAGE SPACE     @Z30SSDP*/ 01416000
         B     @DE00773                                            0773 01417000
@DL00773 DS    0H                                                  0774 01418000
*               R6=R13;             /* COPY CURRENT ADDRESSABILITY   */ 01419000
         LR    R6,R13                                              0774 01420000
*               GEN(DROP R13);      /* REGISTER 13 DROPPED AS BASE   */ 01421000
         DROP R13                                                       01422000
*               GEN(USING @DATD,R6);/* USE REGISTER 6 AS BASE        */ 01423000
         USING @DATD,R6                                                 01424000
*               STM(RACB,R13,XRBESA);/* SAVE REGISTERS               */ 01425000
         L     @05,PSATOLD                                         0777 01426000
         L     @05,TCBRBP(,@05)                                    0777 01427000
         STM   RACB,R13,XRBESA(@05)                                0777 01428000
*               R1=MWARPLPT;        /* POINT TO DUMMY RPL            */ 01429000
         L     R1,MWARPLPT                                         0778 01430000
*               R13=(R1+LENGTH(IFGRPL)+(FWORDLEN-ONE))&(-FWORDLEN);/*   01431000
*                                      PT TO SAVE AREA ON FWORD BDY  */ 01432000
         LA    R13,79                                              0779 01433000
         ALR   R13,R1                                              0779 01434000
         N     R13,@CF05495                                        0779 01435000
*               IF AMBTYPE=AMBINDEX THEN/* IF PROCESSING INDEX       */ 01436000
         L     @05,MWAAMBPT                                        0780 01437000
         TM    AMBTYPE(@05),B'01000000'                            0780 01438000
         BNO   @RF00780                                            0780 01439000
*                 R0=RPLPFMTI;      /* SET INDICATOR FOR INDEX       */ 01440000
         LA    R0,9                                                0781 01441000
*               ELSE                /* IF PROCESSING DATA            */ 01442000
*                 R0=RPLPFMTD;      /* SET INDICATOR FOR DATA        */ 01443000
         B     @RC00780                                            0782 01444000
@RF00780 LA    R0,8                                                0782 01445000
*               RESPECIFY                                          0783 01446000
*                 R15 RESTRICTED;                                  0783 01447000
@RC00780 DS    0H                                                  0784 01448000
*               R15=BIBINTRF;       /* COPY ADDR OF INTERFACE ROUTINE*/ 01449000
         L     @05,ACBAMBL(,RACB)                                  0784 01450000
         L     @14,AMBLBIB(,@05)                                   0784 01451000
         L     R15,BIBINTRF(,@14)                                  0784 01452000
*               R7=ADDR(RBGRS2);    /* POINT TO USER REGISTER SAVE 0785 01453000
*                                      AREA                          */ 01454000
         L     @14,PSATOLD                                         0785 01455000
         L     R7,TCBRBP(,@14)                                     0785 01456000
         LA    R7,RBGRS2(,R7)                                      0785 01457000
*               IF AMBLCAT=ON|DXUKEY<MWAKEY8/* BRANCH ENTER          */ 01458000
*                 THEN              /* VSAM RCD MGMNT IF SPECIAL   0786 01459000
*                                      SYSTEM DATA SET OR KEY 0 TO 7    01460000
*                                      (THIS INCLUDES CBIC, GSR)   0786 01461000
*                                                            @ZA16012*/ 01462000
         TM    AMBLCAT(@05),B'00000010'                            0786 01463000
         BO    @RT00786                                            0786 01464000
         L     @14,OPWCOMWA(,RCORE)                                0786 01465000
         CLI   DXUKEY(@14),128                                     0786 01466000
         BNL   @RF00786                                            0786 01467000
@RT00786 DS    0H                                                  0787 01468000
*                 DO;                                              0787 01469000
*                   IF OPWCBIC=ON THEN/* IF CBIC PROCESSING  @ZA16012*/ 01470000
         TM    OPWCBIC(RCORE),B'10000000'                          0788 01471000
         BNO   @RF00788                                            0788 01472000
*                     DO;           /* CHANGE KEYS           @ZA16012*/ 01473000
*                       RFY                                        0790 01474000
*                         R1 RSTD;  /* USE THIS REG          @ZA16012*/ 01475000
*                       R1=DXUKEY;  /* SET CALLER'S KEY      @ZA16012*/ 01476000
         L     @14,OPWCOMWA(,RCORE)                                0791 01477000
         SLR   R1,R1                                               0791 01478000
         IC    R1,DXUKEY(,@14)                                     0791 01479000
*                       SPKA(R1);   /* CHANGE TO CALLER'S KEY      0792 01480000
*                                                            @ZA16012*/ 01481000
         SPKA  0(R1)                                               0792 01482000
*                       RFY                                        0793 01483000
*                         R1 UNRSTD;/* FREE REG              @ZA16012*/ 01484000
*                     END;          /* END CHANGE            @ZA16012*/ 01485000
*                   ELSE                                           0795 01486000
*                     ;             /* NOT CBIC              @ZA16012*/ 01487000
@RF00788 DS    0H                                                  0796 01488000
*                   GEN(LM RACB,R12,0(R7));/* RESTORE USER'S       0796 01489000
*                                      REGISTERS                     */ 01490000
         LM RACB,R12,0(R7)                                              01491000
*                   GEN(BALR RET,R15);/* REMAIN IN SUPERVISOR MODE   */ 01492000
         BALR RET,R15                                                   01493000
*                   GEN(DROP RBASEA);/* DROP PERMANENT BASE          */ 01494000
         DROP RBASEA                                                    01495000
*                   GEN(USING *,RET);/* SET UP TEMPORORY BASE        */ 01496000
         USING *,RET                                                    01497000
*                   GEN(SPKA  0);   /* GET INTO KEY ZERO     @Z30SSDT*/ 01498000
         SPKA  0                                                        01499000
*                 END;                                             0801 01500000
*               ELSE                /* IF NOT ACB FOR CATALOG        */ 01501000
*                 DO;                                              0802 01502000
         B     @RC00786                                            0802 01503000
@RF00786 DS    0H                                                  0803 01504000
*                   GEN(DROP RET);  /* DROP TEMPORORY BASE           */ 01505000
         DROP RET                                                       01506000
*                   GEN(USING @PSTART,RBASEA);/* RE-ESTABLISH BASE 0804 01507000
*                                      REG                           */ 01508000
         USING @PSTART,RBASEA                                           01509000
*                   GEN(LM RACB,R12,0(R7));/* RESTORE USER'S       0805 01510000
*                                      REGISTERS                     */ 01511000
         LM RACB,R12,0(R7)                                              01512000
*                   GEN(SYNCH (15));/* CHANGE TO PROBLEM PROGRAM   0806 01513000
*                                      STATE                         */ 01514000
         SYNCH (15)                                                     01515000
*                 END;                                             0807 01516000
*               LM(RACB,R13,XRBESA);/* RESTORE REGISTERS             */ 01517000
@RC00786 L     @14,PSATOLD                                         0808 01518000
         L     @01,TCBRBP(,@14)                                    0808 01519000
         LM    RACB,R13,XRBESA(@01)                                0808 01520000
*               GEN(DROP R6);       /* DROP TEMPORORY BASE REGISTER 6*/ 01521000
         DROP R6                                                        01522000
*               GEN(USING @DATD,R13);/* RESTORE REGISTER 13 AS BASE  */ 01523000
         USING @DATD,R13                                                01524000
*               IF RETCODE^=ZERO THEN/* IF ANY ERROR                 */ 01525000
         LTR   RETCODE,RETCODE                                     0811 01526000
         BZ    @RF00811                                            0811 01527000
*                 DO;                                              0812 01528000
*                   MWAPCODE=PP22;  /* SET ERROR CODE                */ 01529000
         MVI   MWAPCODE,X'16'                                      0813 01530000
*                   CALL PROBDT2B;  /* CALL PROBLEM DETERMINATION    */ 01531000
         BAL   @14,PROBDT2B                                        0814 01532000
*                 END;                                             0815 01533000
*               RESPECIFY                                          0816 01534000
*                 R15 UNRESTRICTED;                                0816 01535000
@RF00811 DS    0H                                                  0817 01536000
*               IF AMBPSDS=ON THEN  /* CHK FOR PAGE SPACE    @Z30SSDP*/ 01537000
         L     @05,MWAAMBPT                                        0817 01538000
         TM    AMBPSDS(@05),B'10000000'                            0817 01539000
         BNO   @RF00817                                            0817 01540000
*                 DO;               /* YES,                  @Z30SSDP*/ 01541000
*                   RPLDDDD=RPLDDDD+CTBYTAU;/* POINT TO NEXT CA    0819 01542000
*                                                            @Z30SSDP*/ 01543000
         L     @05,MWARPLPT                                        0819 01544000
         L     @01,OPWWRKPT(,RCORE)                                0819 01545000
         L     @15,RPLDDDD(,@05)                                   0819 01546000
