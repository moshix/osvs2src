*                                                                  1472 03630000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1472 03631000
ENQSAVE  DS    0H                                                  1473 03632000
*   RESPECIFY                                                      1473 03633000
*     IDAESL BASED(MWAESLPT);                                      1473 03634000
*   RESPECIFY                                                      1474 03635000
*     RET RESTRICTED;                                              1474 03636000
*   MWASAVEF=RET;                   /* SAVE RETURN ADDRESS           */ 03637000
         ST    RET,MWASAVEF                                        1475 03638000
*   RESPECIFY                                                      1476 03639000
*     RET UNRESTRICTED;                                            1476 03640000
*   MWAESLPT=OPWESL;                /* GET OPW ESL POINTER           */ 03641000
         L     MWAESLPT,OPWESL(,RCORE)                             1477 03642000
*   IF MWAESLPT=ZERO|               /* IF THERE IS NO ESL OR         */ 03643000
*       ESLACTEN=DIM(ESLENTRY) THEN /* IF ESL IS ALREADY FULL        */ 03644000
         LTR   MWAESLPT,MWAESLPT                                   1478 03645000
         BZ    @RT01478                                            1478 03646000
         CLC   ESLACTEN(2,MWAESLPT),@CH00146                       1478 03647000
         BNE   @RF01478                                            1478 03648000
@RT01478 DS    0H                                                  1479 03649000
*     DO;                                                          1479 03650000
*       MWABYTES=LENGTH(IDAESL);    /* LENGTH OF ENQ SAVE LIST       */ 03651000
         MVC   MWABYTES(3),@CF04592+1                              1480 03652000
*       MWASPOOL=WKPOOL;            /* SUBPOOL NUMBER                */ 03653000
         MVI   MWASPOOL,X'FC'                                      1481 03654000
*       CALL GETCORE;               /* BUILD NEW ESL                 */ 03655000
         BAL   @14,GETCORE                                         1482 03656000
*       MWADDRES->ESLNXPTR=MWAESLPT;/* CHAIN CURRENT ESL TO NEW ESL  */ 03657000
         L     @10,MWADDRES                                        1483 03658000
         ST    MWAESLPT,ESLNXPTR(,@10)                             1483 03659000
*       MWAESLPT=MWADDRES;          /* NEW ESL BECOMES CURRENT ESL   */ 03660000
         LR    MWAESLPT,@10                                        1484 03661000
*       OPWESL=MWAESLPT;            /* INITIALIZE ESL POINTER IN OPW */ 03662000
         ST    MWAESLPT,OPWESL(,RCORE)                             1485 03663000
*       ESLSUBPL=WKPOOL;            /* SUBPOOL NUMBER FOR ESL        */ 03664000
         MVI   ESLSUBPL(MWAESLPT),X'FC'                            1486 03665000
*       ESLLENTH=MWABYTES;          /* LENGTH OF THIS ESL            */ 03666000
         MVC   ESLLENTH(3,MWAESLPT),MWABYTES                       1487 03667000
*       ESLID=IDAESLID;             /* ID FOR THIS BLOCK             */ 03668000
         MVC   ESLID(8,MWAESLPT),@CC04942                          1488 03669000
*     END;                                                         1489 03670000
*   ESLACTEN=ESLACTEN+ONE;          /* INCREMENT NUMBER OF ENTRIES   */ 03671000
@RF01478 LA    @10,1                                               1490 03672000
         AH    @10,ESLACTEN(,MWAESLPT)                             1490 03673000
         STH   @10,ESLACTEN(,MWAESLPT)                             1490 03674000
*   ENQSHR=OFF;                     /* DEQ DOES NOT NEED SHR BIT     */ 03675000
         NI    ENQSHR,B'01111111'                                  1491 03676000
