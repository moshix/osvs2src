*   RESPECIFY                                                      1393 03343000
*     RET UNRESTRICTED;                                            1393 03344000
*   ENQRNIND='I';                   /* RNAME TYPE IN USE             */ 03345000
         MVI   ENQRNIND,C'I'                                       1394 03346000
*   ENQOPT2=ENQOPT2&&ENQOPT2;       /* ZERO OPTION BYTE              */ 03347000
         XC    ENQOPT2(1),ENQOPT2                                  1395 03348000
*   GEN(ENQ (,,E,,SYSTEM),RET=USE,MF=(E,ENQENTRY));/* ENQ IF REGION     03349000
*                                      DOES NOT HAVE RESOURCES       */ 03350000
         ENQ (,,E,,SYSTEM),RET=USE,MF=(E,ENQENTRY)                      03351000
*   IF RETCODE=ZERO THEN            /* IF ENQ SUCCESSFUL             */ 03352000
         LTR   RETCODE,RETCODE                                     1397 03353000
         BNZ   @RF01397                                            1397 03354000
*     CALL ENQSAVE;                 /* RECORD THE ENQ                */ 03355000
         BAL   @14,ENQSAVE                                         1398 03356000
*   ELSE                            /* IF ENQ FAILED                 */ 03357000
*     RETCODE=ENQRETC;              /* SAVE ENQ RETURN CODE          */ 03358000
         B     @RC01397                                            1399 03359000
@RF01397 SLR   RETCODE,RETCODE                                     1399 03360000
