         ALR   @00,MWACISZI                                        0676 01045000
         BCTR  @00,0                                               0676 01046000
         SLR   @00,@10                                             0676 01047000
         SRDA  @00,32                                              0676 01048000
         DR    @00,MWACISZI                                        0676 01049000
         LCR   @01,@01                                             0676 01050000
         ALR   @01,MWABUFNI                                        0676 01051000
         L     @07,MWARQIBF                                        0676 01052000
         CR    @07,@01                                             0676 01053000
         BNL   *+6                                                      01054000
         LR    @07,@01                                             0676 01055000
         LR    MWABUFNI,@07                                        0676 01056000
*     END;                                                         0677 01057000
*   ELSE                            /* NOT DIRECT PROCESSING         */ 01058000
*     DO;                                                          0678 01059000
         B     @RC00671                                            0678 01060000
@RF00671 DS    0H                                                  0679 01061000
*       IF MWACISZI^=ZERO THEN      /* IF INDEX BUFFER SIZE NOT ZERO */ 01062000
         LTR   MWACISZI,MWACISZI                                   0679 01063000
         BZ    @RF00679                                            0679 01064000
*         DO;                                                      0680 01065000
*           MWABUFNI=MAX(MWARQIBF+ONE,(MWABUFNI-(MWABUFSP+MWACISZI-ONE- 01066000
*               MWABUFSP)/MWACISZI));/* DECREASE INDEX BUFFERS       */ 01067000
         LA    @10,1                                               0681 01068000
         AL    @10,MWARQIBF                                        0681 01069000
         LR    @00,MWABUFSP                                        0681 01070000
         ALR   @00,MWACISZI                                        0681 01071000
         BCTR  @00,0                                               0681 01072000
