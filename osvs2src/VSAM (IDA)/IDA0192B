*                                                                  1117 02471000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1117 02472000
VOLPROC  DS    0H                                                  1118 02473000
*   RESPECIFY                                                      1118 02474000
*     RET RESTRICTED;                                              1118 02475000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 02476000
         ST    RET,MWASAVEA                                        1119 02477000
*   RESPECIFY                                                      1120 02478000
*     RET UNRESTRICTED;                                            1120 02479000
*   MWAPFUNC=PDF62;                 /* SET FUNCTION CODE             */ 02480000
         MVI   MWAPFUNC,X'3E'                                      1121 02481000
*   OPWWRKPT=OPWDTWRK;              /* POINT TO AMB WORK AREA OF DATA   02482000
*                                      AMB                           */ 02483000
         L     @05,OPWDTWRK(,RCORE)                                1122 02484000
         ST    @05,OPWWRKPT(,RCORE)                                1122 02485000
*   CALL VMTPROC;                   /* PROCESS VMT                   */ 02486000
         BAL   @14,VMTPROC                                         1123 02487000
*   IF OPWIXWRK^=ZERO THEN          /* IF KSDS                       */ 02488000
