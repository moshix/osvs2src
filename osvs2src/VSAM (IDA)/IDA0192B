         MVC   MWADEVTP(3),CTDEVTYP+1(@01)                         1142 02548000
*   MWADVOPT=MWADVOPT&MWAOMASK;     /* RESET EXTRANEOUS BITS         */ 02549000
         NI    MWADVOPT,B'00001000'                                1143 02550000
*   CALL VMTVOLSR;                  /* CALL VMT SCAN AND VOLUME    1144 02551000
*                                      SERIAL NUMBER SEARCH          */ 02552000
         BAL   @14,VMTVOLSR                                        1144 02553000
*   IF MWARETCD=FOUR THEN           /* IF VMT DEVICE TYPE NOT FOUND  */ 02554000
         CLC   MWARETCD(4),@CF00106                                1145 02555000
         BNE   @RF01145                                            1145 02556000
*     DO;                                                          1146 02557000
*       IF OPWIXDT=ON THEN          /* IF INDEX DATA SET OPEN AS ESDS*/ 02558000
         TM    OPWIXDT(RCORE),B'01000000'                          1147 02559000
         BNO   @RF01147                                            1147 02560000
*         DO;                                                      1148 02561000
*           MWAVMTPT=BIBVMT;        /* POINT TO FIRST VMT            */ 02562000
         L     @01,ACBAMBL(,RACB)                                  1149 02563000
         L     @01,AMBLBIB(,@01)                                   1149 02564000
         L     MWAVMTPT,BIBVMT(,@01)                               1149 02565000
*           IF VMTDEV=MWADEVTP THEN /* IF DEVICE TYPE MATCH          */ 02566000
         CLC   VMTDEV(3,MWAVMTPT),MWADEVTP                         1150 02567000
         BNE   @RF01150                                            1150 02568000
*             MWAVMTPT=VMTNXT;      /* POINT TO NEXT VMT             */ 02569000
         L     MWAVMTPT,VMTNXT(,MWAVMTPT)                          1151 02570000
*           IF MWAVMTPT^=ZERO THEN  /* IF FOUND                      */ 02571000
@RF01150 LTR   MWAVMTPT,MWAVMTPT                                   1152 02572000
         BZ    @RF01152                                            1152 02573000
*             CALL VOLSRCH;         /* SEARCH FOR VOL SER MATCH      */ 02574000
         BAL   @14,VOLSRCH                                         1153 02575000
*         END;                                                     1154 02576000
*       ELSE                        /* INDEX NOT OPENNED AS ESDS     */ 02577000
*         DO;                                                      1155 02578000
         B     @RC01147                                            1155 02579000
@RF01147 DS    0H                                                  1156 02580000
*           IF CTINDXSS=ON THEN     /* IF INDEX SEQUENCE SET WITH  1156 02581000
*                                      DATA                          */ 02582000
