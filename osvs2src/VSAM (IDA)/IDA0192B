         LTR   @05,@05                                             1296 03055000
         BZ    @RF01296                                            1296 03056000
         L     @01,OPWCURPT(,RCORE)                                1296 03057000
         TM    OPWSVWRK(@01),B'00010000'                           1296 03058000
         BNZ   @RF01296                                            1296 03059000
*         DO;                                                      1297 03060000
*           MWADDRES=OPWIXWRK;      /* POINT TO INDEX AMB WORK AREA  */ 03061000
         ST    @05,MWADDRES                                        1298 03062000
*           MWABYTES=OPWIXWRK->AMALEN;/* SIZE OF INDEX AMB WORK AREA */ 03063000
         LH    @05,AMALEN(,@05)                                    1299 03064000
         STCM  @05,7,MWABYTES                                      1299 03065000
*           CALL FREECORE;          /* FREE INDEX AMB WORK AREA      */ 03066000
         BAL   @14,FREECORE                                        1300 03067000
*           OPWIXWRK=ZERO;          /* ZERO PTR TO INDEX AMB WORK  1301 03068000
*                                      AREA                          */ 03069000
         SLR   @05,@05                                             1301 03070000
         ST    @05,OPWIXWRK(,RCORE)                                1301 03071000
*         END;                                                     1302 03072000
*     END;                                                         1303 03073000
@RF01296 DS    0H                                                  1304 03074000
*   RESPECIFY                                                      1304 03075000
*     RETCODE RESTRICTED;                                          1304 03076000
@RF01278 DS    0H                                                  1305 03077000
*   IF OPWMODWK=OFF THEN            /* MODULE WORK AREA DOES NOT   1305 03078000
*                                      EXIST                         */ 03079000
         L     @14,OPWCURPT(,RCORE)                                1305 03080000
         TM    OPWMODWK(@14),B'01000000'                           1305 03081000
         BNZ   @RF01305                                            1305 03082000
*     RETCODE=EIGHT;                /* INDICATE TERMINATING ERROR    */ 03083000
         LA    RETCODE,8                                           1306 03084000
*   ELSE                            /* IF MODULE WORK AREA EXIST     */ 03085000
*     DO;                                                          1307 03086000
         B     @RC01305                                            1307 03087000
@RF01305 DS    0H                                                  1308 03088000
*       IF OPWEMPUP=OFF&OPWERR2B=OFF THEN/* IF OPEN CLUSTER        1308 03089000
*                                      SUCCESSFUL                    */ 03090000
         L     @14,OPWCURPT(,RCORE)                                1308 03091000
         TM    OPWEMPUP(@14),B'00110000'                           1308 03092000
         BNZ   @RF01308                                            1308 03093000
*         DO;                                                      1309 03094000
*           OPWVSMPL=OPWVSMPL&&OPWVSMPL;/* ZERO IDA0192M PARM LIST   */ 03095000
         XC    OPWVSMPL(16,RCORE),OPWVSMPL(RCORE)                  1310 03096000
*           OPWVMANC=AMBLCMB;       /* POINT TO THE ANCHOR BLOCK   1311 03097000
*                                      (CMB)                         */ 03098000
         L     @14,ACBAMBL(,RACB)                                  1311 03099000
         L     @14,AMBLCMB(,@14)                                   1311 03100000
         ST    @14,OPWVMANC(,RCORE)                                1311 03101000
*           OPWVMTYP=FREECLSP;      /* INDICATE TYPE OF FUNCTION     */ 03102000
         MVI   OPWVMTYP(RCORE),X'82'                               1312 03103000
*           CALL IDA0192M;          /* FREE EXCESS SPACE IN CLUSTER     03104000
*                                      BLK                           */ 03105000
         L     @15,@CV05450                                        1313 03106000
         BALR  @14,@15                                             1313 03107000
*         END;                                                     1314 03108000
*       RESPECIFY                                                  1315 03109000
*        (R0,                                                      1315 03110000
*         R1) RESTRICTED;                                          1315 03111000
@RF01308 DS    0H                                                  1316 03112000
*       MWASPOOL=WKPOOL;            /* SUBPOOL NUMBER                */ 03113000
         MVI   MWASPOOL,X'FC'                                      1316 03114000
*       GEN(L R1,@SIZDATD);         /* SIZE OF MWA                   */ 03115000
         L R1,@SIZDATD                                                  03116000
*       MWABYTES=R1;                                               1318 03117000
         STCM  R1,7,MWABYTES                                       1318 03118000
*       R1=R13;                     /* ADDRESS OF MWA                */ 03119000
         LR    R1,R13                                              1319 03120000
*       R0=MWAMNWRD;                /* GET SUBPOOL NO. AND NO. OF  1320 03121000
*                                      BYTES                         */ 03122000
         L     R0,MWAMNWRD                                         1320 03123000
*       GEN(L R13,4(R13));          /* GET ADDR OF PREVIOUS SAVE AREA*/ 03124000
         L R13,4(R13)                                                   03125000
*       GEN(FREEMAIN R,LV=(R0),A=(R1));/* FREE MODULE WORK AREA      */ 03126000
         FREEMAIN R,LV=(R0),A=(R1)                                      03127000
*       RESPECIFY                                                  1323 03128000
*        (R0,                                                      1323 03129000
*         R1) UNRESTRICTED;                                        1323 03130000
*       IF OPWEMPUP=ON THEN         /* EMPTY UPGRADE AIX'S           */ 03131000
         L     @14,OPWCURPT(,RCORE)                                1324 03132000
         TM    OPWEMPUP(@14),B'00100000'                           1324 03133000
         BNO   @RF01324                                            1324 03134000
*         RETCODE=FOUR;             /* SET WARNING CODE              */ 03135000
         LA    RETCODE,4                                           1325 03136000
*       ELSE                        /* NOT EMPTY UPGRADE AIX'S       */ 03137000
*         IF OPWERR2B=ON THEN       /* IF OPEN FAILED                */ 03138000
         B     @RC01324                                            1326 03139000
@RF01324 L     @14,OPWCURPT(,RCORE)                                1326 03140000
         TM    OPWERR2B(@14),B'00010000'                           1326 03141000
         BNO   @RF01326                                            1326 03142000
*           RETCODE=EIGHT;          /* INDICATE TERMINATING ERROR    */ 03143000
         LA    RETCODE,8                                           1327 03144000
*         ELSE                      /* OPEN CLUSTER SUCCESSFUL       */ 03145000
*           RETCODE=ZERO;           /* SET RETURN CODE TO ZERO       */ 03146000
         B     @RC01326                                            1328 03147000
@RF01326 SLR   RETCODE,RETCODE                                     1328 03148000
*     END;                                                         1329 03149000
@RC01326 DS    0H                                                  1329 03150000
@RC01324 DS    0H                                                  1330 03151000
*   RESPECIFY                                                      1330 03152000
*     RET RESTRICTED;                                              1330 03153000
@RC01305 DS    0H                                                  1331 03154000
*   RET=OPWSAVEA;                   /* GET RETURN ADDRESS            */ 03155000
         L     RET,OPWSAVEA(,RCORE)                                1331 03156000
*   RESPECIFY                                                      1332 03157000
*    (RET,                                                         1332 03158000
*     RETCODE) UNRESTRICTED;                                       1332 03159000
*   END TERM192B;                                                  1333 03160000
@EL00021 DS    0H                                                  1333 03161000
@EF00021 DS    0H                                                  1333 03162000
@ER00021 BR    @14                                                 1333 03163000
*CATUPDT:                                                          1334 03164000
*                                                                  1334 03165000
*/********************************************************************/ 03166000
*/*                                                                  */ 03167000
*/*      01 FUNCTION - UPDATE CATALOG BY WRITING NEW OPEN INDICATOR. */ 03168000
*/*      01 INPUT -    1. PARAMETER LIST FOR CATALOG (MWACATPM).     */ 03169000
*/*                    2. POINTERS TO AMB'S IN AMBLDTA AND AMBLIX.   */ 03170000
*/*                    3. INDICATORS TO INDICATE IF DATA OR INDEX    */ 03171000
*/*                       COMPONENT NEEDS THE CATALOG UPDATED.       */ 03172000
*/*      01 OUTPUT -   1. CATALOG UPDATED.                           */ 03173000
*/*                    2. AMBOPEN BIT SET ON IN THE AMB'S.           */ 03174000
*/*                                                                  */ 03175000
*/********************************************************************/ 03176000
*                                                                  1334 03177000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1334 03178000
CATUPDT  DS    0H                                                  1335 03179000
*   RESPECIFY                                                      1335 03180000
*     RET RESTRICTED;                                              1335 03181000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 03182000
         ST    RET,MWASAVEB                                        1336 03183000
*   RESPECIFY                                                      1337 03184000
*     RET UNRESTRICTED;                                            1337 03185000
*   MWACATID=OPO;                   /* SET CALLER ID FOR IDA0192C    */ 03186000
         MVI   MWACATID,X'0C'                                      1338 03187000
*   MWABTOPN=ON;                    /* SET OPEN INDICATOR BIT FOR  1339 03188000
*                                      192C                          */ 03189000
         OI    MWABTOPN,B'10000000'                                1339 03190000
*   RESPECIFY                                                      1340 03191000
*    (R1,                                                          1340 03192000
*     R5,                                                          1340 03193000
*     RETCODE) RESTRICTED;                                         1340 03194000
*   R1=ADDR(MWACATPM);              /* POINT TO CATALOG PARAMETER  1341 03195000
*                                      LIST                          */ 03196000
         LA    R1,MWACATPM                                         1341 03197000
*   IF MWACATDT=ON THEN             /* DATA COMPONENT NEEDS UPDATE   */ 03198000
         TM    MWACATDT,B'10000000'                                1342 03199000
         BNO   @RF01342                                            1342 03200000
*     DO;                                                          1343 03201000
*       R5=AMBLDTA;                 /* POINT TO DATA AMB             */ 03202000
         L     @14,ACBAMBL(,RACB)                                  1344 03203000
         L     R5,AMBLDTA(,@14)                                    1344 03204000
*       OPWWRKPT=OPWDTWRK;          /* POINT TO DATA AMB WORK AREA   */ 03205000
         L     @14,OPWDTWRK(,RCORE)                                1345 03206000
         ST    @14,OPWWRKPT(,RCORE)                                1345 03207000
*       RSTATRTN=IDAR192C;          /* NOTE CALL TO 192C     @Z30SSDP*/ 03208000
         L     @14,OPWCOMWA(,RCORE)                                1346 03209000
         MVC   RSTATRTN(4,@14),@CC04255                            1346 03210000
*       CALL IDA0192C;              /* WRITE THE OPEN INDICATOR      */ 03211000
         L     @15,@CV05359                                        1347 03212000
         BALR  @14,@15                                             1347 03213000
*       RSTATRTN=IDAR192B;          /* INDIC 192B CONTROL    @Z30SSDP*/ 03214000
         L     @14,OPWCOMWA(,RCORE)                                1348 03215000
         MVC   RSTATRTN(4,@14),@CC04253                            1348 03216000
*       IF RETCODE^=ZERO THEN       /* ERROR                         */ 03217000
         LTR   RETCODE,RETCODE                                     1349 03218000
         BZ    @RF01349                                            1349 03219000
*         DO;                                                      1350 03220000
*           MWAPDPRM=RETCODE;       /* SAVE ERROR CODES              */ 03221000
         ST    RETCODE,MWAPDPRM                                    1351 03222000
*           CALL PROBDT2B;          /* PROBLEM DETERMINATION         */ 03223000
         BAL   @14,PROBDT2B                                        1352 03224000
*         END;                                                     1353 03225000
*       OPWDOPEN=ON;                /* IND DATA OPEN INDICATOR     1354 03226000
*                                      UPDATED                       */ 03227000
@RF01349 L     @14,OPWCURPT(,RCORE)                                1354 03228000
         OI    OPWDOPEN(@14),B'10000000'                           1354 03229000
*     END;                                                         1355 03230000
*   IF MWACATIX=ON THEN             /* INDEX COMPONENT NEEDS UPDATE  */ 03231000
@RF01342 TM    MWACATIX,B'10000000'                                1356 03232000
         BNO   @RF01356                                            1356 03233000
*     DO;                                                          1357 03234000
*       R5=AMBLIX;                  /* ADDRESS OF INDEX AMB          */ 03235000
         L     @14,ACBAMBL(,RACB)                                  1358 03236000
         L     R5,AMBLIX(,@14)                                     1358 03237000
*       OPWWRKPT=OPWIXWRK;          /* POINT TO INDEX AMB WORK AREA  */ 03238000
         L     @14,OPWIXWRK(,RCORE)                                1359 03239000
         ST    @14,OPWWRKPT(,RCORE)                                1359 03240000
*       RSTATRTN=IDAR192C;          /* NOTE CALL TO 192C     @Z30SSDP*/ 03241000
         L     @14,OPWCOMWA(,RCORE)                                1360 03242000
         MVC   RSTATRTN(4,@14),@CC04255                            1360 03243000
*       CALL IDA0192C;              /* WRITE OPEN INDICATOR          */ 03244000
         L     @15,@CV05359                                        1361 03245000
         BALR  @14,@15                                             1361 03246000
*       RSTATRTN=IDAR192B;          /* INDIC 192B IN CONTROL @Z30SSDP*/ 03247000
         L     @14,OPWCOMWA(,RCORE)                                1362 03248000
         MVC   RSTATRTN(4,@14),@CC04253                            1362 03249000
*       IF RETCODE^=ZERO THEN       /* ERROR                         */ 03250000
         LTR   RETCODE,RETCODE                                     1363 03251000
         BZ    @RF01363                                            1363 03252000
*         DO;                                                      1364 03253000
*           MWAPDPRM=RETCODE;       /* SAVE ERROR CODE               */ 03254000
         ST    RETCODE,MWAPDPRM                                    1365 03255000
*           CALL PROBDT2B;          /* PROBLEM DETERMINATION         */ 03256000
         BAL   @14,PROBDT2B                                        1366 03257000
