@RF01184 DS    0H                                                  1187 02677000
*         MWAINDFG=OFF;             /* RESET INDICATOR FLAG          */ 02678000
         NI    MWAINDFG,B'01111111'                                1187 02679000
*         CALL VOLSRCH;             /* TRY TO MATCH VOL SER NUMBER   */ 02680000
         BAL   @14,VOLSRCH                                         1188 02681000
*         IF MWALLMNT=ON THEN       /* IF ALL VOLUMES MOUNTED        */ 02682000
         TM    MWALLMNT,B'10000000'                                1189 02683000
         BNO   @RF01189                                            1189 02684000
*           MWARETCD=EIGHT;         /* SET RETCODE TO INDICATE SUCH  */ 02685000
         MVC   MWARETCD(4),@CF00065                                1190 02686000
*         ELSE                      /* NOT ALL VOLUMES MOUNTED       */ 02687000
*           MWAVMTPT=VMTNXT;        /* SEARCH THE REST OF THE VMT'S  */ 02688000
         B     @RC01189                                            1191 02689000
@RF01189 L     MWAVMTPT,VMTNXT(,MWAVMTPT)                          1191 02690000
*       END;                                                       1192 02691000
@RC01189 DS    0H                                                  1193 02692000
*   END;                                                           1193 02693000
@RC01184 DS    0H                                                  1193 02694000
@DE01175 L     @15,MWARETCD                                        1193 02695000
         LTR   @15,@15                                             1193 02696000
         BZ    @DL01175                                            1193 02697000
*   RESPECIFY                                                      1194 02698000
*     RET RESTRICTED;                                              1194 02699000
*   RET=MWASAVEC;                   /* GET RETURN ADDRESS            */ 02700000
         L     RET,MWASAVEC                                        1195 02701000
*   RESPECIFY                                                      1196 02702000
*     RET UNRESTRICTED;                                            1196 02703000
*   END VMTVOLSR;                                                  1197 02704000
@EL00018 DS    0H                                                  1197 02705000
@EF00018 DS    0H                                                  1197 02706000
@ER00018 BR    @14                                                 1197 02707000
*VOLSRCH:                                                          1198 02708000
*                                                                  1198 02709000
*/********************************************************************/ 02710000
*/*                                                                  */ 02711000
*/*      01 FUNCTION - SEARCH FOR VOLUME SERIAL NUMBER MATCH.        */ 02712000
*/*                    IF FOUND INCREMENT VMT USE COUNT BY ONE.      */ 02713000
*/*      01 INPUT -    1. VOLUME MOUNT TABLE.                        */ 02714000
*/*                    2. ADDRESS OF AMB WORK AREA IN OPWWRKPT.      */ 02715000
*/*      01 OUTPUT -   1. MWALLMNT=ON IF ALL VOLUMES ARE MOUNTED.    */ 02716000
*/*                                                                  */ 02717000
*/********************************************************************/ 02718000
*                                                                  1198 02719000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1198 02720000
VOLSRCH  DS    0H                                                  1199 02721000
*   RESPECIFY                                                      1199 02722000
*     RET RESTRICTED;                                              1199 02723000
*   MWASAVED=RET;                   /* SAVE RETURN ADDRESS           */ 02724000
         ST    RET,MWASAVED                                        1200 02725000
*   RESPECIFY                                                      1201 02726000
*     RET UNRESTRICTED;                                            1201 02727000
*   MWALLMNT=ON;                    /* INDICATE ALL VOLUMES MOUNTED  */ 02728000
         OI    MWALLMNT,B'10000000'                                1202 02729000
*   MWACTVOL=ADDR(AMBWORK)+LENGTH(AMBWORK);/* PT TO 1ST VOLUME ENTRY */ 02730000
         LA    MWACTVOL,232                                        1203 02731000
         AL    MWACTVOL,OPWWRKPT(,RCORE)                           1203 02732000
*   DO MWAINDEX=1 TO VOLENTNO;      /* LOOK AT ALL THE VOLUME ENTRIES*/ 02733000
         LA    MWAINDEX,1                                          1204 02734000
         B     @DE01204                                            1204 02735000
@DL01204 DS    0H                                                  1205 02736000
*     MWASWTCH=OFF;                 /* INITIALIZE SWITCH TO OFF      */ 02737000
         NI    MWASWTCH,B'01111111'                                1205 02738000
*     IF CTVOLMT=OFF THEN           /* IF MOUNT BIT IS OFF           */ 02739000
         TM    CTVOLMT(MWACTVOL),B'10000000'                       1206 02740000
         BNZ   @RF01206                                            1206 02741000
*       DO;                                                        1207 02742000
*         DO MWAIND=1 TO VMTNOVOL WHILE MWASWTCH=OFF;/* LOOP THROUGH    02743000
*                                      VMT ENTRIES                   */ 02744000
         LA    MWAIND,1                                            1208 02745000
         B     @DE01208                                            1208 02746000
@DL01208 TM    MWASWTCH,B'10000000'                                1208 02747000
         BNZ   @DC01208                                            1208 02748000
*           IF CTVOLSER=VMTVLSER(MWAIND) THEN/* MATCH VOL SER NO.    */ 02749000
         LR    @09,MWAIND                                          1209 02750000
         SLA   @09,4                                               1209 02751000
         ST    @09,@TF00001                                        1209 02752000
         ALR   @09,MWAVMTPT                                        1209 02753000
         CLC   CTVOLSER(6,MWACTVOL),VMTVLSER-16(@09)               1209 02754000
         BNE   @RF01209                                            1209 02755000
*             DO;                                                  1210 02756000
*               MWASWTCH=ON;        /* SET SWITCH TO INDICATE        */ 02757000
         OI    MWASWTCH,B'10000000'                                1211 02758000
*               VMTUSECT(MWAIND)=VMTUSECT(MWAIND)+ONE;/* INCREASE VMT   02759000
*                                      USE COUNT BY ONE              */ 02760000
         LA    @15,1                                               1212 02761000
         L     @14,@TF00001                                        1212 02762000
         AL    @15,VMTUSECT-16(@14,MWAVMTPT)                       1212 02763000
         L     @14,@TF00001                                        1212 02764000
         ST    @15,VMTUSECT-16(@14,MWAVMTPT)                       1212 02765000
*               CTVOLMT=ON;         /* INDICATE VOLUME MOUNTED       */ 02766000
         OI    CTVOLMT(MWACTVOL),B'10000000'                       1213 02767000
*               CTVOLUCB=VMTUCB(MWAIND);/* UCB ADDR TO AMB WK AREA   */ 02768000
         L     @01,@TF00001                                        1214 02769000
         L     @15,VMTUCB-16(@01,MWAVMTPT)                         1214 02770000
         ST    @15,CTVOLUCB(,MWACTVOL)                             1214 02771000
*               MWAUCBPT=VMTUCB(MWAIND);/* POINT TO UCB              */ 02772000
         LR    MWAUCBPT,@15                                        1215 02773000
*             END;                                                 1216 02774000
*         END;                                                     1217 02775000
@RF01209 AL    MWAIND,@CF00059                                     1217 02776000
@DE01208 CH    MWAIND,VMTNOVOL(,MWAVMTPT)                          1217 02777000
         BNH   @DL01208                                            1217 02778000
@DC01208 DS    0H                                                  1218 02779000
*         IF MWASWTCH=OFF THEN      /* VOL SER NUMBER DOES NOT MATCH */ 02780000
         TM    MWASWTCH,B'10000000'                                1218 02781000
         BNZ   @RF01218                                            1218 02782000
*           MWALLMNT=OFF;           /* INDICATE NOT ALL VOLUME     1219 02783000
*                                      MOUNTED                       */ 02784000
         NI    MWALLMNT,B'01111111'                                1219 02785000
*       END;                                                       1220 02786000
@RF01218 DS    0H                                                  1221 02787000
*     MWACTVOL=MWACTVOL+LENGTH(CTVOLENT)+/* PT TO NEXT VOLUME ENTRY  */ 02788000
*         (LENGTH(CTKEYENT)+CTKEYLEN*TWO)*CTNOKEY+LENGTH(CTEXTENT)*     02789000
*         CTNOEXT;                                                 1221 02790000
@RF01206 LA    @09,20                                              1221 02791000
         LR    @07,MWACTVOL                                        1221 02792000
         ALR   @07,@09                                             1221 02793000
         LH    @01,CTKEYLEN(,MWACTVOL)                             1221 02794000
         ALR   @01,@01                                             1221 02795000
         AL    @01,@CF00146                                        1221 02796000
         MH    @01,CTNOKEY(,MWACTVOL)                              1221 02797000
         ALR   @07,@01                                             1221 02798000
         LR    @01,@09                                             1221 02799000
         SLR   @09,@09                                             1221 02800000
         IC    @09,CTNOEXT(,MWACTVOL)                              1221 02801000
         MR    @00,@09                                             1221 02802000
         ALR   @07,@01                                             1221 02803000
         LR    MWACTVOL,@07                                        1221 02804000
*   END;                                                           1222 02805000
         AL    MWAINDEX,@CF00059                                   1222 02806000
@DE01204 L     @09,OPWWRKPT(,RCORE)                                1222 02807000
         CH    MWAINDEX,VOLENTNO(,@09)                             1222 02808000
         BNH   @DL01204                                            1222 02809000
*   RESPECIFY                                                      1223 02810000
*     RET RESTRICTED;                                              1223 02811000
*   RET=MWASAVED;                   /* GET RETURN ADDRESS            */ 02812000
         L     RET,MWASAVED                                        1224 02813000
*   RESPECIFY                                                      1225 02814000
*     RET UNRESTRICTED;                                            1225 02815000
*   END VOLSRCH;                                                   1226 02816000
@EL00019 DS    0H                                                  1226 02817000
@EF00019 DS    0H                                                  1226 02818000
@ER00019 BR    @14                                                 1226 02819000
*PROBDT2B:                                                         1227 02820000
*                                                                  1227 02821000
*/********************************************************************/ 02822000
*/*                                                                  */ 02823000
*/*      01 FUNCTION - COMPLETE PROBLEM DETERMINATION PARAMETER LIST */ 02824000
*/*                    AND CALL IDA0192P TO WRITE ERROR MESSAGES.    */ 02825000
*/*      01 INPUT -    1. PROBLEM DETERMINATION PARAMETER LIST.      */ 02826000
*/*                    2. CATALOG INTERFACE PARAMETER LIST.          */ 02827000
*/*      01 OUTPUT -   1. ERROR MESSAGE WRITTEN.                     */ 02828000
*/*                    2. IF TERMINATING ERROR THEN GO TO TERMINATION*/ 02829000
*/*                       PROCESSING.                                */ 02830000
*/*                                                                  */ 02831000
*/********************************************************************/ 02832000
*                                                                  1227 02833000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1227 02834000
PROBDT2B DS    0H                                                  1228 02835000
*   RESPECIFY                                                      1228 02836000
*    (R6,                                                          1228 02837000
*     RET) RESTRICTED;                                             1228 02838000
*   R6=RET;                         /* SAVE RETURN ADDRESS           */ 02839000
         LR    R6,RET                                              1229 02840000
*   RESPECIFY                                                      1230 02841000
*     RET UNRESTRICTED;                                            1230 02842000
*   IF OPWMODWK=ON THEN             /* IF MODULE WORK AREA EXIST     */ 02843000
         L     @10,OPWCURPT(,RCORE)                                1231 02844000
         TM    OPWMODWK(@10),B'01000000'                           1231 02845000
         BNO   @RF01231                                            1231 02846000
*     DO;                                                          1232 02847000
*       IF OPWCAT=OFF&OPWSCRA=OFF THEN/* IF NOT OPENNING CATALOG     */ 02848000
         TM    OPWCAT(RCORE),B'11000000'                           1233 02849000
         BNZ   @RF01233                                            1233 02850000
*         DO;                                                      1234 02851000
*           MWACATID=CLC;           /* INDICATE OPEN WANTS CLUSTER 1235 02852000
*                                      NAME                          */ 02853000
         MVI   MWACATID,X'30'                                      1235 02854000
*           IF OPWWRKPT^=ZERO THEN  /* AMB WORK AREA EXIST           */ 02855000
         L     @10,OPWWRKPT(,RCORE)                                1236 02856000
         LTR   @10,@10                                             1236 02857000
         BZ    @RF01236                                            1236 02858000
*             MWACI=CTHSHDSN;       /* GET CI NO. FROM AMB WORK AREA */ 02859000
         MVC   MWACI(3),CTHSHDSN(@10)                              1237 02860000
*           ELSE                    /* AMB WORK AREA POINTER IS ZERO */ 02861000
*             MWACI=OPWDCI;         /* GET CI NO. FROM ACB WORK AREA */ 02862000
         B     @RC01236                                            1238 02863000
@RF01236 MVC   MWACI(3),OPWDCI(RCORE)                              1238 02864000
*           MWACACB=OPWCACB;        /* CATALOG ACB ADDRESS           */ 02865000
@RC01236 L     @10,OPWCACB(,RCORE)                                 1239 02866000
         ST    @10,MWACACB                                         1239 02867000
*           RESPECIFY                                              1240 02868000
*            (R1,                                                  1240 02869000
*             RETCODE) RESTRICTED;                                 1240 02870000
*           MWASFIPT=ZERO;          /* ZERO SFI POINTER IN CAT PARM     02871000
*                                      LIST                          */ 02872000
         SLR   @14,@14                                             1241 02873000
         STCM  @14,7,MWASFIPT                                      1241 02874000
*           R1=ADDR(MWACATPM);      /* POINT TO PARM LIST FOR      1242 02875000
*                                      IDA0192C                      */ 02876000
         LA    R1,MWACATPM                                         1242 02877000
*           RSTATRTN=IDAR192C;      /* NOTE CALL TO 192C     @Z30SSDP*/ 02878000
         L     @14,OPWCOMWA(,RCORE)                                1243 02879000
         MVC   RSTATRTN(4,@14),@CC04255                            1243 02880000
*           CALL IDA0192C;          /* GET CLUSTER NAME              */ 02881000
         L     @15,@CV05359                                        1244 02882000
         BALR  @14,@15                                             1244 02883000
*           RSTATRTN=IDAR192B;      /* INDIC 192B CONTROL    @Z30SSDP*/ 02884000
         L     @14,OPWCOMWA(,RCORE)                                1245 02885000
         MVC   RSTATRTN(4,@14),@CC04253                            1245 02886000
*           MWASFIPT=ADDR(OPWSFI);  /* REINITIALIZE SFI POINTER      */ 02887000
         LA    @10,OPWSFI(,RCORE)                                  1246 02888000
         STCM  @10,7,MWASFIPT                                      1246 02889000
*           IF RETCODE=ZERO THEN    /* IF NO ERROR ENCOUNTERED       */ 02890000
         LTR   RETCODE,RETCODE                                     1247 02891000
         BNZ   @RF01247                                            1247 02892000
*             PDPCLSNM=ADDR(MWACLSNM);/* GET CLUSTER NAME FROM     1248 02893000
*                                      IDA0192C                      */ 02894000
         LA    @10,MWACLSNM                                        1248 02895000
         ST    @10,PDPCLSNM+280(,@14)                              1248 02896000
*           ELSE                    /* NO ERROR                      */ 02897000
*             PDPDSTYP=PDPDDNM;     /* INDICATE DD NAME USED         */ 02898000
         B     @RC01247                                            1249 02899000
@RF01247 L     @14,OPWCOMWA(,RCORE)                                1249 02900000
         MVI   PDPDSTYP+280(@14),X'01'                             1249 02901000
*           RESPECIFY                                              1250 02902000
*             RETCODE UNRESTRICTED;                                1250 02903000
@RC01247 DS    0H                                                  1251 02904000
*         END;                                                     1251 02905000
*       ELSE                        /* IF OPENNING CATALOG           */ 02906000
*         PDPDSTYP=PDPDDNM;         /* INDICATE DD NAME USED         */ 02907000
         B     @RC01233                                            1252 02908000
@RF01233 L     @10,OPWCOMWA(,RCORE)                                1252 02909000
         MVI   PDPDSTYP+280(@10),X'01'                             1252 02910000
*       IF OPWWRKPT^=ZERO THEN      /* IF AMBWK AREA PTR EXIST IN OPW*/ 02911000
@RC01233 L     @10,OPWWRKPT(,RCORE)                                1253 02912000
         LTR   @10,@10                                             1253 02913000
         BZ    @RF01253                                            1253 02914000
*         PDPDSN=ADDR(CTDATANM);    /* SET DATA SET NAME TO DATA SET    02915000
*                                      NAME IN AMB WORK AREA         */ 02916000
         L     @15,OPWCOMWA(,RCORE)                                1254 02917000
         LA    @10,CTDATANM(,@10)                                  1254 02918000
         ST    @10,PDPDSN+280(,@15)                                1254 02919000
*       CALL ERRORFLG;              /* SET ACB ERROR FLAG            */ 02920000
@RF01253 BAL   @14,ERRORFLG                                        1255 02921000
*     END;                                                         1256 02922000
*   R1=OPWCOMWA;                    /* POINT TO COMMON WORK AREA     */ 02923000
@RF01231 L     @10,OPWCOMWA(,RCORE)                                1257 02924000
         LR    R1,@10                                              1257 02925000
*   RSTATRTN=IDAR192P;              /* NOTE CALL TO 192P     @Z30SSDP*/ 02926000
         MVC   RSTATRTN(4,@10),@CC04265                            1258 02927000
*   CALL IDA0192P;                  /* PROBLEM DETERMINATION         */ 02928000
         L     @15,@CV05445                                        1259 02929000
         BALR  @14,@15                                             1259 02930000
*   RSTATRTN=IDAR192B;              /* INDIC 192B CONTROL    @Z30SSDP*/ 02931000
         L     @10,OPWCOMWA(,RCORE)                                1260 02932000
         MVC   RSTATRTN(4,@10),@CC04253                            1260 02933000
*   PDPSFI=ZERO;                    /* ZERO CATALOG SFI POINTER      */ 02934000
         SLR   @15,@15                                             1261 02935000
         LA    @14,280                                             1261 02936000
         ALR   @14,@10                                             1261 02937000
         ST    @15,PDPSFI(,@14)                                    1261 02938000
*   PDPCLSNM=ADDR(JFCBDSNM);        /* RESET DATA SET TYPE TO DD NAME*/ 02939000
         AL    @10,@CF04572                                        1262 02940000
         ST    @10,PDPCLSNM(,@14)                                  1262 02941000
*   PDPDSN=ZERO;                    /* RESET DATA SET NAME TO ZERO   */ 02942000
         ST    @15,PDPDSN(,@14)                                    1263 02943000
*   OPWSFI=OPWSFI&&OPWSFI;          /* ZERO SFI AREA                 */ 02944000
         XC    OPWSFI(12,RCORE),OPWSFI(RCORE)                      1264 02945000
*   IF OPWUACB->ACBERFLG>OPERR128 THEN/* TERMINATING ERROR IN      1265 02946000
*                                      IDA0192B                      */ 02947000
         L     @10,OPWUACB(,RCORE)                                 1265 02948000
         CLI   ACBERFLG(@10),128                                   1265 02949000
         BNH   @RF01265                                            1265 02950000
*     DO;                                                          1266 02951000
*       OPWERR2B=ON;                /* INDICATE OPEN FAILED          */ 02952000
         L     @07,OPWCURPT(,RCORE)                                1267 02953000
         OI    OPWERR2B(@07),B'00010000'                           1267 02954000
*       RETURN TO LTRM192B;         /* TERMINATION PROCESSING        */ 02955000
         LA    @14,LTRM192B                                        1268 02956000
@EL00020 DS    0H                                                  1268 02957000
@EF00020 DS    0H                                                  1268 02958000
@ER00020 BR    @14                                                 1268 02959000
*     END;                                                         1269 02960000
*   RESPECIFY                                                      1270 02961000
*     RET RESTRICTED;                                              1270 02962000
@RF01265 DS    0H                                                  1271 02963000
*   RET=R6;                         /* GET RETURN ADDRESS            */ 02964000
         LR    RET,R6                                              1271 02965000
*   RESPECIFY                                                      1272 02966000
*    (R1,                                                          1272 02967000
*     R6,                                                          1272 02968000
*     RET) UNRESTRICTED;                                           1272 02969000
*   END PROBDT2B;                                                  1273 02970000
         B     @EL00020                                            1273 02971000
*TERM192B:                                                         1274 02972000
*                                                                  1274 02973000
*/********************************************************************/ 02974000
*/*                                                                  */ 02975000
*/*      01 FUNCTION - TERMINATION PROCESSING.                       */ 02976000
*/*                    UPDATE CATALOG, FREE AMB WORK AREAS, FREE     */ 02977000
*/*                    MODULE WORK AREA IF IT EXIST, AND SET ERROR   */ 02978000
*/*                    RETURN CODE IN REGISTER 15.                   */ 02979000
*/*      01 INPUT -    1. POINTER TO AMB WORK AREA IN OPWDTWRK AND   */ 02980000
*/*                       OPWIXWRK.                                  */ 02981000
*/*                    2. POINTER TO MODULE WORK AREA IN REGISTER 13.*/ 02982000
*/*      01 OUTPUT -   1. SET ERROR RETURN CODE IN RETCODE (REG 15). */ 02983000
*/*                       0-CLUSTER WAS SUCCESSFULLY OPENED.         */ 02984000
*/*                       4-WARNING CONDITION.                       */ 02985000
*/*                       8-TERMINATING ERROR.                       */ 02986000
*/*                                                                  */ 02987000
*/********************************************************************/ 02988000
*                                                                  1274 02989000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1274 02990000
TERM192B DS    0H                                                  1275 02991000
*   RESPECIFY                                                      1275 02992000
*     RET RESTRICTED;                                              1275 02993000
*   OPWSAVEA=RET;                   /* SAVE RETURN ADDRESS IN OPW    */ 02994000
