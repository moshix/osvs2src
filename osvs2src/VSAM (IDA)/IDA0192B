*SUBTSKSS:                                                         0908 01852000
*                                                                  0908 01853000
*/********************************************************************/ 01854000
*/*                                                                  */ 01855000
*/*    01 FUNCTION - THIS ROUTINE DOES SHARE PROCESSING.  IT WILL    */ 01856000
*/*                  CONNECT AN ACB TO AN EXISTING STRUCTURE.        */ 01857000
*/*    01 INPUT -    1. AMBLWRIT BIT ON INDICATES OPENING FOR OUTPUT.*/ 01858000
*/*                  2. CMBOUT BIT ON INDICATES EXISTING STRUCTURE   */ 01859000
*/*                     OPENNED FOR OUTPUT.                          */ 01860000
*/*                  3. OPEN INDICATOR FROM CATALOG (CTOPEN).        */ 01861000
*/*                  4. CTSHRF BIT ON INDICATES FULL INTEGRITY.      */ 01862000
*/*                  5. CTSHRW BIT ON INDICATES WRITE INTEGRITY.     */ 01863000
*/*    01 OUTPUT -   1. MWACATON IS SET ON IF CATALOG OPEN INDICATOR */ 01864000
*/*                     NEEDS TO BE SET.                             */ 01865000
*/*                  2. ENQNEDO IS CALLED TO ENQ FOR THE ELEMENTARY  */ 01866000
*/*                     DATA SET.                                    */ 01867000
*/*                                                                  */ 01868000
*/********************************************************************/ 01869000
*                                                                  0908 01870000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0908 01871000
SUBTSKSS DS    0H                                                  0909 01872000
*   RESPECIFY                                                      0909 01873000
*    (RET,                                                         0909 01874000
*     RETCODE) RESTRICTED;                                         0909 01875000
*   MWASAVED=RET;                   /* SAVE RETURN ADDRESS           */ 01876000
         ST    RET,MWASAVED                                        0910 01877000
*   RESPECIFY                                                      0911 01878000
*     RET UNRESTRICTED;                                            0911 01879000
*   IF AMBLWRIT=ON THEN             /* OPENNING FOR OUTPUT           */ 01880000
         L     @14,ACBAMBL(,RACB)                                  0912 01881000
         TM    AMBLWRIT(@14),B'00100000'                           0912 01882000
         BNO   @RF00912                                            0912 01883000
*     DO;                                                          0913 01884000
*       IF CMBOUT=OFF THEN          /* EXISTING STRUCTURE CLEARED FOR   01885000
*                                      OUTPUT                        */ 01886000
         L     @14,AMBLCMB(,@14)                                   0914 01887000
         TM    CMBOUT(@14),B'10000000'                             0914 01888000
         BNZ   @RF00914                                            0914 01889000
*         DO;                                                      0915 01890000
*           CALL ENQEXDTO;          /* CONCURRENT USER FOR OUTPUT  0916 01891000
*                                      EXIST OR NOT                  */ 01892000
         BAL   @14,ENQEXDTO                                        0916 01893000
*           IF RETCODE=ZERO THEN    /* NO CONCURRENT USER FOR OUTPUT */ 01894000
         LTR   RETCODE,RETCODE                                     0917 01895000
         BNZ   @RF00917                                            0917 01896000
*             DO;                                                  0918 01897000
*               IF CTOPEN=ON THEN   /* IF CATALOG OPEN INDICATOR IS     01898000
*                                      ON                            */ 01899000
         L     @14,OPWWRKPT(,RCORE)                                0919 01900000
         TM    CTOPEN(@14),B'10000000'                             0919 01901000
         BNO   @RF00919                                            0919 01902000
*                 DO;                                              0920 01903000
*                   MWAPCODE=PP56;  /* SET PROBLEM CODE              */ 01904000
         MVI   MWAPCODE,X'38'                                      0921 01905000
*                   CALL PROBDT2B;  /* PROBLEM DETERMINATION         */ 01906000
         BAL   @14,PROBDT2B                                        0922 01907000
*                 END;                                             0923 01908000
*               ELSE                /* CATALOG OPEN INDICATOR NOT ON */ 01909000
*                 DO;                                              0924 01910000
         B     @RC00919                                            0924 01911000
@RF00919 DS    0H                                                  0925 01912000
*                   IF OPWWRKPT=OPWDTWRK THEN/* IF IT IS DATA        */ 01913000
         CLC   OPWWRKPT(4,RCORE),OPWDTWRK(RCORE)                   0925 01914000
         BNE   @RF00925                                            0925 01915000
*                     MWACATDT=ON;  /* DATA NEEDS OPEN INDICATOR ON  */ 01916000
         OI    MWACATDT,B'10000000'                                0926 01917000
*                   ELSE            /* IF IT IS INDEX        @ZA03663*/ 01918000
*                     MWACATIX=ON;  /* INDEX NEEDS OPEN INDICATOR ON */ 01919000
         B     @RC00925                                            0927 01920000
@RF00925 OI    MWACATIX,B'10000000'                                0927 01921000
*                 END;                                             0928 01922000
*             END;                                                 0929 01923000
*           ELSE                    /* CONCURRENT USER FOR OUTPUT  0930 01924000
*                                      EXIST                         */ 01925000
*             IF CTSHRF=ON|CTSHRW=ON THEN/* IF FULL SHARE INDICATED  */ 01926000
         B     @RC00917                                            0930 01927000
@RF00917 L     @14,OPWWRKPT(,RCORE)                                0930 01928000
         TM    CTSHRF(@14),B'01100000'                             0930 01929000
         BZ    @RF00930                                            0930 01930000
*               DO;                                                0931 01931000
*                 MWAPCODE=PP52;    /* SET PROBLEM CODE              */ 01932000
         MVI   MWAPCODE,X'34'                                      0932 01933000
*                 CALL PROBDT2B;    /* PROBLEM DETERMINATION         */ 01934000
         BAL   @14,PROBDT2B                                        0933 01935000
*               END;                                               0934 01936000
*           CALL ENQNEDO;           /* ENQ NON-EXCLUSIVE FOR OUTPUT  */ 01937000
@RF00930 DS    0H                                                  0935 01938000
@RC00917 BAL   @14,ENQNEDO                                         0935 01939000
*           IF OPWWRKPT=OPWDTWRK THEN/* IF ENTRY FOR DATA    @Z30SSDP*/ 01940000
         CLC   OPWWRKPT(4,RCORE),OPWDTWRK(RCORE)                   0936 01941000
         BNE   @RF00936                                            0936 01942000
*             DO;                   /* UPDATE TO OUTPUT      @ZA03663*/ 01943000
*               MWAAMBPT=OPWCAMBL->AMBLDTA;/* PT TO DATA AMB @XM01165*/ 01944000
         L     @14,OPWCAMBL(,RCORE)                                0938 01945000
         L     @14,AMBLDTA(,@14)                                   0938 01946000
         ST    @14,MWAAMBPT                                        0938 01947000
*               CALL UPDATDEB;      /* UPDATE DEB TO OUTPUT  @Z30SSDT*/ 01948000
         BAL   @14,UPDATDEB                                        0939 01949000
*               IF AMBLLSR=OFF&AMBLGSR=OFF THEN/* IF NOT LSR OR GSR  */ 01950000
         L     @14,ACBAMBL(,RACB)                                  0940 01951000
         TM    AMBLLSR(@14),B'01100000'                            0940 01952000
         BNZ   @RF00940                                            0940 01953000
*                 DO;               /* UPDATE IOSBS TO OUTPUT      0941 01954000
*                                                            @ZA03663*/ 01955000
*                   MWAIOMBP=AMBIOMB;/* POINT TO IOMB        @Z30SSDP*/ 01956000
         L     @14,MWAAMBPT                                        0942 01957000
         L     MWAIOMBP,AMBIOMB(,@14)                              0942 01958000
*                   DO UNTIL MWAIOMBP=0;/* LOOP THROUGH IOSBS      0943 01959000
*                                                            @Z30SSDT*/ 01960000
@DL00943 DS    0H                                                  0944 01961000
*                     IOSFMSK=IOSFMSK&MWAFMWB;/* REMOVE INHIBIT WRITE   01962000
*                                                            @Z30SSDT*/ 01963000
         L     @14,IOMIOSB(,MWAIOMBP)                              0944 01964000
         NI    IOSFMSK(@14),B'10111111'                            0944 01965000
*                     MWAIOMBP=IOMNXT1;/* POINT TO NEXT SET  @Z30SSDT*/ 01966000
         L     MWAIOMBP,IOMNXT1(,MWAIOMBP)                         0945 01967000
*                   END;            /*                       @Z30SSDT*/ 01968000
@DE00943 LTR   MWAIOMBP,MWAIOMBP                                   0946 01969000
         BNZ   @DL00943                                            0946 01970000
*                 END;              /*                       @ZA03663*/ 01971000
*               IF OPWIXWRK=0 THEN  /* CHK FOR ESDS          @Z30SSDP*/ 01972000
@RF00940 L     @14,OPWIXWRK(,RCORE)                                0948 01973000
         LTR   @14,@14                                             0948 01974000
         BNZ   @RF00948                                            0948 01975000
*                 CMBOUT=ON;        /* THEN INDICATE STRUCTURE OPEN  */ 01976000
         L     @14,ACBAMBL(,RACB)                                  0949 01977000
         L     @14,AMBLCMB(,@14)                                   0949 01978000
         OI    CMBOUT(@14),B'10000000'                             0949 01979000
*             END;                  /*                       @Z30SSDP*/ 01980000
*           ELSE                    /* ENTRY FOR INDEX       @Z30SSDP*/ 01981000
*             DO;                   /* YES,                  @Z30SSDP*/ 01982000
         B     @RC00936                                            0951 01983000
@RF00936 DS    0H                                                  0952 01984000
*               MWAAMBPT=OPWCAMBL->AMBLIX;/* PT TO INDEX AMB @Z30SSDP*/ 01985000
         L     @14,OPWCAMBL(,RCORE)                                0952 01986000
         L     @14,AMBLIX(,@14)                                    0952 01987000
         ST    @14,MWAAMBPT                                        0952 01988000
*               CALL UPDATDEB;      /* UPDATE DEB TO OUTPUT  @Z30SSDT*/ 01989000
         BAL   @14,UPDATDEB                                        0953 01990000
*               CMBOUT=ON;          /* TURN ON CMB OUT BIT           */ 01991000
         L     @14,ACBAMBL(,RACB)                                  0954 01992000
         L     @14,AMBLCMB(,@14)                                   0954 01993000
         OI    CMBOUT(@14),B'10000000'                             0954 01994000
*             END;                  /*                       @Z30SSDP*/ 01995000
*         END;                                                     0956 01996000
@RC00936 DS    0H                                                  0957 01997000
*     END;                                                         0957 01998000
@RF00914 DS    0H                                                  0958 01999000
*   IF ACBEXLST>ONE&EXLLEN>THIRTY5& /* EXIT LIST EXIST AND           */ 02000000
*       EXLJRNF(2)=ON THEN          /* JOURNALLING INDICATED         */ 02001000
@RF00912 L     @14,ACBEXLST(,RACB)                                 0958 02002000
         C     @14,@CF00059                                        0958 02003000
         BNH   @RF00958                                            0958 02004000
         LH    @05,EXLLEN(,@14)                                    0958 02005000
         C     @05,@CF00148                                        0958 02006000
         BNH   @RF00958                                            0958 02007000
         TM    EXLJRNF(@14),B'01000000'                            0958 02008000
         BNO   @RF00958                                            0958 02009000
*     DO;                                                          0959 02010000
*       OPWCAMBL->AMBLDTA->AMBJRN=ON;/* IND JOURNALLING IN DATA AMB  */ 02011000
         L     @14,OPWCAMBL(,RCORE)                                0960 02012000
         L     @05,AMBLDTA(,@14)                                   0960 02013000
         OI    AMBJRN(@05),B'00000010'                             0960 02014000
*       IF OPWCAMBL->AMBLIX^=ZERO THEN/* IF INDEX AMB EXIST          */ 02015000
         L     @14,AMBLIX(,@14)                                    0961 02016000
         LTR   @14,@14                                             0961 02017000
         BZ    @RF00961                                            0961 02018000
*         OPWCAMBL->AMBLIX->AMBJRN=ON;/* IND JOURNALLING IN INDEX AMB*/ 02019000
         OI    AMBJRN(@14),B'00000010'                             0962 02020000
*     END;                                                         0963 02021000
@RF00961 DS    0H                                                  0964 02022000
*   RESPECIFY                                                      0964 02023000
*     RET RESTRICTED;                                              0964 02024000
@RF00958 DS    0H                                                  0965 02025000
*   RET=MWASAVED;                   /* GET RETURN ADDRESS            */ 02026000
         L     RET,MWASAVED                                        0965 02027000
*   RESPECIFY                                                      0966 02028000
*    (RET,                                                         0966 02029000
*     RETCODE) UNRESTRICTED;                                       0966 02030000
*   END SUBTSKSS;                                                  0967 02031000
@EL00011 DS    0H                                                  0967 02032000
@EF00011 DS    0H                                                  0967 02033000
@ER00011 BR    @14                                                 0967 02034000
*UPDATDEB:                                                         0968 02035000
*                                                                  0968 02036000
*/********************************************************************/ 02037000
*/*                                                                  */ 02038000
*/*      01 FUNCTION - UPDATE DEB TO REFLECT OUTPUT                  */ 02039000
*/*      01 INPUT    - POINTER TO AMB IN MWAAMBPT                    */ 02040000
*/*      01 OUTPUT   - DEB UPDATED                                   */ 02041000
*/*                                                                  */ 02042000
*/********************************************************************/ 02043000
*                                                                  0968 02044000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0968 02045000
UPDATDEB DS    0H                                                  0969 02046000
*   RESPECIFY                                                      0969 02047000
*     RET RESTRICTED;                                              0969 02048000
*   RESPECIFY                                                      0970 02049000
*     DEBDASD BASED(MWADEBEX);      /* ADDRESSIBILITY FOR EXTENTS    */ 02050000
*   MWASAVEE=RET;                   /* SAVE RETURN ADDRESS           */ 02051000
         ST    RET,MWASAVEE                                        0971 02052000
*   RESPECIFY                                                      0972 02053000
*     RET UNRESTRICTED;                                            0972 02054000
*   MWADEBPT=AMBDEBAD;              /* POINT TO DEB          @Z30SSDT*/ 02055000
         L     @01,MWAAMBPT                                        0973 02056000
         L     MWADEBPT,AMBDEBAD-1(,@01)                           0973 02057000
         LA    MWADEBPT,0(,MWADEBPT)                               0973 02058000
*   MWADEBEX=ADDR(DEBBASND);        /* POINT TO DEB EXTENT   @Z30SSDT*/ 02059000
         LA    MWADEBEX,DEBBASND(,MWADEBPT)                        0974 02060000
*   DO MWAINDEX=DEBNMEXT TO 1 BY-1; /* LOOP THROUGH EXTENTS  @Z30SSDT*/ 02061000
         SLR   MWAINDEX,MWAINDEX                                   0975 02062000
         IC    MWAINDEX,DEBNMEXT(,MWADEBPT)                        0975 02063000
         B     @DE00975                                            0975 02064000
@DL00975 DS    0H                                                  0976 02065000
*     DEBDVMOD=DEBDVMOD&MWAFMWB;    /* REMOVE INHIBIT WRITE IN FILE     02066000
*                                      MASK                  @Z30SSDT*/ 02067000
         NI    DEBDVMOD(MWADEBEX),B'10111111'                      0976 02068000
*     MWADEBEX=ADDR(DEBDASD)+LENGTH(DEBDASD);/* POINT TO NEXT EXTENT */ 02069000
         AL    MWADEBEX,@CF00146                                   0977 02070000
*   END;                                                           0978 02071000
         BCTR  MWAINDEX,0                                          0978 02072000
@DE00975 LTR   MWAINDEX,MWAINDEX                                   0978 02073000
         BP    @DL00975                                            0978 02074000
*   RESPECIFY                                                      0979 02075000
*     RET RESTRICTED;                                              0979 02076000
*   RET=MWASAVEE;                   /* GET RETURN ADDRESS            */ 02077000
         L     RET,MWASAVEE                                        0980 02078000
*   RESPECIFY                                                      0981 02079000
*     RET UNRESTRICTED;                                            0981 02080000
*   END UPDATDEB;                                                  0982 02081000
@EL00012 DS    0H                                                  0982 02082000
@EF00012 DS    0H                                                  0982 02083000
@ER00012 BR    @14                                                 0982 02084000
*SUBTSKMS:                                                         0983 02085000
*                                                                  0983 02086000
*/********************************************************************/ 02087000
*/*                                                                  */ 02088000
*/*    01 FUNCTION - THIS ROUTINE DOES SHARE PROCESSING FOR THE FIRST*/ 02089000
*/*                  OPEN IN A REGION OF A DATA SET.                 */ 02090000
*/*    01 INPUT -    1. OPEN INDICATOR FROM CATALOG (CTOPEN).        */ 02091000
*/*                  2. AMBLWRIT BIT ON INDICATES OUTPUT PROCESSING. */ 02092000
*/*    01 OUTPUT -   1. MWACATON IS SET ON IF CATALOG OPEN INDICATOR */ 02093000
*/*                     NEEDS TO BE SET IN THE CATALOG.              */ 02094000
*/*                  2. ENQEXDO IS CALLED TO ENQ FOR THE ELEMENTARY  */ 02095000
*/*                     DATA SET.                                    */ 02096000
*/*                                                                  */ 02097000
*/********************************************************************/ 02098000
*                                                                  0983 02099000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0983 02100000
SUBTSKMS DS    0H                                                  0984 02101000
*   RESPECIFY                                                      0984 02102000
*    (RET,                                                         0984 02103000
*     RETCODE) RESTRICTED;                                         0984 02104000
*   MWASAVED=RET;                   /* SAVE RETURN ADDRESS           */ 02105000
         ST    RET,MWASAVED                                        0985 02106000
*   RESPECIFY                                                      0986 02107000
*     RET UNRESTRICTED;                                            0986 02108000
*   CALL ENQEXD;                    /* ENQ EXCLUSIVE                 */ 02109000
         BAL   @14,ENQEXD                                          0987 02110000
*   IF RETCODE=ZERO THEN            /* ENQ SUCCESSFUL                */ 02111000
         LTR   RETCODE,RETCODE                                     0988 02112000
         BNZ   @RF00988                                            0988 02113000
*     DO;                                                          0989 02114000
*       IF ACBRST=OFF&CTOPEN=ON THEN/* CATALOG OPEN INDICATOR ON AND    02115000
*                                      NOT RESET             @XM01165*/ 02116000
         TM    ACBRST(RACB),B'00000100'                            0990 02117000
         BNZ   @RF00990                                            0990 02118000
         L     @14,OPWWRKPT(,RCORE)                                0990 02119000
         TM    CTOPEN(@14),B'10000000'                             0990 02120000
         BNO   @RF00990                                            0990 02121000
*         DO;                                                      0991 02122000
*           MWAPCODE=PP56;          /* SET PROBLEM CODE              */ 02123000
         MVI   MWAPCODE,X'38'                                      0992 02124000
*           CALL PROBDT2B;          /* PROBLEM DETERMINATION         */ 02125000
         BAL   @14,PROBDT2B                                        0993 02126000
*         END;                                                     0994 02127000
*       IF AMBLWRIT=ON|ACBRST=ON THEN/* OPEN FOR OUTPUT OR RESET     */ 02128000
@RF00990 L     @14,ACBAMBL(,RACB)                                  0995 02129000
         TM    AMBLWRIT(@14),B'00100000'                           0995 02130000
         BO    @RT00995                                            0995 02131000
         TM    ACBRST(RACB),B'00000100'                            0995 02132000
         BNO   @RF00995                                            0995 02133000
@RT00995 DS    0H                                                  0996 02134000
*         DO;                                                      0996 02135000
*           CALL ENQEXDO;           /* ENQ EXCLUSIVE FOR OUTPUT      */ 02136000
         BAL   @14,ENQEXDO                                         0997 02137000
*           IF OPWWRKPT=OPWDTWRK THEN/* IF IT IS DATA                */ 02138000
         CLC   OPWWRKPT(4,RCORE),OPWDTWRK(RCORE)                   0998 02139000
         BNE   @RF00998                                            0998 02140000
*             MWACATDT=ON;          /* IND DATA NEEDS OPEN INDR ON   */ 02141000
         OI    MWACATDT,B'10000000'                                0999 02142000
*           IF OPWWRKPT=OPWIXWRK THEN/* IF IT IS INDEX               */ 02143000
@RF00998 CLC   OPWWRKPT(4,RCORE),OPWIXWRK(RCORE)                   1000 02144000
         BNE   @RF01000                                            1000 02145000
*             MWACATIX=ON;          /* IND INDEX NEEDS OPEN INDR ON  */ 02146000
         OI    MWACATIX,B'10000000'                                1001 02147000
*         END;                                                     1002 02148000
*     END;                                                         1003 02149000
*   ELSE                            /* ENQ WAS UNSUCCESSFUL          */ 02150000
*     DO;                                                          1004 02151000
         B     @RC00988                                            1004 02152000
@RF00988 DS    0H                                                  1005 02153000
*       IF RETCODE^=EIGHT|          /* REGION DOES NOT OWN RESOURCES    02154000
*                                      OR                            */ 02155000
*           ACBRST=ON|              /* OPENNED FOR RESTART OR        */ 02156000
*           AMBLWRIT=ON THEN        /* OPENNED FOR OUTPUT            */ 02157000
         C     RETCODE,@CF00065                                    1005 02158000
         BNE   @RT01005                                            1005 02159000
         TM    ACBRST(RACB),B'00000100'                            1005 02160000
         BO    @RT01005                                            1005 02161000
         L     @14,ACBAMBL(,RACB)                                  1005 02162000
         TM    AMBLWRIT(@14),B'00100000'                           1005 02163000
         BNO   @RF01005                                            1005 02164000
@RT01005 DS    0H                                                  1006 02165000
*         DO;                                                      1006 02166000
*           MWAPCODE=PP52;          /* SET PROBLEM CODE              */ 02167000
         MVI   MWAPCODE,X'34'                                      1007 02168000
*           CALL PROBDT2B;          /* PROBLEM DETERMINATION         */ 02169000
         BAL   @14,PROBDT2B                                        1008 02170000
*         END;                                                     1009 02171000
*       ELSE                        /* REGION OWN RESOURCES SHARING     02172000
*                                      STILL POSSIBLE                */ 02173000
*         DO;                                                      1010 02174000
         B     @RC01005                                            1010 02175000
@RF01005 DS    0H                                                  1011 02176000
*           CALL ENQEXDTO;          /* CONCURRENT USER FOR OUTPUT    */ 02177000
         BAL   @14,ENQEXDTO                                        1011 02178000
*           IF RETCODE^=ZERO THEN   /* IF ENQ FAILED                 */ 02179000
         LTR   RETCODE,RETCODE                                     1012 02180000
         BZ    @RF01012                                            1012 02181000
*             DO;                                                  1013 02182000
*               MWAPCODE=PP52;      /* SET PROBLEM CODE              */ 02183000
         MVI   MWAPCODE,X'34'                                      1014 02184000
*               CALL PROBDT2B;      /* PROBLEM DETERMINATION         */ 02185000
         BAL   @14,PROBDT2B                                        1015 02186000
*             END;                                                 1016 02187000
*         END;                                                     1017 02188000
@RF01012 DS    0H                                                  1018 02189000
*     END;                                                         1018 02190000
@RC01005 DS    0H                                                  1019 02191000
*   RESPECIFY                                                      1019 02192000
*     RET RESTRICTED;                                              1019 02193000
@RC00988 DS    0H                                                  1020 02194000
*   RET=MWASAVED;                   /* GET RETURN ADDRESS            */ 02195000
         L     RET,MWASAVED                                        1020 02196000
*   RESPECIFY                                                      1021 02197000
*    (RET,                                                         1021 02198000
*     RETCODE) UNRESTRICTED;                                       1021 02199000
*   END SUBTSKMS;                                                  1022 02200000
@EL00013 DS    0H                                                  1022 02201000
@EF00013 DS    0H                                                  1022 02202000
@ER00013 BR    @14                                                 1022 02203000
*CRTSKRD:                                                          1023 02204000
*                                                                  1023 02205000
*/********************************************************************/ 02206000
*/*                                                                  */ 02207000
*/*    01 FUNCTION - THIS ROUTINE WILL DO SHARE PROCESSING WHEN AN   */ 02208000
*/*                  ACB IS OPENING FOR READ-ONLY AND THERE IS CROSS */ 02209000
*/*                  REGION SHARING.                                 */ 02210000
*/*    01 INPUT -    1. OPEN INDICATOR FROM CATALOG (CTOPEN).        */ 02211000
*/*                  2. CTSHRF ON INDICATES FULL INTEGRITY.          */ 02212000
*/*    01 OUTPUT -   1. ENQNED IS CALLED TO ENQ FOR THE ELEMENTARY   */ 02213000
*/*                     DATA SET.                                    */ 02214000
*/*                                                                  */ 02215000
*/********************************************************************/ 02216000
*                                                                  1023 02217000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1023 02218000
CRTSKRD  DS    0H                                                  1024 02219000
*   RESPECIFY                                                      1024 02220000
*    (RET,                                                         1024 02221000
*     RETCODE) RESTRICTED;                                         1024 02222000
*   MWASAVED=RET;                   /* SAVE RETURN ADDRESS           */ 02223000
         ST    RET,MWASAVED                                        1025 02224000
*   RESPECIFY                                                      1026 02225000
*     RET UNRESTRICTED;                                            1026 02226000
*   CALL ENQNED;                    /* ENQ NON-EXCLUSIVE             */ 02227000
         BAL   @14,ENQNED                                          1027 02228000
*   IF RETCODE^=ZERO&RETCODE^=EIGHT THEN/* REGION DOES NOT OWN     1028 02229000
*                                      RESOURCE                      */ 02230000
         LTR   RETCODE,RETCODE                                     1028 02231000
         BZ    @RF01028                                            1028 02232000
         C     RETCODE,@CF00065                                    1028 02233000
         BE    @RF01028                                            1028 02234000
*     DO;                                                          1029 02235000
*       MWAPCODE=PP52;              /* SET PROBLEM CODE              */ 02236000
         MVI   MWAPCODE,X'34'                                      1030 02237000
*       CALL PROBDT2B;              /* PROBLEM DETERMINATION         */ 02238000
         BAL   @14,PROBDT2B                                        1031 02239000
*     END;                                                         1032 02240000
*   ELSE                            /* REGION OWNS RESOURCES         */ 02241000
*     DO;                                                          1033 02242000
         B     @RC01028                                            1033 02243000
@RF01028 DS    0H                                                  1034 02244000
*       CALL ENQEXDTO;              /* CONCURRENT USER FOR OUTPUT    */ 02245000
         BAL   @14,ENQEXDTO                                        1034 02246000
*       IF RETCODE=ZERO&            /* NO CONCURRENT USER AND        */ 02247000
*           CTOPEN=ON THEN          /* OPEN INDICATOR IS ON          */ 02248000
         LTR   RETCODE,RETCODE                                     1035 02249000
         BNZ   @RF01035                                            1035 02250000
         L     @14,OPWWRKPT(,RCORE)                                1035 02251000
         TM    CTOPEN(@14),B'10000000'                             1035 02252000
         BNO   @RF01035                                            1035 02253000
*         DO;                                                      1036 02254000
*           MWAPCODE=PP56;          /* SET PROBLEM CODE              */ 02255000
         MVI   MWAPCODE,X'38'                                      1037 02256000
*           CALL PROBDT2B;          /* PROBLEM DETERMINATION         */ 02257000
         BAL   @14,PROBDT2B                                        1038 02258000
*         END;                                                     1039 02259000
*       ELSE                        /* CONCURRENT USER EXIST @Z30SSRL*/ 02260000
*         IF RETCODE^=ZERO&         /* RESOURCES USED FOR OUTPUT   1040 02261000
*                                                            @XM01165*/ 02262000
*             CTSHRF=ON THEN        /* SHR IS NOT PERMITTED  @XM01165*/ 02263000
         B     @RC01035                                            1040 02264000
@RF01035 LTR   RETCODE,RETCODE                                     1040 02265000
         BZ    @RF01040                                            1040 02266000
         L     @14,OPWWRKPT(,RCORE)                                1040 02267000
         TM    CTSHRF(@14),B'00100000'                             1040 02268000
         BNO   @RF01040                                            1040 02269000
*           DO;                                                    1041 02270000
*             MWAPCODE=PP52;        /* SET PROBLEM CODE              */ 02271000
         MVI   MWAPCODE,X'34'                                      1042 02272000
*             CALL PROBDT2B;        /* PROBLEM DETERMINATION         */ 02273000
         BAL   @14,PROBDT2B                                        1043 02274000
*           END;                                                   1044 02275000
*     END;                                                         1045 02276000
@RF01040 DS    0H                                                  1045 02277000
@RC01035 DS    0H                                                  1046 02278000
*   RESPECIFY                                                      1046 02279000
*     RET RESTRICTED;                                              1046 02280000
@RC01028 DS    0H                                                  1047 02281000
*   RET=MWASAVED;                   /* GET RETURN ADDRESS            */ 02282000
         L     RET,MWASAVED                                        1047 02283000
*   RESPECIFY                                                      1048 02284000
*    (RET,                                                         1048 02285000
*     RETCODE) UNRESTRICTED;                                       1048 02286000
*   END CRTSKRD;                                                   1049 02287000
@EL00014 DS    0H                                                  1049 02288000
@EF00014 DS    0H                                                  1049 02289000
@ER00014 BR    @14                                                 1049 02290000
*CRTSKWR:                                                          1050 02291000
*                                                                  1050 02292000
*/********************************************************************/ 02293000
*/*                                                                  */ 02294000
*/*    01 FUNCTION - THIS ROUTINE DOES SHARE PROCESSING FOR THE ACB  */ 02295000
*/*                  THAT IS OPENED FOR OUTPUT WHEN THERE IS CROSS   */ 02296000
*/*                  REGION SHARING.                                 */ 02297000
*/*    01 INPUT -    1. OPEN INDICATOR FROM CATALOG (CTOPEN).        */ 02298000
*/*                  2. CTSHRW ON INDICATES WRITE INTEGRITY.         */ 02299000
*/*    01 OUTPUT -   1. ENQNED IS CALLED TO ENQ FOR THE ELEMENTARY   */ 02300000
*/*                     DATA SET.                                    */ 02301000
*/*                                                                  */ 02302000
*/********************************************************************/ 02303000
*                                                                  1050 02304000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1050 02305000
CRTSKWR  DS    0H                                                  1051 02306000
*   RESPECIFY                                                      1051 02307000
*    (RET,                                                         1051 02308000
*     RETCODE) RESTRICTED;                                         1051 02309000
*   MWASAVED=RET;                   /* SAVE RETURN ADDRESS           */ 02310000
         ST    RET,MWASAVED                                        1052 02311000
*   RESPECIFY                                                      1053 02312000
*     RET UNRESTRICTED;                                            1053 02313000
*   CALL ENQEXDT;                   /* ANYONE ENQ'ED ON DATA SET NAME*/ 02314000
         BAL   @14,ENQEXDT                                         1054 02315000
*   IF RETCODE^=ZERO THEN           /* YES SOMEONE ENQ'ED ON DATA SET   02316000
*                                      NAME ALREADY                  */ 02317000
         LTR   RETCODE,RETCODE                                     1055 02318000
         BZ    @RF01055                                            1055 02319000
*     DO;                                                          1056 02320000
*       CALL ENQEXDTO;              /* ANYONE USING RESOURCE FOR   1057 02321000
*                                      OUTPUT                        */ 02322000
         BAL   @14,ENQEXDTO                                        1057 02323000
*       IF RETCODE=ZERO THEN        /* IF NO                         */ 02324000
         LTR   RETCODE,RETCODE                                     1058 02325000
         BNZ   @RF01058                                            1058 02326000
*         DO;                                                      1059 02327000
*           IF CTOPEN=ON THEN       /* IF CATALOG OPEN INDICATOR IS     02328000
*                                      ON                            */ 02329000
         L     @14,OPWWRKPT(,RCORE)                                1060 02330000
         TM    CTOPEN(@14),B'10000000'                             1060 02331000
         BNO   @RF01060                                            1060 02332000
*             DO;                                                  1061 02333000
*               MWAPCODE=PP56;      /* SET PROBLEM CODE              */ 02334000
         MVI   MWAPCODE,X'38'                                      1062 02335000
*               CALL PROBDT2B;      /* PROBLEM DETERMINATION         */ 02336000
         BAL   @14,PROBDT2B                                        1063 02337000
*             END;                                                 1064 02338000
*           ELSE                    /* CATALOG OPEN INDICATOR IS NOT    02339000
*                                      ON                            */ 02340000
*             DO;                                                  1065 02341000
         B     @RC01060                                            1065 02342000
@RF01060 DS    0H                                                  1066 02343000
*               IF OPWWRKPT=OPWDTWRK THEN/* IF IT IS DATA            */ 02344000
         CLC   OPWWRKPT(4,RCORE),OPWDTWRK(RCORE)                   1066 02345000
         BNE   @RF01066                                            1066 02346000
*                 MWACATDT=ON;      /* IND DATA NEEDS OPEN INDR ON   */ 02347000
         OI    MWACATDT,B'10000000'                                1067 02348000
*               IF OPWWRKPT=OPWIXWRK THEN/* IF IT IS INDEX           */ 02349000
@RF01066 CLC   OPWWRKPT(4,RCORE),OPWIXWRK(RCORE)                   1068 02350000
         BNE   @RF01068                                            1068 02351000
*                 MWACATIX=ON;      /* IND INDEX NEEDS OPEN INDR ON  */ 02352000
         OI    MWACATIX,B'10000000'                                1069 02353000
*             END;                                                 1070 02354000
*         END;                                                     1071 02355000
*       ELSE                        /* IF SOMEONE IS USING RESOURCES    02356000
*                                      FOR OUTPUT                    */ 02357000
*         IF CTSHRW=ON THEN         /* NEED DATA INTEGRITY           */ 02358000
         B     @RC01058                                            1072 02359000
@RF01058 L     @14,OPWWRKPT(,RCORE)                                1072 02360000
         TM    CTSHRW(@14),B'01000000'                             1072 02361000
         BNO   @RF01072                                            1072 02362000
*           DO;                                                    1073 02363000
*             MWAPCODE=PP52;        /* SET PROBLEM CODE              */ 02364000
         MVI   MWAPCODE,X'34'                                      1074 02365000
*             CALL PROBDT2B;        /* PROBLEM DETERMINATION         */ 02366000
         BAL   @14,PROBDT2B                                        1075 02367000
*           END;                                                   1076 02368000
*     END;                                                         1077 02369000
*   ELSE                            /* NO ONE ENQ'ED ON DATA SET NAME*/ 02370000
*     DO;                                                          1078 02371000
         B     @RC01055                                            1078 02372000
@RF01055 DS    0H                                                  1079 02373000
*       IF CTOPEN=ON THEN           /* IF OPEN INDICATOR IS ON       */ 02374000
         L     @14,OPWWRKPT(,RCORE)                                1079 02375000
         TM    CTOPEN(@14),B'10000000'                             1079 02376000
         BNO   @RF01079                                            1079 02377000
*         DO;                                                      1080 02378000
*           MWAPCODE=PP56;          /* SET PROBLEM CODE              */ 02379000
         MVI   MWAPCODE,X'38'                                      1081 02380000
*           CALL PROBDT2B;          /* PROBLEM DETERMINATION         */ 02381000
         BAL   @14,PROBDT2B                                        1082 02382000
*         END;                                                     1083 02383000
*       ELSE                        /* OPEN INDICATOR NOT ON         */ 02384000
*         DO;                                                      1084 02385000
         B     @RC01079                                            1084 02386000
@RF01079 DS    0H                                                  1085 02387000
*           IF OPWWRKPT=OPWDTWRK THEN/* IF IT IS DATA                */ 02388000
         CLC   OPWWRKPT(4,RCORE),OPWDTWRK(RCORE)                   1085 02389000
         BNE   @RF01085                                            1085 02390000
*             MWACATDT=ON;          /* DATA NEEDS OPEN INDICATOR ON  */ 02391000
         OI    MWACATDT,B'10000000'                                1086 02392000
*           IF OPWWRKPT=OPWIXWRK THEN/* IF IT IS INDEX               */ 02393000
@RF01085 CLC   OPWWRKPT(4,RCORE),OPWIXWRK(RCORE)                   1087 02394000
         BNE   @RF01087                                            1087 02395000
*             MWACATIX=ON;          /* INDEX NEEDS OPEN INDICATOR ON */ 02396000
         OI    MWACATIX,B'10000000'                                1088 02397000
*         END;                                                     1089 02398000
@RF01087 DS    0H                                                  1090 02399000
*     END;                                                         1090 02400000
@RC01079 DS    0H                                                  1091 02401000
*   CALL ENQNED;                    /* ENQ NON-EXCLUSIVE             */ 02402000
@RC01055 BAL   @14,ENQNED                                          1091 02403000
*   IF RETCODE=ZERO|RETCODE=EIGHT THEN/* IF DATA SET AVAILABLE       */ 02404000
         LTR   RETCODE,RETCODE                                     1092 02405000
         BZ    @RT01092                                            1092 02406000
         C     RETCODE,@CF00065                                    1092 02407000
         BNE   @RF01092                                            1092 02408000
@RT01092 DS    0H                                                  1093 02409000
*     DO;                                                          1093 02410000
*       CALL ENQNEDO;               /* ENQ NON-EXCLUSIVE FOR OUTPUT  */ 02411000
         BAL   @14,ENQNEDO                                         1094 02412000
*       IF RETCODE=EIGHT THEN       /* IF TASK ALREADY HAS RESOURCE  */ 02413000
         C     RETCODE,@CF00065                                    1095 02414000
         BNE   @RF01095                                            1095 02415000
*         DO;                       /* THEN TEST FOR EXCLUSIVE     1096 02416000
*                                      CONTROL                       */ 02417000
*           IF ENQEXSHR=OFF THEN    /* IF NOT SHARED         @Z30SSDP*/ 02418000
         TM    ENQEXSHR,B'00010000'                                1097 02419000
         BNZ   @RF01097                                            1097 02420000
*             DO;                                                  1098 02421000
*               MWAPCODE=PP52;      /* SET PROBLEM CODE              */ 02422000
         MVI   MWAPCODE,X'34'                                      1099 02423000
*               CALL PROBDT2B;      /* PROBLEM DETERMINATION         */ 02424000
         BAL   @14,PROBDT2B                                        1100 02425000
*             END;                                                 1101 02426000
*         END;                                                     1102 02427000
*       ELSE                        /* TASK DOES NOT HAVE RESOURCE   */ 02428000
*         IF RETCODE^=ZERO THEN     /* IF DATA SET NOT AVAILABLE     */ 02429000
         B     @RC01095                                            1103 02430000
@RF01095 LTR   RETCODE,RETCODE                                     1103 02431000
         BZ    @RF01103                                            1103 02432000
*           DO;                                                    1104 02433000
*             MWAPCODE=PP52;        /* SET PROBLEM CODE              */ 02434000
         MVI   MWAPCODE,X'34'                                      1105 02435000
*             CALL PROBDT2B;        /* PROBLEM DETERMINATION         */ 02436000
         BAL   @14,PROBDT2B                                        1106 02437000
*           END;                                                   1107 02438000
*     END;                                                         1108 02439000
*   ELSE                            /* DATA SET NOT AVAILABLE        */ 02440000
*     DO;                                                          1109 02441000
         B     @RC01092                                            1109 02442000
@RF01092 DS    0H                                                  1110 02443000
*       MWAPCODE=PP52;              /* SET PROBLEM CODE              */ 02444000
         MVI   MWAPCODE,X'34'                                      1110 02445000
*       CALL PROBDT2B;              /* PROBLEM DETERMINATION         */ 02446000
         BAL   @14,PROBDT2B                                        1111 02447000
*     END;                                                         1112 02448000
*   RESPECIFY                                                      1113 02449000
*     RET RESTRICTED;                                              1113 02450000
@RC01092 DS    0H                                                  1114 02451000
*   RET=MWASAVED;                   /* GET RETURN ADDRESS            */ 02452000
         L     RET,MWASAVED                                        1114 02453000
*   RESPECIFY                                                      1115 02454000
*    (RET,                                                         1115 02455000
*     RETCODE) UNRESTRICTED;                                       1115 02456000
*   END CRTSKWR;                                                   1116 02457000
@EL00015 DS    0H                                                  1116 02458000
@EF00015 DS    0H                                                  1116 02459000
@ER00015 BR    @14                                                 1116 02460000
*VOLPROC:                                                          1117 02461000
*                                                                  1117 02462000
*/********************************************************************/ 02463000
*/*                                                                  */ 02464000
*/*      01 FUNCTION - PERFORM VOLUME PROCESSING FOR EACH AMB.       */ 02465000
*/*      01 INPUT -    1. POINTERS TO AMB WORK AREAS IN OPWDTWRK AND */ 02466000
*/*                       OPWIXWRK.                                  */ 02467000
*/*      01 OUTPUT -   1. VOLUME PROCESSED.                          */ 02468000
*/*                                                                  */ 02469000
