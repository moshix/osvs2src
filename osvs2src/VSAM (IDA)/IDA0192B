         TM    MWACATIX,B'10000000'                                1282 03011000
         BNO   @RF01282                                            1282 03012000
@RT01282 DS    0H                                                  1283 03013000
*             CALL CATUPDT;         /* UPDATE CATALOG                */ 03014000
         BAL   @14,CATUPDT                                         1283 03015000
*           AMBLDTA->AMBOPEN=ON;    /* TURN ON DATA AMB'S OPEN     1284 03016000
*                                      INDICATOR                     */ 03017000
@RF01282 L     @05,ACBAMBL(,RACB)                                  1284 03018000
         L     @01,AMBLDTA(,@05)                                   1284 03019000
