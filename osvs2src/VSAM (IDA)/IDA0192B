         LTR   @05,@05                                             1124 02490000
         BZ    @RF01124                                            1124 02491000
*     DO;                                                          1125 02492000
*       OPWWRKPT=OPWIXWRK;          /* POINT TO INDEX AMB WORK AREA  */ 02493000
         ST    @05,OPWWRKPT(,RCORE)                                1126 02494000
*       CALL VMTPROC;               /* PROCESS VMT                   */ 02495000
         BAL   @14,VMTPROC                                         1127 02496000
*     END;                                                         1128 02497000
*   OPWWRKPT=ZERO;                  /* ZERO OPW CURRENT AMB WORK AREA   02498000
*                                      POINTER                       */ 02499000
@RF01124 SLR   @05,@05                                             1129 02500000
         ST    @05,OPWWRKPT(,RCORE)                                1129 02501000
*   RESPECIFY                                                      1130 02502000
*     RET RESTRICTED;                                              1130 02503000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 02504000
         L     RET,MWASAVEA                                        1131 02505000
*   RESPECIFY                                                      1132 02506000
*     RET UNRESTRICTED;                                            1132 02507000
*   END VOLPROC;                                                   1133 02508000
@EL00016 DS    0H                                                  1133 02509000
