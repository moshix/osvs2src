*           CALL BUFNOSET;          /* CALCULATE BUFND & BUFNI       */ 00141000
         BAL   @14,BUFNOSET                                        0400 00142000
*           OPWWRKPT=ZERO;          /* ZERO AMB WORK AREA POINTER    */ 00143000
         SLR   @14,@14                                             0401 00144000
         ST    @14,OPWWRKPT(,RCORE)                                0401 00145000
*         END;                                                     0402 00146000
*   IF AMBLCAT=OFF&                 /* NOT CATALOG,NOT SCRA,NOT VVIC */ 00147000
*       AMBLUCRA=OFF&               /* AND NOT UCRA,                 */ 00148000
