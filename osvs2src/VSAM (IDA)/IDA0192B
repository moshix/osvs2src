*VMTVOLSR:                                                         1170 02616000
*                                                                  1170 02617000
*/********************************************************************/ 02618000
*/*                                                                  */ 02619000
*/*      01 FUNCTION - SEARCH FOR LIKE DEVICE TYPE.                  */ 02620000
*/*      01 INPUT -    1. POINTER TO VMT IN MWAVMTPT.                */ 02621000
*/*                    2. ADDRESS OF AMB WORK AREA IN OPWWRKPT.      */ 02622000
*/*                    3. MWADEVTP INITIALIZED.                      */ 02623000
*/*      01 OUTPUT -   1. RETURN CODE SET IN MWARETCD.               */ 02624000
*/*                       4-LIKE DEVICE TYPE NOT FOUND.              */ 02625000
*/*                       8-ALL VOLUMES MOUNTED.                     */ 02626000
*/*                                                                  */ 02627000
*/********************************************************************/ 02628000
*                                                                  1170 02629000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1170 02630000
VMTVOLSR DS    0H                                                  1171 02631000
*   RESPECIFY                                                      1171 02632000
*     RET RESTRICTED;                                              1171 02633000
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */ 02634000
         ST    RET,MWASAVEC                                        1172 02635000
*   RESPECIFY                                                      1173 02636000
*     RET UNRESTRICTED;                                            1173 02637000
*   MWARETCD=ZERO;                  /* ZERO RETURN CODE              */ 02638000
         SLR   @15,@15                                             1174 02639000
         ST    @15,MWARETCD                                        1174 02640000
*   DO WHILE MWARETCD=ZERO;                                        1175 02641000
         B     @DE01175                                            1175 02642000
@DL01175 DS    0H                                                  1176 02643000
*     DO WHILE(MWAVMTPT^=ZERO&VMTDEV^=MWADEVTP);/* MATCH DEVICE TYPE */ 02644000
         B     @DE01176                                            1176 02645000
@DL01176 DS    0H                                                  1177 02646000
*       MWAVMTPT=VMTNXT;            /* POINT TO NEXT VMT             */ 02647000
         L     MWAVMTPT,VMTNXT(,MWAVMTPT)                          1177 02648000
*     END;                                                         1178 02649000
@DE01176 LTR   MWAVMTPT,MWAVMTPT                                   1178 02650000
         BZ    @DC01176                                            1178 02651000
         CLC   VMTDEV(3,MWAVMTPT),MWADEVTP                         1178 02652000
         BNE   @DL01176                                            1178 02653000
@DC01176 DS    0H                                                  1179 02654000
*     IF MWAINDFG=ON&               /* IF FIRST CALL AND             */ 02655000
*         MWAVMTPT=ZERO THEN        /* LIKE DEVICE TYPE NOT FOUND    */ 02656000
         TM    MWAINDFG,B'10000000'                                1179 02657000
         BNO   @RF01179                                            1179 02658000
         LTR   MWAVMTPT,MWAVMTPT                                   1179 02659000
         BNZ   @RF01179                                            1179 02660000
*       DO;                                                        1180 02661000
*         MWAPCODE=PP68;            /* SET PROBLEM CODE              */ 02662000
         MVI   MWAPCODE,X'44'                                      1181 02663000
*         CALL PROBDT2B;            /* PROBLEM DETERMINATION         */ 02664000
         BAL   @14,PROBDT2B                                        1182 02665000
*       END;                                                       1183 02666000
*     IF MWAVMTPT=ZERO THEN         /* DEVICE TYPE NOT FOUND & NOT 1184 02667000
*                                      FIRST CALL                    */ 02668000
@RF01179 LTR   MWAVMTPT,MWAVMTPT                                   1184 02669000
         BNZ   @RF01184                                            1184 02670000
*       MWARETCD=FOUR;              /* SET RETCODE TO INDICATE DEVICE   02671000
*                                      TYPE NOT FOUND                */ 02672000
         MVC   MWARETCD(4),@CF00106                                1185 02673000
*     ELSE                          /* DEVICE TYPE FOUND             */ 02674000
*       DO;                                                        1186 02675000
