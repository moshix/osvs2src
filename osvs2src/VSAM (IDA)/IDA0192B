*/********************************************************************/ 00026000
*/*                                                                  */ 00027000
*/*                     MODULE IDA0192B                              */ 00028000
*/*                                                                  */ 00029000
*/********************************************************************/ 00030000
*                                                                  0372 00031000
*   OPWFLG2=OPWFLG2&&OPWFLG2;       /* ZERO INDICATOR FLAGS IN OPW   */ 00032000
         L     @05,OPWCURPT(,RCORE)                                0372 00033000
         XC    OPWFLG2(1,@05),OPWFLG2(@05)                         0372 00034000
*   RSTATRTN=IDAR192B;              /* INDIC RTN IN CONTROL  @Z30SSDP*/ 00035000
         L     @05,OPWCOMWA(,RCORE)                                0373 00036000
         MVC   RSTATRTN(4,@05),@CC04253                            0373 00037000
*   PDPFUNC=PDF60;                  /* SET FUNCTION CODE             */ 00038000
         AL    @05,@CF05493                                        0374 00039000
         MVI   PDPFUNC(@05),X'3C'                                  0374 00040000
*   PDPSFI=ZERO;                    /* ZERO POINTER TO SFI AREA      */ 00041000
         SLR   @15,@15                                             0375 00042000
         ST    @15,PDPSFI(,@05)                                    0375 00043000
*/********************************************************************/ 00044000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 00045000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 00046000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 00047000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 00048000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 00049000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 00050000
*/********************************************************************/ 00051000
*                                                                  0376 00052000
*   GEN;                                                           0376 00053000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00054000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00055000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00056000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00057000
*                                       IF NO ERROR WAS ENCOUNTERED     00058000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00059000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00060000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00061000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00062000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00063000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00064000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00065000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00066000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00067000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00068000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00069000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00070000
         B     GMN00200                 BYPASS SETTING ERROR            00071000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00072000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00073000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00074000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00075000
*   RESPECIFY                                                      0377 00076000
*     RETCODE RESTRICTED;                                          0377 00077000
*   IF RETCODE=ZERO THEN            /* ERROR IN BUILDING MODULE WK 0378 00078000
*                                      AREA                          */ 00079000
         LTR   RETCODE,RETCODE                                     0378 00080000
         BNZ   @RF00378                                            0378 00081000
*     DO;                                                          0379 00082000
*       OPWUACB->ACBERFLG=OPERR136; /* SET USER ACB ERROR FLAG       */ 00083000
         L     @14,OPWUACB(,RCORE)                                 0380 00084000
         MVI   ACBERFLG(@14),X'88'                                 0380 00085000
*       PDPCODE=PP20;               /* SET PROBLEM CODE              */ 00086000
         L     @14,OPWCOMWA(,RCORE)                                0381 00087000
         MVI   PDPCODE+280(@14),X'14'                              0381 00088000
*       CALL PROBDT2B;              /* PROBLEM DETERMINATION       0382 00089000
*                                      PROCESSING                    */ 00090000
         BAL   @14,PROBDT2B                                        0382 00091000
*     END;                                                         0383 00092000
*   OPWMODWK=ON;                    /* INDICATE MODULE WORK AREA   0384 00093000
*                                      EXIST                         */ 00094000
@RF00378 L     @14,OPWCURPT(,RCORE)                                0384 00095000
         OI    OPWMODWK(@14),B'01000000'                           0384 00096000
*   GEN(ST R13,4(RETCODE));         /* SAVE PREVIOUS SAVE AREA     0385 00097000
*                                      ADDRESS                       */ 00098000
         ST R13,4(RETCODE)                                              00099000
*   R13=RETCODE;                    /* POINT TO MODULE WORK AREA     */ 00100000
         LR    R13,RETCODE                                         0386 00101000
*   GEN(USING @DATD,R13);           /* ADDRESSABILITY FOR AUTOMATICS */ 00102000
         USING @DATD,R13                                                00103000
*   MWASFIPT=ADDR(OPWSFI);          /* SFI AREA ADDRESS INTO CATALOG    00104000
*                                      PARAMETER LIST                */ 00105000
         LA    @14,OPWSFI(,RCORE)                                  0388 00106000
         STCM  @14,7,MWASFIPT                                      0388 00107000
*   CALL ENQPARM;                   /* INITIALIZE ENQ PARAMETER LIST */ 00108000
         BAL   @14,ENQPARM                                         0389 00109000
*   CALL LOCVAL;                    /* LOCATE AND VALIDITY CHECK DATA   00110000
*                                      SET ATTRIBUTES                */ 00111000
         BAL   @14,LOCVAL                                          0390 00112000
*   IF OPWRSTRT=OFF THEN            /* THIS IS NOT A RESTART @Z30SSDT*/ 00113000
         TM    OPWRSTRT(RCORE),B'00001000'                         0391 00114000
         BNZ   @RF00391                                            0391 00115000
*     IF OPWUPGR=ON THEN            /* OPEN FOR UPGRADE              */ 00116000
         L     @14,OPWCURPT(,RCORE)                                0392 00117000
         TM    OPWUPGR(@14),B'00100000'                            0392 00118000
         BNO   @RF00392                                            0392 00119000
*       DO;                                                        0393 00120000
*         OPWBUFND=TWO;             /* SET NUMBER OF DATA BUFFERS    */ 00121000
         MVC   OPWBUFND(4,RCORE),@CF00140                          0394 00122000
*         OPWBUFNI=ONE;             /* SET NUMBER OF INDEX BUFFERS   */ 00123000
         MVC   OPWBUFNI(4,RCORE),@CF00059                          0395 00124000
*       END;                                                       0396 00125000
*     ELSE                          /* NOT OPEN UPGRADE              */ 00126000
*       IF OPWCAMBL=ZERO|           /* IF NOT CONNECTING OR  @OZ04213*/ 00127000
*           OPWPRTBL=ON THEN        /* A PARTIAL BUILD       @OZ04213*/ 00128000
         B     @RC00392                                            0397 00129000
@RF00392 L     @14,OPWCAMBL(,RCORE)                                0397 00130000
         LTR   @14,@14                                             0397 00131000
         BZ    @RT00397                                            0397 00132000
         L     @14,OPWCURPT(,RCORE)                                0397 00133000
