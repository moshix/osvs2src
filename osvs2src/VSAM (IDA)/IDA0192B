         MVC   ESLENQOP-9(1,@10),ENQOPT2                           1492 03681000
*   ESLRNAME(ESLACTEN)=ENQRNAME;    /* SAVE THE RNAME                */ 03682000
         LR    @10,MWAESLPT                                        1493 03683000
         AL    @10,@TF00001                                        1493 03684000
         MVC   ESLRNAME-9(8,@10),ENQRNAME                          1493 03685000
*   RESPECIFY                                                      1494 03686000
*    (RETCODE,                                                     1494 03687000
*     RET) RESTRICTED;                                             1494 03688000
*   RETCODE=ZERO;                   /* INDICATE SUCCESSFUL ENQ       */ 03689000
         SLR   RETCODE,RETCODE                                     1495 03690000
*   RET=MWASAVEF;                   /* GET RETURN ADDRESS            */ 03691000
         L     RET,MWASAVEF                                        1496 03692000
*   RESPECIFY                                                      1497 03693000
*    (RETCODE,                                                     1497 03694000
*     RET) UNRESTRICTED;                                           1497 03695000
*   END ENQSAVE;                                                   1498 03696000
@EL00030 DS    0H                                                  1498 03697000
@EF00030 DS    0H                                                  1498 03698000
@ER00030 BR    @14                                                 1498 03699000
*ERRORFLG:                                                         1499 03700000
*                                                                  1499 03701000
*/********************************************************************/ 03702000
*/*                                                                  */ 03703000
*/*    01 FUNCTION - THIS ROUTINE DETERMINES THE ACBERFLG.           */ 03704000
*/*    01 INPUT -    1. PROBLEM DETERMINATION PROBLEM CODE (MWAPCODE)*/ 03705000
*/*                  2. PROB DETERMINATION FUNCTION CODE (MWAPFUNC)  */ 03706000
*/*                  3. PROB DET SUB FUNCTION CODE (MWAPSFC)         */ 03707000
*/*    01 OUTPUT -   1. PROBLEM DETERMINATION PARAMETER LIST IS      */ 03708000
*/*                     COMPLETED                                    */ 03709000
*/*                  2. THE ACBERFLG IS DETERMINED AND SET           */ 03710000
*/*                                                                  */ 03711000
*/********************************************************************/ 03712000
*                                                                  1499 03713000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1499 03714000
ERRORFLG DS    0H                                                  1500 03715000
*   RESPECIFY                                                      1500 03716000
*    (RET,                                                         1500 03717000
*     R6) RESTRICTED;                                              1500 03718000
*   MWASAVEF=RET;                   /* SAVE RETURN ADDRESS           */ 03719000
         ST    RET,MWASAVEF                                        1501 03720000
*   RESPECIFY                                                      1502 03721000
*     RET UNRESTRICTED;                                            1502 03722000
*   PDPCODE=MWAPCODE;               /* COPY ERROR CODE               */ 03723000
         LA    @10,280                                             1503 03724000
         AL    @10,OPWCOMWA(,RCORE)                                1503 03725000
         IC    @15,MWAPCODE                                        1503 03726000
         STC   @15,PDPCODE(,@10)                                   1503 03727000
*   PDPFUNC=MWAPFUNC;               /* COPY FUNCTION TYPE            */ 03728000
         IC    @15,MWAPFUNC                                        1504 03729000
         STC   @15,PDPFUNC(,@10)                                   1504 03730000
*   PDPSFC=MWAPSFC;                 /* GET SUB FUNCTION CODE         */ 03731000
         IC    @15,MWAPSFC                                         1505 03732000
         STC   @15,PDPSFC(,@10)                                    1505 03733000
*   IF PDPSFI=ZERO&SFIRETCD^=ZERO THEN/* SUB-FUNCTION INFO EXIST     */ 03734000
         SLR   @15,@15                                             1506 03735000
         C     @15,PDPSFI(,@10)                                    1506 03736000
         BNE   @RF01506                                            1506 03737000
         C     @15,SFIRETCD+296(,RCORE)                            1506 03738000
         BE    @RF01506                                            1506 03739000
*     PDPSFI=MWASFIPT;              /* POINT TO SFI AREA             */ 03740000
         L     @15,MWASFIPT-1                                      1507 03741000
         LA    @15,0(,@15)                                         1507 03742000
         ST    @15,PDPSFI(,@10)                                    1507 03743000
*   ELSE                            /* NO SUB-FUNCTION INFORMATIONS  */ 03744000
*     PDPSFI=ZERO;                  /* ZERO SFI POINTER              */ 03745000
         B     @RC01506                                            1508 03746000
@RF01506 L     @10,OPWCOMWA(,RCORE)                                1508 03747000
         SLR   @15,@15                                             1508 03748000
         ST    @15,PDPSFI+280(,@10)                                1508 03749000
*   IF OPWUACB->ACBERFLG<OPWERMAP(PDPCODE) THEN/* IF CURRENT ERROR IS   03750000
*                                      GREATER                       */ 03751000
@RC01506 L     @10,OPWUACB(,RCORE)                                 1509 03752000
         L     @01,OPWCOMWA(,RCORE)                                1509 03753000
         SLR   @15,@15                                             1509 03754000
         IC    @15,PDPCODE+280(,@01)                               1509 03755000
         ST    @15,@TF00001                                        1509 03756000
         ALR   @15,RCORE                                           1509 03757000
         CLC   ACBERFLG(1,@10),OPWERMAP-1(@15)                     1509 03758000
         BNL   @RF01509                                            1509 03759000
*     OPWUACB->ACBERFLG=OPWERMAP(PDPCODE);/* SET GREATER ERROR FLAG  */ 03760000
         LR    @01,RCORE                                           1510 03761000
         AL    @01,@TF00001                                        1510 03762000
         MVC   ACBERFLG(1,@10),OPWERMAP-1(@01)                     1510 03763000
*   RESPECIFY                                                      1511 03764000
*     RET RESTRICTED;                                              1511 03765000
@RF01509 DS    0H                                                  1512 03766000
*   RET=MWASAVEF;                   /* RESTORE RETURN ADDRESS        */ 03767000
         L     RET,MWASAVEF                                        1512 03768000
*   RESPECIFY                                                      1513 03769000
*    (RET,                                                         1513 03770000
*     R6) UNRESTRICTED;                                            1513 03771000
*   END ERRORFLG;                                                  1514 03772000
@EL00031 DS    0H                                                  1514 03773000
@EF00031 DS    0H                                                  1514 03774000
@ER00031 BR    @14                                                 1514 03775000
*GETCORE:                                                          1515 03776000
*                                                                  1515 03777000
*/********************************************************************/ 03778000
*/*                                                                  */ 03779000
*/*      01   FUNCTION-ISSUE GETMAIN TO OBTAIN CORE STORAGE          */ 03780000
*/*      01   INPUT -  1. SUBPOOL NUMBER IN MWASPOOL                 */ 03781000
*/*                    2. NUMBER OF BYTES REQUIRED IN MWABYTES       */ 03782000
*/*                    3. STORAGE PROTECTION KEY OF THE CORE @ZA16012*/ 03783000
*/*                       TO BE GOTTEN IN MWAKEY             @ZA16012*/ 03784000
*/*      01   OUTPUT - 1. ADDRESS OF GOTTEN CORE IN MWADDRES         */ 03785000
*/*                    2. IF GETMAIN FAILED, PROBLEM DETERMINATION   */ 03786000
*/*                       IS CALLED.                                 */ 03787000
*/*                    3. NOTE THAT MWAKEY IS NOT RESET TO   @ZA16012*/ 03788000
*/*                       ZERO, HENCE LATER CALLS TO GETCORE @ZA16012*/ 03789000
*/*                       OR FREECORE SHOULD INSURE THIS     @ZA16012*/ 03790000
*/*                       FIELD IS INITIALIZED CORRECTLY.    @ZA16012*/ 03791000
*/*                                                                  */ 03792000
*/********************************************************************/ 03793000
*                                                                  1515 03794000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1515 03795000
GETCORE  DS    0H                                                  1516 03796000
*   RESPECIFY                                                      1516 03797000
*    (R1,                                                          1516 03798000
*     R6,                                                          1516 03799000
*     R7,                                                          1516 03800000
*     R8,                                                          1516 03801000
*     R9,                                                          1516 03802000
*     RET) RESTRICTED;              /*                       @Z30SSDP*/ 03803000
*   MWASAVEG=RET;                   /* SAVE RETURN ADDRESS           */ 03804000
         ST    RET,MWASAVEG                                        1517 03805000
*   RESPECIFY                                                      1518 03806000
*     RET UNRESTRICTED;                                            1518 03807000
*   IF MWAKEY^=ZERO THEN            /* IF CORE TO BE GOTTEN IN     1519 03808000
*                                      NONZERO KEY           @ZA16012*/ 03809000
         CLI   MWAKEY,0                                            1519 03810000
         BE    @RF01519                                            1519 03811000
*     DO;                           /* CHANGE KEYS           @ZA16012*/ 03812000
*       RFY                                                        1521 03813000
*         R1 RSTD;                  /* USE THIS REG          @ZA16012*/ 03814000
*       R1=MWAKEY;                  /* SET CALLER'S KEY      @ZA16012*/ 03815000
         SLR   R1,R1                                               1522 03816000
         IC    R1,MWAKEY                                           1522 03817000
*       SPKA(R1);                   /* CHANGE KEYS           @ZA16012*/ 03818000
         SPKA  0(R1)                                               1523 03819000
*       RFY                                                        1524 03820000
*         R1 UNRSTD;                /* FREE REG              @ZA16012*/ 03821000
*     END;                          /* END CHANGE KEYS       @ZA16012*/ 03822000
*   ELSE                                                           1526 03823000
*     ;                             /* CORE IS IN KEY ZERO   @ZA16012*/ 03824000
@RF01519 DS    0H                                                  1527 03825000
*   R6=MWASPOOL;                    /* SUBPOOL NUMBER                */ 03826000
         SLR   R6,R6                                               1527 03827000
         IC    R6,MWASPOOL                                         1527 03828000
*   R7=MWABYTES;                    /* NUMBER OF BYTES REQUIRED      */ 03829000
         L     R7,MWABYTES-1                                       1528 03830000
         LA    R7,0(,R7)                                           1528 03831000
*   GEN(GETMAIN RC,LV=(R7),SP=(R6)) /*                       @Z30SSDP*/ 03832000
*   SETS(RETCODE);                  /* ISSUE GETMAIN         @Z30SSDP*/ 03833000
         GETMAIN RC,LV=(R7),SP=(R6)                                     03834000
*   RESPECIFY                                                      1530 03835000
*     RETCODE RESTRICTED;                                          1530 03836000
*   SPKA(0);                        /* RETURN TO KEY ZERO    @ZA16012*/ 03837000
         SLR   @14,@14                                             1531 03838000
         SPKA  0(@14)                                              1531 03839000
*   IF RETCODE=ZERO THEN            /* GETMAIN SUCCESSFUL            */ 03840000
         CR    RETCODE,@14                                         1532 03841000
         BNE   @RF01532                                            1532 03842000
*     DO;                                                          1533 03843000
*       MWADDRES=R1;                /* PTR TO GOTTEN CORE    @Z30SSDP*/ 03844000
         ST    R1,MWADDRES                                         1534 03845000
*       R6=MWADDRES;                /* ADDRESS OF GOTTEN CORE        */ 03846000
         L     @14,MWADDRES                                        1535 03847000
         LR    R6,@14                                              1535 03848000
*       R9=ZERO;                    /* ZERO PAD BYTE                 */ 03849000
         SLR   R9,R9                                               1536 03850000
*       GEN(MVCL R6,R8)                                            1537 03851000
*       NOSETS;                     /* ZERO GOTTEN CORE              */ 03852000
         MVCL R6,R8                                                     03853000
*       RESPECIFY                                                  1538 03854000
*        (R1,                                                      1538 03855000
*         R6,                                                      1538 03856000
*         R7,                                                      1538 03857000
*         R8,                                                      1538 03858000
*         R9) UNRESTRICTED;         /*                       @Z30SSDP*/ 03859000
*     END;                                                         1539 03860000
*   ELSE                            /* IF GETMAIN FAILED             */ 03861000
*     DO;                                                          1540 03862000
         B     @RC01532                                            1540 03863000
@RF01532 DS    0H                                                  1541 03864000
*       MWAPCODE=PP20;              /* SET PROBLEM CODE              */ 03865000
         MVI   MWAPCODE,X'14'                                      1541 03866000
*       CALL PROBDT2B;              /* CALL PROBLEM DETERMINATION    */ 03867000
         BAL   @14,PROBDT2B                                        1542 03868000
*     END;                                                         1543 03869000
*   RESPECIFY                                                      1544 03870000
*     RET RESTRICTED;                                              1544 03871000
@RC01532 DS    0H                                                  1545 03872000
*   RET=MWASAVEG;                   /* GET RETURN ADDRESS            */ 03873000
         L     RET,MWASAVEG                                        1545 03874000
*   RESPECIFY                                                      1546 03875000
*    (RET,                                                         1546 03876000
*     RETCODE) UNRESTRICTED;                                       1546 03877000
*   END GETCORE;                                                   1547 03878000
@EL00032 DS    0H                                                  1547 03879000
@EF00032 DS    0H                                                  1547 03880000
@ER00032 BR    @14                                                 1547 03881000
*FREECORE:                                                         1548 03882000
*                                                                  1548 03883000
*/********************************************************************/ 03884000
*/*                                                                  */ 03885000
*/*      01   FUNCTION-FREEMAIN CORE STORAGE                         */ 03886000
*/*      01   INPUT -  1. SUBPOOL NUMBER IN MWASPOOL                 */ 03887000
*/*                    2. NUMBER OF BYTES TO BE FREED IN MWABYTES    */ 03888000
*/*                    3. ADDRESS OF CORE TO BE FREED IN MWADDRES    */ 03889000
*/*                    4. STORAGE PROTECTION KEY OF THE CORE @ZA16012*/ 03890000
*/*                       TO BE GOTTEN IN MWAKEY             @ZA16012*/ 03891000
*/*      01   OUTPUT - 1. CORE STORAGE FREED                         */ 03892000
*/*                    2. NOTE THAT MWAKEY IS NOT RESET TO   @ZA16012*/ 03893000
*/*                       ZERO, HENCE LATER CALLS TO GETCORE @ZA16012*/ 03894000
*/*                       OR FREECORE SHOULD INSURE THIS     @ZA16012*/ 03895000
*/*                       FIELD IS INITIALIZED CORRECTLY.    @ZA16012*/ 03896000
*/*                                                                  */ 03897000
*/********************************************************************/ 03898000
*                                                                  1548 03899000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1548 03900000
FREECORE DS    0H                                                  1549 03901000
*   RESPECIFY                                                      1549 03902000
*    (R0,                                                          1549 03903000
*     R1,                                                          1549 03904000
*     RET) RESTRICTED;                                             1549 03905000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 03906000
         ST    RET,MWASAVEB                                        1550 03907000
*   RESPECIFY                                                      1551 03908000
*     RET UNRESTRICTED;                                            1551 03909000
*   IF MWAKEY^=ZERO THEN            /* IF CORE TO BE GOTTEN IN     1552 03910000
*                                      NONZERO KEY           @ZA16012*/ 03911000
         CLI   MWAKEY,0                                            1552 03912000
         BE    @RF01552                                            1552 03913000
*     DO;                           /* CHANGE KEYS           @ZA16012*/ 03914000
*       RFY                                                        1554 03915000
*         R1 RSTD;                  /* USE THIS REG          @ZA16012*/ 03916000
*       R1=MWAKEY;                  /* SET CALLER'S KEY      @ZA16012*/ 03917000
         SLR   R1,R1                                               1555 03918000
         IC    R1,MWAKEY                                           1555 03919000
*       SPKA(R1);                   /* CHANGE KEYS           @ZA16012*/ 03920000
         SPKA  0(R1)                                               1556 03921000
*       RFY                                                        1557 03922000
*         R1 UNRSTD;                /* FREE REG              @ZA16012*/ 03923000
*     END;                          /* END CHANGE KEYS       @ZA16012*/ 03924000
*   ELSE                                                           1559 03925000
*     ;                             /* CORE IS IN KEY ZERO   @ZA16012*/ 03926000
@RF01552 DS    0H                                                  1560 03927000
*   R1=MWADDRES;                    /* ADDRESS OF CORE TO BE FREED   */ 03928000
         L     R1,MWADDRES                                         1560 03929000
*   R0=MWAMNWRD;                    /* SUBPOOL NO. AND BYTES TO BE 1561 03930000
*                                      FREE                          */ 03931000
         L     R0,MWAMNWRD                                         1561 03932000
*   GEN(FREEMAIN R,LV=(R0),A=(R1)); /* FREEMAIN                      */ 03933000
         FREEMAIN R,LV=(R0),A=(R1)                                      03934000
*   RESPECIFY                                                      1563 03935000
*     RET RESTRICTED;                                              1563 03936000
*   SPKA(0);                        /* RETURN TO KEY ZERO    @ZA16012*/ 03937000
         SLR   @01,@01                                             1564 03938000
         SPKA  0(@01)                                              1564 03939000
*   RET=MWASAVEB;                   /* GET RETURN ADDRESS            */ 03940000
         L     RET,MWASAVEB                                        1565 03941000
*   RESPECIFY                                                      1566 03942000
*    (R0,                                                          1566 03943000
*     R1,                                                          1566 03944000
*     RET) UNRESTRICTED;                                           1566 03945000
*   END FREECORE;                                                  1567 03946000
@EL00033 DS    0H                                                  1567 03947000
@EF00033 DS    0H                                                  1567 03948000
@ER00033 BR    @14                                                 1567 03949000
@PB00033 DS    0H                                                  1567 03950000
*   END IDA0192B                                                   1568 03951000
*                                                                  1568 03952000
