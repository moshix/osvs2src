         ALR   @01,MWABUFNI                                        0681 01077000
         CR    @10,@01                                             0681 01078000
         BNL   *+6                                                      01079000
         LR    @10,@01                                             0681 01080000
         LR    MWABUFNI,@10                                        0681 01081000
*           MWABUFSP=(MWACISZI*MWABUFNI+MWABFSPD);/* UPDATE CURRENT     01082000
*                                      BUFFER SPACE                  */ 01083000
         LR    @01,MWABUFNI                                        0682 01084000
         MR    @00,MWACISZI                                        0682 01085000
         AL    @01,MWABFSPD                                        0682 01086000
         LR    MWABUFSP,@01                                        0682 01087000
*         END;                                                     0683 01088000
*       IF MWABUFSP>MWABFSP THEN    /* CURRENT BUFFER SPACE TOO BIG  */ 01089000
@RF00679 L     @10,MWABFSP                                         0684 01090000
         CR    MWABUFSP,@10                                        0684 01091000
         BNH   @RF00684                                            0684 01092000
*         DO;                                                      0685 01093000
*           MWABUFND=MAX(MWARQDBF,(MWABUFND-(MWABUFSP+MWACISZD-ONE-     01094000
*               MWABFSP)/MWACISZD));/* DECREASE DATA BUFFERS         */ 01095000
         LR    @00,MWABUFSP                                        0686 01096000
         ALR   @00,MWACISZD                                        0686 01097000
         BCTR  @00,0                                               0686 01098000
         SLR   @00,@10                                             0686 01099000
         SRDA  @00,32                                              0686 01100000
