*   RESPECIFY                                                      1135 02527000
*     RET RESTRICTED;                                              1135 02528000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 02529000
         ST    RET,MWASAVEB                                        1136 02530000
*   RESPECIFY                                                      1137 02531000
*     RET UNRESTRICTED;                                            1137 02532000
*   MWAINDFG=ON;                    /* SET INDICATOR FLAG TO INDICATE   02533000
*                                      FIRST CALL                    */ 02534000
         OI    MWAINDFG,B'10000000'                                1138 02535000
*   MWAVMTPT=BIBVMT;                /* POINT TO VMT FROM BIB         */ 02536000
         L     @01,ACBAMBL(,RACB)                                  1139 02537000
         L     @01,AMBLBIB(,@01)                                   1139 02538000
