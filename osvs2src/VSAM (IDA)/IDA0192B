         BZ    @RF01285                                            1285 03024000
*             AMBLIX->AMBOPEN=ON;   /* TURN ON INDEX AMB'S OPEN    1286 03025000
*                                      INDICATOR                     */ 03026000
         OI    AMBOPEN(@05),B'00010000'                            1286 03027000
*         END;                                                     1287 03028000
@RF01285 DS    0H                                                  1288 03029000
*       MWASPOOL=WKPOOL;            /* SET SUBPOOL NUMBER            */ 03030000
@RF01280 MVI   MWASPOOL,X'FC'                                      1288 03031000
*       IF OPWDTWRK^=ZERO&          /* DATA AMB WORK AREA EXIST AND  */ 03032000
*           OPWSVWRK=OFF THEN       /* OPW INDICATE OKAY TO FREE     */ 03033000
         L     @05,OPWDTWRK(,RCORE)                                1289 03034000
         LTR   @05,@05                                             1289 03035000
