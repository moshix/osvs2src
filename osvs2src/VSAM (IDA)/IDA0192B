*/********************************************************************/ 00351000
*                                                                  0477 00352000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0477 00353000
LOCVAL   DS    0H                                                  0478 00354000
*   RESPECIFY                                                      0478 00355000
*     RET RESTRICTED;                                              0478 00356000
*   RESPECIFY                                                      0479 00357000
*     IDAAMDSB BASED(ADDR(CTAMDSB));/* REBASE AMDSB                  */ 00358000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 00359000
         ST    RET,MWASAVEA                                        0480 00360000
*   RESPECIFY                                                      0481 00361000
*     RET UNRESTRICTED;                                            0481 00362000
*   MWAPFUNC=PDF61;                 /* SET FUNCTION CODE             */ 00363000
         MVI   MWAPFUNC,X'3D'                                      0482 00364000
*   IF OPWCAMBL^=ZERO&ACBRST=ON THEN/* CONNECTING TO EXISTING DATA 0483 00365000
*                                      SET AND RESET INDICATED       */ 00366000
         L     @05,OPWCAMBL(,RCORE)                                0483 00367000
         LTR   @05,@05                                             0483 00368000
         BZ    @RF00483                                            0483 00369000
         TM    ACBRST(RACB),B'00000100'                            0483 00370000
         BNO   @RF00483                                            0483 00371000
*     DO;                                                          0484 00372000
*       MWAPCODE=PP52;              /* SET PROBLEM CODE              */ 00373000
         MVI   MWAPCODE,X'34'                                      0485 00374000
*       CALL PROBDT2B;              /* PROBLEM DETERMINATION         */ 00375000
         BAL   @14,PROBDT2B                                        0486 00376000
*     END;                                                         0487 00377000
*   IF OPWDTWRK=ZERO THEN           /* NO AMB WORK AREAS     @ZA03663*/ 00378000
@RF00483 L     @05,OPWDTWRK(,RCORE)                                0488 00379000
         LTR   @05,@05                                             0488 00380000
