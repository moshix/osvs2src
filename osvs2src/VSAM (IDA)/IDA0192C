*       DSNDD=JFCBDSNM;             /* INIT DSNAME FROM DD STMT      */ 00379000
         L     @02,OPWCOMWA(,RCORE)                                0326 00380000
         MVC   DSNDD(44),JFCBDSNM+100(@02)                         0326 00381000
*       CALL CTINTRFC;              /* INIT FIELDS,PARM LIST, AND  0327 00382000
*                                      ISSUE SVC 26          @X04SVGP*/ 00383000
         BAL   @14,CTINTRFC                                        0327 00384000
*       IF PPCODE=0 THEN            /* ANY ERRORS            @X04SVGP*/ 00385000
         CLI   PPCODE,0                                            0328 00386000
         BNE   @RF00328                                            0328 00387000
*         DO;                       /* NO, CONTINUE          @X04SVGP*/ 00388000
*           CTGFLPTR=CTGFIELD(1);   /* INIT CURRENT FIELD PTR        */ 00389000
