*LOC2:                                                             0426 00728000
*                                                                  0426 00729000
*/********************************************************************/ 00730000
*/*                                                          @X04SVGP*/ 00731000
*/*     01 FUNCTION - LOCATE ALL DATA SET AND VOLUME INFORMATION FOR */ 00732000
*/*                   EACH DATA SET BELONGING TO THE CLUSTER.  INPUT */ 00733000
*/*                   TO THE CATALOG ROUTINES ARE THE CI NBR AND     */ 00734000
*/*                   ADDRESS OF THE CATALOG ACB,  CATALOG IS INVOKED*/ 00735000
*/*                   ONCE PER CI NBR.                       @X04SVGP*/ 00736000
*/*                                                          @X04SVGP*/ 00737000
*/*     01 INPUT    - ACB WORK AREA, LOCATE DESCRIPTOR       @X04SVGP*/ 00738000
*/*                                                          @X04SVGP*/ 00739000
*/*     02 OUTPUT   - ALL DATA SET ATTRIBUTES AND VOLUME INFORMATION */ 00740000
*/*                   ARE MERGED INTO THE AMB WORK AREAS.    @X04SVGP*/ 00741000
*/*                                                          @X04SVGP*/ 00742000
*/********************************************************************/ 00743000
*                                                                  0426 00744000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @X04SVGP*/ 00745000
LOC2     DS    0H                                                  0427 00746000
*   RESPECIFY                                                      0427 00747000
*     AMBWORK BASED(RCOREF);        /*                       @X04SVGP*/ 00748000
*   RSAVLA=RET;                     /* SAVE RETURN REG       @X04SVGP*/ 00749000
         ST    RET,RSAVLA                                          0428 00750000
*   CALL CLUPDESR;                  /* CLEAR UPDATE DESCRIPTOR RECORD*/ 00751000
         BAL   @14,CLUPDESR                                        0429 00752000
*   LOCTYPE=LOCTYPE&&LOCTYPE;       /* CLEAR LOCATE TYPE     @X04SVGP*/ 00753000
         XC    LOCTYPE(1),LOCTYPE                                  0430 00754000
*   PPFUNC=PDF3;                    /* FUNCTION ID           @X04SVGP*/ 00755000
         MVI   PPFUNC,X'03'                                        0431 00756000
*   CALL LOC2INIT;                  /* BUILD AMB WORK        @X04SVGP*/ 00757000
         BAL   @14,LOC2INIT                                        0432 00758000
*   IF PPCODE=0 THEN                /* ANY ERRORS            @X04SVGP*/ 00759000
         CLI   PPCODE,0                                            0433 00760000
         BNE   @RF00433                                            0433 00761000
*     DO;                           /* NO, CONTINUE          @X04SVGP*/ 00762000
*       DSNCI=DSNDCI;               /* PROCESS DATA SET FIRST        */ 00763000
         MVC   DSNCI(3),DSNDCI                                     0435 00764000
*       ID3=ON;                     /* INDICATE LOCATE BY CI NBR     */ 00765000
