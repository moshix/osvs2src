*   DSCATACB=DSPACBPT;              /* INIT CAT ACB PTR              */ 01523000
         L     @02,DSPCIPT1                                        0667 01524000
         L     @02,DSPACBPT(,@02)                                  0667 01525000
         ST    @02,DSCATACB                                        0667 01526000
*   PSWRD=ON;                       /* PERFORM AUTHORIZATION CHK     */ 01527000
         OI    PSWRD,B'00000010'                                   0668 01528000
*   DO CTRB=1 TO DSPCILEN/LENGTH(DSPCINUM(1)) WHILE PPCODE=0;      0669 01529000
         LA    @02,1                                               0669 01530000
         B     @DE00669                                            0669 01531000
@DL00669 CLI   PPCODE,0                                            0669 01532000
