         EX    @14,@SX03933                                        0950 02654000
*   CTGFLPTR=CTGPLPTR+R9;           /* INIT PTR TO 1ST FIELD LIST    */ 02655000
         ALR   @15,R9                                              0951 02656000
         LR    CTGFLPTR,@15                                        0951 02657000
*   IF PSWRD=ON THEN                /* AUTHORIZATION CHECK REQUIRED  */ 02658000
         TM    PSWRD,B'00000010'                                   0952 02659000
         BNO   @RF00952                                            0952 02660000
*     CALL AUTHCHK;                 /* YES, SPECIAL PROCESSING       */ 02661000
         BAL   @14,AUTHCHK                                         0953 02662000
*   ELSE                            /* NO,                           */ 02663000
*     CTGBYPSS=ON;                  /* BYPASS SECURITY CHK           */ 02664000
*                                                                  0954 02665000
         B     @RC00952                                            0954 02666000
@RF00952 L     @01,CTGPLPTR                                        0954 02667000
         OI    CTGBYPSS(@01),B'10000000'                           0954 02668000
*   /*****************************************************************/ 02669000
*   /*                                                               */ 02670000
*   /* IF JSCBPASS IS ON THEN BYPASS AUTHORIZATION CHECKING.         */ 02671000
*   /*                                                               */ 02672000
*   /*****************************************************************/ 02673000
*                                                                  0955 02674000
