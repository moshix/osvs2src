*   FLACTIVE(1,1)=ON;               /* ACTIVATE LOCATE ELEMENT       */ 01518000
         OI    FLACTIVE,B'10000000'                                0665 01519000
*   FLNAME(1,1)='PASSATMP';         /* INIT SYMBOLIC FIELD NAME    0666 01520000
*                                                            @XA04272*/ 01521000
