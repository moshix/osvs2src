*     PPFUNC=0;                     /* NO, ZERO RETURN CODE          */ 00248000
         MVI   PPFUNC,X'00'                                        0279 00249000
*   R6=PD;                          /* LOAD RETURN CODE              */ 00250000
@RF00278 L     R6,PD                                               0280 00251000
*   MAINPOOL=WKPOOL;                /* SET SUBPOOL FOR FREEMAIN      */ 00252000
         MVI   MAINPOOL,X'FC'                                      0281 00253000
*   GEN(L R7,@SIZDATD);             /* LOAD BLOCK SIZE               */ 00254000
         L R7,@SIZDATD                                                  00255000
*   MAINLEN=R7;                     /* SET LENGTH FOR FREEMAIN       */ 00256000
