@RF01070 MVI   PPCODE,X'20'                                        1176 03535000
*   RET=RSAVLC;                     /* RESTORE RETURN REG            */ 03536000
@RC01070 L     RET,RSAVLC                                          1177 03537000
*   END MERGVOL;                    /* RETURN TO CALLER              */ 03538000
@EL00023 DS    0H                                                  1178 03539000
@EF00023 DS    0H                                                  1178 03540000
@ER00023 BR    @14                                                 1178 03541000
*AUTHCHK:                                                          1179 03542000
*                                                                  1179 03543000
*/********************************************************************/ 03544000
*/*                                                                  */ 03545000
*/*     01 FUNCTION - DETERMINE LEVEL OF AUTHORIZATION REQUIRED AND  */ 03546000
*/*                   SET CORRESPONDING INDICATION IN CPL            */ 03547000
*/*                                                                  */ 03548000
*/*     01 INPUT    - ACB, CALLER ID, CPL, AND AMB WORK AREA         */ 03549000
*/*                                                                  */ 03550000
*/*     01 OUTPUT   - AUTHORIZATION LEVEL REQUIRED IS SET IN CPL AND */ 03551000
*/*                   AMB WORK AREA.  THE CPL PASSWORD PTR IS        */ 03552000
*/*                   INITIALIZED IF A PASSWORD WAS SPECIFIED        */ 03553000
*/*                                                                  */ 03554000
*/********************************************************************/ 03555000
*                                                                  1179 03556000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1179 03557000
AUTHCHK  DS    0H                                                  1180 03558000
*   RESPECIFY                                                      1180 03559000
*     IFGACB BASED(ACBPT);                                         1180 03560000
*   RSAVLD=RET;                                                    1181 03561000
         ST    RET,RSAVLD                                          1181 03562000
*   RCOREF=DATAWAPT;                /* PT TO DATA CAT REC            */ 03563000
         L     @15,PARMPT                                          1182 03564000
         L     @14,DATAWAPT(,@15)                                  1182 03565000
         ST    @14,RCOREF                                          1182 03566000
*   IF ID^=OPP THEN                 /* FIRST OPEN LOCATE             */ 03567000
         CLI   ID(@15),16                                          1183 03568000
         BE    @RF01183                                            1183 03569000
*     DO;                           /* YES, CONTINUE                 */ 03570000
*       IF OPWUCRA=ON THEN          /* IF UCRA               @X04SVRP*/ 03571000
         TM    OPWUCRA(RCORE),B'10000000'                          1185 03572000
         BNO   @RF01185                                            1185 03573000
*         CTGBYPSS=ON;              /* THEN IND BYPASS SECURITY    1186 03574000
*                                      CHECK. CATALOG FORCES A     1186 03575000
*                                      SECURITY CHECK ON FIRST LOCATE   03576000
*                                      FOR A UCRA            @X04SVRP*/ 03577000
         L     @01,CTGPLPTR                                        1186 03578000
         OI    CTGBYPSS(@01),B'10000000'                           1186 03579000
*       ELSE                        /* ELSE SET SECURITY LEVEL     1187 03580000
*                                                            @X04SVRP*/ 03581000
*         DO;                       /* SET AUTH LEVEL        @X04SVRP*/ 03582000
         B     @RC01185                                            1187 03583000
@RF01185 DS    0H                                                  1188 03584000
*           IF ACBCNV=ON THEN       /* CONTROL INTERVAL PROCESSING   */ 03585000
         L     @01,ACBPT                                           1188 03586000
         TM    ACBCNV(@01),B'00100000'                             1188 03587000
         BNO   @RF01188                                            1188 03588000
*             DO;                   /* YES,                          */ 03589000
*               CTGCI=ON;           /* SET AUTHORIZATION LEVEL       */ 03590000
         L     @01,CTGPLPTR                                        1190 03591000
         OI    CTGCI(@01),B'00100000'                              1190 03592000
*               OPWCINV=ON;         /* ALSO IN CATALOG W.A.  @X04SVGP*/ 03593000
*               OPWUPD=ON;          /*                       @X04SVGP*/ 03594000
         OI    OPWCINV(RCORE),B'01100000'                          1192 03595000
*             END;                                                 1193 03596000
*           ELSE                    /* NOT CI PROCESSING     @Z30SSRL*/ 03597000
*             DO;                                                  1194 03598000
         B     @RC01188                                            1194 03599000
@RF01188 DS    0H                                                  1195 03600000
*               IF ACBOUT=ON|ACBRST=ON THEN/* OUTPUT OR UPDATE     1195 03601000
*                                                            @X04SVGP*/ 03602000
         L     @15,ACBPT                                           1195 03603000
         TM    ACBOUT(@15),B'00000010'                             1195 03604000
         BO    @RT01195                                            1195 03605000
         TM    ACBRST(@15),B'00000100'                             1195 03606000
         BNO   @RF01195                                            1195 03607000
@RT01195 DS    0H                                                  1196 03608000
*                 DO;               /* YES,                          */ 03609000
*                   CTGUPD=ON;      /* SET UPD AUTH LEVEL            */ 03610000
         L     @01,CTGPLPTR                                        1197 03611000
         OI    CTGUPD(@01),B'00010000'                             1197 03612000
*                   OPWUPD=ON;      /* ALSO IN CATALOG W.A.  @X04SVGP*/ 03613000
         OI    OPWUPD(RCORE),B'00100000'                           1198 03614000
*                 END;                                             1199 03615000
*               ELSE                /* NOT OUTPUT OR UPDATE          */ 03616000
*                 CTGREAD=ON;       /* READ LEVEL REQUIRED           */ 03617000
         B     @RC01195                                            1200 03618000
@RF01195 L     @01,CTGPLPTR                                        1200 03619000
         OI    CTGREAD(@01),B'00001000'                            1200 03620000
*             END;                                                 1201 03621000
@RC01195 DS    0H                                                  1202 03622000
*         END;                                                     1202 03623000
@RC01188 DS    0H                                                  1203 03624000
*       IF ACBPASSW^=0&PWL^=0 THEN  /* ACB PASSWORD EXIST            */ 03625000
@RC01185 L     @01,ACBPT                                           1203 03626000
         L     @15,ACBPASSW(,@01)                                  1203 03627000
         LTR   @15,@15                                             1203 03628000
         BZ    @RF01203                                            1203 03629000
         CLI   PWL(@15),0                                          1203 03630000
         BE    @RF01203                                            1203 03631000
*         DO;                       /* YES, CONTINUE                 */ 03632000
*           MYPW=BLANKS;            /* BLANK PASSWORD FIELD          */ 03633000
         MVC   MYPW(8),BLANKS                                      1205 03634000
*           MYPW(1:MIN(PWL,8))=PWP(1:MIN(PWL,8));/* MOVE IN USER   1206 03635000
*                                      PASSWD                        */ 03636000
         SLR   @14,@14                                             1206 03637000
         IC    @14,PWL(,@15)                                       1206 03638000
