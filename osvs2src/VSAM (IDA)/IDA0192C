*   DSCATACB=AMBCACB;               /* INIT PTR TO CAT ACB           */ 01424000
         L     @02,AMBCACB(,R5)                                    0636 01425000
         ST    @02,DSCATACB                                        0636 01426000
*   DSNCI=AMBDDSN;                  /* INIT DATA SET CINBR           */ 01427000
         MVC   DSNCI(3),AMBDDSN(R5)                                0637 01428000
*   CALL CTINTRFC;                  /* UPDATE OPEN INDICATOR         */ 01429000
         BAL   @14,CTINTRFC                                        0638 01430000
*   RET=RSAVLB;                     /* RESTORE RETURN REG            */ 01431000
         L     RET,RSAVLB                                          0639 01432000
*   END OPOBIT;                     /* RETURN TO CALLER              */ 01433000
@EL00008 DS    0H                                                  0640 01434000
@EF00008 DS    0H                                                  0640 01435000
@ER00008 BR    @14                                                 0640 01436000
*OPPSWD:                                                           0641 01437000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0641 01438000
*                                                                  0641 01439000
OPPSWD   DS    0H                                                  0642 01440000
*/********************************************************************/ 01441000
*/*                                                                  */ 01442000
*/*  01 FUNCTION - PERFORM MASTER LEVEL AUTHORIZATION CHECK ON EVERY */ 01443000
*/*                DATA SET RESIDING WITHIN THE SPECIFIED DATA SPACE */ 01444000
*/*                                                                  */ 01445000
*/*  01 INPUT    - DATA SPACE NAME AND ASSOCIATED VOLUME SERIAL NBR  */ 01446000
*/*                PASSED BY CALLER                                  */ 01447000
*/*                                                                  */ 01448000
*/*  01 OUTPUT   - MASTER LEVEL AUTHORIZATION VERIFIED FOR ALL DATA  */ 01449000
*/*                SETS                                              */ 01450000
*/*                                                                  */ 01451000
*/********************************************************************/ 01452000
*                                                                  0642 01453000
*   RSAVLA=RET;                                                    0642 01454000
         ST    RET,RSAVLA                                          0642 01455000
*   NAME6=ON;                       /* LOCATE BY VOL SER NBR         */ 01456000
         OI    NAME6,B'01000000'                                   0643 01457000
