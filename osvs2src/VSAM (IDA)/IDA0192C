         CR    @14,@11                                             1206 03640000
         BNH   *+6                                                      03641000
         LR    @14,@11                                             1206 03642000
         BCTR  @14,0                                               1206 03643000
         EX    @14,@SM03942                                        1206 03644000
*           CTGPSWD=ADDR(MYPW);     /* CPL PTS TO PASSWORD           */ 03645000
         LA    @15,MYPW                                            1207 03646000
         L     @14,CTGPLPTR                                        1207 03647000
         ST    @15,CTGPSWD(,@14)                                   1207 03648000
*         END;                                                     1208 03649000
*     END;                                                         1209 03650000
*   ELSE                            /* CALLED BY COMMON OPEN         */ 03651000
*     CTGMAST=ON;                   /* MASTER PASSWORD REQUIRED      */ 03652000
         B     @RC01183                                            1210 03653000
@RF01183 L     @01,CTGPLPTR                                        1210 03654000
         OI    CTGMAST(@01),B'01000000'                            1210 03655000
*/*   CHECK FOR AN AUTHORIZED CALLER REQUESTING BYPASS OF AUTHORIZATION 03656000
*     CHECKING.                                              @Z40RSRC*/ 03657000
*                                                                  1211 03658000
*   IF ACBBYPSS=ON THEN             /* IF BYPASS REQUEST     @Z40RSRC*/ 03659000
