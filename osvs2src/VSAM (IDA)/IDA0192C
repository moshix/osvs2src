         ST    @14,FLTSTPTR+220                                    0784 01909000
*   FLTSTLEN(4,3)=2;                                               0785 01910000
         LA    @11,2                                               0785 01911000
         ST    @11,FLTSTLEN+220                                    0785 01912000
*   FLCOND(4,3)=CTGFLDLE;                                          0786 01913000
         MVI   FLCOND+220,X'C0'                                    0786 01914000
*   FLNAME(5,1)='TOVOLDEV  ';       /* ASK FOR TOTAL VOLDEV. @Z30SSMD*/ 01915000
         MVC   FLNAME+240(8),@CC03836                              0787 01916000
*   FLNAME(5,2)='ITYPEXT ';                                        0788 01917000
         MVC   FLNAME+260(8),@CC03801                              0788 01918000
*   FLTSTPTR(5,2)=ADDR(SSTEST);                                    0789 01919000
         ST    @02,FLTSTPTR+260                                    0789 01920000
*   FLTSTLEN(5,2)=1;                                               0790 01921000
         ST    @15,FLTSTLEN+260                                    0790 01922000
*   FLCOND(5,2)=CTGFLDZ;                                           0791 01923000
         MVI   FLCOND+260,X'80'                                    0791 01924000
*   FLNAME(5,3)='CNTREPNO';                                        0792 01925000
         MVC   FLNAME+280(8),@CC03838                              0792 01926000
*   FLTSTPTR(5,3)=ADDR(REPTEST);                                   0793 01927000
         ST    @14,FLTSTPTR+280                                    0793 01928000
*   FLTSTLEN(5,3)=2;                                               0794 01929000
         ST    @11,FLTSTLEN+280                                    0794 01930000
