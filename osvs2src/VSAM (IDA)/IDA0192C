*         R8->AMDNEXT=R6->AMDNEXT;  /* UPD NBR OF EXTENTS    @XA05646*/ 02129000
*                                                                  0843 02130000
         STH   @11,AMDNEXT(,R8)                                    0843 02131000
*       /*************************************************************/ 02132000
*       /*                                                           */ 02133000
*       /* ADD DEL FOR THIS PROCESSING TO EXISTING CAT VALUES        */ 02134000
*       /*                                                           */ 02135000
*       /*************************************************************/ 02136000
*                                                                  0844 02137000
*       R8->AMDNLR=R6->AMDNLR-R7->AMDNLR+R8->AMDNLR;/* UPD NBR LOG 0844 02138000
*                                      RECS                          */ 02139000
@RF00842 L     @11,AMDNLR(,R6)                                     0844 02140000
         SL    @11,AMDNLR(,R7)                                     0844 02141000
         AL    @11,AMDNLR(,R8)                                     0844 02142000
         ST    @11,AMDNLR(,R8)                                     0844 02143000
*       R8->AMDDELR=R6->AMDDELR-R7->AMDDELR+R8->AMDDELR;/* NBR     0845 02144000
*                                      DELETED RECS                  */ 02145000
