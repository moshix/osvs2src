         SL    @11,AMDDELR(,R7)                                    0845 02147000
         AL    @11,AMDDELR(,R8)                                    0845 02148000
         ST    @11,AMDDELR(,R8)                                    0845 02149000
*       R8->AMDIREC=R6->AMDIREC-R7->AMDIREC+R8->AMDIREC;/* NBR     0846 02150000
*                                      INSERTED RECS                 */ 02151000
         L     @11,AMDIREC(,R6)                                    0846 02152000
         SL    @11,AMDIREC(,R7)                                    0846 02153000
         AL    @11,AMDIREC(,R8)                                    0846 02154000
         ST    @11,AMDIREC(,R8)                                    0846 02155000
*       R8->AMDUPR=R6->AMDUPR-R7->AMDUPR+R8->AMDUPR;/* NBR UPDATED 0847 02156000
*                                      RECS                          */ 02157000
         L     @11,AMDUPR(,R6)                                     0847 02158000
         SL    @11,AMDUPR(,R7)                                     0847 02159000
         AL    @11,AMDUPR(,R8)                                     0847 02160000
         ST    @11,AMDUPR(,R8)                                     0847 02161000
*       R8->AMDRETR=R6->AMDRETR-R7->AMDRETR+R8->AMDRETR;/* NBR     0848 02162000
*                                      RETRIEVED REC                 */ 02163000
         L     @11,AMDRETR(,R6)                                    0848 02164000
         SL    @11,AMDRETR(,R7)                                    0848 02165000
         AL    @11,AMDRETR(,R8)                                    0848 02166000
         ST    @11,AMDRETR(,R8)                                    0848 02167000
*       R8->AMDNCIS=R6->AMDNCIS-R7->AMDNCIS+R8->AMDNCIS;/* NBR CI  0849 02168000
*                                      SPLITS                        */ 02169000
         L     @11,AMDNCIS(,R6)                                    0849 02170000
         SL    @11,AMDNCIS(,R7)                                    0849 02171000
         AL    @11,AMDNCIS(,R8)                                    0849 02172000
         ST    @11,AMDNCIS(,R8)                                    0849 02173000
*       R8->AMDNCAS=R6->AMDNCAS-R7->AMDNCAS+R8->AMDNCAS;/* NBR CA  0850 02174000
*                                      SPLITS                        */ 02175000
         L     @11,AMDNCAS(,R6)                                    0850 02176000
         SL    @11,AMDNCAS(,R7)                                    0850 02177000
         AL    @11,AMDNCAS(,R8)                                    0850 02178000
         ST    @11,AMDNCAS(,R8)                                    0850 02179000
*       R8->AMDEXCP=R6->AMDEXCP-R7->AMDEXCP+R8->AMDEXCP;/* UPDATE NBR   02180000
*                                      EXCP'S                        */ 02181000
         L     @11,AMDEXCP(,R6)                                    0851 02182000
         SL    @11,AMDEXCP(,R7)                                    0851 02183000
         AL    @11,AMDEXCP(,R8)                                    0851 02184000
         ST    @11,AMDEXCP(,R8)                                    0851 02185000
*       R7->AMDSTAT=R8->AMDSTAT;    /* UPDATE OPEN STATISTICS.     0852 02186000
*                                                            @Z30SSMD*/ 02187000
         MVC   AMDSTAT(48,R7),AMDSTAT(R8)                          0852 02188000
*       CALL CLUPDESR;              /* CLEAR UPDATE DESCRIPTOR RECORD   02189000
*                                                            @XA02420*/ 02190000
*                                                                  0853 02191000
         BAL   @14,CLUPDESR                                        0853 02192000
*       /*************************************************************/ 02193000
*       /*                                                           */ 02194000
*       /* INIT LOCATE/UPDATE DESCRIPTOR TO UPDATE STATISTICS        */ 02195000
*       /*                                                           */ 02196000
*       /*************************************************************/ 02197000
*                                                                  0854 02198000
*       UPD=ON;                     /* INDICATE UPDATE REQUEST       */ 02199000
         OI    UPD,B'00001000'                                     0854 02200000
*       FLACTIVE(CTR,1)=ON;         /* FIELD ACTIVE                  */ 02201000
         LR    @11,CTR                                             0855 02202000
         MH    @11,@CH00144                                        0855 02203000
         LA    @01,FLACTIVE-60(@11)                                0855 02204000
         OI    0(@01),B'10000000'                                  0855 02205000
*       FLNAME(CTR,1)='AMDSBCAT';   /* FIELD FOR AMDSB               */ 02206000
         LA    @01,FLNAME-60(@11)                                  0856 02207000
         MVC   0(8,@01),@CC03774                                   0856 02208000
*       FLTSTPTR(CTR,1)=CTGFLPT(1); /* PT TO UPDATED AMDSB           */ 02209000
         L     @15,CTGFLPT(,CTGFLPTR)                              0857 02210000
         ST    @15,FLTSTPTR-60(@11)                                0857 02211000
*       FLTSTLEN(CTR,1)=CTGFLNG(1); /* LENGTH OF AMDSB               */ 02212000
         L     @15,CTGFLNG(,CTGFLPTR)                              0858 02213000
         ST    @15,FLTSTLEN-60(@11)                                0858 02214000
*     END;                          /* END OF LOCATE SUCCESSFUL DO 0859 02215000
*                                      GROUP.                @Z30SSMD*/ 02216000
*   ELSE                            /* IF LOCATE NOT SUCCESSFUL THEN,   02217000
*                                                            @Z30SSMD*/ 02218000
*     CTR=CTR-1;                    /* DECRMENT THE COUNTER. @Z30SSMD*/ 02219000
         B     @RC00832                                            0860 02220000
@RF00832 BCTR  CTR,0                                               0860 02221000
*   RET=RSAVLC;                                                    0861 02222000
@RC00832 L     RET,RSAVLC                                          0861 02223000
*   END UPSTAT;                                                    0862 02224000
@EL00017 DS    0H                                                  0862 02225000
@EF00017 DS    0H                                                  0862 02226000
@ER00017 BR    @14                                                 0862 02227000
*UPDSHI:                                                           0863 02228000
*                                                                  0863 02229000
*/********************************************************************/ 02230000
*/*                                                                  */ 02231000
*/*     01 FUNCTION - SET UP THE FIELD DESCRIPTOR RECORD TO UPDATE   */ 02232000
*/*                   THE DATA SET HIGH USED RBA.  THE HIGH USED     */ 02233000
*/*                   RBA IS DETERMINED BY SCANNING FOR THE EOD ARDB.*/ 02234000
*/*                                                                  */ 02235000
*/*     01 INPUT    - FIELD DESCRIPTOR RECORD, R6 PTS TO 1ST ARDB.   */ 02236000
*/*                                                                  */ 02237000
*/*     01 OUTPUT   - FIELD DESCRIPTOR INITIALIZED TO UPDATE DATA SET*/ 02238000
*/*                   HIGH USED RBA                                  */ 02239000
*/*                                                                  */ 02240000
*/********************************************************************/ 02241000
*                                                                  0863 02242000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0863 02243000
UPDSHI   DS    0H                                                  0864 02244000
*   RSAVLC=RET;                     /* SAVE RETURN REG               */ 02245000
*                                                                  0864 02246000
         ST    RET,RSAVLC                                          0864 02247000
*   /*****************************************************************/ 02248000
*   /*                                                               */ 02249000
*   /* LOCATE HIGH USED RBA OF DATA SET BY SCANNING ARDB'S           */ 02250000
*   /*                                                               */ 02251000
*   /*****************************************************************/ 02252000
*                                                                  0865 02253000
*   RESPECIFY                                                      0865 02254000
*     IDAARDB BASED(R6);            /* ADDRESSABILITY-ARDB           */ 02255000
*   DO WHILE ARDEOD=OFF&ARDNPTR^=0; /* LOCATE ARDB WITH HI RBA       */ 02256000
         B     @DE00866                                            0866 02257000
@DL00866 DS    0H                                                  0867 02258000
*     R6=ARDNPTR;                   /* NEXT ARDB                     */ 02259000
         L     R6,ARDNPTR(,R6)                                     0867 02260000
*   END;                                                           0868 02261000
*                                                                  0868 02262000
@DE00866 TM    ARDEOD(R6),B'00001000'                              0868 02263000
         BNZ   @DC00866                                            0868 02264000
         L     @11,ARDNPTR(,R6)                                    0868 02265000
         LTR   @11,@11                                             0868 02266000
         BNZ   @DL00866                                            0868 02267000
@DC00866 DS    0H                                                  0869 02268000
*   /*****************************************************************/ 02269000
*   /*                                                               */ 02270000
*   /* INIT LOCATE/UPDATE DESCRIPTOR TO UPDATE HI RBA OF DS          */ 02271000
*   /*                                                               */ 02272000
*   /*****************************************************************/ 02273000
*                                                                  0869 02274000
*   FLACTIVE(CTR,1)=ON;             /* THIS FIELD ACTIVE             */ 02275000
         LR    @11,CTR                                             0869 02276000
         MH    @11,@CH00144                                        0869 02277000
         LA    @02,FLACTIVE-60(@11)                                0869 02278000
         OI    0(@02),B'10000000'                                  0869 02279000
*   FLNAME(CTR,1)='HURBADS ';       /* FILED FOR DATA SET HI RBA     */ 02280000
         LA    @02,FLNAME-60(@11)                                  0870 02281000
         MVC   0(8,@02),@CC03851                                   0870 02282000
*   FLTSTPTR(CTR,1)=ADDR(ARDHRBA);  /* NEW HI RBA                    */ 02283000
         LA    @02,ARDHRBA(,R6)                                    0871 02284000
         ST    @02,FLTSTPTR-60(@11)                                0871 02285000
*   FLTSTLEN(CTR,1)=LENGTH(ARDHRBA);/* LENGTH OF RBA DATA            */ 02286000
*                                                                  0872 02287000
         LA    @15,4                                               0872 02288000
         ST    @15,FLTSTLEN-60(@11)                                0872 02289000
*   /*****************************************************************/ 02290000
*   /*                                                               */ 02291000
*   /* IF RESTART IS THE CALLER, UNCONDITIONALLY REPLACE HIGH USED   */ 02292000
