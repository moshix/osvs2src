         BNO   @RF00304                                            0304 00323000
@RT00304 DS    0H                                                  0305 00324000
*         OPWBIXCI=0;               /* INIT CI TO ZERO       @X04SVGP*/ 00325000
         SLR   @02,@02                                             0305 00326000
         STCM  @02,7,OPWBIXCI(RCORE)                               0305 00327000
*     END;                          /*                       @X04SVGP*/ 00328000
*   ELSE                            /* NOT A SPECIAL DATA SET      0307 00329000
*                                                            @X04SVRL*/ 00330000
*     DO;                           /* PROCESS NON CAT       @X04SVGP*/ 00331000
         B     @RC00297                                            0307 00332000
@RF00297 DS    0H                                                  0308 00333000
*       CALL CLUPDESR;              /* CLEAR UPDATE DESCRIPTOR RECORD*/ 00334000
         BAL   @14,CLUPDESR                                        0308 00335000
*       PPFUNC=PDF2;                /* NOTE FUNCTION         @X04SVGP*/ 00336000
         MVI   PPFUNC,X'02'                                        0309 00337000
*       NAME44=ON;                  /* LOCATE BY NAME        @X04SVGP*/ 00338000
         OI    NAME44,B'10000000'                                  0310 00339000
*       IF OPWVVIC=OFF THEN         /* VVIC DATA SET         @X04SVGP*/ 00340000
         TM    OPWVVIC(RCORE),B'00100000'                          0311 00341000
         BNZ   @RF00311                                            0311 00342000
*         PSWRD=ON;                 /* NO, DO AUTH CHECK     @X04SVGP*/ 00343000
         OI    PSWRD,B'00000010'                                   0312 00344000
*       ELSE                        /* YES,                  @X04SVGP*/ 00345000
*         DO;                       /*                       @X04SVGP*/ 00346000
         B     @RC00311                                            0313 00347000
