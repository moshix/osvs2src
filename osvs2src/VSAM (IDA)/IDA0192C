*   END CLMAIN;                                                    0558 01144000
@EL00004 DS    0H                                                  0558 01145000
@EF00004 DS    0H                                                  0558 01146000
@ER00004 BR    @14                                                 0558 01147000
*VLTSTMP:                                                          0559 01148000
*                                                                  0559 01149000
*/********************************************************************/ 01150000
*/*                                                                  */ 01151000
*/*     01 FUNCTION - LOCATE VOLUME TIME STAMPS                      */ 01152000
*/*                                                                  */ 01153000
*/*     01 INPUT    - CALLER'S PARAMETER LIST CONTAINING THE VOLUME  */ 01154000
*/*                   SERIAL NBRS FOR WHICH TIME STAMPS ARE DESIRED. */ 01155000
*/*                   ADDRESS OF CATALOG ACB.                        */ 01156000
*/*                                                                  */ 01157000
*/*     02 OUTPUT   - CALLER'S PARAMETER LIST IS UPDATED TO REFLECT  */ 01158000
*/*                   THE TIME STAMPS ASSOCIATED WITH EACH SPECIFIED */ 01159000
*/*                   VOLUME                                         */ 01160000
*/*                                                                  */ 01161000
*/********************************************************************/ 01162000
*                                                                  0559 01163000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0559 01164000
VLTSTMP  DS    0H                                                  0560 01165000
*   RSAVLB=RET;                                                    0560 01166000
         ST    RET,RSAVLB                                          0560 01167000
*   PPFUNC=PDF7;                    /* FUNCTION ID           @X04SVGP*/ 01168000
*                                                                  0561 01169000
         MVI   PPFUNC,X'07'                                        0561 01170000
*   /*****************************************************************/ 01171000
*   /*                                                               */ 01172000
*   /* INIT LOCATE/UPDATE DESCRIPTOR                                 */ 01173000
*   /*                                                               */ 01174000
*   /*****************************************************************/ 01175000
*                                                                  0562 01176000
*   NAME6=ON;                       /* INDICATE VOL SER ENTRY        */ 01177000
*   VMT=ON;                         /* VOL TIME STAMP RETRIEVAL      */ 01178000
         OI    NAME6,B'01000100'                                   0563 01179000
*   DSCATACB=VLCATACB;              /* COPY CATALOG ACB PTR          */ 01180000
         L     @02,PARMPT                                          0564 01181000
         L     @02,VLCATACB(,@02)                                  0564 01182000
         ST    @02,DSCATACB                                        0564 01183000
*   FLACTIVE(1,1)=ON;               /* ACTIVE FIELD                  */ 01184000
         OI    FLACTIVE,B'10000000'                                0565 01185000
*   FLNAME(1,1)='VOLTSTMP';         /* FIELD FOR VOLUME TIME STAMP   */ 01186000
         MVC   FLNAME(8),@CC03795                                  0566 01187000
*   DO CTRB=1 TO VLENTNO;           /* PROCESS ALL VOLSER'S PASSED   */ 01188000
         LA    @02,1                                               0567 01189000
         B     @DE00567                                            0567 01190000
@DL00567 DS    0H                                                  0568 01191000
*     DSNVLSER=VLSER(CTRB);                                        0568 01192000
         MH    @02,@CH03906                                        0568 01193000
         L     @01,PARMPT                                          0568 01194000
         ALR   @01,@02                                             0568 01195000
         BCTR  @01,0                                               0568 01196000
         BCTR  @01,0                                               0568 01197000
         MVC   DSNVLSER(6),VLSER-12(@01)                           0568 01198000
*     CALL CTINTRFC;                /* LOCATE TS THIS VOL            */ 01199000
         BAL   @14,CTINTRFC                                        0569 01200000
*     CTGFLPTR=CTGFIELD(1);         /* ADDRESSABILITY 1ST FIELD      */ 01201000
         L     @01,CTGPLPTR                                        0570 01202000
         L     CTGFLPTR,CTGFIELD(,@01)                             0570 01203000
*     IF CTGFLPT(1)^=0 THEN         /* TIME STAMP RETURNED?          */ 01204000
         L     @15,CTGFLPT(,CTGFLPTR)                              0571 01205000
         LTR   @15,@15                                             0571 01206000
         BZ    @RF00571                                            0571 01207000
*       VLTS(CTRB)=VLTSFA;          /* YES, MOVE TO CALLER AREA      */ 01208000
         L     @14,CTRB                                            0572 01209000
         MH    @14,@CH03906                                        0572 01210000
         L     @01,PARMPT                                          0572 01211000
         ALR   @01,@14                                             0572 01212000
         MVC   VLTS-14(8,@01),VLTSFA(@15)                          0572 01213000
*   END;                                                           0573 01214000
@RF00571 LA    @02,1                                               0573 01215000
         AL    @02,CTRB                                            0573 01216000
@DE00567 ST    @02,CTRB                                            0573 01217000
         L     @01,PARMPT                                          0573 01218000
         CH    @02,VLENTNO(,@01)                                   0573 01219000
         BNH   @DL00567                                            0573 01220000
*   RET=RSAVLB;                                                    0574 01221000
         L     RET,RSAVLB                                          0574 01222000
*   END VLTSTMP;                                                   0575 01223000
@EL00005 DS    0H                                                  0575 01224000
@EF00005 DS    0H                                                  0575 01225000
@ER00005 BR    @14                                                 0575 01226000
*PREFORM:                                                          0576 01227000
*                                                                  0576 01228000
*/********************************************************************/ 01229000
*/*                                                                  */ 01230000
*/*    01 FUNCTION - UPDATE THE PREFORMAT INDICATOR IN THE CATALOG.  */ 01231000
*/*                  THE ARDB'S ARE SCANNED, AND, IF ONE IS FOUND    */ 01232000
*/*                  WITH THE PREFORMAT INDICATOR ON THE BIT IS RESET*/ 01233000
*/*                  IN THE ARDB AS WELL AS THE CORRESPONDING KEY    */ 01234000
*/*                  RANGE ENTRY IN THE CATALOG.                     */ 01235000
*/*                                                                  */ 01236000
*/*    01 INPUT    - AMB, AMDSB, ARDB'S                              */ 01237000
*/*                                                                  */ 01238000
*/*    02 OUTPUT   - PREFORMAT  INDICATOR RESET IN ARDB'S AND ALSO   */ 01239000
*/*                  IN CORRESPONDING CATALOG KEY RANGE ENTRY        */ 01240000
*/*                                                                  */ 01241000
*/********************************************************************/ 01242000
*                                                                  0576 01243000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0576 01244000
PREFORM  DS    0H                                                  0577 01245000
*   RSAVLB=RET;                                                    0577 01246000
         ST    RET,RSAVLB                                          0577 01247000
*   PPFUNC=PDF9;                    /* INIT FUNCTION ID      @X04SVGP*/ 01248000
         MVI   PPFUNC,X'09'                                        0578 01249000
*   UPD=ON;                         /* INDICATE CATALOG UPDATE       */ 01250000
*   ID3=ON;                         /* USING CI INTERFACE TO CAT     */ 01251000
         OI    UPD,B'00101000'                                     0580 01252000
*   DSCATACB=AMBCACB;               /* ADDRESS OF CATALOG ACB        */ 01253000
         L     @02,AMBCACB(,R5)                                    0581 01254000
         ST    @02,DSCATACB                                        0581 01255000
*   DSNCI=AMBDDSN;                  /* CI OF DATA SET                */ 01256000
         MVC   DSNCI(3),AMBDDSN(R5)                                0582 01257000
*   RESPECIFY                                                      0583 01258000
*     IDAARDB BASED(R6);                                           0583 01259000
*   RESPECIFY                                                      0584 01260000
*     IDAAMDSB BASED(AMBDSB);                                      0584 01261000
*   R6=AMDPARDB;                    /* PT TO 1ST ARDB                */ 01262000
         L     @02,AMBDSB(,R5)                                     0585 01263000
         L     R6,AMDPARDB(,@02)                                   0585 01264000
*   DO WHILE R6^=0;                 /* PROCESS ALL ARDB'S            */ 01265000
         B     @DE00586                                            0586 01266000
@DL00586 DS    0H                                                  0587 01267000
*     DO CTR=1 TO 6 WHILE R6^=0;    /* PROCESS 6 ARDB'S AT A TIME    */ 01268000
         LA    CTR,1                                               0587 01269000
@DL00587 LTR   R6,R6                                               0587 01270000
         BZ    @DC00587                                            0587 01271000
*       IF CTR=1 THEN               /* FIRST ARDB                    */ 01272000
         C     CTR,@CF00053                                        0588 01273000
         BNE   @RF00588                                            0588 01274000
*         CALL CLUPDESR;            /* CLEAR UPDATE DESCRIPTOR RECORD*/ 01275000
         BAL   @14,CLUPDESR                                        0589 01276000
*       DO WHILE(R6^=0&ARDPRFMT=OFF);/* SCAN ARDB'S FOR PREFORMAT ON */ 01277000
@RF00588 B     @DE00590                                            0590 01278000
@DL00590 DS    0H                                                  0591 01279000
*         R6=ARDNPTR;               /* PT NEXT ARDB 2                */ 01280000
         L     R6,ARDNPTR(,R6)                                     0591 01281000
*       END;                                                       0592 01282000
@DE00590 LTR   R6,R6                                               0592 01283000
         BZ    @DC00590                                            0592 01284000
         TM    ARDPRFMT(R6),B'01000000'                            0592 01285000
         BZ    @DL00590                                            0592 01286000
@DC00590 DS    0H                                                  0593 01287000
*       IF R6^=0 THEN               /* ARDB CHAIN EXHAUSTED          */ 01288000
         LTR   R6,R6                                               0593 01289000
         BZ    @RF00593                                            0593 01290000
*         DO;                       /* NO, THEN ARDPRFMT MUST BE ON  */ 01291000
*           ARDPRFMT=OFF;           /* TURN OFF PRFMT INDICATOR      */ 01292000
         NI    ARDPRFMT(R6),B'10111111'                            0595 01293000
*           FLACTIVE(CTR,1)=ON;     /* FIELD IS ACTIVE               */ 01294000
         LR    @11,CTR                                             0596 01295000
         MH    @11,@CH00144                                        0596 01296000
         LA    @02,FLACTIVE-60(@11)                                0596 01297000
         OI    0(@02),B'10000000'                                  0596 01298000
*           FLNAME(CTR,1)='ITYPEXT ';/* BYTE TO BE UPDATED IN CAT    */ 01299000
         LA    @02,FLNAME-60(@11)                                  0597 01300000
         MVC   0(8,@02),@CC03801                                   0597 01301000
*           FLTSTPTR(CTR,1)=ADDR(ARDPRF);/* REPLACEMENT BYTE IN CORE */ 01302000
         LA    @02,ARDPRF(,R6)                                     0598 01303000
         ST    @02,FLTSTPTR-60(@11)                                0598 01304000
*           FLTSTLEN(CTR,1)=LENGTH(ARDPRF);/* LENGTH OF UPDATE DATA  */ 01305000
         LA    @02,8                                               0599 01306000
         ST    @02,FLTSTLEN-60(@11)                                0599 01307000
*           FLACTIVE(CTR,2)=ON;     /* TEST FIELD ACTIVE             */ 01308000
         LA    @02,FLACTIVE-40(@11)                                0600 01309000
         OI    0(@02),B'10000000'                                  0600 01310000
*           FLNAME(CTR,2)='RELREPNO';/* RELATIVE REP NBR IS TESTED   */ 01311000
         LA    @02,FLNAME-40(@11)                                  0601 01312000
         MVC   0(8,@02),@CC03802                                   0601 01313000
*           FLTSTPTR(CTR,2)=ADDR(ARDRELNO);/* AGAINST VOL OCCURRENCE    01314000
*                                      NBR                           */ 01315000
         LA    @02,ARDRELNO(,R6)                                   0602 01316000
         ST    @02,FLTSTPTR-40(@11)                                0602 01317000
*           FLTSTLEN(CTR,2)=LENGTH(ARDRELNO);/* INIT LENGTH OF TEST     01318000
*                                      DATA                          */ 01319000
         LA    @02,2                                               0603 01320000
         ST    @02,FLTSTLEN-40(@11)                                0603 01321000
*           FLCOND(CTR,2)=CTGFLDEQ; /* UPDATE THIS OCCURRENCE ONLY   */ 01322000
         LA    @02,FLCOND-40(@11)                                  0604 01323000
         MVI   0(@02),X'80'                                        0604 01324000
*           R6=ARDNPTR;             /* PT TO NEXT ARDB               */ 01325000
         L     R6,ARDNPTR(,R6)                                     0605 01326000
*         END;                                                     0606 01327000
*     END;                                                         0607 01328000
@RF00593 AL    CTR,@CF00053                                        0607 01329000
         C     CTR,@CF00040                                        0607 01330000
         BNH   @DL00587                                            0607 01331000
@DC00587 DS    0H                                                  0608 01332000
*     IF FLACTIVE(1,1)=ON THEN      /* ANY FIELDS ACTIVE             */ 01333000
         TM    FLACTIVE,B'10000000'                                0608 01334000
         BNO   @RF00608                                            0608 01335000
*       DO;                         /* YES,                          */ 01336000
*         CALL CTINTRFC;            /* UPDATE CATALOG                */ 01337000
         BAL   @14,CTINTRFC                                        0610 01338000
*         IF PPCODE^=0 THEN         /* ERRORS                        */ 01339000
         CLI   PPCODE,0                                            0611 01340000
         BE    @RF00611                                            0611 01341000
*           R6=0;                   /* YES, END LOOP                 */ 01342000
         SLR   R6,R6                                               0612 01343000
*       END;                                                       0613 01344000
@RF00611 DS    0H                                                  0614 01345000
*   END;                                                           0614 01346000
@RF00608 DS    0H                                                  0614 01347000
@DE00586 LTR   R6,R6                                               0614 01348000
         BNZ   @DL00586                                            0614 01349000
*   RET=RSAVLB;                                                    0615 01350000
         L     RET,RSAVLB                                          0615 01351000
*   END PREFORM;                                                   0616 01352000
@EL00006 DS    0H                                                  0616 01353000
@EF00006 DS    0H                                                  0616 01354000
@ER00006 BR    @14                                                 0616 01355000
*RCMUPD:                                                           0617 01356000
*                                                                  0617 01357000
*/********************************************************************/ 01358000
*/*                                                                  */ 01359000
*/*     01 FUNCTION - UPDATE THE CATALOG AMDSB STATISTICS            */ 01360000
*/*                                                                  */ 01361000
*/*     01 INPUT    - AMB, AMDSB                                     */ 01362000
*/*                                                                  */ 01363000
*/*     01 OUTPUT   - CATALOG AMDSB STATISTICS ARE UPDATED. THE      */ 01364000
*/*                   STATISTICS SAVED BY OPEN ARE UPDATED TO THE    */ 01365000
*/*                   SAME CATALOG VALUES                            */ 01366000
*/*                                                                  */ 01367000
*/********************************************************************/ 01368000
*                                                                  0617 01369000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0617 01370000
RCMUPD   DS    0H                                                  0618 01371000
*   RSAVLB=RET;                     /* SAVE RETURN ADDRESS           */ 01372000
         ST    RET,RSAVLB                                          0618 01373000
*   PPFUNC=PDF8;                    /* SET FUNCTION CODE     @X04SVGP*/ 01374000
