         TITLE 'IDA0192C - VSAM CATALOG INTERFACE                      *00001000
                        '                                               00002000
IDA0192C CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192C  78.151'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @12,4095(,@03)                                      0001 00012000
         USING @PSTART,@03                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
JSCBDISP EQU 188                                                        00015000
JSCBAOS1 EQU  72                                                        00016000
JSCBAOS2 EQU 192                                                        00017000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00018000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00019000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00020000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00021000
*                                       IF NO ERROR WAS ENCOUNTERED     00022000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00023000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00024000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00025000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00026000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00027000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00028000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00029000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00030000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00031000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00032000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00033000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00034000
         B     GMN00200                 BYPASS SETTING ERROR            00035000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00036000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00037000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00038000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00039000
*   IF R15=0 THEN                   /* GETMAIN FAIL                  */ 00040000
         LTR   R15,R15                                             0220 00041000
         BNZ   @RF00220                                            0220 00042000
*     DO;                                                          0221 00043000
*       GEN(L @15,GMFAIL);          /* INIT RETURN CODE      @X04SVGP*/ 00044000
         L @15,GMFAIL                                                   00045000
*       GEN(LM R0,R12,20(R13));     /* RESTORE REGS                  */ 00046000
         LM R0,R12,20(R13)                                              00047000
*       GEN(L  RET,12(R13));                                       0224 00048000
         L  RET,12(R13)                                                 00049000
*       GEN(BR RET);                /* RETURN TO CALLER              */ 00050000
         BR RET                                                         00051000
*     END;                                                         0226 00052000
*   RESPECIFY                                                      0227 00053000
*     R13 RESTRICTED;                                              0227 00054000
@RF00220 DS    0H                                                  0228 00055000
*   GEN(ST R13,4(R15));             /* SAVE SAVE REG                 */ 00056000
         ST R13,4(R15)                                                  00057000
*   GEN( L R0,20(,R13));            /* IDAGMAIN USES R0, RESTORE IT.    00058000
*                                                            @Z30SSMD*/ 00059000
          L R0,20(,R13)                                                 00060000
*   GEN( L R1,24(,R13));            /* IDAGMAIN USES R1, RESTORE IT.    00061000
*                                                            @Z30SSMD*/ 00062000
          L R1,24(,R13)                                                 00063000
*   R13=R15;                        /* NO,                           */ 00064000
         LR    R13,R15                                             0231 00065000
*   GEN(USING @DATD,R13);           /* ADDRESSABILITY TO WA          */ 00066000
         USING @DATD,R13                                                00067000
*   PARMPT=R1;                      /* INIT PARAMETER PTR            */ 00068000
         ST    R1,PARMPT                                           0233 00069000
*   ACBPT=RACB;                     /* INIT ACB PTR                  */ 00070000
         ST    RACB,ACBPT                                          0234 00071000
*   RESPECIFY                                                      0235 00072000
*     RACB UNRESTRICTED;                                           0235 00073000
*                                                                  0235 00074000
*   /*****************************************************************/ 00075000
*   /*                                                               */ 00076000
*   /* BUILD CATALOG WORK AREA                                       */ 00077000
*   /*                                                               */ 00078000
*   /*****************************************************************/ 00079000
*                                                                  0236 00080000
*   RESPECIFY                                                      0236 00081000
*     IDAAMB BASED(R5);             /*                       @X04SVGP*/ 00082000
*   PPFUNC=PDF1;                    /* NOTE FUNCTION         @X04SVGP*/ 00083000
         MVI   PPFUNC,X'01'                                        0237 00084000
*   SUBPOOL=WKPOOL;                 /* INIT SUBPOOL FOR GETMAIN      */ 00085000
         MVI   SUBPOOL,X'FC'                                       0238 00086000
*   CORELEN=SWKLEN*2;               /* INIT LENGTH FOR GETMAIN       */ 00087000
         LA    CORELEN,2048                                        0239 00088000
*   CALL GETCORE;                   /* GET CORE FOR CAT WORK AREA    */ 00089000
         BAL   @14,GETCORE                                         0240 00090000
*   IF PPCODE=0 THEN                /* GETMAIN SUCCESSFUL    @Z30SSRL*/ 00091000
         CLI   PPCODE,0                                            0241 00092000
         BNE   @RF00241                                            0241 00093000
*     DO;                                                          0242 00094000
*       CTGPLPTR=COREADDR;          /* PTR TO CAT PARAMETER LIST     */ 00095000
         L     @15,COREADDR                                        0243 00096000
         ST    @15,CTGPLPTR                                        0243 00097000
*       CTGFLPTR=CTGPLPTR+LENGTH(CTGPL)+LENGTH(CTGFIELD)*6;/* PTR 1ST   00098000
*                                      FIELD                         */ 00099000
         LA    CTGFLPTR,52                                         0244 00100000
         ALR   CTGFLPTR,@15                                        0244 00101000
*       CATWKPTR=CTGFLPTR+(LENGTH(CTGFL)+LENGTH(CTGFLDAT))*FLMAX;/*     00102000
*                                      PTR TO CAT WORK FOR UPDATE AND   00103000
*                                      RETURN OF CAT INFO            */ 00104000
         LA    @14,432                                             0245 00105000
         ALR   @14,CTGFLPTR                                        0245 00106000
         ST    @14,CATWKPTR                                        0245 00107000
*       CATWKPTR=((CATWKPTR+7)/8)*8;                               0246 00108000
         AL    @14,@CF00070                                        0246 00109000
         SRDA  @14,32                                              0246 00110000
         D     @14,@CF00049                                        0246 00111000
         SLA   @15,3                                               0246 00112000
         ST    @15,CATWKPTR                                        0246 00113000
*       WORKLEN=(COREADDR+SWKLEN*2)-CATWKPTR;/* INIT WORK AREA LENGTH*/ 00114000
         L     @02,COREADDR                                        0247 00115000
         AL    @02,@CF03903                                        0247 00116000
         SLR   @02,@15                                             0247 00117000
         STH   @02,WORKLEN(,@15)                                   0247 00118000
*       CATBLKLN=CATWKPTR-CTGPLPTR; /* INIT LENGTH OF CAT BLKS AND 0248 00119000
*                                      FIELD LISTS                   */ 00120000
         L     @02,CTGPLPTR                                        0248 00121000
         SLR   @15,@02                                             0248 00122000
         STH   @15,CATBLKLN                                        0248 00123000
*       IF ID=OP1 THEN              /* IS THIS 1ST OPEN CALL @X04SVGP*/ 00124000
         L     @02,PARMPT                                          0249 00125000
         CLI   ID(@02),4                                           0249 00126000
         BNE   @RF00249                                            0249 00127000
*         CALL LOC1;                /* YES, PERFORM REQ LOCATES      */ 00128000
         BAL   @14,LOC1                                            0250 00129000
*       ELSE                        /* NOT 1ST OPEN CALL     @X04SVRL*/ 00130000
*         IF ID=OP2 THEN            /* LOCATE DS ATTRIBUTES  @X04SVGP*/ 00131000
         B     @RC00249                                            0251 00132000
@RF00249 L     @02,PARMPT                                          0251 00133000
         CLI   ID(@02),44                                          0251 00134000
         BNE   @RF00251                                            0251 00135000
*           CALL LOC2;              /* YES, DO LOCATE        @X04SVGP*/ 00136000
         BAL   @14,LOC2                                            0252 00137000
*         ELSE                      /* NO,                           */ 00138000
*           IF ID=CLM THEN          /* IS THIS 1ST CLOSE CALL        */ 00139000
         B     @RC00251                                            0253 00140000
@RF00251 L     @02,PARMPT                                          0253 00141000
         CLI   ID(@02),8                                           0253 00142000
         BNE   @RF00253                                            0253 00143000
*             CALL CLMAIN;          /* YES, PERFORM REQ UPDATES      */ 00144000
         BAL   @14,CLMAIN                                          0254 00145000
*           ELSE                    /* NOT 1ST CLOSE CALL    @Z30SSRL*/ 00146000
*             IF ID=OPO THEN        /* UPDATE OPEN INDICATOR         */ 00147000
         B     @RC00253                                            0255 00148000
@RF00253 L     @02,PARMPT                                          0255 00149000
         CLI   ID(@02),12                                          0255 00150000
         BNE   @RF00255                                            0255 00151000
*               CALL OPOBIT;        /* YES, UPD                      */ 00152000
         BAL   @14,OPOBIT                                          0256 00153000
*             ELSE                  /* NOT UPDATE OPEN IND   @Z30SSRL*/ 00154000
*               IF ID=PRE THEN      /* UPDATE PREFORMAT INDICATOR    */ 00155000
         B     @RC00255                                            0257 00156000
@RF00255 L     @02,PARMPT                                          0257 00157000
         CLI   ID(@02),32                                          0257 00158000
         BNE   @RF00257                                            0257 00159000
*                 CALL PREFORM;     /* YES, UPDATE PREFMT IND        */ 00160000
         BAL   @14,PREFORM                                         0258 00161000
*               ELSE                /* NOT UPDATE PREFMT IND @Z30SSRL*/ 00162000
*                 IF ID=OPP THEN    /* PASSWORD VERIFICATION         */ 00163000
         B     @RC00257                                            0259 00164000
@RF00257 L     @02,PARMPT                                          0259 00165000
         CLI   ID(@02),16                                          0259 00166000
         BNE   @RF00259                                            0259 00167000
*                   CALL OPPSWD;    /* YES, CALL PSWRD ROUTINE       */ 00168000
         BAL   @14,OPPSWD                                          0260 00169000
*                 ELSE              /* NOT PSWRD VERIFICATION      0261 00170000
*                                                            @Z30SSRL*/ 00171000
*                   IF ID=VLT THEN  /* TIME STAMP REQUEST            */ 00172000
         B     @RC00259                                            0261 00173000
@RF00259 L     @02,PARMPT                                          0261 00174000
         CLI   ID(@02),20                                          0261 00175000
         BNE   @RF00261                                            0261 00176000
*                     CALL VLTSTMP; /* YES, LOCATE T.S.              */ 00177000
         BAL   @14,VLTSTMP                                         0262 00178000
*                   ELSE            /* NOT TIMESTAMP REQUEST @Z30SSRL*/ 00179000
*                     IF ID=RCM THEN/* RCD MGMT CALL                 */ 00180000
         B     @RC00261                                            0263 00181000
@RF00261 L     @02,PARMPT                                          0263 00182000
         CLI   ID(@02),36                                          0263 00183000
         BNE   @RF00263                                            0263 00184000
*                       CALL RCMUPD;/* RCD MGMT UPDATE       @Z30SSRL*/ 00185000
         BAL   @14,RCMUPD                                          0264 00186000
*                     ELSE          /* NOT RCD MGMT          @Z30SSRL*/ 00187000
*                       IF ID=CLC THEN/* LOCATE CL DSN       @X04SVRL*/ 00188000
         B     @RC00263                                            0265 00189000
@RF00263 L     @02,PARMPT                                          0265 00190000
         CLI   ID(@02),48                                          0265 00191000
         BNE   @RF00265                                            0265 00192000
*                         CALL CLNLOC;/* YES, GET NAME       @X04SVGP*/ 00193000
         BAL   @14,CLNLOC                                          0266 00194000
*                       ELSE        /* NOT LOC CL DSN        @X04SVRL*/ 00195000
*                         IF ID=CLD THEN/* LOCATE EL DSN     @X04SVRL*/ 00196000
         B     @RC00265                                            0267 00197000
@RF00265 L     @02,PARMPT                                          0267 00198000
         CLI   ID(@02),40                                          0267 00199000
         BNE   @RF00267                                            0267 00200000
*                           CALL ELNLOC;/* YES,GET NAME      @X04SVGP*/ 00201000
         BAL   @14,ELNLOC                                          0268 00202000
*                         ELSE      /* NOT LOC EL DSN        @X04SVRL*/ 00203000
*                           CALL DUMLOC;/* DUMMY LOCATE FOR OPENING     00204000
*                                      CATALOG                       */ 00205000
*                                                                  0269 00206000
         B     @RC00267                                            0269 00207000
@RF00267 BAL   @14,DUMLOC                                          0269 00208000
*       /*************************************************************/ 00209000
*       /*                                                           */ 00210000
*       /* FREE CATALOG WORK AREAS                                   */ 00211000
*       /*                                                           */ 00212000
*       /*************************************************************/ 00213000
*                                                                  0270 00214000
*       FREEAD=CTGPLPTR;            /* PT TO CAT PARM LIST   @Z30SSRL*/ 00215000
@RC00267 DS    0H                                                  0270 00216000
@RC00265 DS    0H                                                  0270 00217000
@RC00263 DS    0H                                                  0270 00218000
@RC00261 DS    0H                                                  0270 00219000
@RC00259 DS    0H                                                  0270 00220000
@RC00257 DS    0H                                                  0270 00221000
@RC00255 DS    0H                                                  0270 00222000
@RC00253 DS    0H                                                  0270 00223000
@RC00251 DS    0H                                                  0270 00224000
@RC00249 L     @02,CTGPLPTR                                        0270 00225000
         ST    @02,FREEAD                                          0270 00226000
*       SUBPOOL=WKPOOL;             /* GET SUBPOOL NO.       @Z30SSRL*/ 00227000
         MVI   SUBPOOL,X'FC'                                       0271 00228000
*       FREELEN=CATBLKLN;           /* LENGTH OF PARM LIST   @Z30SSRL*/ 00229000
         LH    @02,CATBLKLN                                        0272 00230000
