*LOC1:                                                             0294 00280000
*                                                                  0294 00281000
*/********************************************************************/ 00282000
*/*                                                          @X04SVGP*/ 00283000
*/*     0U FUNCTION - LOCATE FROM THE CATALOG THE CI NBR FOR EACH    */ 00284000
*/*                   DATA SET ASSOCIATED WITH THE ACB, AND ALSO     */ 00285000
*/*                   THE ADDRESS OF THE CATALOG ACB.        @X04SVGP*/ 00286000
*/*                                                          @X04SVGP*/ 00287000
*/*     01 INPUT    - ACB WORK AREA, LOCATE DESCRIPTOR RECORD, JFCB  */ 00288000
*/*                                                          @X04SVGP*/ 00289000
*/*     01 OUTPUT   - CI NBRS AND CATALOG ACB ADDRESS        @X04SVRL*/ 00290000
*/*                   ARE INITIALIZED IN THE ACB WORK AREA   @X04SVGP*/ 00291000
*/*                                                          @X04SVGP*/ 00292000
*/********************************************************************/ 00293000
*                                                                  0294 00294000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @X04SVGP*/ 00295000
LOC1     DS    0H                                                  0295 00296000
*   RSAVLA=RET;                     /* SAVE RETURN ADDRESS   @X04SVGP*/ 00297000
         ST    RET,RSAVLA                                          0295 00298000
*   RESPECIFY                                                      0296 00299000
*     IFGACB BASED(ACBPT);          /*                       @X04SVGP*/ 00300000
*   IF OPWCAT=ON|OPWSCRA=ON|OPWUCRA=ON THEN/* SPECIAL DATA SET       */ 00301000
         TM    OPWCAT(RCORE),B'11000000'                           0297 00302000
         BNZ   @RT00297                                            0297 00303000
         TM    OPWUCRA(RCORE),B'10000000'                          0297 00304000
         BNO   @RF00297                                            0297 00305000
@RT00297 DS    0H                                                  0298 00306000
*     DO;                           /* YES, CONTINUE         @X04SVGP*/ 00307000
*       OPWBDTCI=CATCIDTA;          /* INIT DATA CI          @X04SVGP*/ 00308000
         SLR   @02,@02                                             0299 00309000
         STCM  @02,7,OPWBDTCI(RCORE)                               0299 00310000
*       OPWBIXCI=CATCIIDX;          /* INIT INDEX CI         @X04SVGP*/ 00311000
         MVC   OPWBIXCI(3,RCORE),@CF00053+1                        0300 00312000
*       OPWCACB=ACBPT;              /* INIT CAT ACB ADDR     @X04SVGP*/ 00313000
         L     @02,ACBPT                                           0301 00314000
         ST    @02,OPWCACB(,RCORE)                                 0301 00315000
*       OPWUPD=ON;                  /* AUTHORIZED FOR UPD    @X04SVGP*/ 00316000
