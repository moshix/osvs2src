*           OPWUPD=ON;              /* AUTHORIZED FOR UPD    @X04SVGP*/ 00349000
*           OPWCINV=ON;             /* AUTHORIZED FOR CINV   @X04SVGP*/ 00350000
         OI    OPWUPD(RCORE),B'01100000'                           0315 00351000
*         END;                      /*                       @X04SVGP*/ 00352000
*       DO CTR=1 TO 6;              /* PROCESS 6 FIELDS      @Y30DSMD*/ 00353000
@RC00311 LA    CTR,1                                               0317 00354000
@DL00317 DS    0H                                                  0318 00355000
*         FLACTIVE(CTR,1)=ON;       /* ACTIVATE 6 LOCATE FIELDS    0318 00356000
*                                                            @Y30DSMD*/ 00357000
         LR    @11,CTR                                             0318 00358000
         MH    @11,@CH00144                                        0318 00359000
         LA    @02,FLACTIVE-60(@11)                                0318 00360000
         OI    0(@02),B'10000000'                                  0318 00361000
*       END;                        /*                       @X04SVGP*/ 00362000
         AL    CTR,@CF00053                                        0319 00363000
         C     CTR,@CF00040                                        0319 00364000
         BNH   @DL00317                                            0319 00365000
*       FLNAME(1,1)='DSTYPNAM';     /* INIT SYMBOLIC FIELD NAMES     */ 00366000
         MVC   FLNAME(8),@CC03760                                  0320 00367000
*       FLNAME(2,1)='CATACB  ';     /*                       @X04SVGP*/ 00368000
         MVC   FLNAME+60(8),@CC03761                               0321 00369000
