         MVI   FLCOND+320,X'80'                                    0800 01943000
*   FLACTIVE(6,3)=OFF;              /* ONLY 1 TEST THIS LOCATE       */ 01944000
         NI    FLACTIVE+340,B'01111111'                            0801 01945000
*   RET=RSAVLC;                     /* RESTORE RETURN REG            */ 01946000
         L     RET,RSAVLC                                          0802 01947000
*   END LOC3;                       /* RETURN TO CALLER              */ 01948000
@EL00015 DS    0H                                                  0803 01949000
@EF00015 DS    0H                                                  0803 01950000
@ER00015 BR    @14                                                 0803 01951000
*LOCSTAT:                                                          0804 01952000
*                                                                  0804 01953000
*/********************************************************************/ 01954000
*/*                                                                  */ 01955000
*/*     01 FUNCTION - LOCATE THE STATISTICS FROM THE CATALOG.  INPUT */ 01956000
*/*                   TO THE CATALOG ROUTINES ARE THE CI NBR OF THE  */ 01957000
*/*                   DATA SET BEING PROCESSED AND THE ADDRESS OF THE*/ 01958000
*/*                   CATALOG ACB.                                   */ 01959000
*/*                                                                  */ 01960000
*/*     01 INPUT    - AMB, LOCATE DESCRIPTOR                         */ 01961000
*/*                                                                  */ 01962000
*/*     02 OUTPUT   - LOCATE DESCRIPTOR INITIALIZED TO LOCATE THE    */ 01963000
*/*                   DATA SET STATISTICS                            */ 01964000
*/*                                                                  */ 01965000
*/********************************************************************/ 01966000
*                                                                  0804 01967000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0804 01968000
LOCSTAT  DS    0H                                                  0805 01969000
*   RSAVLC=RET;                                                    0805 01970000
         ST    RET,RSAVLC                                          0805 01971000
*   CALL CLUPDESR;                  /* CLEAR UPDATE DESCRIPTOR RECORD*/ 01972000
         BAL   @14,CLUPDESR                                        0806 01973000
*   LOCTYPE=LOCTYPE&&LOCTYPE;       /* CLEAR LOCATE TYPE             */ 01974000
*                                                                  0807 01975000
         XC    LOCTYPE(1),LOCTYPE                                  0807 01976000
*   /*****************************************************************/ 01977000
*   /*                                                               */ 01978000
*   /* INIT LOCATE/UPDATE DESCRIPTOR TO LOCATE STATISTICS            */ 01979000
*   /*                                                               */ 01980000
*   /*****************************************************************/ 01981000
*                                                                  0808 01982000
*   ID3=ON;                         /* INDICATE LOCATE BY CI         */ 01983000
         OI    ID3,B'00100000'                                     0808 01984000
*   DSCATACB=AMBCACB;               /* INIT PTR TO CAT ACB           */ 01985000
         L     @02,AMBCACB(,R5)                                    0809 01986000
         ST    @02,DSCATACB                                        0809 01987000
*   DSNCI=AMBDDSN;                  /* INIT CI NBR OF DATA SET       */ 01988000
         MVC   DSNCI(3),AMBDDSN(R5)                                0810 01989000
*   FLACTIVE(1,1)=ON;               /* FIELD IS ACTIVE               */ 01990000
         OI    FLACTIVE,B'10000000'                                0811 01991000
*   FLNAME(1,1)='AMDSBCAT';         /* FIELD IS FOR AMDSB            */ 01992000
         MVC   FLNAME(8),@CC03774                                  0812 01993000
*   IF ID^=RCM THEN                 /* IF NOT CALL FROM EOV          */ 01994000
         L     @02,PARMPT                                          0813 01995000
         CLI   ID(@02),36                                          0813 01996000
         BE    @RF00813                                            0813 01997000
*     DO;                                                          0814 01998000
*                                                                  0814 01999000
*       /*************************************************************/ 02000000
*       /*                                                           */ 02001000
*       /* INIT LOCATE/UPDATE DESCRIPTOR TO LOCATE DATA SET NAME     */ 02002000
*       /*                                                           */ 02003000
*       /*************************************************************/ 02004000
*                                                                  0815 02005000
*       FLACTIVE(2,1)=ON;           /* 2ND FIELD ACTIVE              */ 02006000
         OI    FLACTIVE+60,B'10000000'                             0815 02007000
*       FLNAME(2,1)='ENTNAME ';     /* LOCATE DATA SET NAME          */ 02008000
         MVC   FLNAME+60(8),@CC03826                               0816 02009000
*     END;                                                         0817 02010000
*   RET=RSAVLC;                                                    0818 02011000
@RF00813 L     RET,RSAVLC                                          0818 02012000
*   END LOCSTAT;                                                   0819 02013000
@EL00016 DS    0H                                                  0819 02014000
@EF00016 DS    0H                                                  0819 02015000
@ER00016 BR    @14                                                 0819 02016000
*UPSTAT:                                                           0820 02017000
*                                                                  0820 02018000
*/********************************************************************/ 02019000
*/*                                                                  */ 02020000
*/*     01 FUNCTION - BASED ON THE STATISTICS SAVED AT OPEN, THE     */ 02021000
*/*                   STATISTICS IN THE CATALOG, AND THE AMDSB       */ 02022000
*/*                   STATISTICS, COMPUTE THE NEW STATISTICS TO      */ 02023000
*/*                   BE REPLACED IN THE CATALOG FOR THE DATA SET    */ 02024000
*/*                                                                  */ 02025000
*/*     01 INPUT    - AMDSB,STATISTICS SAVED BY OPEN, AND THE CATALOG*/ 02026000
*/*                   STATISTICS                                     */ 02027000
*/*                                                                  */ 02028000
*/*     02 OUTPUT   - FIELD DESCRIPTOR RECORD INITIALIZED TO UPDATE  */ 02029000
*/*                   THE NEWLY COMPUTED STATISTICS INTO THE CATALOG */ 02030000
*/*     NOTES - WHEN THE CATALOG IS UPDATED, THE OPEN COPY OF THE    */ 02031000
*/*             AMDSB IS UPDATED TO SYNCHRONIZE THE CATALOG AND THE  */ 02032000
*/*             COPY KEPT TO COMPUTE STATISTICS CHANGES.             */ 02033000
*/*                                                                  */ 02034000
*/********************************************************************/ 02035000
*                                                                  0820 02036000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0820 02037000
UPSTAT   DS    0H                                                  0821 02038000
*   RSAVLC=RET;                                                    0821 02039000
         ST    RET,RSAVLC                                          0821 02040000
*   IF ID^=RCM THEN                 /* IF NOT CALL FROM EOV          */ 02041000
         L     @11,PARMPT                                          0822 02042000
         CLI   ID(@11),36                                          0822 02043000
         BE    @RF00822                                            0822 02044000
*     DO;                                                          0823 02045000
*                                                                  0823 02046000
*       /*************************************************************/ 02047000
*       /*                                                           */ 02048000
*       /* SAVE NAME FOR CLOSE                                       */ 02049000
*       /*                                                           */ 02050000
*       /*************************************************************/ 02051000
*                                                                  0824 02052000
*       CTGFLPTR=CTGFIELD(2);       /* ADDRESSABILITY 2ND FIELD      */ 02053000
         L     @11,CTGPLPTR                                        0824 02054000
         L     CTGFLPTR,CTGFIELD+4(,@11)                           0824 02055000
*       IF CTGFLPT(1)^=0 THEN       /* LOCATE SUCCESSFUL?            */ 02056000
         L     @11,CTGFLPT(,CTGFLPTR)                              0825 02057000
         LTR   @11,@11                                             0825 02058000
         BZ    @RF00825                                            0825 02059000
*         DSNCHR44=DSNFA;           /* YES, MOVE DSN CALLER'S AREA   */ 02060000
         L     @01,AMBWKA(,R5)                                     0826 02061000
         MVC   DSNCHR44(44,@01),DSNFA(@11)                         0826 02062000
*     END;                                                         0827 02063000
*                                                                  0827 02064000
@RF00825 DS    0H                                                  0828 02065000
*   /*****************************************************************/ 02066000
*   /*                                                               */ 02067000
*   /* DETERMINE NEW STATISTICS TO BE ENTERED IN CATALOG             */ 02068000
*   /*                                                               */ 02069000
*   /*****************************************************************/ 02070000
*                                                                  0828 02071000
*   CTGFLPTR=CTGFIELD(1);           /* ADDRESSABILITY FIRST FIELD    */ 02072000
@RF00822 L     @11,CTGPLPTR                                        0828 02073000
         L     CTGFLPTR,CTGFIELD(,@11)                             0828 02074000
*   R8=CTGFLPT(1);                  /* ADDRESSABILITY TO CAT AMDSB 0829 02075000
*                                                            @XA05646*/ 02076000
         L     R8,CTGFLPT(,CTGFLPTR)                               0829 02077000
*   R6=AMBDSB;                      /* ADDRESSABILITY TO REAL AMDSB     02078000
*                                                            @XA05646*/ 02079000
         L     R6,AMBDSB(,R5)                                      0830 02080000
*   R7=R6+LENGTH(AMDSTAT);          /* ADDR OF STATISTICS AT OPEN  0831 02081000
*                                                            @XA05646*/ 02082000
         LA    R7,48                                               0831 02083000
         ALR   R7,R6                                               0831 02084000
*   IF R8^=0 THEN                   /* LOCATE SUCCESSFUL?    @XM02250*/ 02085000
         LTR   R8,R8                                               0832 02086000
         BZ    @RF00832                                            0832 02087000
*     DO;                           /* YES, DO:                      */ 02088000
*                                                                  0833 02089000
*       /*************************************************************/ 02090000
*       /*                                                           */ 02091000
*       /* UNCONDITIONAL REPLACEMENT                                 */ 02092000
*       /*                                                           */ 02093000
*       /*************************************************************/ 02094000
*                                                                  0834 02095000
*       R8->AMDSTSP=R6->AMDSTSP;    /* UPDATE SYSTEM TIME STAMP      */ 02096000
         MVC   AMDSTSP(8,R8),AMDSTSP(R6)                           0834 02097000
*       R8->AMDASPA=R6->AMDASPA;    /* UPDATE BYTES OF FREE SPACE    */ 02098000
         L     @11,AMDASPA(,R6)                                    0835 02099000
         ST    @11,AMDASPA(,R8)                                    0835 02100000
*       R8->AMDHLRBA=R6->AMDHLRBA;  /* UPDATE RBA OF HI LEVEL INDX   */ 02101000
         L     @11,AMDHLRBA(,R6)                                   0836 02102000
         ST    @11,AMDHLRBA(,R8)                                   0836 02103000
*       R8->AMDSSRBA=R6->AMDSSRBA;  /* UPDATE 1ST SEQ SET REC RBA    */ 02104000
         L     @11,AMDSSRBA(,R6)                                   0837 02105000
         ST    @11,AMDSSRBA(,R8)                                   0837 02106000
*       IF R6->AMDLM=ON THEN        /* CREATE COMPLETE?      @XM02250*/ 02107000
         TM    AMDLM(R6),B'00001000'                               0838 02108000
         BNO   @RF00838                                            0838 02109000
*         R8->AMDLM=ON;             /* THEN IND IT           @XM02250*/ 02110000
*                                                                  0839 02111000
         OI    AMDLM(R8),B'00001000'                               0839 02112000
*       /*************************************************************/ 02113000
*       /*                                                           */ 02114000
*       /* REPLACE ONLY IF LARGER                                    */ 02115000
*       /*                                                           */ 02116000
*       /*************************************************************/ 02117000
*                                                                  0840 02118000
*       IF(R6->AMDNIL>R8->AMDNIL) THEN                             0840 02119000
