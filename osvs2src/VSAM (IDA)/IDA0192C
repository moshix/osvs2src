         TM    ACBBYPSS(@01),B'00000001'                           1211 03661000
         BNO   @RF01211                                            1211 03662000
*     DO;                           /*                       @Z40RSRC*/ 03663000
*       RFY                                                        1213 03664000
*        (R0,                                                      1213 03665000
*         R1,                                                      1213 03666000
*         RETCODE) RSTD;            /*                       @Z40RSRC*/ 03667000
*       GEN SETS(R0,R1,RETCODE)                                    1214 03668000
*           (TESTAUTH FCTN=1,STATE=YES,KEY=YES,RBLEVEL=2);/* @Z40RSRC*/ 03669000
         TESTAUTH FCTN=1,STATE=YES,KEY=YES,RBLEVEL=2                    03670000
*       IF RETCODE=0 THEN           /* IF AUTH'D CALLER      @Z40RSRC*/ 03671000
         LTR   RETCODE,RETCODE                                     1215 03672000
         BNZ   @RF01215                                            1215 03673000
*         CTGBYPSS=ON;              /* BYPASS AUTH CHK       @Z40RSRC*/ 03674000
         L     @14,CTGPLPTR                                        1216 03675000
         OI    CTGBYPSS(@14),B'10000000'                           1216 03676000
*       ELSE                                                       1217 03677000
*         ;                         /* CALLER NOT AUTH'D     @Z40RSRC*/ 03678000
@RF01215 DS    0H                                                  1218 03679000
*       RFY                                                        1218 03680000
*        (R0,                                                      1218 03681000
*         R1,                                                      1218 03682000
*         RETCODE) UNRSTD;          /*                       @Z40RSRC*/ 03683000
*     END;                          /*                       @Z40RSRC*/ 03684000
*   ELSE                                                           1220 03685000
*     ;                             /* NO BYPASS REQUEST     @Z40RSRC*/ 03686000
@RF01211 DS    0H                                                  1221 03687000
*   RET=RSAVLD;                                                    1221 03688000
         L     RET,RSAVLD                                          1221 03689000
*   END AUTHCHK;                                                   1222 03690000
@EL00024 DS    0H                                                  1222 03691000
@EF00024 DS    0H                                                  1222 03692000
@ER00024 BR    @14                                                 1222 03693000
*AMBWKRB:                                                          1223 03694000
*                                                                  1223 03695000
*/********************************************************************/ 03696000
*/*                                                                  */ 03697000
*/*     01 FUNCTION - THE SIZE OF THE EXISTING AMB WORK AREA HAS BEEN*/ 03698000
*/*                   EXCEEDED.  THIS ROUTINE WILL GET A LARGER WORK */ 03699000
*/*                   AREA, COPY EXISTING INFORMATION FROM THE OLD   */ 03700000
*/*                   WORK AREA, AND THEN FREE THE OLD AREA.         */ 03701000
*/*                                                                  */ 03702000
*/*     01 INPUT    - AMB WORK AREA. AND REGS 6,7,8 WHICH ARE PLACE  */ 03703000
*/*                   HOLDERS WITHIN THE WORK AREA.                  */ 03704000
*/*                                                                  */ 03705000
*/*     02 OUTPUT   - NEW AMB WORK AREA. PLACE HOLDER REGS 6,7,&8    */ 03706000
*/*                   ARE ADJUSTED TO SAME RELATIVE POSITIONS WITHIN */ 03707000
*/*                   THE NEW AMB WORK AREA                          */ 03708000
*/*                                                                  */ 03709000
*/********************************************************************/ 03710000
*                                                                  1223 03711000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1223 03712000
AMBWKRB  DS    0H                                                  1224 03713000
*   RSAVLD=RET;                     /* SAVE RETURN ADDRESS           */ 03714000
         ST    RET,RSAVLD                                          1224 03715000
*   IF(R8+LL3)>(ADDR(AMBWORK)+AMALEN) THEN/* AMBWORK EXCEEDED BY   1225 03716000
*                                      EXTS?                         */ 03717000
         LA    @10,2                                               1225 03718000
         L     @15,CTGFLPT(,CTGFLPTR)                              1225 03719000
         LA    @14,LL1(,@15)                                       1225 03720000
         ICM   @15,12,LL1(@15)                                     1225 03721000
         SRA   @15,16                                              1225 03722000
         ALR   @14,@15                                             1225 03723000
         ALR   @14,@10                                             1225 03724000
         ICM   @15,12,LL2(@14)                                     1225 03725000
         SRA   @15,16                                              1225 03726000
         ALR   @15,@14                                             1225 03727000
         ALR   @15,@10                                             1225 03728000
         ICM   @10,12,LL3(@15)                                     1225 03729000
         SRA   @10,16                                              1225 03730000
         LR    @15,R8                                              1225 03731000
         ALR   @15,@10                                             1225 03732000
         L     @14,RCOREF                                          1225 03733000
         LH    @01,AMALEN(,@14)                                    1225 03734000
         ALR   @01,@14                                             1225 03735000
         CR    @15,@01                                             1225 03736000
         BNH   @RF01225                                            1225 03737000
*     CORELEN=MAX(LL3,SWKLEN);      /* YES, BUMP SIZE BY LARGER OF 1226 03738000
*                                      EXTENT LENGTH OR 500          */ 03739000
         ST    @10,@TF00001                                        1226 03740000
         L     CORELEN,@TF00001                                    1226 03741000
         LA    @15,1024                                            1226 03742000
         CR    CORELEN,@15                                         1226 03743000
         BNL   *+6                                                      03744000
         LR    CORELEN,@15                                         1226 03745000
*   ELSE                            /* NOT EXCEEDED BY EXT   @Z30SSRL*/ 03746000
*     CORELEN=SWKLEN;               /* BUMP SIZE BY 500      @Z30SSRL*/ 03747000
         B     @RC01225                                            1227 03748000
@RF01225 LA    CORELEN,1024                                        1227 03749000
*   CORELEN=CORELEN+AMALEN;         /* ADD IN EXISTING WORK SIZE     */ 03750000
@RC01225 L     @01,RCOREF                                          1228 03751000
         AH    CORELEN,AMALEN(,@01)                                1228 03752000
*   SUBPOOL=WKPOOL;                 /* INIT SUBPOOL NBR              */ 03753000
         MVI   SUBPOOL,X'FC'                                       1229 03754000
*   CALL GETCORE;                   /* GET CORE FOR NEW AMBWORK      */ 03755000
         BAL   @14,GETCORE                                         1230 03756000
*   IF PPCODE=0 THEN                /* IF SUCCESSFUL         @Z30SSRL*/ 03757000
         CLI   PPCODE,0                                            1231 03758000
         BNE   @RF01231                                            1231 03759000
*     DO;                                                          1232 03760000
*       TRANS=COREADDR-RCOREF;      /* DETERMIN AMT TO ADD TO CALLERS   03761000
*                                      PLACE HOLDERS                 */ 03762000
         L     @15,COREADDR                                        1233 03763000
         SL    @15,RCOREF                                          1233 03764000
         ST    @15,TRANS                                           1233 03765000
*       GEN(STM R6,R9,TEMPSR);      /* SAVE REGS                     */ 03766000
         STM R6,R9,TEMPSR                                               03767000
*       R6=ADDR(NEWAMBWK);          /* PT TO NEW BLOCK               */ 03768000
         L     R6,COREADDR                                         1235 03769000
*       R8=ADDR(AMBWORK);           /* PT TO OLD BLOCK               */ 03770000
         L     @15,RCOREF                                          1236 03771000
         LR    R8,@15                                              1236 03772000
*       R7=AMALEN;                  /* LOAD BLOCK LENGTH             */ 03773000
         LH    R7,AMALEN(,@15)                                     1237 03774000
*       R9=R7;                      /* COPY BLOCK LENGTH             */ 03775000
         LR    R9,R7                                               1238 03776000
*       GEN(MVCL R6,R8);            /* COPY BLOCK                    */ 03777000
         MVCL R6,R8                                                     03778000
*       GEN(LM R6,R9,TEMPSR);       /* RESTORE REGS                  */ 03779000
         LM R6,R9,TEMPSR                                                03780000
*       FREELEN=AMALEN;                                            1241 03781000
         L     @15,RCOREF                                          1241 03782000
         LH    @14,AMALEN(,@15)                                    1241 03783000
         STH   @14,FREELEN                                         1241 03784000
*       FREEAD=RCOREF;                                             1242 03785000
         ST    @15,FREEAD                                          1242 03786000
*       CALL FREECORE;              /* FREE OLD AMBWORK              */ 03787000
         BAL   @14,FREECORE                                        1243 03788000
*       IF RCOREF=DATAWAPT THEN     /* EXTENDING DATA AMB WK AREA?   */ 03789000
         L     @15,PARMPT                                          1244 03790000
         CLC   RCOREF(4),DATAWAPT(@15)                             1244 03791000
         BNE   @RF01244                                            1244 03792000
*         DATAWAPT=COREADDR;        /* YES, UPDATE ADDRESS OF DATA   */ 03793000
         L     @14,COREADDR                                        1245 03794000
         ST    @14,DATAWAPT(,@15)                                  1245 03795000
*       ELSE                        /* NO,                           */ 03796000
*         INDXWAPT=COREADDR;        /* UPDATE ADDR OF INDX           */ 03797000
         B     @RC01244                                            1246 03798000
@RF01244 L     @15,COREADDR                                        1246 03799000
         L     @14,PARMPT                                          1246 03800000
         ST    @15,INDXWAPT(,@14)                                  1246 03801000
*       RCOREF=COREADDR;            /* ADDRESSABILITY NEW WK AREA    */ 03802000
@RC01244 L     @15,COREADDR                                        1247 03803000
         ST    @15,RCOREF                                          1247 03804000
*       AMALEN=CORELEN;             /* RECORD NEW WK AREA LENGTH     */ 03805000
         STH   CORELEN,AMALEN(,@15)                                1248 03806000
*       R6=R6+TRANS;                /* UPDATE CALLERS PLACE PTRS     */ 03807000
         L     @10,TRANS                                           1249 03808000
         ALR   R6,@10                                              1249 03809000
*       R7=R7+TRANS;                                               1250 03810000
         ALR   R7,@10                                              1250 03811000
*       R8=R8+TRANS;                                               1251 03812000
         ALR   R8,@10                                              1251 03813000
*     END;                                                         1252 03814000
*   RET=RSAVLD;                     /* RESTORE RETURN ADDR           */ 03815000
@RF01231 L     RET,RSAVLD                                          1253 03816000
*   END AMBWKRB;                    /* RETURN TO CALLER              */ 03817000
@EL00025 DS    0H                                                  1254 03818000
@EF00025 DS    0H                                                  1254 03819000
@ER00025 BR    @14                                                 1254 03820000
*REBLDWA:                                                          1255 03821000
*                                                                  1255 03822000
*/********************************************************************/ 03823000
*/*                                                                  */ 03824000
*/*     01 FUNCTION - THE SIZE OF THE CATALOG WORK AREA HAS BEEN     */ 03825000
*/*                   EXCEEDED.  THIS ROUTINE WILL GET A LARGER      */ 03826000
*/*                   WORK AREA, AND THEN FREE THE OLD WORK AREA     */ 03827000
*/*                                                                  */ 03828000
*/*     01 INPUT    - CPL, OLD CATALOG WORK AREA                     */ 03829000
*/*                                                                  */ 03830000
*/*     02 OUTPUT   - NEW CATALOG WORK AREA, CPL MODIFIED TO POINT   */ 03831000
*/*                   TO NEW WORK AREA                               */ 03832000
*/*                                                                  */ 03833000
*/********************************************************************/ 03834000
*                                                                  1255 03835000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1255 03836000
REBLDWA  DS    0H                                                  1256 03837000
*   RSAVLD=RET;                     /* SAVE RETURN REG               */ 03838000
         ST    RET,RSAVLD                                          1256 03839000
*   CORELEN=REQLEN;                 /* LENGTH FOR GETMAIN            */ 03840000
         L     @02,CATWKPTR                                        1257 03841000
         LH    CORELEN,REQLEN(,@02)                                1257 03842000
*   SUBPOOL=WKPOOL;                 /* SUBPOOL INIT FOR GETMAIN      */ 03843000
         MVI   SUBPOOL,X'FC'                                       1258 03844000
*   CALL GETCORE;                   /* GET CORE FOR NEW WORK AREA    */ 03845000
         BAL   @14,GETCORE                                         1259 03846000
*   IF PPCODE=0 THEN                /* IF SUCCESSFUL         @Z30SSRL*/ 03847000
         CLI   PPCODE,0                                            1260 03848000
         BNE   @RF01260                                            1260 03849000
*     DO;                                                          1261 03850000
*       FREEAD=CTGWKA;              /* ADDR OF OLD WORK AREA         */ 03851000
         L     @11,CTGPLPTR                                        1262 03852000
         L     @11,CTGWKA(,@11)                                    1262 03853000
         ST    @11,FREEAD                                          1262 03854000
*       FREELEN=WORKLEN;            /* LENGTH TO FREE                */ 03855000
         L     @11,CATWKPTR                                        1263 03856000
         LH    @11,WORKLEN(,@11)                                   1263 03857000
         STH   @11,FREELEN                                         1263 03858000
*       CALL FREECORE;              /* FREE OLD WORK AREA            */ 03859000
         BAL   @14,FREECORE                                        1264 03860000
*       CTGWKA=COREADDR;            /* INIT PRT TO NEW WK AREA       */ 03861000
         L     @11,COREADDR                                        1265 03862000
         L     @02,CTGPLPTR                                        1265 03863000
         ST    @11,CTGWKA(,@02)                                    1265 03864000
*       CATWKPTR=CTGWKA;                                           1266 03865000
         ST    @11,CATWKPTR                                        1266 03866000
*       WORKLEN=CORELEN;            /* INIT LENGTH NEW WORK AREA     */ 03867000
         STH   CORELEN,WORKLEN(,@11)                               1267 03868000
*     END;                                                         1268 03869000
*   RET=RSAVLD;                     /* RESTORE RETURN ADDRESS        */ 03870000
@RF01260 L     RET,RSAVLD                                          1269 03871000
*   END REBLDWA;                    /* RETURN TO CALLER              */ 03872000
@EL00026 DS    0H                                                  1270 03873000
@EF00026 DS    0H                                                  1270 03874000
@ER00026 BR    @14                                                 1270 03875000
*GETCORE:                                                          1271 03876000
*                                                                  1271 03877000
*/********************************************************************/ 03878000
*/*                                                                  */ 03879000
*/*     01 FUNCTION - PERFORM GETMAIN FOR A SPECIFIED AMOUNT OF CORE */ 03880000
*/*                   FROM THE REQUESTED SUBPOOL.  ZERO GOTTEN CORE. */ 03881000
*/*                                                                  */ 03882000
*/*     01 INPUT    - MODULE WORKAREA CONTAINING THE SUBPOOL NBR     */ 03883000
*/*                   AND CORE LENGTH REQUIRED.                      */ 03884000
*/*                                                                  */ 03885000
*/*     02 OUTPUT   - COREADDR (MODULE WORK AREA FIELD) CONTAINS THE */ 03886000
*/*                   ADDRESS OF GOTTEN CORE.                        */ 03887000
*/*                                                                  */ 03888000
*/********************************************************************/ 03889000
*                                                                  1271 03890000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1271 03891000
GETCORE  DS    0H                                                  1272 03892000
*   RSAVLE=RET;                     /* SAVE RETURN ADDR              */ 03893000
         ST    RET,RSAVLE                                          1272 03894000
*   CORELEN=(CORELEN+7)/DWORDLEN*DWORDLEN;/* ROUND UP LENGTH FOR GETM*/ 03895000
         LA    @00,7                                               1273 03896000
         ALR   @00,CORELEN                                         1273 03897000
         SRDA  @00,32                                              1273 03898000
         D     @00,@CF00049                                        1273 03899000
         SLA   @01,3                                               1273 03900000
         LR    CORELEN,@01                                         1273 03901000
*   GEN(STM R6,R9,TEMPSR);                                         1274 03902000
         STM R6,R9,TEMPSR                                               03903000
*   R7=SUBPOOL;                     /* INIT SUBPOOL FOR GETMAIN      */ 03904000
         SLR   R7,R7                                               1275 03905000
         IC    R7,SUBPOOL                                          1275 03906000
*   R9=CORELEN;                     /* INIT LENGTH FOR GETMAIN       */ 03907000
         LR    R9,CORELEN                                          1276 03908000
*   GEN(GETMAIN EC,LV=(R9),A=COREADDR,SP=(R7),MF=(E,MAINLIST));    1277 03909000
         GETMAIN EC,LV=(R9),A=COREADDR,SP=(R7),MF=(E,MAINLIST)          03910000
*   IF RETCODE=0 THEN               /* GETMAIN SUCCESSFUL?           */ 03911000
         LTR   RETCODE,RETCODE                                     1278 03912000
         BNZ   @RF01278                                            1278 03913000
*     DO;                           /* YES,DO:                       */ 03914000
*       R8=COREADDR;                /* CLEAR GETMAINED CORE          */ 03915000
         L     R8,COREADDR                                         1280 03916000
*       R7=0;                                                      1281 03917000
         SLR   R7,R7                                               1281 03918000
*       GEN(MVCL R8,R6);                                           1282 03919000
         MVCL R8,R6                                                     03920000
*     END;                                                         1283 03921000
*   ELSE                            /* GETMAIN FAILED        @Z30SSRL*/ 03922000
*     DO;                                                          1284 03923000
         B     @RC01278                                            1284 03924000
@RF01278 DS    0H                                                  1285 03925000
*       PPCODE=PP20;                /* SET PROBLEM CODE      @Z30SSRL*/ 03926000
         MVI   PPCODE,X'14'                                        1285 03927000
*     END;                                                         1286 03928000
*   GEN(LM R6,R9,TEMPSR);                                          1287 03929000
@RC01278 DS    0H                                                  1287 03930000
         LM R6,R9,TEMPSR                                                03931000
*   RET=RSAVLE;                     /* RESTORE RETURN REG            */ 03932000
         L     RET,RSAVLE                                          1288 03933000
*   END GETCORE;                    /* RETURN TO CALLER              */ 03934000
@EL00027 DS    0H                                                  1289 03935000
@EF00027 DS    0H                                                  1289 03936000
@ER00027 BR    @14                                                 1289 03937000
*FREECORE:                                                         1290 03938000
*                                                                  1290 03939000
*/********************************************************************/ 03940000
*/*                                                                  */ 03941000
*/*     01 FUNCTION - PERFORM FREEMAIN FOR A SPECIFIED AMOUNT OF CORE*/ 03942000
*/*                   FROM THE REQUESTED CORE LOCATION.              */ 03943000
*/*                                                                  */ 03944000
*/*     01 INPUT    - MODULE WORK AREA CONTAINING THE ADDRESS OF THE */ 03945000
*/*                   LOCATION TO BE FREED AND LENGTH TO BE FREED.   */ 03946000
*/*                                                                  */ 03947000
*/*     01 OUTPUT   - SPECIFIED CORE IS FREED.                       */ 03948000
*/*                                                                  */ 03949000
*/********************************************************************/ 03950000
*                                                                  1290 03951000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1290 03952000
FREECORE DS    0H                                                  1291 03953000
*   RSAVLE=RET;                     /* SAVE RETURN REG               */ 03954000
         ST    RET,RSAVLE                                          1291 03955000
*   GEN(STM R6,R9,TEMPSR);          /* SAVE REGISTERS                */ 03956000
         STM R6,R9,TEMPSR                                               03957000
*   R7=SUBPOOL;                     /* INIT SUBPOOL FOR FREEMAIN     */ 03958000
         SLR   R7,R7                                               1293 03959000
         IC    R7,SUBPOOL                                          1293 03960000
*   R9=FREELEN;                     /* INIT LENGTH FOR FREEMAIN      */ 03961000
         LH    R9,FREELEN                                          1294 03962000
*   GEN(FREEMAIN E,LV=(R9),A=FREEAD,SP=(R7),MF=(E,MAINLIST));      1295 03963000
         FREEMAIN E,LV=(R9),A=FREEAD,SP=(R7),MF=(E,MAINLIST)            03964000
*   GEN(LM R6,R9,TEMPSR);           /* RESTORE REGISTERS             */ 03965000
         LM R6,R9,TEMPSR                                                03966000
*   RET=RSAVLE;                     /* RESTORE RETURN REG            */ 03967000
         L     RET,RSAVLE                                          1297 03968000
*   END FREECORE;                   /* RETURN TO CALLER              */ 03969000
@EL00028 DS    0H                                                  1298 03970000
@EF00028 DS    0H                                                  1298 03971000
@ER00028 BR    @14                                                 1298 03972000
*CLUPDESR:                                                         1299 03973000
*                                                                  1299 03974000
*/********************************************************************/ 03975000
*/*                                                                  */ 03976000
*/*    01 FUNCTION - ZERO FIELD DESCRIPTOR RECORD.                   */ 03977000
*/*                                                                  */ 03978000
*/*    01 INPUT    - FIELD DESCRIPTOR RECORD.                        */ 03979000
*/*                                                                  */ 03980000
*/*    01 OUTPUT   - FIELD DESCRIPTOR RECORD IS CLEARED.             */ 03981000
*/*                                                                  */ 03982000
*/********************************************************************/ 03983000
*                                                                  1299 03984000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1299 03985000
CLUPDESR DS    0H                                                  1300 03986000
*   RSAVLD=RET;                     /* SAVE RETURN ADDRESS           */ 03987000
         ST    RET,RSAVLD                                          1300 03988000
*   R7=0;                           /* ZERO PAD/LENGTH               */ 03989000
         SLR   R7,R7                                               1301 03990000
*   R8=ADDR(FL(1,1));               /* SET START OF FIELD TO ZERO    */ 03991000
         LA    R8,FL                                               1302 03992000
*   R9=LENGTH(FL(1,1))*LOC18;       /* SET LENGTH OF FIELD TO ZERO   */ 03993000
         LA    R9,360                                              1303 03994000
*   GEN(MVCL R8,R6);                /* ZERO FIELD                    */ 03995000
         MVCL R8,R6                                                     03996000
*   RET=RSAVLD;                     /* RESTORE RETURN ADDRESS        */ 03997000
         L     RET,RSAVLD                                          1305 03998000
