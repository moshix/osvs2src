*     ARINH=ON;                     /* INDICATE DO NOT STAGE DATA    */ 01389000
         LA    @15,CURRENT                                         0572 01390000
         OI    ARINH(@15),B'00100000'                              0572 01391000
*   IF VIRTACQ=ON&                  /* IF ACQUIRE FUNCTION           */ 01392000
*       AMBCREAT=ON&                /* THIS IS A NEW DATA SET OR THIS   01393000
*                                      IS REUSABLE OPT               */ 01394000
