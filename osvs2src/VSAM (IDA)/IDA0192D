*               GEN(WAIT  (R15),ECBLIST=(R1));/* WAIT ON ECBS        */ 01679000
         WAIT  (R15),ECBLIST=(R1)                                       01680000
*               RFY                                                0678 01681000
*                 R1 UNRESTRICTED;                                 0678 01682000
*               ACQPTR=FIRSTACQ;    /* GET FIRST RELINQ LIST         */ 01683000
         L     ACQPTR,FIRSTACQ                                     0679 01684000
*               DO WHILE ACQPTR^=0; /* CHECK ALL ECBS FOR ERR        */ 01685000
