*               FIRST=ON;           /* INDICATE FIRST REQUEST DONE   */ 01199000
@RC00504 OI    FIRST,B'00000100'                                   0510 01200000
*             END;                  /* END FIRST TIME GET INDEX AMB  */ 01201000
*           ELSE                    /* THE INDEX AMB HAS BEEN PASSED */ 01202000
*             DO;                   /* PASS DATA IF NOT SEQ WITH DAT */ 01203000
         B     @RC00502                                            0512 01204000
@RF00502 DS    0H                                                  0513 01205000
*               IF AMDSDT=ON THEN   /* IS SEQ WITH DATA THEN DONE    */ 01206000
         L     @15,AMBDSB(,RAMB)                                   0513 01207000
         TM    AMDSDT(@15),B'00100000'                             0513 01208000
         BNO   @RF00513                                            0513 01209000
*                 DO;               /* THIS IS THE LAST TIME ENTERED */ 01210000
*                   RAMB=0;         /* INDICATE NO MORE AMBS         */ 01211000
         SLR   RAMB,RAMB                                           0515 01212000
*                   IF VIRTACQ=ON&  /* CHECK IF ACQUIRE REQUEST      */ 01213000
*                       VIRTBACB=ON THEN/* IS COMPLETE CLUSTER REQ   */ 01214000
         TM    VIRTACQ(PARMPTR),B'10010000'                        0516 01215000
         BNO   @RF00516                                            0516 01216000
*                     AMBLSTAG=ON;  /* INDICATE THE AMBL IS STAGED   */ 01217000
         L     @15,VIRTACB(,PARMPTR)                               0517 01218000
         L     @15,ACBAMBL(,@15)                                   0517 01219000
         OI    AMBLSTAG(@15),B'00010000'                           0517 01220000
*                 END;              /* END IF SEQ WITH DATA AND LAST */ 01221000
*               ELSE                /* THIS IS NOT SEQ WITH DATA     */ 01222000
*                 DO;               /* RETURN DATA AMB TO CALLER     */ 01223000
         B     @RC00513                                            0519 01224000
@RF00513 DS    0H                                                  0520 01225000
*                   RAMB=AMBLDTA;   /* GET DATA AMB                  */ 01226000
