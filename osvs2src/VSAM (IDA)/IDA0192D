*             R0=CORELEN;           /* GET LENGTH FROM CORE HEADR    */ 00827000
         LH    R0,CORELEN(,COREPTR)                                0391 00828000
*             R1=COREPTR;           /* POINT TO CORE FOR FREEMAIN    */ 00829000
         LR    R1,COREPTR                                          0392 00830000
*             COREPTR=CORECHN;      /* POINT TO NEXT CORE BEFORE FRE */ 00831000
         LR    COREPTR,@15                                         0393 00832000
*             GEN(FREEMAIN  R,LV=(0),A=(1));/* FREE OLD ACQ LIST     */ 00833000
         FREEMAIN  R,LV=(0),A=(1)                                       00834000
*           END;                    /* END IF ACQUIRE LIST FOUND     */ 00835000
*         ELSE                      /* THIS IS NOT AN ACQUIRE LIST   */ 00836000
*           DO;                     /* NORMAL NON ACQUIRE LIST       */ 00837000
         B     @RC00388                                            0396 00838000
@RF00388 DS    0H                                                  0397 00839000
*             PREVCORE=COREPTR;     /* CURRENT BECOMES PREVIOUS      */ 00840000
         ST    COREPTR,PREVCORE                                    0397 00841000
*             COREPTR=CORECHN;      /* NEXT BECORES CURRENT          */ 00842000
         L     COREPTR,CORECHN(,COREPTR)                           0398 00843000
*           END;                    /* NORMAL INCREMENT ON CHAIN     */ 00844000
*       END;                        /* END FREE CORE LOOP            */ 00845000
@RC00388 DS    0H                                                  0400 00846000
@DE00387 SLR   @15,@15                                             0400 00847000
         CR    COREPTR,@15                                         0400 00848000
         BNE   @DL00387                                            0400 00849000
*       FIRSTACQ=0;                 /* INDICATE FIRST ACQUIRE LIST   */ 00850000
         ST    @15,FIRSTACQ                                        0401 00851000
*       LISTOTAL=0;                 /* ZERO COUNT OF LISTS           */ 00852000
         ST    @15,LISTOTAL                                        0402 00853000
*       MAINLEN=LENGTH(ACQINIT)+8;  /* GET NEEDED LENGTH AGAIN       */ 00854000
         MVC   MAINLEN(3),@CF03097+1                               0403 00855000
*       ACQCORE=ON;                 /* INDICATE ACQUIRE LIST         */ 00856000
         OI    ACQCORE,B'00000010'                                 0404 00857000
*       CALL GETMAIN;               /* TRY GETCORE AGAIN             */ 00858000
         BAL   @14,GETMAIN                                         0405 00859000
*       IF PDPCODE=PP1 THEN         /* GIVE UP THIS TIME IF ERROR    */ 00860000
         LA    @15,ERRORWD                                         0406 00861000
         CLI   PDPCODE(@15),20                                     0406 00862000
         BE    @RT00406                                            0406 00863000
*         GOTO EXIT;                /* EXIT NO CAN DO                */ 00864000
*     END;                          /* END RECOVERY ON NO CORE ERR   */ 00865000
*   ACQPTR=COREPTR;                 /* POINT TO RETURNED CORE        */ 00866000
@RF00377 LR    ACQPTR,COREPTR                                      0409 00867000
*   LISTOTAL=LISTOTAL+1;            /* INCR COUNT OF TOTAL LIST      */ 00868000
         LA    @15,1                                               0410 00869000
         AL    @15,LISTOTAL                                        0410 00870000
         ST    @15,LISTOTAL                                        0410 00871000
*   ACQINIT=ACQINIT&&ACQINIT;       /* CLEAR THE NEW LIST            */ 00872000
         XC    ACQINIT(96,ACQPTR),ACQINIT(ACQPTR)                  0411 00873000
*   EXTENSON=EXTENSON&&EXTENSON;    /* CLEAR LIST EXTENSION          */ 00874000
         XC    EXTENSON(9,ACQPTR),EXTENSON(ACQPTR)                 0412 00875000
*   IF VIRTACQ=OFF THEN             /* IS THIS AN RELINQUISH REQUEST */ 00876000
         TM    VIRTACQ(PARMPTR),B'10000000'                        0413 00877000
         BNZ   @RF00413                                            0413 00878000
*     AROP=4;                       /* INDICATE RELINQUISH REQUEST   */ 00879000
         MVI   AROP(ACQPTR),X'04'                                  0414 00880000
*   ELSE                                                           0415 00881000
*     AROP=2;                       /* INDICATE ACQUIRE LIST         */ 00882000
         B     @RC00413                                            0415 00883000
@RF00413 MVI   AROP(ACQPTR),X'02'                                  0415 00884000
*   ARLL=LENGTH(ACQINIT);           /* PUT LENGTH INTO LIST          */ 00885000
@RC00413 MVC   ARLL(4,ACQPTR),@CF03093                             0416 00886000
*   RUCB=LISTUCB(LISTCNT);          /* GET UCB FOR THIS LIST         */ 00887000
         LR    @15,LISTCNT                                         0417 00888000
         SLA   @15,3                                               0417 00889000
         L     @09,DOLISTPT                                        0417 00890000
         AL    @09,@CF03528                                        0417 00891000
         L     RUCB,LISTUCB-4(@15,@09)                             0417 00892000
*   ARVOL=UCBVOLI;                  /* COPY VOLID FROM UCB INTO LIST */ 00893000
         MVC   ARVOL(6,ACQPTR),UCBVOLI(RUCB)                       0418 00894000
*   CALL SELOPTNS;                  /* GO CALC OPTIONS NEEDED        */ 00895000
         BAL   @14,SELOPTNS                                        0419 00896000
*   ARFLG=AMBDISP;                  /* COPY OPTIONS FOR THIS AMB     */ 00897000
         MVC   ARFLG(1,ACQPTR),AMBDISP                             0420 00898000
*   IF ARDRES=ON THEN               /* IS WAIT OPTION SELECTED       */ 00899000
         TM    ARDRES(ACQPTR),B'01000000'                          0421 00900000
         BNO   @RF00421                                            0421 00901000
*     ARECB=ADDR(RELECB);           /* POINT TO ECB IF WAIT OPT      */ 00902000
*                                                                  0422 00903000
         LA    @15,RELECB(,ACQPTR)                                 0422 00904000
         ST    @15,ARECB(,ACQPTR)                                  0422 00905000
*   /*****************************************************************/ 00906000
*   /*                                                               */ 00907000
*   /* THE NEW ACQUIRE LIST MUST BE PUT ON THE FRONT OF THE CHAIN OF */ 00908000
*   /* LISTS, AND THE OLD CHAIN MUST BE CHAINED OFF THE THE NEW LIST */ 00909000
*   /*                                                               */ 00910000
*   /*****************************************************************/ 00911000
*                                                                  0423 00912000
*   IF FIRSTACQ=0 THEN              /* CHECK IF FIRST TIME ENTERED   */ 00913000
@RF00421 L     @15,FIRSTACQ                                        0423 00914000
         LTR   @15,@15                                             0423 00915000
         BNZ   @RF00423                                            0423 00916000
*     FIRSTACQ=ACQPTR;              /* SETUP FIRST ACQ LIST PTR      */ 00917000
         ST    ACQPTR,FIRSTACQ                                     0424 00918000
*   ELSE                            /* THIS IS NOT FORST, MUST CHAIN */ 00919000
*     ACQSAVE->ARNEDB=ACQPTR;       /* ADD LIST TO END OF CHAIN      */ 00920000
         B     @RC00423                                            0425 00921000
@RF00423 L     @15,ACQSAVE                                         0425 00922000
