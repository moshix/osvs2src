         AH    @11,COUNT(,@15)                                     0468 01066000
         STH   @11,COUNT(,@15)                                     0468 01067000
*                   LISTUCB(COUNT)=RUCB;/* GET UCB FRM TIOT ENTRY    */ 01068000
         SLA   @11,3                                               0469 01069000
         AL    @15,@CF03528                                        0469 01070000
         ST    RUCB,LISTUCB-4(@11,@15)                             0469 01071000
*                 END;              /* END ADD VIRTUAL UCB TO LIST   */ 01072000
*             END;                  /* END GET VALID UCB IN LST      */ 01073000
@RF00466 DS    0H                                                  0472 01074000
*           RTIOT=RTIOT+4;          /* INCR TO NEXT TIOT UCB         */ 01075000
@RF00459 LA    RTIOT,4(,RTIOT)                                     0472 01076000
*         END;                      /* END PROCESSING A TIOT ENTRY   */ 01077000
         LA    INDEX,1(,INDEX)                                     0473 01078000
@DE00457 CR    INDEX,LISTCNT                                       0473 01079000
         BNH   @DL00457                                            0473 01080000
*         IF DSABFCHN=0|            /* NO MORE DDNAMES       @Y30LSSM*/ 01081000
*             DSABFCHN->DSABTIOT->TIOEDDNM='        ' THEN/* NO    0474 01082000
*                                      CONCATENATION         @Y30LSSM*/ 01083000
         L     @15,DSABPTR                                         0474 01084000
         L     @15,DSABFCHN(,@15)                                  0474 01085000
         LTR   @15,@15                                             0474 01086000
         BZ    @RT00474                                            0474 01087000
         L     @15,DSABTIOT(,@15)                                  0474 01088000
         CLC   TIOEDDNM(8,@15),@CC03473                            0474 01089000
         BNE   @RF00474                                            0474 01090000
@RT00474 DS    0H                                                  0475 01091000
*           CONCEND=ON;             /* INDICATE STOP GETTING UCBS    */ 01092000
         OI    CONCEND,B'00010000'                                 0475 01093000
*         ELSE                      /* PROCESS               @Y30LSSM*/ 01094000
*           DO;                     /* ANOTHER DSAB          @Y30LSSM*/ 01095000
         B     @RC00474                                            0476 01096000
@RF00474 DS    0H                                                  0477 01097000
*             DSABPTR=DSABFCHN;     /* GET NEXT DSAB         @Y30LSSM*/ 01098000
         L     @15,DSABPTR                                         0477 01099000
