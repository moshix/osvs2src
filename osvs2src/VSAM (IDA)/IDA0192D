*     ARBDUBD=ON;                   /* INDICATE BIND OPTION          */ 01422000
         LA    @15,CURRENT                                         0578 01423000
         OI    ARBDUBD(@15),B'10000000'                            0578 01424000
*   RETURN;                         /* RETURN TO CALLER              */ 01425000
@EL00008 DS    0H                                                  0579 01426000
@EF00008 DS    0H                                                  0579 01427000
@ER00008 BR    @14                                                 0579 01428000
*   RESPECIFY                                                      0580 01429000
