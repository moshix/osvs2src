         TITLE 'IDA0192D - ACQUIRE AND RELINQUISH EXTENTS              *00001000
                        '                                               00002000
IDA0192D CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192D  77.031'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*   PARMPTR=R1;                     /* INIT PARM LIST POINTER        */ 00014000
         LR    PARMPTR,R1                                          0161 00015000
*   R1=R13+36;                      /* POINT TO REG4 IN SAVE LIST    */ 00016000
         LA    R1,36(,R13)                                         0162 00017000
*   GEN(LM R4,R6,36(R13));          /* SAVE REGISTERS                */ 00018000
         LM R4,R6,36(R13)                                               00019000
*   R9=WKPOOL;                      /* LOAD SUBPOOL NUMBER           */ 00020000
         LA    R9,252                                              0164 00021000
*   GEN(L R7,@SIZDATD)              /* GET SIZE OF TEMP AREA         */ 00022000
*   SETS(R7);                                                      0165 00023000
         L R7,@SIZDATD                                                  00024000
*   GEN(GETMAIN EC,LV=(R7),A=(R13),SP=(R9),MF=(E,(1)))             0166 00025000
*   SETS(RETCODE);                                                 0166 00026000
         GETMAIN EC,LV=(R7),A=(R13),SP=(R9),MF=(E,(1))                  00027000
*   GEN(STM R4,R6,36(R13));         /* RESTORE SAVE AREA             */ 00028000
         STM R4,R6,36(R13)                                              00029000
*   IF RETCODE^=0 THEN              /* IF GETMAIN UNSUCCESSFUL       */ 00030000
         LTR   RETCODE,RETCODE                                     0168 00031000
         BZ    @RF00168                                            0168 00032000
*     DO;                           /* BAD GETMAIN CODE              */ 00033000
*       RETCODE=GMFAIL;             /* LOAD RETURN CODE              */ 00034000
         SLR   RETCODE,RETCODE                                     0170 00035000
         ICM   RETCODE,3,GMFAIL                                    0170 00036000
*       GEN(BR RET);                /* RETURN TO CALLER WITH ERROR   */ 00037000
         BR RET                                                         00038000
*     END;                          /* END BAD GETMAIN               */ 00039000
*   ELSE                            /* IF GETMAIN SUCCESSFUL         */ 00040000
*     DO;                           /* GETMAIN SUCCESSFUL            */ 00041000
         B     @RC00168                                            0173 00042000
@RF00168 DS    0H                                                  0174 00043000
*       GEN(L RETCODE,0(R13));      /* GET POINTER TO GOTTEN CORE    */ 00044000
         L RETCODE,0(R13)                                               00045000
*       R6=RETCODE;                 /* GET POINTER TO CORE FOR MVCL  */ 00046000
         LR    R6,RETCODE                                          0175 00047000
*       R9=0;                       /* SET COUNT TO ZERO CORE        */ 00048000
         SLR   R9,R9                                               0176 00049000
*       GEN(MVCL  R6,R8);           /* CLEAR GOTTEN CORE             */ 00050000
         MVCL  R6,R8                                                    00051000
*       GEN(ST R13,4(RETCODE));     /* SAVE SAVE AREA ADDRESS        */ 00052000
         ST R13,4(RETCODE)                                              00053000
*       R13=RETCODE;                /* LOAD PTR TO AUTOMATIC CORE    */ 00054000
         LR    R13,RETCODE                                         0179 00055000
*       GEN(USING @DATD,R13);       /* GET ADDRESSING                */ 00056000
         USING @DATD,R13                                                00057000
*       RESPECIFY                                                  0181 00058000
*         R13 RESTRICTED;           /* RESTRICT REG 13               */ 00059000
*     END;                          /* END GOOD GETMAIN              */ 00060000
*   RESPECIFY                                                      0183 00061000
*     RETCODE UNRESTRICTED;         /* FREE UP RETURN CODE REG       */ 00062000
*                                                                  0183 00063000
@RC00168 DS    0H                                                  0184 00064000
*   /*****************************************************************/ 00065000
*   /*                                                               */ 00066000
*   /* ALL POINTERS AND COUNTS ARE INITIALIZED HERE                  */ 00067000
*   /*                                                               */ 00068000
*   /*****************************************************************/ 00069000
*                                                                  0184 00070000
*   RESPECIFY                                                      0184 00071000
*     IDAPDPRM BASED(ADDR(ERRORWD));/* ERROR CODE BYTES              */ 00072000
*   PDPFUNC=FUNC40;                 /* INDICATE INITIALIZATION       */ 00073000
         LA    @15,ERRORWD                                         0185 00074000
         MVI   PDPFUNC(@15),X'28'                                  0185 00075000
*   RESPECIFY                                                      0186 00076000
*     RET RESTRICTED;               /* RESTRICT RETURN REG           */ 00077000
*   RESPECIFY                                                      0187 00078000
*     IDAVIRT BASED(PARMPTR);       /* PARMLIST BASE ON PARMPTR RG   */ 00079000
*   RESPECIFY                                                      0188 00080000
*     IFGACB BASED(VIRTACB);        /* USE PARM LIST FOR ACB PTR     */ 00081000
*   RESPECIFY                                                      0189 00082000
*     IDAAMDSB BASED(AMBDSB);       /* USE AMB POINTER TO AMDSB      */ 00083000
*   RESPECIFY                                                      0190 00084000
*     DEBBASIC BASED(AMBDEBPT);     /* USE AMB PTR TO DEB            */ 00085000
*   RESPECIFY                                                      0191 00086000
*     DEBDASD BASED(EXTENTS);       /* POINTER TO EACH EXTENT        */ 00087000
*   RESPECIFY                                                      0192 00088000
*     IDAAMBL BASED(ACBAMBL);       /* USE ACB POINTER TO AMBL       */ 00089000
*   RESPECIFY                                                      0193 00090000
*     IDAAMB BASED(RAMB);           /* AMB REG POINTER               */ 00091000
*                                                                  0193 00092000
*   /*****************************************************************/ 00093000
*   /*                                                               */ 00094000
*   /* IF THE CALLER DESIRES ALL OF THE EXTENTS ASSOCIATED WITH AN   */ 00095000
*   /* ACB TO BE PROCESSED, A COUNT OF ALL OF THE VIRTUAL UCBS MUST  */ 00096000
*   /* BE OBTAINED. A LIST OF THERE UCBS INDEXED BY UCBCOUNT IS THEN */ 00097000
*   /* BUILT                                                         */ 00098000
*   /*                                                               */ 00099000
*   /*****************************************************************/ 00100000
*                                                                  0194 00101000
*   IF VIRTBACB=ON THEN             /* CHECK IF ACB OPTION IS REQ    */ 00102000
         TM    VIRTBACB(PARMPTR),B'00010000'                       0194 00103000
         BNO   @RF00194                                            0194 00104000
*     DO;                           /* GET COUNT OF APPLICABLE UCBS  */ 00105000
*       RAMB=AMBLDTA;               /* GET THE DATA AMB      @Y30LSPS*/ 00106000
         L     @15,VIRTACB(,PARMPTR)                               0196 00107000
         L     @15,ACBAMBL(,@15)                                   0196 00108000
         L     RAMB,AMBLDTA(,@15)                                  0196 00109000
*       IF AMBCAT=OFF&              /* CHECK IF CATLG        @Y30LSPS*/ 00110000
*           AMBSCRA=OFF THEN        /* CHECK IF CRA          @Y30LSSM*/ 00111000
         TM    AMBCAT(RAMB),B'00011000'                            0197 00112000
         BNZ   @RF00197                                            0197 00113000
*         DO;                       /* SEARCH DSABS IF NOT CATLG     */ 00114000
*           RESPECIFY                                              0199 00115000
*             RTIOT RESTRICTED;     /* USE TIOT IN THIS ROUTINE      */ 00116000
*           RTIOT=DEBAPPAD->DEBXTNP;/* DEB EXTENTION         @Y30LSPS*/ 00117000
         L     @15,AMBDEBPT(,RAMB)                                 0200 00118000
         L     @15,DEBAPPAD(,@15)                                  0200 00119000
         L     RTIOT,DEBXTNP(,@15)                                 0200 00120000
*           DSABPTR=RTIOT->DEBXDSAB;/* FIRST DSAB PTR        @Y30LSPS*/ 00121000
         L     @15,DEBXDSAB(,RTIOT)                                0201 00122000
         ST    @15,DSABPTR                                         0201 00123000
*           FSTDSAB=DSABPTR;        /* SAVE FIRST DSAB       @Y30LSPS*/ 00124000
         ST    @15,FSTDSAB                                         0202 00125000
*           RTIOT=DSABTIOT;         /* FIRST TIOT ENTRY      @Y30LSPS*/ 00126000
         L     RTIOT,DSABTIOT(,@15)                                0203 00127000
*           LISTCNT=0;              /* INIT LIST COUNT       @Y30LSPS*/ 00128000
         SLR   LISTCNT,LISTCNT                                     0204 00129000
*           DO WHILE CONCEND=OFF;   /* COUNT ALL UCBS IN ALL ENTRYS  */ 00130000
         B     @DE00205                                            0205 00131000
@DL00205 DS    0H                                                  0206 00132000
*             LISTCNT=LISTCNT+(TIOELNGH-16)/4;/* CALC UCB ENTRY    0206 00133000
*                                      ASSOCIATED WITH AMDSBS WITH 0206 00134000
*                                      STAGING OPTION SPECIFIED      */ 00135000
         SLR   @00,@00                                             0206 00136000
         IC    @00,TIOELNGH(,RTIOT)                                0206 00137000
         SL    @00,@CF00078                                        0206 00138000
         SRDA  @00,32                                              0206 00139000
         D     @00,@CF00066                                        0206 00140000
         ALR   LISTCNT,@01                                         0206 00141000
*             IF DSABFCHN=0|        /* NO MORE DDNAMES       @Y30LSPS*/ 00142000
*                 DSABFCHN->DSABTIOT->TIOEDDNM='        ' THEN/* NO     00143000
*                                      CONCATENATION         @Y30LSSM*/ 00144000
         L     @15,DSABPTR                                         0207 00145000
         L     @15,DSABFCHN(,@15)                                  0207 00146000
         LTR   @15,@15                                             0207 00147000
         BZ    @RT00207                                            0207 00148000
         L     @15,DSABTIOT(,@15)                                  0207 00149000
         CLC   TIOEDDNM(8,@15),@CC03473                            0207 00150000
         BNE   @RF00207                                            0207 00151000
@RT00207 DS    0H                                                  0208 00152000
*               CONCEND=ON;         /* INDICATE END OF TIOE SCAN     */ 00153000
         OI    CONCEND,B'00010000'                                 0208 00154000
*             ELSE                  /* PROCESS               @Y30LSPS*/ 00155000
*               DO;                 /* ANOTHER DSAB          @Y30LSPS*/ 00156000
         B     @RC00207                                            0209 00157000
@RF00207 DS    0H                                                  0210 00158000
*                 DSABPTR=DSABFCHN; /* GET NEXT DSAB         @Y30LSPS*/ 00159000
         L     @15,DSABPTR                                         0210 00160000
         L     @15,DSABFCHN(,@15)                                  0210 00161000
         ST    @15,DSABPTR                                         0210 00162000
*                 RTIOT=DSABTIOT;   /* NEXT TIOT ENTRY       @Y30LSPS*/ 00163000
         L     RTIOT,DSABTIOT(,@15)                                0211 00164000
*               END;                /* END GETTING MORE TIOTS      0212 00165000
*                                                            @Y30LSPS*/ 00166000
*           END;                    /* END SCAN TIOT ENTRY FOR CNT   */ 00167000
@RC00207 DS    0H                                                  0213 00168000
@DE00205 TM    CONCEND,B'00010000'                                 0213 00169000
         BZ    @DL00205                                            0213 00170000
*         END;                      /* END SEARCH DSAB CHAIN @Y30LSPS*/ 00171000
*                                                                  0214 00172000
*       /*************************************************************/ 00173000
*       /*                                                           */ 00174000
*       /* IF THE DATA SET IS A CATALOG, OR CATALOG RECOVERY AREA THE*/ 00175000
*       /* DSAB CHAIN MAY NOT BE VALID. THEREFOR, THE CALLER PASSED A*/ 00176000
*       /* POINTER TO THE TIOT ENTRY IN VIRTTIOE. THIS IS THE ONLY   */ 00177000
*       /* TIOT ENTRY, AS CONCATENATED CATALOGS AS ONE DATA SET ARE  */ 00178000
*       /* NOT ALLOWED. ALSO, A CATALOG CAN NEVER SPAN MORE THAN ONE */ 00179000
*       /* VOLUME, SO ONLY ONE UCB NEEDS TO BE PROCESSED. IF A       */ 00180000
*       /* CATALOG IS BEING PROCESSED, THIS CODE WILL CONVERT THE    */ 00181000
*       /* PARAMETER LIST INTO A SPECIAL CASE OF THE BYUCB PARAMETER */ 00182000
*       /* LIST.                                             @Y30LSPS*/ 00183000
*       /*                                                           */ 00184000
*       /*************************************************************/ 00185000
*                                                                  0215 00186000
*       ELSE                        /* AMBCAT IS ON AND THIS IS A CAT*/ 00187000
*         DO;                       /* CONVERT THE PARAMETER LIST    */ 00188000
         B     @RC00197                                            0215 00189000
@RF00197 DS    0H                                                  0216 00190000
*           VIRTBUCB=ON;            /* INDICATE UCB PROVIDED @Y30LSPS*/ 00191000
         OI    VIRTBUCB(PARMPTR),B'00001000'                       0216 00192000
*           RTIOT=VIRTTIOE;         /* GET PTR TO TIOT ENTRY @Y30LSPS*/ 00193000
         L     RTIOT,VIRTTIOE(,PARMPTR)                            0217 00194000
*           VIRTUCB=TIOEFSRT;       /* PUT UCB PTR IN PARM LIST      */ 00195000
         L     @15,TIOEFSRT-1(,RTIOT)                              0218 00196000
         LA    @15,0(,@15)                                         0218 00197000
         ST    @15,VIRTUCB(,PARMPTR)                               0218 00198000
*           LISTCNT=1;              /* COUT OF ONE UCB       @Y30LSPS*/ 00199000
         LA    LISTCNT,1                                           0219 00200000
*         END;                      /* END IF CATALOG        @Y30LSPS*/ 00201000
*       RESPECIFY                                                  0221 00202000
*         RTIOT UNRESTRICTED;       /* FREEUP TIOT PTR REG   @Y30LSPS*/ 00203000
@RC00197 DS    0H                                                  0222 00204000
*       CALL BLDUCBL;               /* GETMAIN A LIST FOR THE UCBS 0222 00205000
*                                      AND PLACE ALL APPLICABLE UCBS    00206000
*                                      INTO THE LIST                 */ 00207000
         BAL   @14,BLDUCBL                                         0222 00208000
*     END;                          /* END SPECIAL ACB CLUSTER PROCE */ 00209000
*                                                                  0223 00210000
*   /*****************************************************************/ 00211000
*   /*                                                               */ 00212000
*   /* IF EOV IS CALLING TO RELINQUISH ALL EXTENTS ON ONE DRIVE PRIOR*/ 00213000
*   /* TO DISMOUNTING THE VOLUME, ONLY ONE UCB WILL BE PRESENT       */ 00214000
*   /*                                                               */ 00215000
*   /*****************************************************************/ 00216000
*                                                                  0224 00217000
*   ELSE                            /* ONLY A ONE ENTRY UCB LIST NED */ 00218000
*     DO;                           /* SETUP TO BUILD ONLY ONE UCB   */ 00219000
         B     @RC00194                                            0224 00220000
@RF00194 DS    0H                                                  0225 00221000
*       LISTCNT=1;                  /* COUNT OF ONE FOR UCBS         */ 00222000
         LA    LISTCNT,1                                           0225 00223000
*       CALL BLDUCBL;               /* BUILD A UCB LIST FOR ONE UCB  */ 00224000
         BAL   @14,BLDUCBL                                         0226 00225000
*     END;                          /* END SPECIAL SINGLE UCB PROC   */ 00226000
*                                                                  0227 00227000
*   /*****************************************************************/ 00228000
*   /*                                                               */ 00229000
*   /* AT THIS POINT THE BYACB CASE AND THE ONEUCB CASES ARE TREATED */ 00230000
*   /* IN THE SAME MANNER. FOR ANY OTHER CASES, THE UCBCOUNT IS STILL*/ 00231000
*   /* ZERO                                                          */ 00232000
*   /*                                                               */ 00233000
*   /*****************************************************************/ 00234000
*                                                                  0228 00235000
*   DO WHILE LISTCNT^=0;            /* PROCESS ALL UCBS IN THE LIST  */ 00236000
@RC00194 B     @DE00228                                            0228 00237000
@DL00228 DS    0H                                                  0229 00238000
*     RUCB=LISTUCB(LISTCNT);        /* GET A UCB FORM THE LIST       */ 00239000
         LR    @15,LISTCNT                                         0229 00240000
         SLA   @15,3                                               0229 00241000
         L     @07,DOLISTPT                                        0229 00242000
         AL    @07,@CF03528                                        0229 00243000
         L     RUCB,LISTUCB-4(@15,@07)                             0229 00244000
*     CALL BLDACQL;                 /* BUILD ONE OR MORE 16 EXTENT 0230 00245000
*                                      ACQUIRE OR RELINQUISH LISTS 0230 00246000
*                                      FOR ALL OF THE EXTENTS ON THE    00247000
*                                      VOLUME SPECIFIED BY UCBPTR    */ 00248000
         BAL   @14,BLDACQL                                         0230 00249000
*     LISTCNT=LISTCNT-1;            /* DECREMENT THE UCB COUNT       */ 00250000
         BCTR  LISTCNT,0                                           0231 00251000
*   END;                            /* ALL ACQUIRE RELINQUISH LISTS     00252000
*                                      HAVE NOW BEEN BUILT           */ 00253000
*                                                                  0232 00254000
@DE00228 SLR   @15,@15                                             0232 00255000
         CR    LISTCNT,@15                                         0232 00256000
         BNE   @DL00228                                            0232 00257000
*   /*****************************************************************/ 00258000
*   /*                                                               */ 00259000
*   /* IF ANY VIRTUAL UCBS WERE FOUND, A LIST WILL HAVE BEEN BUILT BY*/ 00260000
*   /* BLDACQL. THIS LIST, OR CHAIN OF LISTS IF MORE THAN 16 EXTENTS */ 00261000
*   /* WERE ASSOCIATED WITH ANY ONE UCB, IS IN THE PROPER FORM TO    */ 00262000
*   /* ISSUE AN ACQUIRE OF RELINQUISH REQUEST AGAINST                */ 00263000
*   /*                                                               */ 00264000
*   /*****************************************************************/ 00265000
*                                                                  0233 00266000
*   LISTCNT=COUNT;                  /* RESTORE COUNT OF VIRTUAL UCBS    00267000
*                                      VIRTUAL UCBS                  */ 00268000
         L     @11,DOLISTPT                                        0233 00269000
         LH    LISTCNT,COUNT(,@11)                                 0233 00270000
*   ACQPTR=FIRSTACQ;                /* GET THE FIRST ACQUIRE LIST    */ 00271000
         L     ACQPTR,FIRSTACQ                                     0234 00272000
*   IF ACQPTR^=0 THEN               /* CHECK IF ANY ENTRIES          */ 00273000
         CR    ACQPTR,@15                                          0235 00274000
         BE    @RF00235                                            0235 00275000
*     DO;                           /* STAGE ENTRIES IF NOT ZERO     */ 00276000
*       PREVACQ=ADDR(FIRSTACQ)-(ADDR(ARNEDB)-ADDR(ACQINIT));       0237 00277000
         LA    @15,FIRSTACQ                                        0237 00278000
         SL    @15,@CF03227                                        0237 00279000
         ST    @15,PREVACQ                                         0237 00280000
*       DO WHILE ACQPTR^=0;         /* CHECK ALL ENTRIES FOR NULL    */ 00281000
         B     @DE00238                                            0238 00282000
@DL00238 DS    0H                                                  0239 00283000
*         IF ARNMEXT=0 THEN         /* CHECK IF COUNT NULL           */ 00284000
         CLI   ARNMEXT(ACQPTR),0                                   0239 00285000
         BNE   @RF00239                                            0239 00286000
*           PREVACQ->ARNEDB=ARNEDB; /* UNCHAIN NULL LIST             */ 00287000
         L     @15,ARNEDB(,ACQPTR)                                 0240 00288000
         L     @11,PREVACQ                                         0240 00289000
         ST    @15,ARNEDB(,@11)                                    0240 00290000
*         ELSE                                                     0241 00291000
*           PREVACQ=ACQPTR;         /* MAKE THIS PREVIOUS IF NOT NUL */ 00292000
         B     @RC00239                                            0241 00293000
@RF00239 ST    ACQPTR,PREVACQ                                      0241 00294000
*         ACQPTR=ARNEDB;            /* GET NEXT LIST OR ZERO         */ 00295000
@RC00239 L     ACQPTR,ARNEDB(,ACQPTR)                              0242 00296000
*       END;                        /* END CHECK FOR NULL LISTS      */ 00297000
@DE00238 SLR   @15,@15                                             0243 00298000
         CR    ACQPTR,@15                                          0243 00299000
         BNE   @DL00238                                            0243 00300000
*       ACQPTR=FIRSTACQ;            /* POINT TO FIRST AGAIN          */ 00301000
         L     ACQPTR,FIRSTACQ                                     0244 00302000
*       IF ACQPTR^=0 THEN           /* CHECK IF ALL LISTS NULL       */ 00303000
         CR    ACQPTR,@15                                          0245 00304000
         BE    @RF00245                                            0245 00305000
*         CALL STAGIT;              /* GO STAGE ALL ENTRYS IN LISTS  */ 00306000
         BAL   @14,STAGIT                                          0246 00307000
*     END;                          /* END IF ANY LISTS CREATED      */ 00308000
@RF00245 DS    0H                                                  0248 00309000
*EXIT:                              /* ENTRY CAN BE TO HERE ON     0248 00310000
*                                      TERMINATION, NORMAL OR ERROR  */ 00311000
*                                                                  0248 00312000
*   /*****************************************************************/ 00313000
*   /*                                                               */ 00314000
*   /* ANY CORE GOTTEN MUST NOW BE FREED, AND THE CORRECT RETURN     */ 00315000
*   /* INFORMATION MUST BE PASSED TO THE CALLER                      */ 00316000
*   /*                                                               */ 00317000
*   /*****************************************************************/ 00318000
*                                                                  0248 00319000
*   RESPECIFY                                                      0248 00320000
*     COREPTR RESTRICTED;           /* RESTRICT REG FOR THIS RTN     */ 00321000
@RF00235 DS    0H                                                  0248 00322000
EXIT     DS    0H                                                  0249 00323000
*   COREPTR=COREBASE;               /* GET BASE OF CORE CHAIN        */ 00324000
         L     COREPTR,COREBASE                                    0249 00325000
*   DO WHILE COREPTR^=0;            /* FREE ALL CORE ENTRYS          */ 00326000
         B     @DE00250                                            0250 00327000
@DL00250 DS    0H                                                  0251 00328000
*     R0=CORELEN;                   /* GET LENGTH FROM CORE HEADER   */ 00329000
         LH    R0,CORELEN(,COREPTR)                                0251 00330000
*     R1=COREPTR;                   /* POINT TO THIS PIECE OF CORE   */ 00331000
         LR    R1,COREPTR                                          0252 00332000
*     COREPTR=CORECHN;              /* GET NEXT PIECE OF CORE        */ 00333000
         L     COREPTR,CORECHN(,COREPTR)                           0253 00334000
*     GEN(FREEMAIN  R,LV=(0),A=(1));/* FREE THE GOTTEN CORE          */ 00335000
         FREEMAIN  R,LV=(0),A=(1)                                       00336000
*   END;                            /* END FREE GOTTEN CORE LOOP     */ 00337000
@DE00250 LTR   COREPTR,COREPTR                                     0255 00338000
         BNZ   @DL00250                                            0255 00339000
*   RESPECIFY                                                      0256 00340000
*     COREPTR UNRESTRICTED;         /* FREE UP CORE PTR REG          */ 00341000
*                                                                  0256 00342000
*   /*****************************************************************/ 00343000
*   /*                                                               */ 00344000
*   /* EXIT MODULE                                                   */ 00345000
*   /*                                                               */ 00346000
*   /*****************************************************************/ 00347000
*                                                                  0257 00348000
*   IF PDPCODE=0 THEN               /* IF NO ERRORS                  */ 00349000
         LA    @15,ERRORWD                                         0257 00350000
         CLI   PDPCODE(@15),0                                      0257 00351000
         BNE   @RF00257                                            0257 00352000
*     PDPFUNC=0;                    /* THEN ZERO FUNCTION CODE       */ 00353000
         MVI   PDPFUNC(@15),X'00'                                  0258 00354000
*   RESPECIFY                                                      0259 00355000
*    (R6,                                                          0259 00356000
*     R7) RESTRICTED;               /* RESTRICT REGS 6 & 7           */ 00357000
@RF00257 DS    0H                                                  0260 00358000
*   R6=ERRORWD;                     /* LOAD RETURN CODE              */ 00359000
         L     R6,ERRORWD                                          0260 00360000
*   SLL(R6,8);                      /* SET PROB DET INFO FOR CALLER  */ 00361000
         SLL   R6,8                                                0261 00362000
*   MAINPOOL=WKPOOL;                /* SET SUBPOOL FOR FREEMAIN      */ 00363000
         MVI   MAINPOOL,X'FC'                                      0262 00364000
*   GEN(L R7,@SIZDATD);             /* LOAD BLOCK SIZE               */ 00365000
         L R7,@SIZDATD                                                  00366000
*   MAINLEN=R7;                     /* SET LENGTH FOR FREEMAIN       */ 00367000
         STCM  R7,7,MAINLEN                                        0264 00368000
*   R1=R13;                         /* POINT TO BLOCK ADDR           */ 00369000
         LR    R1,R13                                              0265 00370000
*   R0=MAINR0;                      /* LOAD SUBPOOL/LENGTB           */ 00371000
         L     R0,MAINR0                                           0266 00372000
*   GEN(L R13,4(R13));              /* POINT TO LAST SAVEAREA        */ 00373000
         L R13,4(R13)                                                   00374000
*   GEN(FREEMAIN R,LV=(0),A=(1));   /* FREE AUTOMATICS               */ 00375000
         FREEMAIN R,LV=(0),A=(1)                                        00376000
*   RETCODE=R6;                     /* LOAD RETURN CODE REGISTER     */ 00377000
         LR    RETCODE,R6                                          0269 00378000
*   RESPECIFY                                                      0270 00379000
*    (R6,                                                          0270 00380000
*     R7) UNRESTRICTED;             /* UNRESTRICT REGS 6 & 7         */ 00381000
*   GEN(L RET,12(R13));             /* LOAD RETURN ADDRESS           */ 00382000
         L RET,12(R13)                                                  00383000
*   GEN(LM R0,R12,20(R13));         /* RESTORE REGISTERS 0-12        */ 00384000
         LM R0,R12,20(R13)                                              00385000
*   GEN(BR RET);                    /* RETURN TO CALLER              */ 00386000
         BR RET                                                         00387000
*   RESPECIFY                                                      0274 00388000
*    (R0,                                                          0274 00389000
*     R1) UNRESTRICTED;                                            0274 00390000
*BACKOUT:                           /* THIS IS AN ERROR RECOVERY   0275 00391000
*                                      ROUTINE THAT TRIES TO         */ 00392000
*                                                                  0275 00393000
*   /*****************************************************************/ 00394000
*   /*                                                               */ 00395000
*   /* BACK OUT ANY ACQUIRES ISSUED BY ISSUING THE COMPLEMENTARY     */ 00396000
*   /* RELINQUISHES                                                  */ 00397000
*   /*                                                               */ 00398000
*   /*****************************************************************/ 00399000
*                                                                  0275 00400000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0275 00401000
BACKOUT  DS    0H                                                  0276 00402000
*   ARNEDB=0;                       /* TERMINATE THE CHAIN AT ERROR  */ 00403000
         SLR   @15,@15                                             0276 00404000
         ST    @15,ARNEDB(,ACQPTR)                                 0276 00405000
*   ACQPTR=FIRSTACQ;                /* POINT TO FIRST LIST           */ 00406000
         L     ACQPTR,FIRSTACQ                                     0277 00407000
*   DO WHILE ACQPTR^=0;             /* CHANGE ALL LISTS TO REL       */ 00408000
         B     @DE00278                                            0278 00409000
@DL00278 DS    0H                                                  0279 00410000
*     AROP='02'X;                   /* MAKE OPERATION RELINQUISH     */ 00411000
         MVI   AROP(ACQPTR),X'02'                                  0279 00412000
*     ARDRES=OFF;                   /* DO NOT WAIT FOR RELINQUISH    */ 00413000
         NI    ARDRES(ACQPTR),B'10111111'                          0280 00414000
*     ACQPTR=ARNEDB;                /* GET THE NEXT LIST             */ 00415000
         L     ACQPTR,ARNEDB(,ACQPTR)                              0281 00416000
*   END;                            /* ENDCHANGE TO RELINQUISH       */ 00417000
@DE00278 LTR   ACQPTR,ACQPTR                                       0282 00418000
         BNZ   @DL00278                                            0282 00419000
*   ACQPTR=FIRSTACQ;                /* POINT TO START OF CHAIN       */ 00420000
         L     ACQPTR,FIRSTACQ                                     0283 00421000
*   GEN(ICBACREL  MF=(E,(ACQPTR)),TYPE=REL);/* RELINQUISH IT         */ 00422000
*                                                                  0284 00423000
         ICBACREL  MF=(E,(ACQPTR)),TYPE=REL                             00424000
*   /*****************************************************************/ 00425000
*   /*                                                               */ 00426000
*   /* NO ERROR CHECKING IS DONE AS THIS IS A PURE BACKOUT WHICH WILL*/ 00427000
*   /* CONTINUE BLINDLY ALONG UNTIL THE EXTENTS HAVE ALL BEEN TRIED  */ 00428000
*   /*                                                               */ 00429000
*   /*****************************************************************/ 00430000
*                                                                  0285 00431000
*   END BACKOUT;                    /* END OF ERROR BACKOUT MODULE   */ 00432000
@EL00002 DS    0H                                                  0285 00433000
@EF00002 DS    0H                                                  0285 00434000
@ER00002 BR    @14                                                 0285 00435000
*BLDACQL:                           /* BUILD ONE OR MORE ACQUIRE   0286 00436000
*                                      RELINQUISH LISTS              */ 00437000
*                                                                  0286 00438000
*   /*****************************************************************/ 00439000
*   /*                                                               */ 00440000
*   /* THIS ROUTINE CALLS GETLIST TO GET ANY EMPTY ACQUIRE OR        */ 00441000
*   /* RELINQUISH LIST AND CHAIN IT. GETAMB IS THEN CALLED TO GET THE*/ 00442000
*   /* NEXT AMB IN THE CLUSTER. THE EXTENTS OF THE DEB ASSOCIATED    */ 00443000
*   /* WITH THE AMB ARE SEARCHED, AND ANY EXTENTS ON THE DEVICE      */ 00444000
*   /* CURRENTLY BEING PROCESSED ARE MOVED INTO THE ACQUIRE LIST.    */ 00445000
*   /*                                                               */ 00446000
*   /*****************************************************************/ 00447000
*                                                                  0286 00448000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0286 00449000
BLDACQL  DS    0H                                                  0287 00450000
*   RESPECIFY                                                      0287 00451000
*    (EXTENTS,                                                     0287 00452000
*     RXTNT,                                                       0287 00453000
*     INDEX) RESTRICTED;            /* NEEDED REGS HERE              */ 00454000
*   BLDASAVE=RET;                   /* SAVE RETURN REG               */ 00455000
         ST    RET,BLDASAVE                                        0288 00456000
*   PDPFUNC=FUNC42;                 /* INDICATE BUILD ACQUIRE LIST   */ 00457000
         LA    @15,ERRORWD                                         0289 00458000
         MVI   PDPFUNC(@15),X'2A'                                  0289 00459000
*   LASTAMB=OFF;                    /* SET TO CHECK ALL AMBS         */ 00460000
*   FIRST=OFF;                      /* SET UP FOR FIRST AGAIN        */ 00461000
         NI    LASTAMB,B'11011011'                                 0291 00462000
*   CALL GETAMB;                    /* GET THE FIRST AMB             */ 00463000
         BAL   @14,GETAMB                                          0292 00464000
*   CALL GETLIST;                   /* GET A 16 ENTRY ACQUIRE LIST   */ 00465000
         BAL   @14,GETLIST                                         0293 00466000
*   DO WHILE RAMB^=0;               /* PROCESS ALL AMBS              */ 00467000
         B     @DE00294                                            0294 00468000
@DL00294 DS    0H                                                  0295 00469000
*     ACQPTR=ACQBASE(LISTCNT);      /* POINT TO FIRST ENTRY          */ 00470000
         LR    @15,LISTCNT                                         0295 00471000
         SLA   @15,3                                               0295 00472000
         L     @11,DOLISTPT                                        0295 00473000
         L     ACQPTR,ACQBASE-8(@15,@11)                           0295 00474000
*     FIND=OFF;                     /* INIT NOT FOUND INDICATION     */ 00475000
         NI    FIND,B'10111111'                                    0296 00476000
*     EXTENTS=AMBDEBAD+LENGTH(DEBBASIC)-LENGTH(DEBBASND);          0297 00477000
         L     EXTENTS,AMBDEBAD-1(,RAMB)                           0297 00478000
         LA    EXTENTS,0(,EXTENTS)                                 0297 00479000
         SL    EXTENTS,@CF03521                                    0297 00480000
*     IF AMDFAULT=OFF THEN          /* PROCESS IS STAGE REQUESTED    */ 00481000
         L     @15,AMBDSB(,RAMB)                                   0298 00482000
         TM    AMDFAULT(@15),B'01000000'                           0298 00483000
         BNZ   @RF00298                                            0298 00484000
*       DO;                         /* STAGING SECTION               */ 00485000
*         DO WHILE FIND=OFF;        /* SEARCH FOR CORRECT LIST       */ 00486000
         B     @DE00300                                            0300 00487000
@DL00300 DS    0H                                                  0301 00488000
*           IF ARFLG^=AMBDISP|      /* CHECK IS ACQ LIST AND AMB     */ 00489000
*               ACQFULL=ON THEN     /* CHECK IF ENTRY IS FULL THE  0301 00490000
*                                      NEXT LIST MUST BE GOTTEN      */ 00491000
         CLC   ARFLG(1,ACQPTR),AMBDISP                             0301 00492000
         BNE   @RT00301                                            0301 00493000
         TM    ACQFULL(ACQPTR),B'10000000'                         0301 00494000
         BNO   @RF00301                                            0301 00495000
@RT00301 DS    0H                                                  0302 00496000
*             IF ARNEDB=0 THEN      /* CHECK IF ANOTHER LIST         */ 00497000
         L     @15,ARNEDB(,ACQPTR)                                 0302 00498000
         LTR   @15,@15                                             0302 00499000
         BNZ   @RF00302                                            0302 00500000
*               CALL GETLIST;       /* THEN GET AND CHAIN A NEW LIST */ 00501000
         BAL   @14,GETLIST                                         0303 00502000
*             ELSE                                                 0304 00503000
*               ACQPTR=ARNEDB;      /* GET NEXT LIST IN CHAIN        */ 00504000
         B     @RC00302                                            0304 00505000
@RF00302 L     ACQPTR,ARNEDB(,ACQPTR)                              0304 00506000
*           ELSE                                                   0305 00507000
*             FIND=ON;              /* INDICATE CORRECT LIST FOUND   */ 00508000
         B     @RC00301                                            0305 00509000
@RF00301 OI    FIND,B'01000000'                                    0305 00510000
*         END;                      /* END SEARCH LIST FOR OK LIST   */ 00511000
*                                                                  0306 00512000
@RC00301 DS    0H                                                  0306 00513000
@DE00300 TM    FIND,B'01000000'                                    0306 00514000
         BZ    @DL00300                                            0306 00515000
*         /***********************************************************/ 00516000
*         /*                                                         */ 00517000
*         /* IF THIS IS THE SPECIAL ONE EXTENT CASE FROM EOV, THEN   */ 00518000
*         /* THE START AND END CC MUST BE MOVED FROM THE EXTENT LIST */ 00519000
*         /*                                                         */ 00520000
*         /***********************************************************/ 00521000
*                                                                  0307 00522000
*         IF VIRTBEXT=ON THEN       /* CHECK IS SPECIAL ONE EXTENT   */ 00523000
         TM    VIRTBEXT(PARMPTR),B'00000100'                       0307 00524000
         BNO   @RF00307                                            0307 00525000
*           DO;                     /* PROCESS THE EXTENT LIST       */ 00526000
*             RXTNT=VIRTEXT;        /* POINT TO EXTENT               */ 00527000
         L     RXTNT,VIRTEXT(,PARMPTR)                             0309 00528000
*             DO WHILE RXTNT<(VIRTEXT+VIRTLENG);/* PROCESS ALL       */ 00529000
         B     @DE00310                                            0310 00530000
@DL00310 DS    0H                                                  0311 00531000
*               NEXTEXT=RXTNT+XTLNG;/* POINT TO NEXT EXTENT GO       */ 00532000
         LR    @15,RXTNT                                           0311 00533000
         SLR   @11,@11                                             0311 00534000
         ICM   @11,3,XTLNG(RXTNT)                                  0311 00535000
         ALR   @15,@11                                             0311 00536000
         ST    @15,NEXTEXT                                         0311 00537000
*               DO WHILE RXTNT<NEXTEXT;/* PROCESS THIS GROUP         */ 00538000
         B     @DE00312                                            0312 00539000
@DL00312 DS    0H                                                  0313 00540000
*                 DONTUSE=OFF;      /* INIT DONTUSE OFF              */ 00541000
         NI    DONTUSE,B'01111111'                                 0313 00542000
*                 EXTENTS=AMBDEBAD+LENGTH(DEBBASIC)-LENGTH(DEBBASND);   00543000
         L     EXTENTS,AMBDEBAD-1(,RAMB)                           0314 00544000
         LA    EXTENTS,0(,EXTENTS)                                 0314 00545000
         SL    EXTENTS,@CF03521                                    0314 00546000
*                 IF VIRTMNT=OFF THEN/* IF NOT NEWLY MOUNTED         */ 00547000
         TM    VIRTMNT(PARMPTR),B'00000010'                        0315 00548000
         BNZ   @RF00315                                            0315 00549000
*                   DO INDEX=1 TO DEBNMEXT;/* CHECK DEB EXTENTS      */ 00550000
         LA    INDEX,1                                             0316 00551000
         B     @DE00316                                            0316 00552000
@DL00316 DS    0H                                                  0317 00553000
*                     IF DEBUCBA=VIRTUCB THEN/* CHECK THIS UCB EX    */ 00554000
         L     @15,DEBUCBA-1(,EXTENTS)                             0317 00555000
         LA    @15,0(,@15)                                         0317 00556000
         C     @15,VIRTUCB(,PARMPTR)                               0317 00557000
         BNE   @RF00317                                            0317 00558000
*                       DO;         /* ANY EXTENTS DUPLICATE         */ 00559000
*                         IF DEBSTRCC=STRCC THEN/* CHECK IF DUP      */ 00560000
         CLC   DEBSTRCC(2,EXTENTS),STRCC(RXTNT)                    0319 00561000
         BNE   @RF00319                                            0319 00562000
*                           DONTUSE=ON;/* INDICATE EXTENT IS DUP     */ 00563000
         OI    DONTUSE,B'10000000'                                 0320 00564000
*                       END;        /* END CHECKING THIS EXTENT      */ 00565000
@RF00319 DS    0H                                                  0322 00566000
*                     EXTENTS=EXTENTS+LENGTH(DEBDASD);/* GET NX      */ 00567000
@RF00317 LA    EXTENTS,16(,EXTENTS)                                0322 00568000
*                   END;            /* END CHECK IS DUPLICATE EXTENT */ 00569000
         LA    INDEX,1(,INDEX)                                     0323 00570000
@DE00316 L     @15,AMBDEBPT(,RAMB)                                 0323 00571000
         SLR   @11,@11                                             0323 00572000
         IC    @11,DEBNMEXT(,@15)                                  0323 00573000
         CR    INDEX,@11                                           0323 00574000
         BNH   @DL00316                                            0323 00575000
*                 ELSE                                             0324 00576000
*                   DONTUSE=OFF;    /* IF NEW MOUNT THEN MUST BE OK  */ 00577000
         B     @RC00315                                            0324 00578000
@RF00315 NI    DONTUSE,B'01111111'                                 0324 00579000
*                 IF DONTUSE=OFF THEN/* COPY EXTENTS IF GOOD         */ 00580000
@RC00315 TM    DONTUSE,B'10000000'                                 0325 00581000
         BNZ   @RF00325                                            0325 00582000
*                   DO;             /* COPY EXTENTS TO ACQUIRE LIST  */ 00583000
*                     IF ACQCC1(ARNMEXT)^=STRCC|/* DIFFERENT ST      */ 00584000
*                         ACQCC2(ARNMEXT)^=ENDCC THEN/* DIFFER END   */ 00585000
         SLR   @15,@15                                             0327 00586000
         IC    @15,ARNMEXT(,ACQPTR)                                0327 00587000
         SLA   @15,2                                               0327 00588000
         ST    @15,@TF00001                                        0327 00589000
         ALR   @15,ACQPTR                                          0327 00590000
         CLC   ACQCC1-4(2,@15),STRCC(RXTNT)                        0327 00591000
         BNE   @RT00327                                            0327 00592000
         LR    @15,ACQPTR                                          0327 00593000
         AL    @15,@TF00001                                        0327 00594000
         CLC   ACQCC2-4(2,@15),ENDCC(RXTNT)                        0327 00595000
         BE    @RF00327                                            0327 00596000
@RT00327 DS    0H                                                  0328 00597000
*                       DO;         /* IF UNIQUE THEN COPY           */ 00598000
*                         ARNMEXT=ARNMEXT+1;/* INCR EXTENT CNT       */ 00599000
         LA    @15,1                                               0329 00600000
         SLR   @11,@11                                             0329 00601000
         IC    @11,ARNMEXT(,ACQPTR)                                0329 00602000
         ALR   @15,@11                                             0329 00603000
         STC   @15,ARNMEXT(,ACQPTR)                                0329 00604000
*                         ACQCC1(ARNMEXT)=STRCC;/* GET START CC      */ 00605000
         SLA   @15,2                                               0330 00606000
         ST    @15,@TF00001                                        0330 00607000
         ALR   @15,ACQPTR                                          0330 00608000
         MVC   ACQCC1-4(2,@15),STRCC(RXTNT)                        0330 00609000
*                         ACQCC2(ARNMEXT)=ENDCC;/* GET END CC        */ 00610000
         LR    @15,ACQPTR                                          0331 00611000
         AL    @15,@TF00001                                        0331 00612000
         MVC   ACQCC2-4(2,@15),ENDCC(RXTNT)                        0331 00613000
*                         IF ARNMEXT=16 THEN/* ONLY 16 EXTENTS IN AN    00614000
*                                      ACQ LIST              @ZA16054*/ 00615000
         CLI   ARNMEXT(ACQPTR),16                                  0332 00616000
         BNE   @RF00332                                            0332 00617000
*                           DO;     /* SET LIST FULL AND GET NEW LIST   00618000
*                                                            @ZA16054*/ 00619000
*                             ACQFULL=ON;/* INDICATE THIS LIST IS  0334 00620000
*                                      FULL                  @ZA16054*/ 00621000
         OI    ACQFULL(ACQPTR),B'10000000'                         0334 00622000
*                             CALL GETLIST;/* GET NEW LIST@ZA16054   */ 00623000
         BAL   @14,GETLIST                                         0335 00624000
*                           END;    /* END IF LIST IS FULL   @ZA16054*/ 00625000
*                       END;        /* END COPY NEW CYL EXTENT       */ 00626000
@RF00332 DS    0H                                                  0338 00627000
*                   END;            /* END IF EXTENTS TO BE STAGED   */ 00628000
@RF00327 DS    0H                                                  0339 00629000
*                 RXTNT=RXTNT+LENGTH(EXTENT);/* INCR TO NEXT EX      */ 00630000
@RF00325 LA    RXTNT,20(,RXTNT)                                    0339 00631000
*               END;                /* END PROCESSING ONE EXTENT   0340 00632000
*                                      GROUP OCCURANCE               */ 00633000
*                                                                  0340 00634000
@DE00312 C     RXTNT,NEXTEXT                                       0340 00635000
         BL    @DL00312                                            0340 00636000
*               /*****************************************************/ 00637000
*               /*                                                   */ 00638000
*               /* THE POINTERS MUST NOW BE ADVANCED TO THE NEXT     */ 00639000
*               /* GROUP OCCURANCE SET OF EXTENT FIELDS RXTNT HAS    */ 00640000
*               /* BEEN INCREMENTED TO THE END OF A SET OF EXTENTS BY*/ 00641000
*               /* ONE EXTENT LENGTH AT A TIME. IT MUST NOW BE       */ 00642000
*               /* INCREMENTED PAST THE FIXED LENGTH HEADER AND COUNT*/ 00643000
*               /* FIELD OF THE NEXT SET OF EXTENTS                  */ 00644000
*               /*                                                   */ 00645000
*               /*****************************************************/ 00646000
*                                                                  0341 00647000
*               RXTNT=RXTNT+LENGTH(EXTFXD)+LENGTH(XTLNG);          0341 00648000
         LA    RXTNT,9(,RXTNT)                                     0341 00649000
*             END;                  /* END OF ALL EXTENTS AND GROUPS */ 00650000
@DE00310 LH    @15,VIRTLENG(,PARMPTR)                              0342 00651000
         AL    @15,VIRTEXT(,PARMPTR)                               0342 00652000
         CR    RXTNT,@15                                           0342 00653000
         BL    @DL00310                                            0342 00654000
*           END;                    /* END IF EXTENT LIST PASSED     */ 00655000
*         ELSE                      /* ALL EXTENTS OF DEB ARE CHECK  */ 00656000
*           DO;                     /* PROCEED WITH NORMAL CASE      */ 00657000
*                                                                  0344 00658000
         B     @RC00307                                            0344 00659000
@RF00307 DS    0H                                                  0345 00660000
*             /*******************************************************/ 00661000
*             /*                                                     */ 00662000
*             /* WHEN THE CORRECT ACQUIRE RELINQUISH LIST HAS BEEN   */ 00663000
*             /* FOUND, THE DEB ASSOCIARED WITH THE AMB MUST BE      */ 00664000
*             /* SEARCHED FOR ALL EXTENTS THAT ARE ON THE VOLUME IN  */ 00665000
*             /* QUESTION                                            */ 00666000
*             /*                                                     */ 00667000
*             /*******************************************************/ 00668000
*                                                                  0345 00669000
*             DO INDEX=1 TO DEBNMEXT;/* PROCESS ALL EXTENT OF DEB    */ 00670000
         LA    INDEX,1                                             0345 00671000
         B     @DE00345                                            0345 00672000
@DL00345 DS    0H                                                  0346 00673000
*               IF DEBUCBA=RUCB THEN/* CHECK IF THIS IS ON DEVICE    */ 00674000
         L     @15,DEBUCBA-1(,EXTENTS)                             0346 00675000
         LA    @15,0(,@15)                                         0346 00676000
         CR    RUCB,@15                                            0346 00677000
         BNE   @RF00346                                            0346 00678000
*                 DO;               /* COPY EXTENTS TO ACQUIRE LIST  */ 00679000
*                   IF ACQCC1(ARNMEXT)^=DEBSTRCC|/* START NEW CYL    */ 00680000
*                       ACQCC2(ARNMEXT)^=DEBENDCC THEN/* NEW END CYL */ 00681000
         SLR   @15,@15                                             0348 00682000
         IC    @15,ARNMEXT(,ACQPTR)                                0348 00683000
         SLA   @15,2                                               0348 00684000
         ST    @15,@TF00001                                        0348 00685000
         ALR   @15,ACQPTR                                          0348 00686000
         CLC   ACQCC1-4(2,@15),DEBSTRCC(EXTENTS)                   0348 00687000
         BNE   @RT00348                                            0348 00688000
         LR    @15,ACQPTR                                          0348 00689000
         AL    @15,@TF00001                                        0348 00690000
         CLC   ACQCC2-4(2,@15),DEBENDCC(EXTENTS)                   0348 00691000
         BE    @RF00348                                            0348 00692000
@RT00348 DS    0H                                                  0349 00693000
*                     DO;           /* COPY NEW CYL EXTENTS          */ 00694000
*                       ARNMEXT=ARNMEXT+1;/* INCR ACQUIRE EXT CNT    */ 00695000
         LA    @15,1                                               0350 00696000
         SLR   @11,@11                                             0350 00697000
         IC    @11,ARNMEXT(,ACQPTR)                                0350 00698000
         ALR   @15,@11                                             0350 00699000
         STC   @15,ARNMEXT(,ACQPTR)                                0350 00700000
*                       ACQCC1(ARNMEXT)=DEBSTRCC;/* COPY START CC ADD*/ 00701000
         SLA   @15,2                                               0351 00702000
         ST    @15,@TF00001                                        0351 00703000
         ALR   @15,ACQPTR                                          0351 00704000
         MVC   ACQCC1-4(2,@15),DEBSTRCC(EXTENTS)                   0351 00705000
*                       ACQCC2(ARNMEXT)=DEBENDCC;/* COPY END CC O EX */ 00706000
         LR    @15,ACQPTR                                          0352 00707000
         AL    @15,@TF00001                                        0352 00708000
         MVC   ACQCC2-4(2,@15),DEBENDCC(EXTENTS)                   0352 00709000
*                     END;          /* END COPY NEW CYL EXTENTS      */ 00710000
*                   IF ARNMEXT=16 THEN/* ONLY 16 EXTENTS IN AN ACQ LS*/ 00711000
@RF00348 CLI   ARNMEXT(ACQPTR),16                                  0354 00712000
         BNE   @RF00354                                            0354 00713000
*                     DO;           /* SET LIST FULL AND GET NEW LST */ 00714000
*                       ACQFULL=ON; /* INDICATE THIS LIST IS FULL    */ 00715000
         OI    ACQFULL(ACQPTR),B'10000000'                         0356 00716000
*                       CALL GETLIST;/* GET NEW LIST IF THIS ONE FULL*/ 00717000
         BAL   @14,GETLIST                                         0357 00718000
*                     END;          /* END IF LIST IS FULL           */ 00719000
*                 END;              /* END COPY EXTENTS TO LIST      */ 00720000
@RF00354 DS    0H                                                  0360 00721000
*               EXTENTS=EXTENTS+LENGTH(DEBDASD);/* INCR EXTENTS      */ 00722000
@RF00346 LA    EXTENTS,16(,EXTENTS)                                0360 00723000
*             END;                  /* END PROCESSING THIS DEB       */ 00724000
         LA    INDEX,1(,INDEX)                                     0361 00725000
@DE00345 L     @15,AMBDEBPT(,RAMB)                                 0361 00726000
         SLR   @11,@11                                             0361 00727000
         IC    @11,DEBNMEXT(,@15)                                  0361 00728000
         CR    INDEX,@11                                           0361 00729000
         BNH   @DL00345                                            0361 00730000
*           END;                    /* END NORMAL FULL DEB CASE      */ 00731000
*       END;                        /* END IF NOT CYLINDER FAULT     */ 00732000
@RC00307 DS    0H                                                  0364 00733000
*     CALL GETAMB;                  /* GET THE NEXT AMB OR ZERO      */ 00734000
@RF00298 BAL   @14,GETAMB                                          0364 00735000
*   END;                            /* END PROCESSING ALL AMBS       */ 00736000
@DE00294 LTR   RAMB,RAMB                                           0365 00737000
         BNZ   @DL00294                                            0365 00738000
*   RET=BLDASAVE;                   /* RESTORE RETURN REG            */ 00739000
         L     RET,BLDASAVE                                        0366 00740000
*   RETURN;                         /* RETURN TO THE CALLER          */ 00741000
@EL00003 DS    0H                                                  0367 00742000
@EF00003 DS    0H                                                  0367 00743000
@ER00003 BR    @14                                                 0367 00744000
*   RESPECIFY                                                      0368 00745000
*    (EXTENTS,                                                     0368 00746000
*     RXTNT,                                                       0368 00747000
*     INDEX) UNRESTRICTED;          /* FREE UP REGS                  */ 00748000
*   END BLDACQL;                    /* END OF BUILDING ACQUIRE LISTS */ 00749000
*GETLIST:                           /* BUILD A SKELETON ACQUIRE LIST */ 00750000
*                                                                  0370 00751000
*   /*****************************************************************/ 00752000
*   /*                                                               */ 00753000
*   /* THIS ROUTINE GETS CORE FOR AN ACQUIRE LIST AND INITIALIZES IT */ 00754000
*   /* AS AN ACQUIRE OR RELINQUISH LIST AND CHAINS IT FORM ACQBASE IN*/ 00755000
*   /* THE TABLE OF ACQUIRE LISTS OR ON THE END OF A CHAIN. * * THE  */ 00756000
*   /* ACQUIRE RELINQUISH LIST IS THE THE FOLLOWING FORM * * ACQFCHN */ 00757000
*   /* FORWARD CHAIN OF ALL LISTS IN CHAIN* * ACQCHAIN LOOP CHAIN OF */ 00758000
*   /* WAIT AND NON WAIT LST* * ACQFLAGS WORD OF FLAGS DISCRIBING    */ 00759000
*   /* LIST * * BASIC ACQUIRE RELINQUISH PARAMETER LIST * *          */ 00760000
*   /*                                                               */ 00761000
*   /*****************************************************************/ 00762000
*                                                                  0370 00763000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0370 00764000
GETLIST  DS    0H                                                  0371 00765000
*   RESPECIFY                                                      0371 00766000
*    (INDEX,                                                       0371 00767000
*     COREPTR) RESTRICTED;          /* RESTRICT NEED REGS            */ 00768000
*   GETLSAVE=RET;                   /* SAVE RETURN REG               */ 00769000
         ST    RET,GETLSAVE                                        0372 00770000
*   ACQSAVE=ACQPTR;                 /* SAVE CURRENT ACQPTR           */ 00771000
         ST    ACQPTR,ACQSAVE                                      0373 00772000
*   MAINLEN=LENGTH(ACQINIT)+8;      /* GET NEEDED LENGTH             */ 00773000
         MVC   MAINLEN(3),@CF03097+1                               0374 00774000
*   ACQCORE=ON;                     /* INDICATE CORE FOR ACQ LIST    */ 00775000
         OI    ACQCORE,B'00000010'                                 0375 00776000
*   CALL GETMAIN;                   /* USE COMMON GETMAIN RTN        */ 00777000
         BAL   @14,GETMAIN                                         0376 00778000
*   IF PDPCODE=PP1 THEN             /* IF NO CORE THEN PROCESS ALL   */ 00779000
         LA    @15,ERRORWD                                         0377 00780000
         CLI   PDPCODE(@15),20                                     0377 00781000
         BNE   @RF00377                                            0377 00782000
*     DO;                           /* LISTS AND THEN TRY AGAIN      */ 00783000
*       PDPCODE=0;                  /* CLEAR ERROR CODE FOR RETRY    */ 00784000
         MVI   PDPCODE(@15),X'00'                                  0379 00785000
*       ACQPTR=FIRSTACQ;            /* POINT TO FIRST LIST ON CHAIN  */ 00786000
         L     ACQPTR,FIRSTACQ                                     0380 00787000
*       CALL STAGIT;                /* GOT STAGE ALL LISTS           */ 00788000
         BAL   @14,STAGIT                                          0381 00789000
*       DO INDEX=1 TO LISTCNT;      /* ZERO ALL OLD ACQ LIST PTRS    */ 00790000
         LA    INDEX,1                                             0382 00791000
         B     @DE00382                                            0382 00792000
@DL00382 DS    0H                                                  0383 00793000
*         ACQBASE(INDEX)=0;         /* CLEAR ALL OLD PTRS            */ 00794000
         LR    @15,INDEX                                           0383 00795000
         SLA   @15,3                                               0383 00796000
         SLR   @11,@11                                             0383 00797000
         L     @09,DOLISTPT                                        0383 00798000
         ST    @11,ACQBASE-8(@15,@09)                              0383 00799000
*       END;                        /* END CLEAR OLD ACQBASE PTRS    */ 00800000
*                                                                  0384 00801000
         AL    INDEX,@CF00062                                      0384 00802000
@DE00382 CR    INDEX,LISTCNT                                       0384 00803000
         BNH   @DL00382                                            0384 00804000
*       /*************************************************************/ 00805000
*       /*                                                           */ 00806000
*       /* ALL OF THE OLD ACQUIRE LISTS MUST NOW BE FREED. AFTERWORDS*/ 00807000
*       /* A SECOND ATTEMPT WILL BE MADE TO GETMAIN FOR A NEW LIST   */ 00808000
*       /*                                                           */ 00809000
*       /*************************************************************/ 00810000
*                                                                  0385 00811000
*       COREPTR=COREBASE;           /* GET FIRST CORE IN CHAIN       */ 00812000
         L     COREPTR,COREBASE                                    0385 00813000
*       PREVCORE=COREPTR;           /* INIT PREV COREPTR             */ 00814000
         ST    COREPTR,PREVCORE                                    0386 00815000
*       DO WHILE COREPTR^=0;        /* PROCESS ALL CORE ON CHAIN     */ 00816000
         B     @DE00387                                            0387 00817000
@DL00387 DS    0H                                                  0388 00818000
*         IF COREACQ=ON THEN        /* CHECK IF ACQUIRE LIST         */ 00819000
