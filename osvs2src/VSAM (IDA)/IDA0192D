@RC00474 DS    0H                                                  0480 01106000
@DE00455 TM    CONCEND,B'00010000'                                 0480 01107000
         BZ    @DL00455                                            0480 01108000
*     END;                          /* END TIOT SCAN         @Y30LSSM*/ 01109000
*   LISTCNT=COUNT;                  /* SET LISTCNT EQUAL TO VIRT UCB */ 01110000
@RC00443 L     @15,DOLISTPT                                        0482 01111000
         LH    LISTCNT,COUNT(,@15)                                 0482 01112000
*   RET=BLDUSAVE;                   /* RESTORE RETURN REG            */ 01113000
         L     RET,BLDUSAVE                                        0483 01114000
*   RETURN;                         /* RETURN TO THE CALLER          */ 01115000
@EL00005 DS    0H                                                  0484 01116000
@EF00005 DS    0H                                                  0484 01117000
@ER00005 BR    @14                                                 0484 01118000
*   RESPECIFY                                                      0485 01119000
*    (RTIOT,                                                       0485 01120000
*     RUCB,                                                        0485 01121000
*     INDEX) UNRESTRICTED;          /* FREE UP REGS                  */ 01122000
*   END BLDUCBL;                    /* END OF THIS ROUTINE           */ 01123000
*GETAMB:                                                           0487 01124000
*                                                                  0487 01125000
*   /*****************************************************************/ 01126000
*   /*                                                               */ 01127000
*   /* THIS ROUTINE GETS THE NEXT AMB OR ZERO FOR THE CALLER. IT ALSO*/ 01128000
*   /* SETS THE AMBLSTAG BIT INDICATING THAT ALL OF THE EXTENTS      */ 01129000
*   /* ASSOCIATED WITH A PARTICULAR AMBL HAVE BEEN STAGED            */ 01130000
*   /*                                                               */ 01131000
*   /*****************************************************************/ 01132000
*                                                                  0487 01133000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0487 01134000
GETAMB   DS    0H                                                  0488 01135000
*   GETASAVE=RET;                   /* SAVE RETURN REG               */ 01136000
         ST    RET,GETASAVE                                        0488 01137000
*   IF LASTAMB=ON THEN              /* CHECK IF LAST HAS BEEN FOUND  */ 01138000
         TM    LASTAMB,B'00100000'                                 0489 01139000
         BNO   @RF00489                                            0489 01140000
*     DO;                           /* SETUP LAST INDICATORS         */ 01141000
*       RAMB=0;                     /* INDICATE NO MORE AMBS         */ 01142000
         SLR   RAMB,RAMB                                           0491 01143000
*       IF VIRTACQ=ON&              /* IS THIS AN ACQUIRE REQUEST    */ 01144000
*           VIRTBACB=ON THEN        /* IS THIS A COMPLETE ACB REQST  */ 01145000
         TM    VIRTACQ(PARMPTR),B'10010000'                        0492 01146000
         BNO   @RF00492                                            0492 01147000
*         AMBLSTAG=ON;              /* INDICATE THIS AMBL STAGED     */ 01148000
         L     @15,VIRTACB(,PARMPTR)                               0493 01149000
         L     @15,ACBAMBL(,@15)                                   0493 01150000
         OI    AMBLSTAG(@15),B'00010000'                           0493 01151000
*     END;                          /* THE LAST AMB INDICATORS ARE   */ 01152000
*                                                                  0494 01153000
*   /*****************************************************************/ 01154000
*   /*                                                               */ 01155000
*   /* IF THE LAST AMB WAS NOT PREVIOUSLY RETURNED, THEN AN AMB MUST */ 01156000
*   /* BE RETURNED TO THE CALLER                                     */ 01157000
*   /*                                                               */ 01158000
*   /*****************************************************************/ 01159000
*                                                                  0495 01160000
*   ELSE                            /* AN AMB MUST BE RETURNED       */ 01161000
*     DO;                           /* GO GET THE AMB                */ 01162000
         B     @RC00489                                            0495 01163000
@RF00489 DS    0H                                                  0496 01164000
*       IF VIRTBEXT=ON THEN         /* AMB IS PASSED IF BY EXTENT RE */ 01165000
         TM    VIRTBEXT(PARMPTR),B'00000100'                       0496 01166000
         BNO   @RF00496                                            0496 01167000
*         DO;                       /* GET PASSED AMB AND SET LAST   */ 01168000
*           RAMB=VIRTACB;           /* GET PASSED AMB                */ 01169000
         L     RAMB,VIRTACB(,PARMPTR)                              0498 01170000
*           LASTAMB=ON;             /* INDICATE NO MORE AMBS         */ 01171000
         OI    LASTAMB,B'00100000'                                 0499 01172000
*         END;                      /* END OF BY EXTENT CASE         */ 01173000
*       ELSE                        /* AN ACB WAS PASSED. GET AMBS   */ 01174000
*         DO;                       /* GET INDEX AND DATA AMBS       */ 01175000
         B     @RC00496                                            0501 01176000
@RF00496 DS    0H                                                  0502 01177000
*           IF FIRST=OFF THEN       /* IS THIS THE FIRST TIME        */ 01178000
         TM    FIRST,B'00000100'                                   0502 01179000
         BNZ   @RF00502                                            0502 01180000
*             DO;                   /* PROCESS FIRST TIME REQUEST    */ 01181000
*               IF AMBLNUM=1 THEN   /* CK IS ESDS                    */ 01182000
         L     @15,VIRTACB(,PARMPTR)                               0504 01183000
         L     @15,ACBAMBL(,@15)                                   0504 01184000
         CLC   AMBLNUM(2,@15),@CH00062                             0504 01185000
         BNE   @RF00504                                            0504 01186000
*                 DO;               /* ONLY ONE AMB IF ESDS          */ 01187000
*                   RAMB=AMBLDTA;   /* GET THE DATA AMB              */ 01188000
         L     RAMB,AMBLDTA(,@15)                                  0506 01189000
*                   LASTAMB=ON;     /* IND NO MORE AMBS              */ 01190000
