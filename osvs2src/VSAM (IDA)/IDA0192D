*               IF DUPBIT=OFF THEN  /* ADD UNIQUE VIRTUAL UCB        */ 01059000
         TM    DUPBIT,B'00001000'                                  0466 01060000
         BNZ   @RF00466                                            0466 01061000
*                 DO;               /* ADD UCB AND INCR COUNT        */ 01062000
*                   COUNT=COUNT+1;  /* INCR COUNT OF OK UCBS         */ 01063000
         L     @15,DOLISTPT                                        0468 01064000
