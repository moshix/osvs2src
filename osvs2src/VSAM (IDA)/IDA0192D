*         DO INDEX=1 TO LISTCNT;    /* PROCESS ALL ENTRIES IN ENTRY  */ 01025000
         LA    INDEX,1                                             0457 01026000
         B     @DE00457                                            0457 01027000
@DL00457 DS    0H                                                  0458 01028000
*           RUCB=TIOEFSRT;          /* GET UCB FROM TIOT             */ 01029000
         L     RUCB,TIOEFSRT-1(,RTIOT)                             0458 01030000
         LA    RUCB,0(,RUCB)                                       0458 01031000
*           IF UCB2OPT4=ON&         /* CHECK IF VIRTUAL UCB          */ 01032000
*               UCBVOLI^='000000000000'X THEN/* CHECK IF MOUNTED     */ 01033000
         TM    UCB2OPT4(RUCB),B'00001000'                          0459 01034000
         BNO   @RF00459                                            0459 01035000
         CLC   UCBVOLI(6,RUCB),@CB03499                            0459 01036000
         BE    @RF00459                                            0459 01037000
*             DO;                   /* PUT UCB IN LIST IF OK         */ 01038000
*               DUPBIT=OFF;         /* INIT AS NO DUPLICATE          */ 01039000
         NI    DUPBIT,B'11110111'                                  0461 01040000
*               DO INDEX2=1 TO INDEX;/* CHECK ALL PREV UCB ENTRY     */ 01041000
         LA    @15,1                                               0462 01042000
         B     @DE00462                                            0462 01043000
@DL00462 DS    0H                                                  0463 01044000
*                 IF LISTUCB(INDEX2)=RUCB THEN/* CHECK FOR DUP       */ 01045000
         SLA   @15,3                                               0463 01046000
         L     @11,DOLISTPT                                        0463 01047000
         AL    @11,@CF03528                                        0463 01048000
         C     RUCB,LISTUCB-4(@15,@11)                             0463 01049000
         BNE   @RF00463                                            0463 01050000
*                   DUPBIT=ON;      /* INDICATE DUPLICATE UCB        */ 01051000
