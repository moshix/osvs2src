*     ACQBASE(LISTCNT)=ACQPTR;      /* SETUP FIRST LIST FOR UCB      */ 00931000
         ST    ACQPTR,ACQBASE-8(@11,@15)                           0427 00932000
*   RET=GETLSAVE;                   /* RESTORE RETURN REG            */ 00933000
@RF00426 L     RET,GETLSAVE                                        0428 00934000
*   RETURN;                         /* RETURN TO CALLER              */ 00935000
@EL00004 DS    0H                                                  0429 00936000
@EF00004 DS    0H                                                  0429 00937000
@ER00004 BR    @14                                                 0429 00938000
*   RESPECIFY                                                      0430 00939000
*    (INDEX,                                                       0430 00940000
*     COREPTR) UNRESTRICTED;        /* FREEUP USED REGS              */ 00941000
*   END GETLIST;                    /* END OF GET AN ACQUIRE LIST    */ 00942000
*BLDUCBL:                           /* BUILD LIST OF UCBS            */ 00943000
*                                                                  0432 00944000
*   /*****************************************************************/ 00945000
*   /*                                                               */ 00946000
*   /* THIS ROUTINE GETS CORE FOR A UCB LIST AND AN ACQUIRE BASE     */ 00947000
*   /* POINTERS LIST BASED ON THE COUNT OF UCBS TO BE STAGED         */ 00948000
*   /*                                                               */ 00949000
*   /*****************************************************************/ 00950000
*                                                                  0432 00951000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0432 00952000
BLDUCBL  DS    0H                                                  0433 00953000
*   RESPECIFY                                                      0433 00954000
*    (RTIOT,                                                       0433 00955000
*     RUCB,                                                        0433 00956000
*     INDEX) RESTRICTED;            /* RESTRICT NEEDED REGS          */ 00957000
*   BLDUSAVE=RET;                   /* SAVE RETURN REG               */ 00958000
         ST    RET,BLDUSAVE                                        0434 00959000
*   PDPFUNC=FUNC41;                 /* INDICATE BUILD UCB LIST       */ 00960000
         LA    @15,ERRORWD                                         0435 00961000
         MVI   PDPFUNC(@15),X'29'                                  0435 00962000
*   MAINLEN=LISTCNT*8+LENGTH(LISTHDR);/* LENGTH OF NEEDED CORE       */ 00963000
         LR    @15,LISTCNT                                         0436 00964000
         SLA   @15,3                                               0436 00965000
         LA    @15,4(,@15)                                         0436 00966000
         STCM  @15,7,MAINLEN                                       0436 00967000
*   CALL GETMAIN;                   /* USE COMMON GETMAIN RTN        */ 00968000
         BAL   @14,GETMAIN                                         0437 00969000
*   IF PDPCODE=PP1 THEN             /* CHECK OF GETMAIN SUCCESSFUL   */ 00970000
         LA    @15,ERRORWD                                         0438 00971000
         CLI   PDPCODE(@15),20                                     0438 00972000
         BE    @RT00438                                            0438 00973000
*     GOTO EXIT;                    /* GIVEUP AND RETURN             */ 00974000
*   DOLISTPT=COREPTR;               /* POINT TO GOTTEN CORE          */ 00975000
         ST    COREPTR,DOLISTPT                                    0440 00976000
*   COUNT=0;                        /* INITIALIZE COUNT              */ 00977000
         L     @15,DOLISTPT                                        0441 00978000
         SLR   @11,@11                                             0441 00979000
         STH   @11,COUNT(,@15)                                     0441 00980000
*   CONCEND=OFF;                    /* INITIAL CONCATENTION OFF      */ 00981000
         NI    CONCEND,B'11101111'                                 0442 00982000
*   IF VIRTBUCB=ON|                 /* NO SCAN TIOENTRY IF ONE UCB   */ 00983000
*       VIRTBEXT=ON THEN            /* EOV BY EXTENT IS ALSO ONE     */ 00984000
         TM    VIRTBUCB(PARMPTR),B'00001100'                       0443 00985000
         BZ    @RF00443                                            0443 00986000
*     DO;                           /* IF VALID UCB THEN OK          */ 00987000
*       RUCB=VIRTUCB;               /* POINT TO PASSED UCB           */ 00988000
         L     RUCB,VIRTUCB(,PARMPTR)                              0445 00989000
*       IF UCB2OPT4=ON&             /* IF THIS IS VIRTUAL DEV        */ 00990000
*           UCBVOLI^='000000000000'X THEN/* VALID VOLID              */ 00991000
