*                     PDPCODE=PP93; /* INDICATE RELINQUISH DESTAGE E */ 01699000
         MVI   PDPCODE(@04),X'5D'                                  0683 01700000
*                     PDPSFC=ECBCODE;/* INDICATE TYPE OF ERROR     0684 01701000
*                                      CONTINUE TRYING TO RELINQUISH */ 01702000
         IC    @11,ECBCODE(,ACQPTR)                                0684 01703000
         STC   @11,PDPSFC(,@04)                                    0684 01704000
*                   END;            /* END UNRECOVERABLE ERROR       */ 01705000
*                 ACQPTR=ARNEDB;    /* GET NEXT LIST OR ZERO IN CHN  */ 01706000
@RF00681 L     ACQPTR,ARNEDB(,ACQPTR)                              0686 01707000
*               END;                /* END CHECKING ALL ECBS FOR ERR */ 01708000
@DE00680 LTR   ACQPTR,ACQPTR                                       0687 01709000
         BNZ   @DL00680                                            0687 01710000
*             END;                  /* END IF WAIT ISSUED            */ 01711000
*         END;                      /* END IF WAIT OPTION            */ 01712000
@RF00674 DS    0H                                                  0690 01713000
*     END;                          /* END SPECIAL RELINQUISH PROC   */ 01714000
@RF00655 DS    0H                                                  0691 01715000
*   RET=STAGSAVE;                   /* RESTORE RETURN REG            */ 01716000
@RC00587 L     RET,STAGSAVE                                        0691 01717000
*   RETURN;                         /* RETURN TO THE CALLER          */ 01718000
@EL00009 DS    0H                                                  0692 01719000
@EF00009 DS    0H                                                  0692 01720000
@ER00009 BR    @14                                                 0692 01721000
*   RESPECIFY                                                      0693 01722000
