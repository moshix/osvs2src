         ST    RET,STAGSAVE                                        0585 01451000
*   PDPFUNC=FUNC43;                 /* INDICATE ACQUIRE RELINQ FUNC  */ 01452000
         LA    @04,ERRORWD                                         0586 01453000
         MVI   PDPFUNC(@04),X'2B'                                  0586 01454000
*   IF VIRTACQ=ON THEN              /* ACQUIRE PROCESSING            */ 01455000
         TM    VIRTACQ(PARMPTR),B'10000000'                        0587 01456000
         BNO   @RF00587                                            0587 01457000
*     DO;                           /* SPECIAL ACQUIRE PROCESSING    */ 01458000
*       GEN(ICBACREL  MF=(E,(ACQPTR)),TYPE=ACQ);/* ISSUE THE ACQ     */ 01459000
         ICBACREL  MF=(E,(ACQPTR)),TYPE=ACQ                             01460000
*       IF RETCODE^=0 THEN          /* CHECK IF SUCCESSFUL           */ 01461000
         LTR   RETCODE,RETCODE                                     0590 01462000
         BZ    @RF00590                                            0590 01463000
*         DO;                       /* ACQURE SPECIAL ERRORS         */ 01464000
*           DO WHILE ARCOMPC=0;     /* FIND LIST IN ERROR            */ 01465000
         B     @DE00592                                            0592 01466000
@DL00592 DS    0H                                                  0593 01467000
*             ACQPTR=ARNEDB;        /* GET NEXT LIST IN CHAIN        */ 01468000
         L     ACQPTR,ARNEDB(,ACQPTR)                              0593 01469000
*           END;                    /* END WHEN ERROR LIST FOUND     */ 01470000
