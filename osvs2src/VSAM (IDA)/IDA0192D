@EL00006 DS    0H                                                  0529 01247000
@EF00006 DS    0H                                                  0529 01248000
@ER00006 BR    @14                                                 0529 01249000
*   END GETAMB;                     /* END OF GETAMB ROUTINE         */ 01250000
*GETMAIN:                                                          0531 01251000
*                                                                  0531 01252000
*/********************************************************************/ 01253000
*/*                                                                  */ 01254000
*/*   FUNCTION  - OBTAINS STORAGE                                    */ 01255000
*/*   INPUT -     1. AMOUNT OF STORAGE REQUESTED IN MAINLEN          */ 01256000
*/*   OUTPUT  -   1. ADDRESS OF AREA OBTAINED IN MAINADDR            */ 01257000
*/*                                                                  */ 01258000
*/********************************************************************/ 01259000
*                                                                  0531 01260000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0531 01261000
GETMAIN  DS    0H                                                  0532 01262000
*   RESPECIFY                                                      0532 01263000
*     COREPTR RESTRICTED;           /* RESTRICT NEEDED REGS          */ 01264000
*   RSAVLF=RET;                     /* SAVE RETURN ADDRESS           */ 01265000
         ST    RET,RSAVLF                                          0533 01266000
*   RESPECIFY                                                      0534 01267000
*    (R0,                                                          0534 01268000
*     R1,                                                          0534 01269000
*     R6,                                                          0534 01270000
*     R7,                                                          0534 01271000
*     R8,                                                          0534 01272000
*     R9) RESTRICTED;                                              0534 01273000
*   GEN(STM R6,R9,MAINSAVE)                                        0535 01274000
*   NOSETS;                         /* SAVE REGISTERS                */ 01275000
         STM R6,R9,MAINSAVE                                             01276000
*   R7=MAINLEN+LENGTH(COREHDR);     /* LOAD NUMBER OF BYTES REQD     */ 01277000
