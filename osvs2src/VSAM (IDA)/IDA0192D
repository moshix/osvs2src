*                       PDPCODE=PP94;/* INDICATE STAGING PROBLEM     */ 01533000
         MVI   PDPCODE(@11),X'5E'                                  0619 01534000
*                       PDPSFC=45;  /* GIVE 3850 REASON              */ 01535000
         MVI   PDPSFC(@11),X'2D'                                   0620 01536000
*                       R1=ADDR(FORECORE);/* PASS ERROR LIST         */ 01537000
         LR    R1,RCORE                                            0621 01538000
*                       CALL IDA0192P;/* CALL O/C/EOV MSG RTN        */ 01539000
         L     @15,@CV03385                                        0622 01540000
         BALR  @14,@15                                             0622 01541000
*                       OCERROR=ERRORWD;/* RESTORE IDAPDPRM          */ 01542000
         L     @11,ERRORWD                                         0623 01543000
         ST    @11,OCERROR+280(,RCORE)                             0623 01544000
*                       RESPECIFY                                  0624 01545000
*                         IDAPDPRM BASED(ADDR(ERRORWD));           0624 01546000
*                       ERRORWD=0;  /* REMOVE TRACES OF ERROR        */ 01547000
         SLR   @04,@04                                             0625 01548000
         ST    @04,ERRORWD                                         0625 01549000
*                       PDPFUNC=FUNC43;/* INDICATE STAGING FUNC      */ 01550000
         LA    @04,ERRORWD                                         0626 01551000
         MVI   PDPFUNC(@04),X'2B'                                  0626 01552000
*                     END;          /* END IF RETRY SUCCESSFUL       */ 01553000
*                 END;              /* END RETRY WITHOUT BIND        */ 01554000
*               ELSE                /* THIS IS NOT A RETRYABLE ERR   */ 01555000
*                 DO;               /* SET ERROR CODE AND EXIT       */ 01556000
         B     @RC00598                                            0629 01557000
@RF00598 DS    0H                                                  0630 01558000
*                   PDPSFC=ARCOMPC; /* INDICATE TYPE OF ERROR        */ 01559000
         LA    @04,ERRORWD                                         0630 01560000
         LH    @11,ARCOMPC(,ACQPTR)                                0630 01561000
         STC   @11,PDPSFC(,@04)                                    0630 01562000
*                   PDPCODE=PP92;   /* INDICATE STAGING ERROR        */ 01563000
         MVI   PDPCODE(@04),X'5C'                                  0631 01564000
*                   CALL BACKOUT;   /* TRY TO BACKOUT PREV ACQ       */ 01565000
         BAL   @14,BACKOUT                                         0632 01566000
*                   GOTO EXIT;      /* CLEANUP AND LEAVE             */ 01567000
         B     EXIT                                                0633 01568000
*                 END;              /* END ERROR AND QUIT            */ 01569000
*             END;                  /* END NO SPACE RETURN FROM ACQ  */ 01570000
*           ELSE                    /* UNRECOVERABLE TYPE OF ERROR   */ 01571000
*             DO;                   /* SET ERROR CODE AND QUIT       */ 01572000
         B     @RC00596                                            0636 01573000
@RF00596 DS    0H                                                  0637 01574000
*               PDPSFC=ARCOMPC;     /* INDICATE TYPE OF ERROR        */ 01575000
         LA    @04,ERRORWD                                         0637 01576000
         LH    @11,ARCOMPC(,ACQPTR)                                0637 01577000
         STC   @11,PDPSFC(,@04)                                    0637 01578000
*               PDPCODE=PP92;       /* INDICATE STAGING ERROR        */ 01579000
         MVI   PDPCODE(@04),X'5C'                                  0638 01580000
*               CALL BACKOUT;       /* TRY TO BACK OUT PREV ACQUIRS  */ 01581000
         BAL   @14,BACKOUT                                         0639 01582000
*               GOTO EXIT;          /* CLEANUP AND LEAVE             */ 01583000
         B     EXIT                                                0640 01584000
*             END;                  /* END UNRECOVERABLE ERROR       */ 01585000
*         END;                      /* END IF ERROR ON ACQUIRE       */ 01586000
*       ELSE                                                       0643 01587000
*         ;                         /* THE ACQUIRE WAS SUCCESSFUL    */ 01588000
@RF00590 DS    0H                                                  0644 01589000
*     END;                          /* END ACQUIRE LOGIC             */ 01590000
*   ELSE                            /* A RELINQUISH IS TO BE DONE    */ 01591000
*     DO;                           /* PERFORM THE RELINQUISH        */ 01592000
         B     @RC00587                                            0645 01593000
@RF00587 DS    0H                                                  0646 01594000
*       GEN(ICBACREL  MF=(E,(ACQPTR)),TYPE=REL);/* ISSUE RELINQ      */ 01595000
         ICBACREL  MF=(E,(ACQPTR)),TYPE=REL                             01596000
*       IF RETCODE^=0 THEN          /* CHECK IF AN ERROR OCCURRED    */ 01597000
         LTR   RETCODE,RETCODE                                     0647 01598000
         BZ    @RF00647                                            0647 01599000
*         DO;                       /* ANY SPECIAL RELINQUISH ERRORS */ 01600000
*           DO WHILE ARCOMPC=0;     /* SEARCH FOR ERROR LIST         */ 01601000
         B     @DE00649                                            0649 01602000
@DL00649 DS    0H                                                  0650 01603000
*             ACQPTR=ARNEDB;        /* GET NEXT LIST IN CHAIN        */ 01604000
         L     ACQPTR,ARNEDB(,ACQPTR)                              0650 01605000
*           END;                    /* END SEARCH FOR ERROR LIST     */ 01606000
@DE00649 LH    @04,ARCOMPC(,ACQPTR)                                0651 01607000
         N     @04,@CF03535                                        0651 01608000
         LTR   @04,@04                                             0651 01609000
         BZ    @DL00649                                            0651 01610000
*           PDPCODE=PP93;           /* INDICATE RELINQUISH DESTAGE E */ 01611000
         LA    @11,ERRORWD                                         0652 01612000
         MVI   PDPCODE(@11),X'5D'                                  0652 01613000
*           PDPSFC=ARCOMPC;         /* INDICATE TYPE OF ERROR      0653 01614000
*                                      CONTINUE TRYING TO RELINQUISH */ 01615000
         STC   @04,PDPSFC(,@11)                                    0653 01616000
*         END;                      /* END IF AN ERROR OCCURRED      */ 01617000
*       IF ARDRES=ON THEN           /* CHECK IF A WAIT IS REQUESTED  */ 01618000
@RF00647 TM    ARDRES(ACQPTR),B'01000000'                          0655 01619000
         BNO   @RF00655                                            0655 01620000
*         DO;                       /* WAIT AND CHECK FOR ERRORS     */ 01621000
*           MAINLEN=LISTOTAL*4;     /* CALC CORE                     */ 01622000
         L     @04,LISTOTAL                                        0657 01623000
         SLA   @04,2                                               0657 01624000
         STCM  @04,7,MAINLEN                                       0657 01625000
*           CALL GETMAIN;           /* GET CORE FOR WAIT LIST        */ 01626000
         BAL   @14,GETMAIN                                         0658 01627000
*           IF PDPCODE=PP1 THEN     /* CHECK IF GET CORE OK          */ 01628000
         LA    @04,ERRORWD                                         0659 01629000
         CLI   PDPCODE(@04),20                                     0659 01630000
         BE    @RT00659                                            0659 01631000
*             GOTO EXIT;            /* QUIT IF GETMAIN BAD           */ 01632000
*           WAITPTR=COREPTR;        /* POINT TO RETURNED CORE        */ 01633000
         ST    COREPTR,WAITPTR                                     0661 01634000
*           ACQPTR=FIRSTACQ;        /* POINT TO FIRST ACQ LIST       */ 01635000
         L     ACQPTR,FIRSTACQ                                     0662 01636000
*           R15=0;                  /* INIT FOR NO OK ECBS           */ 01637000
         SLR   R15,R15                                             0663 01638000
*           DO WHILE ACQPTR^=0;     /* STOP IF NO MORE LISTS         */ 01639000
         B     @DE00664                                            0664 01640000
@DL00664 DS    0H                                                  0665 01641000
*             IF ARCOMPC=0 THEN     /* INCR WAIT COUNT IF NO ERR     */ 01642000
         LH    @04,ARCOMPC(,ACQPTR)                                0665 01643000
         N     @04,@CF03535                                        0665 01644000
         LTR   @04,@04                                             0665 01645000
         BNZ   @RF00665                                            0665 01646000
*               DO;                 /* SETUP TO USE THIS ECB         */ 01647000
*                 R15=R15+1;        /* INCR COUNT BY ONE IF NO ERR   */ 01648000
         LA    R15,1(,R15)                                         0667 01649000
*                 LISTECB(R15)=ADDR(RELECB);/* POINT TO ECB          */ 01650000
         LR    @04,R15                                             0668 01651000
         SLA   @04,2                                               0668 01652000
         LA    @11,RELECB(,ACQPTR)                                 0668 01653000
         L     @10,WAITPTR                                         0668 01654000
         AL    @10,@CF03528                                        0668 01655000
         ST    @11,LISTECB(@04,@10)                                0668 01656000
*               END;                /* END POINT TO ECB TO WAIT ON   */ 01657000
*             ACQPTR=ARNEDB;        /* GET NEXT LIST IN CHAIN        */ 01658000
@RF00665 L     ACQPTR,ARNEDB(,ACQPTR)                              0670 01659000
*           END;                    /* END BUILD MULTI WAIT LIST     */ 01660000
@DE00664 SLR   @04,@04                                             0671 01661000
         CR    ACQPTR,@04                                          0671 01662000
         BNE   @DL00664                                            0671 01663000
*           RFY                                                    0672 01664000
*             R1 RESTRICTED;                                       0672 01665000
*           R1=WAITPTR;             /* GET REG PTR TO WAIT LIST      */ 01666000
         L     @11,WAITPTR                                         0673 01667000
         LR    R1,@11                                              0673 01668000
*           IF R15^=0 THEN          /* DO NOT WAIT IF 0 COUNT        */ 01669000
         CR    R15,@04                                             0674 01670000
         BE    @RF00674                                            0674 01671000
*             DO;                   /* WAIT OF GOOD LISTS            */ 01672000
*               LASTECB(R15)=ON;    /* INDICATE LAST ECB IN LIST     */ 01673000
         LR    @04,R15                                             0676 01674000
         SLA   @04,2                                               0676 01675000
