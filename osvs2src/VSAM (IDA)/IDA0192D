         L     @15,ACBAMBL(,@15)                                   0520 01228000
         L     RAMB,AMBLDTA(,@15)                                  0520 01229000
*                   LASTAMB=ON;     /* INDICATE NO MORE AMBS IN AMBL */ 01230000
         OI    LASTAMB,B'00100000'                                 0521 01231000
*                 END;              /* END SEPARATE DATA AND INDEX   */ 01232000
*             END;                  /* END IF NOT FIRST TIME FR AMBL */ 01233000
@RC00513 DS    0H                                                  0524 01234000
*         END;                      /* END IFNOT BY EXTENT CASE      */ 01235000
@RC00502 DS    0H                                                  0525 01236000
*     END;                          /* END IF LASTAMB BIT NOT ON     */ 01237000
@RC00496 DS    0H                                                  0526 01238000
*   IF RAMB^=0 THEN                 /* IF AN AMB WAS FOUND THEN      */ 01239000
@RC00489 LTR   RAMB,RAMB                                           0526 01240000
         BZ    @RF00526                                            0526 01241000
*     CALL SELOPTNS;                /* THE NECESSARY OPTIONS SET     */ 01242000
         BAL   @14,SELOPTNS                                        0527 01243000
*   RET=GETASAVE;                   /* RESTORE RETURN REG            */ 01244000
@RF00526 L     RET,GETASAVE                                        0528 01245000
