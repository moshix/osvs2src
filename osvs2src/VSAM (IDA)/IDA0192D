*           LISTUCB(COUNT)=RUCB;    /* POINT TO PASSED UCB           */ 01000000
         SLA   @11,3                                               0449 01001000
         AL    @15,@CF03528                                        0449 01002000
         ST    RUCB,LISTUCB-4(@11,@15)                             0449 01003000
*         END;                      /* END VALID ONE UCB CASE        */ 01004000
*     END;                          /* END ONE UCB CASE              */ 01005000
*   ELSE                            /* THE TIOENTRY MUST BE SCANNED  */ 01006000
*     DO;                           /* FOR UCBS              @Y30LSSM*/ 01007000
         B     @RC00443                                            0452 01008000
@RF00443 DS    0H                                                  0453 01009000
*       DSABPTR=FSTDSAB;            /* GET FIRST DSAB        @Y30LSSM*/ 01010000
         L     @15,FSTDSAB                                         0453 01011000
         ST    @15,DSABPTR                                         0453 01012000
*       RTIOT=DSABTIOT;             /* FIRST TIOT ENTRY      @Y30LSSM*/ 01013000
         L     RTIOT,DSABTIOT(,@15)                                0454 01014000
*       DO WHILE CONCEND=OFF;       /* PROCESS ALL ALLOCATED UCBS    */ 01015000
         B     @DE00455                                            0455 01016000
@DL00455 DS    0H                                                  0456 01017000
*         LISTCNT=(TIOELNGH-16)/4;  /* GET NUMBER OF ENTRIES         */ 01018000
