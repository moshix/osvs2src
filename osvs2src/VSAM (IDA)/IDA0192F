         LA    @15,JESWORK(,@05)                                   0631 01387000
         ST    @15,QMIWRKAD(,@05)                                  0631 01388000
*       QMIRECL=LENGTH(JFCB);       /* SET LENGTH OF RECORD          */ 01389000
         MVC   QMIRECL(2,@05),@CH01428                             0632 01390000
