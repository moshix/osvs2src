@EL00007 DS    0H                                                  0870 02124000
@EF00007 DS    0H                                                  0870 02125000
@ER00007 BR    @14                                                 0870 02126000
*OPNUPGR:                                                          0871 02127000
*                                                                  0871 02128000
*/********************************************************************/ 02129000
*/*                                                                  */ 02130000
*/*   FUNCTION: OPEN UPGRADE SET.  BUILDS UPT, UPGRADE RPL'S, AND    */ 02131000
*/*             UPGRADE ACB'S.  CALLS BLDAMBL TO BUILD THE UPGRADE   */ 02132000
*/*             AMBL'S.  CALLS IDA0192B TO OPEN THE UPGRADE          */ 02133000
*/*             INDEXES.                                             */ 02134000
*/*                                                                  */ 02135000
*/*   INPUTS:   1. PTR TO OPW IN RCORE.                              */ 02136000
*/*                                                                  */ 02137000
*/*   OUTPUTS:  1. PTR TO UPT IN OPWUPT.                             */ 02138000
*/*                                                                  */ 02139000
*/********************************************************************/ 02140000
*                                                                  0871 02141000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0871 02142000
OPNUPGR  DS    0H                                                  0872 02143000
*   RFY                                                            0872 02144000
*     RET RSTD;                                                    0872 02145000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 02146000
         ST    RET,MWASAVEA                                        0873 02147000
*   RFY                                                            0874 02148000
*     RET UNRSTD;                                                  0874 02149000
*   PDPFUNC=PDF89;                  /* INIT FUNCTION CODE            */ 02150000
*                                                                  0875 02151000
         L     @05,OPWCOMWA(,RCORE)                                0875 02152000
         MVI   PDPFUNC+280(@05),X'59'                              0875 02153000
*   /*****************************************************************/ 02154000
*   /*                                                               */ 02155000
*   /* BUILD UPGRADE TABLE AND INITIALIZE HEADER                     */ 02156000
*   /*                                                               */ 02157000
*   /*****************************************************************/ 02158000
*                                                                  0876 02159000
*   OPWVSMPL=OPWVSMPL&&OPWVSMPL;    /* ZERO PARAMETER LIST FOR VSM   */ 02160000
         XC    OPWVSMPL(16,RCORE),OPWVSMPL(RCORE)                  0876 02161000
*   OPWVMLNG=LENGTH(IDAUPT)+LENGTH(UPTAXENT)*OPWNOUPG;/* COMPUTE   0877 02162000
*                                      LENGTH OF UPT                 */ 02163000
         SLR   @05,@05                                             0877 02164000
         IC    @05,OPWNOUPG(,RCORE)                                0877 02165000
         MH    @05,@CH00033                                        0877 02166000
         AL    @05,@CF01709                                        0877 02167000
         STCM  @05,7,OPWVMLNG(RCORE)                               0877 02168000
*   OPWVMTYP=SPHBLK;                /* IND REQUEST FOR SPHERE BLOCK  */ 02169000
         MVI   OPWVMTYP(RCORE),X'80'                               0878 02170000
*   OPWVMANC=ADDR(IDABIB);          /* INIT ANCHOR BLOCK TO BIB      */ 02171000
         L     @05,OPWBIB(,RCORE)                                  0879 02172000
         ST    @05,OPWVMANC(,RCORE)                                0879 02173000
*   CALL IDA0192M;                  /* OBTAIN STORAGE FOR UPT        */ 02174000
         L     @15,@CV04855                                        0880 02175000
         BALR  @14,@15                                             0880 02176000
*   RSTATRTN=IDAR192F;              /* INDIC 192F CONTROL    @Y30DSDP*/ 02177000
         L     @05,OPWCOMWA(,RCORE)                                0881 02178000
         MVC   RSTATRTN(4,@05),@CC04581                            0881 02179000
*   IF OPWVMADR=0 THEN              /* IF STORAGE NOT OBTAINED       */ 02180000
         L     @15,OPWVMADR(,RCORE)                                0882 02181000
         LTR   @15,@15                                             0882 02182000
         BNZ   @RF00882                                            0882 02183000
*     DO;                           /* THEN CALL PROB DET            */ 02184000
*       PDPCODE=PP20;               /* IND GETMAIN ERROR             */ 02185000
         MVI   PDPCODE+280(@05),X'14'                              0884 02186000
*       CALL PROBDT;                /* WRITE PROB DET MESSAGE        */ 02187000
         BAL   @14,PROBDT                                          0885 02188000
*     END;                                                         0886 02189000
*   OPWUPT=OPWVMADR;                /* INIT PTR TO UPT               */ 02190000
@RF00882 L     @05,OPWVMADR(,RCORE)                                0887 02191000
         ST    @05,OPWUPT(,RCORE)                                  0887 02192000
*   UPTID=IDXUPT;                   /* INIT ID FOR UPT               */ 02193000
         MVI   UPTID(@05),B'01000101'                              0888 02194000
*   UPTLEN=OPWVMLNG;                /* INIT LENGTH OF UPT            */ 02195000
         L     @15,OPWVMLNG-1(,RCORE)                              0889 02196000
         LA    @15,0(,@15)                                         0889 02197000
         STH   @15,UPTLEN(,@05)                                    0889 02198000
*   UPTNOENT=OPWNOUPG;              /* INIT NO. OF ENTRIES IN UPT    */ 02199000
         IC    @15,OPWNOUPG(,RCORE)                                0890 02200000
         STC   @15,UPTNOENT(,@05)                                  0890 02201000
*   MWASWPTR=ADDR(BIBUPT);          /* INIT PTR TO SWAP WORD         */ 02202000
         L     @01,OPWBIB(,RCORE)                                  0891 02203000
         LA    @15,BIBUPT(,@01)                                    0891 02204000
         ST    @15,MWASWPTR                                        0891 02205000
*   MWASWAP=OPWVMADR;               /* INIT SWAP CONTENTS            */ 02206000
         ST    @05,MWASWAP                                         0892 02207000
*   CALL UPDSSL;                    /* UPDATE SWAP SAVE LIST         */ 02208000
*                                                                  0893 02209000
         BAL   @14,UPDSSL                                          0893 02210000
*   /*****************************************************************/ 02211000
*   /*                                                               */ 02212000
*   /* INITIALIZE ALTERNATE INDEX ENTRIES IN UPGRADE TABLE           */ 02213000
*   /*                                                               */ 02214000
*   /*****************************************************************/ 02215000
*                                                                  0894 02216000
*   MWAXAMBL=OPWBAMBL;              /* PT TO BASE AMBL               */ 02217000
         L     @05,OPWBAMBL(,RCORE)                                0894 02218000
         ST    @05,MWAXAMBL                                        0894 02219000
*   IF OPWBIXCI^=0 THEN             /* IF KSDS                       */ 02220000
         L     @15,OPWBIXCI-1(,RCORE)                              0895 02221000
         LA    @15,0(,@15)                                         0895 02222000
         LTR   @15,@15                                             0895 02223000
         BZ    @RF00895                                            0895 02224000
*     MWABSEKL=AMDKEYLN;            /* THEN INIT KEY LENGTH          */ 02225000
         L     @05,AMBLDTA(,@05)                                   0896 02226000
         L     @05,AMBDSB(,@05)                                    0896 02227000
         LH    @05,AMDKEYLN(,@05)                                  0896 02228000
         STH   @05,MWABSEKL                                        0896 02229000
*   ELSE                            /* ELSE ESDS                     */ 02230000
*     MWABSEKL=ESDSKEYL;            /* INIT KEY LENGTH TO LENGTH OF     02231000
*                                      RBA                           */ 02232000
         B     @RC00895                                            0897 02233000
@RF00895 MVC   MWABSEKL(2),@CH00095                                0897 02234000
*   DO MWAU=1 TO OPWNOUPG;          /* OPEN UPGRADE AIX'S            */ 02235000
@RC00895 LA    @05,1                                               0898 02236000
         B     @DE00898                                            0898 02237000
@DL00898 DS    0H                                                  0899 02238000
*     OPWCURPT=ADDR(OPWUPAIX(MWAU));/* SET CURR PTR TO UPGR AIX      */ 02239000
         SLA   @05,3                                               0899 02240000
         LA    @05,OPWUPAIX-8(@05,RCORE)                           0899 02241000
         ST    @05,OPWCURPT(,RCORE)                                0899 02242000
*     IF OPWPDTCI=OPWCDTCI THEN     /* IF THIS UPGRADE ALSO A PATH   */ 02243000
         CLC   OPWPDTCI(3,RCORE),OPWCDTCI(@05)                     0900 02244000
         BNE   @RF00900                                            0900 02245000
*       OPWSVWRK=ON;                /* THEN IND DON'T FREE AMB WA'S  */ 02246000
         OI    OPWSVWRK(@05),B'00010000'                           0901 02247000
*     ELSE                          /* UPGRADE NOT ALSO A PATH       */ 02248000
*       OPWSVWRK=OFF;               /* ELSE IND FREE AMB WORK AREAS  */ 02249000
         B     @RC00900                                            0902 02250000
@RF00900 L     @05,OPWCURPT(,RCORE)                                0902 02251000
