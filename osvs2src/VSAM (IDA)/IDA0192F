*           DO;                     /* YES,                  @Z30SSDP*/ 00420000
*             MWAJFTTR(1:3)=TIOEJFCB;/* LD TTR OF NEXT JFCB          */ 00421000
         MVC   MWAJFTTR(3),TIOEJFCB(@14)                           0382 00422000
*             CALL READJFCB;        /* READ NEXT JFCB                */ 00423000
         BAL   @14,READJFCB                                        0383 00424000
*             IF MWAVVNBR<JFCBNVOL THEN/* IF OLD VVOLL NOT LARGE   0384 00425000
*                                      ENOUGH                        */ 00426000
         LH    @05,MWAVVNBR                                        0384 00427000
         L     @01,MWAJFCB                                         0384 00428000
         SLR   @00,@00                                             0384 00429000
         IC    @00,JFCBNVOL(,@01)                                  0384 00430000
         CR    @05,@00                                             0384 00431000
         BNL   @RF00384                                            0384 00432000
*               DO;                 /* THEN FREE OLD VVOLL           */ 00433000
*                 MWAGFADR=MWAVVOLL;/* POINT TO VVOLL                */ 00434000
         L     @05,MWAVVOLL                                        0386 00435000
         ST    @05,MWAGFADR                                        0386 00436000
*                 MWAGFSP=WKPOOL;   /* INIT SUBPOOL                  */ 00437000
         MVI   MWAGFSP,X'FC'                                       0387 00438000
*                 MWAGFLEN=MWAVVLNG;/* INIT LENGTH                   */ 00439000
         MVC   MWAGFLEN(3),MWAVVLNG                                0388 00440000
*                 CALL FREECORE;    /* FREE OLD VVOLL                */ 00441000
         BAL   @14,FREECORE                                        0389 00442000
*                 MWANEWSW=ON;      /* IND NEW VVOLL NEEDED          */ 00443000
         OI    MWANEWSW,B'00001000'                                0390 00444000
*               END;                                               0391 00445000
*           END;                                                   0392 00446000
@RF00384 DS    0H                                                  0393 00447000
*       END;                                                       0393 00448000
@RF00380 DS    0H                                                  0394 00449000
*   END;                                                           0394 00450000
@RF00376 DS    0H                                                  0394 00451000
@DE00342 L     @05,MWADSABP                                        0394 00452000
         LTR   @05,@05                                             0394 00453000
         BZ    @DC00342                                            0394 00454000
         L     @05,MWATIOT                                         0394 00455000
         CLC   TIOEDDNM(8,@05),@CC04828                            0394 00456000
         BE    @DL00342                                            0394 00457000
@DC00342 DS    0H                                                  0395 00458000
*   MWAGFADR=MWAVVOLL;              /* PT TO VVOLL                   */ 00459000
         L     @05,MWAVVOLL                                        0395 00460000
         ST    @05,MWAGFADR                                        0395 00461000
*   MWAGFSP=WKPOOL;                 /* INIT SUBPOOL OF CURRENT VVOLL */ 00462000
         MVI   MWAGFSP,X'FC'                                       0396 00463000
*   MWAGFLEN=MWAVVLNG;              /* INIT LENGTH OF CURRENT VVOLL  */ 00464000
         MVC   MWAGFLEN(3),MWAVVLNG                                0397 00465000
*   CALL FREECORE;                  /* FREE CURRENT VVOLL            */ 00466000
         BAL   @14,FREECORE                                        0398 00467000
*   IF MWAJFBSW=ON THEN             /* IF JFCB WORK AREA OBTAINED    */ 00468000
         TM    MWAJFBSW,B'00000010'                                0399 00469000
         BNO   @RF00399                                            0399 00470000
*     DO;                           /* THEN FREE IT                  */ 00471000
*       MWAGFADR=MWAJFBWA;          /* INIT ADDRESS                  */ 00472000
         L     @05,MWAJFBWA                                        0401 00473000
         ST    @05,MWAGFADR                                        0401 00474000
*       MWAGFSP=WKPOOL;             /* INIT SUBPOOL                  */ 00475000
         MVI   MWAGFSP,X'FC'                                       0402 00476000
*       MWAGFLEN=LENGTH(JFCBWRKA);  /* INIT LENGTH                   */ 00477000
         MVC   MWAGFLEN(3),@CF04928+1                              0403 00478000
*       CALL FREECORE;              /* FREE JFCB WORK AREA           */ 00479000
         BAL   @14,FREECORE                                        0404 00480000
*       MWAJFBSW=OFF;               /* IND WORK AREA FREED           */ 00481000
         NI    MWAJFBSW,B'11111101'                                0405 00482000
*     END;                                                         0406 00483000
*   IF OPWCRA^='000000000000'X THEN /* IF CATLG RECOVERY             */ 00484000
@RF00399 CLC   OPWCRA(6,RCORE),@CB04834                            0407 00485000
         BE    @RF00407                                            0407 00486000
*     DO;                           /* THEN DETERMINE IF CRA VOLUME     00487000
*                                      MOUNTED                       */ 00488000
*       MWAVMT=BIBVMT;              /* PT TO FIRST VMT               */ 00489000
         L     @05,OPWBIB(,RCORE)                                  0409 00490000
         L     @05,BIBVMT(,@05)                                    0409 00491000
         ST    @05,MWAVMT                                          0409 00492000
*       DO WHILE(MWAVMT^=0&MWACRAMT=OFF);/* SCAN VMT CHAIN FOR CRA 0410 00493000
*                                      VOLUME                        */ 00494000
         B     @DE00410                                            0410 00495000
@DL00410 DS    0H                                                  0411 00496000
*         DO MWAI=1 TO VMTNOVOL WHILE(VMTVLSER(MWAI)^=OPWCRA);/* SCAN   00497000
*                                      VMT FOR CRA VOLUME            */ 00498000
         LA    @05,1                                               0411 00499000
         B     @DE00411                                            0411 00500000
@DL00411 LR    @15,@05                                             0411 00501000
         SLA   @15,4                                               0411 00502000
         L     @01,MWAVMT                                          0411 00503000
         ALR   @01,@15                                             0411 00504000
         CLC   VMTVLSER-16(6,@01),OPWCRA(RCORE)                    0411 00505000
         BE    @DC00411                                            0411 00506000
*         END;                                                     0412 00507000
         AL    @05,@CF00053                                        0412 00508000
@DE00411 ST    @05,MWAI                                            0412 00509000
         L     @01,MWAVMT                                          0412 00510000
         CH    @05,VMTNOVOL(,@01)                                  0412 00511000
         BNH   @DL00411                                            0412 00512000
@DC00411 DS    0H                                                  0413 00513000
*         IF MWAI>VMTNOVOL THEN     /* IF CRA VOLUME NOT FOUND       */ 00514000
         L     @05,MWAVMT                                          0413 00515000
         L     @15,MWAI                                            0413 00516000
         CH    @15,VMTNOVOL(,@05)                                  0413 00517000
         BNH   @RF00413                                            0413 00518000
*           MWAVMT=VMTNXT;          /* THEN PT TO NEXT VMT           */ 00519000
         L     @05,VMTNXT(,@05)                                    0414 00520000
         ST    @05,MWAVMT                                          0414 00521000
*         ELSE                      /* CRA VOLUME FOUND              */ 00522000
*           MWACRAMT=ON;            /* IND CRA VOL MOUNTED           */ 00523000
         B     @RC00413                                            0415 00524000
@RF00413 OI    MWACRAMT,B'00000100'                                0415 00525000
*       END;                                                       0416 00526000
@RC00413 DS    0H                                                  0416 00527000
@DE00410 L     @05,MWAVMT                                          0416 00528000
         LTR   @05,@05                                             0416 00529000
         BZ    @DC00410                                            0416 00530000
         TM    MWACRAMT,B'00000100'                                0416 00531000
         BZ    @DL00410                                            0416 00532000
@DC00410 DS    0H                                                  0417 00533000
*       IF MWACRAMT=OFF THEN        /* IF CRA VOL NOT MOUNTED        */ 00534000
         TM    MWACRAMT,B'00000100'                                0417 00535000
         BNZ   @RF00417                                            0417 00536000
*         DO;                       /* THEN WRITE PROB DET MSG       */ 00537000
*           PDPCODE=PP168;          /* THEN SET FAILING ERROR CODE   */ 00538000
         L     @05,OPWCOMWA(,RCORE)                                0419 00539000
         MVI   PDPCODE+280(@05),X'A8'                              0419 00540000
*           CALL PROBDT;            /* WRITE MESSAGE                 */ 00541000
         BAL   @14,PROBDT                                          0420 00542000
*         END;                                                     0421 00543000
*     END;                                                         0422 00544000
@RF00417 DS    0H                                                  0423 00545000
*   RFY                                                            0423 00546000
*     RET RSTD;                                                    0423 00547000
@RF00407 DS    0H                                                  0424 00548000
*   RET=MWASAVEA;                   /* RESTORE RETURN ADDR           */ 00549000
         L     RET,MWASAVEA                                        0424 00550000
*   RFY                                                            0425 00551000
*     RET UNRSTD;                                                  0425 00552000
*   END VOLMNT;                                                    0426 00553000
@EL00002 DS    0H                                                  0426 00554000
@EF00002 DS    0H                                                  0426 00555000
@ER00002 BR    @14                                                 0426 00556000
*NEWDEV:                                                           0427 00557000
*                                                                  0427 00558000
*/********************************************************************/ 00559000
*/*                                                                  */ 00560000
*/*   FUNCTION: VOLUME MOUNT FOR NEW DEVICE TYPE.  THIS ROUTINE      */ 00561000
*/*             MOVES VOLUME SERIAL NUMBERS FROM JFCB AND JFCB       */ 00562000
*/*             EXTENSIONS INTO THE VVOLL.  IDA0192V IS CALLED TO    */ 00563000
*/*             MOUNT THE VOLUMES IN THE VVOLL.  UPON RETURN FROM    */ 00564000
*/*             IDA0192V A VMT IS BUILT FOR THE MOUNTED VOLUMES      */ 00565000
*/*             AND ADDED TO THE VMT CHAIN.                          */ 00566000
*/*                                                                  */ 00567000
*/*   INPUTS    1. PTR TO VVOLL IN OPWVVOLL.                         */ 00568000
*/*             2. PTR TO TIOT ENTRY IN MWATIOT.                     */ 00569000
*/*             3. PTR TO JFCB IN MWAJFCB.                           */ 00570000
*/*             4. NUMBER OF VOLUME SERIAL NUMBERS IN JFCB AND       */ 00571000
*/*                EXTENSIONS IN VVOLLNBR.                           */ 00572000
*/*                                                                  */ 00573000
*/*   OUTPUTS:  1. VMT FOR NEW DEVICE ADDED TO VMT CHAIN.            */ 00574000
*/*                                                                  */ 00575000
*/********************************************************************/ 00576000
*                                                                  0427 00577000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0427 00578000
NEWDEV   DS    0H                                                  0428 00579000
*   RFY                                                            0428 00580000
*     RET RSTD;                                                    0428 00581000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 00582000
         ST    RET,MWASAVEB                                        0429 00583000
*   RFY                                                            0430 00584000
*     RET UNRSTD;                                                  0430 00585000
*                                                                  0430 00586000
*   /*****************************************************************/ 00587000
*   /*                                                               */ 00588000
*   /* MOVE VOLUME SERIAL NUMBER FROM JFCB AND JFCB EXTENSIONS INTO  */ 00589000
*   /* VVOLL.                                                        */ 00590000
*   /*                                                               */ 00591000
*   /*****************************************************************/ 00592000
*                                                                  0431 00593000
*   RFY                                                            0431 00594000
*     VOLSER BASED(MWAPTRA);                                       0431 00595000
*   MWAPTRA=ADDR(JFCBVOLS);         /* PT TO VOL SER NOS. IN JFCB    */ 00596000
         L     @05,MWAJFCB                                         0432 00597000
         LA    @05,JFCBVOLS(,@05)                                  0432 00598000
         ST    @05,MWAPTRA                                         0432 00599000
*   MWANEXTC=JFCBPNVL;              /* SET COUNT FOR NEXT JFCB EXT   */ 00600000
         MVC   MWANEXTC(4),@CF00145                                0433 00601000
*   DO MWAI=1 TO VVOLLNBR;          /* SCAN VOL SER NUMBERS          */ 00602000
         LA    @05,1                                               0434 00603000
         B     @DE00434                                            0434 00604000
@DL00434 DS    0H                                                  0435 00605000
*     IF MWAI>MWANEXTC THEN         /* IF NEXT JFCB EXT NEEDED       */ 00606000
         L     @15,MWANEXTC                                        0435 00607000
         CR    @05,@15                                             0435 00608000
         BNH   @RF00435                                            0435 00609000
*       DO;                         /* THEN READ NEXT EXTENSION      */ 00610000
*         IF MWANEXTC=JFCBPNVL THEN /* IF FIRST EXTENSION            */ 00611000
         C     @15,@CF00145                                        0437 00612000
         BNE   @RF00437                                            0437 00613000
*           MWAJFTTR(1:3)=JFCBEXAD; /* SET DISK ADDR OF FIRST EXT    */ 00614000
         L     @05,MWAJFCB                                         0438 00615000
         MVC   MWAJFTTR(3),JFCBEXAD(@05)                           0438 00616000
*         ELSE                      /* ELSE PROCESSING EXTENSION     */ 00617000
*           MWAJFTTR(1:3)=JFCBXTTR; /* SET DISK ADDR FOR NEXT JFCB 0439 00618000
*                                      EXTENSION                     */ 00619000
         B     @RC00437                                            0439 00620000
@RF00437 L     @05,MWAJFCB                                         0439 00621000
         MVC   MWAJFTTR(3),JFCBXTTR(@05)                           0439 00622000
*         CALL READJFCB;            /* READ JFCB EXTENSION           */ 00623000
@RC00437 BAL   @14,READJFCB                                        0440 00624000
*         MWANEXTC=MWANEXTC+JFCBXNVL;/* INCR CNT FOR NEXT EXTENSION  */ 00625000
         LA    @05,15                                              0441 00626000
         AL    @05,MWANEXTC                                        0441 00627000
         ST    @05,MWANEXTC                                        0441 00628000
*         MWAPTRA=ADDR(JFCBXVOL);   /* PT TO FIRST VOL SER NO. IN  0442 00629000
*                                      JFCB EXTENSION                */ 00630000
         L     @05,MWAJFCB                                         0442 00631000
         LA    @05,JFCBXVOL(,@05)                                  0442 00632000
         ST    @05,MWAPTRA                                         0442 00633000
*       END;                                                       0443 00634000
*     VVOLLVSR(MWAI)=VOLSER;        /* MOVE VOL SER NO. INTO VVOLL   */ 00635000
@RF00435 L     @05,MWAI                                            0444 00636000
         LR    @15,@05                                             0444 00637000
         MH    @15,@CH00033                                        0444 00638000
         L     @01,MWAVVOLL                                        0444 00639000
         ALR   @01,@15                                             0444 00640000
         L     @15,MWAPTRA                                         0444 00641000
         MVC   VVOLLVSR-12(6,@01),VOLSER(@15)                      0444 00642000
*     MWAPTRA=MWAPTRA+LENGTH(VOLSER);/* PT TO NEXT VOLSER            */ 00643000
         AL    @15,@CF00089                                        0445 00644000
         ST    @15,MWAPTRA                                         0445 00645000
*   END;                                                           0446 00646000
         AL    @05,@CF00053                                        0446 00647000
@DE00434 ST    @05,MWAI                                            0446 00648000
         L     @01,MWAVVOLL                                        0446 00649000
         CH    @05,VVOLLNBR(,@01)                                  0446 00650000
         BNH   @DL00434                                            0446 00651000
*   RFY                                                            0447 00652000
*     R1 RSTD;                                                     0447 00653000
*   R1=MWAVVOLL;                    /* PT TO VVOLL                   */ 00654000
         L     R1,MWAVVOLL                                         0448 00655000
*   RFY                                                            0449 00656000
*     R1 UNRSTD;                                                   0449 00657000
*   RSTATRTN=IDAR192V;              /* INDIC 192V CONTROL    @Y30DSDP*/ 00658000
         L     @05,OPWCOMWA(,RCORE)                                0450 00659000
         MVC   RSTATRTN(4,@05),@CC04591                            0450 00660000
*   CALL IDA0192V;                  /* MOUNT VOLUMES                 */ 00661000
         L     @15,@CV04841                                        0451 00662000
         BALR  @14,@15                                             0451 00663000
*   RSTATRTN=IDAR192F;              /* INDIC 192F CONTROL    @Y30DSDP*/ 00664000
         L     @05,OPWCOMWA(,RCORE)                                0452 00665000
         MVC   RSTATRTN(4,@05),@CC04581                            0452 00666000
*   RFY                                                            0453 00667000
*     RETCODE RSTD;                                                0453 00668000
*   IF RETCODE>0 THEN               /* IF ALL VOLUMES NOT MOUNTED    */ 00669000
         LTR   RETCODE,RETCODE                                     0454 00670000
         BNP   @RF00454                                            0454 00671000
*     DO;                           /* THEN IND ALL VOLS NOT MOUNTED */ 00672000
*       IF RETCODE>4 THEN           /* IF ERROR DETECTED BY VOL MNT  */ 00673000
         C     RETCODE,@CF00095                                    0456 00674000
         BNH   @RF00456                                            0456 00675000
*         DO;                       /* THEN DETERMINE TYPE OF ERROR  */ 00676000
*           RFY                                                    0458 00677000
*             RETCODE UNRSTD;                                      0458 00678000
*           PDPCODE=VVOLLPDC;       /* LD PROB DET CODE              */ 00679000
         AL    @05,@CF04927                                        0459 00680000
         L     @15,MWAVVOLL                                        0459 00681000
         IC    @14,VVOLLPDC(,@15)                                  0459 00682000
         STC   @14,PDPCODE(,@05)                                   0459 00683000
*           PDPSFC=VVOLLSFC;        /* LD SUB-FUNCTION CODE          */ 00684000
         IC    @14,VVOLLSFC(,@15)                                  0460 00685000
         STC   @14,PDPSFC(,@05)                                    0460 00686000
*           IF PDPCODE=PP116|       /* IF I/O ERROR READING DSCB     */ 00687000
*               PDPCODE=PP59 THEN   /* OR VOL TIME STAMP ERROR       */ 00688000
         CLI   PDPCODE(@05),116                                    0461 00689000
         BE    @RT00461                                            0461 00690000
         CLI   PDPCODE(@05),59                                     0461 00691000
         BNE   @RF00461                                            0461 00692000
@RT00461 DS    0H                                                  0462 00693000
*             PDPVLSER=ADDR(VVOLLVSR(VVOLLCNT));/* PT TO BAD VOLUME  */ 00694000
         L     @05,OPWCOMWA(,RCORE)                                0462 00695000
         L     @15,MWAVVOLL                                        0462 00696000
         SLR   @14,@14                                             0462 00697000
         IC    @14,VVOLLCNT(,@15)                                  0462 00698000
         MH    @14,@CH00033                                        0462 00699000
         LA    @15,VVOLLVSR-12(@14,@15)                            0462 00700000
         ST    @15,PDPVLSER+280(,@05)                              0462 00701000
*           IF PDPSFC=0&            /* IF NO SUB-FUNCTION CODE       */ 00702000
*               SFIRETCD^=0 THEN    /* AND SFI INITIALIZED           */ 00703000
@RF00461 LA    @05,280                                             0463 00704000
         AL    @05,OPWCOMWA(,RCORE)                                0463 00705000
         CLI   PDPSFC(@05),0                                       0463 00706000
         BNE   @RF00463                                            0463 00707000
         L     @15,MWAVVOLL                                        0463 00708000
         L     @14,SFIRETCD+16(,@15)                               0463 00709000
         LTR   @14,@14                                             0463 00710000
         BZ    @RF00463                                            0463 00711000
*             PDPSFI=ADDR(VVOLLSFI);/* THEN PT TO SFI IN VVOLL       */ 00712000
         LA    @15,VVOLLSFI(,@15)                                  0464 00713000
         ST    @15,PDPSFI(,@05)                                    0464 00714000
*           ELSE                    /* IF NOT                        */ 00715000
*             PDPSFI=0;             /* ZERO SFI PTR                  */ 00716000
         B     @RC00463                                            0465 00717000
@RF00463 L     @05,OPWCOMWA(,RCORE)                                0465 00718000
         SLR   @15,@15                                             0465 00719000
         ST    @15,PDPSFI+280(,@05)                                0465 00720000
*           IF PDPCODE=PP59&        /* IF VOLUME TIME STAMP ERROR    */ 00721000
*               (ACBOUT=ON|ACBRST=ON) THEN/* AND OPEN FOR OUTPUT   0466 00722000
*                                                            @XM01165*/ 00723000
@RC00463 L     @05,OPWCOMWA(,RCORE)                                0466 00724000
         CLI   PDPCODE+280(@05),59                                 0466 00725000
         BNE   @RF00466                                            0466 00726000
         TM    ACBOUT(RACB),B'00000010'                            0466 00727000
         BO    @RT00466                                            0466 00728000
         TM    ACBRST(RACB),B'00000100'                            0466 00729000
         BNO   @RF00466                                            0466 00730000
@RT00466 DS    0H                                                  0467 00731000
*             PDPCODE=PP60;         /* THEN IND TERMINATING ERROR    */ 00732000
         L     @05,OPWCOMWA(,RCORE)                                0467 00733000
         MVI   PDPCODE+280(@05),X'3C'                              0467 00734000
*           CALL PROBDT;            /* WRITE PROB DET MESSAGE        */ 00735000
@RF00466 BAL   @14,PROBDT                                          0468 00736000
*         END;                                                     0469 00737000
*       MWAVOLCT=0;                 /* IND ALL VOLUMES NOT MOUNTED   */ 00738000
@RF00456 SLR   @05,@05                                             0470 00739000
         ST    @05,MWAVOLCT                                        0470 00740000
*     END;                                                         0471 00741000
*   ELSE                            /* ELSE ALL VOLUMES MOUNTED      */ 00742000
*     MWAVOLCT=VVOLLNBR;            /* VOLUME COUNT EQUAL NO. IN   0472 00743000
*                                      VVOLL                         */ 00744000
*                                                                  0472 00745000
         B     @RC00454                                            0472 00746000
@RF00454 L     @05,MWAVVOLL                                        0472 00747000
         LH    @05,VVOLLNBR(,@05)                                  0472 00748000
         ST    @05,MWAVOLCT                                        0472 00749000
*   /*****************************************************************/ 00750000
*   /*                                                               */ 00751000
*   /* DETERMINE NUMBER OF VOLUMES MOUNTED, BUILD VMT AND MOVE VOLUME*/ 00752000
*   /* SERIAL NUMBERS MOUNTED INTO VMT.                              */ 00753000
*   /*                                                               */ 00754000
*   /*****************************************************************/ 00755000
*                                                                  0473 00756000
*   IF MWAVOLCT=0 THEN              /* IF ALL VOLUMES NOT MOUNTED    */ 00757000
@RC00454 L     @05,MWAVOLCT                                        0473 00758000
         LTR   @05,@05                                             0473 00759000
         BNZ   @RF00473                                            0473 00760000
*     DO MWAI=1 TO VVOLLNBR;        /* THEN COUNT NO. OF VOLS MNTED  */ 00761000
         LA    @05,1                                               0474 00762000
         B     @DE00474                                            0474 00763000
@DL00474 DS    0H                                                  0475 00764000
*       IF VVOLLMNT(MWAI)=ON THEN   /* IF VOLUME MOUNTED             */ 00765000
         MH    @05,@CH00033                                        0475 00766000
         L     @01,MWAVVOLL                                        0475 00767000
         ALR   @01,@05                                             0475 00768000
         TM    VVOLLMNT-12(@01),B'10000000'                        0475 00769000
         BNO   @RF00475                                            0475 00770000
*         MWAVOLCT=MWAVOLCT+1;      /* THEN INCR VOLUME COUNT        */ 00771000
         LA    @05,1                                               0476 00772000
         AL    @05,MWAVOLCT                                        0476 00773000
         ST    @05,MWAVOLCT                                        0476 00774000
*     END;                                                         0477 00775000
@RF00475 LA    @05,1                                               0477 00776000
         AL    @05,MWAI                                            0477 00777000
@DE00474 ST    @05,MWAI                                            0477 00778000
         L     @01,MWAVVOLL                                        0477 00779000
         CH    @05,VVOLLNBR(,@01)                                  0477 00780000
         BNH   @DL00474                                            0477 00781000
*   IF OPWFLGS1^=0 THEN             /* IF SYSTEM DATA SET            */ 00782000
@RF00473 CLI   OPWFLGS1(RCORE),0                                   0478 00783000
         BE    @RF00478                                            0478 00784000
*     MWAGFSP=CATPOOL;              /* THEN SET SQA SUBPOOL NUMBER   */ 00785000
         MVI   MWAGFSP,X'E7'                                       0479 00786000
*   ELSE                            /* NO, CHK FOR GSR       @Y30DSDP*/ 00787000
*     IF OPWGSR=ON|OPWCBIC=ON THEN  /* CHK FOR GSR OR CBIC   @ZA16012*/ 00788000
         B     @RC00478                                            0480 00789000
@RF00478 TM    OPWGSR(RCORE),B'01000000'                           0480 00790000
         BO    @RT00480                                            0480 00791000
         TM    OPWCBIC(RCORE),B'10000000'                          0480 00792000
         BNO   @RF00480                                            0480 00793000
@RT00480 DS    0H                                                  0481 00794000
*       MWAGFSP=GSRPOOL;            /* SET GSR SUBPOOL       @Y30DSDP*/ 00795000
         MVI   MWAGFSP,X'F1'                                       0481 00796000
*     ELSE                          /* NOT SYSTEM DATA SET           */ 00797000
*       MWAGFSP=VMTPOOL;            /* SET VMT SUBPOOL               */ 00798000
         B     @RC00480                                            0482 00799000
@RF00480 MVI   MWAGFSP,X'FC'                                       0482 00800000
*   MWAGFLEN=LENGTH(IDAVMT)+LENGTH(VMTVOL)*MWAVOLCT;/* CALCULATE   0483 00801000
*                                      LENGTH OF VMT                 */ 00802000
@RC00480 DS    0H                                                  0483 00803000
@RC00478 L     @05,MWAVOLCT                                        0483 00804000
         SLA   @05,4                                               0483 00805000
         LA    @05,16(,@05)                                        0483 00806000
         STCM  @05,7,MWAGFLEN                                      0483 00807000
*   CALL GETCORE;                   /* OBTAIN STORAGE FOR VMT        */ 00808000
         BAL   @14,GETCORE                                         0484 00809000
*   MWAVMT=MWAGFADR;                /* PT TO VMT                     */ 00810000
         L     @15,MWAGFADR                                        0485 00811000
         ST    @15,MWAVMT                                          0485 00812000
*   VMTID=IDXVMT;                   /* INIT CB ID IN VMT             */ 00813000
         MVI   VMTID(@15),B'00010010'                              0486 00814000
*   VMTLEN=MWAGFLEN;                /* INIT LENGTH IN VMT            */ 00815000
         L     @14,MWAGFLEN-1                                      0487 00816000
         LA    @14,0(,@14)                                         0487 00817000
         STH   @14,VMTLEN(,@15)                                    0487 00818000
*   VMTDEV=MWADEV;                  /* INIT DEVICE TYPE IN VMT       */ 00819000
         MVC   VMTDEV(3,@15),MWADEV                                0488 00820000
*   MWAJ=0;                         /* INIT INDEX FOR VMTVOL         */ 00821000
         SLR   MWAJ,MWAJ                                           0489 00822000
*   DO MWAI=1 TO VVOLLNBR;          /* SCAN ENTRIES IN VVOLL         */ 00823000
         LA    @15,1                                               0490 00824000
         B     @DE00490                                            0490 00825000
@DL00490 DS    0H                                                  0491 00826000
*     IF VVOLLMNT(MWAI)=ON THEN     /* IF VOLUME MOUNTED             */ 00827000
         L     @14,MWAVVOLL                                        0491 00828000
         MH    @15,@CH00033                                        0491 00829000
         ST    @15,@TF00001                                        0491 00830000
         ALR   @15,@14                                             0491 00831000
         TM    VVOLLMNT-12(@15),B'10000000'                        0491 00832000
         BNO   @RF00491                                            0491 00833000
*       DO;                         /* THEN ADD ENTRY TO VMT         */ 00834000
*         MWAJ=MWAJ+1;              /* INCR INDEX FOR VMTVOL         */ 00835000
         AL    MWAJ,@CF00053                                       0493 00836000
*         VMTVLSER(MWAJ)=VVOLLVSR(MWAI);/* MOVE VOLSER INTO VMT      */ 00837000
         L     @15,MWAVMT                                          0494 00838000
         LR    @11,MWAJ                                            0494 00839000
         SLA   @11,4                                               0494 00840000
         ST    @11,@TF00002                                        0494 00841000
         ALR   @11,@15                                             0494 00842000
         LR    @10,@14                                             0494 00843000
         AL    @10,@TF00001                                        0494 00844000
         MVC   VMTVLSER-16(6,@11),VVOLLVSR-12(@10)                 0494 00845000
*         VMTUCB(MWAJ)=VVOLLUCB(MWAI);/* MOVE UCB PTR INTO VMT       */ 00846000
         L     @01,@TF00001                                        0495 00847000
         L     @14,VVOLLUCB-12(@01,@14)                            0495 00848000
         L     @01,@TF00002                                        0495 00849000
         ST    @14,VMTUCB-16(@01,@15)                              0495 00850000
*         VMTOPEN(MWAJ)=ON;         /* IND VOL BEING PROCESSED BY  0496 00851000
*                                      OPEN                          */ 00852000
         AL    @15,@TF00002                                        0496 00853000
         OI    VMTOPEN-16(@15),B'10000000'                         0496 00854000
*       END;                                                       0497 00855000
*   END;                                                           0498 00856000
@RF00491 LA    @15,1                                               0498 00857000
         AL    @15,MWAI                                            0498 00858000
@DE00490 ST    @15,MWAI                                            0498 00859000
         L     @14,MWAVVOLL                                        0498 00860000
         CH    @15,VVOLLNBR(,@14)                                  0498 00861000
         BNH   @DL00490                                            0498 00862000
*   VMTNOVOL=MWAJ;                  /* SET NO. OF ENTRIES IN VMT     */ 00863000
         L     @15,MWAVMT                                          0499 00864000
         STH   MWAJ,VMTNOVOL(,@15)                                 0499 00865000
*   VMTNXT=BIBVMT;                  /* PT TO FIRST VMT ON CHAIN      */ 00866000
         L     @05,OPWBIB(,RCORE)                                  0500 00867000
         L     @14,BIBVMT(,@05)                                    0500 00868000
         ST    @14,VMTNXT(,@15)                                    0500 00869000
*   BIBVMT=MWAVMT;                  /* ADD NEW VMT TO CHAIN          */ 00870000
         ST    @15,BIBVMT(,@05)                                    0501 00871000
*   RFY                                                            0502 00872000
*     RET RSTD;                                                    0502 00873000
*   RET=MWASAVEB;                   /* RESTORE RETURN ADDRESS        */ 00874000
         L     RET,MWASAVEB                                        0503 00875000
*   RFY                                                            0504 00876000
*     RET UNRSTD;                                                  0504 00877000
*   END NEWDEV;                                                    0505 00878000
@EL00003 DS    0H                                                  0505 00879000
@EF00003 DS    0H                                                  0505 00880000
@ER00003 BR    @14                                                 0505 00881000
*OLDDEV:                                                           0506 00882000
*                                                                  0506 00883000
*/********************************************************************/ 00884000
*/*                                                                  */ 00885000
*/*   FUNCTION: MOUNT VOLUMES FOR EXISTING DEVICE TYPE.  THIS        */ 00886000
*/*             ROUTINE COMPARES THE VOLUME SERIAL NUMBERS IN THE    */ 00887000
*/*             JFCB TO THE VOLUME SERIAL NUMBERS IN THE VMT FOR     */ 00888000
*/*             THE SAME DEVICE TYPE. THE VOLUME SERIAL NUMBERS FOR  */ 00889000
*/*             VOLUMES THAT ARE NOT MOUNTED (THOSE NOT IN VMT) ARE  */ 00890000
*/*             MOVED INTO THE VVOLL.  IDA0192V IS CALLED TO MOUNT   */ 00891000
*/*             THE VOLUMES.  UPON RETURN FROM IDA0192V, A NEW VMT   */ 00892000
*/*             IS BUILT, THE OLD VMT IS COPIED INTO IT, AND THE     */ 00893000
*/*             VOLUME SERIAL NUMBERS FOR THE NEW VOLUMES THAT WERE  */ 00894000
*/*             MOUNTED ARE ADDED TO THE NEW VMT.  THE NEW VMT       */ 00895000
*/*             REPLACES THE OLD ONE AND THE OLD VMT IS FREED.       */ 00896000
*/*                                                                  */ 00897000
*/*   INPUTS:   1. PTR TO VMT IN MWAVMT.                             */ 00898000
*/*             2. PTR TO JFCB IN MWAJFCB.                           */ 00899000
*/*             3. PTR TO VVOLL IN MWAVVOLL.                         */ 00900000
*/*             4. NUMBER OF VOLUME SERIAL NUMBERS IN JFCB AND       */ 00901000
*/*                JFCB EXTENSIONS IN VVOLLNBR.                      */ 00902000
*/*                                                                  */ 00903000
*/*   OUTPUTS:  1. NEW VOLUME SERIAL NUMBERS FOR DEVICE TYPE ARE     */ 00904000
*/*               ADDED TO VMT.                                      */ 00905000
*/*                                                                  */ 00906000
*/********************************************************************/ 00907000
*                                                                  0506 00908000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0506 00909000
OLDDEV   DS    0H                                                  0507 00910000
*   RFY                                                            0507 00911000
*     RET RSTD;                                                    0507 00912000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 00913000
         ST    RET,MWASAVEB                                        0508 00914000
*   RFY                                                            0509 00915000
*     RET UNRSTD;                                                  0509 00916000
*                                                                  0509 00917000
*   /*****************************************************************/ 00918000
*   /*                                                               */ 00919000
*   /* COMPARE VOLUME SERIAL NUMBERS IN JFCB TO VMT. MOVE NEW VOLUME */ 00920000
*   /* SERIAL NUMBERS FOR VOLUMES NOT MOUNTED INTO VOLL AND CALL     */ 00921000
*   /* IDA0192V TO MOUNT THEM.                                       */ 00922000
*   /*                                                               */ 00923000
*   /*****************************************************************/ 00924000
*                                                                  0510 00925000
*   RFY                                                            0510 00926000
*     VOLSER BASED(MWAPTRA);                                       0510 00927000
*   MWAPTRA=ADDR(JFCBVOLS);         /* PT TO FIRST VOL SER NO. IN  0511 00928000
*                                      JFCB                          */ 00929000
         L     @05,MWAJFCB                                         0511 00930000
         LA    @05,JFCBVOLS(,@05)                                  0511 00931000
         ST    @05,MWAPTRA                                         0511 00932000
*   MWANEXTC=JFCBPNVL;              /* SET COUNT FOR NEXT JFCB EXT   */ 00933000
         MVC   MWANEXTC(4),@CF00145                                0512 00934000
*   MWAVOLCT=0;                     /* SET VOLUME CTR TO ZERO        */ 00935000
         SLR   @05,@05                                             0513 00936000
         ST    @05,MWAVOLCT                                        0513 00937000
*   DO MWAI=1 TO VVOLLNBR;          /* SCAN VOL SER NOS. IN JFCB     */ 00938000
         LA    @05,1                                               0514 00939000
         B     @DE00514                                            0514 00940000
@DL00514 DS    0H                                                  0515 00941000
*     IF MWAI>MWANEXTC THEN         /* IF NEXT JFCB EXT NEEDED       */ 00942000
         L     @15,MWANEXTC                                        0515 00943000
         CR    @05,@15                                             0515 00944000
         BNH   @RF00515                                            0515 00945000
*       DO;                         /* THEN READ NEXT EXTENSION      */ 00946000
*         IF MWANEXTC=JFCBPNVL THEN /* IF FIRST EXTENSION            */ 00947000
         C     @15,@CF00145                                        0517 00948000
         BNE   @RF00517                                            0517 00949000
*           MWAJFTTR(1:3)=JFCBEXAD; /* SET DISK ADDR OF FIRST EXT    */ 00950000
         L     @05,MWAJFCB                                         0518 00951000
         MVC   MWAJFTTR(3),JFCBEXAD(@05)                           0518 00952000
*         ELSE                      /* ELSE PROCESSING EXTENSION     */ 00953000
*           MWAJFTTR(1:3)=JFCBXTTR; /* SET DISK ADDR FOR NEXT JFCB 0519 00954000
*                                      EXTENSION                     */ 00955000
         B     @RC00517                                            0519 00956000
@RF00517 L     @05,MWAJFCB                                         0519 00957000
         MVC   MWAJFTTR(3),JFCBXTTR(@05)                           0519 00958000
*         CALL READJFCB;            /* READ JFCB EXTENSION           */ 00959000
@RC00517 BAL   @14,READJFCB                                        0520 00960000
*         MWANEXTC=MWANEXTC+JFCBXNVL;/* INCR CNT FOR NEXT EXTENSION  */ 00961000
         LA    @05,15                                              0521 00962000
         AL    @05,MWANEXTC                                        0521 00963000
         ST    @05,MWANEXTC                                        0521 00964000
*         MWAPTRA=ADDR(JFCBXVOL);   /* PT TO FIRST VOL SER NO. IN  0522 00965000
*                                      JFCB EXTENSION                */ 00966000
         L     @05,MWAJFCB                                         0522 00967000
         LA    @05,JFCBXVOL(,@05)                                  0522 00968000
         ST    @05,MWAPTRA                                         0522 00969000
*       END;                                                       0523 00970000
*     DO MWAJ=1 TO VMTNOVOL WHILE(VOLSER^=VMTVLSER(MWAJ));/* SCAN  0524 00971000
*                                      VMT, LOOK FOR VOL SER NO.     */ 00972000
@RF00515 LA    MWAJ,1                                              0524 00973000
         B     @DE00524                                            0524 00974000
@DL00524 LR    @15,MWAJ                                            0524 00975000
         SLA   @15,4                                               0524 00976000
         L     @01,MWAPTRA                                         0524 00977000
         L     @14,MWAVMT                                          0524 00978000
         ALR   @14,@15                                             0524 00979000
         CLC   VOLSER(6,@01),VMTVLSER-16(@14)                      0524 00980000
         BE    @DC00524                                            0524 00981000
*     END;                                                         0525 00982000
         AL    MWAJ,@CF00053                                       0525 00983000
@DE00524 L     @01,MWAVMT                                          0525 00984000
         CH    MWAJ,VMTNOVOL(,@01)                                 0525 00985000
         BNH   @DL00524                                            0525 00986000
@DC00524 DS    0H                                                  0526 00987000
*     IF MWAJ>VMTNOVOL THEN         /* IF VOL SER NO. NOT IN VMT     */ 00988000
         L     @01,MWAVMT                                          0526 00989000
         CH    MWAJ,VMTNOVOL(,@01)                                 0526 00990000
         BNH   @RF00526                                            0526 00991000
*       DO;                         /* THEN ADD VOL SER NO. TO VVOLL */ 00992000
*         MWAVOLCT=MWAVOLCT+1;      /* INCR VOL CTR                  */ 00993000
         LA    @05,1                                               0528 00994000
         AL    @05,MWAVOLCT                                        0528 00995000
         ST    @05,MWAVOLCT                                        0528 00996000
*         VVOLLVSR(MWAVOLCT)=VOLSER;/* MOVE VOL SER NO. FROM JFCB TO    00997000
*                                      VVOLL                         */ 00998000
         MH    @05,@CH00033                                        0529 00999000
         L     @01,MWAVVOLL                                        0529 01000000
         ALR   @01,@05                                             0529 01001000
         L     @05,MWAPTRA                                         0529 01002000
         MVC   VVOLLVSR-12(6,@01),VOLSER(@05)                      0529 01003000
*       END;                                                       0530 01004000
*     MWAPTRA=MWAPTRA+LENGTH(VOLSER);/* PT TO NEXT VOL SER NO. IN  0531 01005000
*                                      JFCB                          */ 01006000
@RF00526 LA    @05,6                                               0531 01007000
         AL    @05,MWAPTRA                                         0531 01008000
         ST    @05,MWAPTRA                                         0531 01009000
*   END;                                                           0532 01010000
         LA    @05,1                                               0532 01011000
         AL    @05,MWAI                                            0532 01012000
@DE00514 ST    @05,MWAI                                            0532 01013000
         L     @01,MWAVVOLL                                        0532 01014000
         CH    @05,VVOLLNBR(,@01)                                  0532 01015000
         BNH   @DL00514                                            0532 01016000
*   IF MWAVOLCT>0 THEN              /* IF ANY VOLUMES TO MOUNT       */ 01017000
         L     @05,MWAVOLCT                                        0533 01018000
         LTR   @05,@05                                             0533 01019000
         BNP   @RF00533                                            0533 01020000
*     DO;                           /* THEN MOUNT NEW VOLUMES        */ 01021000
*       VVOLLNBR=MWAVOLCT;          /* SET NO. OF VOLUMES IN VVOLL   */ 01022000
         L     @15,MWAVVOLL                                        0535 01023000
         STH   @05,VVOLLNBR(,@15)                                  0535 01024000
*       RFY                                                        0536 01025000
*         R1 RSTD;                                                 0536 01026000
*       R1=MWAVVOLL;                /* PT TO VVOLL                   */ 01027000
         LR    R1,@15                                              0537 01028000
*       RFY                                                        0538 01029000
*         R1 UNRSTD;                                               0538 01030000
*       RSTATRTN=IDAR192V;          /* INDIC 192V CONTROL    @Y30DSDP*/ 01031000
         L     @05,OPWCOMWA(,RCORE)                                0539 01032000
         MVC   RSTATRTN(4,@05),@CC04591                            0539 01033000
*       CALL IDA0192V;              /* CALL VOLUME MOUNT             */ 01034000
         L     @15,@CV04841                                        0540 01035000
         BALR  @14,@15                                             0540 01036000
*       RSTATRTN=IDAR192F;          /* INDIC 192F CONTROL    @Y30DSDP*/ 01037000
         L     @05,OPWCOMWA(,RCORE)                                0541 01038000
         MVC   RSTATRTN(4,@05),@CC04581                            0541 01039000
*       RFY                                                        0542 01040000
*         RETCODE RSTD;                                            0542 01041000
*       IF RETCODE>0 THEN           /* IF ALL VOLS NOT MOUNTED       */ 01042000
         LTR   RETCODE,RETCODE                                     0543 01043000
         BNP   @RF00543                                            0543 01044000
*         DO;                       /* THEN IND ALL VOLS NOT MOUNTED */ 01045000
*           IF RETCODE>4 THEN       /* IF ERROR DETECTED BY VOL MNT  */ 01046000
         C     RETCODE,@CF00095                                    0545 01047000
         BNH   @RF00545                                            0545 01048000
*             DO;                   /* THEN DETERMINE TYPE OF ERROR  */ 01049000
*               RFY                                                0547 01050000
*                 RETCODE UNRSTD;                                  0547 01051000
*               PDPCODE=VVOLLPDC;   /* LD PROB DET CODE              */ 01052000
         AL    @05,@CF04927                                        0548 01053000
         L     @15,MWAVVOLL                                        0548 01054000
         IC    @14,VVOLLPDC(,@15)                                  0548 01055000
         STC   @14,PDPCODE(,@05)                                   0548 01056000
*               PDPSFC=VVOLLSFC;    /* LD SUB-FUNCTION CODE          */ 01057000
         IC    @14,VVOLLSFC(,@15)                                  0549 01058000
         STC   @14,PDPSFC(,@05)                                    0549 01059000
*               IF PDPCODE=PP116|   /* IF I/O ERROR READING DSCB     */ 01060000
*                   PDPCODE=PP59 THEN/* OR VOL TIME STAMP ERROR      */ 01061000
         CLI   PDPCODE(@05),116                                    0550 01062000
         BE    @RT00550                                            0550 01063000
         CLI   PDPCODE(@05),59                                     0550 01064000
         BNE   @RF00550                                            0550 01065000
@RT00550 DS    0H                                                  0551 01066000
*                 PDPVLSER=ADDR(VVOLLVSR(VVOLLCNT));/* PT TO BAD   0551 01067000
*                                      VOLUME                        */ 01068000
         L     @05,OPWCOMWA(,RCORE)                                0551 01069000
         L     @15,MWAVVOLL                                        0551 01070000
         SLR   @14,@14                                             0551 01071000
         IC    @14,VVOLLCNT(,@15)                                  0551 01072000
         MH    @14,@CH00033                                        0551 01073000
         LA    @15,VVOLLVSR-12(@14,@15)                            0551 01074000
         ST    @15,PDPVLSER+280(,@05)                              0551 01075000
*               IF PDPSFC=0&        /* IF NO SUB-FUNCTION CODE       */ 01076000
*                   SFIRETCD^=0 THEN/* AND SFI INITIALIZED           */ 01077000
@RF00550 LA    @05,280                                             0552 01078000
         AL    @05,OPWCOMWA(,RCORE)                                0552 01079000
         CLI   PDPSFC(@05),0                                       0552 01080000
         BNE   @RF00552                                            0552 01081000
         L     @15,MWAVVOLL                                        0552 01082000
         L     @14,SFIRETCD+16(,@15)                               0552 01083000
         LTR   @14,@14                                             0552 01084000
         BZ    @RF00552                                            0552 01085000
*                 PDPSFI=ADDR(VVOLLSFI);/* THEN PT TO SFI IN VVOLL   */ 01086000
         LA    @15,VVOLLSFI(,@15)                                  0553 01087000
         ST    @15,PDPSFI(,@05)                                    0553 01088000
*               ELSE                /* OTHERWISE                     */ 01089000
*                 PDPSFI=0;         /* ZERO SFI PTR                  */ 01090000
         B     @RC00552                                            0554 01091000
@RF00552 L     @05,OPWCOMWA(,RCORE)                                0554 01092000
         SLR   @15,@15                                             0554 01093000
         ST    @15,PDPSFI+280(,@05)                                0554 01094000
*               IF PDPCODE=PP59&    /* IF VOLUME TIME STAMP ERROR    */ 01095000
*                   (ACBOUT=ON|ACBRST=ON) THEN/* AND OPEN FOR OUTPUT    01096000
*                                                            @XM01165*/ 01097000
@RC00552 L     @05,OPWCOMWA(,RCORE)                                0555 01098000
         CLI   PDPCODE+280(@05),59                                 0555 01099000
         BNE   @RF00555                                            0555 01100000
         TM    ACBOUT(RACB),B'00000010'                            0555 01101000
         BO    @RT00555                                            0555 01102000
         TM    ACBRST(RACB),B'00000100'                            0555 01103000
         BNO   @RF00555                                            0555 01104000
@RT00555 DS    0H                                                  0556 01105000
*                 PDPCODE=PP60;     /* THEN IND TERMINATING ERROR    */ 01106000
         L     @05,OPWCOMWA(,RCORE)                                0556 01107000
         MVI   PDPCODE+280(@05),X'3C'                              0556 01108000
*               CALL PROBDT;        /* WRITE PROB DET MESSAGE        */ 01109000
@RF00555 BAL   @14,PROBDT                                          0557 01110000
*             END;                                                 0558 01111000
*           MWAVOLCT=0;             /* IND ALL VOLUMES NOT MOUNTED   */ 01112000
@RF00545 SLR   @05,@05                                             0559 01113000
         ST    @05,MWAVOLCT                                        0559 01114000
*         END;                                                     0560 01115000
*       IF MWAVOLCT=0 THEN          /* IF ALL VOLUMES NOT MOUNTED    */ 01116000
@RF00543 L     @05,MWAVOLCT                                        0561 01117000
         LTR   @05,@05                                             0561 01118000
         BNZ   @RF00561                                            0561 01119000
*         DO;                       /* THEN COUNT NO. OF VOLS MNTED  */ 01120000
*           DO MWAI=1 TO VVOLLNBR;  /* COUNT MOUNTED VOLS IN VVOLL   */ 01121000
         LA    @05,1                                               0563 01122000
         B     @DE00563                                            0563 01123000
@DL00563 DS    0H                                                  0564 01124000
*             IF VVOLLMNT(MWAI)=ON THEN/* IF VOLUME MOUNTED          */ 01125000
         MH    @05,@CH00033                                        0564 01126000
         L     @01,MWAVVOLL                                        0564 01127000
         ALR   @01,@05                                             0564 01128000
         TM    VVOLLMNT-12(@01),B'10000000'                        0564 01129000
         BNO   @RF00564                                            0564 01130000
*               MWAVOLCT=MWAVOLCT+1;/* THEN INCR VOLUME COUNT        */ 01131000
         LA    @05,1                                               0565 01132000
         AL    @05,MWAVOLCT                                        0565 01133000
         ST    @05,MWAVOLCT                                        0565 01134000
*           END;                                                   0566 01135000
@RF00564 LA    @05,1                                               0566 01136000
         AL    @05,MWAI                                            0566 01137000
@DE00563 ST    @05,MWAI                                            0566 01138000
         L     @01,MWAVVOLL                                        0566 01139000
         CH    @05,VVOLLNBR(,@01)                                  0566 01140000
         BNH   @DL00563                                            0566 01141000
*         END;                                                     0567 01142000
*                                                                  0567 01143000
*       /*************************************************************/ 01144000
*       /*                                                           */ 01145000
*       /* BUILD A NEW VMT, COPY OLD VMT INTO IT, ADD NEW VOLUME     */ 01146000
*       /* SERIAL NUMBERS AND REPLACE OLD VMT WITH NEW VMT           */ 01147000
*       /*                                                           */ 01148000
*       /*************************************************************/ 01149000
*                                                                  0568 01150000
*       IF MWAVOLCT>0 THEN          /* IF ANY VOLUMES MOUNTED        */ 01151000
@RF00561 L     @05,MWAVOLCT                                        0568 01152000
         LTR   @05,@05                                             0568 01153000
