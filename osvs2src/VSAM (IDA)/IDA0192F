*                 MWAFIX=ON;        /* THEN WE ARE DONE              */ 02098000
         OI    MWAFIX,B'00100000'                                  0861 02099000
*               ELSE                /* WE NEED TO RETRY              */ 02100000
*                 MWAPFECB=0;       /* ZERO ECB FOR RETRY            */ 02101000
         B     @RC00860                                            0862 02102000
@RF00860 SLR   @05,@05                                             0862 02103000
