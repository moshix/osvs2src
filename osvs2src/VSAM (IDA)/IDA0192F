*   RET=MWASAVEB;                   /* RESTORE RETURN ADDRESS        */ 02927000
         L     RET,MWASAVEB                                        1108 02928000
*   RFY                                                            1109 02929000
*     RET UNRSTD;                                                  1109 02930000
*   END CONPATH;                                                   1110 02931000
@EL00011 DS    0H                                                  1110 02932000
@EF00011 DS    0H                                                  1110 02933000
@ER00011 BR    @14                                                 1110 02934000
*BLDAMBL:                                                          1111 02935000
*                                                                  1111 02936000
*/********************************************************************/ 02937000
*/*                                                                  */ 02938000
*/*   FUNCTION: BUILDS AN AMBL AND INITIALIZES IT.  CALLS VATUPD TO  */ 02939000
*/*             ADD AN ENTRY TO VATS FOR AMBL.  CALLS CHNAMBL TO     */ 02940000
*/*             ADD AMBL TO APPROPRIATE CHAIN.                       */ 02941000
*/*                                                                  */ 02942000
*/*   INPUTS:   1. PTR TO OPW IN RCORE.                              */ 02943000
*/*             2. IF DUMMY AMBL FOR BASE OR UPGRADE, THEN MWADAMBL  */ 02944000
*/*                 IS SET ON.                                       */ 02945000
*/*                                                                  */ 02946000
*/*   OUTPUTS:  1. PTR TO AMBL IN MWAXAMBL.                          */ 02947000
*/*                                                                  */ 02948000
*/********************************************************************/ 02949000
*                                                                  1111 02950000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1111 02951000
BLDAMBL  DS    0H                                                  1112 02952000
*   RFY                                                            1112 02953000
*     RET RSTD;                                                    1112 02954000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 02955000
         ST    RET,MWASAVEB                                        1113 02956000
*   RFY                                                            1114 02957000
*     RET UNRSTD;                                                  1114 02958000
*   OPWVSMPL=OPWVSMPL&&OPWVSMPL;    /* ZERO VSM PARAMETER LIST       */ 02959000
         XC    OPWVSMPL(16,RCORE),OPWVSMPL(RCORE)                  1115 02960000
*   IF MWADAMBL=ON|OPWUPGR=ON THEN  /* IF DUMMY AMBL FOR BASE OR   1116 02961000
*                                      UPGRADE                       */ 02962000
         TM    MWADAMBL,B'01000000'                                1116 02963000
         BO    @RT01116                                            1116 02964000
         L     @05,OPWCURPT(,RCORE)                                1116 02965000
         TM    OPWUPGR(@05),B'00100000'                            1116 02966000
         BNO   @RF01116                                            1116 02967000
@RT01116 DS    0H                                                  1117 02968000
*     DO;                           /* THEN INIT VSM PARM LIST FOR 1117 02969000
*                                      PROT SPH BLK                  */ 02970000
*       OPWVMANC=ADDR(IDABIB);      /* INIT PTR TO ANCHOR BLOCK      */ 02971000
         L     @05,OPWBIB(,RCORE)                                  1118 02972000
         ST    @05,OPWVMANC(,RCORE)                                1118 02973000
