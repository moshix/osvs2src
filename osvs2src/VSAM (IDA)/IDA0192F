*   RFY                                                            1002 02556000
*     RET UNRSTD;                                                  1002 02557000
*   END OPNUPGR;                                                   1003 02558000
         B     @EL00008                                            1003 02559000
*UPDSSL:                                                           1004 02560000
*                                                                  1004 02561000
*/********************************************************************/ 02562000
*/*                                                                  */ 02563000
*/*   FUNCTION: UPDATE SWAP SAVE LIST (SSL).  DETERMINES IF THERE IS */ 02564000
*/*             AVAILABLE ENTRY IN SSL.  IF THERE IS AN ENTRY, THE   */ 02565000
*/*             CONTENTS OF MWASWSAV ARE PLACED IN THE ENTRY AND THE */ 02566000
*/*             SSLACEN FIELD IS UPDATED.  IF THER IS NOT AN         */ 02567000
*/*             AVAILABLE ENTRY, A NEW SSL IS BUILT AND INITIALIZED, */ 02568000
*/*             THE CONTENTS OF MWASWSAV ARE LOADED INTO THE FIRST   */ 02569000
*/*             ENTRY, AND THE NEW SSL IS ADDED TO THE SSL CHAIN.    */ 02570000
*/*                                                                  */ 02571000
*/*   INPUTS:   1. CONTENTS OF NEW SSL ENTRY IN MWASWSAV.            */ 02572000
*/*             2. PTR TO SSL CHAIN IN OPWSSL.                       */ 02573000
*/*                                                                  */ 02574000
*/*   OUTPUTS:  1. NEW ENTRY ADDED TO SSL.                           */ 02575000
*/*                                                                  */ 02576000
*/********************************************************************/ 02577000
*                                                                  1004 02578000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1004 02579000
UPDSSL   DS    0H                                                  1005 02580000
*   RFY                                                            1005 02581000
*     RET RSTD;                                                    1005 02582000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 02583000
         ST    RET,MWASAVEB                                        1006 02584000
*   RFY                                                            1007 02585000
*     RET UNRSTD;                                                  1007 02586000
*   IF SSLACEN>=DIM(SSLENTRY) THEN  /* IF SSL FULL                   */ 02587000
         L     @05,OPWSSL(,RCORE)                                  1008 02588000
         LH    @05,SSLACEN(,@05)                                   1008 02589000
         C     @05,@CF00050                                        1008 02590000
         BL    @RF01008                                            1008 02591000
*     DO;                           /* THEN BUILD A NEW SSL          */ 02592000
*       MWAGFSP=WKPOOL;             /* INIT SUBPOOL                  */ 02593000
         MVI   MWAGFSP,X'FC'                                       1010 02594000
*       MWAGFLEN=LENGTH(IDASSL);    /* INIT LENGTH                   */ 02595000
         MVC   MWAGFLEN(3),@CF01418+1                              1011 02596000
*       CALL GETCORE;               /* OBTAIN STORAGE FOR SSL        */ 02597000
         BAL   @14,GETCORE                                         1012 02598000
*       RFY                                                        1013 02599000
*         IDASSL BASED(MWAGFADR);                                  1013 02600000
*       SSLNXPTR=OPWSSL;            /* PT TO FIRST SSL ON CHAIN      */ 02601000
         L     @05,MWAGFADR                                        1014 02602000
         L     @15,OPWSSL(,RCORE)                                  1014 02603000
         ST    @15,SSLNXPTR(,@05)                                  1014 02604000
*       SSLSUBPL=WKPOOL;            /* INIT SUBPOOL IN SSL           */ 02605000
         MVI   SSLSUBPL(@05),X'FC'                                 1015 02606000
*       SSLLENTH=MWAGFLEN;          /* INIT LENGTH OF SSL            */ 02607000
         MVC   SSLLENTH(3,@05),MWAGFLEN                            1016 02608000
*       SSLID=IDASSLID;             /* INIT EBCDIC ID                */ 02609000
         MVC   SSLID(8,@05),@CC02838                               1017 02610000
*       OPWSSL=MWAGFADR;            /* ADD SSL TO CHAIN              */ 02611000
         ST    @05,OPWSSL(,RCORE)                                  1018 02612000
*       RFY                                                        1019 02613000
*         IDASSL BASED(OPWSSL);                                    1019 02614000
*     END;                                                         1020 02615000
*   SSLACEN=SSLACEN+1;              /* INCR NO. OF ACTIVE ENTRIES    */ 02616000
@RF01008 L     @05,OPWSSL(,RCORE)                                  1021 02617000
         LA    @15,1                                               1021 02618000
         AH    @15,SSLACEN(,@05)                                   1021 02619000
         STH   @15,SSLACEN(,@05)                                   1021 02620000
*   SSLENTRY(SSLACEN)=MWASWSAV;     /* ADD NEW ENTRY TO SSL          */ 02621000
         SLA   @15,3                                               1022 02622000
         ALR   @05,@15                                             1022 02623000
         MVC   SSLENTRY-8(8,@05),MWASWSAV                          1022 02624000
*   RFY                                                            1023 02625000
*     RET RSTD;                                                    1023 02626000
*   RET=MWASAVEB;                   /* RESTORE RETURN ADDRESS        */ 02627000
         L     RET,MWASAVEB                                        1024 02628000
*   RFY                                                            1025 02629000
*     RET UNRSTD;                                                  1025 02630000
*   END UPDSSL;                                                    1026 02631000
@EL00009 DS    0H                                                  1026 02632000
@EF00009 DS    0H                                                  1026 02633000
@ER00009 BR    @14                                                 1026 02634000
*OPNPATH:                                                          1027 02635000
*                                                                  1027 02636000
*/********************************************************************/ 02637000
*/*                                                                  */ 02638000
*/*   FUNCTION: OPENS PATH AIX.  CALLS CONPATH TO DETERMINE IF PATH  */ 02639000
*/*             AIX ALREADY OPEN AS A PATH OR UPGRADE AIX. CALLS     */ 02640000
*/*             BLDAMBL TO BUILD PATH AMBL. CALLS IDA0192B TO        */ 02641000
*/*             OPEN PATH AIX.                                       */ 02642000
*/*                                                                  */ 02643000
*/*   INPUTS:   1. PTR TO OPW IN RCORE.                              */ 02644000
*/*                                                                  */ 02645000
*/*   OUTPUTS:  1. OPENED PATH AIX.                                  */ 02646000
*/*                                                                  */ 02647000
*/********************************************************************/ 02648000
*                                                                  1027 02649000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1027 02650000
OPNPATH  DS    0H                                                  1028 02651000
*   RFY                                                            1028 02652000
*     RET RSTD;                                                    1028 02653000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 02654000
         ST    RET,MWASAVEA                                        1029 02655000
*   RFY                                                            1030 02656000
*     RET UNRSTD;                                                  1030 02657000
*   PDPFUNC=PDF90;                  /* SET FUNCTION CODE             */ 02658000
         LA    @05,280                                             1031 02659000
         AL    @05,OPWCOMWA(,RCORE)                                1031 02660000
         MVI   PDPFUNC(@05),X'5A'                                  1031 02661000
*   PDPDSTYP=PDPPATH;               /* IND PATH DATA SET             */ 02662000
         MVI   PDPDSTYP(@05),X'03'                                 1032 02663000
*   OPWCURPT=ADDR(OPWPTAIX);        /* IND OPEN PATH AIX             */ 02664000
         LA    @05,OPWPTAIX(,RCORE)                                1033 02665000
         ST    @05,OPWCURPT(,RCORE)                                1033 02666000
*   CALL CONPATH;                   /* CONNECT PROCESSING FOR PATH 1034 02667000
*                                      AIX                           */ 02668000
         BAL   @14,CONPATH                                         1034 02669000
*   OPWCSTRN=OPWSTRNO;              /* INIT CURRENT STR NO.FOR PATH  */ 02670000
         IC    @05,OPWSTRNO(,RCORE)                                1035 02671000
         STC   @05,OPWCSTRN(,RCORE)                                1035 02672000
*   OPWPATH=ON;                     /* INDICATE OPEN PATH AIX        */ 02673000
         L     @05,OPWCURPT(,RCORE)                                1036 02674000
         OI    OPWPATH(@05),B'01000000'                            1036 02675000
*   CALL BLDAMBL;                   /* BUILD PATH AMBL               */ 02676000
         BAL   @14,BLDAMBL                                         1037 02677000
*   OPWBAMBL->AMBLXPT=OPWPAMBL;     /* INIT PATH-AMBL PTR IN BASE  1038 02678000
*                                      AMBL                          */ 02679000
         L     @05,OPWBAMBL(,RCORE)                                1038 02680000
         L     @15,OPWPAMBL(,RCORE)                                1038 02681000
         ST    @15,AMBLXPT(,@05)                                   1038 02682000
*   AMBLXPT=OPWBAMBL;               /* INIT BASE-AMBL PTR IN PATH  1039 02683000
*                                      AMBL                          */ 02684000
         L     @14,MWAXAMBL                                        1039 02685000
         ST    @05,AMBLXPT(,@14)                                   1039 02686000
*   RACB=OPWUACB;                   /* INIT ACB PTR TO USER ACB      */ 02687000
         L     RACB,OPWUACB(,RCORE)                                1040 02688000
*   ACBAMBL=OPWPAMBL;               /* INIT AMBL PTR IN ACB          */ 02689000
         ST    @15,ACBAMBL(,RACB)                                  1041 02690000
*   AMBLACB=RACB;                   /* INIT ACB PTR IN PATH AMBL     */ 02691000
         ST    RACB,AMBLACB(,@14)                                  1042 02692000
*   MWAMACRF=ACBMACRF;              /* SAVE ACBMACRF                 */ 02693000
         MVC   MWAMACRF(2),ACBMACRF(RACB)                          1043 02694000
*   ACBOUT=OFF;                     /* RESET OUTPUT INDR             */ 02695000
         NI    ACBOUT(RACB),B'11111101'                            1044 02696000
*   CALL IDA0192B;                  /* OPEN PATH AIX                 */ 02697000
         L     @15,@CV04860                                        1045 02698000
         BALR  @14,@15                                             1045 02699000
*   RFY                                                            1046 02700000
*     RETCODE RSTD;                 /*                       @Z30SSDP*/ 02701000
*   RSTATRTN=IDAR192F;              /* INDIC 192F CONTROL    @Y30DSDP*/ 02702000
         L     @14,OPWCOMWA(,RCORE)                                1047 02703000
         MVC   RSTATRTN(4,@14),@CC04581                            1047 02704000
*   PDPDSTYP=PDPDDNM;               /* RESET DATA SET TYPE TO DD NAME*/ 02705000
         AL    @14,@CF04927                                        1048 02706000
         MVI   PDPDSTYP(@14),X'01'                                 1048 02707000
*   ACBMACRF=MWAMACRF;              /* RESTORE ACBMACRF              */ 02708000
         MVC   ACBMACRF(2,RACB),MWAMACRF                           1049 02709000
*   PDPFUNC=PDF90;                  /* RESTORE FUNCTION CODE         */ 02710000
         MVI   PDPFUNC(@14),X'5A'                                  1050 02711000
*   IF RETCODE>4 THEN               /* IF TERMINATING ERROR          */ 02712000
         C     RETCODE,@CF00095                                    1051 02713000
         BNH   @RF01051                                            1051 02714000
*     DO;                           /* THEN TERMINATE OPEN           */ 02715000
*       RFY                                                        1053 02716000
*         RETCODE UNRSTD;                                          1053 02717000
*       MWAOFAIL=ON;                /* IND OPEN FAILURE              */ 02718000
         OI    MWAOFAIL,B'10000000'                                1054 02719000
*       RETURN TO TERM192F;         /* TERMINATE OPEN                */ 02720000
         LA    @14,TERM192F                                        1055 02721000
@EL00010 DS    0H                                                  1055 02722000
@EF00010 DS    0H                                                  1055 02723000
@ER00010 BR    @14                                                 1055 02724000
*     END;                                                         1056 02725000
*   AMBLDTA->AMBTRACE=OPWBAMBL->AMBLDTA->AMBTRACE;/* USE BASE      1057 02726000
*                                      AMBTRACE ADDR         @ZA29388*/ 02727000
@RF01051 L     @05,MWAXAMBL                                        1057 02728000
         L     @01,AMBLDTA(,@05)                                   1057 02729000
         L     @15,OPWBAMBL(,RCORE)                                1057 02730000
         L     @15,AMBLDTA(,@15)                                   1057 02731000
         L     @00,AMBTRACE(,@15)                                  1057 02732000
         ST    @00,AMBTRACE(,@01)                                  1057 02733000
*   IF AMBLKSDS=ON THEN             /* IF KSDS THEN          @ZA29388*/ 02734000
         TM    AMBLKSDS(@05),B'00000100'                           1058 02735000
         BNO   @RF01058                                            1058 02736000
*     AMBLIX->AMBTRACE=AMBLDTA->AMBTRACE;/* USE DATA AMBTRACE ADDR IN   02737000
*                                      INDEX AMB             @ZA29388*/ 02738000
         L     @05,AMBLIX(,@05)                                    1059 02739000
         ST    @00,AMBTRACE(,@05)                                  1059 02740000
*   RFY                                                            1060 02741000
*     RET RSTD;                                                    1060 02742000
@RF01058 DS    0H                                                  1061 02743000
*   RET=MWASAVEA;                   /* RESTORE RETURN ADDRESS        */ 02744000
         L     RET,MWASAVEA                                        1061 02745000
*   RFY                                                            1062 02746000
*     RET UNRSTD;                                                  1062 02747000
*   END OPNPATH;                                                   1063 02748000
         B     @EL00010                                            1063 02749000
*CONPATH:                                                          1064 02750000
*                                                                  1064 02751000
*/********************************************************************/ 02752000
*/*                                                                  */ 02753000
*/*   FUNCTION: CALLED BY OPNPATH TO DETERMINE IF PATH AIX IS        */ 02754000
*/*             ALREADY OPEN AS A PATH (SIMPLE CONNECT) OR AS AN     */ 02755000
*/*             UPGRADE AIX (COMPLEX CONNECT).                       */ 02756000
*/*                                                                  */ 02757000
*/*   INPUTS: 1. PTR TO OPW IN RCORE.                                */ 02758000
*/*                                                                  */ 02759000
*/*   OUTPUTS 1. IF SIMPLE OR COMPLEX CONNECT, THEN OPWCAMBL POINTS  */ 02760000
*/*              TO AMBL CONNECTING TO, OTHERWISE, IT IS SET TO      */ 02761000
*/*              ZERO.                                               */ 02762000
*/*           2. IF SIMPLE CONNECT, THEN OPWPCON POINTS TO PATH AMBL */ 02763000
*/*              CONNECTING TO, OTHERWISE, IT IS SET TO ZERO.        */ 02764000
*/*           3. IF COMPLEX CONNECT, THEN OPWPRTBL IS SET ON,        */ 02765000
*/*              OTHERWISE, IT IS SET OFF.                           */ 02766000
*/*                                                                  */ 02767000
*/********************************************************************/ 02768000
*                                                                  1064 02769000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1064 02770000
CONPATH  DS    0H                                                  1065 02771000
*   RFY                                                            1065 02772000
*     RET RSTD;                                                    1065 02773000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 02774000
         ST    RET,MWASAVEB                                        1066 02775000
*   RFY                                                            1067 02776000
*     RET UNRSTD;                                                  1067 02777000
*   OPWESDS=OFF;                    /* RESET ESDS INDICATOR          */ 02778000
         NI    OPWESDS(RCORE),B'11111101'                          1068 02779000
*   OPWDCI=OPWCDTCI;                /* INIT CINV NO. FOR DATA        */ 02780000
         L     @05,OPWCURPT(,RCORE)                                1069 02781000
         MVC   OPWDCI(3,RCORE),OPWCDTCI(@05)                       1069 02782000
*   OPWKSDS=ON;                     /* IND CLUSTER IS A KSDS         */ 02783000
         OI    OPWKSDS(RCORE),B'00000100'                          1070 02784000
*   IF OPWBCON^=0 THEN              /* IF CONNECT PROCESSING         */ 02785000
         SLR   @05,@05                                             1071 02786000
         L     @15,OPWBCON(,RCORE)                                 1071 02787000
         CR    @15,@05                                             1071 02788000
         BE    @RF01071                                            1071 02789000
*     DO;                           /* THEN INIT SEARCH ARGUMENT     */ 02790000
*       MWAXAMBL=OPWBCON;           /* PT TO PRIMARY AMBL FOR BASE   */ 02791000
         ST    @15,MWAXAMBL                                        1073 02792000
*       OPWCAMBL=0;                 /* ZERO CONNECT AMBL PTR         */ 02793000
         ST    @05,OPWCAMBL(,RCORE)                                1074 02794000
*       DO UNTIL(MWAXAMBL=0|OPWPCON^=0);/* SEARCH PATH AMBL'S        */ 02795000
@DL01075 DS    0H                                                  1076 02796000
*         IF AMBLXPT^=0&            /* IF PATH AMBL EXISTS           */ 02797000
*             AMBLXPT->AMBLPRIM=ON& /* AND PATH AMBL IS PRIMARY      */ 02798000
*             (AMBLXPT->AMBLIDF=OPWIDF&/* AND PATH AMBL IS   @ZA16012*/ 02799000
*             AMBLXPT->AMBLQ2=OPWQ2) THEN/* FOR SAME AIX     @ZA16012*/ 02800000
         L     @05,MWAXAMBL                                        1076 02801000
         L     @05,AMBLXPT(,@05)                                   1076 02802000
         LTR   @05,@05                                             1076 02803000
         BZ    @RF01076                                            1076 02804000
         TM    AMBLPRIM(@05),B'10000000'                           1076 02805000
         BNO   @RF01076                                            1076 02806000
         CLC   AMBLIDF(8,@05),OPWIDF(RCORE)                        1076 02807000
         BNE   @RF01076                                            1076 02808000
         CLC   AMBLQ2(1,@05),OPWQ2(RCORE)                          1076 02809000
         BNE   @RF01076                                            1076 02810000
*           DO;                                                    1077 02811000
*             OPWPCON=AMBLXPT;      /* INIT PTR TO PATH CONNECT AMBL */ 02812000
         ST    @05,OPWPCON(,RCORE)                                 1078 02813000
*             OPWCAMBL=AMBLXPT;     /* INIT CURRENT CONNECT AMBL PTR */ 02814000
         ST    @05,OPWCAMBL(,RCORE)                                1079 02815000
*           END;                                                   1080 02816000
*         ELSE                      /* IF NOT                        */ 02817000
*           MWAXAMBL=AMBLSCHN;      /* PT TO NEXT SECONDARY AMBL FOR    02818000
*                                      BASE                          */ 02819000
         B     @RC01076                                            1081 02820000
@RF01076 L     @05,MWAXAMBL                                        1081 02821000
         L     @05,AMBLSCHN(,@05)                                  1081 02822000
         ST    @05,MWAXAMBL                                        1081 02823000
*       END;                                                       1082 02824000
@RC01076 DS    0H                                                  1082 02825000
@DE01075 SLR   @05,@05                                             1082 02826000
         C     @05,MWAXAMBL                                        1082 02827000
         BE    @DC01075                                            1082 02828000
         C     @05,OPWPCON(,RCORE)                                 1082 02829000
         BE    @DL01075                                            1082 02830000
@DC01075 DS    0H                                                  1083 02831000
*     END;                                                         1083 02832000
*   IF OPWUPT=0 THEN                /* IF UPT IN OPWRK NOT EXISTS  1084 02833000
*                                                            @ZA18253*/ 02834000
@RF01071 L     @05,OPWUPT(,RCORE)                                  1084 02835000
         LTR   @05,@05                                             1084 02836000
         BNZ   @RF01084                                            1084 02837000
*     OPWUPT=BIBUPT;                /* NO USE OLD ONE        @ZA18253*/ 02838000
         L     @05,OPWBIB(,RCORE)                                  1085 02839000
         L     @05,BIBUPT(,@05)                                    1085 02840000
         ST    @05,OPWUPT(,RCORE)                                  1085 02841000
*   IF OPWPCON=0&OPWUPT^=0 THEN     /* IF NOT CONNECT TO PATH AIX AND   02842000
*                                      UPDATE TABLE EXISTS   @ZA13578*/ 02843000
@RF01084 SLR   @05,@05                                             1086 02844000
         C     @05,OPWPCON(,RCORE)                                 1086 02845000
         BNE   @RF01086                                            1086 02846000
         C     @05,OPWUPT(,RCORE)                                  1086 02847000
         BE    @RF01086                                            1086 02848000
*     DO;                           /* INIT SEARCH ARGUMENT          */ 02849000
*       RFY                                                        1088 02850000
*         IFGRPL BASED(UPTRPL(MWAI));                              1088 02851000
*       RFY                                                        1089 02852000
*         IFGACB BASED(RPLDACB);                                   1089 02853000
*       RFY                                                        1090 02854000
*         IDAAMBL BASED(ACBAMBL);                                  1090 02855000
*       DO MWAI=1 TO UPTNOENT       /* SCAN UPT FOR          @ZA16012*/ 02856000
*             WHILE(AMBLIDF^=OPWIDF|AMBLQ2^=OPWQ2);/* MATCH  @ZA16012*/ 02857000
         LA    @05,1                                               1091 02858000
         B     @DE01091                                            1091 02859000
@DL01091 MH    @05,@CH00033                                        1091 02860000
         L     @01,OPWUPT(,RCORE)                                  1091 02861000
         L     @05,UPTRPL-12(@05,@01)                              1091 02862000
         L     @05,RPLDACB(,@05)                                   1091 02863000
         L     @05,ACBAMBL(,@05)                                   1091 02864000
         CLC   AMBLIDF(8,@05),OPWIDF(RCORE)                        1091 02865000
         BNE   @DB01091                                            1091 02866000
         CLC   AMBLQ2(1,@05),OPWQ2(RCORE)                          1091 02867000
         BE    @DC01091                                            1091 02868000
@DB01091 DS    0H                                                  1092 02869000
*       END;                                                       1092 02870000
         LA    @05,1                                               1092 02871000
         AL    @05,MWAI                                            1092 02872000
@DE01091 ST    @05,MWAI                                            1092 02873000
         L     @01,OPWUPT(,RCORE)                                  1092 02874000
         SLR   @00,@00                                             1092 02875000
         IC    @00,UPTNOENT(,@01)                                  1092 02876000
         CR    @05,@00                                             1092 02877000
         BNH   @DL01091                                            1092 02878000
@DC01091 DS    0H                                                  1093 02879000
*       IF MWAI<=UPTNOENT&UPTF1NOP(MWAI)=OFF THEN/* IF PATH AIX IN 1093 02880000
*                                      UPGRADE SET                   */ 02881000
         L     @05,MWAI                                            1093 02882000
         L     @15,OPWUPT(,RCORE)                                  1093 02883000
         SLR   @14,@14                                             1093 02884000
         IC    @14,UPTNOENT(,@15)                                  1093 02885000
         CR    @05,@14                                             1093 02886000
         BH    @RF01093                                            1093 02887000
         MH    @05,@CH00033                                        1093 02888000
         ST    @05,@TF00001                                        1093 02889000
         ALR   @05,@15                                             1093 02890000
         TM    UPTF1NOP-12(@05),B'00010000'                        1093 02891000
         BNZ   @RF01093                                            1093 02892000
*         DO;                                                      1094 02893000
*           OPWPRTBL=ON;            /* THEN IND PARTIAL CB BUILD     */ 02894000
         L     @05,OPWCURPT(,RCORE)                                1095 02895000
         OI    OPWPRTBL(@05),B'00001000'                           1095 02896000
*           OPWCAMBL=ADDR(IDAAMBL); /* INIT CURRENT CONNECT AMBL PTR */ 02897000
         L     @05,@TF00001                                        1096 02898000
         L     @05,UPTRPL-12(@05,@15)                              1096 02899000
         L     @05,RPLDACB(,@05)                                   1096 02900000
         L     @05,ACBAMBL(,@05)                                   1096 02901000
         ST    @05,OPWCAMBL(,RCORE)                                1096 02902000
*           IF MWADTWRK^=0 THEN     /* IF AMBWORKS NOT FREED         */ 02903000
         L     @05,MWADTWRK                                        1097 02904000
         LTR   @05,@05                                             1097 02905000
         BZ    @RF01097                                            1097 02906000
*             DO;                   /* THEN RESTORE PTR'S IN OPW     */ 02907000
*               OPWDTWRK=MWADTWRK;  /* RESTORE DATA AMBWORK PTR      */ 02908000
