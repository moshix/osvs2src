         LA    @15,0(,@15)                                         1356 03728000
         STH   @15,CMBLEN(,@05)                                    1356 03729000
*   CMBCNT=LENGTH(CMBPTRS)/FWORDLEN;/* INIT NO. OF STORAGE BLOCK   1357 03730000
*                                      PTR'S IN CMB                  */ 03731000
         MVC   CMBCNT(2,@05),@CH02818                              1357 03732000
*   IF OPWUPGR=OFF THEN             /* IF NOT UPGRADE AIX            */ 03733000
         L     @01,OPWCURPT(,RCORE)                                1358 03734000
         TM    OPWUPGR(@01),B'00100000'                            1358 03735000
         BNZ   @RF01358                                            1358 03736000
*     CMBNST=OPWCSTRN;              /* THEN INIT NUMBER OF STRINGS   */ 03737000
         IC    @15,OPWCSTRN(,RCORE)                                1359 03738000
         STC   @15,CMBNST(,@05)                                    1359 03739000
*   RFY                                                            1360 03740000
*     RET RSTD;                                                    1360 03741000
@RF01358 DS    0H                                                  1361 03742000
*   RET=MWASAVED;                   /* RESTORE RETURN ADDRESS        */ 03743000
         L     RET,MWASAVED                                        1361 03744000
*   RFY                                                            1362 03745000
*     RET UNRSTD;                                                  1362 03746000
*   END BLDCMB;                                                    1363 03747000
@EL00015 DS    0H                                                  1363 03748000
@EF00015 DS    0H                                                  1363 03749000
@ER00015 BR    @14                                                 1363 03750000
*GETCORE:                                                          1364 03751000
*                                                                  1364 03752000
*/********************************************************************/ 03753000
*/*                                                                  */ 03754000
*/*   FUNCTION: THIS ROUTINE GETS VIRTUAL STORAGE.                   */ 03755000
*/*                                                                  */ 03756000
*/*   INPUTS:   1. SUBPOOL REQUIRED IN MWAGFSP.                      */ 03757000
*/*             2. LENGTH OF STORAGE REQUIRED IN MWAGFLEN.           */ 03758000
*/*                                                                  */ 03759000
*/*   OUTPUTS:  1. GETMAIN IS ISSUED.                                */ 03760000
*/*             2. PPCODE IS SET IF NOT ENOUGH STORAGE AVAILABLE.    */ 03761000
*/*             3. STORAGE ALLOCATED IS ZEROED.                      */ 03762000
*/*                                                                  */ 03763000
*/********************************************************************/ 03764000
*                                                                  1364 03765000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1364 03766000
GETCORE  DS    0H                                                  1365 03767000
*   RFY                                                            1365 03768000
*     RET RSTD;                                                    1365 03769000
*   MWASAVEE=RET;                   /* SAVE RETURN ADDRESS           */ 03770000
         ST    RET,MWASAVEE                                        1366 03771000
*   RFY                                                            1367 03772000
*     RET UNRSTD;                                                  1367 03773000
*   RFY                                                            1368 03774000
*    (R0,                                                          1368 03775000
*     R1,                                                          1368 03776000
*     R6,                                                          1368 03777000
*     R7,                                                          1368 03778000
*     R8,                                                          1368 03779000
*     R9) RSTD;                                                    1368 03780000
*   STM(R6,R9,MWAGFSVE);            /* SAVE WORK REGISTERS           */ 03781000
         STM   R6,R9,MWAGFSVE                                      1369 03782000
*   MWAGFLEN=(MWAGFLEN+SEVEN)&NEGEIGHT;/* ROUND UP TO DOUBLE WORD  1370 03783000
*                                      BOUNDARY                      */ 03784000
         LA    @05,7                                               1370 03785000
         L     @15,MWAGFLEN-1                                      1370 03786000
         LA    @15,0(,@15)                                         1370 03787000
         ALR   @05,@15                                             1370 03788000
         N     @05,@CF04735                                        1370 03789000
         STCM  @05,7,MWAGFLEN                                      1370 03790000
*   R6=MWAGFSP;                     /* LOAD SUBPOOL                  */ 03791000
         SLR   R6,R6                                               1371 03792000
         IC    R6,MWAGFSP                                          1371 03793000
*   R7=MWAGFLEN;                    /* LOAD BLOCK LENGTH             */ 03794000
         LR    R7,@05                                              1372 03795000
*   GEN(GETMAIN RC,LV=(R7),SP=(R6)) /*                       @Y30DSDP*/ 03796000
*   SETS(RETCODE);                  /* GET BLOCK             @Y30DSDP*/ 03797000
         GETMAIN RC,LV=(R7),SP=(R6)                                     03798000
*   RFY                                                            1374 03799000
*     RETCODE RSTD;                                                1374 03800000
*   IF RETCODE=0 THEN               /* WAS GETMAIN SUCCESSFUL        */ 03801000
         LTR   RETCODE,RETCODE                                     1375 03802000
         BNZ   @RF01375                                            1375 03803000
*     DO;                           /* YES - ZERO CORE BLOCK         */ 03804000
*       MWAGFADR=R1;                /* PTR TO GOTTEN CORE    @Y30DSDP*/ 03805000
         ST    R1,MWAGFADR                                         1377 03806000
*       R6=MWAGFADR;                /* POINT TO CORE BLOCK           */ 03807000
         L     @14,MWAGFADR                                        1378 03808000
         LR    R6,@14                                              1378 03809000
*       R7=MWAGFLEN;                /* LOAD BLOCK LENGTH             */ 03810000
         L     R7,MWAGFLEN-1                                       1379 03811000
         LA    R7,0(,R7)                                           1379 03812000
*       R9=0;                       /* ZERO PAD CHARACTER            */ 03813000
         SLR   R9,R9                                               1380 03814000
*       MVCL(R6,R8);                /* ZERO CORE BLOCK               */ 03815000
         MVCL  R6,R8                                               1381 03816000
*     END;                                                         1382 03817000
*   LM(R6,R9,MWAGFSVE);             /* RESTORE WORK REGISTERS        */ 03818000
@RF01375 LM    R6,R9,MWAGFSVE                                      1383 03819000
*   IF RETCODE^=0 THEN              /* WAS GETMAIN SUCCESSFUL        */ 03820000
         LTR   RETCODE,RETCODE                                     1384 03821000
         BZ    @RF01384                                            1384 03822000
*     DO;                                                          1385 03823000
*       PDPCODE=PP20;               /* SET ERROR CODE                */ 03824000
         L     @14,OPWCOMWA(,RCORE)                                1386 03825000
         MVI   PDPCODE+280(@14),X'14'                              1386 03826000
*       CALL PROBDT;                /* CALL PROBLEM DETERMINATION    */ 03827000
         BAL   @14,PROBDT                                          1387 03828000
*     END;                                                         1388 03829000
*   RFY                                                            1389 03830000
*     RETCODE UNRSTD;                                              1389 03831000
@RF01384 DS    0H                                                  1390 03832000
*   RFY                                                            1390 03833000
*     RET RSTD;                                                    1390 03834000
*   RET=MWASAVEE;                   /* RESTORE RETURN ADDRESS        */ 03835000
         L     RET,MWASAVEE                                        1391 03836000
*   RFY                                                            1392 03837000
*     RET UNRSTD;                                                  1392 03838000
*   RFY                                                            1393 03839000
*    (R0,                                                          1393 03840000
*     R1,                                                          1393 03841000
*     R6,                                                          1393 03842000
*     R7,                                                          1393 03843000
*     R8,                                                          1393 03844000
*     R9) UNRSTD;                                                  1393 03845000
*   END GETCORE;                                                   1394 03846000
@EL00016 DS    0H                                                  1394 03847000
@EF00016 DS    0H                                                  1394 03848000
@ER00016 BR    @14                                                 1394 03849000
*FREECORE:                                                         1395 03850000
*                                                                  1395 03851000
*/********************************************************************/ 03852000
*/*                                                                  */ 03853000
*/*   FUNCTION: THIS ROUTINE ISSUES THE FREEMAIN FOR IDA0192F        */ 03854000
*/*                                                                  */ 03855000
*/*   INPUTS:   1. SUBPOOL IN MWAGFSP                                */ 03856000
*/*             2. LENGTH IN MWAGFLEN                                */ 03857000
*/*             3. ADDRESS IN MWAGFADR                               */ 03858000
*/*                                                                  */ 03859000
*/*   OUTPUTS:  1. THE FREEMAIN IS ISSUED                            */ 03860000
*/*                                                                  */ 03861000
*/********************************************************************/ 03862000
*                                                                  1395 03863000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1395 03864000
FREECORE DS    0H                                                  1396 03865000
*   RFY                                                            1396 03866000
*     RET RSTD;                                                    1396 03867000
*   MWASAVEE=RET;                   /* SAVE RETURN ADDRESS           */ 03868000
         ST    RET,MWASAVEE                                        1397 03869000
*   RFY                                                            1398 03870000
*     RET UNRSTD;                                                  1398 03871000
*   RFY                                                            1399 03872000
*    (R0,                                                          1399 03873000
*     R1) RSTD;                                                    1399 03874000
*   R0=MWAGFR0;                     /* INIT REG 0 FOR FREEMAIN       */ 03875000
         L     R0,MWAGFR0                                          1400 03876000
*   R1=MWAGFADR;                    /* INIT REG 1 FOR FREEMAIN       */ 03877000
         L     R1,MWAGFADR                                         1401 03878000
*   GEN(FREEMAIN R,LV=(0),A=(1));   /* ISSUE FREEMAIN                */ 03879000
         FREEMAIN R,LV=(0),A=(1)                                        03880000
*   RFY                                                            1403 03881000
*    (R0,                                                          1403 03882000
*     R1) UNRSTD;                                                  1403 03883000
*   RFY                                                            1404 03884000
*     RET RSTD;                                                    1404 03885000
*   RET=MWASAVEE;                   /* RESTORE RETURN ADDRESS        */ 03886000
         L     RET,MWASAVEE                                        1405 03887000
*   RFY                                                            1406 03888000
*     RET UNRSTD;                                                  1406 03889000
*   END FREECORE;                                                  1407 03890000
@EL00017 DS    0H                                                  1407 03891000
@EF00017 DS    0H                                                  1407 03892000
@ER00017 BR    @14                                                 1407 03893000
*PROBDT:                                                           1408 03894000
*                                                                  1408 03895000
*/********************************************************************/ 03896000
*/*                                                                  */ 03897000
*/*   FUNCTION: PROBLEM DETERMINATION INTERFACE.  INITIALIZES THE    */ 03898000
*/*             CLUSTER NAME IN THE PROBLEM DETERMINATION PARAMETER  */ 03899000
*/*             LIST (IDAPDPRM) TO THE DSNAME SPECIFIED ON THE DD    */ 03900000
*/*             STATEMENT FOR THE SPHERE.  THE ACBERFLG FIELD IN THE */ 03901000
*/*             USER ACB IS INITIALIZED TO THE CODE CORRESPONDING    */ 03902000
*/*             TO THE PROBLEM DETERMINATION ERROR CODE (PPCODE).    */ 03903000
*/*             THE ACBERFLG IS SET BY REFERENCING THE ARRAY         */ 03904000
*/*             OPWERMAP THAT MAPS PPCODE TO ACBERFLG.  THIS ARRAY   */ 03905000
*/*             IS  DEFINED BY IDAERMAP.  IDA0192P IS CALLED         */ 03906000
*/*             TO WRITE THE PROBLEM DETERMINATION MESSAGE.          */ 03907000
*/*                                                                  */ 03908000
*/*   INPUTS:   1. PTR TO FORCORE WHICH CONTAINS THE PROBLEM         */ 03909000
*/*               DETERMINATION PARAMETER LIST IN OPWCOMWA.          */ 03910000
*/*             2. PTR TO USER ACB IN OPWUACB.                       */ 03911000
*/*                                                                  */ 03912000
*/*   OUTPUTS:  1. IF ERROR (ACBERFLG>=128), THEN MWAOFAIL IS SET    */ 03913000
*/*                ON AND CONTROL IS GIVEN TO TERM192A.              */ 03914000
*/*             2. IF WARNING (ACBERFLG<128), THEN CONTROL IS GIVEN  */ 03915000
*/*                TO CALLER.                                        */ 03916000
*/*                                                                  */ 03917000
*/********************************************************************/ 03918000
*                                                                  1408 03919000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1408 03920000
PROBDT   DS    0H                                                  1409 03921000
*   RFY                                                            1409 03922000
*     RET RSTD;                                                    1409 03923000
*   MWASAVEF=RET;                   /* SAVE RETURN ADDRESS           */ 03924000
         ST    RET,MWASAVEF                                        1410 03925000
*   RFY                                                            1411 03926000
*     RET UNRSTD;                                                  1411 03927000
*   RACB=OPWUACB;                   /* PT TO USER ACB                */ 03928000
         L     RACB,OPWUACB(,RCORE)                                1412 03929000
*   ACBERFLG=OPWERMAP(PDPCODE);     /* SET ACBERFLG IN USER ACB      */ 03930000
         L     @05,OPWCOMWA(,RCORE)                                1413 03931000
         SLR   @01,@01                                             1413 03932000
         IC    @01,PDPCODE+280(,@05)                               1413 03933000
         ALR   @01,RCORE                                           1413 03934000
         MVC   ACBERFLG(1,RACB),OPWERMAP-1(@01)                    1413 03935000
*   RFY                                                            1414 03936000
*     R1 RSTD;                                                     1414 03937000
*   R1=OPWCOMWA;                    /* POINT TO FORCORE              */ 03938000
         LR    R1,@05                                              1415 03939000
*   RFY                                                            1416 03940000
*     R1 UNRSTD;                                                   1416 03941000
*   RSTATRTN=IDAR192P;              /* INDIC 192P CONTROL    @Y30DSDP*/ 03942000
         MVC   RSTATRTN(4,@05),@CC04587                            1417 03943000
*   CALL IDA0192P;                  /* WRITE PROB DET MESSAGE        */ 03944000
         L     @15,@CV04818                                        1418 03945000
         BALR  @14,@15                                             1418 03946000
*   RSTATRTN=IDAR192F;              /* INDIC 192F CONTROL    @Y30DSDP*/ 03947000
         L     @05,OPWCOMWA(,RCORE)                                1419 03948000
         MVC   RSTATRTN(4,@05),@CC04581                            1419 03949000
*   IF ACBERFLG>=128 THEN           /* IF TERMINATING ERROR          */ 03950000
         CLI   ACBERFLG(RACB),128                                  1420 03951000
         BL    @RF01420                                            1420 03952000
*     DO;                           /* THEN TERMINATE OPEN           */ 03953000
*       MWAOFAIL=ON;                /* IND OPEN FAILURE              */ 03954000
         OI    MWAOFAIL,B'10000000'                                1422 03955000
*       RETURN TO TERM192F;         /* GOTO TERMINATE OPEN           */ 03956000
         LA    @14,TERM192F                                        1423 03957000
@EL00018 DS    0H                                                  1423 03958000
@EF00018 DS    0H                                                  1423 03959000
@ER00018 BR    @14                                                 1423 03960000
*     END;                                                         1424 03961000
*   RFY                                                            1425 03962000
*     RET RSTD;                                                    1425 03963000
@RF01420 DS    0H                                                  1426 03964000
*   RET=MWASAVEF;                   /* RESTORE RETURN ADDRESS        */ 03965000
         L     RET,MWASAVEF                                        1426 03966000
*   RFY                                                            1427 03967000
*     RET UNRSTD;                                                  1427 03968000
*   END PROBDT;                                                    1428 03969000
         B     @EL00018                                            1428 03970000
@PB00018 DS    0H                                                  1428 03971000
*   END IDA0192F                                                   1429 03972000
*                                                                  1429 03973000
