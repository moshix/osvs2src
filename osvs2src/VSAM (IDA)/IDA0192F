         TITLE 'IDA0192F - VSAM, OPEN CLUSTERS IN SPHERE VS2-3         *00001000
                        '                                               00002000
IDA0192F CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192F  78.199'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @12,4095(,@03)                                      0001 00012000
         USING @PSTART,@03                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
JSCBDISP EQU 188                                                        00015000
JSCBAOS1 EQU  72                                                        00016000
JSCBAOS2 EQU 192                                                        00017000
RBPRFXLN EQU 64                                                         00018000
RBPRFLNA EQU 32                                                         00019000
PRBLEN   EQU 136                                                        00020000
SIRBLEN  EQU 200                                                        00021000
TIRBLEN  EQU 136                                                        00022000
IRBLEN   EQU 128                                                        00023000
SVRBLEN  EQU 224                                                        00024000
*                                                                  0282 00025000
*/********************************************************************/ 00026000
*/*                                                                  */ 00027000
*/*                      MODULE IDA0192F                             */ 00028000
*/*                                                                  */ 00029000
*/********************************************************************/ 00030000
*                                                                  0282 00031000
*   RSTATRTN=IDAR192F;              /* INDIC 192F IN CONTROL @Y30DSDP*/ 00032000
         L     @05,OPWCOMWA(,RCORE)                                0282 00033000
         MVC   RSTATRTN(4,@05),@CC04581                            0282 00034000
*/********************************************************************/ 00035000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 00036000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 00037000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 00038000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 00039000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 00040000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 00041000
*/********************************************************************/ 00042000
*                                                                  0283 00043000
*   GEN;                                                           0283 00044000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00045000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00046000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00047000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00048000
*                                       IF NO ERROR WAS ENCOUNTERED     00049000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00050000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00051000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00052000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00053000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00054000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00055000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00056000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00057000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00058000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00059000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00060000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00061000
         B     GMN00200                 BYPASS SETTING ERROR            00062000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00063000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00064000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00065000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00066000
*   RFY                                                            0284 00067000
*    (R1,                                                          0284 00068000
*     RETCODE) RSTD;                                               0284 00069000
*   IF RETCODE=0 THEN               /* IF GETMAIN ERROR              */ 00070000
         LTR   RETCODE,RETCODE                                     0285 00071000
         BNZ   @RF00285                                            0285 00072000
*     DO;                           /* THEN CALL PROB DET            */ 00073000
*       ACBERFLG=OPERR136;          /* SET ACB ERROR FLAG            */ 00074000
         MVI   ACBERFLG(RACB),X'88'                                0287 00075000
*       PDPCODE=PP20;               /* SET PROB DET ERROR CODE       */ 00076000
         L     @14,OPWCOMWA(,RCORE)                                0288 00077000
         MVI   PDPCODE+280(@14),X'14'                              0288 00078000
*       R1=OPWCOMWA;                /* PT TO COMMON WORK AREA        */ 00079000
         LR    R1,@14                                              0289 00080000
*       RFY                                                        0290 00081000
*        (R1,                                                      0290 00082000
*         RETCODE) UNRSTD;                                         0290 00083000
*       RSTATRTN=IDAR192P;          /* INDIC 192P CONTROL    @Y30DSDP*/ 00084000
         MVC   RSTATRTN(4,@14),@CC04587                            0291 00085000
*       CALL IDA0192P;              /* WRITE PROB DET MESSAGE        */ 00086000
         L     @15,@CV04818                                        0292 00087000
         BALR  @14,@15                                             0292 00088000
*       RSTATRTN=IDAR192F;          /* INDIC 192F CONTROL    @Y30DSDP*/ 00089000
         L     @05,OPWCOMWA(,RCORE)                                0293 00090000
         MVC   RSTATRTN(4,@05),@CC04581                            0293 00091000
*       OPWNOWRK=ON;                /* IND NO MODULE WORK AREA       */ 00092000
         OI    OPWNOWRK(RCORE),B'00010000'                         0294 00093000
*       GOTO TERM192F;              /* RESTORE ACB AND EXIT MODULE   */ 00094000
         B     TERM192F                                            0295 00095000
*     END;                                                         0296 00096000
*                                                                  0296 00097000
*   /*****************************************************************/ 00098000
*   /*                                                               */ 00099000
*   /* ESTABLISH MODULE ADDRESSABILITY                               */ 00100000
*   /*                                                               */ 00101000
*   /*****************************************************************/ 00102000
*                                                                  0297 00103000
*   RFY                                                            0297 00104000
*     R15 RSTD;                                                    0297 00105000
@RF00285 DS    0H                                                  0298 00106000
*   GEN(ST R13,4(R15))                                             0298 00107000
*   REFS(R13,R15) NOSETS;           /* SAVE LAST SAVE AREA PTR       */ 00108000
         ST R13,4(R15)                                                  00109000
*   R13=R15;                        /* SET BASE FOR MOD WORK AREA    */ 00110000
         LR    R13,R15                                             0299 00111000
*   RFY                                                            0300 00112000
*     R15 UNRSTD;                                                  0300 00113000
*   GEN(USING @DATD,R13)                                           0301 00114000
*   REFS(R13) NOSETS;                                              0301 00115000
*                                                                  0301 00116000
         USING @DATD,R13                                                00117000
*   /*****************************************************************/ 00118000
*   /*                                                               */ 00119000
*   /* MAIN ROUTINE                                                  */ 00120000
*   /*                                                               */ 00121000
*   /*****************************************************************/ 00122000
*                                                                  0302 00123000
*   IF OPWDUMMY=OFF THEN            /* IF NOT DUMMY DATA SET         */ 00124000
         TM    OPWDUMMY(RCORE),B'00010000'                         0302 00125000
         BNZ   @RF00302                                            0302 00126000
*     CALL VOLMNT;                  /* MOUNT AND VERIFY VOLUMES      */ 00127000
         BAL   @14,VOLMNT                                          0303 00128000
*   IF OPWRSTRT=OFF THEN            /* IF NOT RESTART PROCESSING     */ 00129000
@RF00302 TM    OPWRSTRT(RCORE),B'00001000'                         0304 00130000
         BNZ   @RF00304                                            0304 00131000
*     DO;                           /* THEN DO NORMAL OPEN           */ 00132000
*       CALL OPNBASE;               /* OPEN BASE DS                  */ 00133000
         BAL   @14,OPNBASE                                         0306 00134000
*       IF OPWDUMMY=OFF THEN        /* IF NOT DUMMY DS               */ 00135000
         TM    OPWDUMMY(RCORE),B'00010000'                         0307 00136000
         BNZ   @RF00307                                            0307 00137000
*         DO;                       /* THEN DO UPGRADE PROCESSING    */ 00138000
*           IF AMBLWRIT=ON&AMBLCINV=OFF THEN/* IF BASE OPEN FOR    0309 00139000
*                                      OUTPUT AND NOT CINV PROCESSING*/ 00140000
         L     @05,MWAXAMBL                                        0309 00141000
         TM    AMBLWRIT(@05),B'00100000'                           0309 00142000
         BNO   @RF00309                                            0309 00143000
         TM    AMBLCINV(@05),B'01000000'                           0309 00144000
         BNZ   @RF00309                                            0309 00145000
*             IF OPWNOUPG>0&BIBUPT=0 THEN/* THEN IF UPGR SET EXISTS     00146000
*                                      AND NOT ALREADY OPEN          */ 00147000
         CLI   OPWNOUPG(RCORE),0                                   0310 00148000
         BNH   @RF00310                                            0310 00149000
         L     @05,OPWBIB(,RCORE)                                  0310 00150000
         L     @05,BIBUPT(,@05)                                    0310 00151000
         LTR   @05,@05                                             0310 00152000
         BNZ   @RF00310                                            0310 00153000
*               CALL OPNUPGR;       /* THEN OPEN UPGRADE SET         */ 00154000
         BAL   @14,OPNUPGR                                         0311 00155000
*           IF OPWPDTCI^=0 THEN     /* IF PATH PROCESSING            */ 00156000
@RF00310 DS    0H                                                  0312 00157000
@RF00309 L     @05,OPWPDTCI-1(,RCORE)                              0312 00158000
         LA    @05,0(,@05)                                         0312 00159000
         LTR   @05,@05                                             0312 00160000
         BZ    @RF00312                                            0312 00161000
*             CALL OPNPATH;         /* THEN OPEN PATH                */ 00162000
         BAL   @14,OPNPATH                                         0313 00163000
*         END;                                                     0314 00164000
@RF00312 DS    0H                                                  0315 00165000
*     END;                                                         0315 00166000
@RF00307 DS    0H                                                  0316 00167000
*TERM192F:                                                         0316 00168000
*                                                                  0316 00169000
*/********************************************************************/ 00170000
*/*                                                                  */ 00171000
*/*   FUNCTION: MODULE TERMINATION.  FREES MODULE WORK AREA AND SETS */ 00172000
*/*             A RETURN CODE IN REGISTER 15.                        */ 00173000
*/*                                                                  */ 00174000
*/*   INPUTS:   1. PTR TO OPW IN RCORE.                              */ 00175000
*/*                                                                  */ 00176000
*/*   OUTPUTS:  1. IF NO ERRORS ARE DETECTED, REGISTER 15 IS SET TO  */ 00177000
*/*                ZERO.  IF AN ERROR IS DETECTED, REGISTER 15 IS    */ 00178000
*/*                SET TO EIGHT.                                     */ 00179000
*/*                                                                  */ 00180000
*/********************************************************************/ 00181000
*                                                                  0316 00182000
*   RFY                                                            0316 00183000
*    (R0,                                                          0316 00184000
*     R1,                                                          0316 00185000
*     R6) RSTD;                                                    0316 00186000
@RF00304 DS    0H                                                  0316 00187000
TERM192F DS    0H                                                  0317 00188000
*   IF OPWNOWRK=OFF THEN            /* IF MODULE WORK AREA EXISTS    */ 00189000
         TM    OPWNOWRK(RCORE),B'00010000'                         0317 00190000
         BNZ   @RF00317                                            0317 00191000
*     DO;                           /* THEN FREE MODULE WORK AREA    */ 00192000
*       MWAGFSP=WKPOOL;             /* INIT SUBPOOL OF WORK AREA     */ 00193000
         MVI   MWAGFSP,X'FC'                                       0319 00194000
*       GEN(L R0,@SIZDATD)                                         0320 00195000
*       SETS(R0);                   /* INIT SIZE                     */ 00196000
         L R0,@SIZDATD                                                  00197000
*       MWAGFLEN=R0;                /* LD LENGTH OF MWA              */ 00198000
         STCM  R0,7,MWAGFLEN                                       0321 00199000
*       R1=R13;                     /* PT TO MODULE WORK AREA        */ 00200000
         LR    R1,R13                                              0322 00201000
*       R0=MWAGFR0;                 /* LD LENGTH AND SUBPPOL OF MWA  */ 00202000
         L     R0,MWAGFR0                                          0323 00203000
*       IF MWAOFAIL=ON THEN         /* IF OPEN FAILURE               */ 00204000
         TM    MWAOFAIL,B'10000000'                                0324 00205000
         BNO   @RF00324                                            0324 00206000
*         R6=8;                     /* THEN IND OPEN FAILURE         */ 00207000
         LA    R6,8                                                0325 00208000
*       ELSE                        /* NO OPEN FAILURE               */ 00209000
*         R6=0;                     /* ELSE IND OPEN SUCCESSFUL      */ 00210000
         B     @RC00324                                            0326 00211000
@RF00324 SLR   R6,R6                                               0326 00212000
*       GEN(L R13,4(R13))                                          0327 00213000
*       SETS(R13);                  /* PT TO LAST SAVE AREA          */ 00214000
@RC00324 DS    0H                                                  0327 00215000
         L R13,4(R13)                                                   00216000
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE MODULE WORK AREA        */ 00217000
         FREEMAIN R,LV=(0),A=(1)                                        00218000
*     END;                                                         0329 00219000
*   ELSE                            /* MODULE WORK AREA DOES NOT   0330 00220000
*                                      EXIST                         */ 00221000
*     R6=8;                         /* ELSE IND OPEN FAILURE         */ 00222000
         B     @RC00317                                            0330 00223000
@RF00317 LA    R6,8                                                0330 00224000
*   GEN REFS(R6,R13) SETS(R12,RET,RETCODE);                        0331 00225000
@RC00317 DS    0H                                                  0331 00226000
         ST    R6,16(R13)               STORE RETURN CODE               00227000
         LM    RET,R12,12(R13)          RESTORE REGISTERS               00228000
         BR    RET                      RETURN TO                       00229000
*   RFY                                                            0332 00230000
*    (R0,                                                          0332 00231000
*     R1,                                                          0332 00232000
*     R6) UNRSTD;                                                  0332 00233000
*VOLMNT:                                                           0333 00234000
*                                                                  0333 00235000
*/********************************************************************/ 00236000
*/*                                                                  */ 00237000
*/*   FUNCTION: VOLUME MOUNT.  THIS ROUTINE SCANS THE TIOT ENTRY     */ 00238000
*/*             FOR THE SPHERE.  FOR EACH DEVICE TYPE IN THE TIOT    */ 00239000
*/*             ENTRY, A VOLUME LIST (VVOLL) IS BUILT.  IF THIS IS   */ 00240000
*/*             A CONNECT OPEN AND A VMT EXISTS FOR THE DEVICE       */ 00241000
*/*             TYPE ASSOCIATED WITH THE TIOT ENTRY, THEN CALL       */ 00242000
*/*             OLDDEV TO MOUNT VOLUMES, OTHERWISE CALL NEWDEV       */ 00243000
*/*             TO MOUNT VOLUMES.  IF THIS IS A PRIMARY OPEN, THEN   */ 00244000
*/*             CALL NEWDEV TO MOUNT VOLUMES.                        */ 00245000
*/*                                                                  */ 00246000
*/*   INPUTS:   1. PTR TO TIOT ENTRY IN OPWTIOT.                     */ 00247000
*/*                                                                  */ 00248000
*/*   OUTPUTS:  1. VOLUMES MOUNTED FOR SPHERE AND VMT'S BUILT.       */ 00249000
*/*                                                                  */ 00250000
*/********************************************************************/ 00251000
*                                                                  0333 00252000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0333 00253000
VOLMNT   DS    0H                                                  0334 00254000
*   PDPFUNC=PDF85;                  /* SET FUNCTION CODE             */ 00255000
         L     @05,OPWCOMWA(,RCORE)                                0334 00256000
         MVI   PDPFUNC+280(@05),X'55'                              0334 00257000
*   RFY                                                            0335 00258000
*     RET RSTD;                                                    0335 00259000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 00260000
         ST    RET,MWASAVEA                                        0336 00261000
*   RFY                                                            0337 00262000
*     RET UNRSTD;                                                  0337 00263000
*   MWADSABP=DXDSABAD;              /* SET UP DSAB ADDR      @Y30DSDP*/ 00264000
         L     @15,DXDSABAD(,@05)                                  0338 00265000
         ST    @15,MWADSABP                                        0338 00266000
*   MWATIOT=OPWTIOT;                /* PT TO TIOT ENTRY FOR DDNAME   */ 00267000
         L     @15,OPWTIOT(,RCORE)                                 0339 00268000
         ST    @15,MWATIOT                                         0339 00269000
*   MWANEWSW=ON;                    /* IND NEW VVOLL NEEDED          */ 00270000
         OI    MWANEWSW,B'00001000'                                0340 00271000
*   MWAJFCB=ADDR(DXJBF);            /* PT TO JFCB IN COMMON WRK AREA */ 00272000
         LA    @05,DXJBF(,@05)                                     0341 00273000
         ST    @05,MWAJFCB                                         0341 00274000
*   DO UNTIL(MWADSABP=0|TIOEDDNM^='        ');/* SCAN TIOT ENTRY FOR    00275000
*                                      ALL DEVICE TYPES      @Z30SSDP*/ 00276000
@DL00342 DS    0H                                                  0343 00277000
*     IF MWANEWSW=ON THEN           /* IF NEW VVOLL NEEDED           */ 00278000
         TM    MWANEWSW,B'00001000'                                0343 00279000
         BNO   @RF00343                                            0343 00280000
*       DO;                         /* THEN BUILD NEW VOLUME LIST  0344 00281000
*                                      (VVOLL) AND INITIALIZE FIXED     00282000
*                                      PART OF HEADER                */ 00283000
*         MWAGFLEN=LENGTH(IDAVVOLL)+LENGTH(VVOLLENT)*JFCBNVOL;/* CALC   00284000
*                                      LENGTH OF VVOLL               */ 00285000
         L     @05,MWAJFCB                                         0345 00286000
         SLR   @15,@15                                             0345 00287000
         IC    @15,JFCBNVOL(,@05)                                  0345 00288000
         MH    @15,@CH00033                                        0345 00289000
         LA    @15,36(,@15)                                        0345 00290000
         STCM  @15,7,MWAGFLEN                                      0345 00291000
*         MWAGFSP=WKPOOL;           /* INIT SUBPOOL                  */ 00292000
         MVI   MWAGFSP,X'FC'                                       0346 00293000
*         CALL GETCORE;             /* OBTAIN STORAGE FOR VVOLL      */ 00294000
         BAL   @14,GETCORE                                         0347 00295000
*         MWAVVOLL=MWAGFADR;        /* INIT PTR TO VVOLL             */ 00296000
         L     @05,MWAGFADR                                        0348 00297000
         ST    @05,MWAVVOLL                                        0348 00298000
*         MWAVVLNG=MWAGFLEN;        /* SAVE LENGTH OF VVOLL          */ 00299000
         MVC   MWAVVLNG(3),MWAGFLEN                                0349 00300000
*         MWAVVNBR=JFCBNVOL;        /* INIT MAX NO. OF ENTRIES IN  0350 00301000
*                                      VVOLL                         */ 00302000
         L     @01,MWAJFCB                                         0350 00303000
         SLR   @00,@00                                             0350 00304000
         IC    @00,JFCBNVOL(,@01)                                  0350 00305000
         STH   @00,MWAVVNBR                                        0350 00306000
*         VVOLLID=ON;               /* IND VOLUME LIST               */ 00307000
*         VVOLLOPE=ON;              /* IND CALLER IS OPEN            */ 00308000
         OI    VVOLLID(@05),B'01010000'                            0352 00309000
*         IF OPWSCRA^=ON&OPWUCRA^=ON&OPWCAT^=ON THEN/* IF NOT      0353 00310000
*                                      CATALOG, SCRA OR UCRA @XM02259*/ 00311000
         TM    OPWSCRA(RCORE),B'01000000'                          0353 00312000
         BO    @RF00353                                            0353 00313000
         TM    OPWUCRA(RCORE),B'10000000'                          0353 00314000
         BO    @RF00353                                            0353 00315000
         TM    OPWCAT(RCORE),B'10000000'                           0353 00316000
         BO    @RF00353                                            0353 00317000
*           VVOLLACB=OPWCACB;       /* INIT PTR TO CATALOG ACB       */ 00318000
         L     @15,OPWCACB(,RCORE)                                 0354 00319000
         ST    @15,VVOLLACB(,@05)                                  0354 00320000
*         MWANEWSW=OFF;             /* RESET NEW VVOLL NEEDED INDR   */ 00321000
@RF00353 NI    MWANEWSW,B'11110111'                                0355 00322000
*       END;                                                       0356 00323000
*                                                                  0356 00324000
*     /***************************************************************/ 00325000
*     /*                                                             */ 00326000
*     /* INITIALIZE VARIABLE PART OF VVOLL HEADER                    */ 00327000
*     /*                                                             */ 00328000
*     /***************************************************************/ 00329000
*                                                                  0357 00330000
*     VVOLLNBR=JFCBNVOL;            /* INIT NO. OF VOLUME ENTRIES    */ 00331000
@RF00343 L     @05,MWAVVOLL                                        0357 00332000
         L     @01,MWAJFCB                                         0357 00333000
         SLR   @00,@00                                             0357 00334000
         IC    @00,JFCBNVOL(,@01)                                  0357 00335000
         STH   @00,VVOLLNBR(,@05)                                  0357 00336000
*     VVOLLTAB=MWATIOT;             /* INIT PTR TO TIOT ENTRY        */ 00337000
         L     @15,MWATIOT                                         0358 00338000
         ST    @15,VVOLLTAB(,@05)                                  0358 00339000
*     VVOLLERR=0;                   /* ZERO RETURN CODE              */ 00340000
         SLR   @14,@14                                             0359 00341000
         ST    @14,VVOLLERR(,@05)                                  0359 00342000
*     VVOLLSFI=VVOLLSFI&&VVOLLSFI;  /* ZERO SUB-FUNCTION INFORMATION */ 00343000
         XC    VVOLLSFI(12,@05),VVOLLSFI(@05)                      0360 00344000
*     MWADEV=UCBTYP(2:4);           /* LD DEVICE TYPE FROM UCB       */ 00345000
         L     @05,TIOEFSRT-1(,@15)                                0361 00346000
         LA    @05,0(,@05)                                         0361 00347000
         MVC   MWADEV(3),UCBTYP+1(@05)                             0361 00348000
*     MWADVOP=MWADVOP&DVOPMASK;     /* RESET UNNECESSARY BITS IN   0362 00349000
*                                      OPTION BYTE                   */ 00350000
         NI    MWADVOP,B'00001000'                                 0362 00351000
*     IF UCB2OPT4=ON THEN           /* IF VIRTUAL UCB                */ 00352000
         TM    UCB2OPT4(@05),B'00001000'                           0363 00353000
         BNO   @RF00363                                            0363 00354000
*       BIBVIRT=ON;                 /* THEN IND VIRT UCB'S IN SPHERE */ 00355000
         L     @05,OPWBIB(,RCORE)                                  0364 00356000
         OI    BIBVIRT(@05),B'10000000'                            0364 00357000
*     IF OPWBCON^=0 THEN            /* IF CONNECTING                 */ 00358000
@RF00363 L     @05,OPWBCON(,RCORE)                                 0365 00359000
         LTR   @05,@05                                             0365 00360000
         BZ    @RF00365                                            0365 00361000
*       DO;                         /* THEN SEARCH VMT CHAIN FOR LIKE   00362000
*                                      DEVICE TYPE                   */ 00363000
*         MWAVMT=BIBVMT;            /* PT TO FIRST VMT               */ 00364000
         L     @05,OPWBIB(,RCORE)                                  0367 00365000
         L     @05,BIBVMT(,@05)                                    0367 00366000
         ST    @05,MWAVMT                                          0367 00367000
*         DO WHILE(MWAVMT^=0&MWADEV^=VMTDEV);/* SCAN VMT'S FOR LIKE     00368000
*                                      DEVICE TYPE                   */ 00369000
         B     @DE00368                                            0368 00370000
@DL00368 DS    0H                                                  0369 00371000
*           MWAVMT=VMTNXT;          /* PT TO NEXT VMT ON CHAIN       */ 00372000
         L     @05,MWAVMT                                          0369 00373000
         L     @05,VMTNXT(,@05)                                    0369 00374000
         ST    @05,MWAVMT                                          0369 00375000
*         END;                                                     0370 00376000
@DE00368 L     @05,MWAVMT                                          0370 00377000
         LTR   @05,@05                                             0370 00378000
         BZ    @DC00368                                            0370 00379000
         CLC   MWADEV(3),VMTDEV(@05)                               0370 00380000
         BNE   @DL00368                                            0370 00381000
@DC00368 DS    0H                                                  0371 00382000
*         IF MWAVMT^=0 THEN         /* IF LIKE DEVICE TYPE FOUND     */ 00383000
         L     @05,MWAVMT                                          0371 00384000
         LTR   @05,@05                                             0371 00385000
         BZ    @RF00371                                            0371 00386000
*           CALL OLDDEV;            /* MOUNT VOLUMES FOR EXISTING  0372 00387000
*                                      DEVICE TYPE                   */ 00388000
         BAL   @14,OLDDEV                                          0372 00389000
*         ELSE                      /* LIKE DEVICE TYPE NOT FOUND    */ 00390000
*           CALL NEWDEV;            /* MOUNT VOLUMES FOR NEW DEVICE     00391000
*                                      TYPE                          */ 00392000
         B     @RC00371                                            0373 00393000
@RF00371 BAL   @14,NEWDEV                                          0373 00394000
*       END;                                                       0374 00395000
*     ELSE                          /* NOT CONNECTING                */ 00396000
*       CALL NEWDEV;                /* NOT CONNECTING. MOUNT VOLUMES    00397000
*                                      FOR NEW DEVICE TYPE           */ 00398000
         B     @RC00365                                            0375 00399000
@RF00365 BAL   @14,NEWDEV                                          0375 00400000
*     IF MWADSABP^=0 THEN           /* IF THERE IS A DSAB POINTER    */ 00401000
@RC00365 L     @05,MWADSABP                                        0376 00402000
         SLR   @15,@15                                             0376 00403000
