         L     RET,MWASAVEB                                        1160 03102000
*   RFY                                                            1161 03103000
*     RET UNRSTD;                                                  1161 03104000
*   END BLDAMBL;                                                   1162 03105000
@EL00012 DS    0H                                                  1162 03106000
@EF00012 DS    0H                                                  1162 03107000
@ER00012 BR    @14                                                 1162 03108000
*VATUPD:                                                           1163 03109000
*                                                                  1163 03110000
*/********************************************************************/ 03111000
*/*                                                                  */ 03112000
*/*   FUNCTION: CALLED BY BLDAMBL TO ADD AN ENTRY TO VAT FOR AN      */ 03113000
*/*             AMBL.                                                */ 03114000
*/*                                                                  */ 03115000
*/*   INPUTS:   1. PTR TO AMBL IN MWAXAMBL.                          */ 03116000
*/*                                                                  */ 03117000
*/*   OUTPUTS:  1. NEW ENTRY ADDED TO VAT FOR AMBL AND AMBL CHECK    */ 03118000
*/*                INFORMATION INITIALIZED IN AMBL.                  */ 03119000
*/*                                                                  */ 03120000
*/********************************************************************/ 03121000
*                                                                  1163 03122000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1163 03123000
VATUPD   DS    0H                                                  1164 03124000
*   RFY                                                            1164 03125000
*     RET RSTD;                                                    1164 03126000
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */ 03127000
         ST    RET,MWASAVEC                                        1165 03128000
*   RFY                                                            1166 03129000
*     RET UNRSTD;                                                  1166 03130000
*   RFY                                                            1167 03131000
*     IDAVAT BASED(MWAPTRA);                                       1167 03132000
*   RFY                                                            1168 03133000
*    (R6,                                                          1168 03134000
*     R7) RSTD;                                                    1168 03135000
*   R6=JSCBSHR;                     /* SAVE JSCBSHR                  */ 03136000
         L     @05,OPWJSTCB(,RCORE)                                1169 03137000
         L     @05,TCBJSCB(,@05)                                   1169 03138000
         L     @15,JSCBSHR(,@05)                                   1169 03139000
         LR    R6,@15                                              1169 03140000
*   IF(JSCBSHR=0|VATCBSTP^=JSCBSTEP) THEN/* IF VAT DOESN'T EXIST     */ 03141000
         LTR   @15,@15                                             1170 03142000
         BZ    @RT01170                                            1170 03143000
         CLC   VATCBSTP+196(1,@05),JSCBSTEP(@05)                   1170 03144000
         BE    @RF01170                                            1170 03145000
@RT01170 DS    0H                                                  1171 03146000
*     DO;                           /* THEN BUILD FIRST VAT          */ 03147000
*       MWAGFSP=VATPOOL;            /* INIT SUBPOOL OF VAT           */ 03148000
         MVI   MWAGFSP,X'FC'                                       1172 03149000
*       MWAGFLEN=LENGTH(IDAVAT);    /* INIT LENGTH OF VAT            */ 03150000
         MVC   MWAGFLEN(3),@CF00034+1                              1173 03151000
*       CALL GETCORE;               /* OBTAIN STORAGE FOR VAT        */ 03152000
         BAL   @14,GETCORE                                         1174 03153000
*       MWAGFADR->VATID=IDXVAT;     /* INIT CB ID            @OZ03937*/ 03154000
         L     @05,MWAGFADR                                        1175 03155000
         MVI   VATID(@05),B'00010001'                              1175 03156000
*       MWAGFADR->VATLEN=MWAGFLEN;  /* INIT LENGTH           @OZ03937*/ 03157000
         L     @15,MWAGFLEN-1                                      1176 03158000
         LA    @15,0(,@15)                                         1176 03159000
         STH   @15,VATLEN(,@05)                                    1176 03160000
*       MWAGFADR->VATVRT=0;         /* IND FIRST VAT         @OZ03937*/ 03161000
         MVI   VATVRT(@05),X'00'                                   1177 03162000
*       MWAGFADR->VATENO=DIM(VATAMBL);/* INIT NO. OF ENTRIES @OZ03937*/ 03163000
         MVI   VATENO(@05),X'10'                                   1178 03164000
*       ADDR(MWAGFADR)->VATCBSTP=JSCBSTEP;/* CURRENT STEP NO @OZ03937*/ 03165000
         LA    @05,MWAGFADR                                        1179 03166000
         L     @01,OPWJSTCB(,RCORE)                                1179 03167000
         L     @15,TCBJSCB(,@01)                                   1179 03168000
         IC    @14,JSCBSTEP(,@15)                                  1179 03169000
         STC   @14,VATCBSTP(,@05)                                  1179 03170000
*       R7=ADDR(MWAGFADR)->IDAVATCB;/* INIT SWAP REG         @ZA07697*/ 03171000
         L     R7,IDAVATCB(,@05)                                   1180 03172000
*       CS(R6,R7,JSCBSHR);          /* COMPARE AND SWAP VAT PTR      */ 03173000
         CS    R6,@07,JSCBSHR(@15)                                 1181 03174000
*       IF R7^=JSCBSHR THEN         /* IF COMPARE AND SWAP FAILED    */ 03175000
         C     R7,JSCBSHR(,@15)                                    1182 03176000
         BE    @RF01182                                            1182 03177000
*         CALL FREECORE;            /* THEN FREE NEW VAT             */ 03178000
         BAL   @14,FREECORE                                        1183 03179000
*     END;                                                         1184 03180000
@RF01182 DS    0H                                                  1185 03181000
*   MWAVATSW=OFF;                   /* RESET VAT ENTRY FOUND SW      */ 03182000
@RF01170 NI    MWAVATSW,B'11101111'                                1185 03183000
*   DO UNTIL(MWAVATSW=ON);                                         1186 03184000
@DL01186 DS    0H                                                  1187 03185000
*     MWAPTRA=JSCBSHR;              /* INIT PTR TO FIRST VAT         */ 03186000
         L     @05,OPWJSTCB(,RCORE)                                1187 03187000
         L     @05,TCBJSCB(,@05)                                   1187 03188000
         L     @05,JSCBSHR(,@05)                                   1187 03189000
         ST    @05,MWAPTRA                                         1187 03190000
*     MWAI=1;                       /* INIT PTR TO FIRST ENTRY       */ 03191000
         MVC   MWAI(4),@CF00053                                    1188 03192000
*     DO UNTIL(MWAPTRA=0|VATAMBL(MWAI)=0);/* SEARCH VAT CHAIN FOR  1189 03193000
*                                      INACTIVE ENTRY                */ 03194000
@DL01189 DS    0H                                                  1190 03195000
*       IF VATNAE<VATENO THEN       /* IF THIS VAT NOT FULL          */ 03196000
         L     @05,MWAPTRA                                         1190 03197000
         L     @15,VATNAE(,@05)                                    1190 03198000
         SLR   @14,@14                                             1190 03199000
         IC    @14,VATENO(,@05)                                    1190 03200000
         CR    @15,@14                                             1190 03201000
         BNL   @RF01190                                            1190 03202000
*         DO;                       /* THEN SEARCH ENTRIES           */ 03203000
*           DO MWAI=1 TO VATENO WHILE(VATAMBL(MWAI)^=0);/* SEARCH FOR   03204000
*                                      INACTIVE ENTRY                */ 03205000
         LA    @05,1                                               1192 03206000
         B     @DE01192                                            1192 03207000
@DL01192 LR    @15,@05                                             1192 03208000
         SLA   @15,2                                               1192 03209000
         L     @01,MWAPTRA                                         1192 03210000
         L     @15,VATAMBL-4(@15,@01)                              1192 03211000
         LTR   @15,@15                                             1192 03212000
         BZ    @DC01192                                            1192 03213000
*           END;                                                   1193 03214000
         AL    @05,@CF00053                                        1193 03215000
@DE01192 ST    @05,MWAI                                            1193 03216000
         L     @01,MWAPTRA                                         1193 03217000
         SLR   @00,@00                                             1193 03218000
         IC    @00,VATENO(,@01)                                    1193 03219000
