         BNH   @DL01192                                            1193 03221000
@DC01192 DS    0H                                                  1194 03222000
*           IF MWAI>VATENO THEN     /* IF INACTIVE ENTRY NOT FOUND   */ 03223000
         L     @05,MWAPTRA                                         1194 03224000
         L     @15,MWAI                                            1194 03225000
         SLR   @14,@14                                             1194 03226000
         IC    @14,VATENO(,@05)                                    1194 03227000
         CR    @15,@14                                             1194 03228000
         BNH   @RF01194                                            1194 03229000
*             DO;                                                  1195 03230000
*               MWAPTRB=MWAPTRA;    /* SAVE PTR TO LAST VAT          */ 03231000
         ST    @05,MWAPTRB                                         1196 03232000
*               MWAPTRA=VATNEXT;    /* POINT TO NEXT VAT             */ 03233000
         L     @05,VATNEXT(,@05)                                   1197 03234000
         ST    @05,MWAPTRA                                         1197 03235000
*               MWAI=1;             /* INIT INDEX TO FIRST ENTRY     */ 03236000
         MVC   MWAI(4),@CF00053                                    1198 03237000
*             END;                                                 1199 03238000
*         END;                                                     1200 03239000
*       ELSE                        /* ELSE VAT IS FULL              */ 03240000
*         DO;                                                      1201 03241000
         B     @RC01190                                            1201 03242000
@RF01190 DS    0H                                                  1202 03243000
*           MWAPTRB=MWAPTRA;        /* SAVE PTR TO LAST VAT          */ 03244000
         L     @05,MWAPTRA                                         1202 03245000
         ST    @05,MWAPTRB                                         1202 03246000
*           MWAPTRA=VATNEXT;        /* POINT TO NEXT VAT             */ 03247000
         L     @05,VATNEXT(,@05)                                   1203 03248000
         ST    @05,MWAPTRA                                         1203 03249000
*           MWAI=1;                 /* INIT INDEX TO FIRST ENTRY     */ 03250000
         MVC   MWAI(4),@CF00053                                    1204 03251000
*         END;                                                     1205 03252000
*     END;                                                         1206 03253000
@RC01190 DS    0H                                                  1206 03254000
@DE01189 L     @05,MWAPTRA                                         1206 03255000
         SLR   @15,@15                                             1206 03256000
         CR    @05,@15                                             1206 03257000
         BE    @DC01189                                            1206 03258000
         L     @14,MWAI                                            1206 03259000
         SLA   @14,2                                               1206 03260000
         C     @15,VATAMBL-4(@14,@05)                              1206 03261000
         BNE   @DL01189                                            1206 03262000
@DC01189 DS    0H                                                  1207 03263000
*     IF MWAPTRA^=0 THEN            /* IF INACTIVE ENTRY FOUND       */ 03264000
         L     @05,MWAPTRA                                         1207 03265000
         SLR   @15,@15                                             1207 03266000
         CR    @05,@15                                             1207 03267000
         BE    @RF01207                                            1207 03268000
*       DO;                         /* THEN ADD ENTRY TO VAT         */ 03269000
*         R6=0;                     /* ZERO COMPARE WORD             */ 03270000
         SLR   R6,R6                                               1209 03271000
*         CS(R6,MWAXAMBL,VATAMBL(MWAI));/* COMPARE AND SWAP AMBL PTR */ 03272000
         L     @14,MWAXAMBL                                        1210 03273000
         LR    @11,@14                                             1210 03274000
         L     @10,MWAI                                            1210 03275000
         LR    @09,@10                                             1210 03276000
         SLA   @09,2                                               1210 03277000
         ALR   @09,@05                                             1210 03278000
         CS    R6,@11,VATAMBL-4(@09)                               1210 03279000
*         IF R6=0 THEN              /* IF CS SUCCESSFUL              */ 03280000
         CR    R6,@15                                              1211 03281000
         BNE   @RF01211                                            1211 03282000
*           DO;                     /* THEN INIT VAT INFO IN AMBL    */ 03283000
*             AMBLVRT=VATVRT;       /* INIT TABLE NUMBER             */ 03284000
         IC    @15,VATVRT(,@05)                                    1213 03285000
         STC   @15,AMBLVRT(,@14)                                   1213 03286000
*             AMBLENO=MWAI;         /* INIT ENTRY NUMBER             */ 03287000
         STC   @10,AMBLENO(,@14)                                   1214 03288000
*             R6=VATNAE;            /* INIT COMPARE WORD TO NUMBER OF   03289000
*                                      ACTIVE ENTRIES                */ 03290000
         L     R6,VATNAE(,@05)                                     1215 03291000
*             DO UNTIL(R7=R6+1);    /* UPDATE NO. OF ENTRIES IN VAT  */ 03292000
@DL01216 DS    0H                                                  1217 03293000
*               R7=R6+1;            /* INIT SWAP WORD WITH         1217 03294000
*                                      INCREMENTED COUNTER           */ 03295000
         LA    R7,1                                                1217 03296000
         ALR   R7,R6                                               1217 03297000
*               CS(R6,R7,VATNAE);   /* COMPARE AND SWAP COUNTER      */ 03298000
         L     @05,MWAPTRA                                         1218 03299000
         CS    R6,@07,VATNAE(@05)                                  1218 03300000
*             END;                                                 1219 03301000
@DE01216 LA    @05,1                                               1219 03302000
         ALR   @05,R6                                              1219 03303000
         CR    R7,@05                                              1219 03304000
         BNE   @DL01216                                            1219 03305000
*             MWAVATSW=ON;          /* IND AMBL ENTRY ADDED TO VAT   */ 03306000
         OI    MWAVATSW,B'00010000'                                1220 03307000
*           END;                                                   1221 03308000
*       END;                                                       1222 03309000
*     ELSE                          /* INACTIVE ENTRY NOT FOUND      */ 03310000
*       DO;                         /* BUILD NEW VAT                 */ 03311000
         B     @RC01207                                            1223 03312000
@RF01207 DS    0H                                                  1224 03313000
*         MWAPTRA=MWAPTRB;          /* PT TO LAST VAT ON CHAIN       */ 03314000
         L     @05,MWAPTRB                                         1224 03315000
         ST    @05,MWAPTRA                                         1224 03316000
*         MWAGFSP=VATPOOL;          /* INIT SUBPOOL OF VAT           */ 03317000
         MVI   MWAGFSP,X'FC'                                       1225 03318000
*         MWAGFLEN=LENGTH(IDAVAT);  /* INIT LENGTH OF VAT            */ 03319000
         MVC   MWAGFLEN(3),@CF00034+1                              1226 03320000
*         CALL GETCORE;             /* OBTAIN STORAGE FOR VAT        */ 03321000
         BAL   @14,GETCORE                                         1227 03322000
*         R6=0;                     /* INIT COMPARE REG TO 0         */ 03323000
         SLR   R6,R6                                               1228 03324000
*         R7=MWAGFADR;              /* INIT SWAP REG                 */ 03325000
         L     R7,MWAGFADR                                         1229 03326000
*         R7->VATID=IDXVAT;         /* INIT CB ID            @OZ03937*/ 03327000
         MVI   VATID(R7),B'00010001'                               1230 03328000
*         R7->VATLEN=MWAGFLEN;      /* INIT LENGTH           @OZ03937*/ 03329000
         L     @05,MWAGFLEN-1                                      1231 03330000
         LA    @05,0(,@05)                                         1231 03331000
         STH   @05,VATLEN(,R7)                                     1231 03332000
*         R7->VATVRT=MWAPTRA->VATVRT+1;/* INIT TABLE NUMBER  @OZ03937*/ 03333000
         L     @05,MWAPTRA                                         1232 03334000
         LA    @15,1                                               1232 03335000
         SLR   @14,@14                                             1232 03336000
         IC    @14,VATVRT(,@05)                                    1232 03337000
         ALR   @15,@14                                             1232 03338000
         STC   @15,VATVRT(,R7)                                     1232 03339000
*         R7->VATENO=DIM(VATAMBL);  /* INIT NO. OF ENTRIES   @OZ03937*/ 03340000
