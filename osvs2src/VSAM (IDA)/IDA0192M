         LTR   @08,@08                                             0271 00526000
         BZ    @RF00271                                            0271 00527000
*             VGTTPSB=GSLPTR;       /* CHAIN NEW PSB TO THE VGTT     */ 00528000
         ST    @05,VGTTPSB(,@08)                                   0272 00529000
*           ELSE                                                   0273 00530000
*             ;                     /* PROT AMBL BLK REQ OR NO VGTT  */ 00531000
@RF00271 DS    0H                                                  0274 00532000
*         END;                                                     0274 00533000
*       RFY                                                        0275 00534000
*         HEBHDREL BASED(MSPHPTR);  /* BASE FOR NEW PROT SPH BLK     */ 00535000
*     END;                                                         0276 00536000
*   ELSE                                                           0277 00537000
*     ;                             /* GET SPACE FROM PROT SPH BLK   */ 00538000
@RF00249 DS    0H                                                  0278 00539000
*   GSLPTR=HEBNBYTE;                /* RETRN ADDR OF REQUESTED SPACE */ 00540000
         LR    @08,GSNDEX                                          0278 00541000
         MH    @08,@CH00096                                        0278 00542000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0278 00543000
         L     @05,OPWVANCS(,RCORE)                                0278 00544000
         L     @05,MSPHPTR(,@05)                                   0278 00545000
         L     @02,HEBNBYTE(,@05)                                  0278 00546000
         ST    @02,GSLPTR(,@08)                                    0278 00547000
*   HEBAVSP=HEBAVSP-GSLREQLN;       /* DECR AVAIL SPACE BY REQ LENGTH*/ 00548000
         L     @08,GSLREQLN-1(,@08)                                0279 00549000
