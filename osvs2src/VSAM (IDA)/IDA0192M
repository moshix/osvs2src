*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 01859000
*   RFY                                                            0671 01860000
*     HEBHDREL BASED(OPWVHDRE);     /* ESTABLISH BASE FOR HDR ELEM   */ 01861000
*   OPWVHDRE=BIBPRSPH;              /* PT TO PROTECTED SPHERE BLK HDR*/ 01862000
         L     @05,OPWBIB(,RCORE)                                  0672 01863000
         L     @05,BIBPRSPH(,@05)                                  0672 01864000
         ST    @05,OPWVHDRE(,RCORE)                                0672 01865000
*   IF ADDR(HEBHDREL)^=0&HEBAVSP^=0 THEN/* ANY EXCESS CORE TO FREE   */ 01866000
         LTR   @05,@05                                             0673 01867000
         BZ    @RF00673                                            0673 01868000
         L     @05,HEBAVSP-1(,@05)                                 0673 01869000
         LA    @05,0(,@05)                                         0673 01870000
         LTR   @05,@05                                             0673 01871000
         BZ    @RF00673                                            0673 01872000
*     CALL FREECORE;                /* YES, FREE EXCESS CORE         */ 01873000
         BAL   @14,FREECORE                                        0674 01874000
*   ELSE                                                           0675 01875000
*     ;                             /* NO, CHECK SPHERE BLOCK        */ 01876000
@RF00673 DS    0H                                                  0676 01877000
*   OPWVHDRE=BIBSPHPT;              /* PT TO SPHERE BLOCK HDR ELEM   */ 01878000
         L     @05,OPWBIB(,RCORE)                                  0676 01879000
         L     @05,BIBSPHPT(,@05)                                  0676 01880000
         ST    @05,OPWVHDRE(,RCORE)                                0676 01881000
*   DO INDEX2=2 TO 1 BY-1;          /* FREE EXCESS SPACE IN SPHERE 0677 01882000
*                                      AND PROTECTED AMBL BLOCKS     */ 01883000
         LA    INDEX2,2                                            0677 01884000
@DL00677 DS    0H                                                  0678 01885000
*     IF ADDR(HEBHDREL)^=0&HEBAVSP^=0 THEN/* EXCESS CORE TO FREE ?   */ 01886000
         L     @11,OPWVHDRE(,RCORE)                                0678 01887000
         LTR   @11,@11                                             0678 01888000
         BZ    @RF00678                                            0678 01889000
         L     @11,HEBAVSP-1(,@11)                                 0678 01890000
         LA    @11,0(,@11)                                         0678 01891000
         LTR   @11,@11                                             0678 01892000
         BZ    @RF00678                                            0678 01893000
*       DO;                         /* YES, FREE EXCESS CORE         */ 01894000
*         CALL FNDCSLEL;            /* FIND CSL CORRESPONDING TO BLK */ 01895000
         BAL   @14,FNDCSLEL                                        0680 01896000
*         RFY                                                      0681 01897000
*           CSLENTRY BASED(OPWVCSLE);/* EST BASE FOR CSL ELEMENT     */ 01898000
*         IF OPWVCSLE^=0 THEN       /* WAS THE CSL ELEMENT FOUND     */ 01899000
         L     @11,OPWVCSLE(,RCORE)                                0682 01900000
         LTR   @11,@11                                             0682 01901000
         BZ    @RF00682                                            0682 01902000
*           DO;                     /* YES, EXCESS CORE CAN BE FREED */ 01903000
*             CALL FREECORE;        /* FREE EXCESS CORE              */ 01904000
         BAL   @14,FREECORE                                        0684 01905000
*             CSLCORLN=HEBLN;       /* UPDATE LENGTH OF CORE IN CSL  */ 01906000
         L     @11,OPWVCSLE(,RCORE)                                0685 01907000
         L     @05,OPWVHDRE(,RCORE)                                0685 01908000
         MVC   CSLCORLN(3,@11),HEBLN(@05)                          0685 01909000
*           END;                                                   0686 01910000
*         ELSE                                                     0687 01911000
*           ;                       /* CSL NOT FOUND, CORE NOT FREED */ 01912000
@RF00682 DS    0H                                                  0688 01913000
*       END;                                                       0688 01914000
*     ELSE                                                         0689 01915000
*       ;                           /* NO EXCESS SPACE IN SPHERE BLK */ 01916000
@RF00678 DS    0H                                                  0690 01917000
*     OPWVHDRE=BIBPSAB;             /* PT TO PROT AMBL BLK HDR ELEM  */ 01918000
         L     @11,OPWBIB(,RCORE)                                  0690 01919000
         L     @11,BIBPSAB(,@11)                                   0690 01920000
         ST    @11,OPWVHDRE(,RCORE)                                0690 01921000
*   END;                                                           0691 01922000
         BCTR  INDEX2,0                                            0691 01923000
         LTR   INDEX2,INDEX2                                       0691 01924000
         BP    @DL00677                                            0691 01925000
*   RFY                                                            0692 01926000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 01927000
*   RET=OPWVSAVE(SAVENDEX);         /* RESTORE RETURN ADDRESS        */ 01928000
         LR    @05,SAVENDEX                                        0693 01929000
         SLA   @05,2                                               0693 01930000
         L     RET,OPWVSAVE-4(@05,RCORE)                           0693 01931000
*   RFY                                                            0694 01932000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 01933000
*   SAVENDEX=SAVENDEX-1;            /* POINT TO LAST SAVE AREA       */ 01934000
         BCTR  SAVENDEX,0                                          0695 01935000
*   END SPHFREE;                    /* RETURN TO THE CALLER          */ 01936000
@EL00009 DS    0H                                                  0696 01937000
@EF00009 DS    0H                                                  0696 01938000
@ER00009 BR    @14                                                 0696 01939000
*                                                                  0697 01940000
*/********************************************************************/ 01941000
*/*                                                                  */ 01942000
*/* FUNCTION: THIS ROUTINE IS USED WHEN AN ERROR CONDITION IS        */ 01943000
*/*           DETECTED DURING OPEN TO RELEASE ALL CORE GOTTEN FOR    */ 01944000
*/*           THIS CLUSTER AND, IF THIS IS A PRIMARY OPEN, ALL CORE  */ 01945000
*/*           GOTTEN FOR THE SPHERE.                                 */ 01946000
*/*                                                                  */ 01947000
*/* INPUTS:   A POINTER WITHIN THE OPEN WORK AREA TO THE CORE SAVE   */ 01948000
*/*           LIST(S).  A POINTER IN OPWVMANC TO THE PROTECTED       */ 01949000
*/*           SPHERE BLOCKS(S), IF THEY ARE TO BE FREED.  OTHERWISE, */ 01950000
*/*           OPWVMANC MUST BE ZERO.                                 */ 01951000
*/*                                                                  */ 01952000
*/* OUTPUTS:  NONE                                                   */ 01953000
*/*                                                                  */ 01954000
*/********************************************************************/ 01955000
*                                                                  0697 01956000
*CLEANUP:                                                          0697 01957000
*   PROC OPTIONS(NOSAVE);                                          0697 01958000
CLEANUP  DS    0H                                                  0698 01959000
*   SAVENDEX=SAVENDEX+1;            /* POINT TO NEXT SAVE AREA       */ 01960000
         AL    SAVENDEX,@CF00082                                   0698 01961000
*   RFY                                                            0699 01962000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 01963000
*   OPWVSAVE(SAVENDEX)=RET;         /* SAVE RETURN ADDRESS           */ 01964000
         LR    @15,SAVENDEX                                        0700 01965000
         SLA   @15,2                                               0700 01966000
         ST    RET,OPWVSAVE-4(@15,RCORE)                           0700 01967000
*   RFY                                                            0701 01968000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 01969000
*   RFY                                                            0702 01970000
*     IDACSL BASED(BIBCSL);         /* EST BASE FOR CSL CHAIN        */ 01971000
*   DO WHILE ADDR(IDACSL)^=0;       /* FREE ALL CORE DEFINED BY THE     01972000
*                                      CSL(S) AND FREE THE CSL(S)    */ 01973000
         B     @DE00703                                            0703 01974000
@DL00703 DS    0H                                                  0704 01975000
*     DO INDEX3=CSLACTEN TO 1 BY-1; /* FREE ALL CORE DEFINED BY THE     01976000
*                                      ACTIVE ENTRIES IN THIS CSL    */ 01977000
         L     @05,OPWBIB(,RCORE)                                  0704 01978000
         L     @05,BIBCSL(,@05)                                    0704 01979000
         LH    INDEX3,CSLACTEN(,@05)                               0704 01980000
         B     @DE00704                                            0704 01981000
@DL00704 DS    0H                                                  0705 01982000
*       RFY                                                        0705 01983000
*         CSLENTRY BASED(ADDR(CSLNTRYS(INDEX3)));/* EST BASE FOR CSL    01984000
*                                      ELEMENT ENTRIES               */ 01985000
*       CSLACTEN=INDEX3-1;          /* DECREMENT NO. ACTIVE ENTRIES  */ 01986000
         L     @08,OPWBIB(,RCORE)                                  0706 01987000
         L     @08,BIBCSL(,@08)                                    0706 01988000
         LR    @05,INDEX3                                          0706 01989000
         BCTR  @05,0                                               0706 01990000
         STH   @05,CSLACTEN(,@08)                                  0706 01991000
*       IF CSLCORLN^=0 THEN         /* ANY CORE TO BE FREED          */ 01992000
         LR    @05,INDEX3                                          0707 01993000
         MH    @05,@CH00096                                        0707 01994000
         LA    @08,CSLNTRYS-12(@05,@08)                            0707 01995000
         L     @05,CSLCORLN-1(,@08)                                0707 01996000
         LA    @05,0(,@05)                                         0707 01997000
         LTR   @05,@05                                             0707 01998000
         BZ    @RF00707                                            0707 01999000
*         DO;                       /* YES, FREE CORE BLOCK          */ 02000000
*           RFY                                                    0709 02001000
*            (R0,                                                  0709 02002000
*             R1) RSTD;             /* RESTRICT REGISTER USAGE       */ 02003000
*           LM(R0,R1,CSLFREMN);     /* LOAD FREEMAIN PARAMETERS      */ 02004000
         LM    R0,R1,CSLFREMN(@08)                                 0710 02005000
*           IF CSLKEY5=ON&CSLPOOLN=RDEBPOOL THEN/* BRANCH ENTRY TO 0711 02006000
*                                      FREEMAIN REQUIRED             */ 02007000
         TM    CSLKEY5(@08),B'10000000'                            0711 02008000
         BNO   @RF00711                                            0711 02009000
         CLI   CSLPOOLN(@08),230                                   0711 02010000
         BNE   @RF00711                                            0711 02011000
*             DO;                   /* YES, OBTAIN LOCAL LOCK FIRST  */ 02012000
*               R0=CSLCORLN;        /* REG 0 MUST ONLY CONTAIN LENGTH   02013000
*                                      FOR BRANCH ENTRY TO FREEMAIN     02014000
*                                      OBTAIN THE LOCAL LOCK         */ 02015000
         LR    R0,@05                                              0713 02016000
*               OPWVRG13=R13;       /* SAVE REG 13                   */ 02017000
         ST    R13,OPWVRG13(,RCORE)                                0714 02018000
*               DO;                 /* SETLOCK (OBTAIN) MODE(UNCOND)    02019000
*                                      TYPE(LOCAL) RELATED(FREEMAIN,I   02020000
*                                      DA0192M(CLEANUP))             */ 02021000
*                 RFY                                              0716 02022000
*                  (GPR11P,                                        0716 02023000
*                   GPR12P,                                        0716 02024000
*                   GPR13P,                                        0716 02025000
*                   GPR14P) RSTD;                                  0716 02026000
*                 GPR13P=PSALITA;   /* ADDRESS OF LOCK INTERFACE   0717 02027000
*                                      TABLE.                        */ 02028000
         L     GPR13P,PSALITA                                      0717 02029000
*                 GEN(LM GPR11P,GPR13P,576(GPR13P));/* LOAD LOCKWORD,   02030000
*                                      CLHT AND ENTRY ADRS.          */ 02031000
         LM GPR11P,GPR13P,576(GPR13P)                                   02032000
*                 GEN(BALR GPR14P,GPR13P);/* BALR TO SETLOCK RTN.    */ 02033000
         BALR GPR14P,GPR13P                                             02034000
*                 RFY                                              0720 02035000
*                  (GPR11P,                                        0720 02036000
*                   GPR12P,                                        0720 02037000
*                   GPR13P,                                        0720 02038000
*                   GPR14P) UNRSTD;                                0720 02039000
*               END;                /* SETLOCK (OBTAIN) MODE(UNCOND)    02040000
*                                      TYPE(LOCAL) RELATED(FREEMAIN,I   02041000
*                                      DA0192M(CLEANUP))             */ 02042000
*               R12=OPWVRG12;       /* RESTORE SECOND BASE REG       */ 02043000
         L     R12,OPWVRG12(,RCORE)                                0722 02044000
*               RFY                                                0723 02045000
*                (R4,                                              0723 02046000
*                 R7,                                              0723 02047000
*                 R11) RSTD;        /* RESTRICT REGISTERS            */ 02048000
*               R11=RBASEA;         /* SAVE MODULE BASE REG  @OZ04229*/ 02049000
         LR    R11,RBASEA                                          0724 02050000
*               R13=RCORE;          /* SAVE PTR TO O/C/E WORK AREA   */ 02051000
         LR    R13,RCORE                                           0725 02052000
*               IF CSLJSTCB=ON THEN /* CHECK CORE TCB OWNERSHIP      */ 02053000
         LR    @10,INDEX3                                          0726 02054000
