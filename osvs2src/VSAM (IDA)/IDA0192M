*           MVCL(R12,R14);          /* CLEAR GOTTEN CORE             */ 01657000
         MVCL  R12,R14                                             0614 01658000
*           LM(R12,R13,OPWVLSAV);   /* RESTORE BASE REG & REG 13     */ 01659000
         LM    R12,R13,OPWVLSAV(RCORE)                             0615 01660000
*           RFY                                                    0616 01661000
*            (R12,                                                 0616 01662000
*             R13,                                                 0616 01663000
*             R14,                                                 0616 01664000
*             R15) UNRSTD;          /* DROP REG RESTRICTIONS         */ 01665000
