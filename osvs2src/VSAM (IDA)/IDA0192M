*            (GPR01F,                                              0593 01604000
*             GPR15F,                                              0593 01605000
*             GPR00F) UNRESTRICTED;                                0593 01606000
*         END;                      /* GETMAIN (RC)                0594 01607000
*                                      LV(LENGTH(IDACSL)) SP(WKPOOL) */ 01608000
*       RFY                                                        0595 01609000
*        (R1,                                                      0595 01610000
*         RETCODE) RSTD;            /* RESTRICT GETMAIN REGISTERS    */ 01611000
@RC00576 DS    0H                                                  0596 01612000
*       IF RETCODE^=0 THEN          /* WAS GETMAIN SUCCESSFUL        */ 01613000
         SLR   @14,@14                                             0596 01614000
         CR    RETCODE,@14                                         0596 01615000
