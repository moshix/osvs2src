         TITLE 'IDA0192M - VSAM I/O SUPPORT VIRTUAL STORAGE MANAGER    *00001000
                        '                                               00002000
IDA0192M CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192M  78.151'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @12,4095(,@03)                                      0001 00012000
         USING @PSTART,@03                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
*   RSTATRTN=IDAR192M;              /* NOTE ENTRY TO IDA0192M        */ 00015000
         L     @08,OPWCOMWA(,RCORE)                                0133 00016000
         MVC   RSTATRTN(4,@08),@CC02991                            0133 00017000
*   RFY                                                            0134 00018000
*     IDABIB BASED(OPWBIB);         /* ESTABLISH BASE FOR BIB        */ 00019000
*   OPWVSMWA=OPWVSMWA&&OPWVSMWA;    /* CLEAR VIRTL STORAGE MNGR W/A  */ 00020000
         XC    OPWVSMWA(96,RCORE),OPWVSMWA(RCORE)                  0135 00021000
*   OPWVRG12=RBASEB;                /* SAVE BASE REG         @OZ04229*/ 00022000
         ST    RBASEB,OPWVRG12(,RCORE)                             0136 00023000
*   OPWVR13=R13;                    /* SAVE ADDRESS OF THE SAVE AREA */ 00024000
         ST    R13,OPWVR13(,RCORE)                                 0137 00025000
*   SAVENDEX=0;                     /* INITIALIZE SAVE AREA INDEX    */ 00026000
         SLR   SAVENDEX,SAVENDEX                                   0138 00027000
*   GSNDEX=1;                       /* INITIALIZE GETSPACE INDEX     */ 00028000
         LA    GSNDEX,1                                            0139 00029000
*   IF OPWVMTYP=DIRCTREQ THEN       /* IS THIS A DIRECT REQUEST      */ 00030000
         CLI   OPWVMTYP(RCORE),133                                 0140 00031000
         BNE   @RF00140                                            0140 00032000
*     CALL DIRREQ;                  /* YES, CALL DIRECT REQUEST RTN  */ 00033000
         BAL   @14,DIRREQ                                          0141 00034000
*   ELSE                            /* NO, CONT CHECKING REQUEST TYPE*/ 00035000
*     IF OPWVMTYP=FREECLSP THEN     /* IS CLUSTER SPACE TO BE FREED  */ 00036000
         B     @RC00140                                            0142 00037000
@RF00140 CLI   OPWVMTYP(RCORE),130                                 0142 00038000
         BNE   @RF00142                                            0142 00039000
*       CALL CLSRFREE;              /* YES, CALL CLUSTER FREE SPACE  */ 00040000
         BAL   @14,CLSRFREE                                        0143 00041000
*     ELSE                          /* NO, CONT CHECKING REQUEST TYPE*/ 00042000
*       IF OPWVMTYP=FREESPH THEN    /* IS SPHERE SPACE TO BE FREED   */ 00043000
         B     @RC00142                                            0144 00044000
@RF00142 CLI   OPWVMTYP(RCORE),131                                 0144 00045000
         BNE   @RF00144                                            0144 00046000
*         CALL SPHFREE;             /* YES, CALL SPHERE FREE SPACE   */ 00047000
         BAL   @14,SPHFREE                                         0145 00048000
*       ELSE                        /* NO, CONT CHECKING REQUEST TYPE*/ 00049000
*         IF OPWVMTYP=FREESREQ THEN /* SPECIAL REQ BLKS TO BE FREED  */ 00050000
         B     @RC00144                                            0146 00051000
@RF00144 CLI   OPWVMTYP(RCORE),135                                 0146 00052000
         BNE   @RF00146                                            0146 00053000
*           CALL SREQFREE;          /* YES, FREE SPECIAL REQ BLOCKS  */ 00054000
         BAL   @14,SREQFREE                                        0147 00055000
*         ELSE                      /* NO, CONT CHECKING REQUEST TYPE*/ 00056000
*           IF OPWVMTYP=ERRCLNUP THEN/* ERROR CLEAN UP TO BE DONE    */ 00057000
         B     @RC00146                                            0148 00058000
@RF00146 CLI   OPWVMTYP(RCORE),132                                 0148 00059000
         BNE   @RF00148                                            0148 00060000
*             CALL CLEANUP;         /* YES, CALL ERROR CLEAN UP RTN  */ 00061000
         BAL   @14,CLEANUP                                         0149 00062000
*           ELSE                    /* NO, CONT CHECKING REQUEST TYPE*/ 00063000
*             DO;                   /* CHECK FOR GETSPACE REQUESTS   */ 00064000
         B     @RC00148                                            0150 00065000
@RF00148 DS    0H                                                  0151 00066000
*               OPWVMLNG=(OPWVMLNG+3)&-FULLWDLN;/* ROUND LENGTH OF 0151 00067000
*                                      REQUEST TO A FULLWORD         */ 00068000
         LA    @08,3                                               0151 00069000
         L     @05,OPWVMLNG-1(,RCORE)                              0151 00070000
         LA    @05,0(,@05)                                         0151 00071000
         ALR   @08,@05                                             0151 00072000
         N     @08,@CF03172                                        0151 00073000
         STCM  @08,7,OPWVMLNG(RCORE)                               0151 00074000
*               GSLREQLN=OPWVMLNG;  /* INIT LENGTH OF REQUEST        */ 00075000
         LR    @05,GSNDEX                                          0152 00076000
         MH    @05,@CH00096                                        0152 00077000
         LA    @05,OPWVGSPL-12(@05,RCORE)                          0152 00078000
         STCM  @08,7,GSLREQLN(@05)                                 0152 00079000
*               IF OPWVMTYP=PRSPHBLK THEN/* PROTECTED SPHERE BLK REQ */ 00080000
         CLI   OPWVMTYP(RCORE),129                                 0153 00081000
         BNE   @RF00153                                            0153 00082000
*                 DO;               /* YES, SET UP PROT SPH BLK PARMS*/ 00083000
*                   OPWVANCS=ADDR(BIBPRSPH);/* SET UP PTR TO       0155 00084000
*                                      PROTECTED SPHERE BLOCK PTR    */ 00085000
         L     @08,OPWBIB(,RCORE)                                  0155 00086000
         LA    @08,BIBPRSPH(,@08)                                  0155 00087000
         ST    @08,OPWVANCS(,RCORE)                                0155 00088000
*                   IF OPWLSR=ON THEN/* IF LSR OPEN          @OZ04230*/ 00089000
         TM    OPWLSR(RCORE),B'00100000'                           0156 00090000
         BNO   @RF00156                                            0156 00091000
*                     GSLSPNO=PRTBLKPL;/* SUBPOOL IN MEMORY  @OZ04230*/ 00092000
         MVI   GSLSPNO(@05),X'FC'                                  0157 00093000
*                   ELSE            /* NOT LSR, SO           @OZ04230*/ 00094000
*                     GSLSPNO=PRTSPPL;/* USE SUBPOOL IN CSA  @OZ04230*/ 00095000
         B     @RC00156                                            0158 00096000
@RF00156 LR    @08,GSNDEX                                          0158 00097000
         MH    @08,@CH00096                                        0158 00098000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0158 00099000
         MVI   GSLSPNO(@08),X'F1'                                  0158 00100000
*                   GSLNOCSL=ON;    /* IND NO CSL ELEM IS TO BE BUILT*/ 00101000
@RC00156 LR    @08,GSNDEX                                          0159 00102000
         MH    @08,@CH00096                                        0159 00103000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0159 00104000
         OI    GSLNOCSL(@08),B'00001000'                           0159 00105000
*                   CALL PRTSPREQ;  /* CALL PROTECTED SPHERE BLK RTN */ 00106000
         BAL   @14,PRTSPREQ                                        0160 00107000
*                 END;                                             0161 00108000
*               ELSE                /* NO, CONT CHECKING REQUEST TYPE*/ 00109000
*                 IF OPWVMTYP=PSAMBLBK THEN/* PROTECTED AMBL BLK REQ */ 00110000
         B     @RC00153                                            0162 00111000
@RF00153 CLI   OPWVMTYP(RCORE),134                                 0162 00112000
         BNE   @RF00162                                            0162 00113000
*                   DO;             /* YES, INIT PROT AMBL BLK PARMS */ 00114000
*                     OPWVANCS=ADDR(BIBPSAB);/* SET UP PTR TO      0164 00115000
*                                      PROTECTED AMBL BLOCK PTR      */ 00116000
         L     @08,OPWBIB(,RCORE)                                  0164 00117000
         LA    @08,BIBPSAB(,@08)                                   0164 00118000
         ST    @08,OPWVANCS(,RCORE)                                0164 00119000
*                     IF OPWGSR=ON|OPWCBIC=ON THEN/* IF REQUEST FOR     00120000
*                                      GSR OR CBIC           @ZA16012*/ 00121000
         TM    OPWGSR(RCORE),B'01000000'                           0165 00122000
         BO    @RT00165                                            0165 00123000
         TM    OPWCBIC(RCORE),B'10000000'                          0165 00124000
         BNO   @RF00165                                            0165 00125000
@RT00165 DS    0H                                                  0166 00126000
*                       GSLSPNO=GSRSPOOL;/* YES, INIT TO GSR SP      */ 00127000
         LR    @08,GSNDEX                                          0166 00128000
         MH    @08,@CH00096                                        0166 00129000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0166 00130000
         MVI   GSLSPNO(@08),X'F1'                                  0166 00131000
*                     ELSE          /* NO, OPEN IS FOR LSR OR DS     */ 00132000
*                       GSLSPNO=AMBLPOOL;/* INIT TO SP FOR LSR OR DS */ 00133000
         B     @RC00165                                            0167 00134000
@RF00165 LR    @08,GSNDEX                                          0167 00135000
         MH    @08,@CH00096                                        0167 00136000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0167 00137000
         MVI   GSLSPNO(@08),X'FC'                                  0167 00138000
*                     CALL PRTSPREQ;/* CALL PROTECTED SPHERE BLK RTN */ 00139000
@RC00165 BAL   @14,PRTSPREQ                                        0168 00140000
*                   END;                                           0169 00141000
*                 ELSE              /* NO, CONT CHECKING REQUEST TYPE*/ 00142000
*                   DO;             /* CHK FOR REQ DRIVEN BY GETSPACE*/ 00143000
         B     @RC00162                                            0170 00144000
@RF00162 DS    0H                                                  0171 00145000
*                     IF OPWVMTYP=SPHBLK THEN/* IS THIS A SPHERE BLK    00146000
*                                      REQUEST                       */ 00147000
         CLI   OPWVMTYP(RCORE),128                                 0171 00148000
         BNE   @RF00171                                            0171 00149000
*                       DO;         /* YES, SET UP SPHERE BLK PARMS  */ 00150000
*                         OPWVANCP=ADDR(BIBSPHPT);/* SET UP PTR TO 0173 00151000
*                                      HEB HDR ELEMENT PTR           */ 00152000
         L     @08,OPWBIB(,RCORE)                                  0173 00153000
         LA    @08,BIBSPHPT(,@08)                                  0173 00154000
         ST    @08,OPWVANCP(,RCORE)                                0173 00155000
*                         OPWVTBLP=ADDR(SPHREQTB);/* SET UP PTR TO 0174 00156000
*                                      REQUEST TYPE TABLE            */ 00157000
         LA    @08,SPHREQTB                                        0174 00158000
         ST    @08,OPWVTBLP(,RCORE)                                0174 00159000
*                       END;                                       0175 00160000
*                     ELSE          /* NO, REQUEST IS CLUSTER RELATED*/ 00161000
*                       DO;         /* SET UP CLUSTER RELTD BLK PARMS*/ 00162000
         B     @RC00171                                            0176 00163000
@RF00171 DS    0H                                                  0177 00164000
*                         RFY                                      0177 00165000
*                           IDACMB BASED(OPWVMANC);/* BASE FOR CMB   */ 00166000
*                         OPWVANCP=ADDR(CMBENTRY(OPWVMTYP));/* SET UP   00167000
*                                      POINTER TO CMB HEB ENTRY      */ 00168000
         SLR   @08,@08                                             0178 00169000
         IC    @08,OPWVMTYP(,RCORE)                                0178 00170000
         LR    @05,@08                                             0178 00171000
         SLA   @05,2                                               0178 00172000
         L     @02,OPWVMANC(,RCORE)                                0178 00173000
         LA    @05,CMBENTRY-4(@05,@02)                             0178 00174000
         ST    @05,OPWVANCP(,RCORE)                                0178 00175000
*                         OPWVTBLP=ADDR(CLSTREQT(OPWVMTYP));/* SET UP   00176000
*                                      POINTER TO REQUEST TYPE TABLE */ 00177000
         MH    @08,@CH02046                                        0179 00178000
         LA    @08,CLSTREQT-10(@08)                                0179 00179000
         ST    @08,OPWVTBLP(,RCORE)                                0179 00180000
*                       END;                                       0180 00181000
*                     IF OPWCAT=ON|OPWSCRA=ON|OPWVVIC=ON THEN/* IS 0181 00182000
*                                      REQUEST FOR CATALOGUE         */ 00183000
@RC00171 TM    OPWCAT(RCORE),B'11100000'                           0181 00184000
         BZ    @RF00181                                            0181 00185000
*                       DO;         /* YES, INIT CATALOG PARAMETERS  */ 00186000
*                         GSLSPNO=REQCATSP;/* INIT SUBPOOL FOR CATLG */ 00187000
         LR    @08,GSNDEX                                          0183 00188000
         MH    @08,@CH00096                                        0183 00189000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0183 00190000
         L     @05,OPWVTBLP(,RCORE)                                0183 00191000
         IC    @02,REQCATSP(,@05)                                  0183 00192000
         STC   @02,GSLSPNO(,@08)                                   0183 00193000
*                         GSLFLAGS=REQCATFG;/* INIT CATALOG FLAGS    */ 00194000
         MVC   GSLFLAGS(1,@08),REQCATFG(@05)                       0184 00195000
*                       END;                                       0185 00196000
*                     ELSE          /* NO, CHECK FOR GSR             */ 00197000
*                       IF OPWGSR=ON THEN/* IS THE REQUSEST FOR GSR  */ 00198000
         B     @RC00181                                            0186 00199000
@RF00181 TM    OPWGSR(RCORE),B'01000000'                           0186 00200000
         BNO   @RF00186                                            0186 00201000
*                         DO;       /* YES, INIT GSR PARAMETERS      */ 00202000
*                           GSLSPNO=REQGSRSP;/* INIT SUPPOOL FOR GSR */ 00203000
         LR    @08,GSNDEX                                          0188 00204000
         MH    @08,@CH00096                                        0188 00205000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0188 00206000
         L     @05,OPWVTBLP(,RCORE)                                0188 00207000
         IC    @02,REQGSRSP(,@05)                                  0188 00208000
         STC   @02,GSLSPNO(,@08)                                   0188 00209000
*                           GSLFLAGS=REQGSRFG;/* INIT GSR FLAGS      */ 00210000
         MVC   GSLFLAGS(1,@08),REQGSRFG(@05)                       0189 00211000
*                         END;                                     0190 00212000
*                       ELSE        /* NO, REQUEST IS FOR O/C/EOV    */ 00213000
*                         IF OPWCBIC=ON THEN/* IS THE REQUEST FOR  0191 00214000
*                                      CBIC                  @ZA16012*/ 00215000
         B     @RC00186                                            0191 00216000
@RF00186 TM    OPWCBIC(RCORE),B'10000000'                          0191 00217000
         BNO   @RF00191                                            0191 00218000
*                           DO;     /* YES, INIT CBIC PARMS  @ZA16012*/ 00219000
*                             GSLSPNO=REQCBCSP;/* INIT SUBPOOL FOR 0193 00220000
*                                      CBIC                  @ZA16012*/ 00221000
         LR    @08,GSNDEX                                          0193 00222000
         MH    @08,@CH00096                                        0193 00223000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0193 00224000
         L     @05,OPWVTBLP(,RCORE)                                0193 00225000
         IC    @02,REQCBCSP(,@05)                                  0193 00226000
         STC   @02,GSLSPNO(,@08)                                   0193 00227000
*                             GSLFLAGS=REQCBCFG;/* INIT CBIC FLAGS 0194 00228000
*                                                            @ZA16012*/ 00229000
         MVC   GSLFLAGS(1,@08),REQCBCFG(@05)                       0194 00230000
*                           END;    /*                       @ZA16012*/ 00231000
*                         ELSE      /* NO, REQUEST IS FOR O/C/EOV    */ 00232000
*                           DO;     /* INIT O/C/EOV PARAMETERS       */ 00233000
         B     @RC00191                                            0196 00234000
@RF00191 DS    0H                                                  0197 00235000
*                             GSLSPNO=REQOCESP;/* INIT O/C/E SUBPOOL */ 00236000
         LR    @08,GSNDEX                                          0197 00237000
         MH    @08,@CH00096                                        0197 00238000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0197 00239000
         L     @05,OPWVTBLP(,RCORE)                                0197 00240000
         IC    @02,REQOCESP(,@05)                                  0197 00241000
         STC   @02,GSLSPNO(,@08)                                   0197 00242000
*                             GSLFLAGS=REQOCEFG;/* INIT O/C/EOV FLAGS*/ 00243000
         MVC   GSLFLAGS(1,@08),REQOCEFG(@05)                       0198 00244000
*                           END;                                   0199 00245000
*                     GSLFLAGS=GSLFLAGS|OPWVMFLG;/* MERGE WITH INPUT    00246000
*                                      FLAGS IN O/C/EOV WORK AREA    */ 00247000
@RC00191 DS    0H                                                  0200 00248000
@RC00186 DS    0H                                                  0200 00249000
@RC00181 LR    @08,GSNDEX                                          0200 00250000
         MH    @08,@CH00096                                        0200 00251000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0200 00252000
         OC    GSLFLAGS(1,@08),OPWVMFLG(RCORE)                     0200 00253000
*                     CALL GETSPACE;/* GET REQUESTED CORE            */ 00254000
         BAL   @14,GETSPACE                                        0201 00255000
*                   END;                                           0202 00256000
*               OPWVMADR=GSLPTR;    /* RETURN ADDR OF CORE TO CALLER */ 00257000
@RC00162 DS    0H                                                  0203 00258000
@RC00153 LR    @08,GSNDEX                                          0203 00259000
         MH    @08,@CH00096                                        0203 00260000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0203 00261000
         L     @08,GSLPTR(,@08)                                    0203 00262000
         ST    @08,OPWVMADR(,RCORE)                                0203 00263000
*             END;                                                 0204 00264000
*   R13=OPWVR13;                    /* RESTORE ADDRESS OF SAVE AREA  */ 00265000
@RC00148 DS    0H                                                  0205 00266000
@RC00146 DS    0H                                                  0205 00267000
@RC00144 DS    0H                                                  0205 00268000
@RC00142 DS    0H                                                  0205 00269000
@RC00140 L     R13,OPWVR13(,RCORE)                                 0205 00270000
*   RETURN;                         /* RETURN TO THE CALLER          */ 00271000
@EL00001 DS    0H                                                  0206 00272000
@EF00001 DS    0H                                                  0206 00273000
@ER00001 LM    @14,@12,12(@13)                                     0206 00274000
         BR    @14                                                 0206 00275000
*                                                                  0207 00276000
*/********************************************************************/ 00277000
*/*                                                                  */ 00278000
*/* FUNCTION: THIS ROUTINE IS USED TO PROCESS DIRECT REQUESTS FOR    */ 00279000
*/*           CORE.                                                  */ 00280000
*/*                                                                  */ 00281000
*/* INPUTS:   VIRTUAL STORAGE MANAGER PARAMETER LIST WITHIN THE OPEN */ 00282000
*/*           WORK AREA.                                             */ 00283000
*/*                                                                  */ 00284000
*/* OUTPUTS:  A POINTER TO THE REQUESTED CORE IN OPWVMADR OR ZERO IF */ 00285000
*/*           UNABLE TO OBTAIN SPACE.                                */ 00286000
*/*                                                                  */ 00287000
*/********************************************************************/ 00288000
*                                                                  0207 00289000
*DIRREQ:                                                           0207 00290000
*   PROC OPTIONS(NOSAVE);                                          0207 00291000
DIRREQ   DS    0H                                                  0208 00292000
*   SAVENDEX=SAVENDEX+1;            /* POINT TO NEXT SAVE AREA       */ 00293000
         AL    SAVENDEX,@CF00082                                   0208 00294000
*   RFY                                                            0209 00295000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 00296000
*   OPWVSAVE(SAVENDEX)=RET;         /* SAVE RETURN ADDRESS           */ 00297000
         LR    @15,SAVENDEX                                        0210 00298000
         SLA   @15,2                                               0210 00299000
         ST    RET,OPWVSAVE-4(@15,RCORE)                           0210 00300000
*   RFY                                                            0211 00301000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 00302000
*   GSLSPNO=OPWVMSP;                /* INIT SUBPOOL NUMBER           */ 00303000
         LR    @08,GSNDEX                                          0212 00304000
         MH    @08,@CH00096                                        0212 00305000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0212 00306000
         IC    @05,OPWVMSP(,RCORE)                                 0212 00307000
         STC   @05,GSLSPNO(,@08)                                   0212 00308000
*   GSLGETLN=OPWVMLNG;              /* INIT LENGTH OF REQUEST        */ 00309000
         MVC   GSLGETLN(3,@08),OPWVMLNG(RCORE)                     0213 00310000
*   GSLFLAGS=OPWVMFLG;              /* INIT REQUEST FLAGS            */ 00311000
         MVC   GSLFLAGS(1,@08),OPWVMFLG(RCORE)                     0214 00312000
*   CALL GETCORE;                   /* CALL GETCORE ROUTINE          */ 00313000
         BAL   @14,GETCORE                                         0215 00314000
*   RFY                                                            0216 00315000
*     CSLENTRY BASED(OPWVCSLE);     /* BASE FOR NEW CSL ENTRY        */ 00316000
*   IF GSLPTR^=0 THEN               /* WAS GETCORE SUCCESSFUL        */ 00317000
         LR    @08,GSNDEX                                          0217 00318000
         MH    @08,@CH00096                                        0217 00319000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0217 00320000
         L     @05,GSLPTR(,@08)                                    0217 00321000
         LTR   @05,@05                                             0217 00322000
         BZ    @RF00217                                            0217 00323000
*     OPWVMLNG=GSLGETLN;            /* RETRN ACTUAL LENGTH TO CALLER */ 00324000
         MVC   OPWVMLNG(3,RCORE),GSLGETLN(@08)                     0218 00325000
*   ELSE                                                           0219 00326000
*     ;                             /* NO, NO FURTHER PROCESSING REQ */ 00327000
@RF00217 DS    0H                                                  0220 00328000
*   OPWVMADR=GSLPTR;                /* RETURN ADDR OF CORE TO CALLER */ 00329000
         LR    @08,GSNDEX                                          0220 00330000
         MH    @08,@CH00096                                        0220 00331000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0220 00332000
         L     @08,GSLPTR(,@08)                                    0220 00333000
         ST    @08,OPWVMADR(,RCORE)                                0220 00334000
*   RFY                                                            0221 00335000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 00336000
*   RET=OPWVSAVE(SAVENDEX);         /* RESTORE RETURN ADDRESS        */ 00337000
         LR    @05,SAVENDEX                                        0222 00338000
         SLA   @05,2                                               0222 00339000
         L     RET,OPWVSAVE-4(@05,RCORE)                           0222 00340000
*   RFY                                                            0223 00341000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 00342000
*   SAVENDEX=SAVENDEX-1;            /* POINT TO LAST SAVE AREA       */ 00343000
         BCTR  SAVENDEX,0                                          0224 00344000
*   END DIRREQ;                     /* RETURN TO THE CALLER          */ 00345000
@EL00002 DS    0H                                                  0225 00346000
@EF00002 DS    0H                                                  0225 00347000
@ER00002 BR    @14                                                 0225 00348000
*                                                                  0226 00349000
*/********************************************************************/ 00350000
*/*                                                                  */ 00351000
*/* FUNCTION: THIS ROUTINE IS USED TO GET SPACE IN PROTECTED SPHERE  */ 00352000
*/*           AND PROTECTED AMBL BLOCKS.                             */ 00353000
*/*                                                                  */ 00354000
*/* INPUTS:   VIRTUAL STORAGE MANAGER AND GETSPACE PARAMETER LISTS   */ 00355000
*/*           WITHIN THE OPEN WORK AREA.                             */ 00356000
*/*                                                                  */ 00357000
*/* OUTPUTS:  A POINTER TO THE REQUESTED CORE IN GSLPTR OR ZERO IF   */ 00358000
*/*           UNABLE TO OBTAIN SPACE.                                */ 00359000
*/*                                                                  */ 00360000
*/********************************************************************/ 00361000
*                                                                  0226 00362000
*PRTSPREQ:                                                         0226 00363000
*   PROC OPTIONS(NOSAVE);                                          0226 00364000
PRTSPREQ DS    0H                                                  0227 00365000
*   SAVENDEX=SAVENDEX+1;            /* POINT TO NEXT SAVE AREA       */ 00366000
         AL    SAVENDEX,@CF00082                                   0227 00367000
*   RFY                                                            0228 00368000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 00369000
*   OPWVSAVE(SAVENDEX)=RET;         /* SAVE RETURN ADDRESS           */ 00370000
         LR    @15,SAVENDEX                                        0229 00371000
         SLA   @15,2                                               0229 00372000
         ST    RET,OPWVSAVE-4(@15,RCORE)                           0229 00373000
*   RFY                                                            0230 00374000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 00375000
*   RFY                                                            0231 00376000
*     HEBHDREL BASED(MSPHPTR);      /* ESTABLISH BASE FOR HEB        */ 00377000
*   IF ADDR(HEBHDREL)^=0&HEBAVSP^=0&HEBAVSP<GSLREQLN THEN/* DET IF 0232 00378000
*                                      ANY BLKS EXIST, IF SO IS    0232 00379000
*                                      ENOUGH CORE AVAILABLE FOR THE    00380000
*                                      REQUEST                       */ 00381000
