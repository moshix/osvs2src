*             END;                                                 0243 00418000
*           ELSE                    /* CSL ENTRY WAS NOT FOUND       */ 00419000
*             HEBAVSP=0;            /* ZERO AMNT OF AVAILABLE SPACE  */ 00420000
         B     @RC00238                                            0244 00421000
