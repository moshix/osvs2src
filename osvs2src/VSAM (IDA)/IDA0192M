         ST    @15,HEBELCHN(,@05)                                  0267 00510000
*           HEBNBYTE=ADDR(HEBHDREL)+LENGTH(HEBHDREL);/* PT TO FIRST     00511000
*                                      BYTE OF UNUSED SPACE IN NEW 0268 00512000
*                                      BLK                           */ 00513000
         ALR   @02,@05                                             0268 00514000
         ST    @02,HEBNBYTE(,@05)                                  0268 00515000
*           MSPHPTR=GSLPTR;         /* CHAIN NEW BLK TO THE BIB      */ 00516000
         ST    @05,MSPHPTR(,@08)                                   0269 00517000
*           RFY                                                    0270 00518000
*             IDAVGTT BASED(BIBVGTT);/* ESTABLISH BASE FOR VGTT      */ 00519000
