         IC    @02,GSLSPNO(,@08)                                   0263 00493000
         STC   @02,HEBSP(,@05)                                     0263 00494000
*           HEBLN=GSLGETLN;         /* INIT SIZE OF NEW BLOCK        */ 00495000
         L     @08,GSLGETLN-1(,@08)                                0264 00496000
