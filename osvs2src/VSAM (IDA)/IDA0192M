*     LM(R0,R1,HEBFREMN);           /* LOAD FREEMAIN PARAMETERS      */ 02235000
         L     @05,OPWVMANC(,RCORE)                                0783 02236000
         LM    R0,R1,HEBFREMN(@05)                                 0783 02237000
*     OPWVMANC=HEBELCHN;            /* POINT TO NEXT PROT SPHERE BLK */ 02238000
         L     @05,HEBELCHN(,@05)                                  0784 02239000
         ST    @05,OPWVMANC(,RCORE)                                0784 02240000
*     GEN( FREEMAIN R,LV=(0),A=(1)) /* FREE PROTECTED SPHERE BLOCK   */ 02241000
*     REFS(R0,R1);                                                 0785 02242000
          FREEMAIN R,LV=(0),A=(1)                                       02243000
*     RFY                                                          0786 02244000
*      (R0,                                                        0786 02245000
*       R1) UNRSTD;                 /* DROP REG RESTRICTIONS         */ 02246000
*     BIBPRSPH=OPWVMANC;            /* UPDATE BIB PROT SPH BLK PTR   */ 02247000
         L     @05,OPWBIB(,RCORE)                                  0787 02248000
         L     @02,OPWVMANC(,RCORE)                                0787 02249000
         ST    @02,BIBPRSPH(,@05)                                  0787 02250000
*     IF BIBVGTT^=0 THEN            /* DOES A VGTT EXIST             */ 02251000
         L     @05,BIBVGTT(,@05)                                   0788 02252000
         LTR   @05,@05                                             0788 02253000
         BZ    @RF00788                                            0788 02254000
*       VGTTPSB=OPWVMANC;           /* YES, UPDATE VGTT PSB PTR      */ 02255000
         ST    @02,VGTTPSB(,@05)                                   0789 02256000
*   END;                                                           0790 02257000
@RF00788 DS    0H                                                  0790 02258000
@DE00781 L     @05,OPWVMANC(,RCORE)                                0790 02259000
         LTR   @05,@05                                             0790 02260000
         BNZ   @DL00781                                            0790 02261000
*   RFY                                                            0791 02262000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 02263000
*   RET=OPWVSAVE(SAVENDEX);         /* RESTORE RETURN ADDRESS        */ 02264000
         LR    @05,SAVENDEX                                        0792 02265000
         SLA   @05,2                                               0792 02266000
         L     RET,OPWVSAVE-4(@05,RCORE)                           0792 02267000
*   RFY                                                            0793 02268000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 02269000
*   SAVENDEX=SAVENDEX-1;            /* POINT TO LAST SAVE AREA       */ 02270000
         BCTR  SAVENDEX,0                                          0794 02271000
*   END CLEANUP;                                                   0795 02272000
@EL00010 DS    0H                                                  0795 02273000
@EF00010 DS    0H                                                  0795 02274000
@ER00010 BR    @14                                                 0795 02275000
*                                                                  0796 02276000
*/********************************************************************/ 02277000
*/*                                                                  */ 02278000
*/* FUNCTION: THIS ROUTINE IS USED TO FREE CLUSTER BLOCKS GOTTEN     */ 02279000
*/*           DURING CLOSE.                                          */ 02280000
*/*                                                                  */ 02281000
*/* INPUTS:   POINTER IN OPWVANCP TO THE CMB.                        */ 02282000
*/*                                                                  */ 02283000
*/* OUTPUTS:  NONE                                                   */ 02284000
*/*                                                                  */ 02285000
*/********************************************************************/ 02286000
*                                                                  0796 02287000
*SREQFREE:                                                         0796 02288000
*   PROC OPTIONS(NOSAVE);                                          0796 02289000
SREQFREE DS    0H                                                  0797 02290000
*   SAVENDEX=SAVENDEX+1;            /* POINT TO NEXT SAVE AREA       */ 02291000
         LA    @05,1                                               0797 02292000
         ALR   SAVENDEX,@05                                        0797 02293000
*   RFY                                                            0798 02294000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 02295000
*   OPWVSAVE(SAVENDEX)=RET;         /* SAVE RETURN ADDRESS           */ 02296000
         LR    @15,SAVENDEX                                        0799 02297000
         SLA   @15,2                                               0799 02298000
         ST    RET,OPWVSAVE-4(@15,RCORE)                           0799 02299000
*   RFY                                                            0800 02300000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 02301000
*   RFY                                                            0801 02302000
*     IDACMB BASED(OPWVANCP);       /* ESTABLISH BASE FOR CMB        */ 02303000
*   DO INDEX2=1 TO LENGTH(CMBPTRS)/4 BY 1;/* SCAN CMB ENTRIES FOR  0802 02304000
*                                      BLOCKS GOTTEN DURING CLOSE    */ 02305000
         LR    INDEX2,@05                                          0802 02306000
@DL00802 DS    0H                                                  0803 02307000
*     RFY                                                          0803 02308000
*       HEBHDREL BASED(CMBENTRY(INDEX2));/* EST HEB HDR ELEM BASE    */ 02309000
*     DO WHILE ADDR(HEBHDREL)^=0&HEBIOSUP=ON;/* SCAN EACH CMB ENTRY     02310000
*                                      FOR BLKS GOTTEN DURING CLOSE  */ 02311000
         B     @DE00804                                            0804 02312000
@DL00804 DS    0H                                                  0805 02313000
*       OPWVHDRE=ADDR(HEBHDREL);    /* PT TO FIRST HDR ELEM IN CHAIN */ 02314000
         LR    @11,INDEX2                                          0805 02315000
         SLA   @11,2                                               0805 02316000
         L     @05,OPWVANCP(,RCORE)                                0805 02317000
         L     @11,CMBENTRY-4(@11,@05)                             0805 02318000
         ST    @11,OPWVHDRE(,RCORE)                                0805 02319000
*       RFY                                                        0806 02320000
*         HEBHDREL BASED(OPWVHDRE); /* EST BASE FOR HEB HDR ELEM     */ 02321000
*       HEBAVSP=HEBLN;              /* FREE ALL CORE DEFND BY HEB HDR*/ 02322000
         MVC   HEBAVSP(3,@11),HEBLN(@11)                           0807 02323000
*       CALL FREECORE;              /* FREE CORE GOTTEN FOR CLOSE    */ 02324000
         BAL   @14,FREECORE                                        0808 02325000
*       CMBENTRY(INDEX2)=HEBELCHN;  /* PT TO NEXT HDR ELEM IN CHAIN  */ 02326000
         LR    @11,INDEX2                                          0809 02327000
         SLA   @11,2                                               0809 02328000
         L     @05,OPWVANCP(,RCORE)                                0809 02329000
         L     @02,OPWVHDRE(,RCORE)                                0809 02330000
         L     @15,HEBELCHN(,@02)                                  0809 02331000
         ST    @15,CMBENTRY-4(@11,@05)                             0809 02332000
*       HEBELCHN=BIBHEBFQ;          /* RETURN HEB HDR ELEMENT TO THE */ 02333000
         L     @11,OPWBIB(,RCORE)                                  0810 02334000
         L     @05,BIBHEBFQ(,@11)                                  0810 02335000
         ST    @05,HEBELCHN(,@02)                                  0810 02336000
*       BIBHEBFQ=OPWVHDRE;          /* FREE QUEUE                    */ 02337000
         ST    @02,BIBHEBFQ(,@11)                                  0811 02338000
*     END;                                                         0812 02339000
@DE00804 LR    @11,INDEX2                                          0812 02340000
         SLA   @11,2                                               0812 02341000
         L     @05,OPWVANCP(,RCORE)                                0812 02342000
         L     @11,CMBENTRY-4(@11,@05)                             0812 02343000
         LTR   @11,@11                                             0812 02344000
         BZ    @DC00804                                            0812 02345000
         TM    HEBIOSUP(@11),B'00010000'                           0812 02346000
         BO    @DL00804                                            0812 02347000
@DC00804 DS    0H                                                  0813 02348000
*   END;                                                           0813 02349000
         AL    INDEX2,@CF00082                                     0813 02350000
         C     INDEX2,@CF03181                                     0813 02351000
         BNH   @DL00802                                            0813 02352000
*   RFY                                                            0814 02353000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 02354000
*   RET=OPWVSAVE(SAVENDEX);         /* RESTORE RETURN ADDRESS        */ 02355000
         LR    @05,SAVENDEX                                        0815 02356000
         SLA   @05,2                                               0815 02357000
         L     RET,OPWVSAVE-4(@05,RCORE)                           0815 02358000
*   RFY                                                            0816 02359000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 02360000
*   SAVENDEX=SAVENDEX-1;            /* POINT TO LAST SAVE AREA       */ 02361000
         BCTR  SAVENDEX,0                                          0817 02362000
*   END SREQFREE;                   /* RETURN TO THE CALLER          */ 02363000
@EL00011 DS    0H                                                  0818 02364000
@EF00011 DS    0H                                                  0818 02365000
@ER00011 BR    @14                                                 0818 02366000
*                                                                  0819 02367000
*/********************************************************************/ 02368000
*/*                                                                  */ 02369000
*/* FUNCTION: THIS ROUTINE IS USED TO FIND THE CORE SAVE LIST ENTRY  */ 02370000
*/*           FOR A CORRESPONDING HEADER ELEMENT.                    */ 02371000
*/*                                                                  */ 02372000
*/* INPUTS:   POINTERS IN OPWVHDRE TO THE HEADER ELEMENT AND IN      */ 02373000
*/*           BIBCSL TO THE CORE SAVE LIST(S).                       */ 02374000
*/*                                                                  */ 02375000
*/* OUTPUTS:  A POINTER IN OPWVCSLE TO THE CORE SAVE LIST ENTRY OR   */ 02376000
*/*           ZERO IF UNABLE TO FIND THE ENTRY.                      */ 02377000
*/*                                                                  */ 02378000
*/********************************************************************/ 02379000
*                                                                  0819 02380000
*FNDCSLEL:                                                         0819 02381000
*   PROC OPTIONS(NOSAVE);                                          0819 02382000
FNDCSLEL DS    0H                                                  0820 02383000
*   RFY                                                            0820 02384000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 02385000
*   RFY                                                            0821 02386000
*     HEBHDREL BASED(OPWVHDRE);     /* EST BASE FOR HDR ELEMENT      */ 02387000
*   OPWVCSLP=BIBCSL;                /* POINT TO FIRST CORE SAVE LIST */ 02388000
         L     @15,OPWBIB(,RCORE)                                  0822 02389000
         L     @15,BIBCSL(,@15)                                    0822 02390000
         ST    @15,OPWVCSLP(,RCORE)                                0822 02391000
*   RFY                                                            0823 02392000
*     IDACSL BASED(OPWVCSLP);       /* EST BASE FOR CORE SAVE LIST   */ 02393000
*   OPWVCSLE=0;                     /* INIT CSL ELEMENT PTR TO ZERO  */ 02394000
         SLR   @15,@15                                             0824 02395000
         ST    @15,OPWVCSLE(,RCORE)                                0824 02396000
*   DO WHILE(OPWVCSLP^=0&OPWVCSLE=0);/* SEARCH ALL THE CSL(S) FOR THE   02397000
*                                      DESIRED CSL ELEMENT           */ 02398000
         B     @DE00825                                            0825 02399000
@DL00825 DS    0H                                                  0826 02400000
*     DO INDEX4=1 TO DIM(CSLNTRYS) BY 1 WHILE OPWVCSLE=0;/* EXAMINE     02401000
*                                      EACH CSL ELEMENT IN THIS CSL  */ 02402000
         LA    INDEX4,1                                            0826 02403000
@DL00826 L     @15,OPWVCSLE(,RCORE)                                0826 02404000
         LTR   @15,@15                                             0826 02405000
         BNZ   @DC00826                                            0826 02406000
*       RFY                                                        0827 02407000
*         CSLENTRY BASED(ADDR(CSLNTRYS(INDEX4)));/* CSL ELEM BASE    */ 02408000
*       IF CSLFREMN=HEBFREMN THEN   /* IS CSL FOR SAME BLK AS HEB    */ 02409000
         LR    @15,INDEX4                                          0828 02410000
         MH    @15,@CH00096                                        0828 02411000
         L     @11,OPWVCSLP(,RCORE)                                0828 02412000
         LA    @15,CSLNTRYS-12(@15,@11)                            0828 02413000
         L     @11,OPWVHDRE(,RCORE)                                0828 02414000
         CLC   CSLFREMN(8,@15),HEBFREMN(@11)                       0828 02415000
         BNE   @RF00828                                            0828 02416000
*         OPWVCSLE=ADDR(CSLENTRY);  /* IF SO, RETURN ADDR TO CALLER  */ 02417000
         ST    @15,OPWVCSLE(,RCORE)                                0829 02418000
*       ELSE                                                       0830 02419000
*         ;                         /* IF NOT, CONTINUE SEARCH       */ 02420000
@RF00828 DS    0H                                                  0831 02421000
*     END;                                                         0831 02422000
         AL    INDEX4,@CF00082                                     0831 02423000
         C     INDEX4,@CF00106                                     0831 02424000
         BNH   @DL00826                                            0831 02425000
@DC00826 DS    0H                                                  0832 02426000
*     OPWVCSLP=CSLNXPTR;            /* POINT TO THE NEXT CSL         */ 02427000
         L     @15,OPWVCSLP(,RCORE)                                0832 02428000
         L     @15,CSLNXPTR(,@15)                                  0832 02429000
         ST    @15,OPWVCSLP(,RCORE)                                0832 02430000
*   END;                                                           0833 02431000
@DE00825 SLR   @15,@15                                             0833 02432000
         C     @15,OPWVCSLP(,RCORE)                                0833 02433000
         BE    @DC00825                                            0833 02434000
         C     @15,OPWVCSLE(,RCORE)                                0833 02435000
         BE    @DL00825                                            0833 02436000
@DC00825 DS    0H                                                  0834 02437000
*   RFY                                                            0834 02438000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 02439000
*   END FNDCSLEL;                   /* RETURN TO THE CALLER          */ 02440000
@EL00012 DS    0H                                                  0835 02441000
@EF00012 DS    0H                                                  0835 02442000
@ER00012 BR    @14                                                 0835 02443000
*                                                                  0836 02444000
*/********************************************************************/ 02445000
*/*                                                                  */ 02446000
*/* FUNCTION: THIS ROUTINE IS USED TO FREE EXCESS CORE IN CLUSTER    */ 02447000
*/*           AND SPHERE BLOCKS WHEN INSUFFICIENT SPACE IS AVAILABLE */ 02448000
*/*           IN THE CURRENT BLOCK TO SATISFY A GET SPACE REQUEST,   */ 02449000
*/*           OR AT THE END OF A SUCCESSFUL OPEN.                    */ 02450000
*/*                                                                  */ 02451000
*/* INPUTS:   A POINTER IN OPWVHDRE TO THE HEADER ELEMENT FOR THE    */ 02452000
*/*           CORE.                                                  */ 02453000
*/*                                                                  */ 02454000
*/* OUTPUTS:  SAME AS INPUT.                                         */ 02455000
*/*                                                                  */ 02456000
*/********************************************************************/ 02457000
*                                                                  0836 02458000
*FREECORE:                                                         0836 02459000
*   PROC OPTIONS(NOSAVE);                                          0836 02460000
FREECORE DS    0H                                                  0837 02461000
*   SAVENDEX=SAVENDEX+1;            /* POINT TO NEXT SAVE AREA       */ 02462000
         AL    SAVENDEX,@CF00082                                   0837 02463000
*   RFY                                                            0838 02464000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 02465000
*   OPWVSAVE(SAVENDEX)=RET;         /* SAVE RETURN ADDRESS           */ 02466000
         LR    @15,SAVENDEX                                        0839 02467000
         SLA   @15,2                                               0839 02468000
         ST    RET,OPWVSAVE-4(@15,RCORE)                           0839 02469000
*   RFY                                                            0840 02470000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 02471000
*   RFY                                                            0841 02472000
*     HEBHDREL BASED(OPWVHDRE);     /* EST BASE FOR HEB HDR ELEMENT  */ 02473000
*   OPWVFMLN=HEBAVSP&(-DOUBLWDL);   /* ROUND-DOWN AVAIL SPACE TO DW  */ 02474000
         L     @11,@CF03173                                        0842 02475000
         L     @08,OPWVHDRE(,RCORE)                                0842 02476000
         L     @05,HEBAVSP-1(,@08)                                 0842 02477000
         LA    @05,0(,@05)                                         0842 02478000
         NR    @05,@11                                             0842 02479000
         STCM  @05,7,OPWVFMLN(RCORE)                               0842 02480000
*   IF OPWVFMLN^=0 THEN             /* LESS THAN A DW TO BE FREED    */ 02481000
         LTR   @05,@05                                             0843 02482000
         BZ    @RF00843                                            0843 02483000
*     DO;                           /* NO, FREE EXCESS CORE          */ 02484000
*       OPWVFMSP=HEBSP;             /* INIT SP OF CORE TO BE FREED   */ 02485000
         IC    @02,HEBSP(,@08)                                     0845 02486000
         STC   @02,OPWVFMSP(,RCORE)                                0845 02487000
*       OPWVFMPT=(HEBNBYTE+7)&(-DOUBLWDL);/* ROUND-UP TO A DW THE PTR   02488000
*                                      TO CORE TO BE FREED           */ 02489000
         LA    @02,7                                               0846 02490000
         AL    @02,HEBNBYTE(,@08)                                  0846 02491000
         NR    @02,@11                                             0846 02492000
         ST    @02,OPWVFMPT(,RCORE)                                0846 02493000
*       RFY                                                        0847 02494000
*        (R0,                                                      0847 02495000
*         R1) RSTD;                 /* RESTRICT REGISTER USAGE       */ 02496000
*       LM(R0,R1,OPWVFMPL);         /* SET UP FREEMAIN REGISTERS     */ 02497000
         LM    R0,R1,OPWVFMPL(RCORE)                               0848 02498000
*       IF HEBKEY5=ON&HEBSP=RDEBPOOL THEN/* BR ENTRY REQUIRED        */ 02499000
         TM    HEBKEY5(@08),B'01000000'                            0849 02500000
         BNO   @RF00849                                            0849 02501000
         CLI   HEBSP(@08),230                                      0849 02502000
         BNE   @RF00849                                            0849 02503000
*         DO;                       /* YES, OBTAIN LOCAL LOCK FIRST  */ 02504000
*           R0=OPWVFMLN;            /* REG 0 MUST ONLY CONTAIN LENGTH   02505000
*                                      FOR BRANCH ENTRY TO FREEMAIN     02506000
*                                      OBTAIN THE LOCAL LOCK         */ 02507000
         LR    R0,@05                                              0851 02508000
*           OPWVRG13=R13;           /* SAVE REG 13                   */ 02509000
         ST    R13,OPWVRG13(,RCORE)                                0852 02510000
*           DO;                     /* SETLOCK (OBTAIN) MODE(UNCOND)    02511000
*                                      TYPE(LOCAL) RELATED(FREEMAIN,I   02512000
*                                      DA0192M(FREECORE))            */ 02513000
*             RFY                                                  0854 02514000
*              (GPR11P,                                            0854 02515000
*               GPR12P,                                            0854 02516000
*               GPR13P,                                            0854 02517000
*               GPR14P) RSTD;                                      0854 02518000
*             GPR13P=PSALITA;       /* ADDRESS OF LOCK INTERFACE   0855 02519000
*                                      TABLE.                        */ 02520000
         L     GPR13P,PSALITA                                      0855 02521000
*             GEN(LM GPR11P,GPR13P,576(GPR13P));/* LOAD LOCKWORD,  0856 02522000
*                                      CLHT AND ENTRY ADRS.          */ 02523000
         LM GPR11P,GPR13P,576(GPR13P)                                   02524000
*             GEN(BALR GPR14P,GPR13P);/* BALR TO SETLOCK RTN.        */ 02525000
         BALR GPR14P,GPR13P                                             02526000
*             RFY                                                  0858 02527000
*              (GPR11P,                                            0858 02528000
*               GPR12P,                                            0858 02529000
*               GPR13P,                                            0858 02530000
*               GPR14P) UNRSTD;                                    0858 02531000
*           END;                    /* SETLOCK (OBTAIN) MODE(UNCOND)    02532000
*                                      TYPE(LOCAL) RELATED(FREEMAIN,I   02533000
*                                      DA0192M(FREECORE))            */ 02534000
*           R12=OPWVRG12;           /* RESTORE SECOND BASE REG       */ 02535000
         L     R12,OPWVRG12(,RCORE)                                0860 02536000
*           RFY                                                    0861 02537000
*            (R4,                                                  0861 02538000
*             R7,                                                  0861 02539000
*             R11) RSTD;            /* RESTRICT REGISTERS            */ 02540000
*           R11=RBASEA;             /* SAVE BASE REGISTER    @OZ04229*/ 02541000
         LR    R11,RBASEA                                          0862 02542000
*           R13=RCORE;              /* SAVE PTR TO O/C/E WORK AREA   */ 02543000
         LR    R13,RCORE                                           0863 02544000
*           IF HEBJSTCB=ON THEN     /* CHECK CORE TCB OWNERSHIP      */ 02545000
         L     @08,OPWVHDRE(,RCORE)                                0864 02546000
         TM    HEBJSTCB(@08),B'10000000'                           0864 02547000
         BNO   @RF00864                                            0864 02548000
*             R4=TCBJSTCB;          /* PT TO JOBSTEP TCB FOR GETMAIN */ 02549000
         L     @08,PSATNEW                                         0865 02550000
         L     R4,TCBJSTCB(,@08)                                   0865 02551000
*           ELSE                    /* NOT JOB STEP RELATED          */ 02552000
*             R4=ADDR(TCB);         /* PT TO CURRENT TCB FOR GETMAIN */ 02553000
         B     @RC00864                                            0866 02554000
@RF00864 L     R4,PSATNEW                                          0866 02555000
*           R7=PSAAOLD;             /* POINT TO ASCB FOR FREEMAIN  0867 02556000
*                                      FREE EXCESS CORE              */ 02557000
@RC00864 L     R7,PSAAOLD                                          0867 02558000
*           GEN( FREEMAIN RU,LV=(0),A=(1),SP=230,BRANCH=YES,KEY=5) 0868 02559000
*           REFS(R0,R1);                                           0868 02560000
          FREEMAIN RU,LV=(0),A=(1),SP=230,BRANCH=YES,KEY=5              02561000
*           RBASEA=R11;             /* RESTORE BASE REGISTER @OZ04229*/ 02562000
         LR    RBASEA,R11                                          0869 02563000
*           RCORE=R13;              /* RESTORE O/C/E WORK AREA PTR   */ 02564000
         LR    RCORE,R13                                           0870 02565000
*           RFY                                                    0871 02566000
*            (R4,                                                  0871 02567000
*             R7,                                                  0871 02568000
*             R11) UNRSTD;          /* DROP REG RESTRICTIONS RELEASE    02569000
*                                      THE LOCAL LOCK                */ 02570000
*           DO;                     /* SETLOCK (RELEASE) TYPE(LOCAL)    02571000
*                                      RELATED(FREEMAIN,IDA0192M(FREE   02572000
*                                      CORE))                        */ 02573000
*             RFY                                                  0873 02574000
*              (GPR11P,                                            0873 02575000
*               GPR12P,                                            0873 02576000
*               GPR13P,                                            0873 02577000
*               GPR14P) RSTD;                                      0873 02578000
*             GPR13P=PSALITA;       /* ADDRESS OF LOCK INTERFACE   0874 02579000
*                                      TABLE.                        */ 02580000
         L     GPR13P,PSALITA                                      0874 02581000
*             GEN(LM GPR11P,GPR13P,588(GPR13P));/* LOAD LOCKWORD,  0875 02582000
*                                      CLHT AND ENTRY ADRS.          */ 02583000
         LM GPR11P,GPR13P,588(GPR13P)                                   02584000
*             GEN(BALR GPR14P,GPR13P);/* BALR TO SETLOCK RTN.        */ 02585000
         BALR GPR14P,GPR13P                                             02586000
*             RFY                                                  0877 02587000
*              (GPR11P,                                            0877 02588000
*               GPR12P,                                            0877 02589000
*               GPR13P,                                            0877 02590000
*               GPR14P) UNRSTD;                                    0877 02591000
*           END;                    /* SETLOCK (RELEASE) TYPE(LOCAL)    02592000
*                                      RELATED(FREEMAIN,IDA0192M(FREE   02593000
*                                      CORE))                        */ 02594000
*           LM(R12,R13,OPWVLSAV);   /* RESTORE REGS USED BY SETLOCK  */ 02595000
         LM    R12,R13,OPWVLSAV(RCORE)                             0879 02596000
*         END;                                                     0880 02597000
*       ELSE                        /* NO, USE SVC FREEMAIN          */ 02598000
*         DO;                       /* CHECK OF CORE TO BE FREED     */ 02599000
         B     @RC00849                                            0881 02600000
@RF00849 DS    0H                                                  0882 02601000
*           IF HEBKEY5=ON THEN      /* IS CORE TO BE FREED IN KEY 5  */ 02602000
         L     @11,OPWVHDRE(,RCORE)                                0882 02603000
         TM    HEBKEY5(@11),B'01000000'                            0882 02604000
         BNO   @RF00882                                            0882 02605000
*             DO;                   /* MODESET EXTKEY(DATAMGT)       */ 02606000
*               SPKA(80);           /* SET PSW KEY                   */ 02607000
         LA    @11,80                                              0884 02608000
         SPKA  0(@11)                                              0884 02609000
*             END;                  /* MODESET EXTKEY(DATAMGT) YES,     02610000
*                                      MODESET TO KEY 5              */ 02611000
*           ELSE                    /* NO, CHECK FOR KEY 7 CORE      */ 02612000
*             IF HEBGSRKT=ON THEN   /* TO BE FREED IN GSR KEY?     0886 02613000
*                                                            @Z40ESSR*/ 02614000
         B     @RC00882                                            0886 02615000
@RF00882 L     @11,OPWVHDRE(,RCORE)                                0886 02616000
         TM    HEBGSRKT(@11),B'00100000'                           0886 02617000
         BNO   @RF00886                                            0886 02618000
*               DO;                 /*                       @Z40ESSR*/ 02619000
*                 RFY                                              0888 02620000
*                   R15 RSTD;       /*                       @Z40ESSR*/ 02621000
*                 R15=HEBGSRK;      /* LOAD GSR KEY INTO REG @Z40ESSR*/ 02622000
         SLR   R15,R15                                             0889 02623000
         IC    R15,HEBGSRK(,@11)                                   0889 02624000
*                 SPKA(R15);        /* MODESET TO GSR KEY    @Z40ESSR*/ 02625000
         SPKA  0(R15)                                              0890 02626000
*                 RFY                                              0891 02627000
*                   R15 UNRSTD;     /*                       @Z40ESSR*/ 02628000
*               END;                /*                       @Z40ESSR*/ 02629000
*             ELSE                                                 0893 02630000
*               ;                   /* NO, CONTINUE TO RUN IN KEY 0  */ 02631000
@RF00886 DS    0H                                                  0894 02632000
*           GEN( FREEMAIN R,LV=(0),A=(1))/* FREE EXCESS CORE         */ 02633000
*           REFS(R0,R1);                                           0894 02634000
@RC00882 DS    0H                                                  0894 02635000
          FREEMAIN R,LV=(0),A=(1)                                       02636000
*           RFY                                                    0895 02637000
*            (R0,                                                  0895 02638000
*             R1) UNRSTD;           /* DROP REGISTER RESTRICTIONS    */ 02639000
*           DO;                     /* MODESET EXTKEY(SUPR)          */ 02640000
*             SPKA(0);              /* SET PSW KEY                   */ 02641000
         SLR   @11,@11                                             0897 02642000
         SPKA  0(@11)                                              0897 02643000
*           END;                    /* MODESET EXTKEY(SUPR) RETURN TO   02644000
*                                      KEY O                         */ 02645000
*         END;                                                     0899 02646000
*       HEBLN=HEBLN-OPWVFMLN;       /* DECR LENGTH OF BLOCK          */ 02647000
@RC00849 L     @11,OPWVHDRE(,RCORE)                                0900 02648000
         L     @08,HEBLN-1(,@11)                                   0900 02649000
         LA    @08,0(,@08)                                         0900 02650000
         L     @05,OPWVFMLN-1(,RCORE)                              0900 02651000
         LA    @05,0(,@05)                                         0900 02652000
         SLR   @08,@05                                             0900 02653000
         STCM  @08,7,HEBLN(@11)                                    0900 02654000
*     END;                                                         0901 02655000
*   ELSE                                                           0902 02656000
*     ;                             /* LESS THAN A DW LEFT - ZERO  0902 02657000
*                                      AVAILABLE SPACE               */ 02658000
@RF00843 DS    0H                                                  0903 02659000
*   HEBAVSP=0;                      /* ZERO AMNT OF AVAILABLE SPACE  */ 02660000
         L     @11,OPWVHDRE(,RCORE)                                0903 02661000
         SLR   @08,@08                                             0903 02662000
         STCM  @08,7,HEBAVSP(@11)                                  0903 02663000
*   RFY                                                            0904 02664000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 02665000
*   RET=OPWVSAVE(SAVENDEX);         /* RESTORE RETURN ADDRESS        */ 02666000
         LR    @11,SAVENDEX                                        0905 02667000
         SLA   @11,2                                               0905 02668000
         L     RET,OPWVSAVE-4(@11,RCORE)                           0905 02669000
*   RFY                                                            0906 02670000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 02671000
*   SAVENDEX=SAVENDEX-1;            /* POINT TO LAST SAVE AREA       */ 02672000
         BCTR  SAVENDEX,0                                          0907 02673000
*   END FREECORE;                   /* RETURN TO THE CALLER          */ 02674000
