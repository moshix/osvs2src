         TM    CSLJSTCB(@10),B'00100000'                           0726 02059000
         BNO   @RF00726                                            0726 02060000
*                 R4=TCBJSTCB;      /* PT TO JOBSTEP TCB FOR GETMAIN */ 02061000
         L     @10,PSATNEW                                         0727 02062000
         L     R4,TCBJSTCB(,@10)                                   0727 02063000
*               ELSE                /* NOT JOB STEP RELATED          */ 02064000
*                 R4=ADDR(TCB);     /* PT TO CURRENT TCB FOR GETMAIN */ 02065000
         B     @RC00726                                            0728 02066000
@RF00726 L     R4,PSATNEW                                          0728 02067000
*               R7=PSAAOLD;         /* POINT TO ASCB FOR FREEMAIN  0729 02068000
*                                      FREE CORE BLOCK               */ 02069000
@RC00726 L     R7,PSAAOLD                                          0729 02070000
*               GEN( FREEMAIN RU,LV=(0),A=(1),SP=230,BRANCH=YES,KEY=5)  02071000
*               REFS(R0,R1);                                       0730 02072000
          FREEMAIN RU,LV=(0),A=(1),SP=230,BRANCH=YES,KEY=5              02073000
*               RBASEA=R11;         /* RESTORE BASE REGISTER @OZ04229*/ 02074000
         LR    RBASEA,R11                                          0731 02075000
*               RCORE=R13;          /* RESTORE O/C/E WORK AREA PTR   */ 02076000
         LR    RCORE,R13                                           0732 02077000
*               RFY                                                0733 02078000
*                (R4,                                              0733 02079000
*                 R7,                                              0733 02080000
*                 R11) UNRSTD;      /* DROP REG RESTRICTIONS RELEASE    02081000
*                                      THE LOCAL LOCK                */ 02082000
*               DO;                 /* SETLOCK (RELEASE) TYPE(LOCAL)    02083000
*                                      RELATED(FREEMAIN,IDA0192M(CLEA   02084000
*                                      NUP))                         */ 02085000
*                 RFY                                              0735 02086000
*                  (GPR11P,                                        0735 02087000
*                   GPR12P,                                        0735 02088000
*                   GPR13P,                                        0735 02089000
*                   GPR14P) RSTD;                                  0735 02090000
*                 GPR13P=PSALITA;   /* ADDRESS OF LOCK INTERFACE   0736 02091000
*                                      TABLE.                        */ 02092000
         L     GPR13P,PSALITA                                      0736 02093000
*                 GEN(LM GPR11P,GPR13P,588(GPR13P));/* LOAD LOCKWORD,   02094000
*                                      CLHT AND ENTRY ADRS.          */ 02095000
         LM GPR11P,GPR13P,588(GPR13P)                                   02096000
*                 GEN(BALR GPR14P,GPR13P);/* BALR TO SETLOCK RTN.    */ 02097000
         BALR GPR14P,GPR13P                                             02098000
*                 RFY                                              0739 02099000
*                  (GPR11P,                                        0739 02100000
*                   GPR12P,                                        0739 02101000
*                   GPR13P,                                        0739 02102000
*                   GPR14P) UNRSTD;                                0739 02103000
*               END;                /* SETLOCK (RELEASE) TYPE(LOCAL)    02104000
*                                      RELATED(FREEMAIN,IDA0192M(CLEA   02105000
*                                      NUP))                         */ 02106000
*               LM(R12,R13,OPWVLSAV);/* RESTOR REGS USED BY   SETLOCK*/ 02107000
         LM    R12,R13,OPWVLSAV(RCORE)                             0741 02108000
*             END;                                                 0742 02109000
*           ELSE                    /* NO, USE SVC FREEMAIN          */ 02110000
*             DO;                   /* CHECK KEY OF CORE TO BE FREED */ 02111000
         B     @RC00711                                            0743 02112000
@RF00711 DS    0H                                                  0744 02113000
*               IF CSLKEY5=ON THEN  /* IS CORE TO BE FREED IN KEY 5  */ 02114000
         LR    @08,INDEX3                                          0744 02115000
         MH    @08,@CH00096                                        0744 02116000
         L     @05,OPWBIB(,RCORE)                                  0744 02117000
         L     @05,BIBCSL(,@05)                                    0744 02118000
         LA    @08,CSLNTRYS-12(@08,@05)                            0744 02119000
         TM    CSLKEY5(@08),B'10000000'                            0744 02120000
         BNO   @RF00744                                            0744 02121000
*                 DO;               /* MODESET EXTKEY(DATAMGT)       */ 02122000
*                   SPKA(80);       /* SET PSW KEY                   */ 02123000
         LA    @08,80                                              0746 02124000
         SPKA  0(@08)                                              0746 02125000
*                 END;              /* MODESET EXTKEY(DATAMGT) YES,     02126000
*                                      MODESET TO KEY 5              */ 02127000
*               ELSE                /* NO, CHECK FOR KEY 7 CORE      */ 02128000
*                 IF CSLGSRKT=ON THEN/* 2 B FREED IN GSR KEY?      0748 02129000
*                                                            @Z40ESSR*/ 02130000
         B     @RC00744                                            0748 02131000
@RF00744 LR    @08,INDEX3                                          0748 02132000
         MH    @08,@CH00096                                        0748 02133000
         L     @05,OPWBIB(,RCORE)                                  0748 02134000
         L     @05,BIBCSL(,@05)                                    0748 02135000
         LA    @08,CSLNTRYS-12(@08,@05)                            0748 02136000
         TM    CSLGSRKT(@08),B'01000000'                           0748 02137000
         BNO   @RF00748                                            0748 02138000
*                   DO;             /*                       @Z40ESSR*/ 02139000
*                     RFY                                          0750 02140000
*                       R15 RSTD;   /*                       @Z40ESSR*/ 02141000
*                     R15=CSLGSRK;  /* LOAD GSR KEY INTO REG @Z40ESSR*/ 02142000
         SLR   R15,R15                                             0751 02143000
         IC    R15,CSLGSRK(,@05)                                   0751 02144000
*                     SPKA(R15);    /* MODESET TO GSR KEY    @Z40ESSR*/ 02145000
         SPKA  0(R15)                                              0752 02146000
*                     RFY                                          0753 02147000
*                       R15 UNRSTD; /*                       @Z40ESSR*/ 02148000
*                   END;            /*                       @Z40ESSR*/ 02149000
*                 ELSE                                             0755 02150000
*                   ;               /* NO, CONTINUE TO RUN IN KEY 0  */ 02151000
@RF00748 DS    0H                                                  0756 02152000
*               GEN( FREEMAIN R,LV=(0),A=(1))/* FREE CORE BLOCK      */ 02153000
*               REFS(R0,R1);                                       0756 02154000
@RC00744 DS    0H                                                  0756 02155000
          FREEMAIN R,LV=(0),A=(1)                                       02156000
*               RFY                                                0757 02157000
*                (R0,                                              0757 02158000
*                 R1) UNRSTD;       /* DROP REG RESTRICTIONS         */ 02159000
*               DO;                 /* MODESET EXTKEY(SUPR)          */ 02160000
*                 SPKA(0);          /* SET PSW KEY                   */ 02161000
         SLR   @08,@08                                             0759 02162000
         SPKA  0(@08)                                              0759 02163000
*               END;                /* MODESET EXTKEY(SUPR) RETURN TO   02164000
*                                      KEY 0                         */ 02165000
*             END;                                                 0761 02166000
*         END;                                                     0762 02167000
*       ELSE                                                       0763 02168000
*         ;                         /* NO CORE BLOCK TO BE FREED     */ 02169000
@RF00707 DS    0H                                                  0764 02170000
*       IF CSLANCPT^=0 THEN         /* ANY HEB HDR ELEM ASSOCIATED 0764 02171000
*                                      WITH THIS CSL ENTRY           */ 02172000
