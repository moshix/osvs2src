         BNZ   @RT00576                                            0576 01550000
         TM    OPWGSR(RCORE),B'01000000'                           0576 01551000
         BO    @RT00576                                            0576 01552000
         TM    OPWCBIC(RCORE),B'10000000'                          0576 01553000
         BNO   @RF00576                                            0576 01554000
@RT00576 DS    0H                                                  0577 01555000
*                                   /* IS REQUEST FOR CATALOGUE OR 0577 01556000
*                                      GSR OR CBIC           @ZA16012   01557000
*                                      YES, GET CORE IN GLOBAL SP    */ 01558000
*         DO;                       /* GETMAIN (RC)                0577 01559000
*                                      LV(LENGTH(IDACSL))          0577 01560000
*                                      SP(SCATPOOL)                  */ 01561000
*           RESPECIFY                                              0578 01562000
*            (GPR01F,                                              0578 01563000
*             GPR15F,                                              0578 01564000
*             GPR00F) RESTRICTED;                                  0578 01565000
*           GPR01F=0;               /* REG 1 MUST BE ZERO            */ 01566000
         SLR   GPR01F,GPR01F                                       0579 01567000
*           GPR15F=0;               /* RC-TYPE GETMAIN               */ 01568000
         SLR   GPR15F,GPR15F                                       0580 01569000
*           GPR00F=LENGTH(IDACSL);  /* LENGTH REQUESTED              */ 01570000
         LA    GPR00F,404                                          0581 01571000
*           GPR15F=GPR15F|((SCATPOOL)*256);/* SP IN BYTE 2           */ 01572000
         O     GPR15F,@CF03179                                     0582 01573000
*           SVC(120);               /* RC/RU-FORM OF GETMAIN         */ 01574000
         SVC   120                                                 0583 01575000
*           RESPECIFY                                              0584 01576000
*            (GPR01F,                                              0584 01577000
*             GPR15F,                                              0584 01578000
*             GPR00F) UNRESTRICTED;                                0584 01579000
*         END;                      /* GETMAIN (RC)                0585 01580000
*                                      LV(LENGTH(IDACSL))          0585 01581000
*                                      SP(SCATPOOL)                  */ 01582000
*       ELSE                        /* NO, OPEN IS FOR A DATA SET GET   01583000
*                                      CORE IN LOCAL SUBPOOL         */ 01584000
*         DO;                       /* GETMAIN (RC)                0586 01585000
*                                      LV(LENGTH(IDACSL)) SP(WKPOOL) */ 01586000
