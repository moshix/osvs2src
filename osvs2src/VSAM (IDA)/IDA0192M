         STCM  @15,7,HEBAVSP(@05)                                  0279 00554000
*   HEBNBYTE=HEBNBYTE+GSLREQLN;     /* UPDATE PTR TO UNUSED SPACE    */ 00555000
         ALR   @02,@08                                             0280 00556000
         ST    @02,HEBNBYTE(,@05)                                  0280 00557000
*   RFY                                                            0281 00558000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 00559000
*   RET=OPWVSAVE(SAVENDEX);         /* RESTORE RETURN ADDRESS        */ 00560000
         LR    @08,SAVENDEX                                        0282 00561000
         SLA   @08,2                                               0282 00562000
         L     RET,OPWVSAVE-4(@08,RCORE)                           0282 00563000
*   RFY                                                            0283 00564000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 00565000
*   SAVENDEX=SAVENDEX-1;            /* POINT TO LAST SAVE AREA       */ 00566000
         BCTR  SAVENDEX,0                                          0284 00567000
*   END PRTSPREQ;                   /* RETURN TO THE CALLER          */ 00568000
         B     @EL00003                                            0285 00569000
*                                                                  0286 00570000
*/********************************************************************/ 00571000
*/*                                                                  */ 00572000
*/* FUNCTION: THIS ROUTINE IS USED TO GET CORE FROM EXISTING BLOCKS  */ 00573000
*/*           OR TO REQUEST A NEW BLOCK IF SUFFICIENT CORE IS NOT    */ 00574000
*/*           AVAILABLE IN AN EXISTING BLOCK OR THIS IS THE FIRST    */ 00575000
*/*           REQUEST FOR CORE.                                      */ 00576000
*/*                                                                  */ 00577000
*/* INPUTS:   VIRTUAL STORAGE MANAGER AND GETSPACE PARAMETER LISTS   */ 00578000
*/*           WITHIN THE OPEN WORK AREA.                             */ 00579000
*/*                                                                  */ 00580000
*/* OUTPUTS:  A POINTER TO THE REQUESTED CORE IN GSLPTR OR ZERO IF   */ 00581000
*/*           UNABLE TO OBTAIN SPACE.                                */ 00582000
*/*                                                                  */ 00583000
*/********************************************************************/ 00584000
*                                                                  0286 00585000
*GETSPACE:                                                         0286 00586000
*   PROC OPTIONS(NOSAVE);                                          0286 00587000
GETSPACE DS    0H                                                  0287 00588000
*   SAVENDEX=SAVENDEX+1;            /* POINT TO NEXT SAVE AREA       */ 00589000
         AL    SAVENDEX,@CF00082                                   0287 00590000
*   RFY                                                            0288 00591000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 00592000
*   OPWVSAVE(SAVENDEX)=RET;         /* SAVE RETURN ADDRESS           */ 00593000
         LR    @15,SAVENDEX                                        0289 00594000
         SLA   @15,2                                               0289 00595000
         ST    RET,OPWVSAVE-4(@15,RCORE)                           0289 00596000
*   RFY                                                            0290 00597000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 00598000
*   RFY                                                            0291 00599000
*     HEBHDREL BASED(MHEBPTR);      /* BASE FOR HEB HDR ELEMENT      */ 00600000
*   OPWVHDRE=ADDR(HEBHDREL);        /* SET UP PTR TO HEB HDR ELEM    */ 00601000
         L     @08,OPWVANCP(,RCORE)                                0292 00602000
         L     @08,MHEBPTR(,@08)                                   0292 00603000
         ST    @08,OPWVHDRE(,RCORE)                                0292 00604000
*   IF ADDR(HEBHDREL)^=0&HEBAVSP^=0&HEBAVSP<GSLREQLN THEN/* DET IF 0293 00605000
*                                      ANY BLKS EXIST, IF SO IS    0293 00606000
*                                      ENOUGH CORE AVAILABLE FOR THE    00607000
*                                      REQUEST                       */ 00608000
         LTR   @08,@08                                             0293 00609000
         BZ    @RF00293                                            0293 00610000
         L     @05,HEBAVSP-1(,@08)                                 0293 00611000
         LA    @05,0(,@05)                                         0293 00612000
         LTR   @05,@05                                             0293 00613000
         BZ    @RF00293                                            0293 00614000
         LR    @05,GSNDEX                                          0293 00615000
         MH    @05,@CH00096                                        0293 00616000
         LA    @05,OPWVGSPL-12(@05,RCORE)                          0293 00617000
         CLC   HEBAVSP(3,@08),GSLREQLN(@05)                        0293 00618000
         BNL   @RF00293                                            0293 00619000
*     DO;                           /* NOT ENOUGH, FREE EXCESS SPACE */ 00620000
*       CALL FNDCSLEL;              /* FIND SAVE LIST FOR THIS BLOCK */ 00621000
         BAL   @14,FNDCSLEL                                        0295 00622000
*       IF OPWVCSLE^=0 THEN         /* WAS CSL FOUND                 */ 00623000
         L     @08,OPWVCSLE(,RCORE)                                0296 00624000
         LTR   @08,@08                                             0296 00625000
         BZ    @RF00296                                            0296 00626000
*         DO;                       /* YES, DET AMOUNT TO BE FREED   */ 00627000
*           RFY                                                    0298 00628000
*             CSLENTRY BASED(OPWVCSLE);/* BASE FOR CSL ENTRY         */ 00629000
*           CALL FREECORE;          /* FREE EXCESS SPACE IN BLOCK    */ 00630000
         BAL   @14,FREECORE                                        0299 00631000
*           CSLCORLN=HEBLN;         /* REFLECT LENGTH IN CSL         */ 00632000
         L     @08,OPWVCSLE(,RCORE)                                0300 00633000
         L     @05,OPWVANCP(,RCORE)                                0300 00634000
         L     @05,MHEBPTR(,@05)                                   0300 00635000
         MVC   CSLCORLN(3,@08),HEBLN(@05)                          0300 00636000
*         END;                                                     0301 00637000
*       ELSE                        /* CSL ELEM WAS NOT FOUND        */ 00638000
*         HEBAVSP=0;                /* ZERO AMNT OF AVAILABLE SPACE  */ 00639000
         B     @RC00296                                            0302 00640000
@RF00296 L     @08,OPWVANCP(,RCORE)                                0302 00641000
         L     @08,MHEBPTR(,@08)                                   0302 00642000
         SLR   @05,@05                                             0302 00643000
         STCM  @05,7,HEBAVSP(@08)                                  0302 00644000
*     END;                                                         0303 00645000
*   ELSE                                                           0304 00646000
*     ;                             /* NO EXCESS CORE TO BE FREED    */ 00647000
@RF00293 DS    0H                                                  0305 00648000
*   IF ADDR(HEBHDREL)=0|HEBAVSP=0 THEN/* IS A NEW CORE BLK NEEDED    */ 00649000
@RC00293 L     @08,OPWVANCP(,RCORE)                                0305 00650000
         L     @08,MHEBPTR(,@08)                                   0305 00651000
         LTR   @08,@08                                             0305 00652000
         BZ    @RT00305                                            0305 00653000
         L     @08,HEBAVSP-1(,@08)                                 0305 00654000
         LA    @08,0(,@08)                                         0305 00655000
         LTR   @08,@08                                             0305 00656000
         BNZ   @RF00305                                            0305 00657000
@RT00305 DS    0H                                                  0306 00658000
*     DO;                           /* YES, GET A NEW CORE BLOCK     */ 00659000
*       CALL GETHDREL;              /* GET A FREE HDR ELEMENT        */ 00660000
         BAL   @14,GETHDREL                                        0307 00661000
*       IF OPWVHDRE=0 THEN          /* WAS A NEW HDR EL AVAILABLE    */ 00662000
         SLR   @08,@08                                             0308 00663000
         C     @08,OPWVHDRE(,RCORE)                                0308 00664000
         BNE   @RF00308                                            0308 00665000
*         DO;                       /* NO, TERMINATE REQUEST         */ 00666000
*           GSLPTR=0;               /* IND REQUEST NOT SATISFIED     */ 00667000
         LR    @05,GSNDEX                                          0310 00668000
         MH    @05,@CH00096                                        0310 00669000
         LA    @05,OPWVGSPL-12(@05,RCORE)                          0310 00670000
