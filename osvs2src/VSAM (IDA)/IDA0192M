         LTR   @05,@05                                             0232 00388000
         BZ    @RF00232                                            0232 00389000
         LR    @05,GSNDEX                                          0232 00390000
         MH    @05,@CH00096                                        0232 00391000
         LA    @05,OPWVGSPL-12(@05,RCORE)                          0232 00392000
         CLC   HEBAVSP(3,@08),GSLREQLN(@05)                        0232 00393000
         BNL   @RF00232                                            0232 00394000
*     DO;                           /* NOT ENOUGH, FREE EXCESS SPACE */ 00395000
*       OPWVHDRE=ADDR(HEBHDREL);    /* SET UP HDR PTR FOR FREECORE   */ 00396000
         ST    @08,OPWVHDRE(,RCORE)                                0234 00397000
*       IF OPWVMTYP=PSAMBLBK THEN   /* IS REQ FOR A PROT AMBL BLK    */ 00398000
         CLI   OPWVMTYP(RCORE),134                                 0235 00399000
         BNE   @RF00235                                            0235 00400000
*         DO;                       /* YES, FREE EXCESS IN PROT AMBL */ 00401000
*           CALL FNDCSLEL;          /* FIND CSL ENTRY FOR PROT AMBL  */ 00402000
         BAL   @14,FNDCSLEL                                        0237 00403000
*           IF OPWVCSLE^=0 THEN     /* WAS CSL ENTRY FOUND           */ 00404000
         L     @08,OPWVCSLE(,RCORE)                                0238 00405000
         LTR   @08,@08                                             0238 00406000
         BZ    @RF00238                                            0238 00407000
*             DO;                   /* YES, FREE EXCESS CORE         */ 00408000
*               RFY                                                0240 00409000
*                 CSLENTRY BASED(OPWVCSLE);/* BASE FOR CSL ENTRY     */ 00410000
*               CALL FREECORE;      /* FREE EXCESS SPACE IN BLOCK    */ 00411000
         BAL   @14,FREECORE                                        0241 00412000
*               CSLCORLN=HEBLN;     /* REFLECT LENGTH IN CSL ENTRY   */ 00413000
