*           RFY                                                    0311 00672000
*             RET RSTD;             /* RESTRICT RETURN REGISTER      */ 00673000
*           RET=OPWVSAVE(SAVENDEX); /* RESTORE RETURN ADDRESS        */ 00674000
         LR    @08,SAVENDEX                                        0312 00675000
         SLA   @08,2                                               0312 00676000
         L     RET,OPWVSAVE-4(@08,RCORE)                           0312 00677000
*           RFY                                                    0313 00678000
*             RET UNRSTD;           /* DROP REGISTER RESTRICTION     */ 00679000
*           SAVENDEX=SAVENDEX-1;    /* POINT TO LAST SAVE AREA       */ 00680000
         BCTR  SAVENDEX,0                                          0314 00681000
*           RETURN;                 /* RETURN TO THE CALLER          */ 00682000
@EL00004 DS    0H                                                  0315 00683000
@EF00004 DS    0H                                                  0315 00684000
@ER00004 BR    @14                                                 0315 00685000
*         END;                                                     0316 00686000
*       ELSE                        /* A NEW HDR ELEMENT WAS FOUND   */ 00687000
*         DO;                       /* GET A NEW CORE BLOCK          */ 00688000
@RF00308 DS    0H                                                  0318 00689000
*           RFY                                                    0318 00690000
*             HEBHDREL BASED(OPWVHDRE);/* BASE FOR NEW HDR ELEM      */ 00691000
*           GSLGETLN=MAX(REQSIZE,GSLREQLN);/* GET LARGER OF AMOUNT 0319 00692000
*                                      REQUESTED OR MAX BLOCK SIZE   */ 00693000
         LR    @08,GSNDEX                                          0319 00694000
         MH    @08,@CH00096                                        0319 00695000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0319 00696000
         L     @05,OPWVTBLP(,RCORE)                                0319 00697000
         LH    @05,REQSIZE(,@05)                                   0319 00698000
         L     @02,GSLREQLN-1(,@08)                                0319 00699000
         LA    @02,0(,@02)                                         0319 00700000
         CR    @05,@02                                             0319 00701000
         BNL   *+6                                                      00702000
         LR    @05,@02                                             0319 00703000
         STCM  @05,7,GSLGETLN(@08)                                 0319 00704000
*           CALL GETCORE;           /* GET CORE FOR NEW BLOCK        */ 00705000
         BAL   @14,GETCORE                                         0320 00706000
*           IF GSLPTR=0 THEN        /* WAS CORE OBTAINED SUCCESSFULLY*/ 00707000
         SLR   @08,@08                                             0321 00708000
         LR    @05,GSNDEX                                          0321 00709000
         MH    @05,@CH00096                                        0321 00710000
         LA    @05,OPWVGSPL-12(@05,RCORE)                          0321 00711000
         C     @08,GSLPTR(,@05)                                    0321 00712000
         BNE   @RF00321                                            0321 00713000
*             DO;                   /* NO, TERMINATE REQUEST         */ 00714000
*               OPWVMADR=0;         /* IND REQUEST NOT SATISFIED     */ 00715000
         ST    @08,OPWVMADR(,RCORE)                                0323 00716000
*               HEBELCHN=BIBHEBFQ;  /* CHAIN HDR EL TO FIRST ON QUE  */ 00717000
         L     @08,OPWVHDRE(,RCORE)                                0324 00718000
         L     @05,OPWBIB(,RCORE)                                  0324 00719000
         L     @02,BIBHEBFQ(,@05)                                  0324 00720000
         ST    @02,HEBELCHN(,@08)                                  0324 00721000
*               BIBHEBFQ=OPWVHDRE;  /* RETRN HDR ELEM TO FREE QUEUE  */ 00722000
         ST    @08,BIBHEBFQ(,@05)                                  0325 00723000
*               RFY                                                0326 00724000
*                 RET RSTD;         /* RESTRICT RETURN REGISTER      */ 00725000
*               RET=OPWVSAVE(SAVENDEX);/* RESTORE RETURN ADDRESS     */ 00726000
         LR    @08,SAVENDEX                                        0327 00727000
         SLA   @08,2                                               0327 00728000
         L     RET,OPWVSAVE-4(@08,RCORE)                           0327 00729000
*               RFY                                                0328 00730000
*                 RET UNRSTD;       /* DROP REGISTER RESTRICTION     */ 00731000
*               SAVENDEX=SAVENDEX-1;/* POINT TO LAST SAVE AREA       */ 00732000
         BCTR  SAVENDEX,0                                          0329 00733000
*               RETURN;             /* RETURN TO THE CALLER          */ 00734000
         B     @EL00004                                            0330 00735000
*             END;                                                 0331 00736000
*           ELSE                    /* CORE WAS OBTAINED             */ 00737000
*             DO;                   /* COMPLETE NEW HDR AND CSL      */ 00738000
@RF00321 DS    0H                                                  0333 00739000
*               CSLANCPT=OPWVANCP;  /* PUT PTR TO ADDR OF FIRST HEB     00740000
*                                      HDR ELEMENT IN THE CSL        */ 00741000
         L     @08,OPWVCSLE(,RCORE)                                0333 00742000
         MVC   CSLANCPT(3,@08),OPWVANCP+1(RCORE)                   0333 00743000
*               HEBSP=GSLSPNO;      /* INIT SP NO. IN NEW HDR ELEM   */ 00744000
         L     @08,OPWVHDRE(,RCORE)                                0334 00745000
         LR    @05,GSNDEX                                          0334 00746000
         MH    @05,@CH00096                                        0334 00747000
         LA    @05,OPWVGSPL-12(@05,RCORE)                          0334 00748000
         IC    @02,GSLSPNO(,@05)                                   0334 00749000
         STC   @02,HEBSP(,@08)                                     0334 00750000
*               HEBLN=GSLGETLN;     /* INIT LENGTH IN NEW HDR ELEM   */ 00751000
         MVC   HEBLN(3,@08),GSLGETLN(@05)                          0335 00752000
*               HEBBLKPT=GSLPTR;    /* PT HDR ELEM TO NEW CORE BLK   */ 00753000
         L     @02,GSLPTR(,@05)                                    0336 00754000
         ST    @02,HEBBLKPT(,@08)                                  0336 00755000
*               HEBNBYTE=GSLPTR;    /* INIT AVAILABLE SPACE PTR      */ 00756000
         ST    @02,HEBNBYTE(,@08)                                  0337 00757000
*               HEBAVSP=GSLGETLN;   /* INIT AMNT OF AVAILABLE SPACE  */ 00758000
         MVC   HEBAVSP(3,@08),GSLGETLN(@05)                        0338 00759000
*               IF GSLKEY5=ON THEN  /* WAS CORE GOTTEN IN KEY 5      */ 00760000
         TM    GSLKEY5(@05),B'01000000'                            0339 00761000
         BNO   @RF00339                                            0339 00762000
*                 DO;               /* YES, CHECK TCB OWNERSHIP      */ 00763000
*                   HEBKEY5=ON;     /* INDICATE CORE GOTTEN IN KEY 5 */ 00764000
         OI    HEBKEY5(@08),B'01000000'                            0341 00765000
*                   IF GSLJSTCB=ON THEN/* IS CORE OWNED BY JOBSTEP 0342 00766000
*                                      TCB                           */ 00767000
         TM    GSLJSTCB(@05),B'00000100'                           0342 00768000
         BNO   @RF00342                                            0342 00769000
*                     HEBJSTCB=ON;  /* IND CORE OWNED BY JOBSTEP TCB */ 00770000
         OI    HEBJSTCB(@08),B'10000000'                           0343 00771000
*                 END;                                             0344 00772000
@RF00342 DS    0H                                                  0345 00773000
*               IF GSLGSRKT=ON THEN /* CORE GOTTEN IN GSR KEY?     0345 00774000
*                                                            @Z40ESSR*/ 00775000
@RF00339 LR    @08,GSNDEX                                          0345 00776000
         MH    @08,@CH00096                                        0345 00777000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0345 00778000
         TM    GSLGSRKT(@08),B'00100000'                           0345 00779000
         BNO   @RF00345                                            0345 00780000
*                 DO;               /*                       @Z40ESSR*/ 00781000
*                   HEBGSRKT=ON;    /* IND AS SUCH IN HEB    @Z40ESSR*/ 00782000
         L     @08,OPWVHDRE(,RCORE)                                0347 00783000
         OI    HEBGSRKT(@08),B'00100000'                           0347 00784000
*                   HEBGSRK=DXUKEY; /* SAVE GSR KEY IN HEB   @Z40ESSR*/ 00785000
         L     @05,OPWCOMWA(,RCORE)                                0348 00786000
         IC    @02,DXUKEY(,@05)                                    0348 00787000
         STC   @02,HEBGSRK(,@08)                                   0348 00788000
*                 END;              /*                       @Z40ESSR*/ 00789000
*               IF GSLIOSUP=ON THEN /* IS THIS A SPECIAL REQ BLLK    */ 00790000
@RF00345 LR    @08,GSNDEX                                          0350 00791000
         MH    @08,@CH00096                                        0350 00792000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0350 00793000
         TM    GSLIOSUP(@08),B'00010000'                           0350 00794000
         BNO   @RF00350                                            0350 00795000
*                 HEBIOSUP=ON;      /* YES, REFLECT SAME IN THE HEB  */ 00796000
         L     @08,OPWVHDRE(,RCORE)                                0351 00797000
         OI    HEBIOSUP(@08),B'00010000'                           0351 00798000
*               RFY                                                0352 00799000
*                 HEBHDREL BASED(MHEBPTR);/* BASE FOR HEB HDR ELEM   */ 00800000
@RF00350 DS    0H                                                  0353 00801000
*               OPWVHDRE->HEBELCHN=MHEBPTR;/* CHAIN NEW HDR ELEMENT     00802000
*                                      TO FIRST HDR ELEM ON CHAIN    */ 00803000
         L     @08,OPWVHDRE(,RCORE)                                0353 00804000
         L     @05,OPWVANCP(,RCORE)                                0353 00805000
         L     @02,MHEBPTR(,@05)                                   0353 00806000
         ST    @02,HEBELCHN(,@08)                                  0353 00807000
*               MHEBPTR=OPWVHDRE;   /* CHAIN NEW HEB HDR ELEM TO ITS    00808000
*                                      ANCHOR BLOCK                  */ 00809000
         ST    @08,MHEBPTR(,@05)                                   0354 00810000
*             END;                                                 0355 00811000
*         END;                                                     0356 00812000
*     END;                                                         0357 00813000
*   ELSE                                                           0358 00814000
*     ;                             /* NEW CORE BLK NOT NEEDED       */ 00815000
@RF00305 DS    0H                                                  0359 00816000
*   GSLPTR=HEBNBYTE;                /* RETURN ADDR OF GOTTEN CORE    */ 00817000
         LR    @08,GSNDEX                                          0359 00818000
         MH    @08,@CH00096                                        0359 00819000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0359 00820000
         L     @05,OPWVANCP(,RCORE)                                0359 00821000
         L     @05,MHEBPTR(,@05)                                   0359 00822000
         L     @02,HEBNBYTE(,@05)                                  0359 00823000
         ST    @02,GSLPTR(,@08)                                    0359 00824000
*   HEBNBYTE=HEBNBYTE+GSLREQLN;     /* UPDATE AVAIL SPACE PTR        */ 00825000
         L     @08,GSLREQLN-1(,@08)                                0360 00826000
         LA    @08,0(,@08)                                         0360 00827000
         ALR   @02,@08                                             0360 00828000
         ST    @02,HEBNBYTE(,@05)                                  0360 00829000
*   HEBAVSP=HEBAVSP-GSLREQLN;       /* DECR AVAILABLE SPACE          */ 00830000
         LCR   @08,@08                                             0361 00831000
         L     @02,HEBAVSP-1(,@05)                                 0361 00832000
         LA    @02,0(,@02)                                         0361 00833000
         ALR   @08,@02                                             0361 00834000
         STCM  @08,7,HEBAVSP(@05)                                  0361 00835000
*   RFY                                                            0362 00836000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 00837000
*   RET=OPWVSAVE(SAVENDEX);         /* RESTORE RETURN ADDRESS        */ 00838000
         LR    @08,SAVENDEX                                        0363 00839000
         SLA   @08,2                                               0363 00840000
         L     RET,OPWVSAVE-4(@08,RCORE)                           0363 00841000
*   RFY                                                            0364 00842000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 00843000
*   SAVENDEX=SAVENDEX-1;            /* POINT TO LAST SAVE AREA       */ 00844000
         BCTR  SAVENDEX,0                                          0365 00845000
*   END GETSPACE;                   /* RETURN TO THE CALLER          */ 00846000
         B     @EL00004                                            0366 00847000
*                                                                  0367 00848000
*/********************************************************************/ 00849000
*/*                                                                  */ 00850000
*/* FUNCTION: THIS ROUTINE IS USED TO GET CORE FOR DIRECT REQUESTS   */ 00851000
*/*           OR FOR NEW BLOCKS.                                     */ 00852000
*/*                                                                  */ 00853000
*/* INPUTS:   GETSPACE PARAMETER LIST WITHIN THE OPEN WORK AREA.     */ 00854000
*/*                                                                  */ 00855000
*/* OUTPUTS:  A POINTER TO THE GOTTEN CORE IN GSLPTR OR ZERO IF      */ 00856000
*/*           UNABLE TO OBTAIN CORE.                                 */ 00857000
*/*                                                                  */ 00858000
*/********************************************************************/ 00859000
*                                                                  0367 00860000
*GETCORE:                                                          0367 00861000
*   PROC OPTIONS(NOSAVE);                                          0367 00862000
GETCORE  DS    0H                                                  0368 00863000
*   SAVENDEX=SAVENDEX+1;            /* POINT TO NEXT SAVE AREA       */ 00864000
         AL    SAVENDEX,@CF00082                                   0368 00865000
*   RFY                                                            0369 00866000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 00867000
*   OPWVSAVE(SAVENDEX)=RET;         /* SAVE RETURN ADDRESS           */ 00868000
         LR    @15,SAVENDEX                                        0370 00869000
         SLA   @15,2                                               0370 00870000
         ST    RET,OPWVSAVE-4(@15,RCORE)                           0370 00871000
*   RFY                                                            0371 00872000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 00873000
*   IF GSLNOCSL^=ON THEN            /* IS A CSL ELEM TO BE BUILT     */ 00874000
         LR    @08,GSNDEX                                          0372 00875000
         MH    @08,@CH00096                                        0372 00876000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0372 00877000
         TM    GSLNOCSL(@08),B'00001000'                           0372 00878000
         BO    @RF00372                                            0372 00879000
*     DO;                           /* YES, BUILD A CSL ELEM         */ 00880000
*       CALL GETCSLEL;              /* GET AN UNUSED CSL ELEMENT     */ 00881000
         BAL   @14,GETCSLEL                                        0374 00882000
*       IF OPWVCSLE=0 THEN          /* WAS A CSL ELEMENT OBTAINED    */ 00883000
         SLR   @08,@08                                             0375 00884000
         C     @08,OPWVCSLE(,RCORE)                                0375 00885000
         BNE   @RF00375                                            0375 00886000
*         DO;                       /* NO, TERMINATE REQUEST         */ 00887000
*           GSLPTR=0;               /* INDICATE REQUEST FAILED       */ 00888000
         LR    @05,GSNDEX                                          0377 00889000
         MH    @05,@CH00096                                        0377 00890000
         LA    @05,OPWVGSPL-12(@05,RCORE)                          0377 00891000
         ST    @08,GSLPTR(,@05)                                    0377 00892000
*           RFY                                                    0378 00893000
*             RET RSTD;             /* RESTRICT RETURN REGISTER      */ 00894000
*           RET=OPWVSAVE(SAVENDEX); /* RESTORE RETURN ADDRESS        */ 00895000
         LR    @08,SAVENDEX                                        0379 00896000
         SLA   @08,2                                               0379 00897000
         L     RET,OPWVSAVE-4(@08,RCORE)                           0379 00898000
*           RFY                                                    0380 00899000
*             RET UNRSTD;           /* DROP REGISTER RESTRICTION     */ 00900000
*           SAVENDEX=SAVENDEX-1;    /* POINT TO LAST SAVE AREA       */ 00901000
         BCTR  SAVENDEX,0                                          0381 00902000
*           RETURN;                 /* RETURN TO THE CALLER          */ 00903000
@EL00005 DS    0H                                                  0382 00904000
@EF00005 DS    0H                                                  0382 00905000
@ER00005 BR    @14                                                 0382 00906000
*         END;                                                     0383 00907000
*       ELSE                                                       0384 00908000
*         ;                         /* A NEW CSL WAS OBTAINED        */ 00909000
@RF00375 DS    0H                                                  0385 00910000
*       RFY                                                        0385 00911000
*         CSLENTRY BASED(OPWVCSLE); /* EST BASE FOR NEW CSL ELEM     */ 00912000
*     END;                                                         0386 00913000
*   ELSE                                                           0387 00914000
*     ;                             /* NO CSL IS TO BE BUILT DET BNDY   00915000
*                                      ALIGNMENT FOR GETMAIN         */ 00916000
@RF00372 DS    0H                                                  0388 00917000
*   GSLGETLN=(GSLGETLN+7)&-DOUBLWDL;/* ROUND REQ LNG TO A DBL WORD   */ 00918000
         LR    @08,GSNDEX                                          0388 00919000
         MH    @08,@CH00096                                        0388 00920000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0388 00921000
         LA    @05,7                                               0388 00922000
         L     @02,GSLGETLN-1(,@08)                                0388 00923000
         LA    @02,0(,@02)                                         0388 00924000
         ALR   @05,@02                                             0388 00925000
         N     @05,@CF03173                                        0388 00926000
         STCM  @05,7,GSLGETLN(@08)                                 0388 00927000
*   IF GSLKEY5=ON&GSLSPNO=RDEBPOOL THEN/* BR ENTRY TO GETMAIN REQD   */ 00928000
         TM    GSLKEY5(@08),B'01000000'                            0389 00929000
         BNO   @RF00389                                            0389 00930000
         CLI   GSLSPNO(@08),230                                    0389 00931000
         BNE   @RF00389                                            0389 00932000
*     DO;                           /* YES, OBTAIN LOCAL LOCK FIRST     00933000
*                                      OBTAIN THE LOCAL LOCK         */ 00934000
*       OPWVRG13=R13;               /* SAVE REG 13                   */ 00935000
         ST    R13,OPWVRG13(,RCORE)                                0391 00936000
*       DO;                         /* SETLOCK (OBTAIN) MODE(UNCOND)    00937000
*                                      TYPE(LOCAL) RELATED(GETMAIN,ID   00938000
*                                      A0192M(GETCORE))              */ 00939000
*         RFY                                                      0393 00940000
*          (GPR11P,                                                0393 00941000
*           GPR12P,                                                0393 00942000
*           GPR13P,                                                0393 00943000
*           GPR14P) RSTD;                                          0393 00944000
*         GPR13P=PSALITA;           /* ADDRESS OF LOCK INTERFACE   0394 00945000
*                                      TABLE.                        */ 00946000
         L     GPR13P,PSALITA                                      0394 00947000
*         GEN(LM GPR11P,GPR13P,576(GPR13P));/* LOAD LOCKWORD, CLHT 0395 00948000
*                                      AND ENTRY ADRS.               */ 00949000
         LM GPR11P,GPR13P,576(GPR13P)                                   00950000
*         GEN(BALR GPR14P,GPR13P);  /* BALR TO SETLOCK RTN.          */ 00951000
         BALR GPR14P,GPR13P                                             00952000
*         RFY                                                      0397 00953000
*          (GPR11P,                                                0397 00954000
*           GPR12P,                                                0397 00955000
*           GPR13P,                                                0397 00956000
*           GPR14P) UNRSTD;                                        0397 00957000
*       END;                        /* SETLOCK (OBTAIN) MODE(UNCOND)    00958000
*                                      TYPE(LOCAL) RELATED(GETMAIN,ID   00959000
*                                      A0192M(GETCORE))              */ 00960000
*       R12=OPWVRG12;               /* RESTORE SECOND BASE REG       */ 00961000
         L     R12,OPWVRG12(,RCORE)                                0399 00962000
*       RFY                                                        0400 00963000
*        (R4,                                                      0400 00964000
*         R7,                                                      0400 00965000
*         R11) RSTD;                /* RESTRICT REGS USED BY GETMAIN */ 00966000
*       R11=RBASEA;                 /* SAVE MODULE BASE REGISTER     */ 00967000
         LR    R11,RBASEA                                          0401 00968000
*       R13=RCORE;                  /* SAVE PTR TO O/C/E WORK AREA   */ 00969000
         LR    R13,RCORE                                           0402 00970000
*       RFY                                                        0403 00971000
*         IDAOPWRK BASED(R13);      /* EST NEW BASE FOR VSAM W/A     */ 00972000
*       GEN REFS(RBASEA,R11);                                      0404 00973000
*                                   /* RE-ESTABLISH MODULE BASE         00974000
          DROP  RBASEA                 DROP MODULE BASE REGISTER        00975000
          USING @PSTART,R11            ESTABLISH ADDRESSABILITY         00976000
*       IF GSLJSTCB=ON THEN         /* CHECK CORE TCB OWNERSHIP      */ 00977000
         LR    @10,GSNDEX                                          0405 00978000
         MH    @10,@CH00096                                        0405 00979000
         LA    @10,OPWVGSPL-12(@10,R13)                            0405 00980000
         TM    GSLJSTCB(@10),B'00000100'                           0405 00981000
         BNO   @RF00405                                            0405 00982000
*         R4=TCBJSTCB;              /* PT TO JOBSTEP TCB FOR GETMAIN */ 00983000
         L     @10,PSATNEW                                         0406 00984000
         L     R4,TCBJSTCB(,@10)                                   0406 00985000
*       ELSE                        /* NOT JOB STEP RELATED          */ 00986000
*         R4=ADDR(TCB);             /* PT TO CURRENT TCB FOR GETMAIN */ 00987000
         B     @RC00405                                            0407 00988000
@RF00405 L     R4,PSATNEW                                          0407 00989000
*       R7=PSAAOLD;                 /* POINT TO ASCB FOR GETMAIN GET    00990000
*                                      CORE FOR THE DEB BLOCK        */ 00991000
@RC00405 L     R7,PSAAOLD                                          0408 00992000
*       DO;                         /* GETMAIN (RC) LV(GSLGETLN)   0409 00993000
*                                      SP(RDEBPOOL) KEY(5) BRANCH    */ 00994000
*         RESPECIFY                                                0410 00995000
*          (GPR01F,                                                0410 00996000
*           GPR03F,                                                0410 00997000
*           GPR00F) RESTRICTED;                                    0410 00998000
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */ 00999000
         SLR   GPR01F,GPR01F                                       0411 01000000
*         GPR03F=0;                 /* RC-TYPE GETMAIN               */ 01001000
         SLR   GPR03F,GPR03F                                       0412 01002000
*         GPR00F=GSLGETLN;          /* LENGTH REQUESTED              */ 01003000
         LR    @10,GSNDEX                                          0413 01004000
         MH    @10,@CH00096                                        0413 01005000
         LA    @10,OPWVGSPL-12(@10,R13)                            0413 01006000
         L     GPR00F,GSLGETLN-1(,@10)                             0413 01007000
         ICM   GPR00F,8,@CB03185                                   0413 01008000
*         GPR03F=GPR03F|((RDEBPOOL)*256);/* SP IN BYTE 2             */ 01009000
         O     GPR03F,@CF03174                                     0414 01010000
*         GPR03F=GPR03F|((5)*65536*16);/* KEY IN BYTE 1              */ 01011000
         O     GPR03F,@CF03176                                     0415 01012000
*         CALL CVTCRMN;             /* BRANCH ENTRY POINT            */ 01013000
         L     @10,CVTPTR                                          0416 01014000
         L     @15,CVTCRMN(,@10)                                   0416 01015000
         BALR  @14,@15                                             0416 01016000
*         RESPECIFY                                                0417 01017000
*          (GPR01F,                                                0417 01018000
*           GPR03F,                                                0417 01019000
*           GPR00F) UNRESTRICTED;                                  0417 01020000
*       END;                        /* GETMAIN (RC) LV(GSLGETLN)   0418 01021000
*                                      SP(RDEBPOOL) KEY(5) BRANCH    */ 01022000
*       RBASEA=R11;                 /* RESTORE MODULE BASE REGISTER  */ 01023000
         LR    RBASEA,R11                                          0419 01024000
*       RCORE=R13;                  /* RESTORE O/C/E WORK AREA PTR   */ 01025000
         LR    RCORE,R13                                           0420 01026000
*       RFY                                                        0421 01027000
*         IDAOPWRK BASED(RCORE);    /* RE-ESTABLISH BASE FOR VSAM W/A*/ 01028000
*       GEN REFS(RBASEA,R11);                                      0422 01029000
*                                   /* RE-ESTABLISH MODULE BASE         01030000
          DROP  R11                    DROP MODULE BASE REGISTER        01031000
          USING @PSTART,RBASEA         ESTABLISH ADDRESSABILITY         01032000
*       RFY                                                        0423 01033000
*        (R4,                                                      0423 01034000
*         R7,                                                      0423 01035000
*         R11) UNRSTD;              /* DROP REGISTER RESTRICTIONS  0423 01036000
*                                      RELEASE THE LOCAL LOCK        */ 01037000
*       DO;                         /* SETLOCK (RELEASE) TYPE(LOCAL)    01038000
*                                      RELATED(GETMAIN,IDA0192M(GETCO   01039000
*                                      RE))                          */ 01040000
*         RFY                                                      0425 01041000
*          (GPR11P,                                                0425 01042000
*           GPR12P,                                                0425 01043000
*           GPR13P,                                                0425 01044000
*           GPR14P) RSTD;                                          0425 01045000
*         GPR13P=PSALITA;           /* ADDRESS OF LOCK INTERFACE   0426 01046000
*                                      TABLE.                        */ 01047000
         L     GPR13P,PSALITA                                      0426 01048000
*         GEN(LM GPR11P,GPR13P,588(GPR13P));/* LOAD LOCKWORD, CLHT 0427 01049000
*                                      AND ENTRY ADRS.               */ 01050000
         LM GPR11P,GPR13P,588(GPR13P)                                   01051000
*         GEN(BALR GPR14P,GPR13P);  /* BALR TO SETLOCK RTN.          */ 01052000
         BALR GPR14P,GPR13P                                             01053000
*         RFY                                                      0429 01054000
*          (GPR11P,                                                0429 01055000
*           GPR12P,                                                0429 01056000
*           GPR13P,                                                0429 01057000
*           GPR14P) UNRSTD;                                        0429 01058000
*       END;                        /* SETLOCK (RELEASE) TYPE(LOCAL)    01059000
*                                      RELATED(GETMAIN,IDA0192M(GETCO   01060000
*                                      RE))                          */ 01061000
*       LM(R12,R13,OPWVLSAV);       /* RESTORE REGS USED BY SETLOCK  */ 01062000
         LM    R12,R13,OPWVLSAV(RCORE)                             0431 01063000
*     END;                                                         0432 01064000
*   ELSE                            /* NO, USE SVC GETMAIN           */ 01065000
*     DO;                           /* CHECK FOR KEY REQUESTS        */ 01066000
         B     @RC00389                                            0433 01067000
@RF00389 DS    0H                                                  0434 01068000
*       IF GSLKEY5=ON THEN          /* IS REQUEST FOR CORE IN KEY 5  */ 01069000
         LR    @08,GSNDEX                                          0434 01070000
         MH    @08,@CH00096                                        0434 01071000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0434 01072000
         TM    GSLKEY5(@08),B'01000000'                            0434 01073000
         BNO   @RF00434                                            0434 01074000
*         DO;                       /* MODESET EXTKEY(DATAMGT)       */ 01075000
*           SPKA(80);               /* SET PSW KEY                   */ 01076000
         LA    @08,80                                              0436 01077000
         SPKA  0(@08)                                              0436 01078000
*         END;                      /* MODESET EXTKEY(DATAMGT) YES,     01079000
*                                      MODESET TO RUN IN KEY 5       */ 01080000
*       ELSE                        /* NO, CHECK FOR GSR REQ @Z40ESSR*/ 01081000
*         IF GSLGSRKT=ON THEN       /* REQ FOR CORE IN GSR KEY?    0438 01082000
*                                                            @Z40ESSR*/ 01083000
         B     @RC00434                                            0438 01084000
@RF00434 LR    @08,GSNDEX                                          0438 01085000
         MH    @08,@CH00096                                        0438 01086000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0438 01087000
         TM    GSLGSRKT(@08),B'00100000'                           0438 01088000
         BNO   @RF00438                                            0438 01089000
*           DO;                     /*                       @Z40ESSR*/ 01090000
*             RFY                                                  0440 01091000
*               R1 RSTD;            /*                       @Z40ESSR*/ 01092000
*             R1=DXUKEY;            /* LOAD GSR KEY INTO REG @Z40ESSR*/ 01093000
         L     @08,OPWCOMWA(,RCORE)                                0441 01094000
         SLR   R1,R1                                               0441 01095000
         IC    R1,DXUKEY(,@08)                                     0441 01096000
*             SPKA(R1);             /* MODESET TO GSR KEY    @Z40ESSR*/ 01097000
         SPKA  0(R1)                                               0442 01098000
*             RFY                                                  0443 01099000
*               R1 UNRSTD;          /*                       @Z40ESSR*/ 01100000
*           END;                    /*                       @Z40ESSR*/ 01101000
*         ELSE                                                     0445 01102000
*           ;                       /* NO, CONTINUE TO RUN IN KEY 0  */ 01103000
@RF00438 DS    0H                                                  0446 01104000
*       IF GSLPGBDY=ON THEN         /* IS CORE TO BE ON A PAGE BNDRY    01105000
*                                      YES, GET CORE ON A PAGE BNDY  */ 01106000
@RC00434 LR    @08,GSNDEX                                          0446 01107000
         MH    @08,@CH00096                                        0446 01108000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0446 01109000
         TM    GSLPGBDY(@08),B'10000000'                           0446 01110000
         BNO   @RF00446                                            0446 01111000
*/*       ? GETMAIN (RC) LV(GSLGETLN) SP(GSLSPNO) BNDRY              */ 01112000
*                                                                  0447 01113000
*         DO;                       /* MANUALY EXPAND GETMAIN TO FIX    01114000
*                                      EXPANSION OF BNDRY PARAMETER  */ 01115000
*           RFY                                                    0448 01116000
*            (GPR01F,                                              0448 01117000
*             GPR15F,                                              0448 01118000
*             GPR00F) RSTD;         /* RESTRICT REGISTERS            */ 01119000
*           GPR01F=0;               /* REGISTER 1 MUST BE ZERO       */ 01120000
         SLR   GPR01F,GPR01F                                       0449 01121000
*           GPR15F=0;               /* RC-TYPE GETMIAN               */ 01122000
         SLR   GPR15F,GPR15F                                       0450 01123000
*           GPR00F=GSLGETLN;        /* LENGTH REQUESTED              */ 01124000
         L     GPR00F,GSLGETLN-1(,@08)                             0451 01125000
         ICM   GPR00F,8,@CB03185                                   0451 01126000
*           GPR15F=GPR15F|((GSLSPNO)*256);/* SUBPOOL IN BYTE 2       */ 01127000
         SLR   @14,@14                                             0452 01128000
         IC    @14,GSLSPNO(,@08)                                   0452 01129000
         SLA   @14,8                                               0452 01130000
         OR    GPR15F,@14                                          0452 01131000
*           GPR15F=GPR15F|'00000004'X;/* INDICATE PAGE BOUNDARY      */ 01132000
         O     GPR15F,@CF03113                                     0453 01133000
*           SVC(120);               /* RC/RU-FORM OF GETMAIN         */ 01134000
         SVC   120                                                 0454 01135000
*           RFY                                                    0455 01136000
*            (GPR01F,                                              0455 01137000
*             GPR15F,                                              0455 01138000
*             GPR00F) UNRSTD;       /* DROP REG RESTRICTS            */ 01139000
*         END;                      /* END GETMAIN EXPANSION         */ 01140000
*       ELSE                        /* PAGE BOUNDRY NOT REQUIRED     */ 01141000
*         DO;                       /* GETMAIN (RC) LV(GSLGETLN)   0457 01142000
*                                      SP(GSLSPNO)                   */ 01143000
         B     @RC00446                                            0457 01144000
@RF00446 DS    0H                                                  0458 01145000
*           RESPECIFY                                              0458 01146000
*            (GPR01F,                                              0458 01147000
*             GPR15F,                                              0458 01148000
*             GPR00F) RESTRICTED;                                  0458 01149000
*           GPR01F=0;               /* REG 1 MUST BE ZERO            */ 01150000
         SLR   GPR01F,GPR01F                                       0459 01151000
*           GPR15F=0;               /* RC-TYPE GETMAIN               */ 01152000
         SLR   GPR15F,GPR15F                                       0460 01153000
*           GPR00F=GSLGETLN;        /* LENGTH REQUESTED              */ 01154000
         LR    @14,GSNDEX                                          0461 01155000
         MH    @14,@CH00096                                        0461 01156000
         LA    @14,OPWVGSPL-12(@14,RCORE)                          0461 01157000
         L     GPR00F,GSLGETLN-1(,@14)                             0461 01158000
         ICM   GPR00F,8,@CB03185                                   0461 01159000
*           GPR15F=GPR15F|((GSLSPNO)*256);/* SP IN BYTE 2            */ 01160000
         SLR   @08,@08                                             0462 01161000
         IC    @08,GSLSPNO(,@14)                                   0462 01162000
         SLA   @08,8                                               0462 01163000
         OR    GPR15F,@08                                          0462 01164000
*           SVC(120);               /* RC/RU-FORM OF GETMAIN         */ 01165000
         SVC   120                                                 0463 01166000
*           RESPECIFY                                              0464 01167000
*            (GPR01F,                                              0464 01168000
*             GPR15F,                                              0464 01169000
*             GPR00F) UNRESTRICTED;                                0464 01170000
*         END;                      /* GETMAIN (RC) LV(GSLGETLN)   0465 01171000
*                                      SP(GSLSPNO)                   */ 01172000
*       DO;                         /* MODESET EXTKEY(SUPR)          */ 01173000
@RC00446 DS    0H                                                  0467 01174000
*         SPKA(0);                  /* SET PSW KEY                   */ 01175000
         SLR   @08,@08                                             0467 01176000
         SPKA  0(@08)                                              0467 01177000
*       END;                        /* MODESET EXTKEY(SUPR) RETURN TO   01178000
*                                      KEY O                         */ 01179000
*     END;                                                         0469 01180000
*   RFY                                                            0470 01181000
*    (R1,                                                          0470 01182000
*     RETCODE) RSTD;                /* RESTRICT GETMAIN REGISTERS    */ 01183000
@RC00389 DS    0H                                                  0471 01184000
*   IF RETCODE^=0 THEN              /* WAS GETMAIN SUCCESSFUL        */ 01185000
         LTR   RETCODE,RETCODE                                     0471 01186000
         BZ    @RF00471                                            0471 01187000
*     DO;                           /* NO, TERMINATE REQUEST         */ 01188000
*       RFY                                                        0473 01189000
*         RETCODE UNRSTD;           /* DROP REGISTER RESTRICTION     */ 01190000
*       RFY                                                        0474 01191000
*         IDACSL BASED(BIBCSL);     /* EST BASE FOR CORE SAVE LIST   */ 01192000
*       IF GSLNOCSL^=ON THEN        /* WAS A CSL ELEMENT GOTTEN      */ 01193000
         LR    @08,GSNDEX                                          0475 01194000
         MH    @08,@CH00096                                        0475 01195000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0475 01196000
         TM    GSLNOCSL(@08),B'00001000'                           0475 01197000
         BO    @RF00475                                            0475 01198000
*         CSLACTEN=CSLACTEN-1;      /* YES, IND CSL ELEM NOT ACTIVE  */ 01199000
         L     @08,OPWBIB(,RCORE)                                  0476 01200000
         L     @08,BIBCSL(,@08)                                    0476 01201000
         LH    @05,CSLACTEN(,@08)                                  0476 01202000
         BCTR  @05,0                                               0476 01203000
         STH   @05,CSLACTEN(,@08)                                  0476 01204000
*       ELSE                                                       0477 01205000
*         ;                         /* NO, CONTINUE TERMINATION      */ 01206000
@RF00475 DS    0H                                                  0478 01207000
*       GSLPTR=0;                   /* IND REQUEST FAILED            */ 01208000
         LR    @08,GSNDEX                                          0478 01209000
         MH    @08,@CH00096                                        0478 01210000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0478 01211000
         SLR   @05,@05                                             0478 01212000
         ST    @05,GSLPTR(,@08)                                    0478 01213000
*       RFY                                                        0479 01214000
*         RET RSTD;                 /* RESTRICT RETURN REGISTER      */ 01215000
*       RET=OPWVSAVE(SAVENDEX);     /* RESTORE RETURN ADDRESS        */ 01216000
         LR    @08,SAVENDEX                                        0480 01217000
         SLA   @08,2                                               0480 01218000
         L     RET,OPWVSAVE-4(@08,RCORE)                           0480 01219000
*       RFY                                                        0481 01220000
*         RET UNRSTD;               /* DROP REGISTER RESTRICTION     */ 01221000
*       SAVENDEX=SAVENDEX-1;        /* POINT TO LAST SAVE AREA       */ 01222000
         BCTR  SAVENDEX,0                                          0482 01223000
*       RETURN;                     /* RETURN TO THE CALLER          */ 01224000
         B     @EL00005                                            0483 01225000
*     END;                                                         0484 01226000
*   ELSE                            /* GETMAIN WAS SUCCESSFUL        */ 01227000
*     DO;                           /* FREE EXCESS CORE IF ANY       */ 01228000
@RF00471 DS    0H                                                  0486 01229000
*       GSLPTR=R1;                  /* GET ADDRESS OF GOTTEN CORE    */ 01230000
         LR    @08,GSNDEX                                          0486 01231000
         MH    @08,@CH00096                                        0486 01232000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0486 01233000
         ST    R1,GSLPTR(,@08)                                     0486 01234000
*       RFY                                                        0487 01235000
*         R1 UNRSTD;                /* DROP REGISTER RESTRICTION     */ 01236000
*       RFY                                                        0488 01237000
*        (R12,                                                     0488 01238000
*         R13,                                                     0488 01239000
*         R14,                                                     0488 01240000
*         R15) RSTD;                /* RESTRICT REGISTER USAGE       */ 01241000
*       OPWVRG13=R13;               /* SAVE R13 IN SAVE AREA         */ 01242000
         ST    R13,OPWVRG13(,RCORE)                                0489 01243000
*       R12=GSLPTR;                 /* POINT TO CORE TO BE CLEARED   */ 01244000
         LR    @05,GSNDEX                                          0490 01245000
         MH    @05,@CH00096                                        0490 01246000
         LA    @05,OPWVGSPL-12(@05,RCORE)                          0490 01247000
         L     @05,GSLPTR(,@05)                                    0490 01248000
         LR    R12,@05                                             0490 01249000
*       R13=GSLGETLN;               /* AMOUNT OF CORE TO BE CLEARED  */ 01250000
         L     R13,GSLGETLN-1(,@08)                                0491 01251000
         LA    R13,0(,R13)                                         0491 01252000
*       R15=0;                      /* PADDING CHARACTER             */ 01253000
         SLR   R15,R15                                             0492 01254000
*       MVCL(R12,R14);              /* CLEAR GOTTEN CORE             */ 01255000
         MVCL  R12,R14                                             0493 01256000
*       LM(R12,R13,OPWVLSAV);       /* RESTORE BASE REG & REG 13     */ 01257000
         LM    R12,R13,OPWVLSAV(RCORE)                             0494 01258000
*       RFY                                                        0495 01259000
*        (R12,                                                     0495 01260000
*         R13,                                                     0495 01261000
*         R14,                                                     0495 01262000
*         R15) UNRSTD;              /* DROP REG RESTRICTIONS         */ 01263000
*       IF GSLNOCSL^=ON THEN        /* WAS A CSL ELEM GOTTEN FOR REQ */ 01264000
         LR    @08,GSNDEX                                          0496 01265000
         MH    @08,@CH00096                                        0496 01266000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0496 01267000
         TM    GSLNOCSL(@08),B'00001000'                           0496 01268000
         BO    @RF00496                                            0496 01269000
*         DO;                       /* YES, INIT CSL ELEMENT         */ 01270000
*           CSLPOOLN=GSLSPNO;       /* INIT SUBPOOL IN CSL ENTRY     */ 01271000
         L     @05,OPWVCSLE(,RCORE)                                0498 01272000
         IC    @02,GSLSPNO(,@08)                                   0498 01273000
         STC   @02,CSLPOOLN(,@05)                                  0498 01274000
*           CSLCORLN=GSLGETLN;      /* INIT LENGTH OF GOTTEN CORE    */ 01275000
         MVC   CSLCORLN(3,@05),GSLGETLN(@08)                       0499 01276000
*           CSLCORPT=GSLPTR;        /* INIT PTR TO GOTTEN CORE       */ 01277000
         L     @02,GSLPTR(,@08)                                    0500 01278000
         ST    @02,CSLCORPT(,@05)                                  0500 01279000
*           CSLANCPT=0;             /* INIT ANCHOR PTR TO ZERO TO IND   01280000
*                                      NO HEB HDR ELEM ASSOC WITH CSL*/ 01281000
         SLR   @02,@02                                             0501 01282000
         STCM  @02,7,CSLANCPT(@05)                                 0501 01283000
*           IF GSLKEY5=ON THEN      /* WAS CORE GOTTEN IN KEY 5      */ 01284000
         TM    GSLKEY5(@08),B'01000000'                            0502 01285000
         BNO   @RF00502                                            0502 01286000
*             DO;                   /* YES, CHECK TCB OWNERSHIP      */ 01287000
*               CSLKEY5=ON;         /* INDICATE CORE GOTTEN IN KEY 5 */ 01288000
         OI    CSLKEY5(@05),B'10000000'                            0504 01289000
*               IF GSLJSTCB=ON THEN /* IS CORE OWNED BY JOBSTEP TCB  */ 01290000
         TM    GSLJSTCB(@08),B'00000100'                           0505 01291000
         BNO   @RF00505                                            0505 01292000
*                 CSLJSTCB=ON;      /* IND CORE OWNED BY JOBSTEP TCB */ 01293000
         OI    CSLJSTCB(@05),B'00100000'                           0506 01294000
*             END;                                                 0507 01295000
@RF00505 DS    0H                                                  0508 01296000
*           IF GSLGSRKT=ON THEN     /* CORE GOTTEN IN GSR KEY?     0508 01297000
*                                                            @Z40ESSR*/ 01298000
@RF00502 LR    @08,GSNDEX                                          0508 01299000
         MH    @08,@CH00096                                        0508 01300000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0508 01301000
         TM    GSLGSRKT(@08),B'00100000'                           0508 01302000
         BNO   @RF00508                                            0508 01303000
*             CSLGSRKT=ON;          /* IND GSR KEY TYPE IN CSL     0509 01304000
*                                                            @Z40ESSR*/ 01305000
         L     @08,OPWVCSLE(,RCORE)                                0509 01306000
         OI    CSLGSRKT(@08),B'01000000'                           0509 01307000
*         END;                                                     0510 01308000
*       ELSE                                                       0511 01309000
*         ;                         /* NO CSL ELEMENT WAS GOTTEN     */ 01310000
@RF00496 DS    0H                                                  0512 01311000
*     END;                                                         0512 01312000
@RC00496 DS    0H                                                  0513 01313000
*   RFY                                                            0513 01314000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 01315000
*   RET=OPWVSAVE(SAVENDEX);         /* RESTORE RETURN ADDRESS        */ 01316000
         LR    @08,SAVENDEX                                        0514 01317000
         SLA   @08,2                                               0514 01318000
         L     RET,OPWVSAVE-4(@08,RCORE)                           0514 01319000
*   RFY                                                            0515 01320000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 01321000
*   SAVENDEX=SAVENDEX-1;            /* POINT TO LAST SAVE AREA       */ 01322000
         BCTR  SAVENDEX,0                                          0516 01323000
*   END GETCORE;                    /* RETURN TO THE CALLER          */ 01324000
         B     @EL00005                                            0517 01325000
*                                                                  0518 01326000
*/********************************************************************/ 01327000
*/*                                                                  */ 01328000
*/* FUNCTION: THIS ROUTINE IS USED TO GET A HEADER ELEMENT FROM THE  */ 01329000
*/*           FREE QUEUE OR TO GET A NEW HEADER ELEMENT BLOCK IF     */ 01330000
*/*           THERE ARE NO FREE HEADER ELEMENTS ON THE FREE QUEUE.   */ 01331000
*/*                                                                  */ 01332000
*/* INPUTS:   POINTERS IN THE BIB TO THE HEADER ELEMENT FREE QUEUE   */ 01333000
*/*           AND THE PROTECTED SPHERE BLOCK.                        */ 01334000
*/*                                                                  */ 01335000
*/* OUTPUTS:  A POINTER TO A FREE HEADER ELEMENT IN OPWVHDRE OR ZERO */ 01336000
*/*           IF UNABLE TO OBTAIN A FREE HEADER ELEMENT.             */ 01337000
*/*                                                                  */ 01338000
*/********************************************************************/ 01339000
*                                                                  0518 01340000
*GETHDREL:                                                         0518 01341000
*   PROC OPTIONS(NOSAVE);                                          0518 01342000
GETHDREL DS    0H                                                  0519 01343000
*   SAVENDEX=SAVENDEX+1;            /* POINT TO NEXT SAVE AREA       */ 01344000
         LA    @08,1                                               0519 01345000
         ALR   SAVENDEX,@08                                        0519 01346000
*   RFY                                                            0520 01347000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 01348000
*   OPWVSAVE(SAVENDEX)=RET;         /* SAVE RETURN ADDRESS           */ 01349000
         LR    @15,SAVENDEX                                        0521 01350000
         SLA   @15,2                                               0521 01351000
         ST    RET,OPWVSAVE-4(@15,RCORE)                           0521 01352000
*   RFY                                                            0522 01353000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 01354000
*   IF BIBHEBFQ=0 THEN              /* ANY FREE HEB HDR ELEMENTS     */ 01355000
         L     @05,OPWBIB(,RCORE)                                  0523 01356000
         L     @02,BIBHEBFQ(,@05)                                  0523 01357000
         LTR   @02,@02                                             0523 01358000
         BNZ   @RF00523                                            0523 01359000
*     DO;                           /* NO, BUILD A NEW HEB BLOCK     */ 01360000
*       GSNDEX=GSNDEX+1;            /* PT TO NEXT GETSPACE PARM LIST */ 01361000
         ALR   GSNDEX,@08                                          0525 01362000
*       GSLREQLN=LENGTH(IDAHEB);    /* INIT REQ LENGTH TO LNG OF HEB */ 01363000
         LR    @08,GSNDEX                                          0526 01364000
         MH    @08,@CH00096                                        0526 01365000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0526 01366000
         MVC   GSLREQLN(3,@08),@CF03177+1                          0526 01367000
*       OPWVANCS=ADDR(BIBPRSPH);    /* SET UP PTR TO PROTECTED SPHERE   01368000
*                                      BLOCK PTR                     */ 01369000
         LA    @05,BIBPRSPH(,@05)                                  0527 01370000
         ST    @05,OPWVANCS(,RCORE)                                0527 01371000
*       IF OPWLSR=ON THEN           /* IF LSR OPEN           @OZ04230*/ 01372000
         TM    OPWLSR(RCORE),B'00100000'                           0528 01373000
         BNO   @RF00528                                            0528 01374000
*         GSLSPNO=PRTBLKPL;         /* SUBPOOL IN MEMORY     @OZ04230*/ 01375000
         MVI   GSLSPNO(@08),X'FC'                                  0529 01376000
*       ELSE                        /* NOT LSR, SO           @OZ04230*/ 01377000
*         GSLSPNO=PRTSPPL;          /* USE SUBPOOL IN CSA    @OZ04230*/ 01378000
         B     @RC00528                                            0530 01379000
@RF00528 LR    @08,GSNDEX                                          0530 01380000
         MH    @08,@CH00096                                        0530 01381000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0530 01382000
         MVI   GSLSPNO(@08),X'F1'                                  0530 01383000
*       GSLNOCSL=ON;                /* IND NO CSL ELEM IS TO BE BUILT*/ 01384000
@RC00528 LR    @08,GSNDEX                                          0531 01385000
         MH    @08,@CH00096                                        0531 01386000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0531 01387000
         OI    GSLNOCSL(@08),B'00001000'                           0531 01388000
*       CALL PRTSPREQ;              /* GET SPACE FOR A NEW HEB BLOCK */ 01389000
         BAL   @14,PRTSPREQ                                        0532 01390000
*       IF GSLPTR=0 THEN            /* WAS A NEW HEB BLOCK GOTTEN    */ 01391000
         SLR   @08,@08                                             0533 01392000
         LR    @05,GSNDEX                                          0533 01393000
         MH    @05,@CH00096                                        0533 01394000
         LA    @05,OPWVGSPL-12(@05,RCORE)                          0533 01395000
         C     @08,GSLPTR(,@05)                                    0533 01396000
         BNE   @RF00533                                            0533 01397000
*         DO;                       /* NO, TERMINATE REQUEST         */ 01398000
*           OPWVHDRE=0;             /* IND UNABLE TO GET HDR ELEMENT */ 01399000
         ST    @08,OPWVHDRE(,RCORE)                                0535 01400000
*           GSNDEX=GSNDEX-1;        /* POINT TO PREVIOUS PARM LIST   */ 01401000
         BCTR  GSNDEX,0                                            0536 01402000
*           RFY                                                    0537 01403000
*             RET RSTD;             /* RESTRICT RETURN REGISTER      */ 01404000
*           RET=OPWVSAVE(SAVENDEX); /* RESTORE RETURN ADDRESS        */ 01405000
         LR    @08,SAVENDEX                                        0538 01406000
         SLA   @08,2                                               0538 01407000
         L     RET,OPWVSAVE-4(@08,RCORE)                           0538 01408000
*           RFY                                                    0539 01409000
*             RET UNRSTD;           /* DROP REGISTER RESTRICTION     */ 01410000
*           SAVENDEX=SAVENDEX-1;    /* POINT TO LAST SAVE AREA       */ 01411000
         BCTR  SAVENDEX,0                                          0540 01412000
*           RETURN;                 /* RETURN TO THE CALLER          */ 01413000
@EL00006 DS    0H                                                  0541 01414000
@EF00006 DS    0H                                                  0541 01415000
@ER00006 BR    @14                                                 0541 01416000
*         END;                                                     0542 01417000
*       ELSE                        /* A NEW HEB WAS GOTTEN          */ 01418000
*         DO;                       /* INIT HEADER OF NEW HEB BLOCK  */ 01419000
@RF00533 DS    0H                                                  0544 01420000
*           RFY                                                    0544 01421000
*             IDAHEB BASED(GSLPTR); /* EST BASE FOR NEW HEB BLOCK    */ 01422000
*           HEBID=IDXHEB;           /* PUT ID OF BLOCK INTO HEB      */ 01423000
         LR    @08,GSNDEX                                          0545 01424000
         MH    @08,@CH00096                                        0545 01425000
         LA    @08,OPWVGSPL-12(@08,RCORE)                          0545 01426000
         L     @05,GSLPTR(,@08)                                    0545 01427000
         MVI   HEBID(@05),B'00010011'                              0545 01428000
*           HEBLEN=GSLREQLN;        /* INIT LENGTH OF HEB BLOCK      */ 01429000
         L     @08,GSLREQLN-1(,@08)                                0546 01430000
         LA    @08,0(,@08)                                         0546 01431000
         STH   @08,HEBLEN(,@05)                                    0546 01432000
*           HEBCNT=DIM(HEBHDELS);   /* INIT NUMBER OF HDR ELEMENTS   */ 01433000
         MVC   HEBCNT(2,@05),@CH00098                              0547 01434000
*           HEBNHEB=BIBHEBPT;       /* CHAIN NEW HEB TO FIRST HEB ON    01435000
*                                      CHAIN OF HEB(S)               */ 01436000
         L     @08,OPWBIB(,RCORE)                                  0548 01437000
         L     @02,BIBHEBPT(,@08)                                  0548 01438000
         ST    @02,HEBNHEB(,@05)                                   0548 01439000
*           BIBHEBPT=GSLPTR;        /* PUT NEW HEB ON THE HEB CHAIN  */ 01440000
         ST    @05,BIBHEBPT(,@08)                                  0549 01441000
*           BIBHEBFQ=ADDR(HEBHDELS);/* PT HEB FREE QUEUE TO FIRST  0550 01442000
*                                      ELEMENT IN THE NEW HEB        */ 01443000
         LA    @05,HEBHDELS(,@05)                                  0550 01444000
         ST    @05,BIBHEBFQ(,@08)                                  0550 01445000
*           DO INDEX1=1 TO DIM(HEBHDELS)-1 BY 1;/* CHAIN NEW HDR   0551 01446000
*                                      ELEMENTS TOGETHER             */ 01447000
         LA    INDEX1,1                                            0551 01448000
@DL00551 DS    0H                                                  0552 01449000
*             RFY                                                  0552 01450000
*               HEBHDREL BASED(ADDR(HEBHDELS(INDEX1)));/* EST BASE 0552 01451000
*                                      FOR HEB HEADER ELEMENTS       */ 01452000
*             HEBELCHN=ADDR(HEBHDELS(INDEX1+1));/* CHAIN THIS HDR  0553 01453000
*                                      ELEMENT TO THE NEXT ONE       */ 01454000
         LR    @08,INDEX1                                          0553 01455000
         MH    @08,@CH00102                                        0553 01456000
         LR    @02,GSNDEX                                          0553 01457000
         MH    @02,@CH00096                                        0553 01458000
         LA    @02,OPWVGSPL-12(@02,RCORE)                          0553 01459000
         L     @02,GSLPTR(,@02)                                    0553 01460000
         LA    @01,0(@08,@02)                                      0553 01461000
         AL    @01,@CF03186                                        0553 01462000
         LA    @08,HEBHDELS(@08,@02)                               0553 01463000
         ST    @08,HEBELCHN(,@01)                                  0553 01464000
*           END;                                                   0554 01465000
         AL    INDEX1,@CF00082                                     0554 01466000
         C     INDEX1,@CF00049                                     0554 01467000
         BNH   @DL00551                                            0554 01468000
*           HEBELCHN=0;             /* ZERO CHAIN PTR IN LAST ELEM   */ 01469000
         LR    @08,INDEX1                                          0555 01470000
         MH    @08,@CH00102                                        0555 01471000
         LR    @02,GSNDEX                                          0555 01472000
         MH    @02,@CH00096                                        0555 01473000
         LA    @02,OPWVGSPL-12(@02,RCORE)                          0555 01474000
         L     @02,GSLPTR(,@02)                                    0555 01475000
         AL    @02,@CF03186                                        0555 01476000
         LA    @08,HEBHDELS-12(@08,@02)                            0555 01477000
         SLR   @02,@02                                             0555 01478000
         ST    @02,HEBELCHN(,@08)                                  0555 01479000
*         END;                                                     0556 01480000
*       GSNDEX=GSNDEX-1;            /* POINT TO PREVIOUS PARM LIST   */ 01481000
         BCTR  GSNDEX,0                                            0557 01482000
*     END;                                                         0558 01483000
*   ELSE                                                           0559 01484000
*     ;                             /* A FREE HDR ELEM IS AVAILABLE  */ 01485000
@RF00523 DS    0H                                                  0560 01486000
*   RFY                                                            0560 01487000
*     HEBHDREL BASED(BIBHEBFQ);     /* BASE FOR FIRST FREE HDR ELEM  */ 01488000
*   OPWVHDRE=BIBHEBFQ;              /* RETURN ADDR OF FREE HDR ELEM  */ 01489000
         L     @08,OPWBIB(,RCORE)                                  0561 01490000
         L     @05,BIBHEBFQ(,@08)                                  0561 01491000
         ST    @05,OPWVHDRE(,RCORE)                                0561 01492000
*   BIBHEBFQ=HEBELCHN;              /* REMOVE HDR ELEM FROM FREE QUE */ 01493000
         L     @05,HEBELCHN(,@05)                                  0562 01494000
         ST    @05,BIBHEBFQ(,@08)                                  0562 01495000
*   RFY                                                            0563 01496000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 01497000
*   RET=OPWVSAVE(SAVENDEX);         /* RESTORE RETURN ADDRESS        */ 01498000
         LR    @08,SAVENDEX                                        0564 01499000
         SLA   @08,2                                               0564 01500000
         L     RET,OPWVSAVE-4(@08,RCORE)                           0564 01501000
*   RFY                                                            0565 01502000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 01503000
*   SAVENDEX=SAVENDEX-1;            /* POINT TO LAST SAVE AREA       */ 01504000
         BCTR  SAVENDEX,0                                          0566 01505000
*   END GETHDREL;                   /* RETURN TO THE CALLER          */ 01506000
         B     @EL00006                                            0567 01507000
*                                                                  0568 01508000
*/********************************************************************/ 01509000
*/*                                                                  */ 01510000
*/* FUNCTION: THIS ROUTINE IS USED TO GET A FREE CORE SAVE LIST      */ 01511000
*/*           ELEMENT IF ANY ARE AVAILABLE, OR TO GET A NEW CORE     */ 01512000
*/*           SAVE LIST IF THERE ARE NO FREE ELEMENTS OR THERE ARE   */ 01513000
*/*           NO CORE SAVE LISTS BUILT.                              */ 01514000
*/*                                                                  */ 01515000
*/* INPUTS:   A POINTER WITHIN THE OPEN WORK AREA TO THE CORE SAVE   */ 01516000
*/*           LIST(S).                                               */ 01517000
*/*                                                                  */ 01518000
*/* OUTPUTS:  A POINTER IN OPWVCSLE TO A FREE CORE SAVE LIST ELEMENT */ 01519000
*/*           OR ZERO IF UNABLE TO OBTAIN A FREE ELEMENT.            */ 01520000
*/*                                                                  */ 01521000
*/********************************************************************/ 01522000
*                                                                  0568 01523000
*GETCSLEL:                                                         0568 01524000
*   PROC OPTIONS(NOSAVE);                                          0568 01525000
GETCSLEL DS    0H                                                  0569 01526000
*   SAVENDEX=SAVENDEX+1;            /* POINT TO NEXT SAVE AREA       */ 01527000
         AL    SAVENDEX,@CF00082                                   0569 01528000
*   RFY                                                            0570 01529000
*     RET RSTD;                     /* RESTRICT RETURN REGISTER      */ 01530000
*   OPWVSAVE(SAVENDEX)=RET;         /* SAVE RETURN ADDRESS           */ 01531000
         LR    @15,SAVENDEX                                        0571 01532000
         SLA   @15,2                                               0571 01533000
         ST    RET,OPWVSAVE-4(@15,RCORE)                           0571 01534000
*   RFY                                                            0572 01535000
*     RET UNRSTD;                   /* DROP REGISTER RESTRICTION     */ 01536000
*   RFY                                                            0573 01537000
*     IDACSL BASED(BIBCSL);         /* EST BASE FOR CORE SAVE LIST   */ 01538000
*   IF BIBCSL=0|CSLACTEN=DIM(CSLNTRYS) THEN/* ANY FREE CSL ELEM      */ 01539000
         L     @08,OPWBIB(,RCORE)                                  0574 01540000
         L     @08,BIBCSL(,@08)                                    0574 01541000
         LTR   @08,@08                                             0574 01542000
         BZ    @RT00574                                            0574 01543000
         CLC   CSLACTEN(2,@08),@CH00106                            0574 01544000
         BNE   @RF00574                                            0574 01545000
@RT00574 DS    0H                                                  0575 01546000
*     DO;                           /* NO, BUILD A NEW CORE SAVE LIST*/ 01547000
*       IF OPWCAT=ON|OPWSCRA=ON|OPWVVIC=ON|OPWGSR=ON|OPWCBIC=ON THEN    01548000
