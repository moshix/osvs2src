         LTR   @08,@08                                             0249 00443000
         BNZ   @RF00249                                            0249 00444000
@RT00249 DS    0H                                                  0250 00445000
*     DO;                           /* YES, BUILD A NEW PROT SPH BLK */ 00446000
*       GSLGETLN=MAX(K,GSLREQLN);   /* INIT LENGTH OF BLOCK          */ 00447000
         LR    @08,GSNDEX                                          0251 00448000
