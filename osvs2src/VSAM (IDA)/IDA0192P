*     FIELDPTR=ZERO;                /* SET FIELD PTR TO ZERO @Y30DSMD*/ 00419000
         B     @RC00251                                            0256 00420000
@RF00251 SLR   FIELDPTR,FIELDPTR                                   0256 00421000
*   CALL LDFIELD;                   /* LOAD FIELD INTO MESSAGE       */ 00422000
@RC00251 BAL   @14,LDFIELD                                         0257 00423000
*/*    PUT DEVICE NAME IN UCB IN MESSAGE                             */ 00424000
*                                                                  0258 00425000
