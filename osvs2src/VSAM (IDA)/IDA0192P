*   CALL WTPRTN;                    /* WRITE TO PROGRAMMER           */ 00538000
         BAL   @14,WTPRTN                                          0296 00539000
*   RESPECIFY                                                      0297 00540000
*     RET RESTRICTED;                                              0297 00541000
*   RET=RSAVEC;                     /* RESTORE RETURN ADDRESS        */ 00542000
         L     RET,RSAVEC(,RCORE)                                  0298 00543000
*   RESPECIFY                                                      0299 00544000
*     RET UNRESTRICTED;                                            0299 00545000
*   RFY                                                            0300 00546000
*     IFGACB BASED(PDPACB);                                        0300 00547000
*   END MSGBUILD;                                                  0301 00548000
@EL00002 DS    0H                                                  0301 00549000
