*                                                                  0302 00563000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0302 00564000
SFIMSG   DS    0H                                                  0303 00565000
*   RESPECIFY                                                      0303 00566000
*    (R12,                                                         0303 00567000
*     RET) RESTRICTED;                                             0303 00568000
*   RSAVED=RET;                     /* SAVE RETURN ADDRESS   @Y30DSMD*/ 00569000
         ST    RET,RSAVED(,RCORE)                                  0304 00570000
*   RESPECIFY                                                      0305 00571000
*     RET UNRESTRICTED;                                            0305 00572000
*   WTPMSG(PTRB)=LPAREN;            /* INSERT LEFT PAREN.    @Y30DSMD*/ 00573000
         LR    @01,RCORE                                           0306 00574000
         ALR   @01,PTRB                                            0306 00575000
         MVI   WTPMSG-1(@01),C'('                                  0306 00576000
*   PTRB=PTRB+ONE;                  /* PT TO NEXT POSITION   @Y30DSMD*/ 00577000
         AL    PTRB,@CF00040                                       0307 00578000
*   R12=SFIRETCD;                   /* GET CATLG RETURN CODE @Y30DSMD*/ 00579000
         L     @01,PDPSFI+280(,RCORE)                              0308 00580000
         L     R12,SFIRETCD(,@01)                                  0308 00581000
*   GEN(CVD R12,PDWORKSP(,RCORE));  /* CONVERT TO DECIMAL    @Y30DSMD*/ 00582000
         CVD R12,PDWORKSP(,RCORE)                                       00583000
*   UNPK(WTPMSG(PTRB:PTRB+TWO),PDWORKSP(7:8));/* CHANGE TO SIGNED  0310 00584000
*                                      DECIMAL               @Y30DSMD*/ 00585000
         LR    @01,RCORE                                           0310 00586000
         ALR   @01,PTRB                                            0310 00587000
         UNPK  WTPMSG-1(3,@01),PDWORKSP+6(2,RCORE)                 0310 00588000
*   WTPMSG(PTRB+TWO)=WTPMSG(PTRB+TWO)|ZEROCHAR;/* TURN OFF SIGN    0311 00589000
*                                                            @Y30DSMD*/ 00590000
         LR    @01,RCORE                                           0311 00591000
         ALR   @01,PTRB                                            0311 00592000
         OI    WTPMSG+1(@01),C'0'                                  0311 00593000
*   PTRB=PTRB+THREE;                /* PT TO NEXT POSITION   @Y30DSMD*/ 00594000
         AL    PTRB,@CF00203                                       0312 00595000
*   WTPMSG(PTRB)=COMMA;             /* INSERT A COMMA        @Y30DSMD*/ 00596000
         LR    @01,RCORE                                           0313 00597000
         ALR   @01,PTRB                                            0313 00598000
         MVI   WTPMSG-1(@01),C','                                  0313 00599000
*   PTRB=PTRB+ONE;                  /* PT TO NEXT POSITION   @Y30DSMD*/ 00600000
         AL    PTRB,@CF00040                                       0314 00601000
*   R12=SFIRSNCD;                   /* GET REASON CODE       @Y30DSMD*/ 00602000
         L     @01,PDPSFI+280(,RCORE)                              0315 00603000
         SLR   R12,R12                                             0315 00604000
         IC    R12,SFIRSNCD(,@01)                                  0315 00605000
*   GEN(CVD R12,PDWORKSP(,RCORE));  /* CONVERT TO DECIMAL    @Y30DSMD*/ 00606000
         CVD R12,PDWORKSP(,RCORE)                                       00607000
*   UNPK(WTPMSG(PTRB:PTRB+TWO),PDWORKSP(7:8));/* CHANGE TO SIGNED  0317 00608000
*                                      DECIMAL               @Y30DSMD*/ 00609000
         LR    @01,RCORE                                           0317 00610000
         ALR   @01,PTRB                                            0317 00611000
         UNPK  WTPMSG-1(3,@01),PDWORKSP+6(2,RCORE)                 0317 00612000
*   WTPMSG(PTRB+TWO)=WTPMSG(PTRB+TWO)|ZEROCHAR;/* TURN OFF SIGN    0318 00613000
*                                                            @Y30DSMD*/ 00614000
         LR    @01,RCORE                                           0318 00615000
         ALR   @01,PTRB                                            0318 00616000
         OI    WTPMSG+1(@01),C'0'                                  0318 00617000
*   PTRB=PTRB+THREE;                /* PT TO NEXT POSITION   @Y30DSMD*/ 00618000
         AL    PTRB,@CF00203                                       0319 00619000
*   WTPMSG(PTRB)=COMMA;             /* INSERT A COMMA        @Y30DSMD*/ 00620000
         LR    @01,RCORE                                           0320 00621000
         ALR   @01,PTRB                                            0320 00622000
         MVI   WTPMSG-1(@01),C','                                  0320 00623000
*   PTRB=PTRB+ONE;                  /* PT TO NEXT POSITION   @Y30DSMD*/ 00624000
         LA    @15,1                                               0321 00625000
         ALR   PTRB,@15                                            0321 00626000
*   WTPMSG(PTRB:PTRB+FIVE)=CATMODID;/* INSERT FIRST PART OF MODULE 0322 00627000
*                                      NAME                  @Y30DSMD*/ 00628000
         LR    @01,RCORE                                           0322 00629000
         ALR   @01,PTRB                                            0322 00630000
         MVC   WTPMSG-1(6,@01),@CC03435                            0322 00631000
*   PTRB=PTRB+SIX;                  /* PT TO NEXT POSITION   @Y30DSMD*/ 00632000
         AL    PTRB,@CF01675                                       0323 00633000
*   MVC(WTPMSG(PTRB:PTRB+ONE),SFIMODID);/* MOVE IN MODULE ID @Y30DSMD*/ 00634000
         LR    @01,RCORE                                           0324 00635000
         ALR   @01,PTRB                                            0324 00636000
         L     @14,PDPSFI+280(,RCORE)                              0324 00637000
         MVC   WTPMSG-1(2,@01),SFIMODID(@14)                       0324 00638000
*   PTRB=PTRB+TWO;                  /* PT TO NEXT POSITION   @Y30DSMD*/ 00639000
         AL    PTRB,@CF00048                                       0325 00640000
*   WTPMSG(PTRB)=RPAREN;            /* INSERT RIGHT PAREN.   @Y30DSMD*/ 00641000
         LR    @01,RCORE                                           0326 00642000
         ALR   @01,PTRB                                            0326 00643000
         MVI   WTPMSG-1(@01),C')'                                  0326 00644000
*   PTRB=PTRB+ONE;                  /* PT TO NEXT POSITION   @Y30DSMD*/ 00645000
         ALR   PTRB,@15                                            0327 00646000
*   RESPECIFY                                                      0328 00647000
*     RET RESTRICTED;                                              0328 00648000
*   RET=RSAVED;                     /* GET RETURN ADDRESS    @Y30DSMD*/ 00649000
         L     RET,RSAVED(,RCORE)                                  0329 00650000
*   RESPECIFY                                                      0330 00651000
*    (R12,                                                         0330 00652000
*     RET) UNRESTRICTED;                                           0330 00653000
*   END SFIMSG;                                                    0331 00654000
@EL00003 DS    0H                                                  0331 00655000
@EF00003 DS    0H                                                  0331 00656000
@ER00003 BR    @14                                                 0331 00657000
*LDFIELD:                                                          0332 00658000
*                                                                  0332 00659000
*/********************************************************************/ 00660000
*/*                                                                  */ 00661000
*/*   FUNCTION=DETERMINES THE LENGTH OF THE FIELD POINTED TO BY      */ 00662000
*/*            FIELDPTR AND LOADS THE CONTENTS OF THE FIELD INTO     */ 00663000
*/*            MESSAGE. IF THE FIELD PTR IS ZERO, A COMMA IS         */ 00664000
*/*            INSERTED INTO THE MESSAGE TO INDICATE A MISSING.      */ 00665000
*/*                                                                  */ 00666000
*/*   INPUT=ADDRESS OF FIELD TO BE LOADED (FIELDPTR)                 */ 00667000
*/*         MAXIMUM LENGTH OF FIELD (FLDLNGTH)                       */ 00668000
*/*                                                                  */ 00669000
*/*   OUTPUT=SPECIFIED FIELD MOVED INTO MESSAGE BUFFER               */ 00670000
*/*                                                                  */ 00671000
*/********************************************************************/ 00672000
*                                                                  0332 00673000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0332 00674000
LDFIELD  DS    0H                                                  0333 00675000
*   RESPECIFY                                                      0333 00676000
*     RET RESTRICTED;                                              0333 00677000
*   RSAVEA=RET;                     /* SAVE RETURN ADDRESS           */ 00678000
         ST    RET,RSAVEA(,RCORE)                                  0334 00679000
*   RESPECIFY                                                      0335 00680000
*     RET UNRESTRICTED;                                            0335 00681000
*   IF FIELDPTR=ZERO|MSGFIELD=' '|  /* TEST FOR ZERO FIELD PTR     0336 00682000
*                                                            @Y30DSMD*/ 00683000
*       MSGFIELD='00'X THEN         /* OR BLANK FIELD OR ZERO      0336 00684000
*                                                            @Y30DSMD*/ 00685000
         LTR   FIELDPTR,FIELDPTR                                   0336 00686000
         BZ    @RT00336                                            0336 00687000
         CLI   MSGFIELD(FIELDPTR),C' '                             0336 00688000
         BE    @RT00336                                            0336 00689000
         CLI   MSGFIELD(FIELDPTR),X'00'                            0336 00690000
         BNE   @RF00336                                            0336 00691000
@RT00336 DS    0H                                                  0337 00692000
*     DO;                           /* FIELD PTR IS ZERO             */ 00693000
*       WTPMSG(PTRB)=COMMA;         /* INDICATE MISSING FIELD        */ 00694000
         LR    @12,RCORE                                           0338 00695000
         ALR   @12,PTRB                                            0338 00696000
         MVI   WTPMSG-1(@12),C','                                  0338 00697000
*       PTRB=PTRB+ONE;              /* INCR PTR TO NEXT FIELD        */ 00698000
         AL    PTRB,@CF00040                                       0339 00699000
*     END;                                                         0340 00700000
*   ELSE                            /* FIELD PTR NOT ZERO    @Y30DSMD*/ 00701000
*     DO;                                                          0341 00702000
         B     @RC00336                                            0341 00703000
@RF00336 DS    0H                                                  0342 00704000
*       DO I=1 TO FLDLNGTH          /* GET LENGTH OF FIELD           */ 00705000
*             WHILE(MSGFIELD(I)^=' ');                             0342 00706000
         LA    I,1                                                 0342 00707000
         B     @DE00342                                            0342 00708000
@DL00342 LR    @12,FIELDPTR                                        0342 00709000
         ALR   @12,I                                               0342 00710000
         BCTR  @12,0                                               0342 00711000
         CLI   MSGFIELD(@12),C' '                                  0342 00712000
         BE    @DC00342                                            0342 00713000
*       END;                                                       0343 00714000
         AL    I,@CF00040                                          0343 00715000
@DE00342 CR    I,FLDLNGTH                                          0343 00716000
         BNH   @DL00342                                            0343 00717000
@DC00342 DS    0H                                                  0344 00718000
*       I=I-ONE;                    /* DECR ONE FOR CORRECT LENGTH 0344 00719000
*                                      LENGTH                        */ 00720000
         BCTR  I,0                                                 0344 00721000
*       IF I>MAXTEXT-PTRB THEN      /* WILL FIELD FIT                */ 00722000
         LA    @12,68                                              0345 00723000
         SLR   @12,PTRB                                            0345 00724000
         CR    I,@12                                               0345 00725000
         BNH   @RF00345                                            0345 00726000
*         DO;                       /* FIELD WILL NOT FIT            */ 00727000
*           PTRB=PTRB-ONE;          /* REMOVE TRAILING BLANK         */ 00728000
         BCTR  PTRB,0                                              0347 00729000
*           CALL WTPRTN;            /* WRITE MESSAGE                 */ 00730000
         BAL   @14,WTPRTN                                          0348 00731000
*           PTRB=NINE;              /* RESET PTR FOR NEXT LINE     0349 00732000
*                                                            @Y30DSMD*/ 00733000
         LA    PTRB,9                                              0349 00734000
*         END;                                                     0350 00735000
*       WTPMSG(PTRB:PTRB+I-ONE)=MSGFIELD;/* MOVE FIELD TO MSG        */ 00736000
@RF00345 LR    @12,RCORE                                           0351 00737000
         ALR   @12,PTRB                                            0351 00738000
         LR    @15,I                                               0351 00739000
         BCTR  @15,0                                               0351 00740000
         EX    @15,@SM03535                                        0351 00741000
*       PTRB=PTRB+I;                /* INCR PTR TO NEXT FIELD        */ 00742000
         ALR   PTRB,I                                              0352 00743000
*       WTPMSG(PTRB)=COMMA;         /* INSERT FIELD DELIMITER        */ 00744000
         LR    @12,RCORE                                           0353 00745000
         ALR   @12,PTRB                                            0353 00746000
         MVI   WTPMSG-1(@12),C','                                  0353 00747000
*       PTRB=PTRB+ONE;              /* INCR PTR TO NEXT FIELD        */ 00748000
         AL    PTRB,@CF00040                                       0354 00749000
*     END;                                                         0355 00750000
*   RESPECIFY                                                      0356 00751000
*     RET RESTRICTED;                                              0356 00752000
@RC00336 DS    0H                                                  0357 00753000
*   RET=RSAVEA;                     /* RESTORE RETURN ADDRESS        */ 00754000
         L     RET,RSAVEA(,RCORE)                                  0357 00755000
*   RESPECIFY                                                      0358 00756000
*     RET UNRESTRICTED;                                            0358 00757000
*   END LDFIELD;                                                   0359 00758000
@EL00004 DS    0H                                                  0359 00759000
@EF00004 DS    0H                                                  0359 00760000
@ER00004 BR    @14                                                 0359 00761000
*MSGINIT:                                                          0360 00762000
*                                                                  0360 00763000
*/********************************************************************/ 00764000
*/*                                                                  */ 00765000
*/*   FUNCTION=INITIALIZE MESSAGE AREA HEADER                        */ 00766000
*/*                                                                  */ 00767000
*/*   INPUT=MESSAGE AREA HEADER AND SPACE FOR THE HEADER             */ 00768000
*/*                                                                  */ 00769000
*/*   OUTPUT=MESSAGE AREA HEADER INITIALIZED IF IT EXIST             */ 00770000
*/*                                                                  */ 00771000
*/********************************************************************/ 00772000
*                                                                  0360 00773000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0360 00774000
MSGINIT  DS    0H                                                  0361 00775000
*   RESPECIFY                                                      0361 00776000
*     RET RESTRICTED;                                              0361 00777000
*   RSAVEA=RET;                     /* SAVE RETURN ADDRESS   @Y30DSMD*/ 00778000
         ST    RET,RSAVEA(,RCORE)                                  0362 00779000
*   RESPECIFY                                                      0363 00780000
*     RET UNRESTRICTED;                                            0363 00781000
*   MSGFLAG=ZERO;                   /* ZERO THE FLAG BYTE    @Y30DSMD*/ 00782000
         LA    @12,280                                             0364 00783000
