         TITLE 'IDA0192P - VSAM OPEN/CLOSE/EOV PROBLEM DETERMINATION   *00001000
                        '                                               00002000
IDA0192P CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192P  78.165'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
RBPRFXLN EQU 64                                                         00011000
RBPRFLNA EQU 32                                                         00012000
PRBLEN   EQU 136                                                        00013000
SIRBLEN  EQU 200                                                        00014000
TIRBLEN  EQU 136                                                        00015000
IRBLEN   EQU 128                                                        00016000
SVRBLEN  EQU 200                                                        00017000
         STM 0,15,320(R1)                                               00018000
         BALR 3,0                                                       00019000
         USING *,3                                                      00020000
*   RCORE=R1;                       /* LD ADDR OF WORK SPACE         */ 00021000
         LR    RCORE,R1                                            0158 00022000
*   RESPECIFY                                                      0159 00023000
*     R1 UNRESTRICTED;                                             0159 00024000
*   GEN SETS(PDPKEY,FORCORE);                                      0160 00025000
*                                                         /*@Y30DSMD*/  00026000
         LA    R12,DXIOB(,RCORE)        LOAD ADDR OF IDAPDPRM           00027000
         MODESET EXTKEY=ZERO,SAVEKEY=PDPKEY(,R12),WORKREG=11            00028000
*   PDWLIST=PDWLIST&&PDWLIST;       /* ZERO WRITE-TO-PROGRAMMER    0161 00029000
*                                      PARAMETER LIST                */ 00030000
         XC    PDWLIST(79,RCORE),PDWLIST(RCORE)                    0161 00031000
*   PDWERRCD=ZERO;                  /* ZERO RETURN CODE              */ 00032000
         SLR   @12,@12                                             0162 00033000
         ST    @12,PDWERRCD(,RCORE)                                0162 00034000
*   PDWORKSP=PDWORKSP&&PDWORKSP;    /* ZERO WORK SPACE               */ 00035000
*                                                                  0163 00036000
         XC    PDWORKSP(8,RCORE),PDWORKSP(RCORE)                   0163 00037000
*   /*****************************************************************/ 00038000
*   /*                                                               */ 00039000
*   /* DETERMINE CALLER AND LOAD MESSAGE NUMBER INTO WTP BUFFER      */ 00040000
*   /*                                                               */ 00041000
*   /*****************************************************************/ 00042000
*                                                                  0164 00043000
*   IF PDPCALL=PDPOPEN THEN         /* IS CALLER OPEN                */ 00044000
         CLI   PDPCALL+280(RCORE),0                                0164 00045000
         BNE   @RF00164                                            0164 00046000
*     WTPMSG=OPMSGID;               /* LD OPEN MESSAGE ID            */ 00047000
         MVI   WTPMSG+7(RCORE),C' '                                0165 00048000
         MVC   WTPMSG+8(63,RCORE),WTPMSG+7(RCORE)                  0165 00049000
         MVC   WTPMSG(7,RCORE),@CC03427                            0165 00050000
*   ELSE                            /* CALLER NOT OPEN       @Y30DSMD*/ 00051000
*     IF PDPCALL=PDPCLOSE THEN      /* IS CALLER CLOSE               */ 00052000
         B     @RC00164                                            0166 00053000
@RF00164 CLI   PDPCALL+280(RCORE),4                                0166 00054000
         BNE   @RF00166                                            0166 00055000
*       WTPMSG=CLMSGID;             /* LD CLOSE MESSAGE ID           */ 00056000
         MVI   WTPMSG+7(RCORE),C' '                                0167 00057000
         MVC   WTPMSG+8(63,RCORE),WTPMSG+7(RCORE)                  0167 00058000
         MVC   WTPMSG(7,RCORE),@CC03429                            0167 00059000
*     ELSE                          /* CALLER NOT CLOSE      @Y30DSMD*/ 00060000
*       IF PDPCALL=PDPEOV THEN      /* IS CALLER EOV                 */ 00061000
         B     @RC00166                                            0168 00062000
@RF00166 CLI   PDPCALL+280(RCORE),8                                0168 00063000
         BNE   @RF00168                                            0168 00064000
*         WTPMSG=EOVMSGID;          /* LD EOV MESSAGE ID             */ 00065000
         MVI   WTPMSG+7(RCORE),C' '                                0169 00066000
         MVC   WTPMSG+8(63,RCORE),WTPMSG+7(RCORE)                  0169 00067000
         MVC   WTPMSG(7,RCORE),@CC03431                            0169 00068000
*       ELSE                        /* CALLER NOT EOV        @Y30DSMD*/ 00069000
*         IF PDPCALL=PDPTCLOS THEN  /* IS CALLER TCLOSE              */ 00070000
         B     @RC00168                                            0170 00071000
@RF00168 CLI   PDPCALL+280(RCORE),12                               0170 00072000
         BNE   @RF00170                                            0170 00073000
*           WTPMSG=TCMSGID;         /* LD TCLOSE MESSAGE ID          */ 00074000
         MVI   WTPMSG+7(RCORE),C' '                                0171 00075000
         MVC   WTPMSG+8(63,RCORE),WTPMSG+7(RCORE)                  0171 00076000
         MVC   WTPMSG(7,RCORE),@CC03433                            0171 00077000
*         ELSE                      /* CALLER NOT TCLOSE     @Y30DSMD*/ 00078000
*           DO;                                                    0172 00079000
         B     @RC00170                                            0172 00080000
@RF00170 DS    0H                                                  0173 00081000
*             PDWERRCD=INVCALL;     /* SET ERROR CODE FOR INVALID  0173 00082000
*                                      CALLER                        */ 00083000
         MVC   PDWERRCD(4,RCORE),@CF00070                          0173 00084000
*             GO TO TERM;           /* RETURN TO CALLER              */ 00085000
         B     TERM                                                0174 00086000
*           END;                                                   0175 00087000
*                                                                  0175 00088000
*   /*****************************************************************/ 00089000
*   /*                                                               */ 00090000
*   /* BEFORE ENTERING MSG AREA SUPPORT LOGIC, TEST IF MSG AREA      */ 00091000
*   /* SUPRESSION WAS REQUESTED BY TERMINATION.                      */ 00092000
*   /*                                                               */ 00093000
*   /*****************************************************************/ 00094000
*                                                                  0176 00095000
*   IF PDPNOMSG^=ON THEN            /* WAS SUPRRESS REQUESTED?       */ 00096000
@RC00170 DS    0H                                                  0176 00097000
@RC00168 DS    0H                                                  0176 00098000
@RC00166 DS    0H                                                  0176 00099000
@RC00164 LA    @12,280                                             0176 00100000
         ALR   @12,RCORE                                           0176 00101000
         TM    PDPNOMSG(@12),B'00010000'                           0176 00102000
         BO    @RF00176                                            0176 00103000
*     IF ACBMSGAR^=ZERO&ACBMSGLN^=ZERO&/* IF MSG AREA EXIST  @Y30DSMD*/ 00104000
*         PDPCALL^=PDPEOV THEN      /* AND IT IS NOT EOV     @Y30DSMD*/ 00105000
         SLR   @15,@15                                             0177 00106000
         L     @14,PDPACB(,@12)                                    0177 00107000
         C     @15,ACBMSGAR(,@14)                                  0177 00108000
         BE    @RF00177                                            0177 00109000
         CH    @15,ACBMSGLN(,@14)                                  0177 00110000
         BE    @RF00177                                            0177 00111000
         CLI   PDPCALL(@12),8                                      0177 00112000
         BE    @RF00177                                            0177 00113000
*       DO;                                                        0178 00114000
*         IF PDPNOTFC=OFF THEN      /* 1ST CALL TO IDA0192P  @Y30DSMD*/ 00115000
         TM    PDPNOTFC(@12),B'10000000'                           0179 00116000
         BNZ   @RF00179                                            0179 00117000
*           CALL MSGINIT;           /* INITIALIZE MSG HEADER @Y30DSMD*/ 00118000
         BAL   @14,MSGINIT                                         0180 00119000
*         ELSE                      /* IF NOT FIRST CALL     @Y30DSMD*/ 00120000
*           CALL MSGSTORE;          /* ATTEMPT TO STORE MSG  @Y30DSMD*/ 00121000
         B     @RC00179                                            0181 00122000
@RF00179 BAL   @14,MSGSTORE                                        0181 00123000
*       END;                                                       0182 00124000
@RC00179 DS    0H                                                  0183 00125000
*   CALL MSGBUILD;                  /* BUILD AND WRITE MESSAGE TO  0183 00126000
*                                      PROGRAMMER                    */ 00127000
*                                                                  0183 00128000
@RF00177 DS    0H                                                  0183 00129000
@RF00176 BAL   @14,MSGBUILD                                        0183 00130000
*   /*****************************************************************/ 00131000
*   /*                                                               */ 00132000
*   /* THIS SECTION OF CODE TESTS THE OPEN/CLOSE/EOV DEBUG BIT IN THE*/ 00133000
*   /* CVT. IF THE DEBUG BIT IS ON, A USER 888 ABEND IS ISSUED AFTER */ 00134000
*   /* THE CALLER'S REGISTERS ARE RESTORED.                          */ 00135000
*   /*                                                               */ 00136000
*   /*****************************************************************/ 00137000
*                                                                  0184 00138000
*   IF ACVTDBUG=ON THEN             /* IF DEBUG BIT ON       @Y30DSMD*/ 00139000
         L     @12,CVTPTR                                          0184 00140000
         TM    ACVTDBUG(@12),B'00001000'                           0184 00141000
         BNO   @RF00184                                            0184 00142000
*     GEN(ABEND 888,DUMP);          /* ABENDS                @Y30DSMD*/ 00143000
*                                                                  0185 00144000
         ABEND 888,DUMP                                                 00145000
*   /*****************************************************************/ 00146000
*   /*                                                               */ 00147000
*   /* WHEN THE VSAM TERMINATION ROUTINE USES THIS MODULE IT REQUESTS*/ 00148000
*   /* SUPRESSION OF THE GTF TRACE. CHECK FOR THE THE SUPRESS        */ 00149000
*   /* REQUEST.                                              @Z30SSMD*/ 00150000
*   /*                                                               */ 00151000
*   /*****************************************************************/ 00152000
*                                                                  0186 00153000
*   IF PDPNOTRC^=ON THEN            /* IF SUPRESS NOT REQUESTED THEN    00154000
*                                      TRACE.                        */ 00155000
@RF00184 TM    PDPNOTRC+280(RCORE),B'00100000'                     0186 00156000
         BO    @RF00186                                            0186 00157000
*     CALL TRACERTN;                /* TRACE DATA                    */ 00158000
         BAL   @14,TRACERTN                                        0187 00159000
*TERM:                              /* TERMINATION PROCESS   @Y30DSMD*/ 00160000
*   RESPECIFY                                                      0188 00161000
*     RETCODE RESTRICTED;                                          0188 00162000
@RF00186 DS    0H                                                  0188 00163000
TERM     DS    0H                                                  0189 00164000
*   RETCODE=PDWERRCD;               /* SET RETURN CODE               */ 00165000
         L     RETCODE,PDWERRCD(,RCORE)                            0189 00166000
*   GEN REFS(PDPKEY,RCORE);                                        0190 00167000
*                                                          /*@Y30DSMD*/ 00168000
         LA    R12,DXIOB(,RCORE)        LOAD ADDR OF IDAPDPRM           00169000
         MODESET KEYADDR=PDPKEY(,R12),WORKREG=11                        00170000
*   GEN(LM 0,14,320(RCORE));        /* RESTORE REGISTERS     @Y30DSMD*/ 00171000
         LM 0,14,320(RCORE)                                             00172000
*   GEN(BR 14);                     /* RETURN TO CALLER      @Y30DSMD*/ 00173000
         BR 14                                                          00174000
*   RESPECIFY                                                      0193 00175000
*     RETCODE UNRESTRICTED;                                        0193 00176000
*MSGBUILD:                                                         0194 00177000
*                                                                  0194 00178000
*/********************************************************************/ 00179000
*/*                                                                  */ 00180000
*/*   FUNCTION=FORMATS THE WRITE TO PROGRAMMER MESSAGE AND CALLS     */ 00181000
*/*            WTPRTN TO ISSUE WTP MESSAGE                           */ 00182000
*/*                                                                  */ 00183000
*/*   INPUT=WTP BUFFER WITH MESSAGE ID INITIALIZED (PDWLIST)         */ 00184000
*/*                                                                  */ 00185000
*/*   OUTPUT=WTP MESSAGE                                             */ 00186000
*/*                                                                  */ 00187000
*/********************************************************************/ 00188000
*                                                                  0194 00189000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0194 00190000
MSGBUILD DS    0H                                                  0195 00191000
*   RESPECIFY                                                      0195 00192000
*    (R12,                                                         0195 00193000
*     RET) RESTRICTED;                                             0195 00194000
*   RSAVEC=RET;                     /* SAVE RETURN ADDRESS           */ 00195000
         ST    RET,RSAVEC(,RCORE)                                  0196 00196000
*   RESPECIFY                                                      0197 00197000
*     RET UNRESTRICTED;                                            0197 00198000
*   PTRB=NINE;                      /* INIT PTR TO 1ST FIELD @Y30DSMD*/ 00199000
         LA    PTRB,9                                              0198 00200000
*/*    LOAD ERROR CODE                                               */ 00201000
*                                                                  0199 00202000
*   R12=PDPCODE;                    /* LOAD MESSAGE CODE             */ 00203000
         SLR   R12,R12                                             0199 00204000
         IC    R12,PDPCODE+280(,RCORE)                             0199 00205000
*   GEN REFS(R12,PDWORKSP);                                        0200 00206000
         CVD   R12,PDWORKSP(,RCORE)     CONVERT TO DECIMAL              00207000
*   UNPK(WTPMSG(PTRB:PTRB+TWO),PDWORKSP(7:8));/* CONVERT CODE TO   0201 00208000
*                                      ZONED DECIMAL AND LOAD INTO 0201 00209000
*                                      MSG                   @Y30DSMD*/ 00210000
         LR    @01,RCORE                                           0201 00211000
         ALR   @01,PTRB                                            0201 00212000
         UNPK  WTPMSG-1(3,@01),PDWORKSP+6(2,RCORE)                 0201 00213000
*   WTPMSG(PTRB+TWO)=WTPMSG(PTRB+TWO)|ZEROCHAR;/* TURN OFF SIGN ZONE    00214000
*                                                            @Y30DSMD*/ 00215000
         LR    @01,RCORE                                           0202 00216000
         ALR   @01,PTRB                                            0202 00217000
         OI    WTPMSG+1(@01),C'0'                                  0202 00218000
*   PTRB=PTRB+THREE;                /* INCR MSG PTR TO NEXT FIELD  0203 00219000
*                                                            @Y30DSMD*/ 00220000
         AL    PTRB,@CF00203                                       0203 00221000
*/*  PLACE THE SUB-FUNCTION CODE, IF PRESENT, IN THE MESSAGE.@Y30DSMD*/ 00222000
*/*  IF NOT PRESENT THEN SKIP TO PROCESS THE NEXT MESSAGE    @Y30DSMD*/ 00223000
*/*  FIELD, THE FUNCTION CODE.                               @Y30DSMD*/ 00224000
*                                                                  0204 00225000
*   IF PDPSFC^=ZERO THEN            /* WAS SUB-FUNCTION PASSED     0204 00226000
*                                                            @Y30DSMD*/ 00227000
         LA    @15,280                                             0204 00228000
         ALR   @15,RCORE                                           0204 00229000
         CLI   PDPSFC(@15),0                                       0204 00230000
         BE    @RF00204                                            0204 00231000
*     DO;                           /* IF SO THEN PROCESS IT.      0205 00232000
*                                                            @Y30DSMD*/ 00233000
*       WTPMSG(PTRB)=LPAREN;        /* INSERT LEFT PAREN.    @Y30DSMD*/ 00234000
         LR    @01,RCORE                                           0206 00235000
         ALR   @01,PTRB                                            0206 00236000
         MVI   WTPMSG-1(@01),C'('                                  0206 00237000
*       PTRB=PTRB+ONE;              /* BUMP TO NEXT POSITION.      0207 00238000
*                                                            @Y30DSMD*/ 00239000
         AL    PTRB,@CF00040                                       0207 00240000
*       IF(PDPCODE=MSS5A&PDPFUNC^=24)|/* MSS CODE 90 BUT NOT MSS/JES3   00241000
*                                                            @ZA33246*/ 00242000
*           PDPCODE=MSS5B|          /* IF MSS CODE 91        @ZA05918*/ 00243000
*           PDPCODE=MSS5C|          /* IF MSS CODE 92        @ZA05918*/ 00244000
*           PDPCODE=MSS5D|          /* IF MSS CODE 93        @ZA05918*/ 00245000
*           PDPCODE=MSS5E THEN      /* IF MSS CODE 94        @ZA05918*/ 00246000
         CLI   PDPCODE(@15),90                                     0208 00247000
         BNE   @GL00006                                            0208 00248000
         CLI   PDPFUNC(@15),24                                     0208 00249000
         BNE   @RT00208                                            0208 00250000
@GL00006 LA    @15,280                                             0208 00251000
         ALR   @15,RCORE                                           0208 00252000
         CLI   PDPCODE(@15),91                                     0208 00253000
         BE    @RT00208                                            0208 00254000
         CLI   PDPCODE(@15),92                                     0208 00255000
         BE    @RT00208                                            0208 00256000
         CLI   PDPCODE(@15),93                                     0208 00257000
         BE    @RT00208                                            0208 00258000
         CLI   PDPCODE(@15),94                                     0208 00259000
         BNE   @RF00208                                            0208 00260000
@RT00208 DS    0H                                                  0209 00261000
*         DO;                       /*                       @ZA05918*/ 00262000
*           UNPK(WTPMSG(PTRB+2:PTRB+4),SFCP);/* UNPACK HEX   @ZA05918*/ 00263000
         LR    @01,RCORE                                           0210 00264000
         ALR   @01,PTRB                                            0210 00265000
         UNPK  WTPMSG+1(3,@01),SFCP+280(2,RCORE)                   0210 00266000
*           WTPMSG(PTRB:PTRB+4)=WTPMSG(PTRB:PTRB+4)&MSSMASK1;/*    0211 00267000
*                                                            @ZA05918*/ 00268000
         LR    @01,RCORE                                           0211 00269000
         ALR   @01,PTRB                                            0211 00270000
         NC    WTPMSG-1(5,@01),MSSMASK1                            0211 00271000
*           TR(WTPMSG(PTRB+2:PTRB+3),TRTAB);/*               @ZA05918*/ 00272000
         LR    @01,RCORE                                           0212 00273000
         ALR   @01,PTRB                                            0212 00274000
         TR    WTPMSG+1(2,@01),TRTAB                               0212 00275000
*           WTPMSG(PTRB:PTRB+4)=WTPMSG(PTRB:PTRB+4)|MSSMASK2;/*    0213 00276000
*                                                            @ZA05918*/ 00277000
         LR    @01,RCORE                                           0213 00278000
         ALR   @01,PTRB                                            0213 00279000
         OC    WTPMSG-1(5,@01),MSSMASK2                            0213 00280000
*           PTRB=PTRB+FIVE;         /* POINT BEYOND          @ZA05918*/ 00281000
         AL    PTRB,@CF00059                                       0214 00282000
*         END;                      /*                       @ZA05918*/ 00283000
*       ELSE                        /* IF NOT MSS CODE       @ZA05918*/ 00284000
*         DO;                       /*                       @ZA05918*/ 00285000
         B     @RC00208                                            0216 00286000
@RF00208 DS    0H                                                  0217 00287000
*           R12=PDPSFC;             /* PICK-UP PASSED SUB-FUNCTION 0217 00288000
*                                      CODE.                 #Y30DSMD*/ 00289000
         SLR   R12,R12                                             0217 00290000
         IC    R12,PDPSFC+280(,RCORE)                              0217 00291000
*           GEN REFS(R12,PDWORKSP);                                0218 00292000
          CVD   R12,PDWORKSP(,RCORE)     CONVERT SUB-FUNCTION TO        00293000
*                                        PACKED DECIMAL                 00294000
*           UNPK(WTPMSG(PTRB:PTRB+TWO),PDWORKSP(7:8));/* CHANGE TO 0219 00295000
*                                      SIGNED DECIMAL.       #Y30DSMD*/ 00296000
         LR    @01,RCORE                                           0219 00297000
         ALR   @01,PTRB                                            0219 00298000
         UNPK  WTPMSG-1(3,@01),PDWORKSP+6(2,RCORE)                 0219 00299000
*           WTPMSG(PTRB+TWO)=WTPMSG(PTRB+TWO)|ZEROCHAR;/* FORCE A  0220 00300000
*                                      ZONE                  #Y30DSMD*/ 00301000
         LR    @01,RCORE                                           0220 00302000
         ALR   @01,PTRB                                            0220 00303000
         OI    WTPMSG+1(@01),C'0'                                  0220 00304000
*           PTRB=PTRB+THREE;        /* BUMP TO NEXT POSITION.      0221 00305000
*                                                            #Y30DSMD*/ 00306000
         AL    PTRB,@CF00203                                       0221 00307000
*         END;                      /*                       @ZA05918*/ 00308000
*       WTPMSG(PTRB)=RPAREN;        /* MOVE IN A RIGHT PAREN.      0223 00309000
*                                                            #Y30DSMD*/ 00310000
@RC00208 LR    @01,RCORE                                           0223 00311000
         ALR   @01,PTRB                                            0223 00312000
         MVI   WTPMSG-1(@01),C')'                                  0223 00313000
*       PTRB=PTRB+ONE;              /* BUMP TO NEXT POSITION.      0224 00314000
*                                                            @Y30DSMD*/ 00315000
         AL    PTRB,@CF00040                                       0224 00316000
*     END;                          /* END OF SUB-FUNCTION         0225 00317000
*                                      PROCESSING.           @Y30DSMD*/ 00318000
*   ELSE                            /* ELSE, SUB-FUNCTION NOT      0226 00319000
*                                      PRESENT.              @Y30DSMD*/ 00320000
*     IF PDPSFI^=ZERO THEN          /* CATALOG SFI EXIST     @Y30DSMD*/ 00321000
         B     @RC00204                                            0226 00322000
@RF00204 L     @15,PDPSFI+280(,RCORE)                              0226 00323000
         LTR   @15,@15                                             0226 00324000
         BZ    @RF00226                                            0226 00325000
*       CALL SFIMSG;                /* WRITE SFI MESSAGE     @Y30DSMD*/ 00326000
         BAL   @14,SFIMSG                                          0227 00327000
*/*    LOAD FUNCTION CODE IDENTIFYING FUNCTION CALLING PROBLEM     0228 00328000
*      DETERMINATION                                                 */ 00329000
*                                                                  0228 00330000
*   IF PDPFUNC=ZERO THEN            /* TEST FOR FUNCTION CODE      0228 00331000
*                                                            @Y30DSMD*/ 00332000
@RF00226 DS    0H                                                  0228 00333000
@RC00204 CLI   PDPFUNC+280(RCORE),0                                0228 00334000
         BNE   @RF00228                                            0228 00335000
*     DO;                           /* FUNCTION CODE NOT SPECIFIED   */ 00336000
*       WTPMSG(PTRB)=COMMA;         /* INSERT COMMA                  */ 00337000
         LR    @01,RCORE                                           0230 00338000
         ALR   @01,PTRB                                            0230 00339000
         MVI   WTPMSG-1(@01),C','                                  0230 00340000
*       PTRB=PTRB+ONE;              /* INCR TO NEXT FIELD            */ 00341000
         AL    PTRB,@CF00040                                       0231 00342000
*     END;                                                         0232 00343000
*   ELSE                            /* FUNCTION CODE SPECIFIED     0233 00344000
*                                                            @Y30DSMD*/ 00345000
*     DO;                                                          0233 00346000
         B     @RC00228                                            0233 00347000
@RF00228 DS    0H                                                  0234 00348000
*       WTPMSG(PTRB)=DASH;          /* ADD FIELD SEPARATOR           */ 00349000
         LR    @01,RCORE                                           0234 00350000
         ALR   @01,PTRB                                            0234 00351000
         MVI   WTPMSG-1(@01),C'-'                                  0234 00352000
*       PTRB=PTRB+ONE;              /* INCR PTR TO FUNC CODE         */ 00353000
         AL    PTRB,@CF00040                                       0235 00354000
*       R12=PDPFUNC;                /* LOAD FUNCTION CODE            */ 00355000
         SLR   R12,R12                                             0236 00356000
         IC    R12,PDPFUNC+280(,RCORE)                             0236 00357000
*       GEN REFS(R12,PDWORKSP);                                    0237 00358000
         CVD   R12,PDWORKSP(,RCORE)     CONVERT TO DECIMAL              00359000
*       UNPK(WTPMSG(PTRB:PTRB+TWO),PDWORKSP(7:8));/* CONVERT FUNCTION   00360000
*                                      CODE TO ZONED DECIMAL AND LOAD   00361000
*                                      INTO MSG              @Y30DSMD*/ 00362000
         LR    @01,RCORE                                           0238 00363000
         ALR   @01,PTRB                                            0238 00364000
         UNPK  WTPMSG-1(3,@01),PDWORKSP+6(2,RCORE)                 0238 00365000
*       WTPMSG(PTRB+TWO)=WTPMSG(PTRB+TWO)|ZEROCHAR;/* SET SIGN ZONE  */ 00366000
         LR    @01,RCORE                                           0239 00367000
         ALR   @01,PTRB                                            0239 00368000
         OI    WTPMSG+1(@01),C'0'                                  0239 00369000
*       PTRB=PTRB+THREE;            /* INCR MSG PTR PAST FIELD     0240 00370000
*                                                            @Y30DSMD*/ 00371000
         AL    PTRB,@CF00203                                       0240 00372000
*       WTPMSG(PTRB)=COMMA;         /* INSERT FIELD DELIMITER        */ 00373000
         LR    @01,RCORE                                           0241 00374000
         ALR   @01,PTRB                                            0241 00375000
         MVI   WTPMSG-1(@01),C','                                  0241 00376000
*       PTRB=PTRB+ONE;              /* INCR PTR TO NEXT FIELD        */ 00377000
         AL    PTRB,@CF00040                                       0242 00378000
*     END;                                                         0243 00379000
*   RESPECIFY                                                      0244 00380000
*    (R12) UNRESTRICTED;            /* FREE UP REG           @Y30LSPS*/ 00381000
@RC00228 DS    0H                                                  0245 00382000
*/*    PUT JOB NAME IN TIOT IN MESSAGE                               */ 00383000
*                                                                  0245 00384000
*   FIELDPTR=ADDR(TIOCNJOB);        /* LOAD ADDR OF JOB NAME         */ 00385000
         L     @12,CVTPTR                                          0245 00386000
         L     @12,CVTTCBP(,@12)                                   0245 00387000
         L     @12,TCBPTR(,@12)                                    0245 00388000
         L     FIELDPTR,TCBTIO(,@12)                               0245 00389000
*   FLDLNGTH=LENGTH(TIOCNJOB);      /* LOAD MAX LENGTH OF FIELD      */ 00390000
         LA    FLDLNGTH,8                                          0246 00391000
*   CALL LDFIELD;                   /* MOVE JOB NAME TO MSG          */ 00392000
         BAL   @14,LDFIELD                                         0247 00393000
*/*    PUT STEP NAME IN TIOT IN MESSAGE                              */ 00394000
*                                                                  0248 00395000
*   FIELDPTR=ADDR(TIOCSTEP);        /* LOAD ADDR OF STEP NAME        */ 00396000
         L     @12,CVTPTR                                          0248 00397000
         L     @12,CVTTCBP(,@12)                                   0248 00398000
         L     @12,TCBPTR(,@12)                                    0248 00399000
         L     FIELDPTR,TCBTIO(,@12)                               0248 00400000
         LA    FIELDPTR,TIOCSTEP(,FIELDPTR)                        0248 00401000
*   FLDLNGTH=LENGTH(TIOCSTEP);      /* LOAD MAX LENGTH OF FIELD      */ 00402000
         LA    FLDLNGTH,16                                         0249 00403000
*   CALL LDFIELD;                   /* MOVE STEP NAME TO MSG         */ 00404000
         BAL   @14,LDFIELD                                         0250 00405000
*/*    PUT DD NAME IN TIOT ENTRY FOR THIS DATA SET IN MESSAGE        */ 00406000
*                                                                  0251 00407000
*   IF PDPTIOT^=ZERO THEN           /* PTR TO TIOT ENTRY     @Y30DSMD*/ 00408000
         L     @12,PDPTIOT+280(,RCORE)                             0251 00409000
         LTR   @12,@12                                             0251 00410000
         BZ    @RF00251                                            0251 00411000
*     DO;                           /* PTR IN PARM LIST              */ 00412000
*       FIELDPTR=ADDR(TIOEDDNM);    /* LOAD ADDRESS OF DD NAME       */ 00413000
         LA    FIELDPTR,TIOEDDNM(,@12)                             0253 00414000
*       FLDLNGTH=LENGTH(TIOEDDNM);  /* LOAD LENGTH OF DD NAME        */ 00415000
         LA    FLDLNGTH,8                                          0254 00416000
*     END;                                                         0255 00417000
