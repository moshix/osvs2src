*/*                                                                  */ 01039000
*/********************************************************************/ 01040000
*                                                                  0425 01041000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0425 01042000
*                                                                  0425 01043000
TRACERTN DS    0H                                                  0426 01044000
*/*    WORK AREA FOR TRACE ROUTINE                                   */ 01045000
*                                                                  0426 01046000
*   DCL                                                            0426 01047000
*     1 TRCWORK,                                                   0426 01048000
*      2 TRCLIST BDY(WORD),         /* GTRACE MACRO PARAMETER LIST   */ 01049000
*       3 TRCLNG PTR(15),           /* LENGTH OF TRACE DATA          */ 01050000
*       3 TRCFID BIT(16),           /* FORMAT APPENDAGE ID           */ 01051000
*       3 TRCADDR PTR(31),          /* ADDR OF TRACE DATA            */ 01052000
*      2 TRCBUFR,                   /* DATA TO BE TRACED             */ 01053000
*       3 TRCDID CHAR(1),           /* ID OF DATA TO BE TRACED       */ 01054000
*       3 TRCDATA CHAR(255);        /* DATA TO BE TRACED             */ 01055000
*/*   PARAMETER LIST FOR FREEMAIN                                    */ 01056000
*                                                                  0427 01057000
*   DCL                                                            0427 01058000
*     1 MAINR0 FIXED(31),           /* PARAMETER LIST FOR FREEMAIN   */ 01059000
*      2 MAINPOOL FIXED(8),         /* SUBPOOL NUMBER                */ 01060000
*      2 MAINLEN FIXED(24);         /* LENGTH OF DATA AREA           */ 01061000
*   DCL                                                            0428 01062000
*     PDWFLAGS FIXED(8);            /* INDICATOR FLAGS               */ 01063000
*   DCL                                                            0429 01064000
*     AMBMASK(*) PTR(31) DEF(AMBLDTA);/* MASK USED TO REF AMB PTRS   */ 01065000
*   DCL                                                            0430 01066000
*     NOTRACE FIXED(8) CONSTANT(12);/* CODE FOR DATA NOT TRACED      */ 01067000
*   RESPECIFY                                                      0431 01068000
*    (NUMENTRY,                                                    0431 01069000
*     FIELDPTR,                                                    0431 01070000
*     FLDLNGTH) UNRESTRICTED;                                      0431 01071000
*   RESPECIFY                                                      0432 01072000
*     RET RESTRICTED;                                              0432 01073000
*   RSAVEA=RET;                     /* SAVE RETURN ADDRESS           */ 01074000
         ST    RET,RSAVEA(,RCORE)                                  0433 01075000
*/********************************************************************/ 01076000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 01077000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 01078000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 01079000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 01080000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 01081000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 01082000
*/********************************************************************/ 01083000
*                                                                  0434 01084000
*   GEN;                                                           0434 01085000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       01086000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   01087000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      01088000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     01089000
*                                       IF NO ERROR WAS ENCOUNTERED     01090000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    01091000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 01092000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           01093000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     01094000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  01095000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    01096000
         B     GMN00200                 BYPASS SETTING ERROR            01097000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 01098000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    01099000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      01100000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          01101000
*   RESPECIFY                                                      0435 01102000
*     RETCODE RESTRICTED;                                          0435 01103000
*   IF RETCODE^=ZERO THEN           /* GETMAIN SUCCESSFUL    @Y30DSMD*/ 01104000
         LTR   RETCODE,RETCODE                                     0436 01105000
         BZ    @RF00436                                            0436 01106000
*     DO;                                                          0437 01107000
*       RESPECIFY                                                  0438 01108000
*         R13 RESTRICTED;                                          0438 01109000
*       R13=R15;                    /* LOAD ADDRESS OF WORK AREA     */ 01110000
         LR    R13,R15                                             0439 01111000
*       GEN(USING @DATD,R13);       /* ESTABLISH ADDRESSABILITY TO 0440 01112000
*                                      WORK AREA                     */ 01113000
         USING @DATD,R13                                                01114000
*     END;                                                         0441 01115000
*   ELSE                            /* GETMAIN FAILED        @Y30DSMD*/ 01116000
*     DO;                                                          0442 01117000
         B     @RC00436                                            0442 01118000
@RF00436 DS    0H                                                  0443 01119000
*       RET=RSAVEA;                 /* RESTORE RETURN ADDRESS        */ 01120000
         L     RET,RSAVEA(,RCORE)                                  0443 01121000
*       RETURN;                     /* RETURN TO CALLER              */ 01122000
@EL00008 DS    0H                                                  0444 01123000
@EF00008 DS    0H                                                  0444 01124000
@ER00008 BR    @14                                                 0444 01125000
*     END;                                                         0445 01126000
*   RESPECIFY                                                      0446 01127000
*    (RET,                                                         0446 01128000
*     RETCODE) UNRESTRICTED;                                       0446 01129000
*                                                                  0446 01130000
@RC00436 DS    0H                                                  0447 01131000
*/*    INITIALIZE GTRACE MACRO PARAMETER LIST                        */ 01132000
*                                                                  0447 01133000
*   TRCFID=VSAMFID;                 /* INIT FORMAT ID FOR VSAM FORMAT   01134000
*                                      APPENDAGE                     */ 01135000
         MVC   TRCFID(2),@CB03380                                  0447 01136000
*   TRCADDR=ADDR(TRCBUFR);          /* INIT ADDR OF TRACE DATA       */ 01137000
         LA    @06,TRCBUFR                                         0448 01138000
         ST    @06,TRCADDR                                         0448 01139000
*   PDWFLAGS=ZERO;                  /* ZERO INDICATOR FLAGS  @Y30DSMD*/ 01140000
         SLR   PDWFLAGS,PDWFLAGS                                   0449 01141000
*/*    TRACE JOB NAME STEP NAME AND TIOT ENTRY FOR DATA SET BEING  0450 01142000
*      PROCESSED                                                     */ 01143000
*                                                                  0450 01144000
*   CALL TRCJOBNM;                  /* TRACE JOB NAME        @Y30DSMD*/ 01145000
         BAL   @14,TRCJOBNM                                        0450 01146000
*   IF PDPTIOT^=ZERO&PDWFLAGS=ZERO THEN/* IF TIOT EXIST      @Y30DSMD*/ 01147000
         SLR   @06,@06                                             0451 01148000
         C     @06,PDPTIOT+280(,RCORE)                             0451 01149000
