*   GEN(SMFWTM (1));                /* WRITE THE SMF RECORD          */ 00741000
         SMFWTM (1)                                                     00742000
*   R0=RCDLEN;                      /* LOAD RECORD LENGTH            */ 00743000
         L     R0,RCDLEN                                           0369 00744000
*   R1=RCDPTR;                      /* COPY PTR TO SMF RECORD        */ 00745000
         L     R1,RCDPTR                                           0370 00746000
*   IF ACVTDBUG=OFF THEN            /* IF OK TO ISSUE FREEMAINS      */ 00747000
         L     @11,CVTPTR                                          0371 00748000
         TM    ACVTDBUG(@11),B'00000010'                           0371 00749000
         BNZ   @RF00371                                            0371 00750000
*     GEN(FREEMAIN R,LV=(0),A=(1)); /* FREE CORE FOR RECORD          */ 00751000
         FREEMAIN R,LV=(0),A=(1)                                        00752000
*   END WRITERCD;                                                  0373 00753000
@EL00007 DS    0H                                                  0373 00754000
@EF00007 DS    0H                                                  0373 00755000
@ER00007 LM    @14,@12,@SA00007                                    0373 00756000
         BR    @14                                                 0373 00757000
*   RESPECIFY                                                      0374 00758000
*    (R0,                                                          0374 00759000
*     R1) RESTRICTED;                                              0374 00760000
*GETSENSE:                                                         0375 00761000
*                                                                  0375 00762000
*/********************************************************************/ 00763000
*/*                                                                  */ 00764000
*/*   01 FUNCTION - OBTAINS SENSE INFORMATION FROM DEVICE IN ORDER   */ 00765000
*/*                 TO DETERMINE WHICH DRIVE VOLUME IS MOUNTED ON.   */ 00766000
*/*   01 INPUT  -   1. ADDRESS OF UCB IN RUCB                        */ 00767000
*/*   01 OUTPUT -   2. CHARACTER THAT INDICATES WHICH DRIVE VOLUME   */ 00768000
*/*                    IS MOUNTED ON                                 */ 00769000
*/*                                                                  */ 00770000
*/********************************************************************/ 00771000
*                                                                  0375 00772000
*   PROC;                                                          0375 00773000
GETSENSE STM   @14,@12,@SA00008                                    0375 00774000
*   IF UCBVTOC^=0 THEN              /* UCB AVAILABLE?        @ZA14574*/ 00775000
         L     @11,UCBVTOC(,RUCB)                                  0376 00776000
         LTR   @11,@11                                             0376 00777000
         BZ    @RF00376                                            0376 00778000
*     DO;                           /* YES-                  @ZA14574*/ 00779000
*       IF CCWBLT=OFF THEN          /* CONTROL BLOCKS & CCW NOT BUILT*/ 00780000
         TM    CCWBLT,B'10000000'                                  0378 00781000
         BNZ   @RF00378                                            0378 00782000
*         DO;                                                      0379 00783000
*           DXCB=DXCB&&DXCB;        /* ZERO CONTROL BLOCK WORK AREA  */ 00784000
*                                                                  0380 00785000
         XC    DXCB(200),DXCB                                      0380 00786000
*           /*********************************************************/ 00787000
*           /*                                                       */ 00788000
*           /* SET UP CONTROL BLOCK BASES                            */ 00789000
*           /*                                                       */ 00790000
*           /*********************************************************/ 00791000
*                                                                  0381 00792000
*           IOBSTPTR=ADDR(DXCB);    /* IOB IS FIRST IN CB            */ 00793000
         LA    @11,DXCB                                            0381 00794000
         ST    @11,IOBSTPTR                                        0381 00795000
*           RESPECIFY                                              0382 00796000
*             RDEB RESTRICTED;                                     0382 00797000
*           RDEB=IOBSTPTR+LENGTH(IOBSTDRD);/* DEB IS SECOND          */ 00798000
         L     @15,IOBSTPTR                                        0383 00799000
         LA    @14,32                                              0383 00800000
         LR    RDEB,@15                                            0383 00801000
         ALR   RDEB,@14                                            0383 00802000
*           DCBPTR=RDEB+LENGTH(DEBBASIC);/* DCB IS THIRD             */ 00803000
*                                                                  0384 00804000
         ALR   @14,RDEB                                            0384 00805000
         ST    @14,DCBPTR                                          0384 00806000
*           /*********************************************************/ 00807000
*           /*                                                       */ 00808000
*           /* SET UP DCB                                            */ 00809000
*           /*                                                       */ 00810000
*           /*********************************************************/ 00811000
*                                                                  0385 00812000
*           DCBDEBAD=ADDR(DEBBASIC);/* DCB POINTS TO DEB             */ 00813000
*                                                                  0385 00814000
         STCM  RDEB,15,DCBDEBAD(@14)                               0385 00815000
*           /*********************************************************/ 00816000
*           /*                                                       */ 00817000
*           /* SET UP DEB                                            */ 00818000
*           /*                                                       */ 00819000
*           /*********************************************************/ 00820000
*                                                                  0386 00821000
*           DEBAPPAD=CVTXAPG;       /* SET IOS APPNG VECTOR TBL ADDR    00822000
*                                                            @Y021130*/ 00823000
         L     @15,CVTPTR                                          0386 00824000
         MVC   DEBAPPAD(4,RDEB),CVTXAPG(@15)                       0386 00825000
*           DEBEXSCL='04'X;         /* SET EXTENT SCALE      @Y021130*/ 00826000
         MVI   DEBEXSCL(RDEB),X'04'                                0387 00827000
*           DEBDCBB=ADDR(IHADCB);   /* DEB POINTS TO DCB     @Y021130*/ 00828000
         STCM  @14,7,DEBDCBB(RDEB)                                 0388 00829000
*           DEBDEBID=IDXDEB;        /* SET DEB ID            @Y021130*/ 00830000
         MVI   DEBDEBID(RDEB),B'00001111'                          0389 00831000
*           DEBNMEXT=1;             /* ONLY ONE EXTENT       @Y021130*/ 00832000
         MVI   DEBNMEXT(RDEB),X'01'                                0390 00833000
*           DEBDAPTR=ADDR(DEBBASND);/* POINT TO FIRST EXTENT @Y021130*/ 00834000
         LA    @15,DEBBASND(,RDEB)                                 0391 00835000
         ST    @15,DEBDAPTR                                        0391 00836000
*           RESPECIFY                                              0392 00837000
*             DEBDASD BASED(DEBDAPTR);/*                     @Y021130*/ 00838000
*           DEBSTRCC(1:2)='0000'X;  /* ZERO STARTING CYLINDER      0393 00839000
*                                                            @Y021130*/ 00840000
         MVC   DEBSTRCC(2,@15),@CB03642                            0393 00841000
*           DEBSTRHH(1:2)='0000'X;  /* ZERO STARTING HEAD    @Y021130*/ 00842000
         MVC   DEBSTRHH(2,@15),@CB03642                            0394 00843000
*           DEBENDHH(1:2)='0000'X;  /* ZERO ENDING HEAD      @Y021130*/ 00844000
         MVC   DEBENDHH(2,@15),@CB03642                            0395 00845000
*           DEBENDCC(1:2)='7FFF'X;  /* SET END CYLINDER HIGH @Y021130*/ 00846000
         MVC   DEBENDCC(2,@15),@CB03643                            0396 00847000
*           DEBNMTRK(1:2)='7FFF'X;  /* SET NUMBER OF TRACKS HIGH   0397 00848000
*                                                            @Y021130*/ 00849000
         MVC   DEBNMTRK(2,@15),@CB03643                            0397 00850000
*           RESPECIFY                                              0398 00851000
*             RDEB UNRESTRICTED;                                   0398 00852000
*                                                                  0398 00853000
*           /*********************************************************/ 00854000
*           /*                                                       */ 00855000
*           /* SET UP IOB                                            */ 00856000
*           /*                                                       */ 00857000
*           /*********************************************************/ 00858000
*                                                                  0399 00859000
*           IOBUNREL=ON;            /* SET NON-SEQUENTIAL BIT        */ 00860000
         L     @11,IOBSTPTR                                        0399 00861000
         OI    IOBUNREL(@11),B'00000010'                           0399 00862000
*           IOBDCBPT=ADDR(IHADCB);  /* IOB POINTS TO DCB             */ 00863000
         ST    @14,IOBDCBPT(,@11)                                  0400 00864000
*           IOBECBPT=ADDR(DXECB);   /* IOB POINTS TO ECB             */ 00865000
         LA    @15,DXECB                                           0401 00866000
         ST    @15,IOBECBPT(,@11)                                  0401 00867000
*           IOBSTART=ADDR(DXCCW1);  /* IOB POINTS TO FIRST CCW       */ 00868000
         LA    @15,DXCCW1                                          0402 00869000
         ST    @15,IOBSTART(,@11)                                  0402 00870000
*           DXCCW1=DXCCW1&&DXCCW1;  /* ZERO CCW1                     */ 00871000
         XC    DXCCW1(8),DXCCW1                                    0403 00872000
*           DXCCW1(1:4)=ADDR(SENSEDTA);/* POINT CCW TO DATA AREA     */ 00873000
         LA    @11,SENSEDTA                                        0404 00874000
         STCM  @11,15,DXCCW1                                       0404 00875000
*           DXCCW1(1)='04'X;        /* SET COMMAND CODE FOR SENSE    */ 00876000
         MVI   DXCCW1,X'04'                                        0405 00877000
*           DXCCW1(5)='20'X;        /* SET SILI BIT ON               */ 00878000
         MVI   DXCCW1+4,X'20'                                      0406 00879000
*           DXCCW1(8)='05'X;        /* SET LENGTH AT 5               */ 00880000
         MVI   DXCCW1+7,X'05'                                      0407 00881000
*           CCWBLT=ON;              /* INDIC CONTROL BLKS & CCW BUILT*/ 00882000
         OI    CCWBLT,B'10000000'                                  0408 00883000
*         END;                                                     0409 00884000
*       DEBUCBA=RUCB;               /* SET UCB PTR IN EXTENT         */ 00885000
@RF00378 L     @11,DEBDAPTR                                        0410 00886000
         STCM  RUCB,7,DEBUCBA(@11)                                 0410 00887000
*       DXECB=DXECB&&DXECB;         /* ZERO ECB                      */ 00888000
         XC    DXECB(4),DXECB                                      0411 00889000
*       SPINDLE=SPINDLE&&SPINDLE;   /* ZERO SPINDLE CHARACTER        */ 00890000
         SLR   @11,@11                                             0412 00891000
         IC    @11,SPINDLE                                         0412 00892000
         LR    @15,@11                                             0412 00893000
         XR    @15,@11                                             0412 00894000
         STC   @15,SPINDLE                                         0412 00895000
*       RESPECIFY                                                  0413 00896000
*         R1 RESTRICTED;                                           0413 00897000
*       R1=IOBSTPTR;                /* POINT TO IOB                  */ 00898000
         L     R1,IOBSTPTR                                         0414 00899000
*       GEN(EXCP (1));              /* EXECUTE THE I/O               */ 00900000
         EXCP (1)                                                       00901000
*       GEN(WAIT ECB=DXECB);        /* WAIT FOR COMPLETE             */ 00902000
         WAIT ECB=DXECB                                                 00903000
*       RESPECIFY                                                  0417 00904000
*         R1 UNRESTRICTED;                                         0417 00905000
*     END;                          /*                       @ZA14574*/ 00906000
*   END GETSENSE;                                                  0419 00907000
@EL00008 DS    0H                                                  0419 00908000
@EF00008 DS    0H                                                  0419 00909000
@ER00008 LM    @14,@12,@SA00008                                    0419 00910000
         BR    @14                                                 0419 00911000
