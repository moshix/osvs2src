         TITLE 'IDA0192S - MVM WRITE VSAM SMF RECORD                   *00001000
                        '                                               00002000
IDA0192S CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192S  78.144'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@03                                         0001 00013000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00014000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00015000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00016000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00017000
*                                       IF NO ERROR WAS ENCOUNTERED     00018000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00019000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00020000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00021000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00022000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00023000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00024000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00025000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00026000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00027000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00028000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00029000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00030000
         B     GMN00200                 BYPASS SETTING ERROR            00031000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00032000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00033000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00034000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00035000
*   IF RETCODE=0 THEN               /* IF GETMAIN FAILED             */ 00036000
         LTR   RETCODE,RETCODE                                     0167 00037000
         BNZ   @RF00167                                            0167 00038000
*     DO;                                                          0168 00039000
*       RETCODE=GMFAIL;             /* INDICATE ERROR                */ 00040000
         L     RETCODE,GMFAIL                                      0169 00041000
*     END;                                                         0170 00042000
*   ELSE                            /* IF GETMAIN SUCCESSFUL         */ 00043000
*     DO;                                                          0171 00044000
         B     @RC00167                                            0171 00045000
@RF00167 DS    0H                                                  0172 00046000
*       GEN(ST R13,4(R15));         /* SAVE SAVE REG                 */ 00047000
         ST R13,4(R15)                                                  00048000
*       GEN( L R0,20(,R13));        /* IDAGMAIN USES R0, RESTORE IT.    00049000
*                                                            @Z30SSMD*/ 00050000
          L R0,20(,R13)                                                 00051000
*       GEN( L R1,24(,R13));        /* IDAGMAIN USES R1, RESTORE IT.    00052000
*                                                            @Z30SSMD*/ 00053000
          L R1,24(,R13)                                                 00054000
*       R13=RETCODE;                /* INIT DATA REG                 */ 00055000
         LR    R13,RETCODE                                         0175 00056000
*       RESPECIFY                                                  0176 00057000
*         R13 RESTRICTED;                                          0176 00058000
*       GEN(USING @DATD,R13);       /* GET ADDRESSING                */ 00059000
         USING @DATD,R13                                                00060000
*       RESPECIFY                                                  0178 00061000
*         R1 RESTRICTED;                                           0178 00062000
*       PPFUNC=PDF30;               /* SET FUNCTION CODE     @Y30DSMD*/ 00063000
         MVI   PPFUNC,X'1E'                                        0179 00064000
*       RCDPARMA=R1;                /* INIT PTR TO PARMS             */ 00065000
         ST    R1,RCDPARMA                                         0180 00066000
*       RESPECIFY                                                  0181 00067000
*         R1 UNRESTRICTED;                                         0181 00068000
*       IF RCDNBR=OPENRCD THEN      /* IF REQUEST FOR OPEN RCD       */ 00069000
         L     @11,RCDPARMA                                        0182 00070000
         CLI   RCDNBR(@11),62                                      0182 00071000
         BNE   @RF00182                                            0182 00072000
*         CALL START62;             /* THEN START RCD 62             */ 00073000
         BAL   @14,START62                                         0183 00074000
*       ELSE                        /* NOT REQ FOR OPEN RCD  @Z30SSRL*/ 00075000
*         IF RCDNBR=CLOSERCD THEN   /* IF REQUEST FOR CLOSE RCD      */ 00076000
         B     @RC00182                                            0184 00077000
@RF00182 L     @11,RCDPARMA                                        0184 00078000
         CLI   RCDNBR(@11),64                                      0184 00079000
         BNE   @RF00184                                            0184 00080000
*           CALL START64;           /* THEN START RCD 64             */ 00081000
         BAL   @14,START64                                         0185 00082000
*       CALL CREACOM;               /* COMPLETE COMMON PART OF RCD   */ 00083000
@RF00184 DS    0H                                                  0186 00084000
@RC00182 BAL   @14,CREACOM                                         0186 00085000
*       IF RCDNBR=OPENRCD THEN      /* IF REQUEST FOR OPEN RCD       */ 00086000
         L     @11,RCDPARMA                                        0187 00087000
         CLI   RCDNBR(@11),62                                      0187 00088000
         BNE   @RF00187                                            0187 00089000
*         CALL FIN62;               /* THEN FINISH RCD 62            */ 00090000
         BAL   @14,FIN62                                           0188 00091000
*       ELSE                        /* NOT REQ FOR OPEN RCD  @Z30SSRL*/ 00092000
*         IF RCDNBR=CLOSERCD THEN   /* IF REQUEST FOR CLOSE RCD      */ 00093000
         B     @RC00187                                            0189 00094000
@RF00187 L     @11,RCDPARMA                                        0189 00095000
         CLI   RCDNBR(@11),64                                      0189 00096000
         BNE   @RF00189                                            0189 00097000
*           CALL FIN64;             /* THEN FINISH RCD 64            */ 00098000
         BAL   @14,FIN64                                           0190 00099000
*       CALL WRITERCD;              /* WRITE THE RECORD              */ 00100000
@RF00189 DS    0H                                                  0191 00101000
@RC00187 BAL   @14,WRITERCD                                        0191 00102000
*EXIT:                              /* EXIT PROCESSING       @Z30SSRL*/ 00103000
*       IF PPCODE=0 THEN            /* IF NO PROBLEM         @Z30SSRL*/ 00104000
EXIT     CLI   PPCODE,0                                            0192 00105000
         BNE   @RF00192                                            0192 00106000
*         PPFUNC=0;                 /* RESET FUNCTION CODE   @Z30SSRL*/ 00107000
         MVI   PPFUNC,X'00'                                        0193 00108000
*       R6=PD;                      /* LOAD RETURN CODE              */ 00109000
@RF00192 L     R6,PD                                               0194 00110000
*       MAINPOOL=WKPOOL;            /* SET SUBPOOL FOR FREEMAIN      */ 00111000
         MVI   MAINPOOL,X'FC'                                      0195 00112000
*       GEN(L R7,@SIZDATD);         /* LOAD BLOCK SIZE               */ 00113000
         L R7,@SIZDATD                                                  00114000
*       MAINLEN=R7;                 /* SET LENGTH FOR FREEMAIN       */ 00115000
         STCM  R7,7,MAINLEN                                        0197 00116000
*       RESPECIFY                                                  0198 00117000
*        (R0,                                                      0198 00118000
*         R1) RESTRICTED;                                          0198 00119000
*       R1=R13;                     /* POINT TO BLOCK ADDR           */ 00120000
         LR    R1,R13                                              0199 00121000
*       R0=MAINR0;                  /* LOAD SUBPOOL/LENGTH           */ 00122000
         L     R0,MAINR0                                           0200 00123000
*       GEN(L R13,4(R13));          /* POINT TO LAST SAVEAREA        */ 00124000
         L R13,4(R13)                                                   00125000
*       IF ACVTDBUG=OFF THEN        /* IF OK TO ISSUE FREEMAINS      */ 00126000
         L     @11,CVTPTR                                          0202 00127000
         TM    ACVTDBUG(@11),B'00000010'                           0202 00128000
         BNZ   @RF00202                                            0202 00129000
*         GEN(FREEMAIN R,LV=(0),A=(1));/* FREE AUTOMATICS            */ 00130000
         FREEMAIN R,LV=(0),A=(1)                                        00131000
*       RESPECIFY                                                  0204 00132000
*        (R0,                                                      0204 00133000
*         R1) UNRESTRICTED;                                        0204 00134000
@RF00202 DS    0H                                                  0205 00135000
*       RETCODE=R6;                 /* LOAD RETURN CODE              */ 00136000
         LR    RETCODE,R6                                          0205 00137000
*     END;                                                         0206 00138000
*   GEN(L RET,12(R13));             /* LOAD RETURN ADDRESS           */ 00139000
@RC00167 DS    0H                                                  0207 00140000
         L RET,12(R13)                                                  00141000
*   GEN(LM R0,R12,20(R13));         /* RESTORE REGISTERS 0-12        */ 00142000
         LM R0,R12,20(R13)                                              00143000
*   GEN(BR RET);                    /* RETURN TO CALLER              */ 00144000
         BR RET                                                         00145000
*START62:                                                          0210 00146000
*                                                                  0210 00147000
*/********************************************************************/ 00148000
*/*                                                                  */ 00149000
*/*   01 FUNCTION - CALCULATES AMOUNT OF STORAGE NEEDED FOR SMF      */ 00150000
*/*                 RECORD 62.                                       */ 00151000
*/*   01 INPUT  -   1. ADDRESS OF ACB IN RCD62ACB                    */ 00152000
*/*   01 OUTPUT -   1. LENGTH OF SMF RECORD 62 IN RCDLEN             */ 00153000
*/*                                                                  */ 00154000
*/********************************************************************/ 00155000
*                                                                  0210 00156000
*   PROC;                                                          0210 00157000
START62  STM   @14,@12,12(@13)                                     0210 00158000
*   RACB=RCD62ACB;                  /* LOAD POINTER TO ACB           */ 00159000
         L     @11,RCDPARMA                                        0211 00160000
         L     RACB,RCD62ACB-1(,@11)                               0211 00161000
         LA    RACB,0(,RACB)                                       0211 00162000
*   IF RCD62OOK=ON THEN             /* IF OPEN WAS SUCCESSFUL        */ 00163000
         TM    RCD62OOK(@11),B'10000000'                           0212 00164000
         BNO   @RF00212                                            0212 00165000
*     DO;                                                          0213 00166000
*       R7=0;                       /* ZERO BLOCK LENGTH             */ 00167000
         SLR   R7,R7                                               0214 00168000
*       VMTPT=BIBVMT;               /* INIT VMT PTR          @Y30DSMD*/ 00169000
         L     @11,ACBAMBL(,RACB)                                  0215 00170000
         L     @11,AMBLBIB(,@11)                                   0215 00171000
         L     @11,BIBVMT(,@11)                                    0215 00172000
         ST    @11,VMTPT                                           0215 00173000
*       DO WHILE VMTPT^=0;          /* PROCESS ALL VMT'S     @Y30DSMD*/ 00174000
         B     @DE00216                                            0216 00175000
@DL00216 DS    0H                                                  0217 00176000
*         R7=R7+VMTNOVOL;           /* ADD VOLS THIS VMT     @Y30DSMD*/ 00177000
         L     @11,VMTPT                                           0217 00178000
         AH    R7,VMTNOVOL(,@11)                                   0217 00179000
*         VMTPT=VMTNXT;             /* ADDRESS NXT VMT       @Y30DSMD*/ 00180000
         L     @11,VMTNXT(,@11)                                    0218 00181000
         ST    @11,VMTPT                                           0218 00182000
*       END;                        /* END OF VMT PROCESSING LOGIC.     00183000
*                                                            @Y30DSMD*/ 00184000
@DE00216 L     @11,VMTPT                                           0219 00185000
         LTR   @11,@11                                             0219 00186000
         BNZ   @DL00216                                            0219 00187000
*       RCDLEN=LENGTH(SMFRCD62)+R7*LENGTH(SMF62VLT);/* CORE NEEDED 0220 00188000
*                                      FOR RCD                       */ 00189000
         LR    @11,R7                                              0220 00190000
         MH    @11,@CH02447                                        0220 00191000
         AL    @11,@CF03646                                        0220 00192000
         ST    @11,RCDLEN                                          0220 00193000
*     END;                                                         0221 00194000
*   ELSE                            /* OPEN NOT SUCCESSFUL   @Z30SSRL*/ 00195000
*     RCDLEN=LENGTH(SMFRCD62);      /* RCD HAS NO VOLUME LIST        */ 00196000
         B     @RC00212                                            0222 00197000
@RF00212 MVC   RCDLEN(4),@CF03646                                  0222 00198000
*   END START62;                                                   0223 00199000
@EL00002 DS    0H                                                  0223 00200000
@EF00002 DS    0H                                                  0223 00201000
@ER00002 LM    @14,@12,12(@13)                                     0223 00202000
         BR    @14                                                 0223 00203000
*   RESPECIFY                                                      0224 00204000
*     RTIOT UNRESTRICTED;                                          0224 00205000
*START64:                                                          0225 00206000
*                                                                  0225 00207000
*/********************************************************************/ 00208000
*/*                                                                  */ 00209000
*/*   01 FUNCTION - CALCULATES AMOUNT OF STORAGE NEEDED FOR SMF      */ 00210000
*/*                 RECORD 64.                                       */ 00211000
*/*   01 INPUT  -   1. ADDRESS OF AMB IN RCD64AMB                    */ 00212000
*/*                 2. ADDRESS OF DEB IN AMBDEBPT                    */ 00213000
*/*   01 OUTPUT -   1. LENGTH OF SMF RECORD 64 IN RCDLEN             */ 00214000
*/*                                                                  */ 00215000
*/********************************************************************/ 00216000
*                                                                  0225 00217000
*   PROC;                                                          0225 00218000
START64  STM   @14,@12,12(@13)                                     0225 00219000
*   R5=RCD64AMB;                    /* LOAD POINTER TO AMB   @Y30DSMD*/ 00220000
         L     @11,RCDPARMA                                        0226 00221000
         L     R5,RCD64AMB-1(,@11)                                 0226 00222000
         LA    R5,0(,R5)                                           0226 00223000
*   RESPECIFY                                                      0227 00224000
*     RDEB RESTRICTED;                                             0227 00225000
*   RDEB=AMBDEBPT;                  /* LOAD POINTER TO DEB           */ 00226000
         L     RDEB,AMBDEBPT(,R5)                                  0228 00227000
*   RCDLEN=LENGTH(SMFRCD64)+LENGTH(SMF64STS)+LENGTH(SMF64EXT)*DEBNMEXT; 00228000
         SLR   @15,@15                                             0229 00229000
         IC    @15,DEBNMEXT(,RDEB)                                 0229 00230000
         MH    @15,@CH03648                                        0229 00231000
         AL    @15,@CF03647                                        0229 00232000
         ST    @15,RCDLEN                                          0229 00233000
*                                   /* GET LENGTH OF RECORD 64       */ 00234000
*   END START64;                                                   0230 00235000
@EL00003 DS    0H                                                  0230 00236000
@EF00003 DS    0H                                                  0230 00237000
@ER00003 LM    @14,@12,12(@13)                                     0230 00238000
         BR    @14                                                 0230 00239000
*   RESPECIFY                                                      0231 00240000
*     RDEB UNRESTRICTED;                                           0231 00241000
*CREACOM:                                                          0232 00242000
*                                                                  0232 00243000
*/********************************************************************/ 00244000
*/*                                                                  */ 00245000
*/*   01 FUNCTION - OBTAINS STORAGE FOR SMF RECORD AND               */ 00246000
*/*                 INITIALIZES COMMON FIELDS.                       */ 00247000
*/*   01 INPUT  -   1. LENGTH OF SMF RECORD IN RCDLEN                */ 00248000
*/*   01 OUTPUT -   1. STORAGE NEEDED TO BUILD SMF RECORD WITH       */ 00249000
*/*                    COMMON FIELDS INITIALIZED. ADDRESS OF         */ 00250000
*/*                    RECORD IS IN RCDPTR.                          */ 00251000
*/*                                                                  */ 00252000
*/********************************************************************/ 00253000
*                                                                  0232 00254000
*   PROC;                                                          0232 00255000
CREACOM  STM   @14,@12,@SA00004                                    0232 00256000
*   PPFUNC=PDF31;                   /* SET FUNCTION CODE     @Y30DSMD*/ 00257000
         MVI   PPFUNC,X'1F'                                        0233 00258000
*   R6=RCDLEN;                      /* LOAD RECORD LENGTH            */ 00259000
         L     R6,RCDLEN                                           0234 00260000
*   GEN(GETMAIN EC,LV=(R6),A=RCDPTR,SP=0,MF=(E,MAINWORK));         0235 00261000
         GETMAIN EC,LV=(R6),A=RCDPTR,SP=0,MF=(E,MAINWORK)               00262000
*   IF RETCODE^=0 THEN              /* IF NO CORE                    */ 00263000
         LTR   RETCODE,RETCODE                                     0236 00264000
         BZ    @RF00236                                            0236 00265000
*     DO;                                                          0237 00266000
*       PPCODE=PP20;                /* GETMAIN FAILURE       @Y30DSMD*/ 00267000
         MVI   PPCODE,X'14'                                        0238 00268000
*       GOTO EXIT;                  /* GET OUT OF MODULE             */ 00269000
         B     EXIT                                                0239 00270000
*     END;                                                         0240 00271000
*   R6=RCDPTR;                      /* LOAD PTR TO CORE              */ 00272000
@RF00236 L     R6,RCDPTR                                           0241 00273000
*   R7=RCDLEN;                      /* LOAD CORE LENGTH              */ 00274000
         L     R7,RCDLEN                                           0242 00275000
*   R9=0;                           /* PAD CHAR/LENGTH               */ 00276000
         SLR   R9,R9                                               0243 00277000
*   GEN(MVCL R6,R8);                /* ZERO CORE                     */ 00278000
         MVCL R6,R8                                                     00279000
*   RESPECIFY                                                      0245 00280000
*     SMFRCD62 BASED(RCDPTR);       /* USE RCD 62 FOR MODEL          */ 00281000
*   SMF62RTY=RCDNBR;                /* INIT RECORD TYPE              */ 00282000
         L     @11,RCDPTR                                          0246 00283000
         L     @01,RCDPARMA                                        0246 00284000
         IC    @00,RCDNBR(,@01)                                    0246 00285000
         STC   @00,SMF62RTY(,@11)                                  0246 00286000
*   R6=ADDR(JMRJOB);                /* POINT TO JOB NAME             */ 00287000
         L     @01,CVTPTR                                          0247 00288000
         L     @01,CVTTCBP(,@01)                                   0247 00289000
         L     @01,TCBPTR(,@01)                                    0247 00290000
         L     @01,TCBTCT(,@01)                                    0247 00291000
         L     @15,TCTJMR(,@01)                                    0247 00292000
         LR    R6,@15                                              0247 00293000
*   R7=ADDR(SMF62JNM);              /* POINT TO SMF JOB NAME         */ 00294000
         LA    R7,SMF62JNM(,@11)                                   0248 00295000
*   R7->NMFLD=R6->NMFLD;            /* MOVE JOB NAME INTO RECORD     */ 00296000
         MVC   NMFLD(8,R7),NMFLD(R6)                               0249 00297000
*   SMF62RST=JMRENTRY;              /* COPY READER START TIME        */ 00298000
         MVC   SMF62RST(4,@11),JMRENTRY(@15)                       0250 00299000
*   SMF62RDT=JMREDATE;              /* COPY READER START DATE        */ 00300000
         MVC   SMF62RDT(4,@11),JMREDATE(@15)                       0251 00301000
*   R6=ADDR(JMRUSEID);              /* POINT TO USER ID              */ 00302000
         LA    R6,JMRUSEID(,@15)                                   0252 00303000
*   R7=ADDR(SMF62UID);              /* POINT TO SMF USER ID          */ 00304000
         LA    R7,SMF62UID(,@11)                                   0253 00305000
*   R7->NMFLD=R6->NMFLD;            /* MOVE USER ID INTO RECORD      */ 00306000
         MVC   NMFLD(8,R7),NMFLD(R6)                               0254 00307000
*   END CREACOM;                                                   0255 00308000
@EL00004 DS    0H                                                  0255 00309000
@EF00004 DS    0H                                                  0255 00310000
@ER00004 LM    @14,@12,@SA00004                                    0255 00311000
         BR    @14                                                 0255 00312000
*FIN62:                                                            0256 00313000
*                                                                  0256 00314000
*/********************************************************************/ 00315000
*/*                                                                  */ 00316000
*/*   01 FUNCTION - COMPLETES BUILD OF SMF RECORD 62.                */ 00317000
*/*   01 INPUT  -   1. ADDRESS OF ACB IN RACB                        */ 00318000
*/*                 2. ADDRESS OF SMF RECORD (SMFRCD62) IN RCDPTR    */ 00319000
*/*   01 OUTPUT -   1. COMPLETED SMF RECORD 62                       */ 00320000
*/*                                                                  */ 00321000
*/********************************************************************/ 00322000
*                                                                  0256 00323000
*   PROC;                                                          0256 00324000
FIN62    STM   @14,@12,12(@13)                                     0256 00325000
*   RESPECIFY                                                      0257 00326000
*     RDEB RESTRICTED;                                             0257 00327000
*   SMF62IND=RCD62IND;              /* COPY INDICATORS               */ 00328000
         L     @15,RCDPTR                                          0258 00329000
         L     @14,RCDPARMA                                        0258 00330000
         MVC   SMF62IND(4,@15),RCD62IND(@14)                       0258 00331000
*   SMF62DNM=RCDCLSNM;              /* MOVE IN DATA SET/CLUSTER NAME */ 00332000
         L     @01,DSNPTR(,@14)                                    0259 00333000
         MVC   SMF62DNM(44,@15),RCDCLSNM(@01)                      0259 00334000
*   IF SMF62DNM(1:20)=SMFCATNM THEN /* IF VSAM CAT OPEN MSG  @XA04428*/ 00335000
         CLC   SMF62DNM(20,@15),@CC03591                           0260 00336000
         BNE   @RF00260                                            0260 00337000
*     SMF62DNM(20:24)=SMFBLANK;     /* THEN EDIT LAST 5 BYTES      0261 00338000
*                                                            @XA04428*/ 00339000
         MVC   SMF62DNM+19(5,@15),@CC03594                         0261 00340000
*   SMF62CVS=CATVLSER;              /* COPY CATALOG VOLSER           */ 00341000
@RF00260 L     @15,RCDPTR                                          0262 00342000
         L     @14,RCDPARMA                                        0262 00343000
         L     @01,RCD62VSR(,@14)                                  0262 00344000
         MVC   SMF62CVS(6,@15),CATVLSER(@01)                       0262 00345000
*   SMF62CNM=CATDSN;                /* COPY CATALOG DATA SET NAME    */ 00346000
         L     @01,RCD64CNM(,@14)                                  0263 00347000
         MVC   SMF62CNM(44,@15),CATDSN(@01)                        0263 00348000
*   IF SMF62OOK=ON THEN             /* IF OPEN OK -                  */ 00349000
         TM    SMF62OOK(@15),B'10000000'                           0264 00350000
         BNO   @RF00264                                            0264 00351000
*     DO;                                                          0265 00352000
*       R6=0;                       /* ZERO LIST INDEX               */ 00353000
*                                                                  0266 00354000
         SLR   R6,R6                                               0266 00355000
*       /*************************************************************/ 00356000
*       /*                                                           */ 00357000
*       /* INSERT AN ENTRY IN SMF RECORD 62 FOR EACH VOLUME SERIAL   */ 00358000
*       /* NUMBER ALLOCATED TO DATA SET                              */ 00359000
*       /*                                                           */ 00360000
*       /*************************************************************/ 00361000
*                                                                  0267 00362000
*       VMTPT=BIBVMT;               /* INIT VMT PTR          @Y30DSMD*/ 00363000
         L     @01,ACBAMBL(,RACB)                                  0267 00364000
         L     @01,AMBLBIB(,@01)                                   0267 00365000
         L     @15,BIBVMT(,@01)                                    0267 00366000
         ST    @15,VMTPT                                           0267 00367000
*       DO WHILE VMTPT^=0;          /* PROCESS ALL VMT'S     @Y30DSMD*/ 00368000
         B     @DE00268                                            0268 00369000
@DL00268 DS    0H                                                  0269 00370000
*         DO C1=1 TO VMTNOVOL;      /* PROCESS ALL VMT ENTRIES       */ 00371000
         LA    @15,1                                               0269 00372000
         B     @DE00269                                            0269 00373000
@DL00269 DS    0H                                                  0270 00374000
*           IF VMTVLSER(C1)^=VLZERO THEN/* VOL ENTRY ACTIVE  @Y30DSMD*/ 00375000
         L     @14,VMTPT                                           0270 00376000
         SLA   @15,4                                               0270 00377000
         ST    @15,@TF00001                                        0270 00378000
         ALR   @15,@14                                             0270 00379000
         CLC   VMTVLSER-16(6,@15),VLZERO                           0270 00380000
         BE    @RF00270                                            0270 00381000
*             DO;                   /* YES, CONTINUE         @Y30DSMD*/ 00382000
*               R6=R6+1;            /* BUMP LIST COUNT       @Y30DSMD*/ 00383000
         AL    R6,@CF00061                                         0272 00384000
*               SMF62VSR(R6)=VMTVLSER(C1);/* MOVE VOL SER TO LIST  0273 00385000
*                                                            @Y30DSMD*/ 00386000
         L     @15,RCDPTR                                          0273 00387000
         LR    @01,R6                                              0273 00388000
         MH    @01,@CH02447                                        0273 00389000
         ST    @01,@TF00002                                        0273 00390000
         ALR   @01,@15                                             0273 00391000
