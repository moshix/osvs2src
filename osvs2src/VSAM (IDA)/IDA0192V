         TITLE 'IDA0192V - MVM VOLUME MOUNT AND VERIFY                 *00001000
                        '                                               00002000
IDA0192V CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192V  78.165'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @12,4095(,@03)                                      0001 00012000
         USING @PSTART,@03                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
RBPRFXLN EQU 64                                                         00015000
RBPRFLNA EQU 32                                                         00016000
PRBLEN   EQU 136                                                        00017000
SIRBLEN  EQU 200                                                        00018000
TIRBLEN  EQU 136                                                        00019000
IRBLEN   EQU 128                                                        00020000
SVRBLEN  EQU 200                                                        00021000
JSCBDISP EQU 188                                                        00022000
JSCBAOS1 EQU  72                                                        00023000
JSCBAOS2 EQU 192                                                        00024000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00025000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL 0      00026000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00027000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00028000
*                                       IF NO ERROR WAS ENCOUNTERED     00029000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00030000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00031000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00032000
         MVCL  @00,@14                  0 THE GOTTEN AREA, R15=0        00033000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00034000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00035000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00036000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00037000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00038000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00039000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00040000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00041000
         B     GMN00200                 BYPASS SETTING ERROR            00042000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00043000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR 0       00044000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00045000
*        0 IF THE GETMAIN WAS UNSUCCESSFUL.                             00046000
*   IF RETCODE=0 THEN               /* IF GETMAIN UNSUCCESSFUL       */ 00047000
         LTR   RETCODE,RETCODE                                     0238 00048000
         BNZ   @RF00238                                            0238 00049000
*     DO;                                                          0239 00050000
*       VVOLLERR=GMFAIL;            /* INIT PROB DET INFO    @Y30DSMD*/ 00051000
         MVC   VVOLLERR(4,R1),GMFAIL                               0240 00052000
*       RETCODE=8;                  /* LOAD RETURN CODE OF 8 @Y30DSMD*/ 00053000
         LA    RETCODE,8                                           0241 00054000
*     END;                                                         0242 00055000
*   ELSE                            /* IF GETMAIN SUCCESSFUL         */ 00056000
*     DO;                                                          0243 00057000
         B     @RC00238                                            0243 00058000
@RF00238 DS    0H                                                  0244 00059000
*       GEN(ST R13,4(R15));         /* SAVE SAVE AREA ADDRESS        */ 00060000
         ST R13,4(R15)                                                  00061000
*       GEN( L R0,20(,R13));        /* IDAGMAIN USES R0, RESTORE IT.    00062000
*                                                            @Z30SSMD*/ 00063000
          L R0,20(,R13)                                                 00064000
*       GEN( L R1,24(,R13));        /* IDAGMAIN USES R1, RESTORE IT.    00065000
*                                                            @Z30SSMD*/ 00066000
          L R1,24(,R13)                                                 00067000
*       R13=R15;                    /* LOAD PTR TO AUTOMATIC CORE    */ 00068000
         LR    R13,R15                                             0247 00069000
*       GEN(USING @DATD,R13);       /* GET ADDRESSING                */ 00070000
         USING @DATD,R13                                                00071000
*       RESPECIFY                                                  0249 00072000
*         R6 RESTRICTED;                                           0249 00073000
*       R6=R1;                      /* COPY PARM PTR                 */ 00074000
         LR    R6,R1                                               0250 00075000
*       RESPECIFY                                                  0251 00076000
*         IDAVVOLL BASED(R6);       /*                       @Y30LSPS*/ 00077000
*       VVOLLFNC=VOLINIT;           /* SET FUNCTION CODE     @Y30LSPS*/ 00078000
         MVI   VVOLLFNC(R6),X'14'                                  0252 00079000
*       RESPECIFY                                                  0253 00080000
*         IDAVUCBL BASED(R6);                                      0253 00081000
*       IF VUCBLID=ON THEN          /* IF USER PASSED UCB LIST       */ 00082000
         TM    VUCBLID(R6),B'10000000'                             0254 00083000
         BNO   @RF00254                                            0254 00084000
*         DO;                       /* INIT LIST PTRS                */ 00085000
*           UCBLPTR=R6;             /* INIT UCB LIST PTR             */ 00086000
         ST    R6,UCBLPTR                                          0256 00087000
*           VOLLPTR=VUCBLVLA;       /* INIT VOLUME LIST PTR          */ 00088000
         L     @14,VUCBLVLA(,R6)                                   0257 00089000
         ST    @14,VOLLPTR                                         0257 00090000
*         END;                                                     0258 00091000
*       ELSE                        /* IF USER PASSED VOLUME LIST    */ 00092000
*         DO;                                                      0259 00093000
