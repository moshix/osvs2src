         L     @05,@TF00001                                        0550 01032000
         ST    RUCB,VMTUCB-16(@05,@11)                             0550 01033000
*                     END;          /*                       @Y30DSMD*/ 01034000
*                   GOTO EXIT;      /* QUIT WITH ERROR       @Y30LSPS*/ 01035000
         B     EXIT                                                0552 01036000
*                 END;              /* END IF VIRTUAL ERROR  @Y30LSPS*/ 01037000
*               ELSE                /* IS NORMAL DEVICE ERR  @Y30LSPS*/ 01038000
*                 DO;               /* NORMAL DEVICE MNT ER  @Y30LSPS*/ 01039000
@RF00527 DS    0H                                                  0555 01040000
*                   MSGDPTR=ADDR(M14E);/* POINT TO SHORT DISMOUNT MSG*/ 01041000
         LA    @11,M14E                                            0555 01042000
         ST    @11,MSGDPTR                                         0555 01043000
*                   CALL SEND;      /* SEND THE MESSAGE              */ 01044000
         BAL   @14,SEND                                            0556 01045000
*                   UCBNOTRD=ON;    /* MASK UCB NOT READY            */ 01046000
         OI    UCBNOTRD(RUCB),B'01000000'                          0557 01047000
*                   CALL MOUNT;     /* REISSUE MOUNT MSG             */ 01048000
         BAL   @14,MOUNT                                           0558 01049000
*                 END;              /* END NORMAL DEVICE     @Y30LSPS*/ 01050000
*             END;                                                 0560 01051000
