*         UCBAMV=OFF;               /* IND TIME STAMP CK     @Y30LSPS*/ 00930000
         NI    UCBAMV(RUCB),B'10111111'                            0511 00931000
*         IF VVOLLOPE=ON THEN       /* IF CALL FROM OPEN     @Y30LSPS*/ 00932000
         TM    VVOLLOPE(@05),B'00010000'                           0512 00933000
         BNO   @RF00512                                            0512 00934000
*           ERFLSAVE=ACBERFLG;      /* SAVE CURRENT ERR FLG  @Y30LSPS*/ 00935000
         IC    @11,ACBERFLG(,RACB)                                 0513 00936000
         STC   @11,ERFLSAVE                                        0513 00937000
*         DO WHILE(VVOLLMNT(C1)=OFF);/* VERIFY VOL                   */ 00938000
@RF00512 B     @DE00514                                            0514 00939000
@DL00514 DS    0H                                                  0515 00940000
*           DEBUCBA=RUCB;           /* INIT UCB ADDR IN WKA          */ 00941000
         L     @11,DEBDAPTR                                        0515 00942000
         STCM  RUCB,7,DEBUCBA(@11)                                 0515 00943000
*           DXECB=DXECB&&DXECB;     /* ZERO ECB                      */ 00944000
