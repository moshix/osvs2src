*   RESPECIFY                                                      0695 01508000
*     RET UNRESTRICTED;             /*                       @Y30DSMD*/ 01509000
*   DEBPTR=VVOLLDEB;                /* INIT BASE PTR FOR DEB @Y30DSMD*/ 01510000
         L     @11,VOLLPTR                                         0696 01511000
         L     @05,VVOLLDEB(,@11)                                  0696 01512000
