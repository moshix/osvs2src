*     CALL VMTSCAN;                 /* YES, SCAN THE VMT'S   @Z30SSJH*/ 00494000
         BAL   @14,VMTSCAN                                         0391 00495000
*   ELSE                            /* NOT AN EOV REQUEST    @Z30SSJH*/ 00496000
*     VMTHIT=OFF;                   /* IND VOLUME NOT IN VMT @Z30SSJH*/ 00497000
