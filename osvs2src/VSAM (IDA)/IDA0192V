*           DXDADDR(1:8)='0000000000000003'X;/* SET SEARCH ID        */ 00946000
         MVC   DXDADDR(8),@CB04491                                 0517 00947000
*           RESPECIFY                                              0518 00948000
*             R1 RESTRICTED;                                       0518 00949000
*           R1=ADDR(IOBSTDRD);                                     0519 00950000
         L     R1,IOBSTPTR                                         0519 00951000
*           GEN(EXCP (1));          /* START THE I/O                 */ 00952000
         EXCP (1)                                                       00953000
*           GEN(WAIT ECB=DXECB);    /* WAIT FOR COMPLETION           */ 00954000
         WAIT ECB=DXECB                                                 00955000
*           RESPECIFY                                              0522 00956000
*             R1 UNRESTRICTED;                                     0522 00957000
*           IF DXECB(1)^='7F'X THEN /* IF ANY ERROR                  */ 00958000
