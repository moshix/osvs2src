         BNE   @RF00452                                            0452 00747000
@RT00452 DS    0H                                                  0453 00748000
*                 DO;               /* YES                           */ 00749000
*                   DMSG=ON;        /* IND NEED FOR DISMOUNT MESSAGE */ 00750000
         OI    DMSG,B'00000001'                                    0454 00751000
*                   CALL ALOCUNIT;  /* CALL FOR MOUNT OF VOLUME      */ 00752000
         BAL   @14,ALOCUNIT                                        0455 00753000
*                 END;                                             0456 00754000
*               ELSE                /* VOLUME NOT DISMOUNTABLE     0457 00755000
*                                                            @Z30SSRL*/ 00756000
*                 VUCBLBAD(C2)=ON;  /* UNIT IS UNUSABLE              */ 00757000
