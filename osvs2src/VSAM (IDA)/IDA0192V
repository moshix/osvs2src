         ST    @11,@TF00001                                        0508 00922000
         ALR   @11,@05                                             0508 00923000
         TM    VVOLLVFY-12(@11),B'01000000'                        0508 00924000
         BNO   @RF00508                                            0508 00925000
*       DO;                                                        0509 00926000
*         RUCB=VVOLLUCB(C1);        /* LOAD PTR TO UCB               */ 00927000
         L     @01,@TF00001                                        0510 00928000
