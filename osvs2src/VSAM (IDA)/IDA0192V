@RF00451 DS    0H                                                  0459 00767000
*                 DMSG=OFF;         /* NO DISMOUNT MSG               */ 00768000
         NI    DMSG,B'11111110'                                    0459 00769000
*                 CALL ALOCUNIT;    /* CALL FOR MOUNT OF VOLUME      */ 00770000
         BAL   @14,ALOCUNIT                                        0460 00771000
*               END;                                               0461 00772000
*             CALL FREELOCK;        /* FREE THE LOCAL LOCK           */ 00773000
@RC00451 BAL   @14,FREELOCK                                        0462 00774000
*           END;                                                   0463 00775000
*       END;                                                       0464 00776000
@RF00442 LA    @05,1                                               0464 00777000
         AL    @05,C2                                              0464 00778000
@DE00441 ST    @05,C2                                              0464 00779000
         L     @01,UCBLPTR                                         0464 00780000
         CH    @05,VUCBLNBR(,@01)                                  0464 00781000
         BNH   @DL00441                                            0464 00782000
@DC00441 DS    0H                                                  0465 00783000
*   END;                                                           0465 00784000
@RF00440 LA    @05,1                                               0465 00785000
         AL    @05,C1                                              0465 00786000
@DE00439 ST    @05,C1                                              0465 00787000
         L     @01,VOLLPTR                                         0465 00788000
         CH    @05,VVOLLNBR(,@01)                                  0465 00789000
         BNH   @DL00439                                            0465 00790000
*   RESPECIFY                                                      0466 00791000
*     RET RESTRICTED;               /*                       @ZA33246*/ 00792000
*   RET=RSAVLA;                     /* RESTORE RETURN ADDRESS        */ 00793000
         L     RET,RSAVLA                                          0467 00794000
*   RESPECIFY                                                      0468 00795000
*    (R11,                                                         0468 00796000
*     RET) UNRESTRICTED;            /*                       @ZA33246*/ 00797000
*   END GOMOUNT;                                                   0469 00798000
@EL00003 DS    0H                                                  0469 00799000
@EF00003 DS    0H                                                  0469 00800000
@ER00003 BR    @14                                                 0469 00801000
*GOVERIFY:                                                         0470 00802000
*                                                                  0470 00803000
*/********************************************************************/ 00804000
*/*                                                                  */ 00805000
*/*   FUNCTION -  VERIFY THE LABEL ON MOUNTED VOLUME.                */ 00806000
*/*   OUTPUT  -   1. CONTROL BLOCK WORK AREA FOR I/O                 */ 00807000
*/*   INPUT  -    1. VOLUME LABEL READ INTO DXCB                     */ 00808000
*/*                                                                  */ 00809000
*/********************************************************************/ 00810000
*                                                                  0470 00811000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0470 00812000
GOVERIFY DS    0H                                                  0471 00813000
*   RESPECIFY                                                      0471 00814000
*     RET RESTRICTED;               /*                       @ZA33246*/ 00815000
*   RSAVLA=RET;                     /* SAVE RETURN ADDRESS           */ 00816000
         ST    RET,RSAVLA                                          0472 00817000
*   RESPECIFY                                                      0473 00818000
*     RET UNRESTRICTED;             /*                       @ZA33246*/ 00819000
*   DXCB=DXCB&&DXCB;                /* ZERO CONTROL BLOCK AREA       */ 00820000
*                                                                  0474 00821000
         XC    DXCB(200),DXCB                                      0474 00822000
*   /*****************************************************************/ 00823000
*   /*                                                               */ 00824000
*   /* SET UP CONTROL BLOCK BASES AND BUILD CHANNEL PROGRAMS         */ 00825000
*   /*                                                               */ 00826000
*   /*****************************************************************/ 00827000
*                                                                  0475 00828000
*   IOBSTPTR=ADDR(DXCB);            /* IOB IS FIRST IN CB            */ 00829000
         LA    @11,DXCB                                            0475 00830000
         ST    @11,IOBSTPTR                                        0475 00831000
*   DEBPTR=IOBSTPTR+LENGTH(IOBSTDRD);/* DEB IS SECOND                */ 00832000
         LA    @14,32                                              0476 00833000
         LR    @09,@11                                             0476 00834000
         ALR   @09,@14                                             0476 00835000
         ST    @09,DEBPTR                                          0476 00836000
*   DCBPTR=DEBPTR+LENGTH(DEBBASIC); /* DCB IS THIRD                  */ 00837000
         ALR   @14,@09                                             0477 00838000
         LR    DCBPTR,@14                                          0477 00839000
*   DCBDEBAD=ADDR(DEBBASIC);        /* DCB POINTS TO DEB             */ 00840000
         STCM  @09,15,DCBDEBAD(DCBPTR)                             0478 00841000
*   DEBAPPAD=CVTXAPG;               /* SET IOS APPENDAGE VECTOR TBL     00842000
*                                      ADDR                          */ 00843000
         L     @01,CVTPTR                                          0479 00844000
         L     @08,CVTXAPG(,@01)                                   0479 00845000
         ST    @08,DEBAPPAD(,@09)                                  0479 00846000
*   DEBEXSCL='04'X;                 /* SET EXTENT SCALE              */ 00847000
         MVI   DEBEXSCL(@09),X'04'                                 0480 00848000
*   DEBDCBB=ADDR(IHADCB);           /* DEB POINTS TO DCB             */ 00849000
         STCM  DCBPTR,7,DEBDCBB(@09)                               0481 00850000
*   DEBDEBID=IDXDEB;                /* SET DEB ID                    */ 00851000
         MVI   DEBDEBID(@09),B'00001111'                           0482 00852000
*   DEBNMEXT=1;                     /* SET EXTENT NUMBER             */ 00853000
         MVI   DEBNMEXT(@09),X'01'                                 0483 00854000
*   IOBUNREL=ON;                    /* SET NON-SEQUENTIAL BIT        */ 00855000
         OI    IOBUNREL(@11),B'00000010'                           0484 00856000
*   IOBDCBPT=ADDR(IHADCB);          /* IOB POINTS TO DCB             */ 00857000
         ST    DCBPTR,IOBDCBPT(,@11)                               0485 00858000
*   IOBECBPT=ADDR(DXECB);           /* IOB POINTS TO ECB             */ 00859000
         LA    @05,DXECB                                           0486 00860000
         ST    @05,IOBECBPT(,@11)                                  0486 00861000
*   IOBSTART=ADDR(DXCCW1);          /* IOB POINTS TO FIRST CCW       */ 00862000
         LA    @05,DXCCW1                                          0487 00863000
         ST    @05,IOBSTART(,@11)                                  0487 00864000
*   DEBDAPTR=ADDR(DEBBASND);                                       0488 00865000
         LA    @11,DEBBASND(,@09)                                  0488 00866000
         ST    @11,DEBDAPTR                                        0488 00867000
*   DXCCW1=DXCCW1&&DXCCW1;          /* ZERO CCW1                     */ 00868000
         XC    DXCCW1(8),DXCCW1                                    0489 00869000
*   DXCCW2=DXCCW1;                  /* ZERO CCW2                     */ 00870000
         MVC   DXCCW2(8),DXCCW1                                    0490 00871000
*   DXCCW3=DXCCW2;                  /* ZERO CCW3                     */ 00872000
         MVC   DXCCW3(8),DXCCW2                                    0491 00873000
*   DXCCW1(1:4)=ADDR(DXDADDR)+3;                                   0492 00874000
         LA    @05,DXDADDR                                         0492 00875000
         AL    @05,@CF00032                                        0492 00876000
         STCM  @05,15,DXCCW1                                       0492 00877000
*   DXCCW1(1)='31'X;                /* SET SEARCH ID COMMAND CODE    */ 00878000
         MVI   DXCCW1,X'31'                                        0493 00879000
*   DXCCW1(5)='40'X;                /* SET COMMAND CHAIN BIT         */ 00880000
         MVI   DXCCW1+4,X'40'                                      0494 00881000
*   DXCCW1(8)='05'X;                /* SET LENGTH                    */ 00882000
         MVI   DXCCW1+7,X'05'                                      0495 00883000
*   DXCCW2(1:4)=ADDR(DXCCW1);                                      0496 00884000
         LA    @05,DXCCW1                                          0496 00885000
         STCM  @05,15,DXCCW2                                       0496 00886000
*   DXCCW2(1)='08'X;                /* SET TIC COMMAND CODE          */ 00887000
         MVI   DXCCW2,X'08'                                        0497 00888000
*   DXCCW2(5)='40'X;                /* SET COMMAND CHAIN BIT         */ 00889000
         MVI   DXCCW2+4,X'40'                                      0498 00890000
*   DXCCW3(1:4)=ADDR(DXLBL);        /* SET ADDRESS OF READ DATA      */ 00891000
         LA    @05,DXLBL                                           0499 00892000
         STCM  @05,15,DXCCW3                                       0499 00893000
*   DXCCW3(1)='06'X;                /* SET READ COMMAND CODE         */ 00894000
         MVI   DXCCW3,X'06'                                        0500 00895000
*   DXCCW3(8)='50'X;                /* SET LENGTH OF DATA            */ 00896000
         MVI   DXCCW3+7,X'50'                                      0501 00897000
*   DEBSTRCC(1:2)='0000'X;          /* ZERO STARTING CYLINDER        */ 00898000
         MVC   DEBSTRCC(2,@11),@CB04489                            0502 00899000
*   DEBSTRHH(1:2)='0000'X;          /* ZERO STARTING HEAD            */ 00900000
         MVC   DEBSTRHH(2,@11),@CB04489                            0503 00901000
*   DEBENDHH(1:2)='0000'X;          /* ZERO ENDING HEAD              */ 00902000
         MVC   DEBENDHH(2,@11),@CB04489                            0504 00903000
*   DEBENDCC(1:2)='7FFF'X;          /* SET END CYLINDER NUMBER       */ 00904000
         MVC   DEBENDCC(2,@11),@CB04490                            0505 00905000
*   DEBNMTRK(1:2)='7FFF'X;          /* SET NUMBER OF TRACKS          */ 00906000
         MVC   DEBNMTRK(2,@11),@CB04490                            0506 00907000
*                                                                  0507 00908000
*   /*****************************************************************/ 00909000
*   /*                                                               */ 00910000
*   /* VERIFY THE VOLUMES THAT HAVE BEEN MOUNTED                     */ 00911000
*   /*                                                               */ 00912000
*   /*****************************************************************/ 00913000
*                                                                  0507 00914000
*   DO C1=1 TO VVOLLNBR;            /* LOOP THRU VOLUME LIST         */ 00915000
         LA    @11,1                                               0507 00916000
         B     @DE00507                                            0507 00917000
@DL00507 DS    0H                                                  0508 00918000
*     IF VVOLLVFY(C1)=ON THEN       /* IF VOL NEEDS VERIFICATION     */ 00919000
         L     @05,VOLLPTR                                         0508 00920000
