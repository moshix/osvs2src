*     R10=R10+LENGTH(CCW);          /* POINT TO NEXT CCW             */ 00744000
         AL    R10,@CF00051                                        0438 00745000
*   END;                            /* BUILD NOP                     */ 00746000
         BCTR  C3,0                                                0439 00747000
@DE00417 LTR   C3,C3                                               0439 00748000
         BP    @DL00417                                            0439 00749000
*   CCWCODE=CCNOP;                  /* SET COMMAND CODE ONLY         */ 00750000
         MVI   CCWCODE(R10),X'03'                                  0440 00751000
*   CCWSLI=ON;                                                     0441 00752000
         OI    CCWSLI(R10),B'00100000'                             0441 00753000
*   CCWLEN=K2;                      /* SET DUMMY LENGTH      @X04SVCJ*/ 00754000
         MVC   CCWLEN(2,R10),@CH00037                              0442 00755000
*   END INITUWCP;                   /*                       @X04SVCJ*/ 00756000
@EL00005 DS    0H                                                  0443 00757000
@EF00005 DS    0H                                                  0443 00758000
@ER00005 BR    @14                                                 0443 00759000
*INITFMTW:                                                         0444 00760000
*                                                                  0444 00761000
*   /*****************************************************************/ 00762000
*   /*                                                               */ 00763000
*   /* INITIALIZE FORMAT WRITE CHANNEL PROGRAM SEGMENT               */ 00764000
*   /*                                                               */ 00765000
*   /*****************************************************************/ 00766000
*                                                                  0444 00767000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @X04SVCJ*/ 00768000
INITFMTW DS    0H                                                  0445 00769000
*   R10=CPAWCPS;                    /* POINT TO FIRST CCW            */ 00770000
         L     @15,BUFCCPA(,R9)                                    0445 00771000
         L     R10,CPAWCPS(,@15)                                   0445 00772000
*   RESPECIFY                                                      0446 00773000
*     CCW BASED(R10);               /* SET UP SEEK HEAD CCW          */ 00774000
*   CCWCODE=CCSEEKH;                /* SET COMMAND CODE              */ 00775000
         MVI   CCWCODE(R10),X'1B'                                  0447 00776000
*   CCWAREA=ADDR(CPAWSEEK);         /* POINT BACK TO HEADER          */ 00777000
         LA    @08,CPAWSEEK(,@15)                                  0448 00778000
         STCM  @08,7,CCWAREA(R10)                                  0448 00779000
*   CCWCC=ON;                       /* SET COMMAND CHAINING BIT      */ 00780000
         OI    CCWCC(R10),B'01000000'                              0449 00781000
*   CCWLEN=LENGTH(CPAWSEEK);        /* SET LENGTH OF SEEK ARG SET UP    00782000
*                                      SID EQ, TIC, WD CCWS          */ 00783000
         MVC   CCWLEN(2,R10),@CH00105                              0450 00784000
*   R10=R10+LENGTH(CCW);            /* POINT TO NEXT CCW             */ 00785000
         LA    @08,8                                               0451 00786000
         ALR   R10,@08                                             0451 00787000
*   CCWCODE=CCSS;                   /* SET SET SECTOR COMMAND CODE 0452 00788000
*                                                            @X04SVCJ*/ 00789000
         MVI   CCWCODE(R10),X'23'                                  0452 00790000
*   CCWCC=ON;                       /* SET COMMAND CHAINING  @X04SVCJ*/ 00791000
*   CCWSLI=ON;                      /* SET SLI               @X04SVCJ*/ 00792000
         OI    CCWCC(R10),B'01100000'                              0454 00793000
*   CCWLEN=K1;                      /* SET LENGTH            @X04SVCJ*/ 00794000
         MVC   CCWLEN(2,R10),@CH00043                              0455 00795000
*   CCWAREA=ADDR(CPAWSECT);         /* SET ADDRESS           @X04SVCJ*/ 00796000
         LA    @07,CPAWSECT(,@15)                                  0456 00797000
         STCM  @07,7,CCWAREA(R10)                                  0456 00798000
*   R10=R10+LENGTH(CCW);            /* UPDATE POINTER        @X04SVCJ*/ 00799000
         ALR   R10,@08                                             0457 00800000
*   CCWCODE=CCSIDEQ;                /* BUILD SEARCH ID EQU CCW       */ 00801000
         MVI   CCWCODE(R10),X'31'                                  0458 00802000
*   CCWAREA=CPAWSID;                /* ADDRESS OF SEARCH ID  @Z30SSDT*/ 00803000
         MVC   CCWAREA(3,R10),CPAWSID+1(@15)                       0459 00804000
*   CCWCC=ON;                       /* COMMAND CHAINING              */ 00805000
         OI    CCWCC(R10),B'01000000'                              0460 00806000
*   CCWLEN=K5;                      /* LENGTH OF SEARCH ID   @X04SVCJ*/ 00807000
         MVC   CCWLEN(2,R10),@CH00047                              0461 00808000
*   R10=R10+LENGTH(CCW);            /* POINT TO NEXT CCW             */ 00809000
         ALR   R10,@08                                             0462 00810000
*   CCWCODE=CCTIC;                  /* BUILD TIC CCW                 */ 00811000
         MVI   CCWCODE(R10),X'08'                                  0463 00812000
*   CCWAREA=R10-LENGTH(CCW);        /* ADDR OF PREVIOUS CCW          */ 00813000
         LCR   @08,@08                                             0464 00814000
         ALR   @08,R10                                             0464 00815000
         STCM  @08,7,CCWAREA(R10)                                  0464 00816000
*   R11=CPAFWCNT;                   /* POINT TO FW COUNT AREA        */ 00817000
         L     R11,CPAFWCNT(,@15)                                  0465 00818000
*   IF BUFCPFCP=ON THEN             /* IF PREFORMAT COMPLETE         */ 00819000
         TM    BUFCPFCP(R9),B'00000010'                            0466 00820000
         BNO   @RF00466                                            0466 00821000
*     C3=R6;                        /* THEN MANY CCW SETS            */ 00822000
         LR    C3,R6                                               0467 00823000
*   ELSE                            /* ONLY ONE              @X04SVCJ*/ 00824000
*     C3=K1;                        /* IF NOT PREF COMP 1 SET CCWS 0468 00825000
*                                                            @X04SVCJ*/ 00826000
         B     @RC00466                                            0468 00827000
@RF00466 LA    C3,1                                                0468 00828000
*   DO C3=C3 TO K1 BY-K1;           /* COMPLETE CCWS         @X04SVCJ*/ 00829000
@RC00466 B     @DE00469                                            0469 00830000
@DL00469 DS    0H                                                  0470 00831000
*     R10=R10+LENGTH(CCW);          /* POINT TO NEXT CCW             */ 00832000
