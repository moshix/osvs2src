*     CCWCODE=CCWCKD;               /* BUILD WCKD CCW                */ 00835000
         MVI   CCWCODE(R10),X'1D'                                  0471 00836000
*     CCWAREA=R11;                  /* POINT TO FW COUNT AREA        */ 00837000
         STCM  R11,7,CCWAREA(R10)                                  0472 00838000
*     R11=R11+DWORDLEN;             /* POINT TO NEXT FW COUNT AREA   */ 00839000
