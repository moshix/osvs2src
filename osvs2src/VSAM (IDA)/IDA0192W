         AL    R10,@CF00051                                        0558 01097000
*       END;                        /*                       @Z30SSRL*/ 01098000
         BCTR  C3,0                                                0559 01099000
@DE00555 LTR   C3,C3                                               0559 01100000
         BP    @DL00555                                            0559 01101000
*     END;                                                         0560 01102000
*   END BLDVPL;                     /*                       @X04SVCJ*/ 01103000
@EL00008 DS    0H                                                  0561 01104000
@EF00008 DS    0H                                                  0561 01105000
@ER00008 BR    @14                                                 0561 01106000
*BLDRCP:                                                           0562 01107000
*                                                                  0562 01108000
*   /*****************************************************************/ 01109000
*   /*                                                               */ 01110000
*   /* BUILD READ CHANNEL PROGRAM SEGMENT                            */ 01111000
*   /*                                                               */ 01112000
*   /*****************************************************************/ 01113000
*                                                                  0562 01114000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @X04SVCJ*/ 01115000
BLDRCP   DS    0H                                                  0563 01116000
*   R10=CPARCPS;                    /* POINT TO FIRST CCW BUILD SEEK    01117000
*                                      HEAD CCW                      */ 01118000
         L     @15,BUFCCPA(,R9)                                    0563 01119000
         L     R10,CPARCPS(,@15)                                   0563 01120000
*   CCWCODE=CCSEEKH;                /* SET COMMAND CODE SEEK HEAD    */ 01121000
         MVI   CCWCODE(R10),X'1B'                                  0564 01122000
*   CCWAREA=ADDR(CPARSEEK);         /* POINT AREA TO HEADER          */ 01123000
         LA    @08,CPARSEEK(,@15)                                  0565 01124000
         STCM  @08,7,CCWAREA(R10)                                  0565 01125000
*   CCWCC=ON;                       /* SET COMMAND CHAINING BIT      */ 01126000
         OI    CCWCC(R10),B'01000000'                              0566 01127000
*   CCWLEN=LENGTH(CPARSEEK);        /* SET SEEK ARG LENGTH BUILD   0567 01128000
*                                      SEARCH ID EQ CCW              */ 01129000
         MVC   CCWLEN(2,R10),@CH00105                              0567 01130000
*   R10=R10+LENGTH(CCW);            /* POINT TO NEXT CCW             */ 01131000
         LA    @08,8                                               0568 01132000
         ALR   R10,@08                                             0568 01133000
*   CCWCODE=CCSS;                   /* SET SET SECTOR COMMAND CODE 0569 01134000
*                                                            @X04SVCJ*/ 01135000
         MVI   CCWCODE(R10),X'23'                                  0569 01136000
*   CCWCC=ON;                       /* SET COMMAND CHAINING  @X04SVCJ*/ 01137000
*   CCWSLI=ON;                      /* SET SLI               @X04SVCJ*/ 01138000
         OI    CCWCC(R10),B'01100000'                              0571 01139000
*   CCWLEN=K1;                      /* SET LENGTH            @X04SVCJ*/ 01140000
         MVC   CCWLEN(2,R10),@CH00043                              0572 01141000
*   CCWAREA=ADDR(CPARSECT);         /* SET ADDRESS           @Z30SSDT*/ 01142000
         LA    @07,CPARSECT(,@15)                                  0573 01143000
         STCM  @07,7,CCWAREA(R10)                                  0573 01144000
*   R10=R10+LENGTH(CCW);            /* UPDATE POINTER        @X04SVCJ*/ 01145000
         ALR   R10,@08                                             0574 01146000
*   CCWCODE=CCSIDEQ;                /* SET COMMAND CODE              */ 01147000
         MVI   CCWCODE(R10),X'31'                                  0575 01148000
*   CCWAREA=ADDR(CPARSID);          /* POINT BACK TO HEADER  @Z30SSDT*/ 01149000
         LA    @07,CPARSID(,@15)                                   0576 01150000
         STCM  @07,7,CCWAREA(R10)                                  0576 01151000
*   CCWCC=ON;                       /* COMMAND CHAINING BIT          */ 01152000
*   CCWSLI=ON;                      /* SUPRESS INCORRECT LENGTH      */ 01153000
         OI    CCWCC(R10),B'01100000'                              0578 01154000
*   CCWLEN=LENGTH(CPARSID);         /* SET SEARCH ID ARG LENGTH BUILD   01155000
*                                      TIC                           */ 01156000
         MVC   CCWLEN(2,R10),@CH00047                              0579 01157000
*   R10=R10+LENGTH(CCW);            /* POINT TO NEXT CCW             */ 01158000
         ALR   R10,@08                                             0580 01159000
*   CCWCODE=CCTIC;                  /* SET COMMAND CODE              */ 01160000
         MVI   CCWCODE(R10),X'08'                                  0581 01161000
*   CCWAREA=ADDR(CCW)-LENGTH(CCW);  /* AREA IS CCW-1 BUILD READ DATA    01162000
*                                      CCWS                          */ 01163000
         LR    @07,R10                                             0582 01164000
         SLR   @07,@08                                             0582 01165000
         STCM  @07,7,CCWAREA(R10)                                  0582 01166000
*   R10=R10+LENGTH(CCW);            /* POINT TO NEXT CCW             */ 01167000
         ALR   R10,@08                                             0583 01168000
*   R12=CPAIDAL;                    /* POINT TO FIRST OF IDAL        */ 01169000
         L     R12,CPAIDAL(,@15)                                   0584 01170000
*   DO C3=R6 TO 1 BY-1;             /* 1 RD CCW/BLK/CINV             */ 01171000
         LR    C3,R6                                               0585 01172000
         B     @DE00585                                            0585 01173000
@DL00585 DS    0H                                                  0586 01174000
*     IF C3=R6 THEN                 /* FIRST READ            @Z30SSRL*/ 01175000
         CR    C3,R6                                               0586 01176000
         BNE   @RF00586                                            0586 01177000
*       CCWCODE=CCRD;               /* NO MT FOR FIRST READ DATA     */ 01178000
         MVI   CCWCODE(R10),X'06'                                  0587 01179000
*     ELSE                          /* NOT FIRST READ        @Z30SSRL*/ 01180000
*       CCWCODE=CCMTRD;             /* MOUNT ON              @Z30SSRL*/ 01181000
         B     @RC00586                                            0588 01182000
@RF00586 MVI   CCWCODE(R10),X'86'                                  0588 01183000
*     CCWAREA=R12;                  /* CCW AREA IN IDAL              */ 01184000
@RC00586 STCM  R12,7,CCWAREA(R10)                                  0589 01185000
*     IF AMBLFSTP=OFF THEN          /* ICI PROCESSING        @X04SVCJ*/ 01186000
         L     @15,ACBAMBL(,RACB)                                  0590 01187000
         TM    AMBLFSTP(@15),B'00010000'                           0590 01188000
         BNZ   @RF00590                                            0590 01189000
*       CCWCC=ON;                   /* CC ON AS USUAL                */ 01190000
         OI    CCWCC(R10),B'01000000'                              0591 01191000
*     CCWIDAL=ON;                   /* SET IDAL BIT ON               */ 01192000
@RF00590 OI    CCWIDAL(R10),B'00000100'                            0592 01193000
*     IF LPMBLKSZ>IOPAGESZ THEN     /* IF PHYSICAL BLKSIZE>PAGE    0593 01194000
*                                                            @X04SVCJ*/ 01195000
         L     @15,AMBPTR(,PARMPTR)                                0593 01196000
         L     @15,AMBEDB(,@15)                                    0593 01197000
         L     @15,EDBLPMBC(,@15)                                  0593 01198000
         L     @15,LPMBLKSZ(,@15)                                  0593 01199000
         C     @15,@CF00039                                        0593 01200000
         BNH   @RF00593                                            0593 01201000
*       R12=R12+K12;                /* BUMP IDAL POINTER     @Z30SSRL*/ 01202000
         AL    R12,@CF00053                                        0594 01203000
*     ELSE                          /* IF PHYSICAL BLKSIZE<=PAGE     */ 01204000
*       R12=R12+K8;                 /* BUMP POINTER TO IDAL  @Z30SSRL*/ 01205000
         B     @RC00593                                            0595 01206000
@RF00593 AL    R12,@CF00051                                        0595 01207000
*     CCWLEN=LPMBLKSZ;              /* SET CCW LENGTH        @X04SVCJ*/ 01208000
@RC00593 L     @15,AMBPTR(,PARMPTR)                                0596 01209000
         L     @15,AMBEDB(,@15)                                    0596 01210000
         L     @15,EDBLPMBC(,@15)                                  0596 01211000
         L     @15,LPMBLKSZ(,@15)                                  0596 01212000
         STH   @15,CCWLEN(,R10)                                    0596 01213000
*     R10=R10+LENGTH(CCW);          /* POINT TO NEXT CCW             */ 01214000
         AL    R10,@CF00051                                        0597 01215000
*   END;                            /* BUILD LAST NOP CCW            */ 01216000
         BCTR  C3,0                                                0598 01217000
@DE00585 LTR   C3,C3                                               0598 01218000
         BP    @DL00585                                            0598 01219000
*   CCWCODE=CCNOP;                  /* LAST CCW IS NOP               */ 01220000
         MVI   CCWCODE(R10),X'03'                                  0599 01221000
*   CCWSLI=ON;                      /* SUPPRESS LENGTH ERROR         */ 01222000
         OI    CCWSLI(R10),B'00100000'                             0600 01223000
*   CCWLEN=K2;                      /* SET DUMMY LENGTH      @X04SVCJ*/ 01224000
         MVC   CCWLEN(2,R10),@CH00037                              0601 01225000
*   END BLDRCP;                     /*                       @X04SVCJ*/ 01226000
