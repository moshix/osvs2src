*     CCWDC=ON;                     /* DATA CHAINING                 */ 00841000
         OI    CCWDC(R10),B'10000000'                              0474 00842000
*     CCWLEN=K8;                    /* LENGTH OF DATA        @X04SVCJ*/ 00843000
         STH   @15,CCWLEN(,R10)                                    0475 00844000
*     R10=R10+LENGTH(CCW);          /* POINT TO NEXT CCW             */ 00845000
         ALR   R10,@15                                             0476 00846000
*     CCWCODE=CCWCKD;               /* BUILD 2ND WCKD CCW            */ 00847000
         MVI   CCWCODE(R10),X'1D'                                  0477 00848000
*     CCWAREA=CPAIDAL;              /* ADDR OF REAL PAGE LIST        */ 00849000
         L     @15,BUFCCPA(,R9)                                    0478 00850000
         MVC   CCWAREA(3,R10),CPAIDAL+1(@15)                       0478 00851000
*     IF AMBLFSTP=OFF               /* ICI SPECIFIED         @X04SVCJ*/ 00852000
*         |BUFCWC=ON THEN           /* WRITE CHECK           @X04SVCJ*/ 00853000
