*   MWASSINC=ON;                    /* SET INCREMENT SW      @Y30DSDP*/ 04074000
         OI    MWASSINC,B'10000000'                                1322 04075000
*   MWASSBYT=ON;                    /* SET BYTE FLAG         @Y30DSDP*/ 04076000
