@DL01960 DS    0H                                                  1961 06344000
*         C2=C2+ONE;                /* INCREMENT CSL INDEX           */ 06345000
         LA    @10,1                                               1961 06346000
         AL    @10,C2                                              1961 06347000
         ST    @10,C2                                              1961 06348000
*       END;                        /* END OF CSL LOCATE             */ 06349000
@DE01960 L     @10,C2                                              1962 06350000
         LR    @06,@10                                             1962 06351000
         SLA   @06,3                                               1962 06352000
