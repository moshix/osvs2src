*   /* DECHNIRB DECHAINS THE IRB FROM THE TCB RB QUEUE.      @ZA02280*/ 06074000
*   /*                                                               */ 06075000
*   /*****************************************************************/ 06076000
*                                                                  1878 06077000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @ZA02280*/ 06078000
DECHNIRB DS    0H                                                  1879 06079000
*   RFY                                                            1879 06080000
*     RET RSTD;                     /* RESERVE REG(14).      @ZA02280*/ 06081000
*   C3=C3+ONE;                      /* INDEX SAVE AREA       @ZA02280*/ 06082000
         LA    @15,1                                               1880 06083000
         AL    @15,C3                                              1880 06084000
         ST    @15,C3                                              1880 06085000
*   MWARETSV(C3)=RET;               /* SAVE RETURN ADDR      @ZA02280*/ 06086000
         SLA   @15,2                                               1881 06087000
         ST    RET,MWARETSV-4(@15)                                 1881 06088000
*   RFY                                                            1882 06089000
*     RET UNRSTD;                   /* RELEASE THE REGISTER. @ZA02280*/ 06090000
*   MWARBPTR=ADDR(RBBASIC);         /* ADDRESS OF TOP RB     @ZA02280*/ 06091000
         L     @09,PSATOLD                                         1883 06092000
         L     MWARBPTR,TCBRBP(,@09)                               1883 06093000
*   RESPECIFY                                                      1884 06094000
*     RBBASIC BASED(MWARBPTR);      /* ADDRESS CURRENT RB    @ZA02280*/ 06095000
*   DO UNTIL MWAIRBPT->RBFACTV=OFF| /* LOOP UNTIL DECHAINED  @ZA02280*/ 06096000
*         RBTCBNXT=ON;              /* OR THE LAST RB FOUND  @ZA02280*/ 06097000
@DL01885 DS    0H                                                  1886 06098000
*     IF RBLINKB=MWAIRBPT THEN      /* IF NEXT RB IS THE IRB @ZA02280*/ 06099000
         L     @09,RBLINKB-1(,MWARBPTR)                            1886 06100000
         LA    @09,0(,@09)                                         1886 06101000
         CR    MWAIRBPT,@09                                        1886 06102000
         BNE   @RF01886                                            1886 06103000
*       DO;                         /* DECHAIN IT            @ZA02280*/ 06104000
*         RBLINKB=MWAIRBPT->RBLINKB;/* UPDATE RB LINK FIELD  @ZA02280*/ 06105000
         MVC   RBLINKB(3,MWARBPTR),RBLINKB(MWAIRBPT)               1888 06106000
*         MWAIRBPT->RBFACTV=OFF;    /* TURN OFF ACTIVE IND   @ZA02280*/ 06107000
         NI    RBFACTV(MWAIRBPT),B'10111111'                       1889 06108000
*       END;                        /* END OF IRB DECHAIN    @ZA02280*/ 06109000
*     MWARBPTR=RBLINK;              /* POINT TO NEXT RB      @ZA02280*/ 06110000
@RF01886 L     MWARBPTR,RBLINK(,MWARBPTR)                          1891 06111000
*   END;                            /* END OF RB SCAN        @ZA02280*/ 06112000
@DE01885 TM    RBFACTV(MWAIRBPT),B'01000000'                       1892 06113000
         BZ    @DC01885                                            1892 06114000
         TM    RBTCBNXT(MWARBPTR),B'10000000'                      1892 06115000
         BNO   @DL01885                                            1892 06116000
@DC01885 DS    0H                                                  1893 06117000
*   RESPECIFY                                                      1893 06118000
*     RBBASIC BASED(TCBRBP);        /* DEFAULT RB BASING     @ZA02280*/ 06119000
*   RFY                                                            1894 06120000
*     RET RSTD;                     /* INFORM THE COMPILER.  @ZA02280*/ 06121000
*   RET=MWARETSV(C3);               /* RESTORE RET REG       @ZA02280*/ 06122000
         L     @09,C3                                              1895 06123000
         LR    @06,@09                                             1895 06124000
         SLA   @06,2                                               1895 06125000
         L     RET,MWARETSV-4(@06)                                 1895 06126000
*   C3=C3-ONE;                      /* DECR SAVE INDEX       @ZA02280*/ 06127000
         BCTR  @09,0                                               1896 06128000
         ST    @09,C3                                              1896 06129000
*   RFY                                                            1897 06130000
*     RET UNRSTD;                   /* RELEASE REG(14).      @ZA02280*/ 06131000
*   END DECHNIRB;                   /* END DECHNIRB.         @ZA02280*/ 06132000
@EL00043 DS    0H                                                  1898 06133000
@EF00043 DS    0H                                                  1898 06134000
@ER00043 BR    @14                                                 1898 06135000
*DISABLE:                                                          1899 06136000
*                                                                  1899 06137000
*/********************************************************************/ 06138000
*/*                                                          @Y30DSDP*/ 06139000
*/*    01 FUNCTION -  THIS ROUTINE WILL DISABLE INTERRUPTS   @Y30DSDP*/ 06140000
*/*    01 INPUT -  1. DISABLE REQUEST COUNTER (DISABLCT)     @Y30DSDP*/ 06141000
*/*    01 OUTPUT - 1. IF DISABLE REQUEST COUNT IS ZERO, INTERRUPTS   */ 06142000
*/*                   ARE DISABLED                           @Y30DSDP*/ 06143000
*/*                2. THE DISABLE REQUEST COUNT IS INCREMENTED BY 1  */ 06144000
*/*                                                          @Y30DSDP*/ 06145000
