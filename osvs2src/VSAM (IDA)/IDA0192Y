         L     @05,C3                                              1951 06317000
         ALR   @05,@15                                             1951 06318000
         ST    @05,C3                                              1951 06319000
*   MWARETSV(C3)=RET;               /* SAVE RETURN ADDRESS           */ 06320000
         SLA   @05,2                                               1952 06321000
         ST    RET,MWARETSV-4(@05)                                 1952 06322000
*   RFY                                                            1953 06323000
*     RET UNRSTD;                                                  1953 06324000
*   IF BLPFDBDC=ON THEN             /* IS THIS A BLDVRP REQUEST      */ 06325000
         TM    BLPFDBDC(RWAPARMP),B'10000000'                      1954 06326000
         BNO   @RF01954                                            1954 06327000
*     DO;                           /* ASSUME LSR AND INIT FOR DBDC  */ 06328000
*       MWAVSRTP=VATVPTR;           /* INITIALIZE VSRT POINTER       */ 06329000
         L     @10,MWAJSCBP                                        1956 06330000
         L     @10,JSCBSHR(,@10)                                   1956 06331000
         L     @10,VATVPTR(,@10)                                   1956 06332000
         ST    @10,MWAVSRTP                                        1956 06333000
*       MWASTRNO=VSRTSTRN;          /* GET NUMBER OF STRINGS         */ 06334000
         SLR   MWASTRNO,MWASTRNO                                   1957 06335000
         IC    MWASTRNO,VSRTSTRN(,@10)                             1957 06336000
*       MWAPLHNX=VSRTPLHH+LENGTH(IDAPLHDR);/* POINT TO FIRST PLH     */ 06337000
         LA    MWAPLHNX,16                                         1958 06338000
