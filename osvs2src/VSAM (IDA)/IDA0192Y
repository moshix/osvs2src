         TM    VSRTCSVG-8(@05),B'00010000'                         1962 06355000
         BZ    @DL01960                                            1962 06356000
*       C2=C2+ONE;                  /* SET INDEX - USE BY SRBPFL RTN */ 06357000
         AL    @10,@CF00084                                        1963 06358000
         ST    @10,C2                                              1963 06359000
*     END;                          /* END OF DBDC INITIALIZATION    */ 06360000
*   ELSE                            /* NOT DBDC,                     */ 06361000
*     DO;                           /* INITIALIZE FOR STRING BUILD   */ 06362000
         B     @RC01954                                            1965 06363000
@RF01954 DS    0H                                                  1966 06364000
*       MWASPHDP=AMBBUFC;           /* SET ADDRESS OF BUFFER HEADER  */ 06365000
         L     @05,MWAAMBPT                                        1966 06366000
