*             ELSE                  /* NO, INDEX PASS                */ 00170000
*               MWAAMBPT=AMBLIX;    /* SET POINTER TO INDEX AMB      */ 00171000
         B     @RC00313                                            0315 00172000
@RF00313 L     @05,BLPUACB(,RWAPARMP)                              0315 00173000
         L     @05,ACBAMBL(,@05)                                   0315 00174000
         L     @05,AMBLIX(,@05)                                    0315 00175000
         ST    @05,MWAAMBPT                                        0315 00176000
*             IF OPWRSTRT=ON THEN   /* IS THIS A VSAM RESTART PATH   */ 00177000
@RC00313 TM    OPWRSTRT(RCORE),B'00001000'                         0316 00178000
         BNO   @RF00316                                            0316 00179000
*               IF BLPFDATA=ON THEN /* IS THIS THE DATA PASS         */ 00180000
         TM    BLPFDATA(RWAPARMP),B'00001000'                      0317 00181000
         BNO   @RF00317                                            0317 00182000
*                 CALL RESTARTN;    /* IF YES, GO TO RESTART RTN     */ 00183000
         BAL   @14,RESTARTN                                        0318 00184000
*               ELSE                /* BYPASS INDEX AMB      @Z30SSDT*/ 00185000
*                 RETCODE=OK;       /* SET RETURN CODE       @Z30SSDT*/ 00186000
         B     @RC00317                                            0319 00187000
@RF00317 SLR   RETCODE,RETCODE                                     0319 00188000
*             ELSE                  /* NORMAL OPEN           @Z30SSDT*/ 00189000
*               CALL STRNGBLD;      /* BUILD STRING BLOCKS   @Z30SSDT*/ 00190000
         B     @RC00316                                            0320 00191000
@RF00316 BAL   @14,STRNGBLD                                        0320 00192000
*           END;                                                   0321 00193000
*                                                                  0321 00194000
@RC00316 DS    0H                                                  0322 00195000
*   /*****************************************************************/ 00196000
*   /*                                                               */ 00197000
*   /* ROUTINE DOES NORMAL DELETION OF WORK AREA'S AS WELL AS        */ 00198000
*   /* DELETING THE RESOURCE POOL STORAGE FOR NORMAL DELVRP AND      */ 00199000
*   /* ABORTING BLDVRP'S                                             */ 00200000
*   /*                                                               */ 00201000
*   /*****************************************************************/ 00202000
*                                                                  0322 00203000
*TERM192Y:                                                         0322 00204000
*   RESPECIFY                                                      0322 00205000
*     RETCODE RESTRICTED;                                          0322 00206000
@RC00310 DS    0H                                                  0322 00207000
@RC00304 DS    0H                                                  0322 00208000
@RC00302 DS    0H                                                  0322 00209000
@RC00293 DS    0H                                                  0322 00210000
TERM192Y DS    0H                                                  0323 00211000
*   IF RETCODE^=GMAINF THEN         /* WAS MODULE WORK AREA OBTAINED */ 00212000
*                                                                  0323 00213000
         LA    @14,8                                               0323 00214000
         CR    RETCODE,@14                                         0323 00215000
         BE    @RF00323                                            0323 00216000
*     /***************************************************************/ 00217000
*     /*                                                             */ 00218000
*     /* MODULE WORK AREA WAS OBTAINED                               */ 00219000
*     /*                                                             */ 00220000
*     /***************************************************************/ 00221000
*                                                                  0324 00222000
*     DO;                           /* FREE WORK AREAS AND POOLS     */ 00223000
*       IF RETCODE=NOSTOR THEN      /* WAS IT A GETMAIN FAILURE      */ 00224000
         C     RETCODE,@CF00086                                    0325 00225000
         BNE   @RF00325                                            0325 00226000
*         RETCODE=GMAINF;           /* CORRECT RETURN CODE           */ 00227000
         LR    RETCODE,@14                                         0326 00228000
*       CALL FREESTOR;              /* FREE STORAGE                  */ 00229000
@RF00325 BAL   @14,FREESTOR                                        0327 00230000
*     END;                          /* END FREE STORAGE              */ 00231000
*                                                                  0328 00232000
*   /*****************************************************************/ 00233000
*   /*                                                               */ 00234000
*   /* NORMAL OS RETURN PROCEDURES ARE EXERCISED. REGISTER 15 HAS THE*/ 00235000
*   /* RETURN CODE LOADED AT THIS POINT AND EXIT IS ACCOMPLISHED VIA */ 00236000
*   /* BR 14.                                                        */ 00237000
*   /*                                                               */ 00238000
*   /*****************************************************************/ 00239000
*                                                                  0329 00240000
*   ELSE                            /* NO MODULE WORKAREA            */ 00241000
*     MWARETCD=RETCODE;             /* LOAD RETURN CODE              */ 00242000
         B     @RC00323                                            0329 00243000
@RF00323 ST    RETCODE,MWARETCD(,RDATA)                            0329 00244000
*   IF(BLPFPRIM=ON                  /* PRIMARY OPEN          @Y30DSDP*/ 00245000
*       |BLPFPART=ON)               /* PARTIAL BUILD         @Y30DSDP*/ 00246000
*       &MWAPPFNC^=PDF70            /* NOT IDA0192W PROBLEM          */ 00247000
*       &MWAPPFNC^=PDF71            /* NOT IDA0192W PROBLEM          */ 00248000
*       &MWARETCD^=ZERO THEN        /* OK?                           */ 00249000
@RC00323 TM    BLPFPRIM(RWAPARMP),B'10010000'                      0330 00250000
         BZ    @RF00330                                            0330 00251000
         CLI   MWAPPFNC(RDATA),70                                  0330 00252000
         BE    @RF00330                                            0330 00253000
         CLI   MWAPPFNC(RDATA),71                                  0330 00254000
         BE    @RF00330                                            0330 00255000
         L     @14,MWARETCD(,RDATA)                                0330 00256000
         LTR   @14,@14                                             0330 00257000
         BZ    @RF00330                                            0330 00258000
*     DO;                           /* TRANSLATE RETURN CODE TO    0331 00259000
*                                      PPCODE                        */ 00260000
*       MWAPPCDE=PP20;              /* SET PPCODE                    */ 00261000
         MVI   MWAPPCDE(RDATA),X'14'                               0332 00262000
*       MWAPPFNC=PDF54;             /* INDIC CONTROL BLK BLD @Z30SSDP*/ 00263000
         MVI   MWAPPFNC(RDATA),X'36'                               0333 00264000
*     END;                                                         0334 00265000
*   LM(RET,R12,MWAUSAVE);           /* LOAD CALLERS REGISTERS        */ 00266000
@RF00330 LM    RET,R12,MWAUSAVE(RDATA)                             0335 00267000
*   GEN(BR 14);                     /* GO BACK TO CALLER             */ 00268000
         BR 14                                                          00269000
*   RESPECIFY                                                      0337 00270000
*    (RET,                                                         0337 00271000
*     RETCODE) UNRSTD;                                             0337 00272000
*INIT192Y:                                                         0338 00273000
*                                                                  0338 00274000
*   /*****************************************************************/ 00275000
*   /*                                                               */ 00276000
*   /* INITIALIZATION AND ADDRESSABILITY THIS ROUTINE PROVIDES       */ 00277000
*   /* STANDARD ENTRANCE CODE AND GETS WORKING STORAGE VIA AN INCLUDE*/ 00278000
*   /* TO IDAGMAIN. RETURN CODE IS CHECKED TO INSURE STORAGE HAS BEEN*/ 00279000
*   /* GOTTEN. ADDRESSABILITY IS SETUP FOR AUTOMATICS AND PARMLIST.  */ 00280000
*   /*                                                               */ 00281000
*   /*****************************************************************/ 00282000
*                                                                  0338 00283000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0338 00284000
INIT192Y DS    0H                                                  0339 00285000
*   RESPECIFY                                                      0339 00286000
*    (R10,                                                         0339 00287000
*     R11,                                                         0339 00288000
*     R13,                                                         0339 00289000
*     RET,                                                         0339 00290000
*     R15,                                                         0339 00291000
*     RETCODE) RSTD;                /*                       @Y30DSDP*/ 00292000
*   R10=RET;                        /* SAVE RETURN ADDRESS           */ 00293000
         LR    R10,RET                                             0340 00294000
*/********************************************************************/ 00295000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 00296000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 00297000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 00298000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 00299000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 00300000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 00301000
*/********************************************************************/ 00302000
*                                                                  0341 00303000
*   GEN;                                                           0341 00304000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00305000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00306000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00307000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00308000
*                                       IF NO ERROR WAS ENCOUNTERED     00309000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00310000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00311000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00312000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00313000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00314000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00315000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00316000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00317000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00318000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00319000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00320000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00321000
         B     GMN00200                 BYPASS SETTING ERROR            00322000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00323000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00324000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00325000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00326000
*   IF R15=ZERO THEN                /* IF GETMAIN FAILED     @ZA11622*/ 00327000
         LTR   R15,R15                                             0342 00328000
         BNZ   @RF00342                                            0342 00329000
*     RETURN TO TERM192Y CODE(GMAINF);/* TERMINATE WITH STORAGE    0343 00330000
*                                      ERRORS                        */ 00331000
*                                                                  0343 00332000
         LA    @14,TERM192Y                                        0343 00333000
         LA    @15,8                                               0343 00334000
@EL00002 DS    0H                                                  0343 00335000
@EF00002 DS    0H                                                  0343 00336000
@ER00002 BR    @14                                                 0343 00337000
*   /*****************************************************************/ 00338000
*   /*                                                               */ 00339000
*   /* SAVE AREA CHAINING                                            */ 00340000
*   /*                                                               */ 00341000
*   /*****************************************************************/ 00342000
*                                                                  0344 00343000
*   MWAFWCH=R15;                    /* R15 EQUALS THE ADDRESS OF   0344 00344000
*                                      GOTTEN STORAGE FROM IDAGMAIN  */ 00345000
@RF00342 ST    R15,MWAFWCH(,RDATA)                                 0344 00346000
*   R11=RDATA;                      /* SAVE CALLERS SAVE AREA        */ 00347000
         LR    R11,RDATA                                           0345 00348000
*   RDATA=R15;                      /* GET OUR SAVE AREA IN DATA REG */ 00349000
         LR    RDATA,R15                                           0346 00350000
*   MWABKCH=R11;                    /* BACK CHAIN                    */ 00351000
         ST    R11,MWABKCH(,RDATA)                                 0347 00352000
*   RESPECIFY                                                      0348 00353000
*    (R11,                                                         0348 00354000
*     RET,                                                         0348 00355000
*     R15,                                                         0348 00356000
*     RETCODE) UNRSTD;              /*                       @Y30DSDP*/ 00357000
*   C3=ZERO;                        /* ZERO SAVE AREA INDEX          */ 00358000
         SLR   @11,@11                                             0349 00359000
         ST    @11,C3                                              0349 00360000
*   MWAWA=RCORE;                    /* SAVE PTR COMWA        @Y30DSDP*/ 00361000
         ST    RCORE,MWAWA                                         0350 00362000
*   MWAJSCBP=TCBJSCB;               /* SET POINTER TO JSCB           */ 00363000
         L     @11,PSATOLD                                         0351 00364000
         L     @11,TCBJSCB(,@11)                                   0351 00365000
         ST    @11,MWAJSCBP                                        0351 00366000
*   IF(BLPFDBDC=ON|BLPFIOSR=ON|     /* CHK DBDC OR IOSUP     @Y30DSDP*/ 00367000
*       BLPFSTAD=ON) THEN           /* OR STRING ADDITION    @Y30DSDP*/ 00368000
         TM    BLPFDBDC(RWAPARMP),B'10000000'                      0352 00369000
         BO    @RT00352                                            0352 00370000
         TM    BLPFIOSR(RWAPARMP),B'00000010'                      0352 00371000
         BO    @RT00352                                            0352 00372000
         TM    BLPFSTAD(RWAPARMP),B'00000001'                      0352 00373000
         BNO   @RF00352                                            0352 00374000
@RT00352 DS    0H                                                  0353 00375000
*     DO;                           /* GET OPEN WORK                 */ 00376000
*       RESPECIFY                                                  0354 00377000
*        (R0,                                                      0354 00378000
*         R1,                                                      0354 00379000
*         R4,                                                      0354 00380000
*         R5,                                                      0354 00381000
*         R14,                                                     0354 00382000
*         R15) RSTD;                                               0354 00383000
*       R4=PROTPOOL;                /* SET PROTECTED SUBPOOL         */ 00384000
         LA    R4,252                                              0355 00385000
*       R5=LENGTH(IDAOPWRK);        /* SET LENGTH            @Y30DSDP*/ 00386000
         LA    R5,588                                              0356 00387000
*       GEN(GETMAIN RC,LV=(R5),SP=(R4))                            0357 00388000
*       SETS(RETCODE);              /* ISSUE GETMAIN         @Y30DSDP*/ 00389000
         GETMAIN RC,LV=(R5),SP=(R4)                                     00390000
*       IF RETCODE^=OK THEN         /* WAS STORAGE OBTAINED          */ 00391000
         LTR   RETCODE,RETCODE                                     0358 00392000
         BZ    @RF00358                                            0358 00393000
*         RETURN TO TERM192Y CODE(NOSTOR);/* TERMINATE               */ 00394000
         LA    @14,TERM192Y                                        0359 00395000
         LA    @15,7                                               0359 00396000
         BR    @14                                                 0359 00397000
*       R4=R1;                      /* LOAD ADDRESSABILITY POINTER 0360 00398000
*                                                            @Z30SSDT*/ 00399000
@RF00358 LR    R4,R1                                               0360 00400000
*       MVCL(R4,R14);               /* ZERO GOTTEN CORE      @Z30SSDT*/ 00401000
         MVCL  R4,R14                                              0361 00402000
*       RCORE=R1;                   /* RESTORE CORE PTR      @Y30DSDP*/ 00403000
         LR    RCORE,R1                                            0362 00404000
*       RFY                                                        0363 00405000
*        (R0,                                                      0363 00406000
*         R1,                                                      0363 00407000
*         R5,                                                      0363 00408000
*         R14,                                                     0363 00409000
*         R15) UNRSTD;              /*                       @Z30SSDT*/ 00410000
*       OPWCOMWA=MWAWA;             /* SET COMWA PTR         @Y30DSDP*/ 00411000
         L     @11,MWAWA                                           0364 00412000
         ST    @11,OPWCOMWA(,RCORE)                                0364 00413000
*       IF BLPFDBDC=ON THEN         /* DBDC                  @Y30DSDP*/ 00414000
         TM    BLPFDBDC(RWAPARMP),B'10000000'                      0365 00415000
         BNO   @RF00365                                            0365 00416000
*         DO;                       /* GET MAIN FOR PG FIX WORK AREA    00417000
*                                                            @Z30SSDP*/ 00418000
*           MWACSLP=ADDR(MWAPGFFM); /* PT TO PAGE FIX FM     @Z30SSDP*/ 00419000
         LA    @11,MWAPGFFM                                        0367 00420000
         ST    @11,MWACSLP                                         0367 00421000
*           OPWVMSP=FIXPOOL;        /* INDIC FIXED CORE      @Z30SSDP*/ 00422000
         MVI   OPWVMSP(RCORE),X'F5'                                0368 00423000
*           OPWVMLNG=LENGTH(PFL)+LENGTH(MWAECB);/* LOAD LENGTH OF PFL*/ 00424000
         MVC   OPWVMLNG(3,RCORE),@CF00034+1                        0369 00425000
*           CALL GETM192Y;          /* GET REQUIRED STORAGE  @Z30SSDP*/ 00426000
         BAL   @14,GETM192Y                                        0370 00427000
*           MWAPGFP=OPWVMADR;       /* SET POINTER                   */ 00428000
