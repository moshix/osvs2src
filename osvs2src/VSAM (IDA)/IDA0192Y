         TITLE 'IDA0192Y - VS2 VSAM STRING BUILD                       *00001000
                        '                                               00002000
IDA0192Y CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192Y  78.151'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @08,4095(,@03)                                      0001 00012000
         LA    @12,4095(,@08)                                      0001 00013000
         USING @PSTART,@03                                         0001 00014000
         USING @PSTART+4095,@08                                    0001 00015000
         USING @PSTART+8190,@12                                    0001 00016000
JSCBDISP EQU 188                                                        00017000
JSCBAOS1 EQU  72                                                        00018000
JSCBAOS2 EQU 192                                                        00019000
RBPRFXLN EQU 64                                                         00020000
RBPRFLNA EQU 32                                                         00021000
PRBLEN   EQU 136                                                        00022000
SIRBLEN  EQU 200                                                        00023000
TIRBLEN  EQU 136                                                        00024000
IRBLEN   EQU 128                                                        00025000
SVRBLEN  EQU 224                                                        00026000
*                                      SAVE AREA                     */ 00027000
         USING @DATD,RDATA                                              00028000
*   CALL INIT192Y;                  /* INITIALIZE MODULE WORK AREA   */ 00029000
*                                                                  0292 00030000
         BAL   @14,INIT192Y                                        0292 00031000
*   /*****************************************************************/ 00032000
*   /*                                                               */ 00033000
*   /* TEST FOR DBDC IF NOT DBDC GO TO STRINGRTN                     */ 00034000
*   /*                                                               */ 00035000
*   /*****************************************************************/ 00036000
*                                                                  0293 00037000
*   IF BLPFDBDC=ON THEN             /* IF DBDC IS CALLER     @Y30DSDP*/ 00038000
