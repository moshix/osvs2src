*       LOCKSAVC=R1;                /* SAVE R1               @Y30DSDP*/ 06174000
         LR    LOCKSAVC,R1                                         1909 06175000
*       R1=R13;                     /* SAVE R13              @Y30DSDP*/ 06176000
         LR    R1,R13                                              1910 06177000
*       GEN SETS(R11,R12,R13,RET) REFS(PSALITA,FLC);               1911 06178000
*                                                          /*@Y30DSDP*/ 06179000
