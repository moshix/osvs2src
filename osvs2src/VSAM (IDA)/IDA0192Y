         LA    MWAPLHNX,16                                         1969 06373000
         AL    MWAPLHNX,AMBUPLH(,@05)                              1969 06374000
*           MWASTRNO=ONE;           /* SET STRING NUMBER TO ONE      */ 06375000
         LA    MWASTRNO,1                                          1970 06376000
*         END;                                                     1971 06377000
*       ELSE                        /* OTHERWISE,                    */ 06378000
*         DO;                       /* GET NORMAL PLH                */ 06379000
         B     @RC01967                                            1972 06380000
@RF01967 DS    0H                                                  1973 06381000
*           MWAPLHNX=AMBPH+LENGTH(IDAPLHDR);/* POINT TO FIRST PLH    */ 06382000
