*   RESPECIFY                                                      0983 02935000
*     RET UNRSTD;                   /*                       @X04SVCJ*/ 02936000
*   RESPECIFY                                                      0984 02937000
*     TCB BASED(DEBTCBB);                                          0984 02938000
*   CSWORD1=K0;                     /* INIT LINK ADDR        @Z30SSDT*/ 02939000
         SLR   CSWORD1,CSWORD1                                     0985 02940000
*   CSWORD2=ADDR(DEBBASIC);         /* INIT ADDR OF DEB      @X04SVCJ*/ 02941000
