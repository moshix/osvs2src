*   END CHNBLKS;                                                   0820 02310000
         B     @EL00009                                            0820 02311000
*BLDEDB:                                                           0821 02312000
*                                                                  0821 02313000
*/********************************************************************/ 02314000
*/*                                                                  */ 02315000
*/*   FUNCTION -  THIS ROUTINE BUILDS THE EDBS                       */ 02316000
*/*   INPUT -        CATALOG WORK AREA (AMBWKA)                      */ 02317000
*/*   OUTPUT -    1. EDBS ARE BUILT                          @Z30SSDT*/ 02318000
*/*               2. EDBAD IS SET TO ADDRESS OF EDB HEADER   @Z30SSDT*/ 02319000
*/*               3. CUREXTNO IS SET TO NUMBER OF EXTENTS            */ 02320000
*/*                                                                  */ 02321000
*/********************************************************************/ 02322000
*                                                                  0821 02323000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0821 02324000
BLDEDB   DS    0H                                                  0822 02325000
*   RESPECIFY                                                      0822 02326000
*    (RET,                                                         0822 02327000
*     R6,                                                          0822 02328000
*     R7,                                                          0822 02329000
*     R8,                                                          0822 02330000
*     R9) RSTD;                     /*                       @X04SVCJ*/ 02331000
*   RETCONT=RETCONT+K1;             /* BUMP CONTROL BY ONE   @Z30SSRL*/ 02332000
         LA    @15,1                                               0823 02333000
         L     @11,RETCONT                                         0823 02334000
         ALR   @11,@15                                             0823 02335000
         ST    @11,RETCONT                                         0823 02336000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS   @Z30SSDT*/ 02337000
         SLA   @11,2                                               0824 02338000
         ST    RET,RETSAVE-4(@11)                                  0824 02339000
*   RESPECIFY                                                      0825 02340000
*     RET UNRSTD;                   /*                       @X04SVCJ*/ 02341000
*   R6=ADDR(AMBWORK)+LENGTH(AMBWORK);                              0826 02342000
         LA    R6,232                                              0826 02343000
         AL    R6,OPWWRKPT(,RCORE)                                 0826 02344000
*   RESPECIFY                                                      0827 02345000
*     CTVOLENT BASED(R6);           /* BASE FOR VOL ENT              */ 02346000
*   RESPECIFY                                                      0828 02347000
*     CTKEYENT BASED(R7);           /* BASE FOR KEY ENT              */ 02348000
*   RESPECIFY                                                      0829 02349000
*     CTEXTENT BASED(R8);           /* BASE FOR EXTENT ENT           */ 02350000
*   CUREXTNO=K0;                    /* ZERO EXTENT COUNT     @Z30SSRL*/ 02351000
