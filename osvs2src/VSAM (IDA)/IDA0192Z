         TITLE 'IDA0192Z - VS2 VSAM CONTROL BLOCK BUILD                *00001000
                        '                                               00002000
IDA0192Z CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0192Z  78.199'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @12,4095(,@03)                                      0001 00012000
         USING @PSTART,@03                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
JSCBDISP EQU 188                                                        00015000
JSCBAOS1 EQU  72                                                        00016000
JSCBAOS2 EQU 192                                                        00017000
RBPRFXLN EQU 64                                                         00018000
RBPRFLNA EQU 32                                                         00019000
PRBLEN   EQU 136                                                        00020000
SIRBLEN  EQU 200                                                        00021000
TIRBLEN  EQU 136                                                        00022000
IRBLEN   EQU 128                                                        00023000
SVRBLEN  EQU 224                                                        00024000
*                                                                  0224 00025000
*   /*****************************************************************/ 00026000
*   /*                                                               */ 00027000
*   /* IDA0192Z - INITIALIZATION                                     */ 00028000
*   /*                                                               */ 00029000
*   /*****************************************************************/ 00030000
*                                                                  0224 00031000
*   RSTATRTN=IDAR192Z;              /* NOTE 192Z IN CONTROL  @Z30SSDT*/ 00032000
         L     @05,OPWCOMWA(,RCORE)                                0224 00033000
         MVC   RSTATRTN(4,@05),@CC05338                            0224 00034000
*/********************************************************************/ 00035000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 00036000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 00037000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 00038000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 00039000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 00040000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 00041000
*/********************************************************************/ 00042000
*                                                                  0225 00043000
*   GEN;                                                           0225 00044000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00045000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00046000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00047000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00048000
*                                       IF NO ERROR WAS ENCOUNTERED     00049000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00050000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00051000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00052000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00053000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00054000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00055000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00056000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00057000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00058000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00059000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00060000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00061000
         B     GMN00200                 BYPASS SETTING ERROR            00062000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00063000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00064000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00065000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00066000
*   RESPECIFY                                                      0226 00067000
*    (R15,                                                         0226 00068000
*     R13) RSTD;                    /*                       @X04SVCJ*/ 00069000
*   PD=ZERO;                        /* ZERO RETURN CODE      @X04SVCJ*/ 00070000
         SLR   @14,@14                                             0227 00071000
         ST    @14,PD(,R13)                                        0227 00072000
*   IF R15=ZERO THEN                /* IF NO CORE AVAILABLE  @X04SVCJ*/ 00073000
         CR    R15,@14                                             0228 00074000
         BNE   @RF00228                                            0228 00075000
*     DO;                           /* ERROR                 @X04SVCJ*/ 00076000
*       OPWMODWK=OFF;               /* NO MODULE WORK AREA   @Z30SSDT*/ 00077000
         L     @14,OPWCURPT(,RCORE)                                0230 00078000
         NI    OPWMODWK(@14),B'10111111'                           0230 00079000
*       PPCODE=PP20;                /* SET PROBLEM CODE      @X04SVCJ*/ 00080000
         MVI   PPCODE(R13),X'14'                                   0231 00081000
*       PPFUNC=PDF50;               /* SET FUNCTION CODE     @X04SVCJ*/ 00082000
         MVI   PPFUNC(R13),X'32'                                   0232 00083000
*     END;                          /*                       @X04SVCJ*/ 00084000
*   ELSE                            /* NO ERROR              @X04SVCJ*/ 00085000
*     DO;                           /* MAINLINE              @X04SVCJ*/ 00086000
         B     @RC00228                                            0234 00087000
@RF00228 DS    0H                                                  0235 00088000
*       PPFUNC=PDF53;               /* SET FUNCTION CODE     @Z30SSDT*/ 00089000
         MVI   PPFUNC(R13),X'35'                                   0235 00090000
*       R15->SABWRD=R13;            /* SAVE SAVE AREA ADDR   @Z30SSDT*/ 00091000
         ST    R13,SABWRD(,R15)                                    0236 00092000
*       R13=R15;                    /* LOAD PTR TO AUTOMATICS      0237 00093000
*                                                            @Z30SSDT*/ 00094000
         LR    R13,R15                                             0237 00095000
*       GEN(USING @DATD,R13);       /* GET BASE FOR AUTOMATICS       */ 00096000
         USING @DATD,R13                                                00097000
*       RETCONT=ZERO;               /* INIT ARRAY CONTROL    @Z30SSDT*/ 00098000
         SLR   @14,@14                                             0239 00099000
         ST    @14,RETCONT                                         0239 00100000
*       RESPECIFY                                                  0240 00101000
*         R15 UNRESTRICTED;         /*                       @Z30SSDT*/ 00102000
*       IF CVTSMCA^=ZERO&           /* IF SMCA EXIST AND     @OZ04709*/ 00103000
*           SMCADSA=ON&             /* DATA SET INFORMATION REQUIRED    00104000
*                                      AND                   @OZ04709*/ 00105000
*           SMCAUSER=ON&SMCAMAN=ON& /* SMF RECORDING AND     @OZ04709*/ 00106000
*           TCBTCT^=ZERO THEN       /* THE TCT EXIST         @OZ04709*/ 00107000
         L     @05,FLCCVT                                          0241 00108000
         L     @05,CVTSMCA(,@05)                                   0241 00109000
         CR    @05,@14                                             0241 00110000
         BE    @RF00241                                            0241 00111000
         TM    SMCADSA(@05),B'00010000'                            0241 00112000
         BNO   @RF00241                                            0241 00113000
         TM    SMCAUSER(@05),B'11000000'                           0241 00114000
         BNO   @RF00241                                            0241 00115000
         L     @05,PSATOLD                                         0241 00116000
         C     @14,TCBTCT(,@05)                                    0241 00117000
         BE    @RF00241                                            0241 00118000
*         SMFACTV=ON;               /* INDICATE SMF ACTIVE   @OZ04709*/ 00119000
         OI    SMFACTV,B'00001000'                                 0242 00120000
*       DO C1=AMBLNUM TO 1 BY-1;    /* LOOP THRU AMB LIST            */ 00121000
*                                                                  0243 00122000
@RF00241 L     @05,ACBAMBL(,RACB)                                  0243 00123000
         LH    @05,AMBLNUM(,@05)                                   0243 00124000
         ST    @05,C1                                              0243 00125000
         B     @DE00243                                            0243 00126000
@DL00243 DS    0H                                                  0244 00127000
*         /***********************************************************/ 00128000
*         /*                                                         */ 00129000
*         /* BUILD BLOCKS PER AMB                                    */ 00130000
*         /*                                                         */ 00131000
*         /***********************************************************/ 00132000
*                                                                  0244 00133000
*         IF C1=AMBLNUM THEN        /* IF DATA PASS          @X04SVCJ*/ 00134000
         L     @01,ACBAMBL(,RACB)                                  0244 00135000
         CH    @05,AMBLNUM(,@01)                                   0244 00136000
         BNE   @RF00244                                            0244 00137000
*           OPWWRKPT=OPWDTWRK;      /* INIT AMB WORK POINTER @X04SVCJ*/ 00138000
         L     @05,OPWDTWRK(,RCORE)                                0245 00139000
         ST    @05,OPWWRKPT(,RCORE)                                0245 00140000
*         ELSE                      /* INDEX PASS            @X04SVCJ*/ 00141000
*           OPWWRKPT=OPWIXWRK;      /* INIT AMB WORK POINTER @X04SVCJ*/ 00142000
         B     @RC00244                                            0246 00143000
@RF00244 L     @05,OPWIXWRK(,RCORE)                                0246 00144000
         ST    @05,OPWWRKPT(,RCORE)                                0246 00145000
*         IF OPWRSTRT=ON THEN       /* IF A RESTART          @Z30SSDT*/ 00146000
@RC00244 TM    OPWRSTRT(RCORE),B'00001000'                         0247 00147000
         BNO   @RF00247                                            0247 00148000
*           CALL RESTINIT;          /* SETUP FOR RESTART     @Z30SSDT*/ 00149000
         BAL   @14,RESTINIT                                        0248 00150000
*         ELSE                      /* IF NOT A RESTART      @Z30SSDT*/ 00151000
*           DO;                     /* NORMAL OPEN PATH      @Z30SSDT*/ 00152000
         B     @RC00247                                            0249 00153000
@RF00247 DS    0H                                                  0250 00154000
*             CALL BLDAMB;          /* BUILD AN AMB          @Z30SSDT*/ 00155000
         BAL   @14,BLDAMB                                          0250 00156000
*             CALL BLDRMEXT;        /* BUILD RECORD MGT EXT  @Z30SSDT*/ 00157000
         BAL   @14,BLDRMEXT                                        0251 00158000
*             CALL BLDAMDSB;        /* BUILD AN AMDSB        @Z30SSDT*/ 00159000
         BAL   @14,BLDAMDSB                                        0252 00160000
*             CALL BLDIWA;          /* BUILD INSERT WORK AREA        */ 00161000
         BAL   @14,BLDIWA                                          0253 00162000
*             CALL BLDARDB;         /* BUILD ARDB            @Z30SSDT*/ 00163000
         BAL   @14,BLDARDB                                         0254 00164000
*             CALL BLDLPMB;         /* BUILD LPMB            @Z30SSDT*/ 00165000
         BAL   @14,BLDLPMB                                         0255 00166000
*           END;                    /*                       @Z30SSDT*/ 00167000
*         CALL BLDEDB;              /* BUILD EDB             @Z30SSDT*/ 00168000
@RC00247 BAL   @14,BLDEDB                                          0257 00169000
*         CALL BLDRDEB;             /* BUILD THE REAL DEB            */ 00170000
         BAL   @14,BLDRDEB                                         0258 00171000
*         IF OPWLSR=OFF&OPWGSR=OFF THEN/* NOT SHRD RESOURCES @Z30SSDT*/ 00172000
         TM    OPWLSR(RCORE),B'01100000'                           0259 00173000
         BNZ   @RF00259                                            0259 00174000
*           CALL BLDSTRBK;          /* BUILD STRING BLOCKS   @X04SVCJ*/ 00175000
         BAL   @14,BLDSTRBK                                        0260 00176000
*         ELSE                      /* SHARED RESOURCES      @Z30SSDT*/ 00177000
*           IF OPWRSTRT=OFF THEN    /* IF NOT A RESTART      @Z30SSDT*/ 00178000
         B     @RC00259                                            0261 00179000
@RF00259 TM    OPWRSTRT(RCORE),B'00001000'                         0261 00180000
         BNZ   @RF00261                                            0261 00181000
*             CALL CHNBLKS;         /* CHAIN CONTROL BLOCKS  @Z30SSDT*/ 00182000
         BAL   @14,CHNBLKS                                         0262 00183000
*       END;                                                       0263 00184000
*                                                                  0263 00185000
@RF00261 DS    0H                                                  0263 00186000
@RC00259 SLR   @05,@05                                             0263 00187000
         BCTR  @05,0                                               0263 00188000
         AL    @05,C1                                              0263 00189000
         ST    @05,C1                                              0263 00190000
@DE00243 LTR   @05,@05                                             0263 00191000
         BP    @DL00243                                            0263 00192000
*       /*************************************************************/ 00193000
*       /*                                                           */ 00194000
*       /* IDA0192Z - FINAL PROCESSING                               */ 00195000
*       /*                                                           */ 00196000
*       /*************************************************************/ 00197000
*                                                                  0264 00198000
*       RESPECIFY                                                  0264 00199000
*         RDEB RESTRICTED;                                         0264 00200000
*       IF AMBLCAT=OFF THEN         /* CAT, SCRA OR VVIC     @Z30SSDT*/ 00201000
         L     @10,ACBAMBL(,RACB)                                  0265 00202000
         TM    AMBLCAT(@10),B'00000010'                            0265 00203000
         BNZ   @RF00265                                            0265 00204000
*         DO C1=AMBLNUM TO 1 BY-1;  /* LOOP THRU AMB LIST            */ 00205000
         LH    @10,AMBLNUM(,@10)                                   0266 00206000
         ST    @10,C1                                              0266 00207000
         B     @DE00266                                            0266 00208000
@DL00266 DS    0H                                                  0267 00209000
*           AMBP=(ACBAMBL+(C1-K1)*LENGTH(AMBLDTA))->AMBLDTA;/* GET 0267 00210000
*                                      AMB                           */ 00211000
         BCTR  @10,0                                               0267 00212000
         SLA   @10,2                                               0267 00213000
         AL    @10,ACBAMBL(,RACB)                                  0267 00214000
         L     @10,AMBLDTA(,@10)                                   0267 00215000
         ST    @10,AMBP                                            0267 00216000
*           RDEB=AMBDEBAD;          /* LOAD PTR TO DEB               */ 00217000
         L     RDEB,AMBDEBAD-1(,@10)                               0268 00218000
         LA    RDEB,0(,RDEB)                                       0268 00219000
*           CALL HOOKDEB;           /* CHAIN THE DEB                 */ 00220000
         BAL   @14,HOOKDEB                                         0269 00221000
*         END;                                                     0270 00222000
         SLR   @10,@10                                             0270 00223000
         BCTR  @10,0                                               0270 00224000
         AL    @10,C1                                              0270 00225000
         ST    @10,C1                                              0270 00226000
@DE00266 LTR   @10,@10                                             0270 00227000
         BP    @DL00266                                            0270 00228000
*       RESPECIFY                                                  0271 00229000
*         RDEB UNRESTRICTED;                                       0271 00230000
@RF00265 DS    0H                                                  0272 00231000
*                                                                  0272 00232000
*       /*************************************************************/ 00233000
*       /*                                                           */ 00234000
*       /* EXIT FROM IDA0192Z                                        */ 00235000
*       /*                                                           */ 00236000
*       /*************************************************************/ 00237000
*                                                                  0272 00238000
*EXIT:                                                             0272 00239000
*       RESPECIFY                                                  0272 00240000
*        (R0,                                                      0272 00241000
*         R1) RSTD;                 /*                       @Z30SSDT*/ 00242000
EXIT     DS    0H                                                  0273 00243000
*       GEN(L R0,@SIZDATD);         /* LOAD BLOCK SIZE       @Z30SSDT*/ 00244000
         L R0,@SIZDATD                                                  00245000
*       R1=R13;                     /* SET ADDRESS           @Z30SSDT*/ 00246000
         LR    R1,R13                                              0274 00247000
*       R13=SABWRD;                 /* POINT TO LAST SAVE AREA     0275 00248000
*                                                            @Z30SSDT*/ 00249000
         L     R13,SABWRD(,R13)                                    0275 00250000
*       IF PPCODE=K0 THEN           /* ERRORS                @Z30SSRL*/ 00251000
         CLI   PPCODE(R13),0                                       0276 00252000
         BNE   @RF00276                                            0276 00253000
*         PPFUNC=K0;                /* NO, ZERO RETURN CODE  @Z30SSRL*/ 00254000
         MVI   PPFUNC(R13),X'00'                                   0277 00255000
*       GEN(FREEMAIN R,LV=(0),A=(1));/*                      @X04SVCJ*/ 00256000
@RF00276 DS    0H                                                  0278 00257000
         FREEMAIN R,LV=(0),A=(1)                                        00258000
*       RESPECIFY                                                  0279 00259000
*        (R0,                                                      0279 00260000
*         R1) UNRESTRICTED;         /*                       @Z30SSDT*/ 00261000
*     END;                          /*                       @X04SVCJ*/ 00262000
*   LM(RET,R12,SAREGS);             /* RESTORE REGISTERS 14-12     0281 00263000
*                                                            @Z30SSDT*/ 00264000
@RC00228 LM    RET,R12,SAREGS(R13)                                 0281 00265000
*   GEN(BR RET);                    /* RETURN TO CALLER              */ 00266000
         BR RET                                                         00267000
*   RESPECIFY                                                      0283 00268000
*     SAVAREA BASED(R13->SABWRD);   /* ADDRESSIBILITY FOR PD @Z30SSDT*/ 00269000
*BLDAMB:                                                           0284 00270000
*                                                                  0284 00271000
*/********************************************************************/ 00272000
*/*                                                                  */ 00273000
*/*   FUNCTION -  THIS ROUTINE BUILDS AN AMB                         */ 00274000
*/*   INPUT -     1. CATALOG WORKAREA FOR COMPONENT DATA SET         */ 00275000
*/*               2. ACB                                             */ 00276000
*/*               3. AMBL                                            */ 00277000
*/*   OUTPUT -    1. AMB IS BUILT IN PROTECT BLOCK           @Z30SSDT*/ 00278000
*/*               2. FIELDS ARE INITIALIZED TO REFLECT               */ 00279000
*/*                  CURRENT PROCESSING                              */ 00280000
*/*               3. AMBP IS LOADED WITH AMB ADDRESS         @X04SVCJ*/ 00281000
*/*               4. AMBAD IS SET TO AMB ADDRESS IN AUTOMATICS       */ 00282000
*/*                                                                  */ 00283000
*/********************************************************************/ 00284000
*                                                                  0284 00285000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0284 00286000
BLDAMB   DS    0H                                                  0285 00287000
*   RESPECIFY                                                      0285 00288000
*     RET RSTD;                     /*                       @X04SVCJ*/ 00289000
*   RFY                                                            0286 00290000
*     IDAAMDSB BASED(ADDR(CTAMDSB));/* NOT IN AMB YET        @ZA18252*/ 00291000
*   RETCONT=RETCONT+K1;             /* BUMP CONTROL BY ONE   @Z30SSRL*/ 00292000
         LA    @15,1                                               0287 00293000
         AL    @15,RETCONT                                         0287 00294000
         ST    @15,RETCONT                                         0287 00295000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS   @Z30SSDT*/ 00296000
         SLA   @15,2                                               0288 00297000
         ST    RET,RETSAVE-4(@15)                                  0288 00298000
*   RESPECIFY                                                      0289 00299000
*     RET UNRSTD;                   /*                       @X04SVCJ*/ 00300000
*   OPWVMLNG=LENGTH(IDAAMB);        /* INIT FOR GET CORE REQUEST   0290 00301000
*                                                            @Z30SSDT*/ 00302000
         MVC   OPWVMLNG(3,RCORE),@CF02849+1                        0290 00303000
*   OPWVMTYP=PROTBLK;               /* INDICATE PROTECT BLOCK      0291 00304000
*                                                            @Z30SSDT*/ 00305000
         MVI   OPWVMTYP(RCORE),X'02'                               0291 00306000
*   OPWVMANC=ADDR(IDACMB);          /* SET ADDRESS OF ANCHOR BLOCK 0292 00307000
*                                                            @X04SVCJ*/ 00308000
         L     @05,ACBAMBL(,RACB)                                  0292 00309000
         L     @05,AMBLCMB(,@05)                                   0292 00310000
         ST    @05,OPWVMANC(,RCORE)                                0292 00311000
*   OPWVMFLG=ZERO;                  /* RESET FLAG FIELD      @X04SVCJ*/ 00312000
         MVI   OPWVMFLG(RCORE),X'00'                               0293 00313000
*   CALL IDA0192M;                  /* GET CORE FOR AMB      @Z30SSDT*/ 00314000
         L     @15,@CV05525                                        0294 00315000
         BALR  @14,@15                                             0294 00316000
*   RSTATRTN=IDAR192Z;              /* NOTE 192Z IN CONTROL  @Z30SSDT*/ 00317000
         L     @05,OPWCOMWA(,RCORE)                                0295 00318000
         MVC   RSTATRTN(4,@05),@CC05338                            0295 00319000
*   IF OPWVMADR=ZERO THEN           /* WAS STORAGE OBTAINED  @X04SVCJ*/ 00320000
         L     @05,OPWVMADR(,RCORE)                                0296 00321000
         LTR   @05,@05                                             0296 00322000
         BNZ   @RF00296                                            0296 00323000
*     DO;                           /* ERROR                 @X04SVCJ*/ 00324000
*       PPCODE=PP20;                /* SET ERROR CODE        @X04SVCJ*/ 00325000
         L     @05,SABWRD(,R13)                                    0298 00326000
         MVI   PPCODE(@05),X'14'                                   0298 00327000
*       RETURN TO EXIT;             /* TERMINATE             @Z30SSDT*/ 00328000
         LA    @14,EXIT                                            0299 00329000
@EL00002 DS    0H                                                  0299 00330000
@EF00002 DS    0H                                                  0299 00331000
@ER00002 BR    @14                                                 0299 00332000
*     END;                          /*                       @X04SVCJ*/ 00333000
*   AMBAD=OPWVMADR;                 /* SAVE AMB ADDRESS      @X04SVCJ*/ 00334000
@RF00296 L     @05,OPWVMADR(,RCORE)                                0301 00335000
         ST    @05,AMBAD                                           0301 00336000
*   AMBP=AMBAD;                     /* LOAD AMB POINTER      @X04SVCJ*/ 00337000
         ST    @05,AMBP                                            0302 00338000
*   AMBDORGA=ON;                    /* DSORG INDICATES AM/0          */ 00339000
         OI    AMBDORGA(@05),B'00001000'                           0303 00340000
*   AMBEXFG=AMBEXOFF;               /* SET LOCK BIT OFF              */ 00341000
         OI    AMBEXFG(@05),B'00000010'                            0304 00342000
*   AMBCDSN=CTHSHCAT;               /* INIT CATALOG NAME, HASHED     */ 00343000
         L     @15,OPWWRKPT(,RCORE)                                0305 00344000
         L     @14,CTHSHCAT(,@15)                                  0305 00345000
         STCM  @14,7,AMBCDSN(@05)                                  0305 00346000
*   AMBCACB=CTHSHCAT;               /* SAVE CATL ACB PTR             */ 00347000
         ST    @14,AMBCACB(,@05)                                   0306 00348000
*   AMBDDSN=CTHSHDSN;               /* INIT AMD DATA SET NAME        */ 00349000
         MVC   AMBDDSN(3,@05),CTHSHDSN(@15)                        0307 00350000
*   IF CTEXEX^=NOEXEXIT THEN        /* EXCEPTION EXIT EXIST  @Z30SSDT*/ 00351000
         CLC   CTEXEX(8,@15),NOEXEXIT                              0308 00352000
         BE    @RF00308                                            0308 00353000
*     AMBEXEX=CTEXEX;               /* INITIALIZE EXIT NAME  @Z30SSDT*/ 00354000
         MVC   AMBEXEX(8,@05),CTEXEX(@15)                          0309 00355000
*   IF AMBLCAT=OFF THEN             /* IF NOT SYSTEM DS      @Z30SSDT*/ 00356000
@RF00308 L     @05,ACBAMBL(,RACB)                                  0310 00357000
         TM    AMBLCAT(@05),B'00000010'                            0310 00358000
         BNZ   @RF00310                                            0310 00359000
*     CALL TIOTSRCH;                /* THEN DETERMINE OFFSET OF TIOT    00360000
*                                      ENTRY                 @X04SVRP*/ 00361000
         BAL   @14,TIOTSRCH                                        0311 00362000
*   ELSE                            /* ELSE SYSTEM DATA SET  @X04SVRP*/ 00363000
*     AMBTIOT=ACBTIOT;              /* INIT TIOT OFFSET      @X04SVRP*/ 00364000
         B     @RC00310                                            0312 00365000
@RF00310 LH    @05,ACBTIOT(,RACB)                                  0312 00366000
         L     @01,AMBP                                            0312 00367000
         STH   @05,AMBTIOT(,@01)                                   0312 00368000
*   AMBID=IDXAMB;                   /* INIT AMB ID                   */ 00369000
@RC00310 L     @05,AMBP                                            0313 00370000
         MVI   AMBID(@05),B'01000000'                              0313 00371000
*   AMBLEN=LENGTH(IDAAMB);          /* INIT AMB LENGTH               */ 00372000
         MVC   AMBLEN(2,@05),@CH02849                              0314 00373000
*   IF ACBCAT=ON THEN               /* IF CATALOG INDICATED  @Z30SSRL*/ 00374000
         TM    ACBCAT(RACB),B'00010000'                            0315 00375000
         BNO   @RF00315                                            0315 00376000
*     AMBCAT=ON;                    /* SET CATALOG BIT ON    @Z30SSRL*/ 00377000
         OI    AMBCAT(@05),B'00010000'                             0316 00378000
*   IF ACBSCRA=ON THEN              /* IF SCRA INDICATED     @Z30SSRL*/ 00379000
@RF00315 TM    ACBSCRA(RACB),B'00001000'                           0317 00380000
         BNO   @RF00317                                            0317 00381000
*     AMBSCRA=ON;                   /* SET SCRA BIT ON       @Z30SSRL*/ 00382000
         L     @05,AMBP                                            0318 00383000
