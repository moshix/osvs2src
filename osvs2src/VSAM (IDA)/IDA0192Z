         ALR   @01,@15                                             0797 02224000
         SLA   @01,3                                               0797 02225000
         STH   @01,AMBSZWR(,@09)                                   0797 02226000
*   /*****************************************************************/ 02227000
*   /*                                                               */ 02228000
*   /* SIZE OF THE FORMAT WRITE CHANNEL PROGRAM (AMBSZFW) . . . . . .*/ 02229000
*   /* THREE BASE CCWS (SEEK, SET SECTOR, AND NOP REPRESENTED BY K3) */ 02230000
*   /* PLUS TWO WCKD CCWS PER BLOCK, THE FIRST DATA CHAINED TO THE   */ 02231000
*   /* SECOND (K2*R6). ADDITIONAL SPACE IS ALLOWED FOR A WORST CASE  */ 02232000
*   /* SITUATION OF A SID AND TIC PER BLOCK FOR A CONTROL INTERVAL   */ 02233000
*   /* WITH MULTIPLE PHYSICAL BLOCKS (K2*R7).                @ZA27961*/ 02234000
*   /*                                                               */ 02235000
*   /*****************************************************************/ 02236000
*                                                                  0798 02237000
*   AMBSZFW=(K2*(R6+R7)+K3)*K8;     /* SET LENGTH WORST CASE @ZA27961*/ 02238000
         LR    @14,R6                                              0798 02239000
         ALR   @14,R7                                              0798 02240000
         ALR   @14,@14                                             0798 02241000
         ALR   @14,@15                                             0798 02242000
         SLA   @14,3                                               0798 02243000
         STH   @14,AMBSZFW(,@09)                                   0798 02244000
*   RESPECIFY                                                      0799 02245000
*     R7 UNRESTRICTED;              /* RELEASE WORK REGISTER @ZA27961*/ 02246000
*   R6=VSRTBUFH;                    /* PT 1ST BFR SP HDR     @X04SVGP*/ 02247000
         L     R6,VSRTBUFH(,VSRTAD)                                0800 02248000
*   DO WHILE BSPHBSZ<AMDCINV&R6^=K0;/* FIND BFR SP           @Z30SSDT*/ 02249000
         B     @DE00801                                            0801 02250000
@DL00801 DS    0H                                                  0802 02251000
*     R6=BSPHNBSP;                  /* CHAIN NXT BFR SP      @X04SVGP*/ 02252000
         L     R6,BSPHNBSP(,R6)                                    0802 02253000
*   END;                            /*                       @X04SVGP*/ 02254000
@DE00801 L     @09,AMBP                                            0803 02255000
         L     @09,AMBDSB(,@09)                                    0803 02256000
         L     @07,BSPHBSZ(,R6)                                    0803 02257000
         C     @07,AMDCINV(,@09)                                   0803 02258000
