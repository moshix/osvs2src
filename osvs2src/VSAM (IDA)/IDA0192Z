         ST    @05,RETCONT                                         0404 00702000
*   RESPECIFY                                                      0405 00703000
*     RET UNRSTD;                   /*                       @X04SVCJ*/ 00704000
*   RFY                                                            0406 00705000
*     IDAAMDSB BASED(AMBDSB);       /* RESET                 @ZA18252*/ 00706000
*   END BLDAMB;                                                    0407 00707000
         B     @EL00002                                            0407 00708000
*TIOTSRCH:                                                         0408 00709000
*                                                                  0408 00710000
*/********************************************************************/ 00711000
*/*                                                                  */ 00712000
*/*   FUNCTION -  THIS ROUTINE SEARCHES THE CONCATENATED ENTRIES IN  */ 00713000
*/*               THE TIOT ENTRY FOR THE SPHERE TO DETERMINE WHICH   */ 00714000
*/*               ENTRY CORRESPONDES TO THE DEVICE TYPE FOR THE      */ 00715000
*/*               ELEMENTARY DATA SET BEING PROCESSED.  WHEN THE     */ 00716000
*/*               CORRECT ENTRY IS FOUND, THE OFFSET TO IT IS        */ 00717000
*/*               CALCULATED AND SET IN THE AMB.                     */ 00718000
*/*   INPUT -     ADDRESS OF TIOT ENTRY IN OPWTIOT.                  */ 00719000
*/*   OUTPUT -    AMBTIOT INITIALIZED TO CORRECT OFFSET.             */ 00720000
*/*                                                                  */ 00721000
*/********************************************************************/ 00722000
*                                                                  0408 00723000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @X04SVRP*/ 00724000
TIOTSRCH DS    0H                                                  0409 00725000
*   RFY                                                            0409 00726000
*    (RET,                                                         0409 00727000
*     RTIOT) RSTD;                  /*                       @X04SVRP*/ 00728000
*   RETCONT=RETCONT+K1;             /* BUMP CONTROL BY ONE   @Z30SSRL*/ 00729000
         LA    @15,1                                               0410 00730000
         AL    @15,RETCONT                                         0410 00731000
         ST    @15,RETCONT                                         0410 00732000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS   @Z30SSDT*/ 00733000
         SLA   @15,2                                               0411 00734000
         ST    RET,RETSAVE-4(@15)                                  0411 00735000
*   RFY                                                            0412 00736000
*     RET UNRSTD;                   /*                       @X04SVRP*/ 00737000
*   RTIOT=OPWTIOT;                  /* LD ADDR OF TIOT ENTRY @XO4SVRP*/ 00738000
         L     RTIOT,OPWTIOT(,RCORE)                               0413 00739000
*   DSABPTR=DXDSABAD;               /* INIT DSAB POINTER     @Z30SSDT*/ 00740000
         L     @08,OPWCOMWA(,RCORE)                                0414 00741000
         L     @08,DXDSABAD(,@08)                                  0414 00742000
         ST    @08,DSABPTR                                         0414 00743000
*   DO UNTIL AMBTIOT^=0;            /* SCAN CONCATENTED TIOT ENTRIES    00744000
*                                                            @Z30SSDT*/ 00745000
@DL00415 DS    0H                                                  0416 00746000
*     IF(UCBTYP(3:4)=CTDEVTYP(3:4)& /* IF DEV TYPES MATCH    @X04SVRP*/ 00747000
