*     RET UNRSTD;                   /*                       @X04SVCJ*/ 03688000
*   END BLDSTRBK;                   /*                       @X04SVCJ*/ 03689000
         B     @EL00015                                            1170 03690000
*RESTINIT:                                                         1171 03691000
*                                                                  1171 03692000
*/********************************************************************/ 03693000
*/*                                                                  */ 03694000
*/*   FUNCTION - INITIALIZE POINTERS IN THE MODULE WORK AREA WITH    */ 03695000
*/*              ADDRESSES OF CONTROL BLOCKS THAT ALREADY EXIST AT   */ 03696000
*/*              AT THE TIME OF A RESTART                            */ 03697000
*/*   INPUT    - POINTER TO THE ACB IN RACB                          */ 03698000
*/*   OUTPUT   - MODULE WORK AREA POINTERS INITIALIZED               */ 03699000
*/*                                                                  */ 03700000
*/********************************************************************/ 03701000
*                                                                  1171 03702000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @Z30SSDT*/ 03703000
RESTINIT DS    0H                                                  1172 03704000
*   RESPECIFY                                                      1172 03705000
*     RET RSTD;                     /*                       @Z30SSDT*/ 03706000
*   RETCONT=RETCONT+K1;             /* BUMP CONTROL BY ONE   @Z30SSRL*/ 03707000
         LA    @15,1                                               1173 03708000
         AL    @15,RETCONT                                         1173 03709000
         ST    @15,RETCONT                                         1173 03710000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS   @Z30SSDT*/ 03711000
         SLA   @15,2                                               1174 03712000
         ST    RET,RETSAVE-4(@15)                                  1174 03713000
*   RESPECIFY                                                      1175 03714000
*     RET UNRSTD;                   /*                       @Z30SSDT*/ 03715000
*   IF C1=AMBLNUM THEN              /* IS AMB FOR DATA       @Z30SSDT*/ 03716000
         L     @05,ACBAMBL(,RACB)                                  1176 03717000
         L     @15,C1                                              1176 03718000
         CH    @15,AMBLNUM(,@05)                                   1176 03719000
         BNE   @RF01176                                            1176 03720000
*     AMBAD=AMBLDTA;                /* YES, GET DATA AMB PTR @Z30SSDT*/ 03721000
         L     @05,AMBLDTA(,@05)                                   1177 03722000
         ST    @05,AMBAD                                           1177 03723000
*   ELSE                            /* AMB IS FOR INDEX      @Z30SSDT*/ 03724000
*     AMBAD=AMBLIX;                 /* GET INDEX AMB PTR     @Z30SSDT*/ 03725000
         B     @RC01176                                            1178 03726000
@RF01176 L     @05,ACBAMBL(,RACB)                                  1178 03727000
         L     @05,AMBLIX(,@05)                                    1178 03728000
         ST    @05,AMBAD                                           1178 03729000
*   AMBP=AMBAD;                     /* AMB ADDRESSIBILITY    @Z30SSDT*/ 03730000
@RC01176 L     @05,AMBAD                                           1179 03731000
         ST    @05,AMBP                                            1179 03732000
*   AMBWKA=OPWWRKPT;                /* POINTER TO AMB WORK   @Z30SSDT*/ 03733000
         L     @15,OPWWRKPT(,RCORE)                                1180 03734000
         ST    @15,AMBWKA(,@05)                                    1180 03735000
*   RESPECIFY                                                      1181 03736000
*     RTIOT RESTRICTED;                                            1181 03737000
*   RTIOT=TCBTIO+AMBTIOT;           /* ADDRESS OF TIOT ENTRY @Z30SSDT*/ 03738000
         L     @15,PSATOLD                                         1182 03739000
         LH    RTIOT,AMBTIOT(,@05)                                 1182 03740000
         AL    RTIOT,TCBTIO(,@15)                                  1182 03741000
*   DSABPTR=QDBFELMP;               /* GET PTR TO FIRST DSAB @Z30SSDT*/ 03742000
         L     @08,TCBJSCB(,@15)                                   1183 03743000
         L     @08,JSCDSABQ(,@08)                                  1183 03744000
         L     @08,QDBFELMP(,@08)                                  1183 03745000
         ST    @08,DSABPTR                                         1183 03746000
*   DO WHILE RTIOT^=DSABTIOT;       /* GET CORRECT DSAB      @Z30SSDT*/ 03747000
         B     @DE01184                                            1184 03748000
@DL01184 DS    0H                                                  1185 03749000
*     IF DSABPTR=ZERO THEN          /* CHECK FOR END OF CHN  @Z30SSDT*/ 03750000
         L     @08,DSABPTR                                         1185 03751000
         LTR   @08,@08                                             1185 03752000
         BNZ   @RF01185                                            1185 03753000
*       DO;                         /* IF NO DSAB FOR AMB,   @Z30SSDT*/ 03754000
*         PPCODE=PP78;              /* SET ERROR CODE AND    @Z30SSDT*/ 03755000
         L     @08,SABWRD(,R13)                                    1187 03756000
         MVI   PPCODE(@08),X'4E'                                   1187 03757000
*         RETURN TO EXIT;           /* EXIT MODULE           @Z30SSDT*/ 03758000
         LA    @14,EXIT                                            1188 03759000
@EL00016 DS    0H                                                  1188 03760000
@EF00016 DS    0H                                                  1188 03761000
@ER00016 BR    @14                                                 1188 03762000
*       END;                        /*                       @Z30SSDT*/ 03763000
*     DSABPTR=DSABFCHN;             /* POINT TO NEXT DSAB    @Z30SSDT*/ 03764000
@RF01185 L     @08,DSABPTR                                         1190 03765000
         L     @08,DSABFCHN(,@08)                                  1190 03766000
         ST    @08,DSABPTR                                         1190 03767000
*   END;                            /*                       @Z30SSDT*/ 03768000
@DE01184 L     @08,DSABPTR                                         1191 03769000
         C     RTIOT,DSABTIOT(,@08)                                1191 03770000
         BNE   @DL01184                                            1191 03771000
*   AMDSBAD=AMBDSB;                 /* INIT AMDSB PTR        @Z30SSDT*/ 03772000
         L     @08,AMBP                                            1192 03773000
         L     @05,AMBDSB(,@08)                                    1192 03774000
         ST    @05,AMDSBAD                                         1192 03775000
*   ARDBAD=AMDPARDB;                /* INIT PTR FIRST ARDB   @Z30SSDT*/ 03776000
         L     @05,AMDPARDB(,@05)                                  1193 03777000
         ST    @05,ARDBAD                                          1193 03778000
*   LPMBAD=AMBEDB->EDBLPMBC;        /* GET PTR TO FIRST LPMB @Z30SSDT*/ 03779000
         L     @08,AMBEDB(,@08)                                    1194 03780000
         L     @08,EDBLPMBC(,@08)                                  1194 03781000
         ST    @08,LPMBAD                                          1194 03782000
*   CALL BLDAMDSB;                  /* BUILD AN AMDSB        @Z30SSDT*/ 03783000
         BAL   @14,BLDAMDSB                                        1195 03784000
*   CALL BLDARDB;                   /* BUILD ARDBS           @Z30SSDT*/ 03785000
         BAL   @14,BLDARDB                                         1196 03786000
*   RESPECIFY                                                      1197 03787000
*     RET RSTD;                     /*                       @Z30SSDT*/ 03788000
*   RET=RETSAVE(RETCONT);           /* LOAD RETURN ADDRESS   @Z30SSDT*/ 03789000
         L     @08,RETCONT                                         1198 03790000
         LR    @05,@08                                             1198 03791000
         SLA   @05,2                                               1198 03792000
         L     RET,RETSAVE-4(@05)                                  1198 03793000
*   RETCONT=RETCONT-K1;             /* DECR CONTROL BY ONE   @Z30SSRL*/ 03794000
         BCTR  @08,0                                               1199 03795000
         ST    @08,RETCONT                                         1199 03796000
*   RESPECIFY                                                      1200 03797000
*    (RTIOT,                                                       1200 03798000
*     RET) UNRSTD;                  /*                       @Z30SSDT*/ 03799000
*   END RESTINIT;                   /*                       @Z30SSDT*/ 03800000
