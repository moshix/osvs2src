         LTR   R6,R6                                               0803 02260000
         BNZ   @DL00801                                            0803 02261000
@DC00801 DS    0H                                                  0804 02262000
*   IF R6=K0 THEN                   /* FIND BFR SP           @Z30SSDT*/ 02263000
         LTR   R6,R6                                               0804 02264000
         BNZ   @RF00804                                            0804 02265000
*     DO;                           /*                       @X04SVGP*/ 02266000
*       PPCODE=PP120;               /* NO, SET PROB CODE     @X04SVGP*/ 02267000
         L     @09,SABWRD(,R13)                                    0806 02268000
         MVI   PPCODE(@09),X'78'                                   0806 02269000
*       RETURN TO EXIT;             /* EXIT MODULE           @X04SVCJ*/ 02270000
         LA    @14,EXIT                                            0807 02271000
@EL00009 DS    0H                                                  0807 02272000
@EF00009 DS    0H                                                  0807 02273000
@ER00009 BR    @14                                                 0807 02274000
*     END;                          /*                       @X04SVGP*/ 02275000
*   AMBBUFC=R6;                     /* AMB PTS TO BFR SP HDR @X04SVGP*/ 02276000
@RF00804 L     @09,AMBP                                            0809 02277000
         ST    R6,AMBBUFC(,@09)                                    0809 02278000
*   IF ACBDFR=ON THEN               /* DEFER SPECIFIED       @Z30SSDT*/ 02279000
         TM    ACBDFR(RACB),B'00001000'                            0810 02280000
         BNO   @RF00810                                            0810 02281000
*     AMBDFR=ON;                    /* YES, SET ON IN AMB    @Z30SSDT*/ 02282000
         OI    AMBDFR(@09),B'00001000'                             0811 02283000
*   AMBCPA=VSRTCPAH;                /* AMB PTS TO CPA HDR    @Z30SSDT*/ 02284000
@RF00810 L     @09,AMBP                                            0812 02285000
         L     @07,VSRTCPAH(,VSRTAD)                               0812 02286000
         ST    @07,AMBCPA(,@09)                                    0812 02287000
*   AMBWSHD=VSRTWAH;                /* AMB PTS TO WS HDR     @Z30SSDT*/ 02288000
