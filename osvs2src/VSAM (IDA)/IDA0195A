@EF00011 DS    0H                                                  0739 02225000
@ER00011 LM    @14,@12,@SA00011                                    0739 02226000
         BR    @14                                                 0739 02227000
*DUMPSDS:                                                          0740 02228000
*                                                                  0740 02229000
*/********************************************************************/ 02230000
*/*  FUNCTION - DISPLAY CONTROL BLOCKS CONNECTED WITH THE    @ZA16012*/ 02231000
*/*             SYSTEM DATA SET OR THE CBIC OPTION           @ZA16012*/ 02232000
*/*  INPUT - RWORK1 CONTAINS THE VGTT ADDRESS.               @Z40WSRL*/ 02233000
*/*  OUTPUT - DATA SET CONTROL BLOCKS ARE DISPLAYED.         @Z40WSRL*/ 02234000
*/********************************************************************/ 02235000
*                                                                  0740 02236000
*   PROC;                           /*                       @Z40WSRL*/ 02237000
DUMPSDS  STM   @14,@05,@SA00012                                    0740 02238000
         STM   @07,@12,@SA00012+32                                 0740 02239000
*   RESPECIFY                                                      0741 02240000
*     IDAVGTT BASED(RWORK1);        /* ADDRESSABILITY FOR VGTT     0741 02241000
*                                                            @Z40WSRL*/ 02242000
*   RESPECIFY                                                      0742 02243000
*     IDABIB BASED(VGTTBIB);        /* ADDRESSABILITY FOR BIB      0742 02244000
*                                                            @Z40WSRL*/ 02245000
*   RESPECIFY                                                      0743 02246000
*     IDAAMBL BASED(BIBPAMBL);      /* ADDRESSABILITY FOR AMBL     0743 02247000
*                                                            @Z40WSRL*/ 02248000
*   RESPECIFY                                                      0744 02249000
*     IFGACB BASED(AMBLACB);        /* ADDRESSABILITY FOR ACB      0744 02250000
*                                                            @Z40WSRL*/ 02251000
*   RESPECIFY                                                      0745 02252000
*     IDACMB BASED(AMBLCMB);        /* ADDRESSABILITY FOR CMB      0745 02253000
*                                                            @Z40WSRL*/ 02254000
*   IF VGTTBIB^=ZERO THEN           /* IF THE BIB EXIST      @Z40WSRL*/ 02255000
         SLR   @12,@12                                             0746 02256000
         L     @08,VGTTBIB(,RWORK1)                                0746 02257000
         CR    @08,@12                                             0746 02258000
         BE    @RF00746                                            0746 02259000
*     DO;                           /*                       @Z40WSRL*/ 02260000
*       IF BIBPAMBL^=ZERO THEN      /* IF THE AMBL EXIST     @Z40WSRL*/ 02261000
