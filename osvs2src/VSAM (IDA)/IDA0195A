         TITLE 'IDA0195A-VSAM SNAP FORMAT ROUTINE.                     *00001000
                        '                                               00002000
IDA0195A CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0195A  78.151'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @09,0                                               0001 00011000
@PSTART  LA    @10,4095(,@09)                                      0001 00012000
         LA    @11,4095(,@10)                                      0001 00013000
         USING @PSTART,@09                                         0001 00014000
         USING @PSTART+4095,@10                                    0001 00015000
         USING @PSTART+8190,@11                                    0001 00016000
JSCBDISP EQU 188                                                        00017000
JSCBAOS1 EQU  72                                                        00018000
JSCBAOS2 EQU 192                                                        00019000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00020000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00021000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00022000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00023000
*                                       IF NO ERROR WAS ENCOUNTERED     00024000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00025000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00026000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00027000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00028000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00029000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00030000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00031000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00032000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00033000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00034000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00035000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00036000
         B     GMN00200                 BYPASS SETTING ERROR            00037000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00038000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00039000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00040000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00041000
*   IF R15=0 THEN                   /* GETMAIN FAILURE       @ZA11576*/ 00042000
         LTR   R15,R15                                             0208 00043000
         BNZ   @RF00208                                            0208 00044000
*     DO;                           /* YES                   @ZA11576*/ 00045000
*       GEN;                                                       0210 00046000
*                                    /* START ASSEM CODE     @ZA11576*/ 00047000
      LA @15,4                       INIT RETURN CODE        @ZA11576   00048000
      LM R0,R12,20(R13)              RESTORE REGS            @ZA11576   00049000
      L  R14,12(R13)                 FETCH RETURN ADDRESS    @ZA11576   00050000
      BR R14                         RETURN TO         @ZA11576         00051000
*     END;                          /* END GETMAIN ERROR     @ZA11576*/ 00052000
*   RESPECIFY                                                      0212 00053000
*     R13 RESTRICTED;               /* HOLD SAVE AREA ADDR   @ZA11576*/ 00054000
@RF00208 DS    0H                                                  0213 00055000
*   GEN;                                                           0213 00056000
*                                                                  0213 00057000
*                                    /* START ASSEM CODE     @ZA11576*/ 00058000
    ST  R13,4(R15)                   SAVE SAVE REGISTER      @ZA11576   00059000
    L   R0,20(R13)         IDAGMAIN USES R0, RESTORE IT      @ZA11576   00060000
    L   R1,24(R13)         IDAGMAIN USES R1, RESTORE IT      @ZA11576   00061000
    LR  R13,R15                      SET NEW SAVE AREA ADDR  @ZA11576   00062000
    USING @DATD,R13                  ADDRESSABILITY TO WORKAR@ZA11576   00063000
*   /*****************************************************************/ 00064000
*   /*                                                               */ 00065000
*   /* THIS CODE IS NOW RUNNING IN PSW KEY TCBPKF.                   */ 00066000
*   /*                                                               */ 00067000
*   /*****************************************************************/ 00068000
*                                                                  0214 00069000
*   TCBAD=ADPLTCB;                  /* MAKE LOCAL TCB PTR.           */ 00070000
         L     @12,ADPLTCB(,R1)                                    0214 00071000
         ST    @12,TCBAD                                           0214 00072000
*   RFY                                                            0215 00073000
*     TCB BASED(TCBAD);             /* TCB IS NOW LOCATED FROM     0215 00074000
*                                      TCBPTR.                       */ 00075000
*   KEYTCB=TCBPKF;                  /* SAVE USERS KEY.               */ 00076000
         IC    @08,TCBPKF(,@12)                                    0216 00077000
         STC   @08,KEYTCB                                          0216 00078000
*   RFY                                                            0217 00079000
*     IDAVSRT BASED(CBSVPTR(KEYTCB/16+ONE));/* SUPPORT MULTI-VSRT.   */ 00080000
*   ASCBPTR=ASVTENTY(ADPLASID);     /* MAKE LOCAL ASCB PTR.          */ 00081000
         LH    @12,ADPLASID(,R1)                                   0218 00082000
         SLA   @12,2                                               0218 00083000
         L     @08,CVTPTR                                          0218 00084000
         L     @08,CVTASVT(,@08)                                   0218 00085000
         L     @12,ASVTENTY-4(@12,@08)                             0218 00086000
         ST    @12,ASCBPTR                                         0218 00087000
*   TRTAB(1)='.';                   /* SET 1ST BYTE OF TABLE.        */ 00088000
         MVI   TRTAB,C'.'                                          0219 00089000
*   MVC(TRTAB(2:256),TRTAB(1:255)); /* PROPAGATE THE PERIOD.         */ 00090000
         MVC   TRTAB+1(255),TRTAB                                  0220 00091000
*   DO I=1 TO LENGTH(VALTAB);       /* LOOP THROUGH ENTIRE TABLE.    */ 00092000
         LA    I,1                                                 0221 00093000
@DL00221 DS    0H                                                  0222 00094000
*     TRTAB(VALTAB(I)+1)=VALTAB(I); /* SET TRANSLATABLE CHARACTERS.  */ 00095000
         SLR   @12,@12                                             0222 00096000
         IC    @12,VALTAB-1(I)                                     0222 00097000
         LA    @08,TRTAB(@12)                                      0222 00098000
         LA    @12,VALTAB-1(I)                                     0222 00099000
         MVC   0(1,@08),0(@12)                                     0222 00100000
*   END;                            /* END OF LOOP FOR TRANLATE    0223 00101000
*                                      TABLE.                        */ 00102000
         AL    I,@CF00043                                          0223 00103000
         C     I,@CF03067                                          0223 00104000
         BNH   @DL00221                                            0223 00105000
*   DO I=1 TO DIM(BIBTABL);         /* SCAN ENTIRE TABLE.            */ 00106000
         LA    I,1                                                 0224 00107000
@DL00224 DS    0H                                                  0225 00108000
*     BIBTABL(I)=ZERO;              /* CLEAR ANY RESIDUE.            */ 00109000
         LR    @12,I                                               0225 00110000
         SLA   @12,2                                               0225 00111000
         SLR   @08,@08                                             0225 00112000
         ST    @08,BIBTABL-4(@12)                                  0225 00113000
*   END;                            /* END OF TABLE CLEARING.        */ 00114000
         AL    I,@CF00043                                          0226 00115000
         C     I,@CF01219                                          0226 00116000
         BNH   @DL00224                                            0226 00117000
*   DO I=1 TO DIM(CMBTABL);         /* SCAN ENTIRE TABLE.            */ 00118000
         LA    I,1                                                 0227 00119000
@DL00227 DS    0H                                                  0228 00120000
*     CMBTABL(I)=ZERO;              /* CLEAR ANY RESIDUE.            */ 00121000
         LR    @12,I                                               0228 00122000
         SLA   @12,2                                               0228 00123000
         SLR   @08,@08                                             0228 00124000
         ST    @08,CMBTABL-4(@12)                                  0228 00125000
*   END;                            /* END OF TABLE CLEARING.        */ 00126000
*                                                                  0229 00127000
         AL    I,@CF00043                                          0229 00128000
         C     I,@CF01219                                          0229 00129000
         BNH   @DL00227                                            0229 00130000
*   /*****************************************************************/ 00131000
*   /*                                                               */ 00132000
*   /* INITIALIZE THE ESTAE PARAMETER LIST PASSED TO SVC ESTAE.      */ 00133000
*   /*                                                               */ 00134000
*   /*****************************************************************/ 00135000
*                                                                  0230 00136000
*   RFY                                                            0230 00137000
*    (R9,                                                          0230 00138000
*     R10,                                                         0230 00139000
*     R11,                                                         0230 00140000
*     R12,                                                         0230 00141000
*     R13) RSTD;                    /* MAKE NOTE FOR COMPILER.       */ 00142000
*   RETRYR1=R1;                     /* SAVE REGISTER FOR RETRY.      */ 00143000
         ST    R1,RETRYR1                                          0231 00144000
*   RETRYR9=R9;                     /* SAVE REGISTER FOR RETRY.      */ 00145000
         ST    R9,RETRYR9                                          0232 00146000
*   RETRYR10=R10;                   /* SAVE REGISTER FOR RETRY.      */ 00147000
         ST    R10,RETRYR10                                        0233 00148000
*   RETRYR11=R11;                   /* SAVE REGISTER FOR RETRY.      */ 00149000
         ST    R11,RETRYR11                                        0234 00150000
*   RETRYR12=R12;                   /* SAVE REGISTER FOR RETRY.      */ 00151000
         ST    R12,RETRYR12                                        0235 00152000
*   RETRYR13=R13;                   /* SAVE REGISTER FOR RETRY.      */ 00153000
         ST    R13,RETRYR13                                        0236 00154000
*   RFY                                                            0237 00155000
*    (R9,                                                          0237 00156000
*     R10,                                                         0237 00157000
*     R11,                                                         0237 00158000
*     R12,                                                         0237 00159000
*     R13) UNRSTD;                  /* MAKE NOTE FOR COMPILER.       */ 00160000
*   RETRYPTR=ADDR(LEMSG);           /* ESTABLISH RETRY POINT FOR   0238 00161000
*                                      ERROR MSG.                    */ 00162000
         LA    @12,LEMSG                                           0238 00163000
         ST    @12,RETRYPTR                                        0238 00164000
*   RETRYADR=ADDR(LEOD);            /* ESTABLISH RETRY ADDRESS.      */ 00165000
         LA    @12,LEOD                                            0239 00166000
         ST    @12,RETRYADR                                        0239 00167000
*   RETRY=OFF;                      /* NOTE TAHT NO RETRY YET      0240 00168000
*                                      PERFORMED.                    */ 00169000
         NI    RETRY,B'01111111'                                   0240 00170000
*   LISTESTA=ESTAELST;              /* MOVE LIST TO RE-ENTRANT AREA. */ 00171000
         MVC   LISTESTA(16),ESTAELST                               0241 00172000
*   RFY                                                            0242 00173000
*    (R0,                                                          0242 00174000
*     R14,                                                         0242 00175000
*     R15) RSTD;                    /* RESERVE THESE REGS FOR      0242 00176000
*                                      COMPILER.                     */ 00177000
*   MWAR0=R0;                       /* SAVE R0 ACROSS SVC CALL.      */ 00178000
         ST    R0,MWAR0                                            0243 00179000
*   MWAR14=R14;                     /* SAVE R14 ACROSS SVC INTERFACE.*/ 00180000
         ST    R14,MWAR14                                          0244 00181000
*   MWAR15=R15;                     /* SAVE THIS REGISTER ACROSS SVC.*/ 00182000
         ST    R15,MWAR15                                          0245 00183000
*   RFY                                                            0246 00184000
*    (R0,                                                          0246 00185000
*     R14,                                                         0246 00186000
*     R15) UNRSTD;                  /* RELEASE THESE REGS TO       0246 00187000
*                                      COMPILER.                     */ 00188000
*   GEN REFS(LISTESTA) SETS(R0,R1,R14,R15);                        0247 00189000
*                                          /* ESTABLISH RECOVERY */     00190000
      ESTAE RCVRRTN,CT,PARAM=(R13),RECORD=YES,MF=(E,LISTESTA)  @ZA11576 00191000
*   RFY                                                            0248 00192000
*    (R0,                                                          0248 00193000
*     R14,                                                         0248 00194000
*     R15) RSTD;                    /* RESERVE THESE REGS FOR      0248 00195000
*                                      COMPILER.                     */ 00196000
*   R0=MWAR0;                       /* SAVE R0 ACROSS SVC CALL.      */ 00197000
         L     R0,MWAR0                                            0249 00198000
*   R1=RETRYR1;                     /* SAVE R1 ACROSS SVC.           */ 00199000
         L     R1,RETRYR1                                          0250 00200000
*   R14=MWAR14;                     /* SAVE R14 ACROSS SVC INTERFACE.*/ 00201000
         L     R14,MWAR14                                          0251 00202000
*   R15=MWAR15;                     /* SAVE THIS REGISTER ACROSS SVC.*/ 00203000
         L     R15,MWAR15                                          0252 00204000
*   RFY                                                            0253 00205000
*    (R0,                                                          0253 00206000
*     R14,                                                         0253 00207000
*     R15) UNRSTD;                  /* RELEASE THESE REGS TO       0253 00208000
*                                      COMPILER.                     */ 00209000
*   CALL SPACE;                     /* START DISPLAY WITH BLANK LINE.*/ 00210000
*                                                                  0254 00211000
         BAL   @14,SPACE                                           0254 00212000
*   /*****************************************************************/ 00213000
*   /*                                                               */ 00214000
*   /* LOCATE AND DISPLAY THE VSAM FIELD IN THE JSCB (JSCBSHR).      */ 00215000
*   /*                                                               */ 00216000
*   /*****************************************************************/ 00217000
*                                                                  0255 00218000
*LVAT:                              /* LABEL FOR CALL TO DUMPVAT.    */ 00219000
*                                                                  0255 00220000
*   /*****************************************************************/ 00221000
*   /*                                                               */ 00222000
*   /* THE FOLLOWING IMBEDDED CHARACTER STRING PROVIDES A POINT AT   */ 00223000
*   /* WHICH A PROGRAM CHECK MAY BE FORCED TO TEST THE RETRY FACILITY*/ 00224000
*   /* OF THIS MODULE.                                               */ 00225000
*   /*                                                               */ 00226000
*   /*****************************************************************/ 00227000
*                                                                  0255 00228000
*   GOTO LVAT1;                     /* BRANCH AROUND THE CHARACTER 0255 00229000
*                                      STRING.                       */ 00230000
         B     LVAT1                                               0255 00231000
*   GEN(DC  CL8'RETRY   '       LABEL FOR FORCING ERROR. );/* LABEL  */ 00232000
         DC  CL8'RETRY   '       LABEL FOR FORCING ERROR.               00233000
*LVAT1:                             /* RESUME NORMAL CODE.           */ 00234000
*   GEN(BCR  0,0);                  /* PROVIDE RECOGNIZABLE OPCODE.  */ 00235000
LVAT1    BCR  0,0                                                       00236000
*   CALL DUMPVAT;                   /* DISPLAY THE VAT LOCATOR FIELD.*/ 00237000
*                                                                  0258 00238000
         BAL   @14,DUMPVAT                                         0258 00239000
*   /*****************************************************************/ 00240000
*   /*                                                               */ 00241000
*   /* DOES THE TCB HAVE A SYSTEM KEY ('00'X TO '70'X)?      @ZA12980*/ 00242000
*   /*                                                               */ 00243000
*   /*****************************************************************/ 00244000
*                                                                  0259 00245000
*   IF KEYTCB<PPKEYTCB THEN         /* IS THE CALLER A SYSTEM KEY? 0259 00246000
*                                                            @ZA12980*/ 00247000
         CLI   KEYTCB,128                                          0259 00248000
         BNL   @RF00259                                            0259 00249000
*     DO;                           /* YES, SCAN FOR GSR D/S AND GSR    00250000
*                                      POOLS.                @ZA12980*/ 00251000
*                                                                  0260 00252000
*       /*************************************************************/ 00253000
*       /*                                                           */ 00254000
*       /* LOCATE AND DISPLAY ANY OPEN VSAM GSR DATA SETS.           */ 00255000
*       /*                                                           */ 00256000
*       /*************************************************************/ 00257000
*                                                                  0261 00258000
*LOGSR:                             /* LABEL FOR CALL TO DUMP OPEN 0261 00259000
*                                      GSR D/S.                      */ 00260000
*       CALL DUMPOGSR;              /* DUMP OPEN GSR DATA SETS.      */ 00261000
*                                                                  0261 00262000
LOGSR    BAL   @14,DUMPOGSR                                        0261 00263000
*       /*************************************************************/ 00264000
*       /*                                                           */ 00265000
*       /* LOCATE AND DISPLAY, IF APPLICABLE, THE VSAM GSR POOL.     */ 00266000
*       /*                                                           */ 00267000
*       /*************************************************************/ 00268000
*                                                                  0262 00269000
*LAGSR:                             /* LABEL FOR CALL TO DUMP ACTIVE    00270000
*                                      GSR POOL.                     */ 00271000
*       CALL DUMPAGSR;              /* DUMP GSR POOL IF PRESENT.     */ 00272000
LAGSR    BAL   @14,DUMPAGSR                                        0262 00273000
*     END;                          /* END, SCAN FOR GSR D/S AND GSR    00274000
*                                      POOLS.                @ZA12980*/ 00275000
*                                                                  0263 00276000
*   /*****************************************************************/ 00277000
*   /*                                                               */ 00278000
*   /* LOCATE AND DISPLAY THE VGTT CHAIN FOR THE ASCB.               */ 00279000
*   /*                                                               */ 00280000
*   /*****************************************************************/ 00281000
*                                                                  0264 00282000
*LVGTT:                             /* LABEL FOR CALL TO DISPLAY VGTT   00283000
*                                      CHAIN.                        */ 00284000
*   CALL DUMPVGTT;                  /* DUMP ANY VGTTS.               */ 00285000
@RF00259 DS    0H                                                  0264 00286000
LVGTT    BAL   @14,DUMPVGTT                                        0264 00287000
*LEOD:                              /* LABEL FOR PROCESS OF END OF 0265 00288000
*                                      VSAM DATA.                    */ 00289000
*   LINE='END OF VSAM DATA';        /* PRINT FINAL MESSAGE.          */ 00290000
LEOD     MVI   LINE+16,C' '                                        0265 00291000
         MVC   LINE+17(103),LINE+16                                0265 00292000
         MVC   LINE(16),@CC03123                                   0265 00293000
*   CALL PRINT;                     /* DISPLAY THE MESSAGE.          */ 00294000
         BAL   @14,PRINT                                           0266 00295000
*   CALL SPACE;                     /* INSURE A LINE SPACE.          */ 00296000
         BAL   @14,SPACE                                           0267 00297000
*   GEN( ESTAE  0 CANCEL ESTAE. );  /* RELEASE ESTAE EXIT.           */ 00298000
*                                                                  0268 00299000
          ESTAE  0 CANCEL ESTAE.                                        00300000
*   /*****************************************************************/ 00301000
*   /*                                                               */ 00302000
*   /* BEFORE RETURNING TO IEAVAD08 COMPILER WILL FORCE OLDKEY(0).   */ 00303000
*   /*                                                               */ 00304000
*   /*****************************************************************/ 00305000
*                                                                  0269 00306000
*   GEN;                                                           0269 00307000
*                                                                  0269 00308000
*                                     /* START ASSEM CODE    @ZA11576*/ 00309000
       LR  @01,@13                    FETCH ADDR OF CORE     @ZA11576   00310000
       L   @13,4(@13)                 RESET SAVE AREA ADDR   @ZA11576   00311000
       L   @00,@SIZDATD               FETCH LENGTH TO FREE   @ZA11576   00312000
       FREEMAIN R,LV=(0),A=(1)        FREE WORK AREA         @ZA11576   00313000
       LM  @14,@12,12(@13)            LOAD RETURN REGISTERS    @ZA11576 00314000
       SLR @15,@15                    SET RETURN CODE TO ZERO  @ZA11576 00315000
       BR  @14                        RETURN TO          @ZA11576       00316000
*   /*****************************************************************/ 00317000
*   /*                                                               */ 00318000
*   /* LEMSG IS THE RETRY POINT TO DISPLAY AN ERROR MSG.             */ 00319000
*   /*                                                               */ 00320000
*   /*****************************************************************/ 00321000
*                                                                  0270 00322000
*LEMSG:                             /* LABEL FOR THE RETRY POINT IS     00323000
*                                      LEMSG.                        */ 00324000
*   LINE='IDA0195A DATA SUPPRESSED DUE TO ERROR';/* ERROR MESSAGE.   */ 00325000
LEMSG    MVI   LINE+37,C' '                                        0270 00326000
         MVC   LINE+38(82),LINE+37                                 0270 00327000
         MVC   LINE(37),@CC03127                                   0270 00328000
*   CALL PRINT;                     /* DISPLAY THE ERROR MESSAGE.    */ 00329000
         BAL   @14,PRINT                                           0271 00330000
*   CALL SPACE;                     /* FOLLOW THAT WITH A BLANK LINE.*/ 00331000
         BAL   @14,SPACE                                           0272 00332000
*   GOTO RETRYADR;                  /* GOTO RETRY ADDRESS.           */ 00333000
*                                                                  0273 00334000
         L     @12,RETRYADR                                        0273 00335000
         BR    @12                                                 0273 00336000
*   /*****************************************************************/ 00337000
*   /*                                                               */ 00338000
*   /* CONTROL HAS NOW PASSED TO THE ADDRESS IN RETRYADR.            */ 00339000
*   /*                                                               */ 00340000
*   /*****************************************************************/ 00341000
*                                                                  0274 00342000
*DUMPVAT:                                                          0274 00343000
*                                                                  0274 00344000
*/********************************************************************/ 00345000
*/*  FUNCTION - DISPLAY THE VSAM FIELD IN THE JSCB(JSCBSHR).         */ 00346000
*/*  INPUT - TCB IS USED TO FIND THE JSCB FOR THIS TCB.              */ 00347000
*/*  OUTPUT - THE VALUE CONTAINED IN JSCBSHR IS DISPLAYED.           */ 00348000
*/********************************************************************/ 00349000
*                                                                  0274 00350000
*   PROC;                                                          0274 00351000
DUMPVAT  STM   @14,@12,@SA00002                                    0274 00352000
*   DUMPID=IDVAT;                   /* IDENTIFY THE DATA.            */ 00353000
         MVI   DUMPID+12,C' '                                      0275 00354000
         MVC   DUMPID+13(18),DUMPID+12                             0275 00355000
         MVC   DUMPID(12),IDVAT                                    0275 00356000
*   IDSIZE=LENGTH(IDVAT);           /* INDICATE ID SZIE.             */ 00357000
         MVI   IDSIZE,X'0C'                                        0276 00358000
*   DUMPAD=ADDR(JSCBSHR);           /* DUMP JSCBSHR FIELD.           */ 00359000
         L     @12,TCBAD                                           0277 00360000
         L     @12,TCBJSCB(,@12)                                   0277 00361000
         LA    @12,JSCBSHR(,@12)                                   0277 00362000
         ST    @12,DUMPAD                                          0277 00363000
*   DUMPLEN=4;                      /* PASS DATA SIZE.               */ 00364000
         MVC   DUMPLEN(4),@CF00118                                 0278 00365000
*   CALL DUMPER;                    /* DISPLAY THE VAT LOCATION.     */ 00366000
         BAL   @14,DUMPER                                          0279 00367000
*   END;                            /* END OF PROCEDURE DUMPVAT.     */ 00368000
@EL00002 DS    0H                                                  0280 00369000
@EF00002 DS    0H                                                  0280 00370000
@ER00002 LM    @14,@12,@SA00002                                    0280 00371000
         BR    @14                                                 0280 00372000
*DUMPOGSR:                                                         0281 00373000
*                                                                  0281 00374000
*/********************************************************************/ 00375000
*/*  FUNCTION - DUMP CONTROL BLOCKS FOR ANY OPEN VSAM GSR DATA SETS. */ 00376000
*/*  INPUT - TCB IS USED TO FIND DATA SETS OPEN FOR THIS TCB.        */ 00377000
*/*  OUTPUT - CONTROL BLOCKS FOR ANY AND ALL GSR DATA SETS.          */ 00378000
*/*                                                                  */ 00379000
*/********************************************************************/ 00380000
*                                                                  0281 00381000
*   PROC;                                                          0281 00382000
DUMPOGSR STM   @14,@12,@SA00003                                    0281 00383000
*   RWORK1=TCBDEB;                  /* LOCATE FIRST DEB OFF TCB.     */ 00384000
*                                                                  0282 00385000
         L     @12,TCBAD                                           0282 00386000
         L     RWORK1,TCBDEB(,@12)                                 0282 00387000
*   /*****************************************************************/ 00388000
*   /*                                                               */ 00389000
*   /* THIS ROUTINE OPERATES IN PSW KEY TCBPKF, IF ANY FETCH         */ 00390000
*   /* PROTECTED DEB.S ARE ENCOUNTERED A PROGRAM CHECK WILL OCCUR.   */ 00391000
*   /* DEB.S SHOULD BE IN STORE PROTECTED-ONLY KEY 5 STORAGE         */ 00392000
*   /*                                                               */ 00393000
*   /*****************************************************************/ 00394000
*                                                                  0283 00395000
*   GSRFOUND=OFF;                   /* INITIALIZE THE SWITCH.        */ 00396000
*                                                                  0283 00397000
         NI    GSRFOUND,B'01111111'                                0283 00398000
*   /*****************************************************************/ 00399000
*   /*                                                               */ 00400000
*   /* SCAN THE TCB DEB CHAIN FINDING VSAM GSR DEB.S                 */ 00401000
*   /*                                                               */ 00402000
*   /*****************************************************************/ 00403000
*                                                                  0284 00404000
*   DEBCTR=ONE;                     /* INITIALIZE DEB LOOPING      0284 00405000
*                                      COUNTER.                      */ 00406000
         LA    DEBCTR,1                                            0284 00407000
*   RFY                                                            0285 00408000
*     DEB BASED(ADDR(DEBBASIC)-LENGTH(DEB));/* USE PHYSICAL OFFSET   */ 00409000
*   DO WHILE RWORK1^=ZERO;          /* LOCATE ALL OPEN DATA SETS FOR    00410000
*                                      THIS TCB.                     */ 00411000
         B     @DE00286                                            0286 00412000
@DL00286 DS    0H                                                  0287 00413000
*     IF DEBAMTYP=VSAMDEB THEN      /* IS THIS A VSAM DEB ?          */ 00414000
         LR    @12,RWORK1                                          0287 00415000
         SL    @12,@CF02382                                        0287 00416000
         CLI   DEBAMTYP(@12),X'01'                                 0287 00417000
         BNE   @RF00287                                            0287 00418000
*       DO;                         /* PROCESS THE VSAM DEB.         */ 00419000
*         RFY                                                      0289 00420000
*           IDAAMB BASED(DEBDCBAD); /* AMB IS LOCATED FROM THE DEB.  */ 00421000
*         IF AMBID=VSAMAMB&AMBGSR=ON THEN/* IS THIS A GSR DATA SET?  */ 00422000
         L     @08,DEBDCBAD(,RWORK1)                               0290 00423000
         CLI   AMBID(@08),X'40'                                    0290 00424000
         BNE   @RF00290                                            0290 00425000
         TM    AMBGSR(@08),B'00100000'                             0290 00426000
         BNO   @RF00290                                            0290 00427000
*           DO;                     /* PROCESS THE AMB GSR DATA SET. */ 00428000
*             GSRFOUND=ON;          /* INDICATE GSR DATA SET FOUND.  */ 00429000
*                                                                  0292 00430000
         OI    GSRFOUND,B'10000000'                                0292 00431000
*             /*******************************************************/ 00432000
*             /*                                                     */ 00433000
*             /* DISPLAY THE DEB ID AND IT.S ADDRESS                 */ 00434000
*             /*                                                     */ 00435000
*             /*******************************************************/ 00436000
*                                                                  0293 00437000
*             DUMPID=IDDEB;         /* ID OF DATA IS DEB             */ 00438000
         MVI   DUMPID+12,C' '                                      0293 00439000
         MVC   DUMPID+13(18),DUMPID+12                             0293 00440000
         MVC   DUMPID(12),IDDEB                                    0293 00441000
*             IDSIZE=LENGTH(IDDEB); /* INDICATE DUMP ID SIZE.        */ 00442000
         MVI   IDSIZE,X'0C'                                        0294 00443000
*             DUMPAD=RWORK1;        /* ADDRESS OF DATA TO DISPLAY    */ 00444000
         ST    RWORK1,DUMPAD                                       0295 00445000
*             DUMPLEN=(DEBLNGTH*DWDSIZE)+DEBXLNGH;/* CALCULATE SIZE. */ 00446000
         SLR   @08,@08                                             0296 00447000
         IC    @08,DEBLNGTH(,@12)                                  0296 00448000
         SLA   @08,3                                               0296 00449000
         L     @12,DEBXTNP(,@12)                                   0296 00450000
         AH    @08,DEBXLNGH(,@12)                                  0296 00451000
         ST    @08,DUMPLEN                                         0296 00452000
*             DEBCB=ON;             /* INDICATE DEB CONTROL BLOCK FOR   00453000
*                                      SUPPRESSION                   */ 00454000
         OI    DEBCB,B'10000000'                                   0297 00455000
*             CALL DUMPER;          /* DUMP THE INDICATED DATA.      */ 00456000
*                                                                  0298 00457000
         BAL   @14,DUMPER                                          0298 00458000
*             /*******************************************************/ 00459000
*             /*                                                     */ 00460000
*             /* DISPLAY THE AMB ASSOCIATED WITH THE DEB.            */ 00461000
*             /*                                                     */ 00462000
*             /*******************************************************/ 00463000
*                                                                  0299 00464000
*             DUMPID=IDAMB;         /* ID OF DATA IS AMB.            */ 00465000
         MVI   DUMPID+3,C' '                                       0299 00466000
         MVC   DUMPID+4(27),DUMPID+3                               0299 00467000
         MVC   DUMPID(3),IDAMB                                     0299 00468000
*             IDSIZE=LENGTH(IDAMB); /* INDICATE DUMP ID SIZE.        */ 00469000
         MVI   IDSIZE,X'03'                                        0300 00470000
*             DUMPAD=ADDR(IDAAMB);  /* DATA IS THE AMB               */ 00471000
         L     @12,DEBDCBAD(,RWORK1)                               0301 00472000
         ST    @12,DUMPAD                                          0301 00473000
*             DUMPLEN=LENGTH(IDAAMB);/* IDICATE THE SIZE             */ 00474000
         MVC   DUMPLEN(4),@CF02928                                 0302 00475000
*             CALL DUMPER;          /* DUMP THE DATA.                */ 00476000
*                                                                  0303 00477000
         BAL   @14,DUMPER                                          0303 00478000
*             /*******************************************************/ 00479000
*             /*                                                     */ 00480000
*             /* CALL DUMPDEB TO DISPLAY DATA SET CONTROL BLOCKS.    */ 00481000
*             /*                                                     */ 00482000
*             /*******************************************************/ 00483000
*                                                                  0304 00484000
*             CALL DUMPDEB;         /* DISPLAY DEB DATA SET.         */ 00485000
         BAL   @14,DUMPDEB                                         0304 00486000
*             RFY                                                  0305 00487000
*               IFGACB BASED(DEBDCBAD);/* LOCATE THE ACB FROM THE  0305 00488000
*                                      DEB.                          */ 00489000
*           END;                    /* END OF PATH FOR AMB GSR DATA     00490000
*                                      SET.                          */ 00491000
*         ELSE                      /* IF NOT AN AMB THEN,           */ 00492000
*           IF ACBID=VSAMACB&ACBGSR=ON THEN/* DOES DEB POINT TO ACB? */ 00493000
         B     @RC00290                                            0307 00494000
@RF00290 L     @12,DEBDCBAD(,RWORK1)                               0307 00495000
         CLI   ACBID(@12),160                                      0307 00496000
         BNE   @RF00307                                            0307 00497000
         TM    ACBGSR(@12),B'00100000'                             0307 00498000
         BNO   @RF00307                                            0307 00499000
*             DO;                   /* DEB POINTS TO GSR ACB, PROCESS   00500000
*                                      IT.                           */ 00501000
*               GSRFOUND=ON;        /* INDICATE GSR DATA SET WAS   0309 00502000
*                                      FOUND OPEN.                   */ 00503000
*                                                                  0309 00504000
         OI    GSRFOUND,B'10000000'                                0309 00505000
*               /*****************************************************/ 00506000
*               /*                                                   */ 00507000
*               /* DISPLAY THE DEB ID AND IT.S ADDRESS               */ 00508000
*               /*                                                   */ 00509000
*               /*****************************************************/ 00510000
*                                                                  0310 00511000
*               DUMPID=IDDEB;       /* ID OF DATA IS DEB             */ 00512000
         MVI   DUMPID+12,C' '                                      0310 00513000
         MVC   DUMPID+13(18),DUMPID+12                             0310 00514000
         MVC   DUMPID(12),IDDEB                                    0310 00515000
*               IDSIZE=LENGTH(IDDEB);/* INDICATE DUMP ID SIZE.       */ 00516000
         MVI   IDSIZE,X'0C'                                        0311 00517000
*               DUMPAD=RWORK1;      /* ADDRESS OF DATA TO DISPLAY    */ 00518000
         ST    RWORK1,DUMPAD                                       0312 00519000
*               DUMPLEN=(DEBLNGTH*DWDSIZE)+DEBXLNGH;/* CALCULATE   0313 00520000
*                                      SIZE.                         */ 00521000
         LR    @12,RWORK1                                          0313 00522000
         SL    @12,@CF02382                                        0313 00523000
         SLR   @08,@08                                             0313 00524000
         IC    @08,DEBLNGTH(,@12)                                  0313 00525000
         SLA   @08,3                                               0313 00526000
         L     @12,DEBXTNP(,@12)                                   0313 00527000
         AH    @08,DEBXLNGH(,@12)                                  0313 00528000
         ST    @08,DUMPLEN                                         0313 00529000
*               DEBCB=ON;           /* INDICATE DEB CONTROL BLOCK FOR   00530000
*                                      SUPPRESSION                   */ 00531000
         OI    DEBCB,B'10000000'                                   0314 00532000
*               CALL DUMPER;        /* DUMP THE INDICATED DATA.      */ 00533000
*                                                                  0315 00534000
         BAL   @14,DUMPER                                          0315 00535000
*               /*****************************************************/ 00536000
*               /*                                                   */ 00537000
*               /* DISPLAY THE ACB ASSOCIATED WITH THE DEB.          */ 00538000
*               /*                                                   */ 00539000
*               /*****************************************************/ 00540000
*                                                                  0316 00541000
*               DUMPID=IDACB;       /* ID OF DATA IS ACB.            */ 00542000
         MVI   DUMPID+3,C' '                                       0316 00543000
         MVC   DUMPID+4(27),DUMPID+3                               0316 00544000
         MVC   DUMPID(3),IDACB                                     0316 00545000
*               IDSIZE=LENGTH(IDACB);/* INDICATE DUMP ID SIZE.       */ 00546000
         MVI   IDSIZE,X'03'                                        0317 00547000
*               DUMPAD=ADDR(IFGACB);/* DISPLAY THE ACB .             */ 00548000
         L     @12,DEBDCBAD(,RWORK1)                               0318 00549000
         ST    @12,DUMPAD                                          0318 00550000
*               DUMPLEN=LENGTH(IFGACB);/* DUMP ENTIRE ACB.           */ 00551000
         MVC   DUMPLEN(4),@CF03311                                 0319 00552000
*               CALL DUMPER;        /* DUMP INDICATED DATA.          */ 00553000
*                                                                  0320 00554000
         BAL   @14,DUMPER                                          0320 00555000
*               /*****************************************************/ 00556000
*               /*                                                   */ 00557000
*               /* DISPLAY THE AMBL ASSOCIATED WITH THE ACB.         */ 00558000
*               /*                                                   */ 00559000
*               /*****************************************************/ 00560000
*                                                                  0321 00561000
*               RFY                                                0321 00562000
*                 IDAAMBL BASED(ACBAMBL);/* LOCATE THE AMBL FROM THE    00563000
*                                      ACB.                          */ 00564000
*               DUMPID=IDAMBL;      /* THE ID IS THE AMBL.           */ 00565000
         MVI   DUMPID+4,C' '                                       0322 00566000
         MVC   DUMPID+5(26),DUMPID+4                               0322 00567000
         MVC   DUMPID(4),IDAMBL                                    0322 00568000
*               IDSIZE=LENGTH(IDAMBL);/* INDICATE THE SIZE OF DUMP 0323 00569000
*                                      ID.                           */ 00570000
         MVI   IDSIZE,X'04'                                        0323 00571000
*               DUMPAD=ADDR(IDAAMBL);/* PASS THE AMBL ADDRESS.       */ 00572000
         L     @12,DEBDCBAD(,RWORK1)                               0324 00573000
         L     @12,ACBAMBL(,@12)                                   0324 00574000
         ST    @12,DUMPAD                                          0324 00575000
*               DUMPLEN=LENGTH(IDAAMBL);/* INDICATE THE SIZE TO    0325 00576000
*                                      PRINT.                        */ 00577000
         MVC   DUMPLEN(4),@CF03312                                 0325 00578000
*               CALL DUMPER;        /* DISPLAY INDICATED DATA.       */ 00579000
*                                                                  0326 00580000
         BAL   @14,DUMPER                                          0326 00581000
*               /*****************************************************/ 00582000
*               /*                                                   */ 00583000
*               /* VERIFY THAT THE CMB HAS NOT YET BEEN PRINTED.     */ 00584000
*               /*                                                   */ 00585000
*               /*****************************************************/ 00586000
*                                                                  0327 00587000
*               RFY                                                0327 00588000
*                 IDACMB BASED(RWORK2);/* LOCATE THE CMB FROM WORK 0327 00589000
*                                      REG.                          */ 00590000
*               RWORK2=AMBLCMB;     /* OBTAIN THE CMB ADDRESS.       */ 00591000
         L     @12,DEBDCBAD(,RWORK1)                               0328 00592000
         L     @12,ACBAMBL(,@12)                                   0328 00593000
         L     RWORK2,AMBLCMB(,@12)                                0328 00594000
*               TABLOOP=ON;         /* ALLOW TABLE LOOPING.          */ 00595000
         OI    TABLOOP,B'10000000'                                 0329 00596000
*               TABFOUND=OFF;       /* INDICATE ENTRY NOT FOUND.     */ 00597000
         NI    TABFOUND,B'01111111'                                0330 00598000
*               DO J=1 TO DIM(CMBTABL)/* LOOP THRU ALL OF TABL.      */ 00599000
*                     WHILE(TABLOOP=ON&CMBTABL(J)^=ZERO);/* UNTIL  0331 00600000
*                                      FOUND OR END.                 */ 00601000
         LA    J,1                                                 0331 00602000
@DL00331 TM    TABLOOP,B'10000000'                                 0331 00603000
         BNO   @DC00331                                            0331 00604000
         LR    @12,J                                               0331 00605000
         SLA   @12,2                                               0331 00606000
         L     @12,CMBTABL-4(@12)                                  0331 00607000
         LTR   @12,@12                                             0331 00608000
         BZ    @DC00331                                            0331 00609000
*                 IF CMBTABL(J)=ADDR(IDACMB) THEN/* IS THIS CMB IN 0332 00610000
*                                      THE TABLE?                    */ 00611000
         CR    @12,RWORK2                                          0332 00612000
         BNE   @RF00332                                            0332 00613000
*                   DO;             /* IF ENTRY FOUND THEN MAKE    0333 00614000
*                                      NOTES.                        */ 00615000
*                     TABFOUND=ON;  /* NOTE ENTRY ALREADY IN TABLE.  */ 00616000
         OI    TABFOUND,B'10000000'                                0334 00617000
*                     TABLOOP=OFF;  /* TURN OFF THE LOOP.            */ 00618000
         NI    TABLOOP,B'01111111'                                 0335 00619000
*                   END;            /* END OF PATH FOR ENTRY FOUND.  */ 00620000
*               END;                /* END OF LOOP THROUGH TABLE.    */ 00621000
@RF00332 AL    J,@CF00043                                          0337 00622000
         C     J,@CF01219                                          0337 00623000
         BNH   @DL00331                                            0337 00624000
@DC00331 DS    0H                                                  0338 00625000
*               IF TABFOUND=OFF&J<DIM(CMBTABL) THEN/* IF A NEW CMB 0338 00626000
*                                      THEN,                         */ 00627000
         TM    TABFOUND,B'10000000'                                0338 00628000
         BNZ   @RF00338                                            0338 00629000
         C     J,@CF01219                                          0338 00630000
         BNL   @RF00338                                            0338 00631000
*                 DO;               /* EXECUTE SPECIAL PATH FOR NEW     00632000
*                                      CMB.                          */ 00633000
*                   CMBTABL(J)=ADDR(IDACMB);/* PLACE THE CMB IN    0340 00634000
*                                      TABLE.                        */ 00635000
*                                                                  0340 00636000
         LR    @12,J                                               0340 00637000
         SLA   @12,2                                               0340 00638000
         ST    RWORK2,CMBTABL-4(@12)                               0340 00639000
*                   /*************************************************/ 00640000
*                   /*                                               */ 00641000
*                   /* DISPLAY THE CMB ASSOCIATED WITH THE AMBL.     */ 00642000
*                   /*                                               */ 00643000
*                   /*************************************************/ 00644000
*                                                                  0341 00645000
*                   DUMPID=IDCMB;   /* SET UP THE DUMP I.D.          */ 00646000
         MVI   DUMPID+3,C' '                                       0341 00647000
         MVC   DUMPID+4(27),DUMPID+3                               0341 00648000
         MVC   DUMPID(3),IDCMB                                     0341 00649000
*                   IDSIZE=LENGTH(IDCMB);/* INDICATE DUMP ID SIZE.   */ 00650000
         MVI   IDSIZE,X'03'                                        0342 00651000
*                   DUMPAD=ADDR(IDACMB);/* POINT TO THIS CMB.        */ 00652000
         ST    RWORK2,DUMPAD                                       0343 00653000
*                   DUMPLEN=LENGTH(IDACMB);/* PASS THE SIZE OF THE 0344 00654000
*                                      DATA.                         */ 00655000
         MVC   DUMPLEN(4),@CF00370                                 0344 00656000
*                   CALL DUMPER;    /* DUMP INDICATED DATA.          */ 00657000
         BAL   @14,DUMPER                                          0345 00658000
*                 END;              /* END OF PATH FOR NEW CMB.      */ 00659000
*             END;                  /* END OF PATH FOR GSR ACB/DEB.  */ 00660000
@RF00338 DS    0H                                                  0348 00661000
*       END;                        /* END OF PATH FOR VSAM DEB (GSR    00662000
*                                      OR NON-GSR).                  */ 00663000
@RF00307 DS    0H                                                  0348 00664000
@RC00290 DS    0H                                                  0349 00665000
*REDEB:                             /* LABEL FOR RETRY ON DEB CHAIN     00666000
*                                      ELEMENT.                      */ 00667000
*     DEBCTR=DEBCTR+ONE;            /* COUNT EACH DEB SCANNED.       */ 00668000
*                                                                  0349 00669000
@RF00287 DS    0H                                                  0349 00670000
REDEB    AL    DEBCTR,@CF00043                                     0349 00671000
*     /***************************************************************/ 00672000
*     /*                                                             */ 00673000
*     /* IF DEB CHAIN EXCEEDS DEBCBMX FORCE END OF CHAIN.            */ 00674000
*     /*                                                             */ 00675000
*     /***************************************************************/ 00676000
*                                                                  0350 00677000
*     IF RWORK1=DEBDEBB|            /* IF BLOCK POINTS TO ITSELF OR  */ 00678000
*         DEBCTR>DEBCNTMX THEN      /* HAVE MAX NUMBER DEBS BEEN   0350 00679000
*                                      SCANNED?                      */ 00680000
         L     @12,DEBDEBB-1(,RWORK1)                              0350 00681000
         LA    @12,0(,@12)                                         0350 00682000
         CR    RWORK1,@12                                          0350 00683000
         BE    @RT00350                                            0350 00684000
         C     DEBCTR,DEBCNTMX                                     0350 00685000
         BNH   @RF00350                                            0350 00686000
@RT00350 DS    0H                                                  0351 00687000
*       DO;                         /* MAXIMUM NUMBER OF DEBS      0351 00688000
*                                      REACHED, LOOP PROBABLE.       */ 00689000
*         LINE='EXCESSIVE DEB CHAIN DETECTED BY IDA0195A';/* ERROR   */ 00690000
         MVI   LINE+40,C' '                                        0352 00691000
         MVC   LINE+41(79),LINE+40                                 0352 00692000
         MVC   LINE(40),@CC03148                                   0352 00693000
*         CALL PRINT;               /* DISPLAY THE ERROR MESSAGE.    */ 00694000
         BAL   @14,PRINT                                           0353 00695000
*         CALL SPACE;               /* LEAVE A BLANK LINE FOR      0354 00696000
*                                      POSTERITY.                    */ 00697000
         BAL   @14,SPACE                                           0354 00698000
*         RWORK1=ZERO;              /* FORCE END OF CHAIN.           */ 00699000
         SLR   RWORK1,RWORK1                                       0355 00700000
*       END;                        /* END OF DEB ERROR TRAP.        */ 00701000
*     ELSE                          /* OTHERWISE CHAIN IS ALRIGHT.   */ 00702000
*       RWORK1=DEBDEBB;             /* GET THE NEXT DEB IN THE CHAIN.*/ 00703000
         B     @RC00350                                            0357 00704000
@RF00350 L     @12,DEBDEBB-1(,RWORK1)                              0357 00705000
         LA    @12,0(,@12)                                         0357 00706000
         LR    RWORK1,@12                                          0357 00707000
*   END;                            /* END OF PATH FOR LOCATING OPEN    00708000
*                                      DATA SETS FOR THIS TCB.       */ 00709000
@RC00350 DS    0H                                                  0358 00710000
@DE00286 LTR   RWORK1,RWORK1                                       0358 00711000
         BNZ   @DL00286                                            0358 00712000
*   IF GSRFOUND=OFF THEN            /* WERE ANY GSR DATA SETS FOUND? */ 00713000
         TM    GSRFOUND,B'10000000'                                0359 00714000
         BNZ   @RF00359                                            0359 00715000
*     DO;                           /* NO GSR DATA SETS WERE FOUND 0360 00716000
*                                      OPEN.                         */ 00717000
*       LINE='NO VSAM GSR DATA SETS FOUND OPEN';/* SEND MSG.         */ 00718000
         MVI   LINE+32,C' '                                        0361 00719000
         MVC   LINE+33(87),LINE+32                                 0361 00720000
         MVC   LINE(32),@CC03151                                   0361 00721000
*       CALL PRINT;                 /* PRINT THE MSG.                */ 00722000
         BAL   @14,PRINT                                           0362 00723000
*       CALL SPACE;                 /* SPACE ONE LINE.               */ 00724000
         BAL   @14,SPACE                                           0363 00725000
*     END;                          /* END OF PATH IF NO GSR D/S   0364 00726000
*                                      FOUND.                        */ 00727000
*   END DUMPOGSR;                   /* END OF ROUTINE FOR OPEN VSAM     00728000
*                                      GSR DATA SETS.                */ 00729000
@EL00003 DS    0H                                                  0365 00730000
@EF00003 DS    0H                                                  0365 00731000
@ER00003 LM    @14,@12,@SA00003                                    0365 00732000
         BR    @14                                                 0365 00733000
*DUMPVGTT:                                                         0366 00734000
*                                                                  0366 00735000
*/********************************************************************/ 00736000
*/*  FUNCTION - DISPLAY THE VGTT CHAIN FOR AN ASCB.                  */ 00737000
*/*  INPUT - THE ASCB IS PASSED IN ASCBPTR..                         */ 00738000
*/*  OUTPUT - THE VGTT CHAIN IS DISPLAYED.                           */ 00739000
*/********************************************************************/ 00740000
*                                                                  0366 00741000
*   PROC;                                                          0366 00742000
*                                                                  0366 00743000
DUMPVGTT STM   @14,@12,@SA00004                                    0366 00744000
*   /*****************************************************************/ 00745000
*   /*                                                               */ 00746000
*   /* DISPLAY THE VGTT CHAIN AND ASSOCIATED STORAGE.                */ 00747000
*   /*                                                               */ 00748000
*   /*****************************************************************/ 00749000
*                                                                  0367 00750000
*   IDSIZE=LENGTH(VGTTNAME);        /* INDICATE SIZE OF DUMP ID.     */ 00751000
         MVI   IDSIZE,X'04'                                        0367 00752000
*   RFY                                                            0368 00753000
*     IDAVGTT BASED(RWORK1);        /* LOCATE VGTT FROM LOCAL PTR.   */ 00754000
*   RWORK1=ASCBVGTT;                /* OBTAIN FIRST VGTT.            */ 00755000
         L     @12,ASCBPTR                                         0369 00756000
         L     RWORK1,ASCBVGTT(,@12)                               0369 00757000
*   VGTFOUND=OFF;                   /* INITIALIZE SWITCH.            */ 00758000
         NI    VGTFOUND,B'01111111'                                0370 00759000
*   CBCTR1=ONE;                     /* INITIALIZE FIRST LOOP COUNTER.*/ 00760000
         LA    CBCTR1,1                                            0371 00761000
*   DO WHILE(RWORK1^=ZERO&          /* DISPLAY IF NON-ZERO AND       */ 00762000
*         VGTTID=VGTTNAME);         /* IF VGTT ID INTACT.            */ 00763000
         B     @DE00372                                            0372 00764000
@DL00372 DS    0H                                                  0373 00765000
*     DUMPID=VGTTNAME;              /* INDICATE ID OF DATA.          */ 00766000
         MVI   DUMPID+4,C' '                                       0373 00767000
         MVC   DUMPID+5(26),DUMPID+4                               0373 00768000
         MVC   DUMPID(4),@CC01966                                  0373 00769000
*     VGTFOUND=ON;                  /* INDICATE VGTT CHAIN FOUND.    */ 00770000
         OI    VGTFOUND,B'10000000'                                0374 00771000
*     IDSIZE=LENGTH(VGTTNAME);      /* PASS THE DUMPID SIZE.         */ 00772000
         MVI   IDSIZE,X'04'                                        0375 00773000
*     DUMPAD=ADDR(IDAVGTT);         /* DISPLAT HTE VGTT.             */ 00774000
         ST    RWORK1,DUMPAD                                       0376 00775000
*     DUMPLEN=VGTTSIZE;             /* USE SIZE FROM THE VGTT.       */ 00776000
         L     @12,VGTTSIZE(,RWORK1)                               0377 00777000
         ST    @12,DUMPLEN                                         0377 00778000
*     CALL DUMPER;                  /* PRINT INDICATED DATA.         */ 00779000
*                                                                  0378 00780000
         BAL   @14,DUMPER                                          0378 00781000
*     /***************************************************************/ 00782000
*     /*                                                             */ 00783000
*     /* IF THIS VGTT CAN HAVE PSB IN CSA DISPLAY IT.                */ 00784000
*     /*                                                             */ 00785000
*     /***************************************************************/ 00786000
*                                                                  0379 00787000
*     IF VGTTOPEN=ON THEN           /* IS THIS DATA SET VGTT?        */ 00788000
         TM    VGTTOPEN(RWORK1),B'00001000'                        0379 00789000
         BNO   @RF00379                                            0379 00790000
*       DO;                         /* PROCESS A PSB CHAIN FOR THIS     00791000
*                                      PSB.                          */ 00792000
*         CBCTR2=ONE;               /* INITAILIZE SECOND COUNTER.    */ 00793000
         LA    CBCTR2,1                                            0381 00794000
*         RWORK2=VGTTPSB;           /* OBTAIN FIRST PSB IN CHAIN.    */ 00795000
         L     RWORK2,VGTTPSB(,RWORK1)                             0382 00796000
*         DO WHILE RWORK2^=ZERO;    /* IF DATA PRESENT,              */ 00797000
         B     @DE00383                                            0383 00798000
@DL00383 DS    0H                                                  0384 00799000
*           RFY                                                    0384 00800000
*             HEBHDREL BASED(RWORK2);/* USE LOCAL DATA PTR.          */ 00801000
*           DUMPID=IDPSB;           /* IDENTIFY THE DATA.            */ 00802000
         MVI   DUMPID+3,C' '                                       0385 00803000
         MVC   DUMPID+4(27),DUMPID+3                               0385 00804000
         MVC   DUMPID(3),IDPSB                                     0385 00805000
*           IDSIZE=LENGTH(IDPSB);   /* INDICATE DUMP ID SIZE.        */ 00806000
         MVI   IDSIZE,X'03'                                        0386 00807000
*           DUMPAD=RWORK2;          /* PASS THE PSB ADDRESS.         */ 00808000
         ST    RWORK2,DUMPAD                                       0387 00809000
*           DUMPLEN=HEBLN;          /* USE THE PSB DUMMY HEB FOR   0388 00810000
*                                      SIZE.                         */ 00811000
         L     @12,HEBLN-1(,RWORK2)                                0388 00812000
         LA    @12,0(,@12)                                         0388 00813000
         ST    @12,DUMPLEN                                         0388 00814000
*           CALL DUMPER;            /* DUMP THE PROT. SPHERE BLOCK.  */ 00815000
         BAL   @14,DUMPER                                          0389 00816000
*           CBCTR2=CBCTR2+ONE;      /* INCREMENT ELEMENT COUNTER.    */ 00817000
*                                                                  0390 00818000
         AL    CBCTR2,@CF00043                                     0390 00819000
*           /*********************************************************/ 00820000
*           /*                                                       */ 00821000
*           /* IF THE PSB FOR THIS VGTT EXCEEDS VSAMCBMX FORCE END OF*/ 00822000
*           /* CHAIN                                                 */ 00823000
*           /*                                                       */ 00824000
*           /*********************************************************/ 00825000
*                                                                  0391 00826000
*           IF RWORK2=HEBELCHN|     /* DOES BLOCK POINT TO ITSELF?   */ 00827000
*               CBCTR2>VSAMCBMX THEN/* MAX CHAIN LENGTH REACHED?     */ 00828000
         C     RWORK2,HEBELCHN(,RWORK2)                            0391 00829000
         BE    @RT00391                                            0391 00830000
         C     CBCTR2,VSAMCBMX                                     0391 00831000
         BNH   @RF00391                                            0391 00832000
@RT00391 DS    0H                                                  0392 00833000
*             DO;                   /* CHAIN LOOP PROBABLE.          */ 00834000
*               LINE='EXCESSIVE VGTT PSB CHAIN DETECTED BY IDA0195A';   00835000
         MVI   LINE+45,C' '                                        0393 00836000
         MVC   LINE+46(74),LINE+45                                 0393 00837000
         MVC   LINE(45),@CC03159                                   0393 00838000
*                                   /* ERROR                         */ 00839000
*               CALL PRINT;         /* DISPLAY THE ERROR MESSAGE.    */ 00840000
         BAL   @14,PRINT                                           0394 00841000
*               CALL SPACE;         /* SPACE UP ANOTHER LINE.        */ 00842000
         BAL   @14,SPACE                                           0395 00843000
*               RWORK2=ZERO;        /* FORCE END OF CHAIN.           */ 00844000
         SLR   RWORK2,RWORK2                                       0396 00845000
*             END;                  /* END OF ERROR DETECTION.       */ 00846000
*           ELSE                    /* OTHER WISE BLOCK IS FINE.     */ 00847000
*             RWORK2=HEBELCHN;      /* OBTAIN NEXT PSB IN CHAIN.     */ 00848000
         B     @RC00391                                            0398 00849000
@RF00391 L     RWORK2,HEBELCHN(,RWORK2)                            0398 00850000
*         END;                      /* END OF PATH FOR PSB CHAIN   0399 00851000
*                                      SCAN.                         */ 00852000
@RC00391 DS    0H                                                  0399 00853000
@DE00383 LTR   RWORK2,RWORK2                                       0399 00854000
         BNZ   @DL00383                                            0399 00855000
*       END;                        /* END OF PATH FOR D/S TYPE VGTT.*/ 00856000
*     IF VGTTSDS=ON|VGTTCBIC=ON THEN/* IF SYS. DS OR CBIC    @ZA16012*/ 00857000
@RF00379 TM    VGTTSDS(RWORK1),B'10000100'                         0401 00858000
         BZ    @RF00401                                            0401 00859000
*       CALL DUMPSDS;               /* DUMP CONTROL BLOCKS CONNECTED    00860000
*                                      TO THE SYSTEM DATA SET OR THE    00861000
*                                      CBIC OPTION           @ZA16012*/ 00862000
         BAL   @14,DUMPSDS                                         0402 00863000
*REVGTT:                            /* LABEL FOR RETRY ON VGTT CHAIN.*/ 00864000
*     CBCTR1=CBCTR1+ONE;            /* INCREMENT CHAIN ELEMENT COUNT.*/ 00865000
*                                                                  0403 00866000
@RF00401 DS    0H                                                  0403 00867000
REVGTT   AL    CBCTR1,@CF00043                                     0403 00868000
*     /***************************************************************/ 00869000
*     /*                                                             */ 00870000
*     /* FORCE LOGICAL END OF CHAIN IF VGTTCBMX EXCEEDED.    @ZA12980*/ 00871000
*     /*                                                             */ 00872000
*     /***************************************************************/ 00873000
*                                                                  0404 00874000
*     IF RWORK1=VGTTNEXT|           /* DOES BLOCK POINT TO ITSELF?   */ 00875000
*         CBCTR1>VGTTCBMX THEN      /* MAX CHAIN SIZE REACHED?     0404 00876000
*                                                            @ZA12980*/ 00877000
         C     RWORK1,VGTTNEXT(,RWORK1)                            0404 00878000
         BE    @RT00404                                            0404 00879000
         C     CBCTR1,VGTTCBMX                                     0404 00880000
         BNH   @RF00404                                            0404 00881000
@RT00404 DS    0H                                                  0405 00882000
*       DO;                         /* PROBABLE CHAIN LOOP.          */ 00883000
*         LINE='EXCESSIVE VGTT CHAIN DETECTED BY IDA0195A';/* ERROR  */ 00884000
         MVI   LINE+41,C' '                                        0406 00885000
         MVC   LINE+42(78),LINE+41                                 0406 00886000
         MVC   LINE(41),@CC03166                                   0406 00887000
*         CALL PRINT;               /* DISPLAY THE ERROR MESSAGE.    */ 00888000
         BAL   @14,PRINT                                           0407 00889000
*         CALL SPACE;               /* SPACE A BLANK LINE.           */ 00890000
         BAL   @14,SPACE                                           0408 00891000
*         RWORK1=ZERO;              /* FORCE END OF CHAIN.           */ 00892000
         SLR   RWORK1,RWORK1                                       0409 00893000
*       END;                        /* END OF ERROR DETECTION.       */ 00894000
*     ELSE                          /* OTHER WISE THE CHAIN IS FINE. */ 00895000
*       RWORK1=VGTTNEXT;            /* OBTAIN NEXT VGTT IN CHAIN.    */ 00896000
         B     @RC00404                                            0411 00897000
@RF00404 L     RWORK1,VGTTNEXT(,RWORK1)                            0411 00898000
*   END;                            /* END OF VGTT CHAIN LOOP.       */ 00899000
@RC00404 DS    0H                                                  0412 00900000
@DE00372 LTR   RWORK1,RWORK1                                       0412 00901000
         BZ    @DC00372                                            0412 00902000
         CLC   VGTTID(4,RWORK1),@CC01966                           0412 00903000
         BE    @DL00372                                            0412 00904000
@DC00372 DS    0H                                                  0413 00905000
*   IF VGTFOUND=OFF THEN            /* WERE ANY VGTT.S FOUND?        */ 00906000
         TM    VGTFOUND,B'10000000'                                0413 00907000
         BNZ   @RF00413                                            0413 00908000
*     DO;                           /* NO VGTT CHAIN WAS FOUND.      */ 00909000
*       LINE='NO VSAM VGTT CHAIN PRESENT FOR THIS ASCB';/* MSG       */ 00910000
         MVI   LINE+40,C' '                                        0415 00911000
         MVC   LINE+41(79),LINE+40                                 0415 00912000
         MVC   LINE(40),@CC03170                                   0415 00913000
*       CALL PRINT;                 /* OUTPUT THE MSG.               */ 00914000
         BAL   @14,PRINT                                           0416 00915000
*       CALL SPACE;                 /* SKIP ONE LINE.                */ 00916000
         BAL   @14,SPACE                                           0417 00917000
*     END;                          /* END OF PATH FOR NO VGTT CHAIN.*/ 00918000
*   END DUMPVGTT;                   /* END OF PATH FOR DUMPVGTT.     */ 00919000
@EL00004 DS    0H                                                  0419 00920000
@EF00004 DS    0H                                                  0419 00921000
@ER00004 LM    @14,@12,@SA00004                                    0419 00922000
         BR    @14                                                 0419 00923000
*DUMPAGSR:                                                         0420 00924000
*                                                                  0420 00925000
*/********************************************************************/ 00926000
*/*  FUNCTION - DISPLAY FOR A  GSR POOL CONTROL BLOCKS.              */ 00927000
*/*  INPUT - PROCESSING BEGINS WITH THE CVT.                         */ 00928000
*/*  OUTPUT - GSR CONTROL BLOCKS ARE DISPLAYED.                      */ 00929000
*/********************************************************************/ 00930000
*                                                                  0420 00931000
*   PROC;                                                          0420 00932000
DUMPAGSR STM   @14,@12,@SA00005                                    0420 00933000
*   IF CVTCBSP=ZERO THEN            /* IS THE AMCBS PRESENT?         */ 00934000
         L     @12,CVTPTR                                          0421 00935000
         L     @12,CVTCBSP(,@12)                                   0421 00936000
         LTR   @12,@12                                             0421 00937000
         BNZ   @RF00421                                            0421 00938000
*     GSRPOOL=OFF;                  /* IF NOT THEN GSR NOT PRESENT.  */ 00939000
         NI    GSRPOOL,B'01111111'                                 0422 00940000
*   ELSE                            /* OTHERWISE THE AMCBS IS      0423 00941000
*                                      PRESENT.                      */ 00942000
*     IF CBSVPTR(KEYTCB/16+ONE)^=ZERO THEN/* IS THERE A VSRT PTR?    */ 00943000
         B     @RC00421                                            0423 00944000
@RF00421 SLR   @12,@12                                             0423 00945000
         IC    @12,KEYTCB                                          0423 00946000
         SRL   @12,4                                               0423 00947000
         SLA   @12,3                                               0423 00948000
         L     @08,CVTPTR                                          0423 00949000
         L     @08,CVTCBSP(,@08)                                   0423 00950000
         L     @12,CBSVPTR(@12,@08)                                0423 00951000
         LTR   @12,@12                                             0423 00952000
         BZ    @RF00423                                            0423 00953000
*       GSRPOOL=ON;                 /* YES. INDICATE POLL PTR      0424 00954000
*                                      PRESENT.                      */ 00955000
         OI    GSRPOOL,B'10000000'                                 0424 00956000
*     ELSE                          /* NO VSRT PTR ALTHOUGH AMCBS  0425 00957000
*                                      PRESENT.                      */ 00958000
*       GSRPOOL=OFF;                /* INDICATE NO GSR POOL PRESENT. */ 00959000
         B     @RC00423                                            0425 00960000
@RF00423 NI    GSRPOOL,B'01111111'                                 0425 00961000
*   IF GSRPOOL=OFF THEN             /* IS THERE A GSR POOL IN THE  0426 00962000
*                                      SYSTEM?                       */ 00963000
@RC00423 DS    0H                                                  0426 00964000
@RC00421 TM    GSRPOOL,B'10000000'                                 0426 00965000
         BNZ   @RF00426                                            0426 00966000
*     DO;                           /* PATH FOR NO POOL PRESENT.     */ 00967000
*       LINE='VSAM GSR POOL NOT PRESENT';                          0428 00968000
         MVI   LINE+25,C' '                                        0428 00969000
         MVC   LINE+26(94),LINE+25                                 0428 00970000
         MVC   LINE(25),@CC03176                                   0428 00971000
*       CALL PRINT;                 /* DISPLAY THE ABOVE MSG.        */ 00972000
         BAL   @14,PRINT                                           0429 00973000
*       CALL SPACE;                 /* SPACE UP ONE LINE.            */ 00974000
         BAL   @14,SPACE                                           0430 00975000
*     END;                          /* END OF PATH FOR NO POOL     0431 00976000
*                                      PRESENT.                      */ 00977000
*   ELSE                            /* OTHERWISE A GSR POOL IS     0432 00978000
*                                      PRESENT.                      */ 00979000
*     IF GSRFOUND=ON|               /* IF GSR DATA SETS FOUND OR IF  */ 00980000
*         (TCBJSTCB=ADDR(TCB)&VSRTASCB=ASCBPTR) THEN/* JSTCB | OWNER    00981000
*                                      ASID                          */ 00982000
         B     @RC00426                                            0432 00983000
@RF00426 TM    GSRFOUND,B'10000000'                                0432 00984000
         BO    @RT00432                                            0432 00985000
         L     @12,TCBAD                                           0432 00986000
         C     @12,TCBJSTCB(,@12)                                  0432 00987000
         BNE   @RF00432                                            0432 00988000
         SLR   @12,@12                                             0432 00989000
         IC    @12,KEYTCB                                          0432 00990000
         SRL   @12,4                                               0432 00991000
         SLA   @12,3                                               0432 00992000
         L     @08,CVTPTR                                          0432 00993000
         L     @08,CVTCBSP(,@08)                                   0432 00994000
         L     @12,CBSVPTR(@12,@08)                                0432 00995000
         CLC   VSRTASCB(4,@12),ASCBPTR                             0432 00996000
         BNE   @RF00432                                            0432 00997000
@RT00432 DS    0H                                                  0433 00998000
*       DO;                         /* PATH FOR GSR POOL PRESENT   0433 00999000
*                                      STARTS                        */ 01000000
*                                                                  0433 01001000
*         /***********************************************************/ 01002000
*         /*                                                         */ 01003000
*         /* DISPLAY THE AMCBS.                                      */ 01004000
*         /*                                                         */ 01005000
*         /***********************************************************/ 01006000
*                                                                  0434 01007000
*         DUMPID=IDAMCBS;           /* IDENTIFY THE DATA.            */ 01008000
         MVI   DUMPID+5,C' '                                       0434 01009000
         MVC   DUMPID+6(25),DUMPID+5                               0434 01010000
         MVC   DUMPID(5),IDAMCBS                                   0434 01011000
*         IDSIZE=LENGTH(IDAMCBS);   /* PASS THE SZIE OF DUMP ID.     */ 01012000
         MVI   IDSIZE,X'05'                                        0435 01013000
*         DUMPAD=ADDR(AMCBS);       /* LOCATE THE GLOBAL AMCBS.      */ 01014000
         L     @12,CVTPTR                                          0436 01015000
         L     @12,CVTCBSP(,@12)                                   0436 01016000
         ST    @12,DUMPAD                                          0436 01017000
*         DUMPLEN=LENGTH(AMCBS);    /* USE MACRO LENGTH.             */ 01018000
         MVC   DUMPLEN(4),@CF01739                                 0437 01019000
*         CALL DUMPER;              /* DISPLAY THE VSRT BASE(AMCBS). */ 01020000
*                                                                  0438 01021000
         BAL   @14,DUMPER                                          0438 01022000
*         /***********************************************************/ 01023000
*         /*                                                         */ 01024000
*         /* DISPLAY THE VSRT.                                       */ 01025000
*         /*                                                         */ 01026000
*         /***********************************************************/ 01027000
*                                                                  0439 01028000
*         DUMPID=IDVSRT;            /* IDENTIFY THE VSRT.            */ 01029000
         MVI   DUMPID+4,C' '                                       0439 01030000
         MVC   DUMPID+5(26),DUMPID+4                               0439 01031000
         MVC   DUMPID(4),IDVSRT                                    0439 01032000
*         IDSIZE=LENGTH(IDVSRT);    /* INDICATE THE DUMP ID SIZE.    */ 01033000
         MVI   IDSIZE,X'04'                                        0440 01034000
*         DUMPAD=ADDR(IDAVSRT);     /* LOCATE THE VSRT.              */ 01035000
         SLR   @12,@12                                             0441 01036000
         IC    @12,KEYTCB                                          0441 01037000
         SRL   @12,4                                               0441 01038000
         SLA   @12,3                                               0441 01039000
         L     @08,CVTPTR                                          0441 01040000
         L     @08,CVTCBSP(,@08)                                   0441 01041000
         L     @12,CBSVPTR(@12,@08)                                0441 01042000
         ST    @12,DUMPAD                                          0441 01043000
*         DUMPLEN=VSRTLEN;          /* PASS THE SIZE FROM THE BLOCK. */ 01044000
         LH    @12,VSRTLEN(,@12)                                   0442 01045000
         ST    @12,DUMPLEN                                         0442 01046000
*         CALL DUMPER;              /* DISPLAY THE KEY 7 VSRT.       */ 01047000
*                                                                  0443 01048000
         BAL   @14,DUMPER                                          0443 01049000
*         /***********************************************************/ 01050000
*         /*                                                         */ 01051000
*         /* DISPLAY THE WORKING STORAGE HEADER FOR CPA.S (CPA WSHD).*/ 01052000
*         /*                                                         */ 01053000
*         /***********************************************************/ 01054000
*                                                                  0444 01055000
*         RWORK1=VSRTCPAH;          /* LOCATE THE CPA WSHD.          */ 01056000
         SLR   @12,@12                                             0444 01057000
         IC    @12,KEYTCB                                          0444 01058000
         SRL   @12,4                                               0444 01059000
         SLA   @12,3                                               0444 01060000
         L     @08,CVTPTR                                          0444 01061000
         L     @08,CVTCBSP(,@08)                                   0444 01062000
         L     @12,CBSVPTR(@12,@08)                                0444 01063000
         L     RWORK1,VSRTCPAH(,@12)                               0444 01064000
*         RESPECIFY                                                0445 01065000
*           IDAWSHD BASED(RWORK1);  /* USE LOCAL POINTER.            */ 01066000
*         CBCTR1=ONE;               /* INITIALIZE ELEMENT COUNTER.   */ 01067000
         LA    CBCTR1,1                                            0446 01068000
*         DO WHILE RWORK1^=0;       /* SCAN UNTIL END OF CHAIN.      */ 01069000
         B     @DE00447                                            0447 01070000
@DL00447 DS    0H                                                  0448 01071000
*           DUMPID=IDCPA;           /* IDENTIFY THE DATA.            */ 01072000
         MVI   DUMPID+8,C' '                                       0448 01073000
         MVC   DUMPID+9(22),DUMPID+8                               0448 01074000
         MVC   DUMPID(8),IDCPA                                     0448 01075000
*           IDSIZE=LENGTH(IDCPA);   /* PASS SIZE OF DUMP ID.         */ 01076000
         MVI   IDSIZE,X'08'                                        0449 01077000
*           DUMPAD=RWORK1;          /* PASS THE DATA ADDR.           */ 01078000
         ST    RWORK1,DUMPAD                                       0450 01079000
*           DUMPLEN=LENGTH(IDAWSHD);/* USE MACRO LENGTH.             */ 01080000
         MVC   DUMPLEN(4),@CF00032                                 0451 01081000
*           CALL DUMPER;            /* DISPLAY THE DATA.             */ 01082000
*                                                                  0452 01083000
         BAL   @14,DUMPER                                          0452 01084000
*           /*********************************************************/ 01085000
*           /*                                                       */ 01086000
*           /* DISPLAY STORAGE OWNED BY THE CPA WSHD.                */ 01087000
*           /*                                                       */ 01088000
*           /*********************************************************/ 01089000
*                                                                  0453 01090000
*           CBCTR2=ONE;             /* INITIALIZE ELEMENT COUNTER.   */ 01091000
         LA    @12,1                                               0453 01092000
         LR    CBCTR2,@12                                          0453 01093000
*           DO I1=1 TO WSHDNUS;     /* LOOP THRU WSHD SLOTS          */ 01094000
         LR    I1,@12                                              0454 01095000
         B     @DE00454                                            0454 01096000
@DL00454 DS    0H                                                  0455 01097000
*             DUMPID=IDCPAOS;       /* IDENTIFY THE STORAGE.         */ 01098000
         MVI   DUMPID+22,C' '                                      0455 01099000
         MVC   DUMPID+23(8),DUMPID+22                              0455 01100000
         MVC   DUMPID(22),IDCPAOS                                  0455 01101000
*             IDSIZE=LENGTH(IDCPAOS);/* INDICATE SIZE OF DUMPID.     */ 01102000
         MVI   IDSIZE,X'16'                                        0456 01103000
*             DUMPAD=WSHDSAD(I1);   /* OBTAIN ADDR FROM SLOT.        */ 01104000
         LR    @12,I1                                              0457 01105000
         SLA   @12,4                                               0457 01106000
         L     @08,WSHDSAD-16(@12,RWORK1)                          0457 01107000
         ST    @08,DUMPAD                                          0457 01108000
*             DUMPLEN=WSHDSFLN(I1); /* OBTAIN LENGTH FROM SLOT.      */ 01109000
         L     @12,WSHDSFLN-17(@12,RWORK1)                         0458 01110000
         LA    @12,0(,@12)                                         0458 01111000
         ST    @12,DUMPLEN                                         0458 01112000
*             CALL DUMPER;          /* DISPLAY OWNED STORAGE.        */ 01113000
         BAL   @14,DUMPER                                          0459 01114000
*             IF I1>DIM(WSHDSLT) THEN/* ERROR IN COUNT?              */ 01115000
         C     I1,@CF00118                                         0460 01116000
         BNH   @RF00460                                            0460 01117000
*               DO;                 /* ERROR DETECTED.               */ 01118000
*                 LINE=                                            0462 01119000
*'EXCESSIVE CPA WSHD SLOT NUMBER DETECTED BY IDA0195A';            0462 01120000
         MVI   LINE+51,C' '                                        0462 01121000
         MVC   LINE+52(68),LINE+51                                 0462 01122000
         MVC   LINE(51),@CC03180                                   0462 01123000
*                 CALL PRINT;       /* DISPLAY THE ABOVE ERROR MSG.  */ 01124000
         BAL   @14,PRINT                                           0463 01125000
*                 CALL SPACE;       /* SPACE UP ONE LINE ALSO.       */ 01126000
         BAL   @14,SPACE                                           0464 01127000
*                 I1=WSHDNUS;       /* FORCE END OF SCAN.            */ 01128000
         LH    I1,WSHDNUS(,RWORK1)                                 0465 01129000
*               END;                /* END OF ERROR DETECTION GROUP. */ 01130000
*           END;                    /* END OF THE PATH FOR WSHD OWNED   01131000
*                                      STORAGE.                      */ 01132000
@RF00460 AL    I1,@CF00043                                         0467 01133000
@DE00454 CH    I1,WSHDNUS(,RWORK1)                                 0467 01134000
         BNH   @DL00454                                            0467 01135000
*RECPAWSD:                          /* LABEL FOR RETRY ON NEXT CPA 0468 01136000
*                                      WSHD.                         */ 01137000
*           CBCTR1=CBCTR1+ONE;      /* COUNT ELEMENTS.               */ 01138000
*                                                                  0468 01139000
RECPAWSD AL    CBCTR1,@CF00043                                     0468 01140000
*           /*********************************************************/ 01141000
*           /*                                                       */ 01142000
*           /* IF CHAIN EXCEEDS VSAMCBMX FORCE END OF CHAIN.         */ 01143000
*           /*                                                       */ 01144000
*           /*********************************************************/ 01145000
*                                                                  0469 01146000
*           IF RWORK1=WSHDNEXT|     /* DOES BLOCK POINT TO ITSELF?   */ 01147000
*               CBCTR1>VSAMCBMX THEN/* SHOULD CHAIN HAVE ENDED?      */ 01148000
         L     @12,WSHDNEXT-1(,RWORK1)                             0469 01149000
         LA    @12,0(,@12)                                         0469 01150000
         CR    RWORK1,@12                                          0469 01151000
         BE    @RT00469                                            0469 01152000
         C     CBCTR1,VSAMCBMX                                     0469 01153000
         BNH   @RF00469                                            0469 01154000
@RT00469 DS    0H                                                  0470 01155000
*             DO;                   /* PROBABLE CHAIN LOOP         0470 01156000
