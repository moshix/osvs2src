*               LINE='EXCESSIVE CPA WSHD CHAIN DETECTED BY IDA0195A';   01158000
         MVI   LINE+45,C' '                                        0471 01159000
         MVC   LINE+46(74),LINE+45                                 0471 01160000
         MVC   LINE(45),@CC03186                                   0471 01161000
*               CALL PRINT;         /* DISPLAY THE ABOVE MESSAGE.    */ 01162000
         BAL   @14,PRINT                                           0472 01163000
*               CALL SPACE;         /* ALSO PRINT A BLANK LINE.      */ 01164000
         BAL   @14,SPACE                                           0473 01165000
*               RWORK1=ZERO;        /* FORCE END OF CHAIN.           */ 01166000
         SLR   RWORK1,RWORK1                                       0474 01167000
*             END;                  /* END OF ERROR DETECTION.       */ 01168000
*           ELSE                    /* OTHERWISE BLOCK IS FINE.      */ 01169000
*             RWORK1=WSHDNEXT;      /* GET THE NEXT WSHD IN THE    0476 01170000
*                                      CHAIN.                        */ 01171000
         B     @RC00469                                            0476 01172000
@RF00469 L     @12,WSHDNEXT-1(,RWORK1)                             0476 01173000
         LA    @12,0(,@12)                                         0476 01174000
         LR    RWORK1,@12                                          0476 01175000
*         END;                      /* END OF THE PATH FOR CPA WSHD     01176000
*                                      AND ASSOCIATED STORAGE.       */ 01177000
*                                                                  0477 01178000
@RC00469 DS    0H                                                  0477 01179000
@DE00447 LTR   RWORK1,RWORK1                                       0477 01180000
         BNZ   @DL00447                                            0477 01181000
*         /***********************************************************/ 01182000
*         /*                                                         */ 01183000
*         /* DISPLAY THE WORKING STORAGE HEADER (WSHD).              */ 01184000
*         /*                                                         */ 01185000
*         /***********************************************************/ 01186000
*                                                                  0478 01187000
*         RWORK1=VSRTWAH;           /* POINT TO THE WSHD.            */ 01188000
         SLR   @12,@12                                             0478 01189000
         IC    @12,KEYTCB                                          0478 01190000
         SRL   @12,4                                               0478 01191000
         SLA   @12,3                                               0478 01192000
         L     @08,CVTPTR                                          0478 01193000
         L     @08,CVTCBSP(,@08)                                   0478 01194000
         L     @12,CBSVPTR(@12,@08)                                0478 01195000
         L     RWORK1,VSRTWAH(,@12)                                0478 01196000
*         RESPECIFY                                                0479 01197000
*           IDAWSHD BASED(RWORK1);  /* USE LOCAL POINTER.            */ 01198000
*         CBCTR1=ONE;               /* INITIALZE ELEMENT COUNT.      */ 01199000
         LA    CBCTR1,1                                            0480 01200000
*         DO WHILE RWORK1^=0;       /* LOOP UNTIL END OF CHAIN.      */ 01201000
         B     @DE00481                                            0481 01202000
@DL00481 DS    0H                                                  0482 01203000
*           DUMPID=IDWSHD;          /* NAME THE DATA.                */ 01204000
         MVI   DUMPID+4,C' '                                       0482 01205000
         MVC   DUMPID+5(26),DUMPID+4                               0482 01206000
         MVC   DUMPID(4),IDWSHD                                    0482 01207000
*           IDSIZE=LENGTH(IDWSHD);  /* INDICATE DUMP I D SIZE.       */ 01208000
         MVI   IDSIZE,X'04'                                        0483 01209000
*           DUMPAD=RWORK1;          /* PASS THE DATA ADRS.           */ 01210000
         ST    RWORK1,DUMPAD                                       0484 01211000
*           DUMPLEN=LENGTH(IDAWSHD);/* USE MACRO LENGTH.             */ 01212000
         MVC   DUMPLEN(4),@CF00032                                 0485 01213000
*           CALL DUMPER;            /* DISPLAY NAMED DATA.           */ 01214000
*                                                                  0486 01215000
         BAL   @14,DUMPER                                          0486 01216000
*           /*********************************************************/ 01217000
*           /*                                                       */ 01218000
*           /* DISPLAY STORAGE OWNED BY THE WSHD.                    */ 01219000
*           /*                                                       */ 01220000
*           /*********************************************************/ 01221000
*                                                                  0487 01222000
*           CBCTR2=ONE;             /* INITAILIZE 2ND CHAIN COUNT.   */ 01223000
         LA    @12,1                                               0487 01224000
         LR    CBCTR2,@12                                          0487 01225000
*           DO I1=1 TO WSHDNUS;     /* SCAN THRU SLOTS               */ 01226000
         LR    I1,@12                                              0488 01227000
         B     @DE00488                                            0488 01228000
@DL00488 DS    0H                                                  0489 01229000
*             DUMPID=IDWSHDOS;      /* NAME THE DATA.                */ 01230000
         MVI   DUMPID+18,C' '                                      0489 01231000
         MVC   DUMPID+19(12),DUMPID+18                             0489 01232000
         MVC   DUMPID(18),IDWSHDOS                                 0489 01233000
*             IDSIZE=LENGTH(IDWSHDOS);/* PASS DUMP ID SIZE.          */ 01234000
         MVI   IDSIZE,X'12'                                        0490 01235000
*             DUMPAD=WSHDSAD(I1);   /* OBTAIN ADDR FROM THE SLOT.    */ 01236000
         LR    @12,I1                                              0491 01237000
         SLA   @12,4                                               0491 01238000
         L     @08,WSHDSAD-16(@12,RWORK1)                          0491 01239000
         ST    @08,DUMPAD                                          0491 01240000
*             DUMPLEN=WSHDSFLN(I1); /* OBTAIN LENGTH FROM SLOT.      */ 01241000
         L     @12,WSHDSFLN-17(@12,RWORK1)                         0492 01242000
         LA    @12,0(,@12)                                         0492 01243000
         ST    @12,DUMPLEN                                         0492 01244000
*             CALL DUMPER;          /* DISPLAY OWNED STORAGE.        */ 01245000
         BAL   @14,DUMPER                                          0493 01246000
*             IF I1>DIM(WSHDSLT) THEN/* ERROR IN SLOT COUNT?         */ 01247000
         C     I1,@CF00118                                         0494 01248000
         BNH   @RF00494                                            0494 01249000
*               DO;                 /* ERROR DETECTED.               */ 01250000
*                 LINE='EXCESSIVE WSHD SLOT NUMBER DETECTED BY IDA0195A 01251000
*                     ;                                            0496 01252000
         MVI   LINE+47,C' '                                        0496 01253000
         MVC   LINE+48(72),LINE+47                                 0496 01254000
         MVC   LINE(47),@CC03191                                   0496 01255000
*                 CALL PRINT;       /* DISPLAY THE ABOVE ERROR MSG.  */ 01256000
         BAL   @14,PRINT                                           0497 01257000
*                 CALL SPACE;       /* SPACE UP ANOTHER LINE.        */ 01258000
         BAL   @14,SPACE                                           0498 01259000
*                 I1=WSHDNUS;       /* FORCE END OF SCAN.            */ 01260000
         LH    I1,WSHDNUS(,RWORK1)                                 0499 01261000
*               END;                /* END OF ERROR DETECTION GROUP. */ 01262000
*           END;                    /* END OF THE PATH FOR WSHD OWNED   01263000
*                                      STORAGE .                     */ 01264000
@RF00494 AL    I1,@CF00043                                         0501 01265000
@DE00488 CH    I1,WSHDNUS(,RWORK1)                                 0501 01266000
         BNH   @DL00488                                            0501 01267000
*REWSHD:                            /* LABEL FOR RETRY ON NEXT WSHD. */ 01268000
*           CBCTR1=CBCTR1+ONE;      /* COUNT EACH WSHD ELEMENT.      */ 01269000
*                                                                  0502 01270000
REWSHD   AL    CBCTR1,@CF00043                                     0502 01271000
*           /*********************************************************/ 01272000
*           /*                                                       */ 01273000
*           /* IF CHAIN EXCEEDS VSAMCBMX FORCE END OF CHAIN.         */ 01274000
*           /*                                                       */ 01275000
*           /*********************************************************/ 01276000
*                                                                  0503 01277000
*           IF RWORK1=WSHDNEXT|     /* DOES BLOCK POINT TO ITSELF?   */ 01278000
*               CBCTR1>VSAMCBMX THEN/* SHOULD THE CHAIN HAVE ENDED?  */ 01279000
         L     @12,WSHDNEXT-1(,RWORK1)                             0503 01280000
         LA    @12,0(,@12)                                         0503 01281000
         CR    RWORK1,@12                                          0503 01282000
         BE    @RT00503                                            0503 01283000
         C     CBCTR1,VSAMCBMX                                     0503 01284000
         BNH   @RF00503                                            0503 01285000
@RT00503 DS    0H                                                  0504 01286000
*             DO;                   /* PROBABLE CHAIN LOOP.          */ 01287000
*               LINE='EXCESSIVE WSHD CHAIN DETECTED BY IDA0195A';/*     01288000
*                                      ERROR                         */ 01289000
         MVI   LINE+41,C' '                                        0505 01290000
         MVC   LINE+42(78),LINE+41                                 0505 01291000
         MVC   LINE(41),@CC03197                                   0505 01292000
*               CALL PRINT;         /* DISPLAY THE ABOVE ERROR     0506 01293000
*                                      MESSAGE.                      */ 01294000
         BAL   @14,PRINT                                           0506 01295000
*               CALL SPACE;         /* BUT ALSO LEAVE A BLANK LINE.  */ 01296000
         BAL   @14,SPACE                                           0507 01297000
*               RWORK1=ZERO;        /* FORCE END OF CHAIN.           */ 01298000
         SLR   RWORK1,RWORK1                                       0508 01299000
*             END;                  /* END OF ERROR DETECTION GROUP. */ 01300000
*           ELSE                    /* OTHERWISE THE BLOCK IS FINE.  */ 01301000
*             RWORK1=WSHDNEXT;      /* OBTAIN THE NEXT WSHD IN THE 0510 01302000
*                                      CHAIN.                        */ 01303000
         B     @RC00503                                            0510 01304000
@RF00503 L     @12,WSHDNEXT-1(,RWORK1)                             0510 01305000
         LA    @12,0(,@12)                                         0510 01306000
         LR    RWORK1,@12                                          0510 01307000
*         END;                      /* END OF THE PATH FOR WSHD AND     01308000
*                                      ASSOCIATED STORAGE.           */ 01309000
*                                                                  0511 01310000
@RC00503 DS    0H                                                  0511 01311000
@DE00481 LTR   RWORK1,RWORK1                                       0511 01312000
         BNZ   @DL00481                                            0511 01313000
*         /***********************************************************/ 01314000
*         /*                                                         */ 01315000
*         /* DISPLAY VSRT CSL OWNED STORAGE.                         */ 01316000
*         /*                                                         */ 01317000
*         /***********************************************************/ 01318000
*                                                                  0512 01319000
*         MWADCTR=ONE;              /* INITIALIZE TO FIRST ENTRY.    */ 01320000
         LA    MWADCTR,1                                           0512 01321000
*         TABLOOP=ON;               /* ALLOW LOOPING.                */ 01322000
         OI    TABLOOP,B'10000000'                                 0513 01323000
*         DO WHILE(TABLOOP=ON);                                    0514 01324000
         B     @DE00514                                            0514 01325000
@DL00514 DS    0H                                                  0515 01326000
*           DUMPID=IDVSRTOS;        /* ID OF THE DATA.               */ 01327000
         MVI   DUMPID+18,C' '                                      0515 01328000
         MVC   DUMPID+19(12),DUMPID+18                             0515 01329000
         MVC   DUMPID(18),IDVSRTOS                                 0515 01330000
*           IDSIZE=LENGTH(IDVSRTOS);/* INDICATE SIZE OF DUMP ID.     */ 01331000
         MVI   IDSIZE,X'12'                                        0516 01332000
*           DUMPAD=VSRTCSAD(MWADCTR);/* PASS ADDRESS OF OWNED      0517 01333000
*                                      STORAGE.                      */ 01334000
         LR    @12,MWADCTR                                         0517 01335000
         SLA   @12,3                                               0517 01336000
         SLR   @08,@08                                             0517 01337000
         IC    @08,KEYTCB                                          0517 01338000
         SRL   @08,4                                               0517 01339000
         SLA   @08,3                                               0517 01340000
         L     @05,CVTPTR                                          0517 01341000
         L     @05,CVTCBSP(,@05)                                   0517 01342000
         L     @08,CBSVPTR(@08,@05)                                0517 01343000
         L     @05,VSRTCSAD-9(@12,@08)                             0517 01344000
         LA    @05,0(,@05)                                         0517 01345000
         ST    @05,DUMPAD                                          0517 01346000
*           DUMPLEN=VSRTCSLN(MWADCTR);/* PASS SIZE OF STORAGE.       */ 01347000
         L     @12,VSRTCSLN-9(@12,@08)                             0518 01348000
         LA    @12,0(,@12)                                         0518 01349000
         ST    @12,DUMPLEN                                         0518 01350000
*           CALL DUMPER;            /* DUMP OUT THE DATA.            */ 01351000
         BAL   @14,DUMPER                                          0519 01352000
*           IF VSRTCSVS(MWADCTR)=ON THEN/* LAST ENTRY IN TABLE FOUND?*/ 01353000
         LR    @12,MWADCTR                                         0520 01354000
         SLA   @12,3                                               0520 01355000
         SLR   @08,@08                                             0520 01356000
         IC    @08,KEYTCB                                          0520 01357000
         SRL   @08,4                                               0520 01358000
         SLA   @08,3                                               0520 01359000
         L     @05,CVTPTR                                          0520 01360000
         L     @05,CVTCBSP(,@05)                                   0520 01361000
         L     @08,CBSVPTR(@08,@05)                                0520 01362000
         ALR   @08,@12                                             0520 01363000
         TM    VSRTCSVS-8(@08),B'01000000'                         0520 01364000
         BNO   @RF00520                                            0520 01365000
*             TABLOOP=OFF;          /* YES,STOP LOOPING.             */ 01366000
         NI    TABLOOP,B'01111111'                                 0521 01367000
*           ELSE                    /* LAST ENTRY NOT FOUND.         */ 01368000
*             DO;                   /* MORE ENTRIES EXIST.           */ 01369000
         B     @RC00520                                            0522 01370000
@RF00520 DS    0H                                                  0523 01371000
*               MWADCTR=MWADCTR+ONE;/* BUMP LOOP COUNTER.            */ 01372000
         AL    MWADCTR,@CF00043                                    0523 01373000
*               IF MWADCTR>MAXVCSLN THEN/* SHOULD END OF TABLE OCCUR?*/ 01374000
         C     MWADCTR,MAXVCSLN                                    0524 01375000
         BNH   @RF00524                                            0524 01376000
*                 DO;               /* AN ERROR HAS OCCURRED.        */ 01377000
*                   LINE='EXCESSIVE VSRT CSL DETECTED BY IDA0195A';/*   01378000
*                                      ERROR                         */ 01379000
         MVI   LINE+39,C' '                                        0526 01380000
         MVC   LINE+40(80),LINE+39                                 0526 01381000
         MVC   LINE(39),@CC03201                                   0526 01382000
*                   CALL PRINT;     /* DISPLAY ERROR MSG.            */ 01383000
         BAL   @14,PRINT                                           0527 01384000
*                   CALL SPACE;     /* SPACE UP ONE LINE.            */ 01385000
         BAL   @14,SPACE                                           0528 01386000
*                   TABLOOP=OFF;    /* FORCE END OF TABLE.           */ 01387000
         NI    TABLOOP,B'01111111'                                 0529 01388000
*                 END;              /* END OF ERROR DETECTION GROUP. */ 01389000
*             END;                  /* END OF PATH FOR NOT LAST    0531 01390000
*                                      ENTRY.                        */ 01391000
@RF00524 DS    0H                                                  0532 01392000
*         END;                      /* END OF VSRT OWNED CORE LOOP.  */ 01393000
@RC00520 DS    0H                                                  0532 01394000
@DE00514 TM    TABLOOP,B'10000000'                                 0532 01395000
         BO    @DL00514                                            0532 01396000
*       END;                        /* END OF PATH IF VSRT PRESENT.  */ 01397000
*   END DUMPAGSR;                   /* END OF DUMP ACTIVE GSR POOL 0534 01398000
*                                      PATH.                         */ 01399000
@EL00005 DS    0H                                                  0534 01400000
@EF00005 DS    0H                                                  0534 01401000
@ER00005 LM    @14,@12,@SA00005                                    0534 01402000
         BR    @14                                                 0534 01403000
*DUMPER:                                                           0535 01404000
*                                                                  0535 01405000
*/********************************************************************/ 01406000
*/* FUNCTION - THIS ROUTINE IS CALLED TO DISPLAY PIECES OF NAMED     */ 01407000
*/*            DATA.                                                 */ 01408000
*/* INPUT - PASSED ARE DUMPID, DUMPAD, DUMPLEN AND IDSIZE.           */ 01409000
*/* OUTPUT - THE DUMPID, ADDRESS AND LENGTH ARE DISPLAYED FOLLOWED   */ 01410000
*/*          BY THE DATA ITSELF.                                     */ 01411000
*/*          BOTH DATA ADDRESS AND LENGTH MUST BE NON-ZERO.          */ 01412000
*/********************************************************************/ 01413000
*                                                                  0535 01414000
*   PROC;                                                          0535 01415000
*                                                                  0535 01416000
DUMPER   STM   @14,@12,@SA00006                                    0535 01417000
*   /*****************************************************************/ 01418000
*   /*                                                               */ 01419000
*   /* DISPLAY THE ID OF THE DATA, THE DATA ADDRESS AND THE SIZE.    */ 01420000
*   /*                                                               */ 01421000
*   /*****************************************************************/ 01422000
*                                                                  0536 01423000
*   CALL SPACE;                     /* PECEED DATA WITH A BLANK LINE.*/ 01424000
         BAL   @14,SPACE                                           0536 01425000
*   LINE(1:IDSIZE)=DUMPID;          /* MOVE IN VARIABLE DUMP ID.     */ 01426000
         SLR   @08,@08                                             0537 01427000
         IC    @08,IDSIZE                                          0537 01428000
         BCTR  @08,0                                               0537 01429000
         EX    @08,@SM03319                                        0537 01430000
*   CONIN=ADDR(DUMPAD);             /* PASS THE ADDRESS OF THE DATA. */ 01431000
         LA    CONIN,DUMPAD                                        0538 01432000
*   CONOUT=0;                       /* ALLOW THE DATA TO STAY IN   0539 01433000
*                                      CONWORK.                      */ 01434000
         SLR   CONOUT,CONOUT                                       0539 01435000
*   CALL CONVERT;                   /* CONVERT DATA ADDRES TO PRINT     01436000
*                                      FORM                          */ 01437000
         BAL   @14,CONVERT                                         0540 01438000
*   LINE(IDSIZE+2:IDSIZE+7)=CONWORK(3:8);/* MOVE IN DATA ADDRESS.    */ 01439000
         SLR   @08,@08                                             0541 01440000
         IC    @08,IDSIZE                                          0541 01441000
         LA    @07,LINE+1(@08)                                     0541 01442000
         MVC   0(6,@07),CONWORK+2                                  0541 01443000
*   CONIN=ADDR(DUMPLEN);            /* PASS THE DATA LENGTH.         */ 01444000
         LA    CONIN,DUMPLEN                                       0542 01445000
*   CALL CONVERT;                   /* CONVERT DATA LENGTH TO PRINT     01446000
*                                      FORM.                         */ 01447000
         BAL   @14,CONVERT                                         0543 01448000
*   LINE(IDSIZE+9:IDSIZE+14)=CONWORK(3:8);/* MOVE IN DATA SIZE.      */ 01449000
         SLR   @12,@12                                             0544 01450000
         IC    @12,IDSIZE                                          0544 01451000
         LA    @08,LINE+8(@12)                                     0544 01452000
         MVC   0(6,@08),CONWORK+2                                  0544 01453000
*   CALL PRINT;                     /* PRINT DATA NAME, ADDRESS AND     01454000
*                                      LENGTH.                       */ 01455000
*                                                                  0545 01456000
         BAL   @14,PRINT                                           0545 01457000
*   /*****************************************************************/ 01458000
*   /*                                                               */ 01459000
*   /* NOW DISPLAY THE DATA ITSELF.                                  */ 01460000
*   /*                                                               */ 01461000
*   /*****************************************************************/ 01462000
*                                                                  0546 01463000
*   LLEMPTY=OFF;                    /* PROHIBIT LINE SUPRESSION OF 0546 01464000
*                                      REPEATED DATA.                */ 01465000
         NI    LLEMPTY,B'01111111'                                 0546 01466000
*   IF DEBCB=OFF THEN               /* IS A DEB BEING DUMPED?        */ 01467000
         TM    DEBCB,B'10000000'                                   0547 01468000
         BNZ   @RF00547                                            0547 01469000
*     DO;                           /* PROCESS THE DATA ITSELF IF NOT   01470000
*                                      A DEB.                        */ 01471000
*       IF DUMPLEN^=0&              /* IF THE DATA IS PRESENT THEN 0549 01472000
*                                      DISPLAY IT.                   */ 01473000
*           DUMPADA^=0 THEN         /* IF THE DATA IS PRESENT THEN 0549 01474000
*                                      DISPLAY IT.                   */ 01475000
         L     @12,DUMPLEN                                         0549 01476000
         LTR   @12,@12                                             0549 01477000
         BZ    @RF00549                                            0549 01478000
         L     @12,DUMPADA-1                                       0549 01479000
         LA    @12,0(,@12)                                         0549 01480000
         LTR   @12,@12                                             0549 01481000
         BZ    @RF00549                                            0549 01482000
*         DO;                       /* DISPLAY DATA WHOSE ADDRESS IS    01483000
*                                      NON-ZERO.                     */ 01484000
*           CALL SPACE;             /* IF DATA DISPLAYED, PRECEED  0551 01485000
*                                      WITH A BLANK LINE.            */ 01486000
         BAL   @14,SPACE                                           0551 01487000
*           SDUMPAD=DUMPAD&'FFFFFFE0'X;/* BACK-UP START ADRS TO 32 0552 01488000
*                                      BYTE BDY.                     */ 01489000
         L     @12,DUMPAD                                          0552 01490000
         LR    SDUMPAD,@12                                         0552 01491000
         N     SDUMPAD,@CF03209                                    0552 01492000
*           EDUMPAD=DUMPAD+DUMPLEN; /* NOTE LAST BYTE OF DATA.       */ 01493000
         AL    @12,DUMPLEN                                         0553 01494000
         LR    EDUMPAD,@12                                         0553 01495000
*           CDUMPAD=SDUMPAD;        /* SET STARTING LINE ADDRESS.    */ 01496000
         ST    SDUMPAD,CDUMPAD                                     0554 01497000
*           DUMPLEN=(EDUMPAD+31/32*32)-SDUMPAD;/* ROUND DUMP SZIE.   */ 01498000
         LR    @12,EDUMPAD                                         0555 01499000
         SLR   @12,SDUMPAD                                         0555 01500000
         ST    @12,DUMPLEN                                         0555 01501000
*           DO DUMPLEN=DUMPLEN TO 1 BY-32;/* DISPLAY 32 BYTES PER  0556 01502000
*                                      LINE.                         */ 01503000
         B     @DE00556                                            0556 01504000
@DL00556 DS    0H                                                  0557 01505000
*             IF LLEMPTY=OFF THEN   /* IS SKIPPING DUPLICATE DATA  0557 01506000
*                                      ALLOWED?                      */ 01507000
         TM    LLEMPTY,B'10000000'                                 0557 01508000
         BNZ   @RF00557                                            0557 01509000
*               DO;                 /* FORCE DISPLAY OF DATA EVEN IF    01510000
*                                      REPEATED.                     */ 01511000
*                 LASTLINE=CURRLINE;/* SAVE LINE FOR DUPLICATE TEST  */ 01512000
         L     @12,CDUMPAD                                         0559 01513000
         MVC   LASTLINE(32),CURRLINE(@12)                          0559 01514000
*                 IF CDUMPAD=DUMPAD THEN/* IF BLANKS ARE NOT NEEDED     01515000
*                                      THEN,                         */ 01516000
         C     @12,DUMPAD                                          0560 01517000
         BNE   @RF00560                                            0560 01518000
*                   LLEMPTY=ON;     /* NOW ALLOW DUPLICATE LINE    0561 01519000
*                                      SUPRESSION.                   */ 01520000
         OI    LLEMPTY,B'10000000'                                 0561 01521000
*                 CALL DUMPLINE;    /* DISPLAY 32 BYTE LINE OF DATA. */ 01522000
@RF00560 BAL   @14,DUMPLINE                                        0562 01523000
*               END;                /* END OF PATH IF SUPRESSION NOT    01524000
*                                      ALLOWED.                      */ 01525000
*             ELSE                  /* OTHERWISE SUPRESSION IS     0564 01526000
*                                      ALLOWED.                      */ 01527000
*               DO;                 /* PROCESSING FOR DUPLICATES   0564 01528000
*                                      ALLOWED.                      */ 01529000
         B     @RC00557                                            0564 01530000
@RF00557 DS    0H                                                  0565 01531000
*                 IF CURRLINE=LASTLINE THEN/* ARE PAST & CURRENT   0565 01532000
*                                      EQUAL ?                       */ 01533000
         L     @12,CDUMPAD                                         0565 01534000
         CLC   CURRLINE(32,@12),LASTLINE                           0565 01535000
         BNE   @RF00565                                            0565 01536000
*                   DO;             /* PROCESS EQUAL LINES OF DATA.  */ 01537000
*                     IF SKIPPING=OFF THEN/* SKIP MSG ALREADY      0567 01538000
*                                      PRINTED?                      */ 01539000
         TM    SKIPPING,B'10000000'                                0567 01540000
         BNZ   @RF00567                                            0567 01541000
*                       DO;         /* SKIP MSG NOT YET PRINTED.     */ 01542000
*                         SKIPPING=ON;/* NOTE MSG HAS APPEARED.      */ 01543000
         OI    SKIPPING,B'10000000'                                0569 01544000
*                         LINE=' '; /* CLEAR OUT THE LINE.           */ 01545000
         MVI   LINE+1,C' '                                         0570 01546000
         MVC   LINE+2(118),LINE+1                                  0570 01547000
         MVI   LINE,C' '                                           0570 01548000
*                         LINE(10:31)='ABOVE LINE IS REPEATED';    0571 01549000
         MVC   LINE+9(22),@CC03213                                 0571 01550000
*                         CALL PRINT;/* PRINT THE MESSAGE.           */ 01551000
         BAL   @14,PRINT                                           0572 01552000
*                       END;        /* END OF SKIP PATH.             */ 01553000
*                     DUMPAD=DUMPAD+32;/* INCREMENT FOR SKIPPED LINE.*/ 01554000
@RF00567 LA    @12,32                                              0574 01555000
         L     @00,DUMPAD                                          0574 01556000
         ALR   @00,@12                                             0574 01557000
         ST    @00,DUMPAD                                          0574 01558000
*                     CDUMPAD=CDUMPAD+32;/* INCREMENT FOR SKIPPED  0575 01559000
*                                      LINE.                         */ 01560000
         AL    @12,CDUMPAD                                         0575 01561000
         ST    @12,CDUMPAD                                         0575 01562000
*                   END;            /* END OF PATH FOR EQUAL LINES.  */ 01563000
*                 ELSE              /* SKIP ALLOWED BUT LINES NOT  0577 01564000
*                                      EQUAL.                        */ 01565000
*                   DO;             /* PROCESS FOR LINES NOT EQUAL.  */ 01566000
         B     @RC00565                                            0577 01567000
@RF00565 DS    0H                                                  0578 01568000
*                     SKIPPING=OFF; /* REMEMBER NO LINES SKIPPED.    */ 01569000
         NI    SKIPPING,B'01111111'                                0578 01570000
*                     LASTLINE=CURRLINE;/* SAVE LINE FOR LATER.      */ 01571000
         L     @12,CDUMPAD                                         0579 01572000
         MVC   LASTLINE(32),CURRLINE(@12)                          0579 01573000
*                     CALL DUMPLINE;/* DISPLAY NEW LINE.             */ 01574000
         BAL   @14,DUMPLINE                                        0580 01575000
*                   END;            /* END OF PATH FOR NEW LINE.     */ 01576000
*               END;                /* END OF PATH FOR SUPRESSION  0582 01577000
*                                      ALLOWED.                      */ 01578000
@RC00565 DS    0H                                                  0583 01579000
*           END;                    /* END OF MULTI-LINE DISPLAY   0583 01580000
*                                      LOOP.                         */ 01581000
@RC00557 L     @12,@CF03210                                        0583 01582000
         AL    @12,DUMPLEN                                         0583 01583000
         ST    @12,DUMPLEN                                         0583 01584000
@DE00556 LTR   @12,@12                                             0583 01585000
         BP    @DL00556                                            0583 01586000
*         END;                      /* END OF PATH FOR NON-ZERO DATA    01587000
*                                      ADDRESS.                      */ 01588000
*     END;                          /* END OF PATH FOR NON-DEB DATA. */ 01589000
@RF00549 DS    0H                                                  0586 01590000
*   DUMPID=' ';                     /* INSURE BLANK LINE.            */ 01591000
@RF00547 MVI   DUMPID+1,C' '                                       0586 01592000
         MVC   DUMPID+2(29),DUMPID+1                               0586 01593000
         MVI   DUMPID,C' '                                         0586 01594000
*   CALL SPACE;                     /* INSURE BLANK LINE AFTER DATA. */ 01595000
         BAL   @14,SPACE                                           0587 01596000
*   DEBCB=OFF;                      /* RESET DEB SWITCH.             */ 01597000
         NI    DEBCB,B'01111111'                                   0588 01598000
*   END DUMPER;                     /* END OF PROC DUMER.            */ 01599000
@EL00006 DS    0H                                                  0589 01600000
@EF00006 DS    0H                                                  0589 01601000
@ER00006 LM    @14,@12,@SA00006                                    0589 01602000
         BR    @14                                                 0589 01603000
*DUMPLINE:                                                         0590 01604000
*                                                                  0590 01605000
*/********************************************************************/ 01606000
*/* FUNCTION - THIS ROUTINE IS CALLED BY DUMPER TO DISPLAY HEX 20    */ 01607000
*/*            BYTES OF STORAGE.                                     */ 01608000
*/* INPUT - PASSED IS DUMPAD.                                        */ 01609000
*/* OUTPUT - FROM LEFT TO RIGHT OUTPUT IS: 1) DATA ADDRESS, 2) THE   */ 01610000
*/*          DATA ITSELF GROUPED INTO WORDS AND 3) A TRANSLATION     */ 01611000
*/*          TO EBCDIC FROM BINARY(HEX).                             */ 01612000
*/********************************************************************/ 01613000
*                                                                  0590 01614000
*   PROC;                                                          0590 01615000
DUMPLINE STM   @14,@12,@SA00007                                    0590 01616000
*   LINE=' ';                       /* CLEAR PRINT LINE.             */ 01617000
         MVI   LINE+1,C' '                                         0591 01618000
         MVC   LINE+2(118),LINE+1                                  0591 01619000
         MVI   LINE,C' '                                           0591 01620000
*   ASTRX1='*';                     /* INITAILZE LEFT *.             */ 01621000
         MVI   ASTRX1,C'*'                                         0592 01622000
*   ASTRX2='*';                     /* INITIALIZE RIGHT *.           */ 01623000
         MVI   ASTRX2,C'*'                                         0593 01624000
*   EBCDIC=CURRLINE;                /* SAVE THE LINE FOR TRANSLATION.*/ 01625000
         L     @07,CDUMPAD                                         0594 01626000
         MVC   EBCDIC(32),CURRLINE(@07)                            0594 01627000
*   TR(EBCDIC,TRTAB);               /* TRANSLATE NOW TO EBCDIC FOR 0595 01628000
*                                      OUTPUT.                       */ 01629000
         TR    EBCDIC(32),TRTAB                                    0595 01630000
*   CONIN=ADDR(CDUMPAD);            /* CONVERT THE STARTING ADDRESS     01631000
*                                      OF THE LINE.                  */ 01632000
         LA    CONIN,CDUMPAD                                       0596 01633000
*   CONOUT=0;                       /* LEAVE THE DATA IN CONWORK.    */ 01634000
         SLR   CONOUT,CONOUT                                       0597 01635000
*   CALL CONVERT;                   /* CHANGE ADDRESS TO PRINT FORM  */ 01636000
         BAL   @14,CONVERT                                         0598 01637000
*   LINE(1:6)=CONWORK(3:8);         /* MOVE DATA TO PRINT LINE.      */ 01638000
         MVC   LINE(6),CONWORK+2                                   0599 01639000
*   I=10;                           /* SET STARTING COLUMN OF DATA.  */ 01640000
         LA    I,10                                                0600 01641000
*   DO J=1 TO 8;                    /* DO 2 GROUPS OF FOUR WORDS   0601 01642000
*                                      EACH.                         */ 01643000
         LA    J,1                                                 0601 01644000
@DL00601 DS    0H                                                  0602 01645000
*     IF CDUMPAD=DUMPAD THEN        /* ANY STARTING BLANKS NEEDED?   */ 01646000
         L     @12,DUMPAD                                          0602 01647000
         C     @12,CDUMPAD                                         0602 01648000
         BNE   @RF00602                                            0602 01649000
*       DO;                         /* NO BLANKS NEEDED, CONTINUE.   */ 01650000
*         CONIN=DUMPAD;             /* PASS DATA.                    */ 01651000
         L     @07,DUMPAD                                          0604 01652000
         LR    CONIN,@07                                           0604 01653000
*         CONOUT=ADDR(LINE(I));     /* PLACE PRINT FORM IN PRINT   0605 01654000
*                                      LINE.                         */ 01655000
         LA    CONOUT,LINE-1(I)                                    0605 01656000
*         CALL CONVERT;             /* CONVERT DATA TO PRINT FORM.   */ 01657000
         BAL   @14,CONVERT                                         0606 01658000
*         DUMPAD=DUMPAD+4;          /* INCREMENT FOR THE WORD DUMPED.*/ 01659000
         LA    @12,4                                               0607 01660000
         AL    @12,DUMPAD                                          0607 01661000
         ST    @12,DUMPAD                                          0607 01662000
*       END;                        /* TO HERE IF BLANKS NEEDED.     */ 01663000
*                                                                  0608 01664000
*     /***************************************************************/ 01665000
*     /*                                                             */ 01666000
*     /* PROVIDE BLANKS FOR BOTH LEFT AND RIGHT SIDES OF PAGE.       */ 01667000
*     /*                                                             */ 01668000
*     /***************************************************************/ 01669000
*                                                                  0609 01670000
*     ELSE                          /* BLANKS ARE NEEDED FOR AT LEAST   01671000
*                                      ONE WORD.                     */ 01672000
*       EBCDIC((((J-1)*4)+1):(((J-1)*4)+4))='    ';/* FOUR BLANKS.   */ 01673000
         B     @RC00602                                            0609 01674000
@RF00602 LR    @12,J                                               0609 01675000
         BCTR  @12,0                                               0609 01676000
         SLA   @12,2                                               0609 01677000
         LA    @15,EBCDIC(@12)                                     0609 01678000
         MVC   0(4,@15),@CC03220                                   0609 01679000
*     CDUMPAD=CDUMPAD+4;            /* INCREMENT FOR NEXT WORD.      */ 01680000
@RC00602 LA    @12,4                                               0610 01681000
         AL    @12,CDUMPAD                                         0610 01682000
         ST    @12,CDUMPAD                                         0610 01683000
*     IF DUMPAD>=EDUMPAD THEN       /* LAST BYTE OF DATA DUMPED?     */ 01684000
         C     EDUMPAD,DUMPAD                                      0611 01685000
         BH    @RF00611                                            0611 01686000
*       DUMPAD=ZERO;                /* FORCE BLANKING TO END OF LINE.*/ 01687000
         SLR   @12,@12                                             0612 01688000
         ST    @12,DUMPAD                                          0612 01689000
*     I=I+9;                        /* A WORD IS EIGHT CHARS PLUS A     01690000
*                                      BLANK.                        */ 01691000
@RF00611 AL    I,@CF00033                                          0613 01692000
*     IF J=4 THEN                   /* IF AT THE END OF A GROUP OF 0614 01693000
*                                      FOUR,                         */ 01694000
         C     J,@CF00118                                          0614 01695000
         BNE   @RF00614                                            0614 01696000
*       I=I+3;                      /* BETWEEN GROUPS OF FOUR GIVE 0615 01697000
*                                      3+1 BLANKS.                   */ 01698000
         AL    I,@CF00088                                          0615 01699000
*   END;                            /* END OF PATH FOR A DATA WORD.  */ 01700000
@RF00614 AL    J,@CF00043                                          0616 01701000
         C     J,@CF00064                                          0616 01702000
         BNH   @DL00601                                            0616 01703000
*   CALL PRINT;                     /* MOVE THE LINE TO DATASET.     */ 01704000
         BAL   @14,PRINT                                           0617 01705000
*   END DUMPLINE;                   /* END OF PROC DUMPLINE.         */ 01706000
@EL00007 DS    0H                                                  0618 01707000
@EF00007 DS    0H                                                  0618 01708000
@ER00007 LM    @14,@12,@SA00007                                    0618 01709000
         BR    @14                                                 0618 01710000
*CONVERT:                                                          0619 01711000
*                                                                  0619 01712000
*/********************************************************************/ 01713000
*/* FUNCTION - THIS ROUTINE IS CALLED TO CONVERT DATA FOR PRINTING.  */ 01714000
*/* INPUT - PASSED ARE CONIN AND CONOUT.                             */ 01715000
*/* OUTPUT - DATA IN CONIN IS CONVERTED FROM BINARY TO PRINTER       */ 01716000
*/*          FORMAT HEX. IF CONOUT IS NOT ZERO THE DATA IS PLACED    */ 01717000
*/*          VIA CONOUT. IF CONOUT IS ZERO THEN THE DATA REMAINS     */ 01718000
*/*          IN CONWORK.                                             */ 01719000
*/********************************************************************/ 01720000
*                                                                  0619 01721000
*   PROC;                                                          0619 01722000
CONVERT  STM   @14,@12,12(@13)                                     0619 01723000
*   CONWORK(1:4)=CONIN->CONMASK(1:4);/* MOVE TO WKAREA       @OZ07692*/ 01724000
         MVC   CONWORK(4),CONMASK(CONIN)                           0620 01725000
*   UNPK(CONWORK,CONWORK(1:5));     /* EXPAND INPUT WORD     @OZ07692*/ 01726000
         UNPK  CONWORK(9),CONWORK(5)                               0621 01727000
*   CONWORK=CONWORK&'0F0F0F0F0F0F0F0F0F'X;/* MASK OFF ZONES          */ 01728000
         NC    CONWORK(9),@CB03225                                 0622 01729000
*   TR(CONWORK,CONTAB);             /* CONVERT TO EBCDIC             */ 01730000
         TR    CONWORK(9),CONTAB                                   0623 01731000
*   IF CONOUT^=0 THEN               /* IF NEED FOR OUTPUT            */ 01732000
         LTR   CONOUT,CONOUT                                       0624 01733000
         BZ    @RF00624                                            0624 01734000
*     CONOUT->CONMASK=CONWORK;      /* MOVE OUT OUTPUT               */ 01735000
         MVC   CONMASK(8,CONOUT),CONWORK                           0625 01736000
*   END CONVERT;                                                   0626 01737000
@EL00008 DS    0H                                                  0626 01738000
@EF00008 DS    0H                                                  0626 01739000
@ER00008 LM    @14,@12,12(@13)                                     0626 01740000
         BR    @14                                                 0626 01741000
*SPACE:                                                            0627 01742000
*                                                                  0627 01743000
*/********************************************************************/ 01744000
*/* FUNCTION - THIS ROUTINE IS CALLED TO SPACE UP ONE LINE.          */ 01745000
*/* INPUT - NO INPUT IS REQUIRED.                                    */ 01746000
*/* OUTPUT - SPACING IS ACCOMPLISHED BY OUTPUTTING A BLANK LINE.     */ 01747000
*/********************************************************************/ 01748000
*                                                                  0627 01749000
*   PROC;                                                          0627 01750000
SPACE    STM   @14,@12,@SA00009                                    0627 01751000
*   LINE=' ';                       /* BLANK OUT LINE                */ 01752000
         MVI   LINE+1,C' '                                         0628 01753000
         MVC   LINE+2(118),LINE+1                                  0628 01754000
         MVI   LINE,C' '                                           0628 01755000
*   CALL PRINT;                     /* PRINT LINE                    */ 01756000
         BAL   @14,PRINT                                           0629 01757000
*   END SPACE;                      /* END OF SPACE PROCEDURE.       */ 01758000
@EL00009 DS    0H                                                  0630 01759000
@EF00009 DS    0H                                                  0630 01760000
@ER00009 LM    @14,@12,@SA00009                                    0630 01761000
         BR    @14                                                 0630 01762000
*PRINT:                                                            0631 01763000
*                                                                  0631 01764000
*/********************************************************************/ 01765000
*/* FUNCTION - OUTPUT A SINGLE LINE TO THE SNAP DATA SET.            */ 01766000
*/* INPUT - PASSED TO THIS ROUTINE IS PLINE.                         */ 01767000
*/* OUTPUT - THE SNAP PRINT ROUTINE IS CALLED TO DISPLAY A LINE.     */ 01768000
*/*          DATA IS MOVED FROM LINE (TCB KEY) TO SNAPLINE (KEY 0).  */ 01769000
*/********************************************************************/ 01770000
*                                                                  0631 01771000
*   PROC;                                                          0631 01772000
*                                                                  0631 01773000
PRINT    STM   @14,@12,@SA00010                                    0631 01774000
*   /*****************************************************************/ 01775000
*   /*                                                               */ 01776000
*   /* SWITCH TO ZERO FOR DATA MOVE.                                 */ 01777000
*   /*                                                               */ 01778000
*   /*****************************************************************/ 01779000
*                                                                  0632 01780000
*   SNAPLINE=LINE;                  /* MOVE FROM LOCAL TO SNAP     0632 01781000
*                                      BUFFER.                       */ 01782000
         L     @12,ADPLBUF(,R1)                                    0632 01783000
         MVC   SNAPLINE(120,@12),LINE                              0632 01784000
*   CALL PRINTRTN;                  /* GO TO SNAP PRINT ROUTINE.     */ 01785000
*                                                                  0633 01786000
         L     @15,ADPLPRNT(,R1)                                   0633 01787000
         BALR  @14,@15                                             0633 01788000
*   /*****************************************************************/ 01789000
*   /*                                                               */ 01790000
*   /* BACK TO TCBPKF.                                               */ 01791000
*   /*                                                               */ 01792000
*   /*****************************************************************/ 01793000
*                                                                  0634 01794000
*   END PRINT;                      /* END OF PRINT PROCEDURE.       */ 01795000
@EL00010 DS    0H                                                  0634 01796000
@EF00010 DS    0H                                                  0634 01797000
@ER00010 LM    @14,@12,@SA00010                                    0634 01798000
         BR    @14                                                 0634 01799000
*DUMPDEB:                                                          0635 01800000
*                                                                  0635 01801000
*/********************************************************************/ 01802000
*/*  FUNCTION - DISPLAY FOR A GIVEN DEB THE DATA SET CONRTOL BLOCKS. */ 01803000
*/*  INPUT - RWORK1 CONTAINS THE DEB ADDRESS.                        */ 01804000
*/*  OUTPUT - DATA SET CONTROL BLOCKS ARE DISPLAYED.                 */ 01805000
*/********************************************************************/ 01806000
*                                                                  0635 01807000
*   PROC;                                                          0635 01808000
*                                                                  0635 01809000
DUMPDEB  STM   @14,@12,@SA00011                                    0635 01810000
*   /*****************************************************************/ 01811000
*   /*                                                               */ 01812000
*   /* SCAN THE BIB TABLE TO SEE IF BIB ALREADY DUMPED.              */ 01813000
*   /*                                                               */ 01814000
*   /*****************************************************************/ 01815000
*                                                                  0636 01816000
*   RFY                                                            0636 01817000
*     IDAAMBL BASED(AMBPAMBL);      /* LOCATE THE AMBL FROM THE AMB. */ 01818000
*   RFY                                                            0637 01819000
*     IDAAMB BASED(DEBDCBAD);       /* LOCATE THE AMB FORNM THE DEB. */ 01820000
*   RFY                                                            0638 01821000
*     IDABIB BASED(RWORK2);         /* USE LOCAL PTR FOR THE BIB.    */ 01822000
*   RWORK2=AMBLBIB;                 /* OBTAIN BIB ADDRESSABILITY.    */ 01823000
         L     @12,DEBDCBAD(,RWORK1)                               0639 01824000
         L     @12,AMBPAMBL(,@12)                                  0639 01825000
         L     RWORK2,AMBLBIB(,@12)                                0639 01826000
*   TABLOOP=ON;                     /* ALLOW TABLE LOOPING.          */ 01827000
         OI    TABLOOP,B'10000000'                                 0640 01828000
*   TABFOUND=OFF;                   /* INDICATE LOOKING FOR ENTRY.   */ 01829000
         NI    TABFOUND,B'01111111'                                0641 01830000
*   DO J=1 TO DIM(BIBTABL)          /* LOOP THRU ALL OF TABLE,       */ 01831000
*         WHILE(TABLOOP=ON&BIBTABL(J)^=ZERO);/* UNLESS FOUND OR END. */ 01832000
         LA    J,1                                                 0642 01833000
@DL00642 TM    TABLOOP,B'10000000'                                 0642 01834000
         BNO   @DC00642                                            0642 01835000
         LR    @12,J                                               0642 01836000
         SLA   @12,2                                               0642 01837000
         L     @12,BIBTABL-4(@12)                                  0642 01838000
         LTR   @12,@12                                             0642 01839000
         BZ    @DC00642                                            0642 01840000
*     IF BIBTABL(J)=ADDR(IDABIB) THEN/* IS BIB IN THIS TABLE?        */ 01841000
         CR    @12,RWORK2                                          0643 01842000
         BNE   @RF00643                                            0643 01843000
*       DO;                         /* IF SO MAKE NOTES.             */ 01844000
*         TABFOUND=ON;              /* NOTE BIB ALREADY IN TABLE.    */ 01845000
         OI    TABFOUND,B'10000000'                                0645 01846000
*         TABLOOP=OFF;              /* STOP THE LOOPING.             */ 01847000
         NI    TABLOOP,B'01111111'                                 0646 01848000
*       END;                        /* END OF PATH FOE ENTRY FOUND.  */ 01849000
*   END;                            /* END OF LOOP THROUGH THE TABLE.*/ 01850000
@RF00643 AL    J,@CF00043                                          0648 01851000
         C     J,@CF01219                                          0648 01852000
         BNH   @DL00642                                            0648 01853000
@DC00642 DS    0H                                                  0649 01854000
*   IF TABFOUND=OFF&J<DIM(BIBTABL) THEN/* IF A NEW BIB,              */ 01855000
         TM    TABFOUND,B'10000000'                                0649 01856000
         BNZ   @RF00649                                            0649 01857000
         C     J,@CF01219                                          0649 01858000
         BNL   @RF00649                                            0649 01859000
*     DO;                           /* EXECUTE SPECIAL PATH FOR A NEW   01860000
*                                      BIB.                          */ 01861000
*       BIBTABL(J)=ADDR(IDABIB);    /* PLACE BIB IN THE TABLE.       */ 01862000
*                                                                  0651 01863000
         LR    @12,J                                               0651 01864000
         SLA   @12,2                                               0651 01865000
         ST    RWORK2,BIBTABL-4(@12)                               0651 01866000
*       /*************************************************************/ 01867000
*       /*                                                           */ 01868000
*       /* DISPLAY THE BIB AND ASSOCIATED BLOCKS.                    */ 01869000
*       /*                                                           */ 01870000
*       /*************************************************************/ 01871000
*                                                                  0652 01872000
*       DUMPID=IDBIB;               /* INDICATE BIB.                 */ 01873000
         MVI   DUMPID+3,C' '                                       0652 01874000
         MVC   DUMPID+4(27),DUMPID+3                               0652 01875000
         MVC   DUMPID(3),IDBIB                                     0652 01876000
*       IDSIZE=LENGTH(IDBIB);       /* INDICATE SIZE OF DUMP ID.     */ 01877000
         MVI   IDSIZE,X'03'                                        0653 01878000
*       DUMPAD=ADDR(IDABIB);        /* PASS THE DATA ADDRESS.        */ 01879000
         ST    RWORK2,DUMPAD                                       0654 01880000
*       DUMPLEN=LENGTH(IDABIB);     /* USE THE SIZE OF THE MACRO.    */ 01881000
         MVC   DUMPLEN(4),@CF02405                                 0655 01882000
*       CALL DUMPER;                /* PRINT OFF THE BIB.            */ 01883000
*                                                                  0656 01884000
         BAL   @14,DUMPER                                          0656 01885000
*       /*************************************************************/ 01886000
*       /*                                                           */ 01887000
*       /* DISPLAY THE VMT CHAIN.                                    */ 01888000
*       /*                                                           */ 01889000
*       /*************************************************************/ 01890000
*                                                                  0657 01891000
*       RFY                                                        0657 01892000
*         IDAVMT BASED(RWORK3);     /* USE LOCAL BASE POINTER.       */ 01893000
*       RWORK3=BIBVMT;              /* OBTAIN FIRST VMT ADDRESS.     */ 01894000
         L     RWORK3,BIBVMT(,RWORK2)                              0658 01895000
*       CBCTR1=ONE;                 /* INITIALIZE LOOP COUNTER.      */ 01896000
         LA    CBCTR1,1                                            0659 01897000
*       DO WHILE RWORK3^=ZERO;      /* LOOP UNTIL ZERO POINTER.      */ 01898000
         B     @DE00660                                            0660 01899000
@DL00660 DS    0H                                                  0661 01900000
*         DUMPID=IDVMT;             /* NAME THE DATA.                */ 01901000
         MVI   DUMPID+3,C' '                                       0661 01902000
         MVC   DUMPID+4(27),DUMPID+3                               0661 01903000
         MVC   DUMPID(3),IDVMT                                     0661 01904000
*         IDSIZE=LENGTH(IDVMT);     /* PASS THE SIZE FO THE DUMP ID. */ 01905000
         MVI   IDSIZE,X'03'                                        0662 01906000
*         DUMPAD=ADDR(IDAVMT);      /* PASS THE VMT ADDRESS.         */ 01907000
         ST    RWORK3,DUMPAD                                       0663 01908000
*         DUMPLEN=VMTLEN;           /* VMT IS VARIABLE PASS THE    0664 01909000
*                                      LENGTH.                       */ 01910000
         LH    @12,VMTLEN(,RWORK3)                                 0664 01911000
         ST    @12,DUMPLEN                                         0664 01912000
*         CALL DUMPER;              /* DISPLAY THE INDICATED DATA.   */ 01913000
         BAL   @14,DUMPER                                          0665 01914000
*         CBCTR1=CBCTR1+ONE;        /* COUNT ELEMENTS.               */ 01915000
*                                                                  0666 01916000
         AL    CBCTR1,@CF00043                                     0666 01917000
*         /***********************************************************/ 01918000
*         /*                                                         */ 01919000
*         /* IF VMT CHAIN EXCEEDS VSAMCBMX FORCE END OF CHAIN.       */ 01920000
*         /*                                                         */ 01921000
*         /***********************************************************/ 01922000
*                                                                  0667 01923000
*         IF RWORK3=VMTNXT|         /* DOES BLOCK POINT TO ITSELF?   */ 01924000
*             CBCTR1>VSAMCBMX THEN  /* SHOULD CHAIN HAVE ENDED?      */ 01925000
         C     RWORK3,VMTNXT(,RWORK3)                              0667 01926000
         BE    @RT00667                                            0667 01927000
         C     CBCTR1,VSAMCBMX                                     0667 01928000
         BNH   @RF00667                                            0667 01929000
@RT00667 DS    0H                                                  0668 01930000
*           DO;                     /* PORBABLE ENDLESS LOOP.        */ 01931000
*             LINE='EXCESSIVE GSR VMT CHAIN DETECTED BY IDA0195A';/*    01932000
*                                      ERROR                         */ 01933000
         MVI   LINE+44,C' '                                        0669 01934000
         MVC   LINE+45(75),LINE+44                                 0669 01935000
         MVC   LINE(44),@CC03240                                   0669 01936000
*             CALL PRINT;           /* DISPLAY ERROR MSG.            */ 01937000
         BAL   @14,PRINT                                           0670 01938000
*             CALL SPACE;           /* SPACE UP A LINE.              */ 01939000
         BAL   @14,SPACE                                           0671 01940000
*             RWORK3=ZERO;          /* FORCE END OF CHAIN.           */ 01941000
         SLR   RWORK3,RWORK3                                       0672 01942000
*           END;                    /* END OF ERROR GROUP.           */ 01943000
*         ELSE                      /* OTHERWISE BLOCK IS FINE.      */ 01944000
*           RWORK3=VMTNXT;          /* OBTAIN NEXT VMT IN CHAIN.     */ 01945000
         B     @RC00667                                            0674 01946000
@RF00667 L     RWORK3,VMTNXT(,RWORK3)                              0674 01947000
*       END;                        /* END OF PATH THROUGH THE VMT.S */ 01948000
@RC00667 DS    0H                                                  0675 01949000
@DE00660 LTR   RWORK3,RWORK3                                       0675 01950000
         BNZ   @DL00660                                            0675 01951000
*REVMT:                             /* LABEL FOR RETRY OT COVER VMT     01952000
*                                      CHAIN.                        */ 01953000
*                                                                  0676 01954000
*       /*************************************************************/ 01955000
*       /*                                                           */ 01956000
*       /* DISPLAY THE PSB CHAIN.                                    */ 01957000
*       /*                                                           */ 01958000
*       /*************************************************************/ 01959000
*                                                                  0676 01960000
*       RWORK3=BIBPRSPH;            /* OBTAIN THE PSB ADDRESS.       */ 01961000
REVMT    L     RWORK3,BIBPRSPH(,RWORK2)                            0676 01962000
*       CBCTR1=ONE;                 /* RESTART COUNTING.             */ 01963000
         LA    CBCTR1,1                                            0677 01964000
*       RFY                                                        0678 01965000
*         HEBHDREL BASED(RWORK3);   /* USE LOCAL BASE POINTER.       */ 01966000
*       DO WHILE RWORK3^=ZERO;      /* DISPLAY ONLY WHILE PRESENT.   */ 01967000
*                                                                  0679 01968000
         B     @DE00679                                            0679 01969000
@DL00679 DS    0H                                                  0680 01970000
*         /***********************************************************/ 01971000
*         /*                                                         */ 01972000
*         /* DISPLAY THE PSB ITSELF.                                 */ 01973000
*         /*                                                         */ 01974000
*         /***********************************************************/ 01975000
*                                                                  0680 01976000
*         DUMPID=IDPSB;             /* NAME THE DATA.                */ 01977000
         MVI   DUMPID+3,C' '                                       0680 01978000
         MVC   DUMPID+4(27),DUMPID+3                               0680 01979000
         MVC   DUMPID(3),IDPSB                                     0680 01980000
*         IDSIZE=LENGTH(IDPSB);     /* INDICATE THE DUMP ID SIZE.    */ 01981000
         MVI   IDSIZE,X'03'                                        0681 01982000
*         DUMPAD=RWORK3;            /* PASS THE DATA ADDRESS.        */ 01983000
         ST    RWORK3,DUMPAD                                       0682 01984000
*         DUMPLEN=HEBLN;            /* USE THE DUMMY HEB ENTRY IN THE   01985000
*                                      PSB.                          */ 01986000
         L     @12,HEBLN-1(,RWORK3)                                0683 01987000
         LA    @12,0(,@12)                                         0683 01988000
         ST    @12,DUMPLEN                                         0683 01989000
*         CALL DUMPER;              /* DISPLAY THE PSB ITSELF.       */ 01990000
*                                                                  0684 01991000
         BAL   @14,DUMPER                                          0684 01992000
*         /***********************************************************/ 01993000
*         /*                                                         */ 01994000
