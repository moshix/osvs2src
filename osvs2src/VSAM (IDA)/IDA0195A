*                                                            @Z40WSRL*/ 02779000
         B     @RC00859                                            0861 02780000
@RF00859 MVI   LINE+49,C' '                                        0861 02781000
         MVC   LINE+50(70),LINE+49                                 0861 02782000
         MVC   LINE(49),@CC03302                                   0861 02783000
*                 CALL PRINT;       /* DISPLAY ERROR MESSAGE @Z40WSRL*/ 02784000
@RC00859 BAL   @14,PRINT                                           0862 02785000
*                 CALL SPACE;       /* SPACE UP ONE LINE     @Z40WSRL*/ 02786000
         BAL   @14,SPACE                                           0863 02787000
*                 RWORK3=ZERO;      /* FORCE END OF CHAIN    @Z40WSRL*/ 02788000
         SLR   RWORK3,RWORK3                                       0864 02789000
*               END;                /* END OF ERROR DETECTION GROUP     02790000
*                                                            @Z40WSRL*/ 02791000
*             ELSE                  /* OTERWISE THE BLOCK IS FINE  0866 02792000
*                                                            @Z40WSRL*/ 02793000
*               RWORK3=HEBELCHN;    /* OBTAIN NEXT PSAB IF PRESENT 0866 02794000
*                                                            @Z40WSRL*/ 02795000
         B     @RC00857                                            0866 02796000
@RF00857 L     RWORK3,HEBELCHN(,RWORK3)                            0866 02797000
*           END;                    /* END OF PATH THRU PSAB CHAIN 0867 02798000
*                                                            @Z40WSRL*/ 02799000
@RC00857 DS    0H                                                  0867 02800000
@DE00850 LTR   RWORK3,RWORK3                                       0867 02801000
         BNZ   @DL00850                                            0867 02802000
*         END;                      /*                       @Z40WSRL*/ 02803000
*     END;                          /*                       @Z40WSRL*/ 02804000
*   END DUMPSDS;                    /*                       @Z40WSRL*/ 02805000
@EL00012 DS    0H                                                  0870 02806000
@EF00012 DS    0H                                                  0870 02807000
@ER00012 LM    @14,@05,@SA00012                                    0870 02808000
         LM    @07,@12,@SA00012+32                                 0870 02809000
         BR    @14                                                 0870 02810000
*RCVRRTN:                                                          0871 02811000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0871 02812000
*                                                                  0871 02813000
RCVRRTN  DS    0H                                                  0872 02814000
*/********************************************************************/ 02815000
*/*                                                                  */ 02816000
*/* MODULE NAME:         IDA0195A,RCVRRTN                            */ 02817000
*/*                                                                  */ 02818000
*/* DESCRIPTIVE NAME:    VSAM SNAP FORMAT ESTAE ROUTINE.             */ 02819000
*/*                                                                  */ 02820000
*/* COPYRIGHT:           NONE                                        */ 02821000
*/*                                                                  */ 02822000
*/* STATUS:              NEW  FOR VSAM RELEASE 2 (VS2-3 ICR).        */ 02823000
*/*                                                                  */ 02824000
*/* FUNCTION:                                                        */ 02825000
*/*                                                                  */ 02826000
*/*         PASS CONTROL TO AN APPROPRIATE ROUTINE FOR               */ 02827000
*/*         FOR RETRY OF THE VSAM SNAP ROUTINE.                      */ 02828000
*/*                                                                  */ 02829000
*/* NOTES:                NONE                                       */ 02830000
*/*                                                                  */ 02831000
*/* DEPENDENCIES:         RTM FOR SERVICES                           */ 02832000
*/*                                                                  */ 02833000
*/* RESTRICTIONS:         NONE                                       */ 02834000
*/*                                                                  */ 02835000
*/* REGISTER CONVENTIONS:                                            */ 02836000
*/*   R0 - ENTRY CONDITION CODE.                                     */ 02837000
*/*        0-ACTIVE I/O QUIESCED, RESTORABLE.                        */ 02838000
*/*        4-ACTIVE I/O HALTED, NOT RESTORABLE.                      */ 02839000
*/*        8-NO I/O WAS ACTIVE.                                      */ 02840000
*/*        12-NO SDWA (RTCA) IS PRESENT.                             */ 02841000
*/*        16-NO I/O PROCESSING WAS PERFORMED.                       */ 02842000
*/*   R1 - IF NO SDWA, THE ABEND CODE.                               */ 02843000
*/*        IF SDWA IS PRESENT, ADDRESS OF THE SDWA.                  */ 02844000
*/*   R2 - IF NO SDWA, THE ESTAE PARM LIST.                          */ 02845000
*/*        IF SDWA PRESENT, UNPREDICTABLE.                           */ 02846000
*/*   R3-R12 - UNPREDICTABLE.                                        */ 02847000
*/*   R13 - ID SDWA IS PRESENT, SAVE AREA ADDRESS.                   */ 02848000
*/*         IF NO SDWA, UNPREDICTABLE.                               */ 02849000
*/*   R14 - RETURN ADDRESS.                                          */ 02850000
*/*   R15 - BASE ADDRESS FOR THIS MODULE.                            */ 02851000
*/*                                                                  */ 02852000
*/* PATCH LABEL:        PATCH5A 50 BYTES RESERVED FOR                */ 02853000
*/*                      FUTURE USE                                  */ 02854000
*/*                                                                  */ 02855000
*/* MODULE TYPE:         PROCEDURE                                   */ 02856000
*/*                                                                  */ 02857000
*/* PROCESSOR:           PLS2                                        */ 02858000
*/* MODULE SIZE:         XXXX BYTES                                  */ 02859000
*/* ATTRIBUTES:          REENTERABLE,SUPERVISOR STATE, KEY 0,TCBPKF  */ 02860000
*/*                                                                  */ 02861000
*/* ENTRY POINT:         RCVRRTN, MAIN ENTRY POINT                   */ 02862000
*/*                                                                  */ 02863000
*/* PURPOSE:                                                         */ 02864000
*/*         THIS MODULE RECEIVES CONTROL FROM RTM IF THE VSAM SNAP   */ 02865000
*/*          FORMAT ROUTINE WAS IN PROGRESS AT THE TIME OF AN ERROR. */ 02866000
*/*                                                                  */ 02867000
*/* LINKAGE:                                                         */ 02868000
*/*         BRANCH ENTERED BY RTM.                                   */ 02869000
*/*                                                                  */ 02870000
*/* INPUT:                                                           */ 02871000
*/*         SEE REGISTER CONVENTIONS ABOVE.                          */ 02872000
*/* OUTPUT:                                                          */ 02873000
*/*   R15 - RETURN CODE.                                             */ 02874000
*/*         0-CONTINUE TERMINATION, PROCESS FURTHER ESTAE ROUTINES.  */ 02875000
*/*         4-RETRY IS TO BE SCHEDULED, R0 HAS ADDRESS OF MODULE.    */ 02876000
*/*         16-CONTINUE TERMINATION, DO NOT PROCESS ESTAE ROUTINES.  */ 02877000
*/*                                                                  */ 02878000
*/* EXIT NORMAL:                                                     */ 02879000
*/*         RETURN TO CALLER WITH RETURN CODE IN R15.                */ 02880000
*/*         THIS MODULE MAY SET R15 TO FOUR.                         */ 02881000
*/*                                                                  */ 02882000
*/* EXIT ERROR:                                                      */ 02883000
*/*         RETURN TO CALLER WITH RETURN CODE IN R15.                */ 02884000
*/*         THIS MODULE MAY SET R15 TO ZERO.                         */ 02885000
*/*                                                                  */ 02886000
*/* EXTERNAL REFERENCES:                                             */ 02887000
*/*                                                                  */ 02888000
*/*   ROUTINES:          NONE                                        */ 02889000
*/*                                                                  */ 02890000
*/*   DATA AREAS:        RTCA                                        */ 02891000
*/*                                                                  */ 02892000
*/*   CONTROL BLOCKS:    SYSTEM: ASCB CVT RTCA                       */ 02893000
*/*                                                                  */ 02894000
*/* TABLES/WORK AREAS:   RTCA                                        */ 02895000
*/*                                                                  */ 02896000
*/* MACROS:              MAPPING: CVT IHAASCB                        */ 02897000
*/*                      IHASDWA                                     */ 02898000
*/*                                                                  */ 02899000
*/* CHANGE ACTIVITY:     THIS MODULE IS NEW FOR VSAM RELEASE 2.      */ 02900000
*/*                                                                  */ 02901000
*/********************************************************************/ 02902000
*/********************************************************************/ 02903000
*/* FUNCTION - RECEIVE CONTROL WHEN IDA0195A PROGRAM CHECKS AND RETRY*/ 02904000
*/* INPUT - THIS IS A RECOVERY ROUTINE AND IS PASSED THE SDWA.       */ 02905000
*/* OUTPUT - THIS ROUTINE CAUSES IDA0195A TO REGAIN CONTROL.         */ 02906000
*/********************************************************************/ 02907000
*                                                                  0872 02908000
*   GEN;                                                           0872 02909000
*       /* GENERATE PROLOG. */                                          02910000
         BALR @09,0 ESTABLISH BASE REGISTER.                            02911000
         DROP @10   RELEASE BASE REGISTER.                              02912000
         DROP @11   RELEASE OTHER BASE REGISTER.                        02913000
RSTART2  SR  @15,@15 CLEAR REGISTER OF RESIDUE.                         02914000
         USING  RSTART2,@09  MODULE BASE IS NEW NINE.                   02915000
         LA  @15,12(0,0) PLACE CODE FOR TEST IN R15.                    02916000
         CR  @00,@15 WAS A SDWA PASSED TO THIS ROUTINE.                 02917000
         BNE RSTART3 IF SDWA PASSED GOTO RSTART3.                       02918000
* NO SDWA WAS PASSED, RETURN TO RTM WITH PERCOLATION.                   02919000
         SR  @15,@15 CLEAR RETURN CODE TO ZERO.                         02920000
         BR  @14  TAKE EXIT TO RTM.                                     02921000
* SDWA WAS PASSED, PROCEED WITH RECOVERY ROUTINE.                       02922000
RSTART3 STM  @14,@12,12(@13) SAVE REGISTERS PASSED.                     02923000
         USING  @DATD,@12 RE-ESTABLISH DATAREG AS R12.                  02924000
         DROP R13                     STOP USING R13 FOR DATARE@ZA11576 02925000
*   RFY                                                            0873 02926000
*    (R12,                                                         0873 02927000
*     R13) RSTD;                    /* MAKE A NOTE FOR THE COMPILER.    02928000
*                                                            @ZA11576*/ 02929000
*   R12=SDWAPARM;                   /* RESTORE DATAREG AS 12.        */ 02930000
*                                                                  0874 02931000
         L     R12,SDWAPARM(,R1)                                   0874 02932000
*   /*****************************************************************/ 02933000
*   /*                                                               */ 02934000
*   /* TEST FOR ADDITIONAL CONDITIONS WHICH PROHIBIT RETRY.          */ 02935000
*   /*                                                               */ 02936000
*   /*****************************************************************/ 02937000
*                                                                  0875 02938000
*   IF SDWACLUP^=ON&                /* ENTRY NOT FOR CLEANUP?        */ 02939000
*       SDWANRBE^=ON THEN           /* REALLY THIS RB LEVEL?         */ 02940000
         TM    SDWACLUP(R1),B'11000000'                            0875 02941000
         BNZ   @RF00875                                            0875 02942000
*     DO;                           /* RETRY MAY BE POSSIBLE.        */ 02943000
*       IF SDWAEAS^=ON THEN         /* ERROR RECORD NEEDED?          */ 02944000
         TM    SDWAEAS(R1),B'00001000'                             0877 02945000
         BO    @RF00877                                            0877 02946000
*         DO;                       /* MAKE AN ERROR RECORD.         */ 02947000
*                                                                  0878 02948000
*           /*********************************************************/ 02949000
*           /*                                                       */ 02950000
*           /* SWITCH TO KEY ZERO.                                   */ 02951000
*           /*                                                       */ 02952000
*           /*********************************************************/ 02953000
*                                                                  0879 02954000
*           SDWAMODN='IDA0195A';    /* NOTE MODULE IN ERROR.         */ 02955000
         MVC   SDWAMODN(8,R1),@CC03310                             0879 02956000
*           SDWACSCT='IDA0195A';    /* NOTE CSECT IN CONTROL.        */ 02957000
         MVC   SDWACSCT(8,R1),@CC03310                             0880 02958000
*           SDWAREXN='IDA0195A';    /* NOTE RECOVERY RTNE NAME.      */ 02959000
*                                                                  0881 02960000
         MVC   SDWAREXN(8,R1),@CC03310                             0881 02961000
*           /*********************************************************/ 02962000
*           /*                                                       */ 02963000
*           /* NOW BACK TO TCB PKF.                                  */ 02964000
*           /*                                                       */ 02965000
*           /*********************************************************/ 02966000
*                                                                  0882 02967000
*         END;                      /* END OF ERROR RECORDING.       */ 02968000
*                                                                  0882 02969000
*       /*************************************************************/ 02970000
*       /*                                                           */ 02971000
*       /* HAS THIS CODE JUST BEEN ENTERED OR ARE WE LOOPING?        */ 02972000
*       /*                                                           */ 02973000
*       /*************************************************************/ 02974000
*                                                                  0883 02975000
*       IF RETRY=ON THEN            /* IS RETRY ALREADY IN PROGRESS? */ 02976000
@RF00877 TM    RETRY,B'10000000'                                   0883 02977000
         BNO   @RF00883                                            0883 02978000
*         DO;                       /* RECURSION IN PROGRESS.        */ 02979000
*           GEN;                                                   0885 02980000
*         /* RETURN TO RTM WITH PERCOLATION. */                         02981000
         LM @14,@12,12(@13) RESTORE PASSED REGISTERS.                   02982000
         SR  @15,@15 CLEAR RETURN CODE.                                 02983000
         BR  @14 EXIT TO RTM.                                           02984000
*         END;                      /* END OF PATH FOR RECURSION.    */ 02985000
*                                                                  0886 02986000
*       /*************************************************************/ 02987000
*       /*                                                           */ 02988000
*       /* RETRY MAY BE ATTEMPTED, PROCEED.                          */ 02989000
*       /*                                                           */ 02990000
*       /*************************************************************/ 02991000
*                                                                  0887 02992000
*       ELSE                        /* OTHERWISE RECURSION IS NOT IN    02993000
*                                      PROGRESS.                     */ 02994000
*         DO;                       /* PROCESSING FOR ATTEMPT AT   0887 02995000
*                                      RETRY.                        */ 02996000
         B     @RC00883                                            0887 02997000
@RF00883 DS    0H                                                  0888 02998000
*           RETRY=ON;               /* NOTE RETRY ATTEMPT.           */ 02999000
*                                                                  0888 03000000
         OI    RETRY,B'10000000'                                   0888 03001000
*           /*********************************************************/ 03002000
*           /*                                                       */ 03003000
*           /* SWITCH TO KEY ZERO.                                   */ 03004000
*           /*                                                       */ 03005000
*           /*********************************************************/ 03006000
*                                                                  0889 03007000
*           SDWARTYA=RETRYPTR;      /* ESTABLISH POINT OF RETRY.     */ 03008000
         L     @08,RETRYPTR                                        0889 03009000
         ST    @08,SDWARTYA(,R1)                                   0889 03010000
*           SDWARCDE=RETRYCDE;      /* SET CODE FOR RETRY.           */ 03011000
         MVI   SDWARCDE(R1),X'04'                                  0890 03012000
*           SDWAUPRG=ON;            /* USE REGISTERS FOR RETRY.      */ 03013000
*           SDWAFREE=ON;            /* FREE MAIN SDWA AT RETRY.      */ 03014000
         OI    SDWAUPRG(R1),B'00001100'                            0892 03015000
*           SDWASR01=RETRYR1;       /* ESTABLISH RETRY REGISTER.     */ 03016000
         L     @08,RETRYR1                                         0893 03017000
         ST    @08,SDWASR01(,R1)                                   0893 03018000
*           SDWASR09=RETRYR9;       /* ESTABLISH RETRY REGISTER.     */ 03019000
         L     @08,RETRYR9                                         0894 03020000
         ST    @08,SDWASR09(,R1)                                   0894 03021000
*           SDWASR10=RETRYR10;      /* ESTABLISH RETRY REGISTER.     */ 03022000
         L     @08,RETRYR10                                        0895 03023000
         ST    @08,SDWASR10(,R1)                                   0895 03024000
*           SDWASR11=RETRYR11;      /* ESTABLISH RETRY REGISTER.     */ 03025000
         L     @08,RETRYR11                                        0896 03026000
         ST    @08,SDWASR11(,R1)                                   0896 03027000
*           SDWASR12=RETRYR12;      /* ESTABLISH RETRY REGISTER.     */ 03028000
         L     @08,RETRYR12                                        0897 03029000
         ST    @08,SDWASR12(,R1)                                   0897 03030000
*           SDWASR13=RETRYR13;      /* ESTABLISH RETRY REGISTER.     */ 03031000
         L     @08,RETRYR13                                        0898 03032000
         ST    @08,SDWASR13(,R1)                                   0898 03033000
*           GEN;                                                   0899 03034000
*          /* BUILD EXIT CODE. */                                       03035000
         L  @00,SDWARTYA(,@01)  LOAD RETRY ADDRESS.                     03036000
         LM  @01,@12,24(@13)  RESTORE ENTRY REGS.                       03037000
         L  @14,12(,@13) RESTORE RETURN ADDRESS.                        03038000
         SR  @15,@15 CLEAR REGISTER OF RESIDUE.                         03039000
         LA  @15,4(0,0) SET RETURN TO RETRY.                            03040000
         BR  @14  TAKE EXIT TO RTM.                                     03041000
*         END;                      /* END OF PROCESSING FOR RETRY.  */ 03042000
*     END;                          /* END OF PATH, RETRY POSSIBLE.  */ 03043000
*                                                                  0901 03044000
*   /*****************************************************************/ 03045000
*   /*                                                               */ 03046000
*   /* RETRY IS PROHIBITTED BY REASONS FOR ESTAE ENTRY.              */ 03047000
*   /*                                                               */ 03048000
*   /*****************************************************************/ 03049000
*                                                                  0902 03050000
*   ELSE                            /* OTHERWISE REASON FOR ENTRY  0902 03051000
*                                      PRECLUDES RETRY.              */ 03052000
*     DO;                           /* RETRY PROHIBITTED..           */ 03053000
         B     @RC00875                                            0902 03054000
@RF00875 DS    0H                                                  0903 03055000
*       GEN;                                                       0903 03056000
*         /* RETURN TO RTM WITH PERCOLATION. */                         03057000
         LM @14,@12,12(@13) RESTORE PASSED REGISTERS.                   03058000
         SR  @15,@15 CLEAR RETURN CODE.                                 03059000
         BR  @14 EXIT TO RTM.                                           03060000
*     END;                          /* END OF PATH FOR RETRY       0904 03061000
*                                      PROHIBITTED..                 */ 03062000
*   END RCVRRTN;                    /* END OF RCVRRTN PROCUDURE.     */ 03063000
@EL00013 DS    0H                                                  0905 03064000
@EF00013 DS    0H                                                  0905 03065000
@ER00013 BR    @14                                                 0905 03066000
*   GEN;                                                           0906 03067000
*                                     /* REBASING CODE       @ZA11576*/ 03068000
       USING @DATD,R13                RE-ESTABLISH DATAREG   @ZA11576   03069000
       DROP  R12                      DROP R12 AS DATAREG BAS@ZA11576   03070000
*   END IDA0195A                    /* THIS IS THE LAST LINE OF THE     03071000
*                                      MODULE.                       */ 03072000
