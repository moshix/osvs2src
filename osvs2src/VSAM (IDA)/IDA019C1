*     END;                                                         0605 01255000
*   IF ACBOPEN=ON THEN              /* ACB IS OPEN                   */ 01256000
@RF00601 TM    ACBOPEN(RBLKPTR),B'00010000'                        0606 01257000
         BNO   @RF00606                                            0606 01258000
*     DO;                           /* ERROR PROCESS                 */ 01259000
*       REG0=ACBISOPN;              /* SET REASON CODE               */ 01260000
         LA    REG0,12                                             0608 01261000
*       GO TO EXITBAD;              /* EXIT WITH ERROR               */ 01262000
         B     EXITBAD                                             0609 01263000
*     END;                                                         0610 01264000
*                                                                  0610 01265000
*   /*****************************************************************/ 01266000
