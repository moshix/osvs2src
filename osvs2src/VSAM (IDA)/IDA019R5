*             ELSE                  /* FOR NON-SHARED, USE   @X04SVMR*/ 00740000
*               RBUFC=BUFCHAIN;     /* THE PHYSICAL CHAIN.   @X04SVMR*/ 00741000
         B     @RC00521                                            0523 00742000
@RF00521 L     RBUFC,BUFCHAIN(,RBUFC)                              0523 00743000
*           END;                    /* END OF NON-ERROR      @X04SVMR*/ 00744000
@RC00521 DS    0H                                                  0525 00745000
*       END;                        /* END OF ERROR SEARCH           */ 00746000
@RC00518 DS    0H                                                  0525 00747000
@DE00517 LTR   RPARM1,RPARM1                                       0525 00748000
