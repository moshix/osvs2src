*     RWORK2,                                                      0508 00685000
*     R14) UNRSTD;                  /* RELEASE REGS          @ZA14801*/ 00686000
*                                                                  0509 00687000
*/*   FINDBUFC--LOCATE AN ERROR BUFC                                 */ 00688000
*/* FUNCTION = SEARCH FOR AN ERROR BUFC                              */ 00689000
*/* INPUT = RBUFC POINTS TO THE CHAIN OF BUFCS TO BE SEARCHED        */ 00690000
*/*         FOR AN ERROR BUFC, RPARM1 = ZERO IF NO BUFC FOUND YET.   */ 00691000
*/* OUTPUT = RPARM1 IS ZERO IF NO ERROR IS FOUND.  IF AN ERROR IS    */ 00692000
*/*         FOUND, RPARM1 AND RBUFC WILL POINT TO THE BUFC IN        */ 00693000
*/*         ERROR.                                           @X04SVMR*/ 00694000
*                                                                  0509 00695000
*FINDBUFC:                                                         0509 00696000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0509 00697000
FINDBUFC DS    0H                                                  0510 00698000
*   RFY                                                            0510 00699000
*     RWORK4 RSTD;                  /* SAVE REG FOR CALLER   @X04SVMR*/ 00700000
*   RFY                                                            0511 00701000
*    (RWORK1,                                                      0511 00702000
*     R14) RSTD;                    /* SAVE RETURN REGISTER          */ 00703000
*/*  FOR SHARED RESOURCES SEARCH THE I/O BUFC CHAIN.  FOR NON-SHARED */ 00704000
*/*  SEARCH THE PHYSICAL BUFC CHAIN.                         @X04SVMR*/ 00705000
*                                                                  0512 00706000
*   IF AMBLSR=ON|AMBGSR=ON THEN     /* FOR SHARED RESOURCE   @X04SVMR*/ 00707000
         TM    AMBLSR(RAMB),B'01100000'                            0512 00708000
         BZ    @RF00512                                            0512 00709000
*     RWORK1=RBUFC;                 /* SET SEARCH LIMIT      @X04SVMR*/ 00710000
         LR    RWORK1,RBUFC                                        0513 00711000
*   ELSE                            /* FOR NON-SHARED        @X04SVMR*/ 00712000
*     RWORK1=ZERO;                  /* SEARCH TIL END        @X04SVMR*/ 00713000
         B     @RC00512                                            0514 00714000
@RF00512 SLR   RWORK1,RWORK1                                       0514 00715000
*   IF RPARM1=ZERO THEN             /* IF NO IOB FOUND YET,  @X04SVMR*/ 00716000
@RC00512 LTR   RPARM1,RPARM1                                       0515 00717000
         BNZ   @RF00515                                            0515 00718000
*     DO;                           /* SEARCH FOR ONE.       @X04SVMR*/ 00719000
*/*  SEARCH UNTIL THE END OF THE BUFFER CHAIN OR UNTIL THE ERROR     */ 00720000
*/*  BUFC IS FOUND.                                          @X04SVMR*/ 00721000
*                                                                  0517 00722000
*       DO UNTIL RPARM1^=ZERO|RBUFC=RWORK1;/* SCH TIL DONE   @X04SVMR*/ 00723000
@DL00517 DS    0H                                                  0518 00724000
*         IF BUFCER1=ON|BUFCER2=ON THEN/* IF AN I/O ERROR SET RC     */ 00725000
         TM    BUFCER1(RBUFC),B'00011000'                          0518 00726000
         BZ    @RF00518                                            0518 00727000
*           RPARM1=RBUFC;           /* TO NON-ZERO.                  */ 00728000
         LR    RPARM1,RBUFC                                        0519 00729000
*         ELSE                      /* GET NEXT BUFC PTR             */ 00730000
*           DO;                     /* NON-ERROR BUFC        @X04SVMR*/ 00731000
         B     @RC00518                                            0520 00732000
@RF00518 DS    0H                                                  0521 00733000
*             IF AMBLSR=ON|AMBGSR=ON THEN/* IF SHARED RESOURCE,    0521 00734000
*                                                            @X04SVMR*/ 00735000
         TM    AMBLSR(RAMB),B'01100000'                            0521 00736000
         BZ    @RF00521                                            0521 00737000
*               RBUFC=BUFCNXT1;     /* USE THE I/O CHAIN.    @X04SVMR*/ 00738000
