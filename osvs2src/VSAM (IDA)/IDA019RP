               ES       '                                               00002000
IDA019RP CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IDA019RP  78.172'                                 0001 00009000
IDATJXIT DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00192-IDATJXIT)                                   00013000
         ENTRY IDATJXIT                                                 00014000
IDAENDRQ DS    0H                                                  0001 00015000
         USING *,@15                                               0001 00016000
         B     @PROLOG                                             0001 00017000
         DC    AL2(@EP00263-IDAENDRQ)                                   00018000
         ENTRY IDAENDRQ                                                 00019000
IDAPTCBV DS    0H                                                  0001 00020000
         USING *,@15                                               0001 00021000
         B     @PROLOG                                             0001 00022000
         DC    AL2(@EP00477-IDAPTCBV)                                   00023000
         ENTRY IDAPTCBV                                                 00024000
IDAPGETM DS    0H                                                  0001 00025000
         USING *,@15                                               0001 00026000
         B     @PROLOG                                             0001 00027000
         DC    AL2(@EP00492-IDAPGETM)                                   00028000
         ENTRY IDAPGETM                                                 00029000
IDAPFREE DS    0H                                                  0001 00030000
         USING *,@15                                               0001 00031000
         B     @PROLOG                                             0001 00032000
