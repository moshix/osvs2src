         DROP  @15                                                      00040000
@PROLOG  BALR  @11,0                                               0001 00041000
@PSTART  DS    0H                                                  0001 00042000
         USING @PSTART,@11                                         0001 00043000
         AH    @15,4(,@15)                                         0001 00044000
         BR    @15                                                 0001 00045000
@EP00001 DS    0H                                                  0002 00046000
*                                                                  0192 00047000
*/* IDATJXIT -- JRNAD ROUTINE INTERFACE                              */ 00048000
*/* FUNCTION = SAVE REGISTERS, TAKE JRNAD EXIT, RESTORE REGISTERS.   */ 00049000
*/* INPUT = STANDARD REGISTERS.                                      */ 00050000
*/* OUTPUT = STANDARD REGISTERS.                                     */ 00051000
*                                                                  0192 00052000
*IDATJXIT:                                                         0192 00053000
*   ENTRY;                          /* TAKE JOURNAL EXIT             */ 00054000
@EP00192 DS    0H                                                  0193 00055000
*   RFY                                                            0193 00056000
*    (RWORK1,                                                      0193 00057000
*     RWORK2,                                                      0193 00058000
*     R13) RSTD;                    /* GET REGS FOR ACB, R1          */ 00059000
*   IF AMBLPATH=ON|                 /* DO NOT JOURNAL PATH           */ 00060000
*       AMBLUPGR=ON THEN            /* OR UPGRADE ACCESSES.          */ 00061000
         L     @10,AMBPAMBL(,RAMB)                                 0194 00062000
         TM    AMBLPATH(@10),B'11000000'                           0194 00063000
         BNZ   @RT00194                                            0194 00064000
*     RETURN;                       /* IGNORE JRNAD.                 */ 00065000
*   IF AMBDFR=ON&                   /* IF THE DFR OPTION AND         */ 00066000
*       PLHJCODE=PLHJOER THEN       /* OUTPUT ERROR, FIND            */ 00067000
         TM    AMBDFR(RAMB),B'00001000'                            0196 00068000
         BNO   @RF00196                                            0196 00069000
         CLI   PLHJCODE(RPLH),B'00101000'                          0196 00070000
         BNE   @RF00196                                            0196 00071000
*     PLHACB=AMBLACB;               /* SET ERROR ACB         @ZA14829*/ 00072000
         L     @10,AMBLACB(,@10)                                   0197 00073000
         ST    @10,PLHACB(,RPLH)                                   0197 00074000
*   ELSE                            /* OTHERWISE, USE RPL PTR        */ 00075000
*     IF AMBLBASE=ON&               /* IF JRNAD FOR BASE             */ 00076000
*         PLHXPLH^=ZERO THEN        /* ACCESSED THROUGH A PATH,      */ 00077000
         B     @RC00196                                            0198 00078000
@RF00196 L     @10,AMBPAMBL(,RAMB)                                 0198 00079000
         TM    AMBLBASE(@10),B'00010000'                           0198 00080000
         BNO   @RF00198                                            0198 00081000
         L     @10,PLHXPLH(,RPLH)                                  0198 00082000
         LTR   @10,@10                                             0198 00083000
         BZ    @RF00198                                            0198 00084000
*       PLHACB=PLHXPLH->PLHCRPL->RPLDACB;/* SET TO PATH ACB  @ZA14829*/ 00085000
         L     @10,PLHCRPL(,@10)                                   0199 00086000
         L     @10,RPLDACB(,@10)                                   0199 00087000
         ST    @10,PLHACB(,RPLH)                                   0199 00088000
*   RFY                                                            0200 00089000
*     IFGACB BASED(PLHACB);         /* DEFINE ACB BASE       @ZA14829*/ 00090000
@RF00198 DS    0H                                                  0200 00091000
@RC00196 DS    0H                                                  0201 00092000
*/*  MAKE SURE JRNAD IS PRESENT FIRST.                               */ 00093000
*                                                                  0201 00094000
*   IF ACBUEL>ONE&                  /* SEE IF EXIT LIST      @ZA14829*/ 00095000
*       EXLLEN=>35&                 /* EXISTS WHICH HAS      @ZA14829*/ 00096000
*       EXLJRNF(2)=ON THEN          /* ACTIVE JRNAD          @ZA14829*/ 00097000
         L     @10,PLHACB(,RPLH)                                   0201 00098000
         L     @10,ACBUEL(,@10)                                    0201 00099000
         C     @10,@CF00047                                        0201 00100000
         BNH   @RF00201                                            0201 00101000
         LH    @07,EXLLEN(,@10)                                    0201 00102000
         C     @07,@CF02406                                        0201 00103000
         BL    @RF00201                                            0201 00104000
         TM    EXLJRNF(@10),B'01000000'                            0201 00105000
         BNO   @RF00201                                            0201 00106000
*     DO;                           /*                       @ZA14829*/ 00107000
*/*  SAVE INPUT REGISTERS.                                           */ 00108000
*                                                                  0203 00109000
*       GEN SETS(PLHRETO,PLHRET1)(IDASVR14);/* SAVE RETURN           */ 00110000
         IDASVR14                                                       00111000
*       PLHAMB=RAMB;                /* SAVE RAMB                     */ 00112000
         ST    RAMB,PLHAMB(,RPLH)                                  0204 00113000
*       PLHRBUFC=RBUFC;             /* SAVE RBUFC                    */ 00114000
         ST    RBUFC,PLHRBUFC(,RPLH)                               0205 00115000
*       PLHPARM1=RPARM1;            /* SAVE RPARM1                   */ 00116000
         ST    RPARM1,PLHPARM1(,RPLH)                              0206 00117000
*       PLHR13=R13;                 /* SAVE R13                      */ 00118000
         ST    R13,PLHR13(,RPLH)                                   0207 00119000
*       RFY                                                        0208 00120000
*        (RWORK0,                                                  0208 00121000
*         R15) RSTD;                /* GET REGISTERS                 */ 00122000
*       R15=EXLJRNP;                /* POINT TO JOURNAL EXIT         */ 00123000
         L     @10,PLHACB(,RPLH)                                   0209 00124000
         L     @10,ACBUEL(,@10)                                    0209 00125000
         ICM   R15,15,EXLJRNP(@10)                                 0209 00126000
*/*  LOAD EXIT IF NECESSARY.                                         */ 00127000
*                                                                  0210 00128000
*       IF EXLJRNF(3)=ON            /* SEE IF LINK                   */ 00129000
*         THEN                      /* HERE FOR LINK                 */ 00130000
         TM    EXLJRNF(@10),B'00100000'                            0210 00131000
         BNO   @RF00210                                            0210 00132000
*         DO;                                                      0211 00133000
*           RWORK1=R1;              /* SAVE REGISTER ONE             */ 00134000
         LR    RWORK1,R1                                           0212 00135000
*           GEN(LOAD  EPLOC=(R15)); /* LOAD ROUTINE                  */ 00136000
         LOAD  EPLOC=(R15)                                              00137000
*           R15=RWORK0;             /* GET ADDR INTO R15             */ 00138000
         LR    R15,RWORK0                                          0214 00139000
*           R1=RWORK1;              /* RESTORE REGISTER 1            */ 00140000
         LR    R1,RWORK1                                           0215 00141000
*         END;                      /* END OF LOAD CODE              */ 00142000
*       IF RPLREQ>=RPLPFMTD&        /* HAS THE ORIGINAL              */ 00143000
*           RPLREQ<RPLGETIX THEN    /* RPL ADDR BEEN SAVED?          */ 00144000
@RF00210 CLI   RPLREQ(RRPL),8                                      0217 00145000
         BL    @RF00217                                            0217 00146000
         CLI   RPLREQ(RRPL),11                                     0217 00147000
         BNL   @RF00217                                            0217 00148000
*         DO;                       /* YES, SWAP IT.                 */ 00149000
*           PLHIOSRQ=ON;            /* I/O SUPPORT REQ       @ZA06117*/ 00150000
         OI    PLHIOSRQ(RPLH),B'01000000'                          0219 00151000
*           RWORK1=PLHSLRBA;        /* GET SAVED RPL ADDRESS         */ 00152000
         L     RWORK1,PLHSLRBA(,RPLH)                              0220 00153000
*           PLHSLRBA=PLHCRPL;       /* SWAP THE CURRENT AND          */ 00154000
         L     @14,PLHCRPL(,RPLH)                                  0221 00155000
         ST    @14,PLHSLRBA(,RPLH)                                 0221 00156000
*           PLHCRPL=RWORK1;         /* SAVED RPLS                    */ 00157000
         ST    RWORK1,PLHCRPL(,RPLH)                               0222 00158000
*         END;                      /* END OF RPL SWAP               */ 00159000
*       RFY                                                        0224 00160000
*         RWORK2 UNRSTD;            /* DROP ACB BASE                 */ 00161000
@RF00217 DS    0H                                                  0225 00162000
*/*  SET UP COMMON PARAMETER LIST INFORMATION AND TAKE EXIT.         */ 00163000
*                                                                  0225 00164000
*       IF AMBTYPE=AMBDATA THEN     /* SEE IF DATA AMB               */ 00165000
         TM    AMBTYPE(RAMB),B'01000000'                           0225 00166000
         BNZ   @RF00225                                            0225 00167000
*         PLHDSTYP=PLHTDATA;        /* SET CODE FOR DATA             */ 00168000
         MVI   PLHDSTYP(RPLH),B'00000001'                          0226 00169000
*       ELSE                        /* HERE FOR INDEX AMB            */ 00170000
*         PLHDSTYP=PLHTIDX;         /* SET CODE FOR INDEX            */ 00171000
         B     @RC00225                                            0227 00172000
@RF00225 MVI   PLHDSTYP(RPLH),B'00000010'                          0227 00173000
*       R1=ADDR(PLHCRPL);           /* POINT TO PARM LIST            */ 00174000
@RC00225 LA    R1,PLHCRPL(,RPLH)                                   0228 00175000
*I01:                                                              0229 00176000
*       DO;                         /* IDACALL(JRNAD)                */ 00177000
I01      DS    0H                                                  0230 00178000
*         CALL JRNAD;               /* EXIT TO JRNAD                 */ 00179000
         BALR  @14,@15                                             0230 00180000
*         GEN(BALR @11,0);          /* LOAD ADDR OF REENTRY          */ 00181000
         BALR @11,0                                                     00182000
*         GEN(USING *,@11);         /* ESTABLISH ADDRESSING          */ 00183000
         USING *,@11                                                    00184000
*         GEN(L     @11,=A(@PSTART));/* RELOAD BASE                  */ 00185000
         L     @11,=A(@PSTART)                                          00186000
*         GEN(USING @PSTART,@11);   /* ESTABLISH ADDRESSING          */ 00187000
         USING @PSTART,@11                                              00188000
*       END I01;                    /* TAKE JRNAD EXIT               */ 00189000
*       RRPL=RPLSAVE;               /* RESTORE RRPL                  */ 00190000
         L     RRPL,RPLSAVE(,R1)                                   0236 00191000
*       RPLH=RPLPLHPT;              /* RESTORE RPLH                  */ 00192000
         L     RPLH,RPLPLHPT(,RRPL)                                0237 00193000
*       IF PLHIOSRQ=ON THEN         /* IF I/O SUPPORT        @ZA06117*/ 00194000
         TM    PLHIOSRQ(RPLH),B'01000000'                          0238 00195000
         BNO   @RF00238                                            0238 00196000
*         DO;                       /* RESTORE RPL ADDR      @ZA06117*/ 00197000
*           PLHIOSRQ=OFF;           /* RESET FLAG.           @ZA06117*/ 00198000
         NI    PLHIOSRQ(RPLH),B'10111111'                          0240 00199000
*           RWORK1=PLHSLRBA;        /* GET SAVED RPL ADDRESS         */ 00200000
         L     RWORK1,PLHSLRBA(,RPLH)                              0241 00201000
*           PLHSLRBA=PLHCRPL;       /* SWAP THE CURRENT AND          */ 00202000
         L     @14,PLHCRPL(,RPLH)                                  0242 00203000
         ST    @14,PLHSLRBA(,RPLH)                                 0242 00204000
*           PLHCRPL=RWORK1;         /* SAVED RPLS                    */ 00205000
         ST    RWORK1,PLHCRPL(,RPLH)                               0243 00206000
*         END;                      /* END I/O SUPPORT       @ZA06117*/ 00207000
*/*  DELETE EXIT IF IT WAS LOADED.                                   */ 00208000
*                                                                  0245 00209000
*       IF EXLJRNF(3)=ON            /* WAS ROUTINE LOADED            */ 00210000
*         THEN                      /* IF SO, MUST DELETE            */ 00211000
@RF00238 L     @14,PLHACB(,RPLH)                                   0245 00212000
         L     @14,ACBUEL(,@14)                                    0245 00213000
         TM    EXLJRNF(@14),B'00100000'                            0245 00214000
         BNO   @RF00245                                            0245 00215000
*         DO;                                                      0246 00216000
*           RWORK1=R1;              /* SAVE REGISTER ONE             */ 00217000
         LR    RWORK1,R1                                           0247 00218000
*           R15=EXLJRNP;            /* POINT TO RTN NAME             */ 00219000
         ICM   R15,15,EXLJRNP(@14)                                 0248 00220000
*           GEN(DELETE EPLOC=(R15));/* DELETE ROUTINE                */ 00221000
         DELETE EPLOC=(R15)                                             00222000
*           R1=RWORK1;              /* RESTORE REGISTER ONE          */ 00223000
         LR    R1,RWORK1                                           0250 00224000
*         END;                      /* END OF DELETE CODE            */ 00225000
*       RFY                                                        0252 00226000
*        (RWORK0,                                                  0252 00227000
*         R15) UNRSTD;              /* DROP REGISTERS                */ 00228000
@RF00245 DS    0H                                                  0253 00229000
*/*  RESTORE REGISTERS                                               */ 00230000
*                                                                  0253 00231000
*       RAMB=PLHAMB;                /* RESTORE RAMB                  */ 00232000
         L     RAMB,PLHAMB(,RPLH)                                  0253 00233000
*       RBUFC=PLHRBUFC;             /* RESTORE RBUFC                 */ 00234000
         L     RBUFC,PLHRBUFC(,RPLH)                               0254 00235000
*       RPARM1=PLHPARM1;            /* RESTORE RPARM1                */ 00236000
         L     RPARM1,PLHPARM1(,RPLH)                              0255 00237000
*       R13=PLHR13;                 /* RESTORE R13                   */ 00238000
         L     R13,PLHR13(,RPLH)                                   0256 00239000
*       GEN(IDARST14);                                             0257 00240000
         IDARST14                                                       00241000
*     END;                          /*                       @ZA14829*/ 00242000
*   RWORK1=PLHCRPL->RPLDACB;        /* POINT TO ACB          @ZA14829*/ 00243000
@RF00201 L     @10,PLHCRPL(,RPLH)                                  0259 00244000
         L     RWORK1,RPLDACB(,@10)                                0259 00245000
*   GEN(LA RWORK1,0(RWORK1));       /* DROP HI ORDER BYTE    @ZA14829*/ 00246000
         LA RWORK1,0(RWORK1)                                            00247000
*   PLHACB=RWORK1;                  /* RESTORE ACB ADDR      @ZA14829*/ 00248000
         ST    RWORK1,PLHACB(,RPLH)                                0261 00249000
*   RETURN;                         /* RETURN TO CALLER              */ 00250000
@EL00001 DS    0H                                                  0262 00251000
@EF00001 DS    0H                                                  0262 00252000
@ER00001 BR    @14                                                 0262 00253000
*                                                                  0263 00254000
*/* IDAENDRQ -- ENDREQ ROUTINE                                       */ 00255000
*/*01 FUNCTION = QUIESCE I/O, FORCE WRITING, SUBTRACT EXCESS BUFFERS,*/ 00256000
*/*   AND DISCONNECT THE PLACEHOLDER FROM THE RPL.                   */ 00257000
*/*01 INPUT = RRPL IS A POINTER TO AN RPL AT THE HEAD OF A CHAIN,    */ 00258000
*/*   RAMB POINTS TO THE DATA AMB, AND RPLH IS THE PLH ADDRESS.      */ 00259000
*/*01 OUTPUT = REGISTERS SAME AS UPON ENTRY.                         */ 00260000
*                                                                  0263 00261000
*IDAENDRQ:                                                         0263 00262000
*   ENTRY;                          /* END REQUEST ROUTINE           */ 00263000
@EP00263 DS    0H                                                  0264 00264000
*   RFY                                                            0264 00265000
*     IFGACB BASED(RPLDACB);        /* DEFINE ACB BASE               */ 00266000
*   RESPECIFY                                                      0265 00267000
*     IDAAMBL;                      /* DEFINE AMBL BASE REG          */ 00268000
*   RESPECIFY                                                      0266 00269000
*    (RWORK1) RESTRICTED;           /* GET REGISTERS         @ZA14801*/ 00270000
*   RFY                                                            0267 00271000
*     R14 RSTD;                     /* GET REGISTER                  */ 00272000
*/*  HONOR THE ENDREQ UNLESS THE USER IS CALLING IN CREATE MODE.     */ 00273000
*/*  (ALLOWING MULTIPLE ENDREQS IN CREATE WILL CAUSE DATA LOSS.)     */ 00274000
*                                                                  0268 00275000
*   IF^(ACBIOSFG=OFF&AMBCREAT=ON) THEN/* VALID ENDREQ?               */ 00276000
         L     @15,RPLDACB(,RRPL)                                  0268 00277000
         TM    ACBIOSFG(@15),B'00000001'                           0268 00278000
         BNZ   @RT00268                                            0268 00279000
         TM    AMBCREAT(RAMB),B'10000000'                          0268 00280000
         BO    @RF00268                                            0268 00281000
@RT00268 DS    0H                                                  0269 00282000
*     DO;                           /* HERE IF VALID ENDREQ          */ 00283000
*/* PREVENT MORE RPLS IN THE CHAIN FROM STARTING BY SETTING THE      */ 00284000
*/* ENDREQ FLAG                                                      */ 00285000
*                                                                  0270 00286000
*       PLHENDRQ=ON;                /* INDICATE ENDREQ ISSUED        */ 00287000
         OI    PLHENDRQ(RPLH),B'01000000'                          0270 00288000
*       RRPL=PLHCRPL;               /* POINT TO CURRENT RPL          */ 00289000
         L     RRPL,PLHCRPL(,RPLH)                                 0271 00290000
*/* IF AN RPL IS ASSOCIATED WITH THE PLACEHOLDER, PROCESS THE        */ 00291000
*/* ENDREQ REQUEST                                                   */ 00292000
*                                                                  0272 00293000
*       IF RRPL=ZERO THEN           /* IF AN RPL IS NOT      @ZA14801*/ 00294000
         LTR   RRPL,RRPL                                           0272 00295000
         BNZ   @RF00272                                            0272 00296000
*         RRPL=PLHMRPL;             /* ASSOCIATED--NO-OP     @ZA14801*/ 00297000
         L     RRPL,PLHMRPL(,RPLH)                                 0273 00298000
*       ELSE                        /* IF RPL IS ASSOC W     @ZA14801*/ 00299000
*         DO;                       /* THE PLH, COME HERE            */ 00300000
*                                                                  0274 00301000
         B     @RC00272                                            0274 00302000
@RF00272 DS    0H                                                  0275 00303000
*           /*********************************************************/ 00304000
*           /*                                                       */ 00305000
*           /* IF THE CURRENT RPL IS ACTIVE, ALLOW IT TO COMPLETE    */ 00306000
*           /* BEFORE ATTEMPTING TO USE THE PLACEHOLDER.             */ 00307000
*           /*                                                       */ 00308000
*           /*********************************************************/ 00309000
*                                                                  0275 00310000
*           TS(RPLACTIV);           /* IF RPL IS ACTIVE      @ZA14801*/ 00311000
         TS    RPLACTIV(RRPL)                                      0275 00312000
*           BC(8,NOWAIT);           /* WAIT FOR IT.          @ZA14801*/ 00313000
         BC    8,NOWAIT                                            0276 00314000
*           RWORK1=R1;              /* SAVE REGISTER ONE             */ 00315000
         LR    RWORK1,R1                                           0277 00316000
*           IF RPLECBSW=OFF THEN    /* SEE WHERE ECB IS              */ 00317000
         TM    RPLECBSW(RRPL),B'00000001'                          0278 00318000
         BNZ   @RF00278                                            0278 00319000
*             R1=ADDR(RPLECB);      /* POINT TO IT IN RPL            */ 00320000
         LA    @15,RPLECB(,RRPL)                                   0279 00321000
         LR    R1,@15                                              0279 00322000
*           ELSE                    /* OR IN USER AREA               */ 00323000
*             R1=ADDR(USERECB);                                    0280 00324000
         B     @RC00278                                            0280 00325000
@RF00278 L     R1,RPLECBPT(,RRPL)                                  0280 00326000
*           IF ECBPOST=OFF THEN     /* SEE IF POSTED YET             */ 00327000
@RC00278 TM    ECBPOST(R1),B'01000000'                             0281 00328000
         BNZ   @RF00281                                            0281 00329000
*             DO;                   /* NOT POSTED            @OZ02513*/ 00330000
*               IF ECBWAIT=ON THEN  /* AND WAITING           @OZ02513*/ 00331000
         TM    ECBWAIT(R1),B'10000000'                             0283 00332000
         BNO   @RF00283                                            0283 00333000
*                 DO;               /* WAITING               @OZ02513*/ 00334000
*                   RRPL=RWORK1;    /* RESTORE RPL REG       @ZA29389*/ 00335000
         LR    RRPL,RWORK1                                         0285 00336000
*                   RPLERREG='08'X; /* SET LOGIC ERR         @OZ02513*/ 00337000
         MVI   RPLERREG(RRPL),X'08'                                0286 00338000
*                   RPLERRCD=RPLINVEQ;/* INVALID ENDREQ      @OZ02513*/ 00339000
         MVI   RPLERRCD(RRPL),X'D0'                                0287 00340000
*                   GOTO EQEXIT;    /* EXIT ENDREQ           @OZ02513*/ 00341000
         B     EQEXIT                                              0288 00342000
*                 END;              /* END WAITING           @OZ02513*/ 00343000
*               ELSE                                               0290 00344000
*                 GEN(WAIT ECB=(1));/* WAIT COMPLETE         @OZ02513*/ 00345000
@RF00283 DS    0H                                                  0290 00346000
         WAIT ECB=(1)                                                   00347000
*             END;                  /* END NOT POSTED        @OZ02513*/ 00348000
*           R1=RWORK1;              /* RESTORE                       */ 00349000
@RF00281 LR    R1,RWORK1                                           0292 00350000
*NOWAIT:                                                           0293 00351000
*           RFY                                                    0293 00352000
*             RWORK1 UNRSTD;        /* DROP WORK REGISTER            */ 00353000
NOWAIT   DS    0H                                                  0294 00354000
*           GEN(IDASVR14);          /* SAVE RETURN ADDR              */ 00355000
         IDASVR14                                                       00356000
*           RFY                                                    0295 00357000
*             R14 UNRSTD;           /* RELEASE REGISTER              */ 00358000
*/* POINT TO MASTER RPL.                                     @ZA14801*/ 00359000
*                                                                  0296 00360000
*           RRPL=PLHMRPL;           /* FIRST RPL IN CH.      @ZA14801*/ 00361000
         L     @10,PLHMRPL(,RPLH)                                  0296 00362000
         LR    RRPL,@10                                            0296 00363000
*/* FORCE SYNCHRONOUS PROCESSING                                     */ 00364000
*                                                                  0297 00365000
*           PLHIWAIT=ON;            /* SET SYNCHRONOUS       @ZA14801*/ 00366000
         OI    PLHIWAIT(RPLH),B'00000001'                          0297 00367000
*           PLHCRPL=PLHMRPL;        /* ENSURE RPL-ACB AGREE          */ 00368000
         ST    @10,PLHCRPL(,RPLH)                                  0298 00369000
*           RPLFDBWD=ZERO;          /* CLEAR ERROR STATUS            */ 00370000
         SLR   @10,@10                                             0299 00371000
         ST    @10,RPLFDBWD(,RRPL)                                 0299 00372000
*/* FOR CREATE MODE, WRITE OUT ALL DATA AND COMPLETE THE INDEXES     */ 00373000
*                                                                  0300 00374000
*           IF AMBCREAT=ON&         /* IF CREATE MODE                */ 00375000
*               ARDHRBA^=ZERO       /* AND THE DATA SET IS NOT       */ 00376000
*             THEN                  /* NULL, PAD OUT CA, ETC.        */ 00377000
         TM    AMBCREAT(RAMB),B'10000000'                          0300 00378000
         BNO   @RF00300                                            0300 00379000
         L     @05,PLHARDB(,RPLH)                                  0300 00380000
         CL    @10,ARDHRBA(,@05)                                   0300 00381000
         BE    @RF00300                                            0300 00382000
*             DO;                                                  0301 00383000
*                                                                  0301 00384000
*/* IF THIS IS A KEY RANGE DATA SET, CALL CONTROL INTERVAL SPLIT FOR */ 00385000
*/* ALL NON-LAST KEY RANGES. (CI SPLIT WILL CALL INDEX CREATE FOR AN */ 00386000
*/* INSERT, PREFORMAT THE FIRST CONTROL AREA IN THE NEXT KEY RANGE,  */ 00387000
*/* AND CALL INDEX INSERT FOR A WRITE.)                              */ 00388000
