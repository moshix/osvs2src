*                                                                  0182 00429000
*   /*****************************************************************/ 00430000
*   /*                                                               */ 00431000
*   /* IF THE EVENT HAS COMPLETED, RETURN TO THE CALLER.             */ 00432000
*   /*                                                               */ 00433000
*   /*****************************************************************/ 00434000
*                                                                  0183 00435000
*   IF IOECBPT=ON THEN              /* IF ECB POSTED                 */ 00436000
         L     @15,IOMECBPT(,RPARM1)                               0183 00437000
         TM    IOECBPT(@15),B'01000000'                            0183 00438000
         BNO   @RF00183                                            0183 00439000
*     DO;                                                          0184 00440000
*       GEN(IDASVR14);              /* SAVE R14              @ZA14824*/ 00441000
         IDASVR14                                                       00442000
*       IF AMBTRACE^=ZERO THEN      /* IS IDATRACE ACTIVE    @ZA29388*/ 00443000
         L     @15,AMBTRACE(,RAMB)                                 0186 00444000
