*                                                                  0155 00317000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0155 00318000
SCANBUFF DS    0H                                                  0156 00319000
*   RESPECIFY                                                      0156 00320000
*     IDABUFDR BASED(AMBBUFC);                                     0156 00321000
*   RESPECIFY                                                      0157 00322000
*     IDACPA BASED(BUFCCPA);                                       0157 00323000
*   RESPECIFY                                                      0158 00324000
*    (RWORK1,                                                      0158 00325000
*     RWORK2,                                                      0158 00326000
*     RWORK3) RESTRICTED;                                          0158 00327000
*   IOMXSCAN=OFF;                   /* SET IOMXSCAN OFF              */ 00328000
         L     @15,IOMUFLD(,RPARM1)                                0159 00329000
         NI    IOMXSCAN(@15),B'11011111'                           0159 00330000
*   RWORK1=IOMBUFC;                 /* POINT TO FIRST BUFC IN      0160 00331000
*                                      BUFCNXT1 CHAIN                */ 00332000
         L     RWORK1,IOMBUFC(,RPARM1)                             0160 00333000
*   RWORK3=RWORK1->BUFCORBA;        /* RBA IN RWORK3                 */ 00334000
         L     RWORK3,BUFCORBA(,RWORK1)                            0161 00335000
*   DO UNTIL RWORK1=IOMBUFC;        /* GO THRU CHAIN                 */ 00336000
@DL00162 DS    0H                                                  0163 00337000
*     IF RWORK1->BUFCER2=OFF&       /* IF NO WRITE ERROR             */ 00338000
*         RWORK1->BUFCCPA->CPAWV=ON THEN/* AND BUF WRITTEN           */ 00339000
         TM    BUFCER2(RWORK1),B'00001000'                         0163 00340000
         BNZ   @RF00163                                            0163 00341000
         L     @15,BUFCCPA(,RWORK1)                                0163 00342000
         TM    CPAWV(@15),B'10000000'                              0163 00343000
         BNO   @RF00163                                            0163 00344000
*       DO;                                                        0164 00345000
*         RWORK2=BUFDBUFC;          /* POINT TO FIRST BUFC           */ 00346000
         L     @15,AMBBUFC(,RAMB)                                  0165 00347000
         L     RWORK2,BUFDBUFC(,@15)                               0165 00348000
*         DO UNTIL RWORK2=0;        /* GO THRU BUFC CHAIN            */ 00349000
@DL00166 DS    0H                                                  0167 00350000
*           IF RWORK3=RWORK2->BUFCDDDD&/* RBA MATCH                  */ 00351000
*               RWORK1^=RWORK2&     /* AND NOT SAME BUFC             */ 00352000
*               RWORK2->BUFCVAL=ON& /* DOWN LEVEL BUFC MARKED VALID  */ 00353000
*               RWORK2->BUFCRRD=OFF THEN/* NO READ REQ'D             */ 00354000
         CL    RWORK3,BUFCDDDD(,RWORK2)                            0167 00355000
         BNE   @RF00167                                            0167 00356000
         CLR   RWORK1,RWORK2                                       0167 00357000
