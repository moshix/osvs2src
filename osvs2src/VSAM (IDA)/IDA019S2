         TITLE 'IDA019S2-FAST PATH I/O DRIVER                          *00001000
                        '                                               00002000
IDA019S2 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IDA019S2  78.180'                                 0001 00009000
S2TERM   DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00537-S2TERM)                                     00013000
         ENTRY S2TERM                                                   00014000
         DROP  @15                                                      00015000
@PROLOG  BALR  @11,0                                               0001 00016000
@PSTART  DS    0H                                                  0001 00017000
         USING @PSTART,@11                                         0001 00018000
         AH    @15,4(,@15)                                         0001 00019000
         BR    @15                                                 0001 00020000
@EP00001 DS    0H                                                  0002 00021000
RBPRFXLN EQU 64                                                         00022000
RBPRFLNA EQU 32                                                         00023000
PRBLEN   EQU 136                                                        00024000
SIRBLEN  EQU 200                                                        00025000
TIRBLEN  EQU 136                                                        00026000
IRBLEN   EQU 128                                                        00027000
SVRBLEN  EQU 224                                                        00028000
*   PLHAR14=R14;                    /* SAVE RETURN REG               */ 00029000
         ST    R14,PLHAR14(,RPLH)                                  0202 00030000
*   RFY                                                            0203 00031000
*     R14 UNRSTD;                                                  0203 00032000
*   RIOMB=PLHDIOB;                  /* IOMB PTR TO RIOMB             */ 00033000
*                                                                  0204 00034000
         L     RIOMB,PLHDIOB(,RPLH)                                0204 00035000
*   /*****************************************************************/ 00036000
*   /*                                                               */ 00037000
*   /* ON AN MP SYSTEM, THE ONLY VALID TEST FOR SRB MODE IS TO TEST  */ 00038000
*   /* THE CURRENT TCB POINTER. THE SRB MODE INDICATOR IN LCCA CAN'T */ 00039000
*   /* BE TESTED SINCE THE ADDRESS OF LCCA VARIES WITH THE CPU.      */ 00040000
*   /*                                                               */ 00041000
*   /*****************************************************************/ 00042000
*                                                                  0205 00043000
*   IF PSATOLD^=ZERO THEN           /* SRB MODE ?                    */ 00044000
         L     @10,PSATOLD                                         0205 00045000
         LTR   @10,@10                                             0205 00046000
         BZ    @RF00205                                            0205 00047000
*     IF PSWPPBIT=ON THEN           /* NO, IS CALLER IN PROBLEM    0206 00048000
*                                      STATE?                @Z40GSDT*/ 00049000
         L     @01,TCBRBP(,@10)                                    0206 00050000
         TM    PSWPPBIT(@01),B'00000001'                           0206 00051000
         BNO   @RF00206                                            0206 00052000
*       DO;                         /* YES                           */ 00053000
*         PLHSR=ON;                 /* SET SUSPEND/RESUME    @Z40GSDT*/ 00054000
*                                                                  0208 00055000
         OI    PLHSR(RPLH),B'00000001'                             0208 00056000
*         /***********************************************************/ 00057000
*         /*                                                         */ 00058000
*         /* SET UP ENTRY REGS FOR SVC 121                           */ 00059000
*         /*                                                         */ 00060000
*         /***********************************************************/ 00061000
*                                                                  0209 00062000
*         RFY                                                      0209 00063000
*          (R1,                                                    0209 00064000
*           R15) RSTD;                                             0209 00065000
*         R0=RPLH;                  /* PLH                           */ 00066000
         LR    R0,RPLH                                             0210 00067000
*         R1=RIOMB;                 /* IOMB                          */ 00068000
         LR    R1,RIOMB                                            0211 00069000
*         R15=RBUFC;                /* BUFC                          */ 00070000
         LR    R15,RBUFC                                           0212 00071000
*         GEN(SVC 121);             /* TO SVC                        */ 00072000
         SVC 121                                                        00073000
*         RFY                                                      0214 00074000
*           R15 UNRSTD;                                            0214 00075000
*         RFY                                                      0215 00076000
*           R14 RSTD;                                              0215 00077000
*         R14=PLHAR14;              /* RESTORE R14                   */ 00078000
         L     R14,PLHAR14(,RPLH)                                  0216 00079000
*         RFY                                                      0217 00080000
*           R14 UNRSTD;                                            0217 00081000
*         RETURN;                   /* BACK TO IDA019S1              */ 00082000
@EL00001 DS    0H                                                  0218 00083000
@EF00001 DS    0H                                                  0218 00084000
@ER00001 BR    @14                                                 0218 00085000
*       END;                        /* END RB                        */ 00086000
*SETBLKS:                                                          0220 00087000
*                                                                  0220 00088000
*   /*****************************************************************/ 00089000
*   /*                                                               */ 00090000
*   /* UPDATE CONTROL BLOCKS - IOSB, IOMB, AND SRB                   */ 00091000
