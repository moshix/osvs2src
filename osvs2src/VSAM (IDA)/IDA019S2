*       RFY                                                        0429 00882000
*         R1 UNRSTD;                                               0429 00883000
*       LM(R0,R13,R6->IOMSAVER);    /* RESTORE REGS          @Z40TSDT*/ 00884000
         LM    R0,R13,IOMSAVER(R6)                                 0430 00885000
*     END;                                                         0431 00886000
*   RFY                                                            0432 00887000
*    (R0,                                                          0432 00888000
*     R6,                                                          0432 00889000
*     RW1) UNRSTD;                                                 0432 00890000
@RF00413 DS    0H                                                  0433 00891000
*                                                                  0433 00892000
*   /*****************************************************************/ 00893000
*   /*                                                               */ 00894000
*   /* TRANSLATE CHANNEL PROGRAMS PRIOR TO ISSUING STARTIO MACRO     */ 00895000
*   /*                                                               */ 00896000
*   /*****************************************************************/ 00897000
*                                                                  0433 00898000
*   IF BUFCMW=ON THEN               /* WRITE CP EXISTS               */ 00899000
         TM    BUFCMW(RBUFC),B'10000000'                           0433 00900000
         BNO   @RF00433                                            0433 00901000
*     DO;                                                          0434 00902000
*       RFY                                                        0435 00903000
