*       CPAWREAL=RW3;               /* REPLACE PTR WITH NEW REAL ADDR*/ 00938000
         ST    RW3,CPAWREAL(,@14)                                  0446 00939000
*       RW3=CPAWCPS;                /* PTR TO CP START               */ 00940000
         L     RW3,CPAWCPS(,@14)                                   0447 00941000
*       RESPECIFY                                                  0448 00942000
*        (CCW) BASED(RW3);                                         0448 00943000
*       IF R13^=ZERO THEN           /* PAGE HAS BEEN MOVED           */ 00944000
         LTR   R13,R13                                             0449 00945000
         BZ    @RF00449                                            0449 00946000
*         DO;                                                      0450 00947000
*WRITEL:                                                           0451 00948000
*           DO UNTIL RW3=CPAWCPE;   /* CHANGE WRITE CCWS TO REAL ADDR*/ 00949000
WRITEL   DS    0H                                                  0451 00950000
@DL00451 DS    0H                                                  0452 00951000
*             CCWDA=CCWDA+R13;      /* PUT NEW REAL ADDR IN CCW      */ 00952000
         L     @14,CCWDA-1(,RW3)                                   0452 00953000
         LA    @14,0(,@14)                                         0452 00954000
         ALR   @14,R13                                             0452 00955000
         STCM  @14,7,CCWDA(RW3)                                    0452 00956000
*             RW3=RW3+EIGHT;        /* MOVE PTR TO NEXT CCW          */ 00957000
         AL    RW3,@CF00064                                        0453 00958000
*           END WRITEL;                                            0454 00959000
@DE00451 L     @14,BUFCCPA(,RBUFC)                                 0454 00960000
         C     RW3,CPAWCPE(,@14)                                   0454 00961000
         BNE   @DL00451                                            0454 00962000
*           RW3=CPAWCKS;            /* PTR TO FIRST CHECK DA CCW     */ 00963000
         L     RW3,CPAWCKS(,@14)                                   0455 00964000
*           IF BUFCWC=ON THEN       /* WRITE CHECK EXISTS            */ 00965000
