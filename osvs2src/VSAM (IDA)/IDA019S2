*       GEN SETS(RW3) REFS(RW4)     /* CONVERT ADDRESS TO NEW REAL   */ 00927000
*           (LRA   RW3,0(RW4));                                    0443 00928000
@RC00440 DS    0H                                                  0443 00929000
         LRA   RW3,0(RW4)                                               00930000
*       BC(7,PAGEOUT);              /* ERROR LOCATING REAL ADDRESS   */ 00931000
         BC    7,PAGEOUT                                           0444 00932000
*       R13=RW3-CPAWREAL;           /* FIND OFFSET BETWEEN THE NEW 0445 00933000
*                                      REAL ADDR AND THE LAST REAL   */ 00934000
