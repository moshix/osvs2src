         BNZ   @RF00228                                            0228 00117000
*     DO;                           /* YES SET FOR SRB MODE          */ 00118000
*       SRBPTCB=RPLTCBPT;           /* TCB ADDRESS FROM RPL  @ZA20760*/ 00119000
         L     @10,IOSSRB(,IOSBPTR)                                0230 00120000
         L     @15,PLHCRPL(,RPLH)                                  0230 00121000
         L     @15,RPLTCBPT(,@15)                                  0230 00122000
         ST    @15,SRBPTCB(,@10)                                   0230 00123000
*       IOMSRBM=ON;                 /* SET SRB IND                   */ 00124000
         OI    IOMSRBM(RIOMB),B'00000010'                          0231 00125000
*       IOMPGAD=R0;                 /* SAVE RESUME ADDR      @Z40GSDT*/ 00126000
         ST    R0,IOMPGAD(,RIOMB)                                  0232 00127000
*     END;                                                         0233 00128000
*   ELSE                            /* NO, SET FOR TCB MODE          */ 00129000
*     DO;                                                          0234 00130000
