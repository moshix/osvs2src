SETBLKS  IPK                                                       0220 00098000
*   GEN(MODESET EXTKEY=ZERO);       /* SET PSW KEY TO ZERO   @Z40TSDT*/ 00099000
         MODESET EXTKEY=ZERO                                            00100000
*   IOSBPTR=IOMIOSB;                /* PTR TO IOSB IN REGISTER       */ 00101000
         L     IOSBPTR,IOMIOSB(,RIOMB)                             0222 00102000
*   RCPA=IOMCPA;                    /* CPA BASE                      */ 00103000
         L     RCPA,IOMCPA(,RIOMB)                                 0223 00104000
*   IOMCKEY=RPLH;                   /* USER KEY INTO IOMB    @Z40GSDT*/ 00105000
