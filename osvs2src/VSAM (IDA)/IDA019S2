*   IOSCKEY=RPLH;                   /* USER KEY INTO IOSB    @Z40GSDT*/ 00107000
         STC   RPLH,IOSCKEY(,IOSBPTR)                              0225 00108000
*   RPLH=IOMPLH;                    /* RESTORE PLH VALUE IN REG 2    */ 00109000
         L     RPLH,IOMPLH(,RIOMB)                                 0226 00110000
*   IOSIPIB=CZERO;                  /* ZERO THE IPIB ADDRESS @ZA14789*/ 00111000
         SLR   @10,@10                                             0227 00112000
         ST    @10,IOSIPIB(,IOSBPTR)                               0227 00113000
*   IF PSATOLD=ZERO THEN            /* SRB USER?             @Z40GSDT*/ 00114000
         L     @10,PSATOLD                                         0228 00115000
