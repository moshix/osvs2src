         STH   @00,SRBPASID(,@14)                                  0294 00393000
*   SRBASCB=ADDR(ASCB);             /* ADDR SPACE TO DISPATCH@ZA20760*/ 00394000
         ST    @15,SRBASCB(,@14)                                   0295 00395000
*BLDVPL:                                                           0296 00396000
*                                                                  0296 00397000
*   /*****************************************************************/ 00398000
*   /*                                                               */ 00399000
*   /* GENERATE VIRTUAL PAGE LIST AND VIRTUAL SUB-AREA LIST          */ 00400000
*   /*                                                               */ 00401000
*   /*****************************************************************/ 00402000
*                                                                  0296 00403000
*   RFY                                                            0296 00404000
*    (R14) RSTD;                                                   0296 00405000
BLDVPL   DS    0H                                                  0297 00406000
*   RFY                                                            0297 00407000
*    (VSL) BASED(R14);                                             0297 00408000
*   R14=IOMVSL;                     /* POINTER TO VSL                */ 00409000
*                                                                  0298 00410000
         L     R14,IOMVSL(,RIOMB)                                  0298 00411000
*   /*****************************************************************/ 00412000
*   /*                                                               */ 00413000
*   /* A VALID FUNCTION WAS SPECIFIED FOR THIS BUFFER AND THE BUFFER */ 00414000
*   /* ADDRESS IS A VIRTUAL ADDRESS BUT THE VIRTUAL PAGE LIST WAS NOT*/ 00415000
*   /* VALID SO IT NEEDS TO BE GENERATED                             */ 00416000
*   /*                                                               */ 00417000
*   /*****************************************************************/ 00418000
*                                                                  0299 00419000
*   RFY                                                            0299 00420000
*     R15 RSTD;                     /* GET WORK REGISTER             */ 00421000
*   PAGAD(ONE)=BUFCBAD;             /* BUFFER ADDR                   */ 00422000
         L     @09,BUFCCPA(,RBUFC)                                 0300 00423000
