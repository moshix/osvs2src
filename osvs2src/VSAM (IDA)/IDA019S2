*   IF BUFCMW=ON THEN               /* WRITE IS ON                   */ 00148000
         TM    BUFCMW(RBUFC),B'10000000'                           0241 00149000
         BNO   @RF00241                                            0241 00150000
*     R0=BUFCORBA;                  /* OUTPUT RBA                    */ 00151000
         L     R0,BUFCORBA(,RBUFC)                                 0242 00152000
*   ELSE                            /* INPUT RBA                     */ 00153000
*     R0=BUFCDDDD;                                                 0243 00154000
         B     @RC00241                                            0243 00155000
@RF00241 L     R0,BUFCDDDD(,RBUFC)                                 0243 00156000
*                                                                  0244 00157000
*/********************************************************************/ 00158000
*/*                                                                  */ 00159000
*/*     FUNCTION - CONVERT RBA INTO MBBCCHHR                         */ 00160000
*/*     INPUT    - R0 CONTAINS THE RBA TO BE CONVERTED               */ 00161000
*/*     OUTPUT   - CONVERTED RBA IN CONVOUT                          */ 00162000
*/*                LPMB POINTER IN RLPMB                             */ 00163000
*/*                                                                  */ 00164000
*/********************************************************************/ 00165000
*                                                                  0244 00166000
*   RFY                                                            0244 00167000
*    (R1,                                                          0244 00168000
*     R2,                                                          0244 00169000
*     R6,                                                          0244 00170000
*     RW4,                                                         0244 00171000
*     RLPMB,                                                       0244 00172000
*     R14) RESTRICTED;                                             0244 00173000
@RC00241 DS    0H                                                  0245 00174000
*   RFY                                                            0245 00175000
*    (IDAEDBHD) BASED(RLPMB);                                      0245 00176000
*   RLPMB=AMBEDB;                                                  0246 00177000
         L     @15,IOMAMB(,RIOMB)                                  0246 00178000
         L     RLPMB,AMBEDB(,@15)                                  0246 00179000
*   IOMSAVE9=EDBNO;                 /* SAVE NBR OF EDB ENTRIES       */ 00180000
         SLR   @15,@15                                             0247 00181000
         IC    @15,EDBNO(,RLPMB)                                   0247 00182000
         ST    @15,IOMSAVE9(,RIOMB)                                0247 00183000
*   IOMSAVEA=EDBLEN;                /* SAVE EDB ENTRY LENGTH         */ 00184000
         LH    @07,EDBLEN(,RLPMB)                                  0248 00185000
         ST    @07,IOMSAVEA(,RIOMB)                                0248 00186000
*   RFY                                                            0249 00187000
*    (IDAEDB) BASED(RLPMB);                                        0249 00188000
*   RLPMB=RLPMB+LENGTH(IDAEDBHD);                                  0250 00189000
*                                                                  0250 00190000
         AL    RLPMB,@CF00064                                      0250 00191000
*   /*****************************************************************/ 00192000
*   /*                                                               */ 00193000
*   /* TEST INPUT RBA AGAINST LOW AND HIGH RBA RANGE FOR EACH EDB    */ 00194000
*   /* ENTRY TO FIND CORRECT ONE                                     */ 00195000
*   /*                                                               */ 00196000
*   /*****************************************************************/ 00197000
*                                                                  0251 00198000
*   RFY                                                            0251 00199000
*    (RW1) RESTRICTED;                                             0251 00200000
*   DO R6=IOMSAVE9 TO ONE BY-1;                                    0252 00201000
         LR    R6,@15                                              0252 00202000
         B     @DE00252                                            0252 00203000
@DL00252 DS    0H                                                  0253 00204000
*     IF R0=>EDBLORBA&R0=<EDBHIRBA THEN                            0253 00205000
*                                                                  0253 00206000
         L     @15,EDBLORBA(,RLPMB)                                0253 00207000
         CLR   R0,@15                                              0253 00208000
         BL    @RF00253                                            0253 00209000
         CL    R0,EDBHIRBA(,RLPMB)                                 0253 00210000
         BH    @RF00253                                            0253 00211000
*       /*************************************************************/ 00212000
*       /*                                                           */ 00213000
*       /* EDB FOUND - NOW SET UP MBBCCHHR                           */ 00214000
*       /*                                                           */ 00215000
*       /*************************************************************/ 00216000
*                                                                  0254 00217000
*       DO;                                                        0254 00218000
*         CONVOUT(ONE:FOUR)=ZERO;   /* ZERO 1ST WD OF CONVOUT        */ 00219000
         SLR   @09,@09                                             0255 00220000
         STCM  @09,15,CONVOUT(IOSBPTR)                             0255 00221000
*         CONM=EDBM;                /* M FROM EDB                    */ 00222000
         IC    @09,EDBM(,RLPMB)                                    0256 00223000
         STC   @09,CONM(,IOSBPTR)                                  0256 00224000
*         R6=EDBSTTRK;              /* SAVE STARTING TRACK           */ 00225000
         L     R6,EDBSTTRK(,RLPMB)                                 0257 00226000
*         RW1=R0-EDBLORBA;          /* RBA RELATIVE TO EXTENT        */ 00227000
         LCR   @15,@15                                             0258 00228000
         ALR   @15,R0                                              0258 00229000
         LR    RW1,@15                                             0258 00230000
*         RFY                                                      0259 00231000
*          (IDALPMB) BASED(RLPMB);                                 0259 00232000
*         RLPMB=EDBLPMBA;                                          0260 00233000
         L     RLPMB,EDBLPMBA(,RLPMB)                              0260 00234000
*                                                                  0261 00235000
*         /***********************************************************/ 00236000
*         /*                                                         */ 00237000
*         /* OPTIMIZE CODE BY USING ASSEMBLER CODE FOR DIVISION      */ 00238000
*         /*                                                         */ 00239000
*         /***********************************************************/ 00240000
*                                                                  0261 00241000
*         RFY                                                      0261 00242000
*          (RW2,                                                   0261 00243000
*           RW3,                                                   0261 00244000
*           R15) RESTRICTED;                                       0261 00245000
*         GENERATE REFS(LPMAUSZ,LPMBPTRK,LPMBLKSZ);                0262 00246000
*        CONCC=RW1/LPMAUSZ;                                             00247000
*        RW3=RW1//LPMAUSZ;                                              00248000
         LR    RW3,RW1                  RBA RELATIVE TO EXTENT IN ODD   00249000
         SR    RW2,RW2                  ZERO EVEN REGISTER              00250000
         D     RW2,LPMAUSZ(,RLPMB)      RELATIVE CYLINDER VALUE         00251000
         STH   RW3,CONCC(IOSBPTR)       STORE RELATIVE CYLINDER VALUE   00252000
         LR    RW3,RW2                  REMAINDER INTO ODD REGISTER     00253000
*                                                                       00254000
*        CONHH=RW3/LPMBPTRK;                                            00255000
*        RW1=RW3//LPMBPTRK;                                             00256000
         SR    RW2,RW2                  ZERO EVEN REGISTER              00257000
         D     RW2,LPMBPTRK(,RLPMB)     RELATIVE TRACK VALUE            00258000
         STH   RW3,CONHH(IOSBPTR)       STORE RELATIVE TRACK VALUE      00259000
         LR    RW1,RW2                  REMAINDER INTO RW1 FOR SS RTN   00260000
*        RW3=(RW2+LPMBLKSZ-1)/LPMBLKSZ;  /* RECORD LESS 1            */ 00261000
         L     R15,LPMBLKSZ(,RLPMB)     BLOCKSIZE                       00262000
         AR    RW2,R15                  ADD BLOCKSIZE TO TRK REMAINDER  00263000
         BCTR  RW2,@00                  REDUCE BY ONE                   00264000
         LR    RW3,RW2                  REMAINDER INTO ODD REGISTER     00265000
         SLR   RW2,RW2                  ZERO EVEN REGISTER FOR DIVISION 00266000
         DR    RW2,R15                  DIVIDE BY BLOCKSIZE             00267000
*         RFY                                                      0263 00268000
*          (R15) UNRSTD;                                           0263 00269000
*         CONR=RW3+ONE;             /* RECORD STORED                 */ 00270000
         LA    @15,1                                               0264 00271000
         ALR   @15,RW3                                             0264 00272000
         STC   @15,CONR(,IOSBPTR)                                  0264 00273000
*         RFY                                                      0265 00274000
*          (RW3) UNRSTD;                                           0265 00275000
*         RW2=CONCC*LPMTRKAU+CONHH+R6;                             0266 00276000
         SLR   RW2,RW2                                             0266 00277000
         ICM   RW2,3,CONCC(IOSBPTR)                                0266 00278000
         MH    RW2,LPMTRKAU(,RLPMB)                                0266 00279000
         SLR   @15,@15                                             0266 00280000
         ICM   @15,3,CONHH(IOSBPTR)                                0266 00281000
         ALR   RW2,@15                                             0266 00282000
         ALR   RW2,R6                                              0266 00283000
*         RFY                                                      0267 00284000
*          (RW3,                                                   0267 00285000
*           R15) RESTRICTED;                                       0267 00286000
*         GENERATE REFS(LPMTPC);                                   0268 00287000
*        CONCC=RW2/LPMTPC;                                              00288000
*        CONHH=RW2//LPMTPC;                                             00289000
         LR    RW3,RW2                  LOAD ODD REGISTER               00290000
         SR    RW2,RW2                  ZERO EVEN REGISTER              00291000
         LH    R15,LPMTPC(,RLPMB)                                       00292000
         DR    RW2,R15                  DETERMINE REAL CCHH             00293000
         STH   RW3,CONCC(IOSBPTR)       REAL CYLINDER VALUE - CC        00294000
         STH   RW2,CONHH(IOSBPTR)       REAL TRACK VALUE - HH           00295000
*         RFY                                                      0269 00296000
*          (RW2,                                                   0269 00297000
*           RW3,                                                   0269 00298000
*           R15) UNRSTD;                                           0269 00299000
*                                                                  0270 00300000
*         /***********************************************************/ 00301000
*         /*                                                         */ 00302000
*         /* SET SECTOR NUMBER FROM LPMB SECTOR TABLE IF AN RPS      */ 00303000
*         /* DEVICE                                                  */ 00304000
*         /*                                                         */ 00305000
*         /***********************************************************/ 00306000
*                                                                  0270 00307000
*         IF LPMBRPS=ON THEN        /* RPS DEV?                      */ 00308000
         TM    LPMBRPS(RLPMB),B'10000000'                          0270 00309000
         BNO   @RF00270                                            0270 00310000
*           DO;                     /* YES                           */ 00311000
*             CPARRPS=ON;           /* READ RPS FLG                  */ 00312000
*             CPAWRPS=ON;           /* WRITE RPS FLG                 */ 00313000
         L     @15,BUFCCPA(,RBUFC)                                 0273 00314000
         OI    CPARRPS(@15),B'00011000'                            0273 00315000
*             CPARSECT=LPMBSST(CONR);/* SET SECTOR NO.               */ 00316000
         SLR   @09,@09                                             0274 00317000
         IC    @09,CONR(,IOSBPTR)                                  0274 00318000
         SLR   @08,@08                                             0274 00319000
         IC    @08,LPMBSST-1(@09,RLPMB)                            0274 00320000
         STC   @08,CPARSECT(,@15)                                  0274 00321000
*             CPAWSECT=LPMBSST(CONR);/* SET SECTOR NO.               */ 00322000
         STC   @08,CPAWSECT(,@15)                                  0275 00323000
*           END;                                                   0276 00324000
*         ELSE                      /* NON RPS DEV                   */ 00325000
*           DO;                                                    0277 00326000
         B     @RC00270                                            0277 00327000
@RF00270 DS    0H                                                  0278 00328000
*             CPARRPS=OFF;          /* RESET                         */ 00329000
*             CPAWRPS=OFF;          /* RESET                         */ 00330000
         L     @15,BUFCCPA(,RBUFC)                                 0279 00331000
         NI    CPARRPS(@15),B'11100111'                            0279 00332000
*           END;                                                   0280 00333000
*         GOTO SETASID;             /* EDB FOUND                     */ 00334000
         B     SETASID                                             0281 00335000
*       END;                                                       0282 00336000
*     ELSE                                                         0283 00337000
*       RLPMB=RLPMB+IOMSAVEA;       /* NEXT EDB ENTRY                */ 00338000
@RF00253 AL    RLPMB,IOMSAVEA(,RIOMB)                              0283 00339000
*   END;                                                           0284 00340000
*                                                                  0284 00341000
         BCTR  R6,0                                                0284 00342000
@DE00252 LTR   R6,R6                                               0284 00343000
         BP    @DL00252                                            0284 00344000
*   /*****************************************************************/ 00345000
*   /*                                                               */ 00346000
*   /* EDB NOT FOUND, SET ERR AND EXIT                               */ 00347000
*   /*                                                               */ 00348000
*   /*****************************************************************/ 00349000
*                                                                  0285 00350000
*   RFY                                                            0285 00351000
*    (R0,                                                          0285 00352000
*     R6,                                                          0285 00353000
*     RW1,                                                         0285 00354000
*     R14) UNRESTRICTED;                                           0285 00355000
*                                                                  0285 00356000
*   /*****************************************************************/ 00357000
*   /*                                                               */ 00358000
*   /* SET MODE TO USER'S KEY WHILE SETTING BUFC BIT                 */ 00359000
*   /*                                                               */ 00360000
*   /*****************************************************************/ 00361000
*                                                                  0286 00362000
*   SPKA(IOMCKEY);                  /* SWITCH TO USER KEY    @Z40TSDT*/ 00363000
         SLR   @15,@15                                             0286 00364000
         IC    @15,IOMCKEY(,RIOMB)                                 0286 00365000
         SPKA  0(@15)                                              0286 00366000
*   BUFCXEDB=ON;                    /* INVALID RBA                   */ 00367000
         OI    BUFCXEDB(RBUFC),B'00000100'                         0287 00368000
*   BUFCEPT=ON;                     /* COMPLETE BIT ON               */ 00369000
         OI    BUFCEPT(RBUFC),B'00000001'                          0288 00370000
*   BUFCMW=OFF;                     /* RESET WRITE IND               */ 00371000
*   BUFCRRD=OFF;                    /* READ INDICATOR OFF            */ 00372000
         NI    BUFCMW(RBUFC),B'01011111'                           0290 00373000
*   GEN(MODESET EXTKEY=ZERO);       /* RESET KEY TO ZERO     @Z40TSDT*/ 00374000
         MODESET EXTKEY=ZERO                                            00375000
*   GOTO EXIT;                      /* EXIT, ERROR                   */ 00376000
         B     EXIT                                                0292 00377000
*SETASID:                                                          0293 00378000
*                                                                  0293 00379000
*   /*****************************************************************/ 00380000
*   /*                                                               */ 00381000
*   /* STORE ASID IN IOSB                                            */ 00382000
*   /*                                                               */ 00383000
*   /*****************************************************************/ 00384000
*                                                                  0293 00385000
*   IOSASID=ASCBASID;               /* ADDR SPACE ID IN IOSB         */ 00386000
SETASID  L     @15,PSAAOLD                                         0293 00387000
         MVC   IOSASID(2,IOSBPTR),ASCBASID(@15)                    0293 00388000
*   SRBPASID=ASCBASID;              /* ADDR SPACE ID IN SRB  @ZA20760*/ 00389000
