*         IOSVST=ADDR(CPACCWSS);    /* YES                           */ 00919000
         LA    @14,CPACCWSS(,RW4)                                  0441 00920000
         ST    @14,IOSVST(,IOSBPTR)                                0441 00921000
*       ELSE                        /* NO, SKIP SET SECTOR           */ 00922000
*         IOSVST=ADDR(CPACCWSI);                                   0442 00923000
         B     @RC00440                                            0442 00924000
@RF00440 LA    @14,CPACCWSI(,RW4)                                  0442 00925000
