*   /*                                                               */ 01160000
*   /* DELETE THE FRR ESTABLISHED FOR THE FIXED PAGES                */ 01161000
*   /*                                                               */ 01162000
*   /*****************************************************************/ 01163000
*                                                                  0506 01164000
*   GEN SETS(RW1,RW2,RW3,FRRSCURR,FRRSELEN) REFS(PSACSTK,FRRS,FRRSEMP)  01165000
*       (SETFRR  D,WRKREGS=(RW2,RW3));                             0506 01166000
         SETFRR  D,WRKREGS=(RW2,RW3)                                    01167000
*   IF IOMSRBM=ON THEN              /* SRB MODE?                     */ 01168000
*                                                                  0507 01169000
         TM    IOMSRBM(RIOMB),B'00000010'                          0507 01170000
         BNO   @RF00507                                            0507 01171000
*     /***************************************************************/ 01172000
*     /*                                                             */ 01173000
*     /* SRB MODE- RETURN TO SRB DISPATCHER. S1RESUME WILL GET       */ 01174000
*     /* CONTROL UNDER AN SRB AS A RESULT OF EITHER A SCHEDULE FROM  */ 01175000
*     /* THE DIE RTN (IDA019S4) OR FROM POST STATUS VIA THE ADDRESS  */ 01176000
*     /* PLACED IN IOSPGAD BY EITHER THE CHANNEL END APP (IDA121A3)  */ 01177000
*     /* OR THE ABNORMAL END APP (IDA121A4).                         */ 01178000
*     /*                                                             */ 01179000
*     /***************************************************************/ 01180000
*                                                                  0508 01181000
*     RETURN TO CVTSRBRT;           /* SRB RETURN POINT              */ 01182000
*                                                                  0508 01183000
         L     @10,CVTPTR                                          0508 01184000
         L     @14,CVTSRBRT(,@10)                                  0508 01185000
         BR    @14                                                 0508 01186000
*   /*****************************************************************/ 01187000
*   /*                                                               */ 01188000
*   /* TASK MODE - SUSPEND THE CURRENT RB AND INITIALIZE THE IOMB FOR*/ 01189000
*   /* RESUME PROCESSING.                                    @Z40GSDT*/ 01190000
*   /*                                                               */ 01191000
*   /*****************************************************************/ 01192000
*                                                                  0509 01193000
*   RESPECIFY                                                      0509 01194000
*    (R0,                                                          0509 01195000
