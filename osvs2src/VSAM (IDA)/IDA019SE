         BNZ   @RF00312                                            0312 00107000
*         RPARM1=ADDR(DIWATV);      /* YES. POINT TO DIWA    @ZA08242*/ 00108000
         L     RPARM1,AMBIWA(,RAMB)                                0313 00109000
         LA    RPARM1,DIWATV(,RPARM1)                              0313 00110000
