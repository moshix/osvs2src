         IDASVR14                                                       00054000
*   CALL EXIT;                      /* FOR FUTURE USE        @ZA15091*/ 00055000
         BAL   @14,EXIT                                            0300 00056000
*   RFY                                                            0301 00057000
*     R14 UNRSTD;                   /* RELEASE R14           @X04SVMR*/ 00058000
*                                                                  0302 00059000
*   /*****************************************************************/ 00060000
*   /*                                                               */ 00061000
*   /* IDADRQ ROUTINE FUNCTION - DEFER PROCESSING OF A REQUEST WHEN A*/ 00062000
*   /* REQUIRED RESOURCE IS TEMPORARILY NOT AVAILABLE. INPUT- RWORK0 */ 00063000
*   /* = 0 - DIWATV IS RESOURCE, IDA019RM OR IDA019R8 IS CALLER. = 4 */ 00064000
*   /* - RPARM1 POINTS TO IOB IOBLOCK IS RESOURCE, IDA019R3 IS       */ 00065000
*   /* CALLER. = 8 - AMBRSC IS RESOURCE, IDAEOVIF IS CALLER. RWORK3  */ 00066000
*   /* IS PTR TO AMB WHOSE RESOURCE FLAG COULD NOT BE OBTAINED. = 12 */ 00067000
*   /* - AMBEOVPT POINTS TO IOB, IOBLOCK IS RESOURCE, IDAEOVIF IS    */ 00068000
*   /* CALLER. = 16 - RESOURCE TEST FIELDS ALREADY IN PLH. OUTPUT-   */ 00069000
*   /* NONE REGISTERS RAMB,RBUFC,RPARM1 ARE SAVED IN PLH AND ARE     */ 00070000
*   /* RESTORED BEFORE RETURN                                        */ 00071000
*   /*                                                               */ 00072000
*   /*****************************************************************/ 00073000
*                                                                  0302 00074000
*IDADRQ:                                                           0302 00075000
*   ENTRY;                                                         0302 00076000
@EP00302 DS    0H                                                  0303 00077000
*   RFY                                                            0303 00078000
*     RWORK0 RSTD;                                                 0303 00079000
*   RFY                                                            0304 00080000
*     RWORK3 RSTD;                  /* GET AMB REG           @XO4SVMR*/ 00081000
*   GEN(IDASVR14);                  /* SAVE R14                      */ 00082000
         IDASVR14                                                       00083000
*   PLHAMB=RAMB;                    /* SAVE AMB PTR          @X04SVMR*/ 00084000
         ST    RAMB,PLHAMB(,RPLH)                                  0306 00085000
*   PLHRBUFC=RBUFC;                 /* SAVE BUFC PTR         @X04SVMR*/ 00086000
