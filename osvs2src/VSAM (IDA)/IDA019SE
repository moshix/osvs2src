*         IF RWORK0=DEFR4 THEN      /* EOV IOBLOCK           @ZA08242*/ 00112000
         B     @RC00312                                            0314 00113000
@RF00312 CL    RWORK0,@CF00147                                     0314 00114000
         BNE   @RF00314                                            0314 00115000
*           RPARM1=ADDR(IOMLOCK);   /* IOBLOCK PTR           @ZA08242*/ 00116000
         L     RPARM1,PLHDIOB(,RPLH)                               0315 00117000
         LA    RPARM1,IOMLOCK(,RPARM1)                             0315 00118000
*         ELSE                      /*                       @ZA08242*/ 00119000
*           IF RWORK0=DEFR8 THEN    /* MULTI EOV REQ         @ZA08242*/ 00120000
         B     @RC00314                                            0316 00121000
@RF00314 CL    RWORK0,@CF00052                                     0316 00122000
         BNE   @RF00316                                            0316 00123000
*             DO;                   /*                       @ZA08242*/ 00124000
*               RFY                                                0318 00125000
*                 IDAAMB BASED(RWORK3);/* NEW AMB BASE       @ZA08242*/ 00126000
*               RPARM1=ADDR(AMBXRSC);/* YES. AMB PTR         @ZA08242*/ 00127000
         L     RPARM1,AMBEOVPT(,RWORK3)                            0319 00128000
         LA    RPARM1,AMBXRSC(,RPARM1)                             0319 00129000
*               RFY                                                0320 00130000
*                 IDAAMB BASED(RAMB);/* RESET AMB BASE       @ZA08242*/ 00131000
*             END;                  /* END CODE 8            @ZA08242*/ 00132000
*           ELSE                    /*                       @ZA08242*/ 00133000
*             IF RWORK0=DEFR12 THEN /* EOV IOBLOCK           @ZA08242*/ 00134000
         B     @RC00316                                            0322 00135000
@RF00316 CL    RWORK0,@CF00469                                     0322 00136000
         BNE   @RF00322                                            0322 00137000
*               DO;                 /*                       @ZA08242*/ 00138000
*                 RPARM1=ADDR(AMBXEVPT->IOMLOCK);/*          @ZA08242   00139000
*                                      LOCK BYTE PTR         @ZA08242*/ 00140000
