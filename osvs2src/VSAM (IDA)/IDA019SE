*         GEN(USING @PSTART,@11);   /* ESTABLISH ADDRESSING          */ 00205000
         USING @PSTART,@11                                              00206000
*       END I01;                    /* TRY RESTART.          @X04SVMR*/ 00207000
*       RWORK0=PLHARET;             /* RESTORE DFR CODE      @X04SVMR*/ 00208000
