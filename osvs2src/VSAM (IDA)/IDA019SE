*               END;                /* END OF CODE 12        @ZA08242*/ 00144000
*       RFY                                                        0326 00145000
*         RWORK3 UNRSTD;            /* DROP WORK REG         @ZA08242*/ 00146000
@RF00322 DS    0H                                                  0326 00147000
@RC00316 DS    0H                                                  0326 00148000
@RC00314 DS    0H                                                  0326 00149000
@RC00312 DS    0H                                                  0327 00150000
*       PLHDRRSC=RPARM1;            /* RSRC FLAG PTR         @ZA08242*/ 00151000
         ST    RPARM1,PLHDRRSC(,RPLH)                              0327 00152000
*       PLHDRMSK=BIT0MASK;          /* MASK FOR TEST         @ZA08242*/ 00153000
