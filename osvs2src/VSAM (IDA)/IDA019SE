*     END;                          /*                       @ZA08242*/ 00155000
*   RWORK2=AMBPH;                   /* LOAD POINTER TO PLHDR @X04SVLC*/ 00156000
@RF00310 L     RWORK2,AMBPH(,RAMB)                                 0330 00157000
