         BNO   @RF00213                                            0213 00211000
*     RWORK1=RPLPTERM;              /* YES, THE PREVIOUS REQUEST IS     00212000
*                                      TERMRPL                       */ 00213000
         LA    RWORK1,52                                           0214 00214000
*   ELSE                                                           0215 00215000
*     ;                             /* ELSE CONTINUE                 */ 00216000
*                                                                  0215 00217000
@RF00213 DS    0H                                                  0216 00218000
*   /*****************************************************************/ 00219000
*   /*                                                               */ 00220000
*   /* VALIDITY CHECK DATA SET ATTRIBUTES, RPL OPTIONS, AND          */ 00221000
*   /* PROCESSING OPTIONS.                                           */ 00222000
*   /*                                                               */ 00223000
*   /*****************************************************************/ 00224000
*                                                                  0216 00225000
*   IF RPLCNV=ON|RPLASY=ON|RPLCHAIN^=ZERO|/* INVALID RPL OPTIONS     */ 00226000
*       AMBLPATH=ON|AMBCREAT=ON|    /* INVALID PROCESSING OPTIONS    */ 00227000
*       AMBGSR=ON|AMBLSR=ON|        /* INVALID PROCESSING OPTIONS    */ 00228000
*       AMDRRDS=ON|AMDSPAN=ON       /* INVALID DATASET ATTRIBUTES    */ 00229000
*     THEN                                                         0216 00230000
         TM    RPLCNV(RRPL),B'00100000'                            0216 00231000
