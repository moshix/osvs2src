@RF00223 DS    0H                                                  0226 00280000
*           RWORK2=RPLH;            /* SAVE REGISTER 2               */ 00281000
         LR    RWORK2,RPLH                                         0226 00282000
*           RPLH=ZERO;              /* CLEAR WORK REGISTER           */ 00283000
         SLR   RPLH,RPLH                                           0227 00284000
*           GEN(IPK);               /* OBTAIN KEY                    */ 00285000
         IPK                                                            00286000
*           IF RPLH^=ZERO THEN      /* KEY 0 ?                       */ 00287000
         LTR   RPLH,RPLH                                           0229 00288000
         BZ    @RF00229                                            0229 00289000
*             DO;                   /* NO                            */ 00290000
*               RPLH=RWORK2;        /* RESTORE REGISTER 2            */ 00291000
         LR    RPLH,RWORK2                                         0231 00292000
*               RWORK1=RPLINTRM;    /* SET ERROR CODE                */ 00293000
         LA    RWORK1,48                                           0232 00294000
*             END;                  /* END OF ERROR CASE             */ 00295000
*           ELSE                                                   0234 00296000
*             DO;                   /* SUPV. STATE AND KEY 0         */ 00297000
         B     @RC00229                                            0234 00298000
@RF00229 DS    0H                                                  0235 00299000
*               RPLH=RWORK2;        /* RESTORE REG2                  */ 00300000
*                                                                  0235 00301000
         LR    RPLH,RWORK2                                         0235 00302000
*               /*****************************************************/ 00303000
*               /*                                                   */ 00304000
*               /* IF EOV WAS IN PROCESS (PLHINEOV = ON) WHEN THE    */ 00305000
*               /* REQUEST WAS TERMINATED, THE REQUEST WILL BE       */ 00306000
*               /* REJECTED AND AN ERROR CODE WILL BE RETURNED TO THE*/ 00307000
*               /* USER.                                             */ 00308000
*               /*                                                   */ 00309000
*               /*****************************************************/ 00310000
*                                                                  0236 00311000
*               IF PLHINEOV=ON THEN /* WAS EOV IN PROCESS?           */ 00312000
         TM    PLHINEOV(RPLH),B'10000000'                          0236 00313000
         BNO   @RF00236                                            0236 00314000
*                 RWORK1=RPLINTRM;  /* SET ERROR CODE                */ 00315000
*                                                                  0237 00316000
         LA    RWORK1,48                                           0237 00317000
*               /*****************************************************/ 00318000
*               /*                                                   */ 00319000
*               /* IF THE IOMB IS LOCKED BY I/O MANAGEMENT ROUTINES, */ 00320000
*               /* THE REQUEST WAS TERMINATED DURING ITS I/O         */ 00321000
*               /* OPERATION, POSSIBLE DATA SET ERROR, SET           */ 00322000
*               /* INFORMATION CODE TO WARN THE USER.                */ 00323000
*               /*                                                   */ 00324000
*               /*****************************************************/ 00325000
*                                                                  0238 00326000
*               ELSE                                               0238 00327000
*                 IF IOMLOCK^=ZERO&IOMEOVTS=OFF THEN               0238 00328000
         B     @RC00236                                            0238 00329000
@RF00236 L     @10,PLHDIOB(,RPLH)                                  0238 00330000
         CLI   IOMLOCK(@10),0                                      0238 00331000
         BE    @RF00238                                            0238 00332000
         TM    IOMEOVTS(@10),B'00100000'                           0238 00333000
         BNZ   @RF00238                                            0238 00334000
*                   DO;             /* IOMB LOCKED BY I/O MANAGER    */ 00335000
*                     RPLERRCD=RPLDSERR;/* SET INFORMATION CODE      */ 00336000
         MVI   RPLERRCD(RRPL),X'24'                                0240 00337000
*                     PLHDSERR=ON;  /* SET FDBK INDICATOR IN PLH     */ 00338000
         OI    PLHDSERR(RPLH),B'00000100'                          0241 00339000
*                     IOECB=POSTED; /* SET POST FLAG                 */ 00340000
         L     @07,IOMECBPT(,@10)                                  0242 00341000
         MVC   IOECB(4,@07),POSTED                                 0242 00342000
*                     IOMLOCK=OFF;  /* RELEASE IOMB                  */ 00343000
         MVI   IOMLOCK(@10),B'00000000'                            0243 00344000
*                     PLHTMIOB=ON;  /* SET PROCESS BIT IN PLH        */ 00345000
         OI    PLHTMIOB(RPLH),B'00000100'                          0244 00346000
*                   END;            /* END OF IOMB LOCKED CASE       */ 00347000
*             END;                  /* END OF KEY 0 CASE             */ 00348000
@RF00238 DS    0H                                                  0246 00349000
@RC00236 DS    0H                                                  0247 00350000
*         END;                      /* END OF SUPV. STATE CASE       */ 00351000
@RC00229 DS    0H                                                  0248 00352000
*     END;                          /* END OF VALIDITY CHECK         */ 00353000
@RC00223 DS    0H                                                  0249 00354000
*   RFY                                                            0249 00355000
*    (RWORK2,                                                      0249 00356000
*     R15) UNRSTD;                                                 0249 00357000
@RC00216 DS    0H                                                  0250 00358000
*   IF RWORK1^=ZERO THEN            /* ANY ERROR ?                   */ 00359000
         LTR   RWORK1,RWORK1                                       0250 00360000
         BZ    @RF00250                                            0250 00361000
*     DO;                           /* YES                           */ 00362000
*       RPLERREG='08'X;             /* SET LOGICAL ERROR             */ 00363000
         MVI   RPLERREG(RRPL),X'08'                                0252 00364000
*       RPLERRCD=RWORK1;            /* SET FEEDBACK CODE             */ 00365000
         STC   RWORK1,RPLERRCD(,RRPL)                              0253 00366000
*     END;                          /* END OF ERROR CASE             */ 00367000
*   ELSE                                                           0255 00368000
*     ;                             /* NO ERROR                      */ 00369000
@RF00250 DS    0H                                                  0256 00370000
*   RFY                                                            0256 00371000
*     RWORK1 UNRSTD;                                               0256 00372000
*   RETURN;                         /* RETURN TO MAIN ROUTINE        */ 00373000
@EL00002 DS    0H                                                  0257 00374000
@EF00002 DS    0H                                                  0257 00375000
@ER00002 BR    @14                                                 0257 00376000
*   END VALCHK;                     /* END OF VALCHK PROC.           */ 00377000
*                                                                  0259 00378000
*/* ******************************************************************  00379000
*  * RELINX: ROUTINE USED TO RELEASE THE INDEX BUFC(S) THAT IS OWNED *  00380000
*  *         BY THE SPECIFIED PLH.                                   *  00381000
*  *******************************************************************/ 00382000
*                                                                  0259 00383000
*RELINX:                                                           0259 00384000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0259 00385000
RELINX   DS    0H                                                  0260 00386000
*   GEN(IDASVR14);                  /* SAVE RETURN ADDRESS           */ 00387000
         IDASVR14                                                       00388000
*   RFY                                                            0261 00389000
*     R14 UNRSTD;                                                  0261 00390000
*                                                                  0261 00391000
*   /*****************************************************************/ 00392000
*   /*                                                               */ 00393000
*   /* CHECK INDEX BUFC CHAIN AND RELEASE ALL OWNED INDEX BUFC'S     */ 00394000
*   /*                                                               */ 00395000
*   /*****************************************************************/ 00396000
*                                                                  0262 00397000
*   RBUFC=BUFDBUFC;                 /* LOAD FIRST INDEX BUFC         */ 00398000
         L     @10,AMBBUFC(,RAMB)                                  0262 00399000
         L     RBUFC,BUFDBUFC(,@10)                                0262 00400000
*   DO UNTIL RBUFC=ZERO;            /* LOOP THROUGH BUFC CHAIN       */ 00401000
@DL00263 DS    0H                                                  0264 00402000
*     IF BUFCPLH=RPLH THEN          /* DOES PLH OWN BUFC?            */ 00403000
         C     RPLH,BUFCPLH(,RBUFC)                                0264 00404000
         BNE   @RF00264                                            0264 00405000
*       DO;                         /* YES                           */ 00406000
*         IF BUFCMW=ON THEN         /* MUST WRITE BIT ON?            */ 00407000
         TM    BUFCMW(RBUFC),B'10000000'                           0266 00408000
         BNO   @RF00266                                            0266 00409000
*           DO;                     /* YES                           */ 00410000
*             RPLERRCD=RPLDSERR;    /* SET FEEDBACK CODE             */ 00411000
         MVI   RPLERRCD(RRPL),X'24'                                0268 00412000
*             PLHDSERR=ON;          /* SET FDBK INDICATOR IN PLH     */ 00413000
         OI    PLHDSERR(RPLH),B'00000100'                          0269 00414000
*             BUFCMW=OFF;           /* SET MUST WRITE BIT OFF        */ 00415000
         NI    BUFCMW(RBUFC),B'01111111'                           0270 00416000
*           END;                    /* END OF MUST WRITE CASE        */ 00417000
*         ELSE                                                     0272 00418000
*           ;                       /* ELSE NO MUST WRITE DATA       */ 00419000
@RF00266 DS    0H                                                  0273 00420000
*         BUFCVAL=OFF;              /* INVALIDATE BUFC               */ 00421000
         NI    BUFCVAL(RBUFC),B'11111011'                          0273 00422000
*I02:                                                              0274 00423000
*         DO;                       /* IDACALL(IDAFREEB)             */ 00424000
I02      DS    0H                                                  0275 00425000
*           CALL IDAFREEB;          /* EXIT TO IDAFREEB              */ 00426000
         L     @15,@CV00991                                        0275 00427000
         BALR  @14,@15                                             0275 00428000
*           GEN(BALR @11,0);        /* LOAD ADDR OF REENTRY          */ 00429000
         BALR @11,0                                                     00430000
*           GEN(USING *,@11);       /* ESTABLISH ADDRESSING          */ 00431000
         USING *,@11                                                    00432000
*           GEN(L     @11,=A(@PSTART));/* RELOAD BASE                */ 00433000
         L     @11,=A(@PSTART)                                          00434000
*           GEN(USING @PSTART,@11); /* ESTABLISH ADDRESSING          */ 00435000
         USING @PSTART,@11                                              00436000
*         END I02;                  /* FREE BUFC                     */ 00437000
*         PLHTMINX=ON;              /* TURN PROCESS BIT ON           */ 00438000
         OI    PLHTMINX(RPLH),B'00001000'                          0281 00439000
*       END;                        /* END OF OWNED CASE             */ 00440000
*     ELSE                                                         0283 00441000
*       ;                           /* ELSE NOT OWNED BY PLH         */ 00442000
@RF00264 DS    0H                                                  0284 00443000
*     RBUFC=BUFCHAIN;               /* GET NEXT BUFC IN CHAIN        */ 00444000
         L     RBUFC,BUFCHAIN(,RBUFC)                              0284 00445000
*   END;                            /* END OF DO-UNTIL               */ 00446000
@DE00263 LTR   RBUFC,RBUFC                                         0285 00447000
         BNZ   @DL00263                                            0285 00448000
*   GEN REFS(HWORD4)(IDARST14);     /* RESTORE RETURN ADDRESS        */ 00449000
         IDARST14                                                       00450000
*   RETURN;                         /* RETURN TO MAIN ROUTINE        */ 00451000
@EL00003 DS    0H                                                  0287 00452000
@EF00003 DS    0H                                                  0287 00453000
@ER00003 BR    @14                                                 0287 00454000
*   END RELINX;                     /* END OF RELINX PROC.           */ 00455000
*                                                                  0289 00456000
*/********************************************************************/ 00457000
*/*  RELDIWA: ROUTINE USED TO RELEASE THE DIWA IF OWNED.             */ 00458000
*/*                                                                  */ 00459000
*/*  FUNCTION: CHECK IF THIS REQUEST OWNS THE DIWA, FREE THE INSERT  */ 00460000
*/*            BUFFER BEFORE RELEASING THE DIWA.  THE INSERT BUFFER  */ 00461000
*/*            CAN BE RELEASED IN ONE OF TWO WAYS:                   */ 00462000
*/*            (1) IF THE INSERT BUFFER WAS SWAPPED WITH THE DATA    */ 00463000
*/*                BUFC BY THE CI SPLIT ROUTINE, IT HAS TO BE        */ 00464000
*/*                SWAPPED BACK AND FREED BY IDAFREEB.               */ 00465000
*/*            (2) IF THE INSERT BUFFER WAS ADDED TO THE DATA BUFFER */ 00466000
*/*                CHAIN, IT HAS TO BE SUBTRACTED FROM THE CHAIN BY  */ 00467000
*/*                IDASBF.                                           */ 00468000
*/********************************************************************/ 00469000
*                                                                  0289 00470000
*RELDIWA:                                                          0289 00471000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0289 00472000
RELDIWA  DS    0H                                                  0290 00473000
*   GEN(IDASVR14);                  /* SAVE RETURN ADDRESS           */ 00474000
         IDASVR14                                                       00475000
*   IF DIWATV^=ZERO THEN            /* IS DIWA BUSY?                 */ 00476000
         L     @10,AMBIWA(,RAMB)                                   0291 00477000
         CLI   DIWATV(@10),0                                       0291 00478000
         BE    @RF00291                                            0291 00479000
*     IF DIWPLH=RPLH THEN           /* DOES PLH OWN DIWA ?           */ 00480000
         C     RPLH,DIWPLH(,@10)                                   0292 00481000
         BNE   @RF00292                                            0292 00482000
*       DO;                         /* YES                           */ 00483000
*                                                                  0293 00484000
*         /***********************************************************/ 00485000
*         /*                                                         */ 00486000
*         /* CHECK IF A SWAP WAS DONE BETWEEN THE INSERT BUFFER      */ 00487000
*         /* POINTER (DIWBUFC) AND THE DATA BUFC POINTER (PLHDBUFC)  */ 00488000
*         /*                                                         */ 00489000
*         /***********************************************************/ 00490000
*                                                                  0294 00491000
*         IF DIWBUFC^=BUFDBUFC THEN /* DOES DIWBUFC PT TO INSERT BUF?*/ 00492000
         L     @05,DIWBUFC(,@10)                                   0294 00493000
         L     @15,AMBBUFC(,RAMB)                                  0294 00494000
         L     @15,BUFDBUFC(,@15)                                  0294 00495000
         CR    @05,@15                                             0294 00496000
         BE    @RF00294                                            0294 00497000
*           IF PLHDBUFC=BUFDBUFC THEN/* PLH PTS TO INSERT BUFC?      */ 00498000
         C     @15,PLHDBUFC(,RPLH)                                 0295 00499000
         BNE   @RF00295                                            0295 00500000
*             DO;                   /* YES, SWAP                     */ 00501000
*               PLHDBUFC=DIWBUFC;   /* RESTORE DATA BUFC PTR         */ 00502000
         ST    @05,PLHDBUFC(,RPLH)                                 0297 00503000
*               DIWBUFC=BUFDBUFC;   /* RESTORE INSERT BUFC PTR       */ 00504000
         ST    @15,DIWBUFC(,@10)                                   0298 00505000
*             END;                  /* END OF SWAP CASE              */ 00506000
*           ELSE                    /* ELSE DATA BUFC PTR INCORRECT  */ 00507000
*             DO;                   /* SEARCH DATA BUFC CHAIN        */ 00508000
         B     @RC00295                                            0300 00509000
@RF00295 DS    0H                                                  0301 00510000
*               DIWBUFC=BUFDBUFC;   /* RESET INSERT BUF. PTR         */ 00511000
         L     @10,AMBIWA(,RAMB)                                   0301 00512000
         L     @05,AMBBUFC(,RAMB)                                  0301 00513000
         L     @05,BUFDBUFC(,@05)                                  0301 00514000
         ST    @05,DIWBUFC(,@10)                                   0301 00515000
*               CALL FINDBUF;       /* CALL FINDBUFC ROUTINE         */ 00516000
         BAL   @14,FINDBUF                                         0302 00517000
*             END;                  /* END OF ERROR CASE             */ 00518000
*         ELSE                      /* DIWBUFC PTS TO INSERT BUF     */ 00519000
*           IF PLHDBUFC=DIWBUFC THEN/* PLHDBUFC PTS TO INSERT BUF?   */ 00520000
         B     @RC00294                                            0304 00521000
@RF00294 L     @10,AMBIWA(,RAMB)                                   0304 00522000
         CLC   PLHDBUFC(4,RPLH),DIWBUFC(@10)                       0304 00523000
         BNE   @RF00304                                            0304 00524000
*             CALL FINDBUF;         /* YES, SEARCH FOR DATA BUF PTR  */ 00525000
         BAL   @14,FINDBUF                                         0305 00526000
*           ELSE                                                   0306 00527000
*             ;                     /* ELSE PLHDBUFC IS GOOD         */ 00528000
@RF00304 DS    0H                                                  0307 00529000
*         RBUFC=DIWBUFC;            /* LOAD INSERT BUFC ADDR.        */ 00530000
@RC00294 L     @10,AMBIWA(,RAMB)                                   0307 00531000
         L     RBUFC,DIWBUFC(,@10)                                 0307 00532000
*         IF BUFCMW=ON THEN         /* IS MUST WRITE BIT ON?         */ 00533000
         TM    BUFCMW(RBUFC),B'10000000'                           0308 00534000
         BNO   @RF00308                                            0308 00535000
*           DO;                     /* YES                           */ 00536000
*             RPLERRCD=RPLDSERR;    /* SET INFORMATION CODE          */ 00537000
         MVI   RPLERRCD(RRPL),X'24'                                0310 00538000
*             PLHDSERR=ON;          /* SET FDBK INDICATOR IN PLH     */ 00539000
         OI    PLHDSERR(RPLH),B'00000100'                          0311 00540000
*             BUFCMW=OFF;           /* RESET MUST WRITE BIT          */ 00541000
         NI    BUFCMW(RBUFC),B'01111111'                           0312 00542000
*           END;                    /* END OF MUST WRITE CASE        */ 00543000
*         ELSE                                                     0314 00544000
*           ;                       /* ELSE MUST WRITE BIT NOT ON    */ 00545000
*                                                                  0314 00546000
@RF00308 DS    0H                                                  0315 00547000
*         /***********************************************************/ 00548000
*         /*                                                         */ 00549000
*         /* IF THE INSERT BUFFER IS NOT IN THE DATA BUFFER CHAIN,   */ 00550000
*         /* CALL IDAFREEB TO FREE THE BUFFER                        */ 00551000
*         /*                                                         */ 00552000
*         /***********************************************************/ 00553000
*                                                                  0315 00554000
*         IF RBUFC=BUFCNXT2 THEN    /* NOT IN DATA BUFFER CHAIN?     */ 00555000
         L     @10,BUFCNXT2-1(,RBUFC)                              0315 00556000
         LA    @10,0(,@10)                                         0315 00557000
         CR    RBUFC,@10                                           0315 00558000
         BNE   @RF00315                                            0315 00559000
*I03:                                                              0316 00560000
*           DO;                     /* IDACALL(IDAFREEB)             */ 00561000
I03      DS    0H                                                  0317 00562000
*             CALL IDAFREEB;        /* EXIT TO IDAFREEB              */ 00563000
         L     @15,@CV00991                                        0317 00564000
         BALR  @14,@15                                             0317 00565000
*             GEN(BALR @11,0);      /* LOAD ADDR OF REENTRY          */ 00566000
         BALR @11,0                                                     00567000
*             GEN(USING *,@11);     /* ESTABLISH ADDRESSING          */ 00568000
         USING *,@11                                                    00569000
*             GEN(L     @11,=A(@PSTART));/* RELOAD BASE              */ 00570000
         L     @11,=A(@PSTART)                                          00571000
*             GEN(USING @PSTART,@11);/* ESTABLISH ADDRESSING         */ 00572000
         USING @PSTART,@11                                              00573000
*           END I03;                /* FREE INSERT BUFFER            */ 00574000
*                                                                  0322 00575000
*         /***********************************************************/ 00576000
*         /*                                                         */ 00577000
*         /* IF THE INSERT BUFFER IS IN THE DATA BUFFER CHAIN, CALL  */ 00578000
*         /* IDASBF TO SUBTRACT THE INSERT BUFFER FROM THE CHAIN.    */ 00579000
*         /*                                                         */ 00580000
*         /***********************************************************/ 00581000
*                                                                  0323 00582000
*         ELSE                                                     0323 00583000
*I04:                                                              0323 00584000
*           DO;                     /* IDACALL(IDASBF)               */ 00585000
         B     @RC00315                                            0323 00586000
@RF00315 DS    0H                                                  0323 00587000
I04      DS    0H                                                  0324 00588000
*             CALL IDASBF;          /* EXIT TO IDASBF                */ 00589000
         L     @15,@CV00990                                        0324 00590000
         BALR  @14,@15                                             0324 00591000
*             GEN(BALR @11,0);      /* LOAD ADDR OF REENTRY          */ 00592000
         BALR @11,0                                                     00593000
*             GEN(USING *,@11);     /* ESTABLISH ADDRESSING          */ 00594000
         USING *,@11                                                    00595000
*             GEN(L     @11,=A(@PSTART));/* RELOAD BASE              */ 00596000
         L     @11,=A(@PSTART)                                          00597000
*             GEN(USING @PSTART,@11);/* ESTABLISH ADDRESSING         */ 00598000
         USING @PSTART,@11                                              00599000
*           END I04;                /* SUBTRACT INSERT BUFFER IF IN     00600000
*                                      DATA BUFFER CHAIN             */ 00601000
*         DIWATV=ZERO;              /* RELEASE DIWA                  */ 00602000
@RC00315 L     @10,AMBIWA(,RAMB)                                   0330 00603000
         MVI   DIWATV(@10),X'00'                                   0330 00604000
*         PLHTMDIW=ON;              /* SET PROCESS BIT IN PLH        */ 00605000
         OI    PLHTMDIW(RPLH),B'00100000'                          0331 00606000
*       END;                        /* END OF DIWA CASE              */ 00607000
*     ELSE                                                         0333 00608000
*       ;                           /* ELSE PLH DOES NOT OWN DIWA    */ 00609000
@RF00292 DS    0H                                                  0334 00610000
*   ELSE                                                           0334 00611000
*     ;                             /* ELSE DIWA NOT BUSY            */ 00612000
@RF00291 DS    0H                                                  0335 00613000
*   GEN REFS(HWORD4)(IDARST14);     /* RESTORE RETURN ADDRESS        */ 00614000
         IDARST14                                                       00615000
*   RETURN;                         /* RETURN TO MAIN ROUTINE        */ 00616000
@EL00004 DS    0H                                                  0336 00617000
@EF00004 DS    0H                                                  0336 00618000
@ER00004 BR    @14                                                 0336 00619000
*   END RELDIWA;                    /* END OF RELDIWA PROC.          */ 00620000
*                                                                  0338 00621000
*   /*****************************************************************/ 00622000
*   /*                                                               */ 00623000
*   /* FINDBUF: ROUTINE USED TO SEARCH THROUGH DATA BUFC CHAIN TO    */ 00624000
*   /* FIND A DATA BUFC THAT BELONGS TO THIS PLH                     */ 00625000
*   /*                                                               */ 00626000
*   /*****************************************************************/ 00627000
*                                                                  0338 00628000
*FINDBUF:                                                          0338 00629000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0338 00630000
FINDBUF  DS    0H                                                  0339 00631000
*   RFY                                                            0339 00632000
*     R14 RSTD;                                                    0339 00633000
*   RBUFC=BUFDBUFC->BUFCHAIN;       /* GET 1ST DATA BUFC             */ 00634000
         L     @15,AMBBUFC(,RAMB)                                  0340 00635000
         L     @15,BUFDBUFC(,@15)                                  0340 00636000
         L     RBUFC,BUFCHAIN(,@15)                                0340 00637000
*   DO UNTIL RBUFC=ZERO;            /* SEARCH DATA BUF CHAIN         */ 00638000
@DL00341 DS    0H                                                  0342 00639000
*     IF BUFCAVL^=CZERO&BUFCPLH=RPLH THEN                          0342 00640000
         CLI   BUFCAVL(RBUFC),B'00000000'                          0342 00641000
         BE    @RF00342                                            0342 00642000
         C     RPLH,BUFCPLH(,RBUFC)                                0342 00643000
         BNE   @RF00342                                            0342 00644000
*       DO;                         /* DOES BUFC BELONG TO THIS PLH? */ 00645000
*         PLHDBUFC=RBUFC;           /* SET DATA BUFC PTR             */ 00646000
         ST    RBUFC,PLHDBUFC(,RPLH)                               0344 00647000
*         RBUFC=ZERO;               /* SET LOOP SWITCH               */ 00648000
         SLR   RBUFC,RBUFC                                         0345 00649000
*       END;                        /* END OF FOUND DATA BUFC CASE   */ 00650000
*     ELSE                          /* ELSE BUFC DOES NOT BELONG TO     00651000
*                                      PLH                           */ 00652000
*       DO;                         /* GET NEXT BUFC IN CHAIN        */ 00653000
         B     @RC00342                                            0347 00654000
@RF00342 DS    0H                                                  0348 00655000
*         RBUFC=BUFCHAIN;           /* GET NEXT PTR                  */ 00656000
         L     RBUFC,BUFCHAIN(,RBUFC)                              0348 00657000
*         IF RBUFC=ZERO THEN        /* REACH END OF CHAIN ?          */ 00658000
         LTR   RBUFC,RBUFC                                         0349 00659000
         BNZ   @RF00349                                            0349 00660000
*           DO;                     /* YES, ERROR IN PLH DATA BUF PTR*/ 00661000
*             RPLERRCD=RPLDBPER;    /* SET INFO. CODE                */ 00662000
         MVI   RPLERRCD(RRPL),X'28'                                0351 00663000
*             PLHDBPER=ON;          /* SET INDICATOR IN PLH          */ 00664000
         OI    PLHDBPER(RPLH),B'00000010'                          0352 00665000
*           END;                    /* END OF DATA BUFC PTR ERROR  0353 00666000
*                                      CASE                          */ 00667000
*         ELSE                                                     0354 00668000
*           ;                       /* ELSE, TRY NEXT BUFC           */ 00669000
@RF00349 DS    0H                                                  0355 00670000
*       END;                        /* END OF ELSE                   */ 00671000
*   END;                            /* END OF DO-UNTIL LOOP          */ 00672000
@RC00342 DS    0H                                                  0356 00673000
@DE00341 LTR   RBUFC,RBUFC                                         0356 00674000
         BNZ   @DL00341                                            0356 00675000
*   RETURN;                         /* RETURN TO CALLER              */ 00676000
@EL00005 DS    0H                                                  0357 00677000
@EF00005 DS    0H                                                  0357 00678000
@ER00005 BR    @14                                                 0357 00679000
*   RFY                                                            0358 00680000
*     R14 UNRSTD;                                                  0358 00681000
*   END FINDBUF;                    /* END OF FINDBUF PROC           */ 00682000
*                                                                  0360 00683000
*/********************************************************************  00684000
** RELDTA:  ROUTINE USED TO RELEASE THE EXCESS DATA BUFC'S OF THIS   *  00685000
**          PLH.                                                     *  00686000
** FUNCTION: CHECK IF THERE IS AN ERROR IN THE DATA BUFC POINTER.    *  00687000
**           RELEASE DATA BUFC'S FOR REUSE BY CALLING IDASBF. BEFORE *  00688000
**           CALLING IDASBF, GO THROUGH BUFFER CHAIN AND TURN ALL THE*  00689000
**           BUFCMW BITS OFF TO AVOID FORCE WRITE IN IDASBF.         *  00690000
******************************************************************** */ 00691000
*                                                                  0360 00692000
*RELDTA:                                                           0360 00693000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0360 00694000
RELDTA   DS    0H                                                  0361 00695000
*   GEN(IDASVR14);                  /* SAVE RETURN ADDRESS           */ 00696000
         IDASVR14                                                       00697000
*   IF PLHDBPER=OFF THEN            /* ANY ERROR IN DATA BUFC PTR?   */ 00698000
         TM    PLHDBPER(RPLH),B'00000010'                          0362 00699000
         BNZ   @RF00362                                            0362 00700000
*     DO;                           /* NO, RELEASE DATA BUFFERS      */ 00701000
*       RBUFC=PLHDBUFC;             /* SET UP WORK REGISTER          */ 00702000
         L     RBUFC,PLHDBUFC(,RPLH)                               0364 00703000
*       DO UNTIL RBUFC=PLHDBUFC;    /* GO THROUGH BUF CHAIN          */ 00704000
@DL00365 DS    0H                                                  0366 00705000
*         IF BUFCMW=ON THEN         /* TEST FOR BUFCMW               */ 00706000
