         OI    PLHTMDTA(RPLH),B'00010000'                          0383 00753000
*     END;                          /* END OF DATA BUFC RELEASE      */ 00754000
*   ELSE                                                           0385 00755000
*     ;                             /* ELSE ERROR IN DATA BUFC PTR   */ 00756000
@RF00362 DS    0H                                                  0386 00757000
*   GEN REFS(HWORD4)(IDARST14);     /* RESTORE RETURN ADDRESS        */ 00758000
         IDARST14                                                       00759000
