*     DO;                           /* ELSE VALID REQUEST            */ 00253000
*                                                                  0218 00254000
         B     @RC00216                                            0218 00255000
@RF00216 DS    0H                                                  0219 00256000
*       /*************************************************************/ 00257000
*       /*                                                           */ 00258000
*       /* CHECK IF USER IS IN SUPERVISOR STATE AND KEY 0            */ 00259000
*       /*                                                           */ 00260000
*       /*************************************************************/ 00261000
*                                                                  0219 00262000
*       RFY                                                        0219 00263000
*        (RWORK2,                                                  0219 00264000
*         R15) RSTD;                                               0219 00265000
*       RWORK2=RRPL;                /* SAVE RPL ADDRESS              */ 00266000
         LR    RWORK2,RRPL                                         0220 00267000
*       GEN(TESTAUTH STATE=YES,RBLEVEL=1);/* TEST FOR SUPV. STATE    */ 00268000
         TESTAUTH STATE=YES,RBLEVEL=1                                   00269000
*       RRPL=RWORK2;                /* RESTORE RPL ADDRESS           */ 00270000
         LR    RRPL,RWORK2                                         0222 00271000
*       IF R15^=ZERO THEN           /* NOT SUPV. STATE               */ 00272000
         LTR   R15,R15                                             0223 00273000
         BZ    @RF00223                                            0223 00274000
*         RWORK1=RPLINTRM;          /* SET ERROR CODE                */ 00275000
         LA    RWORK1,48                                           0224 00276000
*       ELSE                                                       0225 00277000
*         DO;                       /* ELSE TEST FOR KEY 0           */ 00278000
