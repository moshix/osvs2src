*       GEN(IDASVR14);              /* SAVE RETURN ADDRESS IN PLH    */ 00047000
*                                                                  0165 00048000
         IDASVR14                                                       00049000
*       /*************************************************************/ 00050000
*       /*                                                           */ 00051000
*       /* RELEASE BUFC HEADER IF OWNED                              */ 00052000
*       /*                                                           */ 00053000
*       /*************************************************************/ 00054000
*                                                                  0166 00055000
*       IF PLHBUFHD=ON THEN         /* DOES PLH OWN THE BUFC HDR?    */ 00056000
