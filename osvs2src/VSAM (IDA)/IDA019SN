         BNO   @RF00366                                            0366 00708000
*           DO;                     /* YES                           */ 00709000
*             BUFCMW=OFF;           /* SET BIT OFF                   */ 00710000
         NI    BUFCMW(RBUFC),B'01111111'                           0368 00711000
*             RPLERRCD=RPLDSERR;    /* SET FEEDBACK CODE             */ 00712000
         MVI   RPLERRCD(RRPL),X'24'                                0369 00713000
*             PLHDSERR=ON;          /* SET FDBK INDICATOR IN PLH     */ 00714000
         OI    PLHDSERR(RPLH),B'00000100'                          0370 00715000
*           END;                    /* END OF MUST WRITE             */ 00716000
*         ELSE                                                     0372 00717000
*           ;                                                      0372 00718000
@RF00366 DS    0H                                                  0373 00719000
*         RBUFC=BUFCNXT2;           /* NEXT BUFC                     */ 00720000
         L     @10,BUFCNXT2-1(,RBUFC)                              0373 00721000
         LA    @10,0(,@10)                                         0373 00722000
         LR    RBUFC,@10                                           0373 00723000
*       END;                        /* END OF DO-UNTIL LOOP          */ 00724000
*                                                                  0374 00725000
@DE00365 C     RBUFC,PLHDBUFC(,RPLH)                               0374 00726000
         BNE   @DL00365                                            0374 00727000
*       /*************************************************************/ 00728000
*       /*                                                           */ 00729000
*       /* SET UP PARAMETER TO CALL IDASBF TO SUBTRACT EXCESS DATA   */ 00730000
*       /* BUFC'S                                                    */ 00731000
*       /*                                                           */ 00732000
*       /*************************************************************/ 00733000
*                                                                  0375 00734000
*       RBUFC=ZERO;                 /* SET PARAMETER FOR IDASBF      */ 00735000
         SLR   RBUFC,RBUFC                                         0375 00736000
*I05:                                                              0376 00737000
*       DO;                         /* IDACALL(IDASBF)               */ 00738000
