                        '                                               00002000
IDA019SN CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA019SN  78.188'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @11,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@11                                         0001 00012000
*                                                                  0157 00013000
*   /*****************************************************************/ 00014000
*   /*                                                               */ 00015000
*   /* TERMRPL MAIN ROUTINE                                          */ 00016000
*   /*                                                               */ 00017000
*   /*****************************************************************/ 00018000
*                                                                  0157 00019000
*   RPLFDBWD=ZERO;                  /* CLEAR RPL FEEDBACK WORD       */ 00020000
         SLR   @15,@15                                             0157 00021000
         ST    @15,RPLFDBWD(,RRPL)                                 0157 00022000
*   RBUFC=R14;                      /* SAVE RETURN ADDRESS           */ 00023000
