         LA    @05,0(,@05)                                         0516 00609000
         ST    @05,RPLDACB(,@14)                                   0516 00610000
*           DXUACBAD->ACBDEB->DEBDCBB=DXUACBAD;/* POINT DDEB TO USER    00611000
*                                      ACB.                          */ 00612000
         L     @14,ACBDEB-1(,@05)                                  0517 00613000
         STCM  @05,7,DEBDCBB(@14)                                  0517 00614000
*           DXUACBAD->ACBAMBL->AMBLACB=DXUACBAD;/* POINT AMBL TO USER   00615000
*                                      ACB                           */ 00616000
         L     @14,ACBAMBL(,@05)                                   0518 00617000
         ST    @05,AMBLACB(,@14)                                   0518 00618000
*         END;                      /* END OF COPY ACB PROCESSING. 0519 00619000
*                                                            @Y30DSMD*/ 00620000
*       CALL WRBUFFER;              /* CALL ROUTINE TO ISSUE WRTBFR  */ 00621000
@RF00514 BAL   @14,WRBUFFER                                        0520 00622000
*       IF RETCODE=EIGHT&RPLFDBK=RPLNOPLH THEN/* IF PLH NOT AVAILABLE*/ 00623000
         C     RETCODE,@CF00037                                    0521 00624000
         BNE   @RF00521                                            0521 00625000
         L     @14,MWARPLPT                                        0521 00626000
         CLC   RPLFDBK(3,@14),@CF00214+1                           0521 00627000
         BNE   @RF00521                                            0521 00628000
*         DO;                       /* GET ONE FROM IDA0192Y @Y30DSMD*/ 00629000
*           MWASAVED=RCORE;         /* SAVE POINTER TO ACB WORKAREA  */ 00630000
         LR    MWASAVED,RCORE                                      0523 00631000
*           MWASAVEE=RACB;          /* SAVE THE ACB POINTER. @Y30DSMD*/ 00632000
         LR    MWASAVEE,RACB                                       0524 00633000
*           RCORE=ADDR(FORCORE);    /* GET POINTER TO COMMON WORKAREA*/ 00634000
         L     RCORE,CLWCOMWK(,RCORE)                              0525 00635000
*           BLPFLAGS=ZERO;          /* CLEAR THE PARAMETER LIST FLAGS   00636000
*                                                            @Z30SSRL*/ 00637000
         LA    @14,MWABLPRM                                        0526 00638000
         SLR   @06,@06                                             0526 00639000
         STH   @06,BLPFLAGS(,@14)                                  0526 00640000
*           IF AMBLGSR=ON THEN      /* IS THIS A GSR DATA SET?     0527 00641000
*                                                            @Y30DSMD*/ 00642000
         L     @06,ACBAMBL(,RACB)                                  0527 00643000
         TM    AMBLGSR(@06),B'01000000'                            0527 00644000
         BNO   @RF00527                                            0527 00645000
*             BLPFGSR=ON;           /* IF SO SET P/L SWITCH. @Y30DSMD*/ 00646000
         OI    BLPFGSR(@14),B'00001000'                            0528 00647000
*           ELSE                    /* IF NOT THEN IT IS LSR.      0529 00648000
*                                                            @Y30DSMD*/ 00649000
*             BLPFLSR=ON;           /* INDICATE LSR REQUEST  @Y30DSMD*/ 00650000
         B     @RC00527                                            0529 00651000
@RF00527 LA    @14,MWABLPRM                                        0529 00652000
         OI    BLPFLSR(@14),B'00010000'                            0529 00653000
*           BLPFBLD=ON;             /* INDICATE BUILD REQUEST      0530 00654000
*                                                            @Y30DSMD*/ 00655000
@RC00527 LA    @14,MWABLPRM                                        0530 00656000
         OI    BLPFBLD(@14),B'01000000'                            0530 00657000
*           BLPFIOSR=ON;            /* INDICATE I/O SUPPORT REQUEST  */ 00658000
         OI    BLPFIOSR(@14),B'00000010'                           0531 00659000
*           RACB=MWARPLPT;          /* ADDRESS OF PSUEDO PARM LIST   */ 00660000
         L     RACB,MWARPLPT                                       0532 00661000
*           CALL IDA0192Y;          /* CALL BLDVRP ROUTINE   @Y30DSMD*/ 00662000
         L     @15,@CV04985                                        0533 00663000
         BALR  @14,@15                                             0533 00664000
*           RCORE->RSTATRTN=IDAR200B;/* NOTE 200B IN CONTROL @Y30DSMD*/ 00665000
