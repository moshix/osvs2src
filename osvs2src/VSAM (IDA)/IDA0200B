         LR    MWALCKC,R1                                          0970 02049000
*   R1=R13;                         /* SWITCH REGISTER 13 FOR LATER.    02050000
*                                                            @Y30DSMD*/ 02051000
         LR    R1,R13                                              0971 02052000
*   GEN SETS(R11,R12,R13,RET) REFS(PSALITA,FLC);                   0972 02053000
*                                                          /*@Y30DSMD*/ 02054000
          SETLOCK RELEASE,TYPE=LOCAL,                                  *02055000
               RELATED=(LOCAL,IDA0200T(DECHNDEB))                       02056000
*   R13=R1;                         /* RE-ESTABLISH REGISTER 13.   0973 02057000
*                                                            @Y30DSMD*/ 02058000
         LR    R13,R1                                              0973 02059000
*   R1=MWALCKC;                     /* RELOAD REGISTER ONE.  @Y30DSMD*/ 02060000
         LR    R1,MWALCKC                                          0974 02061000
*   R12=MWALCKB;                    /* RE-LOAD REGISTER 12.  @Y30DSMD*/ 02062000
         L     R12,MWALCKB                                         0975 02063000
*   RFY                                                            0976 02064000
*     R11 RSTD;                     /* INFORM THE COMPILER.  @Y30DSMD*/ 02065000
*   R11=MWALCKA;                    /* RE-LOAD FINAL REGISTER.     0977 02066000
*                                                            @Y30DSMD*/ 02067000
         L     R11,MWALCKA                                         0977 02068000
*   RFY                                                            0978 02069000
*     R11 UNRSTD;                   /* INFORM THE COMPILER.  @Y30DSMD*/ 02070000
*   MWADEBPT=R6;                    /* COPY DEB POINTER      @Y30DSMD*/ 02071000
         LR    MWADEBPT,R6                                         0979 02072000
*   R1=DEBDCBB;                     /* POINT TO CURRENT DEB  @Y30DSMD*/ 02073000
         L     R1,DEBDCBB-1(,MWADEBPT)                             0980 02074000
         LA    R1,0(,R1)                                           0980 02075000
*   IF R1^=ZERO THEN                /* DOES THE DEB EXIST?   @Y30DSMD*/ 02076000
         LTR   R1,R1                                               0981 02077000
         BZ    @RF00981                                            0981 02078000
*     GEN(DEBCHK (1),TYPE=DELETE,AM=VSAM);/* DELETE DEB FROM DEB   0982 02079000
*                                      TABLE                         */ 02080000
         DEBCHK (1),TYPE=DELETE,AM=VSAM                                 02081000
*   RFY                                                            0983 02082000
*     RET RSTD;                     /* INFORM THE COMPILER.  @Y30DSMD*/ 02083000
@RF00981 DS    0H                                                  0984 02084000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 02085000
         L     RET,MWASAVEA                                        0984 02086000
*   RESPECIFY                                                      0985 02087000
*    (R1,                                                          0985 02088000
*     R6,                                                          0985 02089000
*     R7,                                                          0985 02090000
*     RET) UNRESTRICTED;                                           0985 02091000
*   RFY                                                            0986 02092000
*     TCB BASED(PSATOLD);           /* RESET TO DEFAULT TCB        0986 02093000
*                                      (CURRENT).            @Z30SSMD*/ 02094000
*   END DEHOOK;                                                    0987 02095000
@EL00015 DS    0H                                                  0987 02096000
@EF00015 DS    0H                                                  0987 02097000
@ER00015 BR    @14                                                 0987 02098000
*FREECORE:                                                         0988 02099000
*                                                                  0988 02100000
*/********************************************************************/ 02101000
*/*                                                                  */ 02102000
*/*      01 FUNCTION - FREE GOTTEN STORAGE                           */ 02103000
*/*      01 INPUTS -   1. ADDRESS OF STORAGE TO BE FREED IN MWADDRES */ 02104000
*/*                    2. NUMBER OF BYTES TO BE FREED IN MWABYTES    */ 02105000
*/*                    3. SUBPOOL NUMBER IN MWASPOOL                 */ 02106000
*/*      01 OUTPUTS -  1. CORE STORAGE FREED                         */ 02107000
*/*                                                                  */ 02108000
*/* IN VS2 THIS ROUTINE IA ALTERED TO PROVIDE KEY SUPPORT FOR        */ 02109000
*/* GSR KEY & KEY 5 STORAGE. THE CALLING ROUTINE MUST SET    @Z40ESSR*/ 02110000
*/* MWAFRKEY. IF MWAFRKEY IS K50 THEN THE CORE IS ASSUMED TO @Z40ESSR*/ 02111000
*/* BE IN KEY 5. IF MWAFRKEY IS SET TO GSR KEY STORAGE IS IN @Z40ESSR*/ 02112000
*/* GSR KEY(0 TO 7). IF NEITHER GSR KEY NOR K50 IS THE VALUE @Z40ESSR*/ 02113000
*/* IN MWAFRKEY THEN THE CORE WILL BE FREEMAINED WHILE IN    @Z40ESSR*/ 02114000
*/* KEY 0(DEFAULT). MWAFRKEY IS SET TO 0 AT THE CONCLUSION   @Z40ESSR*/ 02115000
*/* OF THIS ROUTINE                                          @Z40ESSR*/ 02116000
*/********************************************************************/ 02117000
*                                                                  0988 02118000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0988 02119000
FREECORE DS    0H                                                  0989 02120000
*   RESPECIFY                                                      0989 02121000
*    (R0,                                                          0989 02122000
*     R1,                                                          0989 02123000
*     R7,                                                          0989 02124000
*     RET) RESTRICTED;              /*                       @Y30DSMD*/ 02125000
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */ 02126000
         ST    RET,MWASAVEC                                        0990 02127000
*   RESPECIFY                                                      0991 02128000
*     RET UNRESTRICTED;                                            0991 02129000
*   R1=MWADDRES;                    /* ADDRESS OF CORE TO BE FREED   */ 02130000
         L     R1,MWADDRES                                         0992 02131000
*   R0=MWAMNWRD;                    /* SUBPOOL NO. AND BYTES TO BE 0993 02132000
*                                      FREE                          */ 02133000
*                                                                  0993 02134000
         L     R0,MWAMNWRD                                         0993 02135000
*   /*****************************************************************/ 02136000
*   /*                                                               */ 02137000
*   /* IS A DEB BEING FREED? IF SO THEN A SPECIAL BRANCH . ENTRY TO  */ 02138000
*   /* FREEMAIN MUST BE USED.                                @Y30DSMD*/ 02139000
*   /*                                                               */ 02140000
*   /*****************************************************************/ 02141000
*                                                                  0994 02142000
*   IF MWAFRKEY=K50&MWASPOOL=RDEBPOOL THEN/* IS THIS A DEB?  @Y30DSMD*/ 02143000
         CLI   MWAFRKEY,X'50'                                      0994 02144000
         BNE   @RF00994                                            0994 02145000
         CLI   MWASPOOL,230                                        0994 02146000
         BNE   @RF00994                                            0994 02147000
*     DO;                           /* IF DEB AND NOT CTLG OR GSR  0995 02148000
*                                      THEN PROCESS IT.      @Y30DSMD*/ 02149000
*                                                                  0995 02150000
*       /*************************************************************/ 02151000
*       /*                                                           */ 02152000
*       /* THE BRANCH ENTRY TO FREEMAIN USES R3,4,7. SAVE THEM.      */ 02153000
*       /*                                                           */ 02154000
*       /*************************************************************/ 02155000
*                                                                  0996 02156000
*       MWADEBR3=R3;                /* SAVE OFF REG(3).      @Y30DSMD*/ 02157000
         LR    MWADEBR3,R3                                         0996 02158000
*       MWADEBR4=R4;                /* SAVE OFF REG(4) IN MWA.     0997 02159000
*                                                            @Y30DSMD*/ 02160000
         LR    MWADEBR4,R4                                         0997 02161000
*       MWADEBR7=R7;                /* SAVE OFF REG(7) IN THE MWA. 0998 02162000
*                                                            @Y30DSMD*/ 02163000
*                                                                  0998 02164000
         ST    R7,MWADEBR7                                         0998 02165000
*       /*************************************************************/ 02166000
*       /*                                                           */ 02167000
*       /* OBTAIN THE LOCAL LOCK TO SERIALIZE FREEMAIN.      @Y30DSMD*/ 02168000
*       /*                                                           */ 02169000
*       /*************************************************************/ 02170000
*                                                                  0999 02171000
*       RFY                                                        0999 02172000
*         R11 RSTD;                 /* INFORM THE COMPILER.  @Y30DSMD*/ 02173000
*       MWALCKA=R11;                /* SAVE OFF REGISTER ELEVEN.   1000 02174000
