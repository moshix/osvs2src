*   RFY                                                            1217 02924000
*     RET RSTD;                     /* RESERVE REG(14).      @ZA02280*/ 02925000
*   IF IQEIRB->RBIQE->IQETCBA^=ADDR(TCB) THEN/* OUR TCB ?    @ZA02280*/ 02926000
