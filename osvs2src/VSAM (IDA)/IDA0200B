*     RETCODE) UNRESTRICTED;                                       0919 01859000
*   END PROBDT;                                                    0920 01860000
@EL00013 DS    0H                                                  0920 01861000
@EF00013 DS    0H                                                  0920 01862000
@ER00013 BR    @14                                                 0920 01863000
*ERRORFLG:                                                         0921 01864000
*                                                                  0921 01865000
*/********************************************************************/ 01866000
*/*                                                                  */ 01867000
*/*    01 FUNCTION - THIS ROUTINE SELECT THE CORRECT ERROR FLAG FOR  */ 01868000
*/*                  THE ACBERFLG                                    */ 01869000
*/*    01 INPUT -    1. PROBLEM DETERMINATION PROBLEM CODE (MWAPCODE)*/ 01870000
*/*                  2. PROBLEM DETERMINATION FUNCTION CODE(MWAPFUNC)*/ 01871000
*/*                  3. PROB DETERMINATION SUB FUNCTION CODE(MWAPSFC)*/ 01872000
*/*    01 OUTPUT -   1. THE CURRENT ERROR IS SET                     */ 01873000
*/*                  2. ACBERFLG IS SET IF CURRENT ERROR IS GREATER  */ 01874000
*/*                                                                  */ 01875000
*/********************************************************************/ 01876000
*                                                                  0921 01877000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0921 01878000
ERRORFLG DS    0H                                                  0922 01879000
*   RESPECIFY                                                      0922 01880000
*     RET RESTRICTED;                                              0922 01881000
*   MWASAVEE=RET;                   /* SAVE RETURN ADDRESS           */ 01882000
         LR    MWASAVEE,RET                                        0923 01883000
*   RESPECIFY                                                      0924 01884000
*     RET UNRESTRICTED;                                            0924 01885000
*   PDPFUNC=MWAPFUNC;               /* GET FUNCTION CODE             */ 01886000
         LA    @11,280                                             0925 01887000
         AL    @11,CLWCOMWK(,RCORE)                                0925 01888000
         IC    @09,MWAPFUNC                                        0925 01889000
         STC   @09,PDPFUNC(,@11)                                   0925 01890000
*   PDPCODE=MWAPCODE;               /* GET PROBLEM CODE              */ 01891000
         SLR   @09,@09                                             0926 01892000
         IC    @09,MWAPCODE                                        0926 01893000
         STC   @09,PDPCODE(,@11)                                   0926 01894000
*   PDPSFC=MWAPSFC;                 /* GET SUB FUNCTION CODE         */ 01895000
         IC    @06,MWAPSFC                                         0927 01896000
         STC   @06,PDPSFC(,@11)                                    0927 01897000
*   IF PDPACB->ACBERFLG<IDAERMAP(PDPCODE) THEN/* IF CURRENT ERROR IS    01898000
*                                      GREATER THAN ACB ERROR FLAG   */ 01899000
         L     @11,PDPACB(,@11)                                    0928 01900000
         SLR   @06,@06                                             0928 01901000
         IC    @06,IDAERMAP-1(@09)                                 0928 01902000
         SLR   @09,@09                                             0928 01903000
         IC    @09,ACBERFLG(,@11)                                  0928 01904000
         CR    @06,@09                                             0928 01905000
         BNH   @RF00928                                            0928 01906000
*     PDPACB->ACBERFLG=IDAERMAP(PDPCODE);/* SET THE HIGHER ERROR CODE*/ 01907000
         STC   @06,ACBERFLG(,@11)                                  0929 01908000
*   RESPECIFY                                                      0930 01909000
*     RET RESTRICTED;                                              0930 01910000
@RF00928 DS    0H                                                  0931 01911000
*   RET=MWASAVEE;                   /* RESTORE RETURN ADDRESS        */ 01912000
         LR    RET,MWASAVEE                                        0931 01913000
*   RESPECIFY                                                      0932 01914000
*     RET UNRESTRICTED;                                            0932 01915000
*   END ERRORFLG;                                                  0933 01916000
@EL00014 DS    0H                                                  0933 01917000
@EF00014 DS    0H                                                  0933 01918000
@ER00014 BR    @14                                                 0933 01919000
*DEHOOK:                                                           0934 01920000
*                                                                  0934 01921000
*/********************************************************************/ 01922000
*/*                                                                  */ 01923000
*/*      01 FUNCTION - DECHAIN DEB AND REMOVE DEB FROM DEB TABLE     */ 01924000
*/*      01 INPUTS -   1. ADDRESS OF DEB IN MWADEBPT                 */ 01925000
*/*      01 OUTPUTS -  1. DEB DECHAINED AND DELETED FROM DEB TABLE   */ 01926000
*/*                                                                  */ 01927000
*/* THIS ROUTINE IS ALTERD FOR VS2. IN VS2 A DEB CHAIN/DECHAIN       */ 01928000
*/* PROTOCOL EXISTS FOR THE MP ENVIRONMENT.                         .*/ 01929000
*/********************************************************************/ 01930000
*                                                                  0934 01931000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0934 01932000
DEHOOK   DS    0H                                                  0935 01933000
*   RESPECIFY                                                      0935 01934000
*    (R1,                                                          0935 01935000
*     R6,                                                          0935 01936000
*     R7,                                                          0935 01937000
*     RET) RESTRICTED;                                             0935 01938000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 01939000
         ST    RET,MWASAVEA                                        0936 01940000
*   RESPECIFY                                                      0937 01941000
*     RET UNRESTRICTED;                                            0937 01942000
*   R6=MWADEBPT;                    /* SAVE POINTER TO DEB FOR     0938 01943000
*                                      DECHAIN                       */ 01944000
         LR    R6,MWADEBPT                                         0938 01945000
*   RFY                                                            0939 01946000
*     R11 RSTD;                     /* INFORM THE COMPILER.  @Y30DSMD*/ 01947000
*   MWALCKA=R11;                    /* SAVE OFF REGISTER ELEVEN.   0940 01948000
*                                                            @Y30DSMD*/ 01949000
         ST    R11,MWALCKA                                         0940 01950000
*   RFY                                                            0941 01951000
*     R11 UNRSTD;                   /* INFORM THE COMPILER.  @Y30DSMD*/ 01952000
*   MWALCKB=R12;                    /* SAVE OFF REGISTER TWELVE.   0942 01953000
*                                                            @Y30DSMD*/ 01954000
         ST    R12,MWALCKB                                         0942 01955000
*   MWALCKC=R1;                     /* REGISTER ONE ALSO IS SAVED. 0943 01956000
*                                                            @Y30DSMD*/ 01957000
         LR    MWALCKC,R1                                          0943 01958000
*   R1=R13;                         /* SWITCH REGISTER 13 FOR LATER.    01959000
*                                                            @Y30DSMD*/ 01960000
         LR    R1,R13                                              0944 01961000
*   GEN SETS(R11,R12,R13,RET) REFS(PSALITA,FLC);                   0945 01962000
*                                                          /*@Y30DSMD*/ 01963000
          SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                       *01964000
               RELATED=(LOCAL,IDA0200T(DECHNDEB))                       01965000
*   R13=R1;                         /* RE-ESTABLISH REGISTER 13.   0946 01966000
*                                                            @Y30DSMD*/ 01967000
         LR    R13,R1                                              0946 01968000
*   R1=MWALCKC;                     /* RELOAD REGISTER ONE.  @Y30DSMD*/ 01969000
         LR    R1,MWALCKC                                          0947 01970000
*   R12=MWALCKB;                    /* RE-LOAD REGISTER 12.  @Y30DSMD*/ 01971000
         L     R12,MWALCKB                                         0948 01972000
*   RFY                                                            0949 01973000
*     R11 RSTD;                     /* INFORM THE COMPILER.  @Y30DSMD*/ 01974000
*   R11=MWALCKA;                    /* RE-LOAD FINAL REGISTER.     0950 01975000
*                                                            @Y30DSMD*/ 01976000
         L     R11,MWALCKA                                         0950 01977000
*   RFY                                                            0951 01978000
*     R11 UNRSTD;                   /* INFORM THE COMPILER.  @Y30DSMD*/ 01979000
*   RFY                                                            0952 01980000
*     TCB BASED(DEBTCBAD);          /* USE THE TCB FROM THE DEB    0952 01981000
*                                      (CURRENT | JS).               */ 01982000
*   IF TCBDEB=MWADEBPT THEN         /* IS MY DEB THE FIRST ON THE  0953 01983000
*                                      CHAIN?                        */ 01984000
         L     @09,DEBTCBAD(,MWADEBPT)                             0953 01985000
         C     MWADEBPT,TCBDEB(,@09)                               0953 01986000
         BNE   @RF00953                                            0953 01987000
*     DO;                           /* PROCESS THE FIRST DEB ON THE     01988000
*                                      CHAIN.                @Y30DSMD*/ 01989000
*       MWARDEB=MWADEBPT;           /* SET UP OUR DEB PTR.   @Y30DSMD*/ 01990000
         LR    MWARDEB,MWADEBPT                                    0955 01991000
*       MWANDEB=DEBDEBB;            /* SET UP NEXT DEB PTR.  @Y30DSMD*/ 01992000
         L     MWANDEB,DEBDEBB-1(,MWADEBPT)                        0956 01993000
         LA    MWANDEB,0(,MWANDEB)                                 0956 01994000
*       CS(MWARDEB,MWANDEB,TCBDEB); /* MAKE NEXT DEB THE FIRST.      */ 01995000
         L     @15,DEBTCBAD(,MWADEBPT)                             0957 01996000
         CS    MWARDEB,@09,TCBDEB(@15)                             0957 01997000
*       BC(8,DECHND1);              /* GOTO DECHND1 IF SUCCESSFUL. 0958 01998000
*                                                            @Y30DSMD*/ 01999000
         BC    8,DECHND1                                           0958 02000000
*     END;                          /* END OF FIRST DEB PROCESSING.     02001000
*                                                            @Y30DSMD*/ 02002000
*   MWADEBPT=DEBTCBAD+FWORDLEN;     /* LOAD PTR TO 1ST DEB IN      0960 02003000
*                                      DEB-CHAIN                     */ 02004000
@RF00953 LA    @09,4                                               0960 02005000
         AL    @09,DEBTCBAD(,MWADEBPT)                             0960 02006000
         LR    MWADEBPT,@09                                        0960 02007000
*   DO WHILE(DEBDEBB^=R6&DEBDEBB^=0);/* IF NO MATCH FOUND    @Y30DSMD*/ 02008000
         B     @DE00961                                            0961 02009000
@DL00961 DS    0H                                                  0962 02010000
*     MWADEBPT=DEBDEBB;             /* POINT TO NEXT DEB     @Y30DSMD*/ 02011000
         L     @09,DEBDEBB-1(,MWADEBPT)                            0962 02012000
         LA    @09,0(,@09)                                         0962 02013000
         LR    MWADEBPT,@09                                        0962 02014000
*   END;                            /*                       @Y30DSMD*/ 02015000
@DE00961 L     @09,DEBDEBB-1(,MWADEBPT)                            0963 02016000
         LA    @09,0(,@09)                                         0963 02017000
         CR    @09,R6                                              0963 02018000
         BE    @DC00961                                            0963 02019000
         LTR   @09,@09                                             0963 02020000
         BNZ   @DL00961                                            0963 02021000
@DC00961 DS    0H                                                  0964 02022000
*   IF DEBDEBB^=ZERO THEN           /* IF DEB MATCH FOUND    @Y30DSMD*/ 02023000
         L     @09,DEBDEBB-1(,MWADEBPT)                            0964 02024000
         LA    @09,0(,@09)                                         0964 02025000
         LTR   @09,@09                                             0964 02026000
         BZ    @RF00964                                            0964 02027000
*     DEBDEBB=DEBDEBB->DEBDEBB;     /* DECHAIN THE DEB       @Y30DSMD*/ 02028000
         L     @09,DEBDEBB-1(,@09)                                 0965 02029000
         LA    @09,0(,@09)                                         0965 02030000
         STCM  @09,7,DEBDEBB(MWADEBPT)                             0965 02031000
*DECHND1:                           /* DECHAIN DEB           @Z30SSRL*/ 02032000
*   RFY                                                            0966 02033000
*     R11 RSTD;                     /* MAKE A NOTE FOR THE COMPILER.    02034000
*                                                            @Y30DSMD*/ 02035000
@RF00964 DS    0H                                                  0966 02036000
DECHND1  DS    0H                                                  0967 02037000
*   MWALCKA=R11;                    /* SAVE OFF REGISTER ELEVEN.   0967 02038000
*                                                            @Y30DSMD*/ 02039000
         ST    R11,MWALCKA                                         0967 02040000
*   RFY                                                            0968 02041000
*     R11 UNRSTD;                   /* THE COMPILER NEEDS REGISTER 0968 02042000
*                                      INFO.                 @Y30DSMD*/ 02043000
*   MWALCKB=R12;                    /* SAVE OFF REGISTER TWELVE.   0969 02044000
*                                                            @Y30DSMD*/ 02045000
         ST    R12,MWALCKB                                         0969 02046000
*   MWALCKC=R1;                     /* REGISTER ONE ALSO IS SAVED. 0970 02047000
