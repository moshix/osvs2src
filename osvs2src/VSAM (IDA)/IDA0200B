         CR    @07,@08                                             0658 01062000
         BE    @GL00012                                            0658 01063000
         L     @08,CLWAMBPT(,RCORE)                                0658 01064000
         CLC   AMBCACB(4,@07),AMBCACB(@08)                         0658 01065000
         BNE   @GL00012                                            0658 01066000
         CLC   AMBDDSN(3,@07),AMBDDSN(@08)                         0658 01067000
         BE    @RT00658                                            0658 01068000
@GL00012 L     @08,AMBLDTA(,MWAMBLPT)                              0658 01069000
         L     @07,CLWAMBPT(,RCORE)                                0658 01070000
         CLC   AMBCACB(4,@08),AMBCACB(@07)                         0658 01071000
         BNE   @RF00658                                            0658 01072000
         CLC   AMBDDSN(3,@08),AMBDDSN(@07)                         0658 01073000
         BNE   @RF00658                                            0658 01074000
@RT00658 DS    0H                                                  0659 01075000
*             DO;                                                  0659 01076000
*               MWASWTCH=ON;        /* SET SWITCH TO INDICATE        */ 01077000
         OI    MWASWTCH,B'10000000'                                0660 01078000
*               IF CMBOUT=ON THEN   /* TEST FOR OTHER STRUCTURE      */ 01079000
         L     @08,AMBLCMB(,MWAMBLPT)                              0661 01080000
         TM    CMBOUT(@08),B'10000000'                             0661 01081000
         BNO   @RF00661                                            0661 01082000
*                 MWAOUTSW=ON;      /* IND OTHER STRUCTURE FOR OUTPUT   01083000
*                                      EXIST                         */ 01084000
         OI    MWAOUTSW,B'10000000'                                0662 01085000
*             END;                                                 0663 01086000
@RF00661 DS    0H                                                  0664 01087000
*           RESPECIFY                                              0664 01088000
*             IDAAMBL BASED(ACBAMBL);                              0664 01089000
@RF00658 DS    0H                                                  0665 01090000
*         END;                                                     0665 01091000
*     END;                                                         0666 01092000
@RF00654 AL    MWAINDEX,@CF00051                                   0666 01093000
@DE00653 SLR   @08,@08                                             0666 01094000
         IC    @08,VATENO(,MWAVATPT)                               0666 01095000
         CR    MWAINDEX,@08                                        0666 01096000
         BNH   @DL00653                                            0666 01097000
*     MWAVATPT=VATNEXT;             /* POINT TO NEXT VAT             */ 01098000
         L     MWAVATPT,VATNEXT(,MWAVATPT)                         0667 01099000
*   END;                                                           0668 01100000
@DE00652 LTR   MWAVATPT,MWAVATPT                                   0668 01101000
         BZ    @DC00652                                            0668 01102000
         TM    MWAOUTSW,B'10000000'                                0668 01103000
         BZ    @DL00652                                            0668 01104000
@DC00652 DS    0H                                                  0669 01105000
*   MWAVATPT=JSCBSHR;               /* RESET VAT POINTED TO JSCBSHR  */ 01106000
         L     @01,PSATOLD                                         0669 01107000
         L     @01,TCBJSCB(,@01)                                   0669 01108000
         L     MWAVATPT,JSCBSHR(,@01)                              0669 01109000
*   CALL SHAREDEQ;                  /* DEQ RESOURCES                 */ 01110000
         BAL   @14,SHAREDEQ                                        0670 01111000
*   RESPECIFY                                                      0671 01112000
*     RET RESTRICTED;                                              0671 01113000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 01114000
         L     RET,MWASAVEA                                        0672 01115000
*   RESPECIFY                                                      0673 01116000
*     RET UNRESTRICTED;                                            0673 01117000
*   END SHARE;                                                     0674 01118000
@EL00006 DS    0H                                                  0674 01119000
@EF00006 DS    0H                                                  0674 01120000
@ER00006 BR    @14                                                 0674 01121000
*SHAREDEQ:                                                         0675 01122000
*                                                                  0675 01123000
*/********************************************************************/ 01124000
*/*      01 FUNCTION - ISSUE DEQ FOR RESOURCES THAT ARE NO LONGER    */ 01125000
*/*                    NEEDED                                        */ 01126000
*/*      01 INPUTS -   1. ADDRESS OF AMB IN CLWAMBPT                 */ 01127000
*/*      01 OUTPUTS -  1. RESOURCES THAT ARE NOT NEEDED WERE DEQUEUED*/ 01128000
*/*                                                                  */ 01129000
*/********************************************************************/ 01130000
*                                                                  0675 01131000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0675 01132000
SHAREDEQ DS    0H                                                  0676 01133000
*   RESPECIFY                                                      0676 01134000
*    (RET,                                                         0676 01135000
*     RETCODE) RESTRICTED;                                         0676 01136000
*   MWASAVEB=RET;                   /* GET RETURN ADDRESS            */ 01137000
         ST    RET,MWASAVEB                                        0677 01138000
*   RESPECIFY                                                      0678 01139000
*     RET UNRESTRICTED;                                            0678 01140000
*   ENQDIR=ON;                      /* INDICATE ENQ DIRECTED         */ 01141000
         OI    ENQDIR,B'00000001'                                  0679 01142000
*   ENQRNCI=AMBDDSN;                /* CI NUMBER FOR RNAME           */ 01143000
         L     @14,CLWAMBPT(,RCORE)                                0680 01144000
         MVC   ENQRNCI(3),AMBDDSN(@14)                             0680 01145000
