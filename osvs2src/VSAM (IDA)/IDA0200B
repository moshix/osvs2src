         LR    RET,R9                                              0460 00426000
*   RESPECIFY                                                      0461 00427000
*    (R9,                                                          0461 00428000
*     RET,                                                         0461 00429000
*     RETCODE) UNRESTRICTED;                                       0461 00430000
*   END INIT200B;                                                  0462 00431000
         B     @EL00002                                            0462 00432000
*VALCHECK:                                                         0463 00433000
*                                                                  0463 00434000
*/********************************************************************/ 00435000
*/*                                                                  */ 00436000
*/*      01 FUNCTION - VALIDITY CHECK AMBL AND AMB DEB'S             */ 00437000
*/*      01 INPUTS -   1. ADDRESS OF AMBL IN MWAMBLPT                */ 00438000
*/*      01 OUTPUTS -  1. VALIDITY CHECKS SUCCESSFUL                 */ 00439000
*/*                                                                  */ 00440000
*/********************************************************************/ 00441000
*                                                                  0463 00442000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0463 00443000
VALCHECK DS    0H                                                  0464 00444000
*   RESPECIFY                                                      0464 00445000
*    (RET,                                                         0464 00446000
*     RETCODE) RESTRICTED;                                         0464 00447000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 00448000
         ST    RET,MWASAVEA                                        0465 00449000
*   RESPECIFY                                                      0466 00450000
*     RET UNRESTRICTED;                                            0466 00451000
*   MWAPFUNC=PDF111;                /* SET FUNCTION CODE             */ 00452000
         MVI   MWAPFUNC,X'6F'                                      0467 00453000
*   MWAVATPT=JSCBSHR;               /* POINT TO THE VAT              */ 00454000
         L     @14,PSATOLD                                         0468 00455000
         L     @14,TCBJSCB(,@14)                                   0468 00456000
         L     MWAVATPT,JSCBSHR(,@14)                              0468 00457000
*   DO WHILE AMBLVRT^=VATVRT&VATNEXT^=ZERO;/* FIND CORRECT VAT     0469 00458000
*                                      EXTENSION                     */ 00459000
         B     @DE00469                                            0469 00460000
@DL00469 DS    0H                                                  0470 00461000
*     MWAVATPT=VATNEXT;             /* POINT TO NEXT EXTENSION       */ 00462000
         L     MWAVATPT,VATNEXT(,MWAVATPT)                         0470 00463000
*   END;                                                           0471 00464000
@DE00469 L     @14,ACBAMBL(,RACB)                                  0471 00465000
         CLC   AMBLVRT(1,@14),VATVRT(MWAVATPT)                     0471 00466000
         BE    @DC00469                                            0471 00467000
         L     @14,VATNEXT(,MWAVATPT)                              0471 00468000
         LTR   @14,@14                                             0471 00469000
         BNZ   @DL00469                                            0471 00470000
@DC00469 DS    0H                                                  0472 00471000
*   IF AMBLENO<=VATENO&             /* IF USER AMBL IS VALID         */ 00472000
*       ACBAMBL=VATAMBL(AMBLENO) THEN/* CHECK AMBL AGAINST VAT       */ 00473000
         L     @14,ACBAMBL(,RACB)                                  0472 00474000
         CLC   AMBLENO(1,@14),VATENO(MWAVATPT)                     0472 00475000
         BH    @RF00472                                            0472 00476000
         SLR   @11,@11                                             0472 00477000
         IC    @11,AMBLENO(,@14)                                   0472 00478000
         SLA   @11,2                                               0472 00479000
         C     @14,VATAMBL-4(@11,MWAVATPT)                         0472 00480000
         BNE   @RF00472                                            0472 00481000
*     DO;                           /* IF AMBL IS VALID              */ 00482000
*       RESPECIFY                                                  0474 00483000
*         R1 RESTRICTED;                                           0474 00484000
*       R1=AMBLDTA;                 /* POINT TO DATA AMB             */ 00485000
         L     R1,AMBLDTA(,@14)                                    0475 00486000
*       GEN(DEBCHK (1),TYPE=VERIFY,AM=VSAM);/* VERIFY DATA DEB       */ 00487000
         DEBCHK (1),TYPE=VERIFY,AM=VSAM                                 00488000
*       IF AMBLIX^=ZERO THEN        /* IF KSDS                       */ 00489000
         L     @14,ACBAMBL(,RACB)                                  0477 00490000
         L     @14,AMBLIX(,@14)                                    0477 00491000
         LTR   @14,@14                                             0477 00492000
         BZ    @RF00477                                            0477 00493000
*         DO;                                                      0478 00494000
*           R1=AMBLIX;              /* POINT TO INDEX AMB            */ 00495000
         LR    R1,@14                                              0479 00496000
*           GEN(DEBCHK (1),TYPE=VERIFY,AM=VSAM);/* VERIFY INDEX DEB  */ 00497000
         DEBCHK (1),TYPE=VERIFY,AM=VSAM                                 00498000
*         END;                                                     0481 00499000
*       RESPECIFY                                                  0482 00500000
*         R1 UNRESTRICTED;                                         0482 00501000
@RF00477 DS    0H                                                  0483 00502000
*     END;                                                         0483 00503000
*   ELSE                            /* IF AMBL IS INVALID            */ 00504000
*     DO;                                                          0484 00505000
         B     @RC00472                                            0484 00506000
@RF00472 DS    0H                                                  0485 00507000
*       MWAPCODE=PP78;              /* PROBLEM CODE FOR INVALID AMBL */ 00508000
         MVI   MWAPCODE,X'4E'                                      0485 00509000
*       CLWTERM=ON;                 /* INDICATE TERMINATING ERROR    */ 00510000
         OI    CLWTERM(RCORE),B'00010000'                          0486 00511000
*       CALL PROBDT;                /* PROBLEM DETERMINATION         */ 00512000
         BAL   @14,PROBDT                                          0487 00513000
*       RETURN TO LTRM200B;         /* GO TO TERM200B                */ 00514000
         LA    @14,LTRM200B                                        0488 00515000
@EL00003 DS    0H                                                  0488 00516000
@EF00003 DS    0H                                                  0488 00517000
@ER00003 BR    @14                                                 0488 00518000
*     END;                                                         0489 00519000
*   MWAVATPT=JSCBSHR;               /* RESTORE VAT POINTER           */ 00520000
@RC00472 L     @14,PSATOLD                                         0490 00521000
         L     @14,TCBJSCB(,@14)                                   0490 00522000
         L     MWAVATPT,JSCBSHR(,@14)                              0490 00523000
*   RESPECIFY                                                      0491 00524000
*     RET RESTRICTED;                                              0491 00525000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 00526000
         L     RET,MWASAVEA                                        0492 00527000
*   RESPECIFY                                                      0493 00528000
*    (RET,                                                         0493 00529000
*     RETCODE) UNRESTRICTED;                                       0493 00530000
*   END VALCHECK;                                                  0494 00531000
         B     @EL00003                                            0494 00532000
*WRITBUFR:                                                         0495 00533000
*                                                                  0495 00534000
*/********************************************************************/ 00535000
*/*                                                                  */ 00536000
*/*      01 FUNCTION - DYNAMICALLY GENERATE RPL, PLH & IO BLOCKS FOR */ 00537000
*/*                    INTERFACE WITH RECORD MANAGEMENT TO WRITE OUT */ 00538000
*/*                    BUFFERS                                       */ 00539000
*/*      01 INPUTS -   1. KEY LENGTH FROM AMDSB                      */ 00540000
*/*                    2. POINTER TO VAT IN JSCBSHR                  */ 00541000
*/*      01 OUTPUTS -  1. BUFFERS FOR AMBL WRITTEN OUT               */ 00542000
*/*                                                                  */ 00543000
*/* IN VS2 THIS ROUTINE IS CHANGED TO INTERFACE WITH IDA0192Y TO     */ 00544000
*/* OBTAIN AND BUILD THE I/O BLOCKS NECESSARY FOR THE WRTBFR.       .*/ 00545000
*/********************************************************************/ 00546000
*                                                                  0495 00547000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0495 00548000
WRITBUFR DS    0H                                                  0496 00549000
*   RESPECIFY                                                      0496 00550000
*    (RET,                                                         0496 00551000
*     RETCODE) RESTRICTED;          /*                       @Y30DSMD*/ 00552000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 00553000
         ST    RET,MWASAVEA                                        0497 00554000
*   RESPECIFY                                                      0498 00555000
*     RET UNRESTRICTED;                                            0498 00556000
*   PDPFUNC=PDF115;                 /* SET FUNCTION CODE             */ 00557000
         L     @14,CLWCOMWK(,RCORE)                                0499 00558000
         MVI   PDPFUNC+280(@14),X'73'                              0499 00559000
*   CLWAMBPT=AMBLDTA;               /* ADDRESSABILITY TO DATA AMB    */ 00560000
         L     @05,ACBAMBL(,RACB)                                  0500 00561000
         L     @11,AMBLDTA(,@05)                                   0500 00562000
         ST    @11,CLWAMBPT(,RCORE)                                0500 00563000
*   MWABYTES=((LENGTH(IFGRPL)+(FWORDLEN-ONE))&(-FWORDLEN))+LENGTH( 0501 00564000
*       MWABUFSA);                  /* BUILD DUMMY RPL AND REGISTER     00565000
*                                      SAVE AREA                     */ 00566000
         MVC   MWABYTES(3),@CF02732+1                              0501 00567000
*   IF AMBLGSR=ON THEN              /* IS THIS A GSR DATA SET?     0502 00568000
*                                                            @Z30SSMD*/ 00569000
         TM    AMBLGSR(@05),B'01000000'                            0502 00570000
         BNO   @RF00502                                            0502 00571000
*     DO;                           /* IF SO THE N USE GSR SUBPOOL 0503 00572000
*                                      AND KEY.              @Z30SSMD*/ 00573000
*       MWASPOOL=GSRSPOOL;          /* USE THE GSR POOL.     @Z30SSMD*/ 00574000
         MVI   MWASPOOL,X'F1'                                      0504 00575000
*       MWAFRKEY=DXUKEY;            /* REQUEST GSR KEY       @Z40ESSR*/ 00576000
         MVC   MWAFRKEY(1),DXUKEY(@14)                             0505 00577000
*     END;                          /* END OF GSR DO GROUP.  @Z30SSMD*/ 00578000
*   ELSE                            /* OTHERWISE JUST DEFAULT TO   0507 00579000
*                                      USERPOOL.             @Z30SSMD*/ 00580000
*     MWASPOOL=USERPOOL;            /* UNPROTECTED SUBPOOL FOR     0507 00581000
*                                      PROBLEM PROGRAM STATE         */ 00582000
         B     @RC00502                                            0507 00583000
@RF00502 MVI   MWASPOOL,X'FA'                                      0507 00584000
*   CALL GETCORE;                   /* BUILD DUMMY RPL               */ 00585000
@RC00502 BAL   @14,GETCORE                                         0508 00586000
*   IF MWANOCOR=OFF THEN            /* BUILD DUMMY RPL SUCCESSFUL    */ 00587000
         TM    MWANOCOR,B'10000000'                                0509 00588000
         BNZ   @RF00509                                            0509 00589000
*     DO;                                                          0510 00590000
*       MWARPLBD=ON;                /* INDICATE DUMMY RPL BUILT      */ 00591000
         OI    MWARPLBD,B'10000000'                                0511 00592000
*       MWARPLPT=MWADDRES;          /* ADDRESSABILITY FOR DUMMY RPL  */ 00593000
         L     @14,MWADDRES                                        0512 00594000
         ST    @14,MWARPLPT                                        0512 00595000
*       RPLDACB=RACB;               /* POINT TO USER ACB.    @Y30DSMD*/ 00596000
         ST    RACB,RPLDACB(,@14)                                  0513 00597000
*       IF RACB=DXPACBAD THEN       /* IS THIS ACB THE ACB COPY?   0514 00598000
*                                                            @Y30DSMD*/ 00599000
         L     @05,CLWCOMWK(,RCORE)                                0514 00600000
         L     @11,DXPACBAD-1(,@05)                                0514 00601000
