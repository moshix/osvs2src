         BZ    @RF00568                                            0568 00761000
*             DO;                                                  0569 00762000
*               MWAPCODE=PP128;     /* SET PROBLEM CODE      @Y30DSMD*/ 00763000
         MVI   MWAPCODE,X'80'                                      0570 00764000
*               CALL PROBDT;        /* CALL PROBLEM DETERMINATION    */ 00765000
         BAL   @14,PROBDT                                          0571 00766000
*             END;                  /* END OF WRTBFR ERROR LOGIC.  0572 00767000
*                                                            @Y30DSMD*/ 00768000
*         END;                      /* END OF LOGIC FOR PLH        0573 00769000
*                                      AVAILABLE.            @Y30DSMD*/ 00770000
@RF00568 DS    0H                                                  0574 00771000
*     END;                          /* END OF RPL OBTAINED DO GROUP.    00772000
*                                                            @Z30SSMD*/ 00773000
@RC00521 DS    0H                                                  0575 00774000
*   IF MWARPLBD=ON THEN             /* IF DUMMY RPL EXIST            */ 00775000
@RF00509 TM    MWARPLBD,B'10000000'                                0575 00776000
         BNO   @RF00575                                            0575 00777000
*     DO;                           /* LOGIC IF RPL WAS OBTAINED.    */ 00778000
*       MWADDRES=MWARPLPT;          /* POINT TO DUMMY RPL            */ 00779000
         L     @14,MWARPLPT                                        0577 00780000
         ST    @14,MWADDRES                                        0577 00781000
*       IF AMBLGSR=ON THEN          /* IS THIS A GSR DATA SET?     0578 00782000
*                                                            @Z30SSMD*/ 00783000
         L     @14,ACBAMBL(,RACB)                                  0578 00784000
         TM    AMBLGSR(@14),B'01000000'                            0578 00785000
         BNO   @RF00578                                            0578 00786000
*         DO;                       /* IF SO THE N USE GSR SUBPOOL 0579 00787000
*                                      AND KEY.              @Z30SSMD*/ 00788000
*           MWASPOOL=GSRSPOOL;      /* USE THE GSR POOL.     @Z30SSMD*/ 00789000
         MVI   MWASPOOL,X'F1'                                      0580 00790000
*           MWAFRKEY=DXUKEY;        /* REQUEST GSR KEY       @Z40ESSR*/ 00791000
         L     @14,CLWCOMWK(,RCORE)                                0581 00792000
         MVC   MWAFRKEY(1),DXUKEY(@14)                             0581 00793000
*         END;                      /* END OF GSR DO GROUP.  @Z30SSMD*/ 00794000
*       ELSE                        /* OTHERWISE JUST DEFAULT TO   0583 00795000
*                                      USERPOOL.             @Z30SSMD*/ 00796000
*         MWASPOOL=USERPOOL;        /* UNPROTECTED SUBPOOL NUMBER    */ 00797000
         B     @RC00578                                            0583 00798000
@RF00578 MVI   MWASPOOL,X'FA'                                      0583 00799000
*       MWABYTES=LENGTH(IFGRPL)+LENGTH(MWABUFSA);/* SIZE OF DUMMY RPL   00800000
*                                      AND REGISTER SAVE AREA        */ 00801000
@RC00578 MVC   MWABYTES(3),@CF02732+1                              0584 00802000
*       CALL FREECORE;              /* FREE DUMMY RPL AND RELATED  0585 00803000
*                                      CORE                          */ 00804000
         BAL   @14,FREECORE                                        0585 00805000
*     END;                          /* END OF LOGIC IF RPL OBTAINED.    00806000
*                                                            @Y30DSMD*/ 00807000
*   CLWAMBPT=ZERO;                  /* ZERO AMB POINTER IN CLW       */ 00808000
@RF00575 SLR   @14,@14                                             0587 00809000
         ST    @14,CLWAMBPT(,RCORE)                                0587 00810000
*   IF RACB=DXPACBAD THEN           /* IS THIS ACB THE COPY ACB?   0588 00811000
*                                                            @Y30DSMD*/ 00812000
         L     @14,CLWCOMWK(,RCORE)                                0588 00813000
         L     @05,DXPACBAD-1(,@14)                                0588 00814000
         LA    @05,0(,@05)                                         0588 00815000
         CR    RACB,@05                                            0588 00816000
         BNE   @RF00588                                            0588 00817000
*     DO;                           /* PROCESS THE AFFECTED BLOCKS.     00818000
*                                                            @Y30DSMD*/ 00819000
*       DXUACBAD->ACBDEB->DEBDCBB=DXPACBAD;/* POINT TO COPY ACB      */ 00820000
         L     @14,DXUACBAD-1(,@14)                                0590 00821000
         LA    @14,0(,@14)                                         0590 00822000
         L     @01,ACBDEB-1(,@14)                                  0590 00823000
         STCM  @05,7,DEBDCBB(@01)                                  0590 00824000
*       DXUACBAD->ACBAMBL->AMBLACB=DXPACBAD;/* POINT TO COPY ACB.    */ 00825000
         L     @14,ACBAMBL(,@14)                                   0591 00826000
         ST    @05,AMBLACB(,@14)                                   0591 00827000
*     END;                          /* END OF SPECIAL COPY ACB     0592 00828000
*                                      PROCESSING.           @Y30DSMD*/ 00829000
*   RESPECIFY                                                      0593 00830000
*     RET RESTRICTED;                                              0593 00831000
@RF00588 DS    0H                                                  0594 00832000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 00833000
         L     RET,MWASAVEA                                        0594 00834000
*   RESPECIFY                                                      0595 00835000
*    (RET,                                                         0595 00836000
*     RETCODE) UNRESTRICTED;        /*                       @Y30DSMD*/ 00837000
*   END WRITBUFR;                                                  0596 00838000
@EL00004 DS    0H                                                  0596 00839000
@EF00004 DS    0H                                                  0596 00840000
@ER00004 BR    @14                                                 0596 00841000
*WRBUFFER:                                                         0597 00842000
*                                                                  0597 00843000
*/********************************************************************/ 00844000
*/*                                                                  */ 00845000
*/*      01 FUNCTION - INTERFACE WITH RECORD MANAGEMENT TO WRITE OUT */ 00846000
*/*                    BUFFERS                                       */ 00847000
*/*      01 INPUTS -   1. ADDRESS OF DUMMY RPL IN MWARPLPT           */ 00848000
*/*      01 OUTPUTS -  1. WRTBFR PERFORMED                           */ 00849000
*/*                                                                  */ 00850000
*/* THIS ROUTINE IS ALTERED FOR VS2. THE R/M INTERFACE HAS          .*/ 00851000
*/* CHANGED FOR VSAM REL 2. R/M WILL RETURN CONTROL IN THE KEY OF    */ 00852000
*/* IOMCKEY (FOR GSR THIS MEANS KEY 7). ALSO R/M WILL BE BRANCHED TO */ 00853000
*/* FOR GSR DATA SETS IN ORDER TO KEEP THE SUPERVISOR STATE  BIT     */ 00854000
*/* SINCE SYNCH WILL FORCE PROBLEM STATE.                           .*/ 00855000
*/********************************************************************/ 00856000
*                                                                  0597 00857000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0597 00858000
WRBUFFER DS    0H                                                  0598 00859000
*   RESPECIFY                                                      0598 00860000
*    (R0,                                                          0598 00861000
*     R1,                                                          0598 00862000
*     R6,                                                          0598 00863000
*     R7,                                                          0598 00864000
*     RET,                                                         0598 00865000
*     RETCODE) RESTRICTED;          /*                       @Z30SSMD*/ 00866000
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */ 00867000
         ST    RET,MWASAVEC                                        0599 00868000
*   RESPECIFY                                                      0600 00869000
*     RET UNRESTRICTED;                                            0600 00870000
*   STM(RACB,R13,XRBESA);           /* SAVE REGISTER IN RB SAVE AREA */ 00871000
         L     @14,PSATOLD                                         0601 00872000
         L     @14,TCBRBP(,@14)                                    0601 00873000
         STM   RACB,R13,XRBESA(@14)                                0601 00874000
*   R6=R13;                         /* GET ADDRESSABILITY            */ 00875000
         LR    R6,R13                                              0602 00876000
*   GEN(DROP R13);                  /* DROP R13 AS BASE REGISTER     */ 00877000
         DROP R13                                                       00878000
*   GEN(USING @DATD,R6);            /* TEMPORORY ADDRESSABILITY      */ 00879000
         USING @DATD,R6                                                 00880000
*   R0=RPLWRTB;                     /* INDICATE WRTBFR REQUEST       */ 00881000
         LA    R0,15                                               0605 00882000
*   R1=MWARPLPT;                    /* POINT TO DUMMY RPL            */ 00883000
         L     R1,MWARPLPT                                         0606 00884000
*   R13=(R1+LENGTH(IFGRPL)+(FWORDLEN-ONE))&(-FWORDLEN);/* PT TO REG     00885000
*                                      SAVE AREA ON FULL WORD BDY    */ 00886000
         LA    R13,79                                              0607 00887000
         ALR   R13,R1                                              0607 00888000
         N     R13,@CF05073                                        0607 00889000
*   R15=BIBINTRF;                   /* ADDRESS OF INTERFACE ROUTINE  */ 00890000
         L     @05,ACBAMBL(,RACB)                                  0608 00891000
         L     @14,AMBLBIB(,@05)                                   0608 00892000
         L     R15,BIBINTRF(,@14)                                  0608 00893000
*   R7=ADDR(RBGRS2);                /* POINT TO USER REGISTER SAVE 0609 00894000
*                                      AREA                          */ 00895000
         L     @14,PSATOLD                                         0609 00896000
         L     R7,TCBRBP(,@14)                                     0609 00897000
         LA    R7,RBGRS2(,R7)                                      0609 00898000
*   IF AMBLLSR=ON THEN              /* IF THIS IS JUST LSR CONTINUE.    00899000
*                                                            @Y30DSMD*/ 00900000
         TM    AMBLLSR(@05),B'00100000'                            0610 00901000
         BNO   @RF00610                                            0610 00902000
*     DO;                           /* BY DOING SYNCH TO R/M FOR   0611 00903000
*                                      WRBFR.                @Y30DSMD*/ 00904000
*       GEN(LM RACB,R12,0(R7));     /* RESTORE USER'S REGISTERS    0612 00905000
*                                                            @Z30SSMD*/ 00906000
         LM RACB,R12,0(R7)                                              00907000
*       GEN(SYNCH (15));            /* GO TO WRTBFR IN USER KEY      */ 00908000
         SYNCH (15)                                                     00909000
*       LM(RACB,R13,XRBESA);        /* RESTORE REGISTERS             */ 00910000
         L     @14,PSATOLD                                         0614 00911000
         L     @14,TCBRBP(,@14)                                    0614 00912000
         LM    RACB,R13,XRBESA(@14)                                0614 00913000
*       GEN(DROP R6);               /* DROP TEMPORORY ADDRESSABILITY */ 00914000
         DROP R6                                                        00915000
*       GEN(USING @DATD,R13);       /* REESTABLISH ADDRESSABILITY    */ 00916000
         USING @DATD,R13                                                00917000
*     END;                          /* END OF LOGIC FOR LSR ONLY.  0617 00918000
*                                                            @Y30DSMD*/ 00919000
*   ELSE                            /* PROCESS NON-LSR BUFFERS     0618 00920000
*                                                            @Z30SSRL*/ 00921000
*     DO;                                                          0618 00922000
         B     @RC00610                                            0618 00923000
@RF00610 DS    0H                                                  0619 00924000
*       GEN( DROP R13);             /* R13 IS NOT USABLE AT THIS   0619 00925000
*                                      POINT.                @Z30SSMD*/ 00926000
          DROP R13                                                      00927000
*       GEN( USING @DATD,R6);       /* SO USE R13 AS SAVED IN R6.  0620 00928000
*                                                            @Z30SSMD*/ 00929000
          USING @DATD,R6                                                00930000
*       IF AMBLGSR=ON THEN          /* IF THIS IS GSR CONTINUE.    0621 00931000
*                                                            @Z30SSMD*/ 00932000
         L     @14,ACBAMBL(,RACB)                                  0621 00933000
         TM    AMBLGSR(@14),B'01000000'                            0621 00934000
         BNO   @RF00621                                            0621 00935000
*         DO;                       /* BY DOING BRANCH TO R/M FOR  0622 00936000
*                                      WRBFR.                @Y30DSMD*/ 00937000
*           RFY                                                    0623 00938000
*             R14 RSTD;             /*                       @Z40ESSR*/ 00939000
*           R14=DXUKEY;             /* LOAD USER KEY INTO REG      0624 00940000
*                                                            @Z40ESSR*/ 00941000
         L     @05,CLWCOMWK(,RCORE)                                0624 00942000
         SLR   R14,R14                                             0624 00943000
         IC    R14,DXUKEY(,@05)                                    0624 00944000
*           SPKA(R14);              /* MODESET TO USER KEY   @Z40ESSR*/ 00945000
         SPKA  0(R14)                                              0625 00946000
*           RFY                                                    0626 00947000
*             R14 UNRSTD;           /*                       @Z40ESSR*/ 00948000
*           GEN(LM RACB,R12,0(R7)); /* RESTORE USER'S REGISTERS    0627 00949000
*                                                            @Z40ESSR*/ 00950000
         LM RACB,R12,0(R7)                                              00951000
*           GEN(BALR  14,15 );      /* GO TO WRTBFR IN PRIV KEY    0628 00952000
*                                                            @Y30DSMD*/ 00953000
         BALR  14,15                                                    00954000
*           GEN( MODESET EXTKEY=ZERO);/* INSURE KEY ZERO.    @Y30DSMD*/ 00955000
          MODESET EXTKEY=ZERO                                           00956000
*           LM(RACB,R13,XRBESA);    /* RESTORE REGISTERS     @Y30DSMD*/ 00957000
         L     @14,PSATOLD                                         0630 00958000
         L     @14,TCBRBP(,@14)                                    0630 00959000
         LM    RACB,R13,XRBESA(@14)                                0630 00960000
*           GEN(DROP R6);           /* DROP TEMPORORY ADDRESSABILITY */ 00961000
         DROP R6                                                        00962000
*           GEN(USING @DATD,R13);   /* REESTABLISH ADDRESSABILITY    */ 00963000
         USING @DATD,R13                                                00964000
*         END;                      /* END OF LOGIC FOR GSR ONLY.  0633 00965000
*                                                            @Y30DSMD*/ 00966000
*     END;                          /* END OF DO GROUP FOR GSR ONLY.    00967000
*                                                            @Z30SSMD*/ 00968000
@RF00621 DS    0H                                                  0635 00969000
*   IF RETCODE^=ZERO THEN           /* ERROR IN WRTBFR               */ 00970000
@RC00610 LTR   RETCODE,RETCODE                                     0635 00971000
         BZ    @RF00635                                            0635 00972000
*     DO;                                                          0636 00973000
*       CLWCNOUP=ON;                /* INDICATE NO CATALOG UPDATE    */ 00974000
         OI    CLWCNOUP(RCORE),B'01000000'                         0637 00975000
*       MWAPCODE=PP128;             /* SET PROBLEM CODE              */ 00976000
         MVI   MWAPCODE,X'80'                                      0638 00977000
*       CALL PROBDT;                /* PROBLEM DETERMINATION         */ 00978000
         BAL   @14,PROBDT                                          0639 00979000
*     END;                                                         0640 00980000
*   RESPECIFY                                                      0641 00981000
*     RET RESTRICTED;                                              0641 00982000
@RF00635 DS    0H                                                  0642 00983000
*   RET=MWASAVEC;                   /* GET RETURN ADDRESS            */ 00984000
         L     RET,MWASAVEC                                        0642 00985000
*   RESPECIFY                                                      0643 00986000
*    (R0,                                                          0643 00987000
*     R1,                                                          0643 00988000
*     R6,                                                          0643 00989000
*     R7,                                                          0643 00990000
*     RET,                                                         0643 00991000
*     RETCODE) UNRESTRICTED;        /*                       @Z30SSMD*/ 00992000
*   END WRBUFFER;                                                  0644 00993000
@EL00005 DS    0H                                                  0644 00994000
@EF00005 DS    0H                                                  0644 00995000
@ER00005 BR    @14                                                 0644 00996000
*SHARE:                                                            0645 00997000
*                                                                  0645 00998000
*/********************************************************************/ 00999000
*/*                                                                  */ 01000000
*/*      01 FUNCTION - SEARCH FOR CONCURRENT USER OF RESOURCES       */ 01001000
*/*                    DEQ ALL UNUSED RESOURCES                      */ 01002000
*/*      01 INPUTS -   1. POINTER TO AMB IN CLWAMBPT                 */ 01003000
*/*      01 OUTPUTS -  1. RESOURCES NOT CONCURRENTLY USED WERE DEQ   */ 01004000
*/*                                                                  */ 01005000
*/********************************************************************/ 01006000
*                                                                  0645 01007000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0645 01008000
SHARE    DS    0H                                                  0646 01009000
*   RESPECIFY                                                      0646 01010000
*     RET RESTRICTED;                                              0646 01011000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 01012000
         ST    RET,MWASAVEA                                        0647 01013000
*   RESPECIFY                                                      0648 01014000
*     RET UNRESTRICTED;                                            0648 01015000
*   MWAVATPT=JSCBSHR;               /* POINT TO FIRST VAT            */ 01016000
         L     @01,PSATOLD                                         0649 01017000
         L     @01,TCBJSCB(,@01)                                   0649 01018000
         L     MWAVATPT,JSCBSHR(,@01)                              0649 01019000
*   MWASWTCH=OFF;                   /* IND OTHER STRUCTURE EXIST     */ 01020000
         NI    MWASWTCH,B'01111111'                                0650 01021000
*   MWAOUTSW=OFF;                   /* IND OTHER OUTPUT STRUCTURE  0651 01022000
*                                      EXIST                         */ 01023000
         NI    MWAOUTSW,B'01111111'                                0651 01024000
*   DO WHILE MWAVATPT^=ZERO&        /* LOOP UNTIL VAT END OR         */ 01025000
*         MWAOUTSW=OFF;             /* STRUCTURE FOR OUTPUT FOUND    */ 01026000
         B     @DE00652                                            0652 01027000
@DL00652 DS    0H                                                  0653 01028000
*     DO MWAINDEX=1 TO VATENO;      /* INDEX FOR AMBL ENTRY          */ 01029000
         LA    MWAINDEX,1                                          0653 01030000
         B     @DE00653                                            0653 01031000
@DL00653 DS    0H                                                  0654 01032000
*       IF VATAMBL(MWAINDEX)^=ZERO& /* IF NOT AN INACTIVE AMBL AND   */ 01033000
*           VATAMBL(MWAINDEX)->AMBLDTA^=AMBLDTA&/* NOT OWN DATA AMB  */ 01034000
*           MWAOUTSW=OFF THEN       /* STRUCTURE FOR OUTPUT NOT FOUND*/ 01035000
         SLR   @08,@08                                             0654 01036000
         LR    @15,MWAINDEX                                        0654 01037000
         SLA   @15,2                                               0654 01038000
         L     @15,VATAMBL-4(@15,MWAVATPT)                         0654 01039000
         CR    @15,@08                                             0654 01040000
         BE    @RF00654                                            0654 01041000
         L     @01,ACBAMBL(,RACB)                                  0654 01042000
         CLC   AMBLDTA(4,@15),AMBLDTA(@01)                         0654 01043000
         BE    @RF00654                                            0654 01044000
         TM    MWAOUTSW,B'10000000'                                0654 01045000
         BNZ   @RF00654                                            0654 01046000
*         DO;                                                      0655 01047000
*           RESPECIFY                                              0656 01048000
*             IDAAMBL BASED(MWAMBLPT);                             0656 01049000
*           MWAMBLPT=VATAMBL(MWAINDEX);/* POINT TO AMBL IN VAT       */ 01050000
         LR    MWAMBLPT,@15                                        0657 01051000
*           IF((AMBLIX^=ZERO&       /* INDEX AMB EXIST AND           */ 01052000
*               AMBLIX->AMBCACB=AMBCACB&/* INDEX AMB'S CATALOG ACB 0658 01053000
*                                      MATCH                         */ 01054000
*               AMBLIX->AMBDDSN=AMBDDSN)|/* AND DATA SET NAME MATCH     01055000
*                                      OR                            */ 01056000
*               (AMBLDTA->AMBCACB=AMBCACB&/* DATA AMB'S CATALOG ACB     01057000
*                                      MATCH                         */ 01058000
*               AMBLDTA->AMBDDSN=AMBDDSN)) THEN/* AND DATA SET NAME     01059000
*                                      MATCH                         */ 01060000
