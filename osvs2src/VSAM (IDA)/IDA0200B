@EL00018 DS    0H                                                  1147 02663000
@EF00018 DS    0H                                                  1147 02664000
@ER00018 BR    @14                                                 1147 02665000
*FREEHEB:                                                          1148 02666000
*                                                                  1148 02667000
*/********************************************************************/ 02668000
*/*                                                                  */ 02669000
*/*      01 FUNCTION - FREE STORAGE POINTED TO BY HEADER ELEMENT     */ 02670000
*/*                    BLOCK                                         */ 02671000
*/*      01 INPUTS -   1. ADDR OF FIRST HEB OF HEB CHAIN IN MWAHEBPT */ 02672000
*/*      01 OUTPUTS -  1. STORAGE POINTED TO BY HEB'S WERE FREED     */ 02673000
*/*                    2. THE FREED HEB'S WERE CHAINED TO THE BIB    */ 02674000
*/*                                                                  */ 02675000
*/* IN VS2 THIS ROUTINE IS ALTERED TO DETECT KEY 5 AND KEY 7        .*/ 02676000
*/* CORE. MWAFRKEY IS THEN SET FOR THE FREECORE ROUTINE.            .*/ 02677000
*/********************************************************************/ 02678000
*                                                                  1148 02679000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1148 02680000
FREEHEB  DS    0H                                                  1149 02681000
*   RESPECIFY                                                      1149 02682000
*     RET RESTRICTED;                                              1149 02683000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 02684000
         ST    RET,MWASAVEB                                        1150 02685000
*   RESPECIFY                                                      1151 02686000
*     RET UNRESTRICTED;                                            1151 02687000
*   DO WHILE MWAHEBPT^=ZERO;        /* FREE HEB CHAIN                */ 02688000
         B     @DE01152                                            1152 02689000
@DL01152 DS    0H                                                  1153 02690000
*     MWADDRES=HEBBLKPT;            /* POINT TO CORE TO BE FREED     */ 02691000
         L     @08,HEBBLKPT(,MWAHEBPT)                             1153 02692000
         ST    @08,MWADDRES                                        1153 02693000
*     MWASPOOL=HEBSP;               /* SUBPOOL NUMBER                */ 02694000
         IC    @08,HEBSP(,MWAHEBPT)                                1154 02695000
         STC   @08,MWASPOOL                                        1154 02696000
*     MWABYTES=HEBLN;               /* LENGTH OF CORE TO BE FREED    */ 02697000
         MVC   MWABYTES(3),HEBLN(MWAHEBPT)                         1155 02698000
*     IF HEBKEY5=ON THEN            /* IS CORE IN KEY 5 CORE?      1156 02699000
*                                                            @Y30DSMD*/ 02700000
         TM    HEBKEY5(MWAHEBPT),B'01000000'                       1156 02701000
         BNO   @RF01156                                            1156 02702000
*       MWAFRKEY=K50;               /* YES, SET SW FOR FREECORE.   1157 02703000
*                                                            @Y30DSMD*/ 02704000
         MVI   MWAFRKEY,X'50'                                      1157 02705000
*     ELSE                          /* IF NOT KEY 5 CORE     @Z30SSRL*/ 02706000
*       IF HEBGSRKT=ON THEN         /* IF GSR BLOCK THEN     @Z40ESSR*/ 02707000
         B     @RC01156                                            1158 02708000
@RF01156 TM    HEBGSRKT(MWAHEBPT),B'00100000'                      1158 02709000
         BNO   @RF01158                                            1158 02710000
*         MWAFRKEY=HEBGSRK;         /* MOVE GSR USER KEY     @Z40ESSR*/ 02711000
*                                                                  1159 02712000
         IC    @08,HEBGSRK(,MWAHEBPT)                              1159 02713000
         STC   @08,MWAFRKEY                                        1159 02714000
*     /***************************************************************/ 02715000
*     /*                                                             */ 02716000
*     /* IF NOT KEY 5 & NOT GSR THEN DEFAULT TO KEY 0        @Z40ESSR*/ 02717000
*     /*                                                             */ 02718000
*     /***************************************************************/ 02719000
*                                                                  1160 02720000
*     CALL FREECORE;                /* FREE CORE INDICATED IN HEB    */ 02721000
@RF01158 DS    0H                                                  1160 02722000
@RC01156 BAL   @14,FREECORE                                        1160 02723000
*     MWASVPTR=HEBELCHN;            /* SAVE POINTER TO NEXT HEB      */ 02724000
         L     @08,HEBELCHN(,MWAHEBPT)                             1161 02725000
         ST    @08,MWASVPTR                                        1161 02726000
*     HEBHDREL=HEBHDREL&&HEBHDREL;  /* ZERO OUT HEB ELEMENT          */ 02727000
         XC    HEBHDREL(24,MWAHEBPT),HEBHDREL(MWAHEBPT)            1162 02728000
*     HEBELCHN=BIBHEBFQ;            /* CHAIN TO FREE HEB CHAIN       */ 02729000
         L     @01,ACBAMBL(,RACB)                                  1163 02730000
         L     @15,AMBLBIB(,@01)                                   1163 02731000
         L     @14,BIBHEBFQ(,@15)                                  1163 02732000
         ST    @14,HEBELCHN(,MWAHEBPT)                             1163 02733000
*     BIBHEBFQ=MWAHEBPT;            /* CHAIN UP WITH BIB             */ 02734000
         ST    MWAHEBPT,BIBHEBFQ(,@15)                             1164 02735000
*     MWAHEBPT=MWASVPTR;            /* POINT TO NEXT HEB             */ 02736000
         LR    MWAHEBPT,@08                                        1165 02737000
*   END;                                                           1166 02738000
@DE01152 LTR   MWAHEBPT,MWAHEBPT                                   1166 02739000
         BNZ   @DL01152                                            1166 02740000
*   RESPECIFY                                                      1167 02741000
*     RET RESTRICTED;                                              1167 02742000
*   RET=MWASAVEB;                   /* GET RETURN ADDRESS            */ 02743000
         L     RET,MWASAVEB                                        1168 02744000
*   RESPECIFY                                                      1169 02745000
*     RET UNRESTRICTED;                                            1169 02746000
*   END FREEHEB;                                                   1170 02747000
@EL00019 DS    0H                                                  1170 02748000
@EF00019 DS    0H                                                  1170 02749000
@ER00019 BR    @14                                                 1170 02750000
*PAGEFREE:                                                         1171 02751000
*                                                                  1171 02752000
