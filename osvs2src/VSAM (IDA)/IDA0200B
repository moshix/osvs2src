@RF00402 DS    0H                                                  0404 00230000
@RF00401 L     @05,CLWAMBPT(,RCORE)                                0404 00231000
         L     MWADEBPT,AMBDEBAD-1(,@05)                           0404 00232000
         LA    MWADEBPT,0(,MWADEBPT)                               0404 00233000
*           CALL DEHOOK;            /* DECHAIN DEB,DELETE FROM DEB 0405 00234000
*                                      TABLE                         */ 00235000
         BAL   @14,DEHOOK                                          0405 00236000
*           IF AMBLIX^=ZERO THEN    /* IF KSDS                       */ 00237000
         L     @05,ACBAMBL(,RACB)                                  0406 00238000
         L     @05,AMBLIX(,@05)                                    0406 00239000
         LTR   @05,@05                                             0406 00240000
         BZ    @RF00406                                            0406 00241000
*             DO;                                                  0407 00242000
*               CLWAMBPT=AMBLIX;    /* POINT TO INDEX AMB            */ 00243000
         ST    @05,CLWAMBPT(,RCORE)                                0408 00244000
*               MWADEBPT=AMBDEBAD;  /* GET ADDRESS OF DEB            */ 00245000
         L     MWADEBPT,AMBDEBAD-1(,@05)                           0409 00246000
         LA    MWADEBPT,0(,MWADEBPT)                               0409 00247000
*               CALL DEHOOK;        /* DECHAIN DEB,DELETE FROM DEB 0410 00248000
*                                      TABLE                         */ 00249000
         BAL   @14,DEHOOK                                          0410 00250000
*             END;                                                 0411 00251000
*         END;                                                     0412 00252000
@RF00406 DS    0H                                                  0413 00253000
*       CLWAMBPT=ZERO;              /* ZERO CURRENT AMB POINTER IN 0413 00254000
*                                      CLW                           */ 00255000
@RF00398 SLR   @05,@05                                             0413 00256000
         ST    @05,CLWAMBPT(,RCORE)                                0413 00257000
*       IF CLWPRMCL=ON&             /* PRIMARY CLOSE AND BIB SHOWS   */ 00258000
*           BIBVIRT=ON THEN         /* VIRTUAL DEVICES               */ 00259000
         TM    CLWPRMCL(RCORE),B'10000000'                         0414 00260000
         BNO   @RF00414                                            0414 00261000
         L     @05,ACBAMBL(,RACB)                                  0414 00262000
         L     @05,AMBLBIB(,@05)                                   0414 00263000
         TM    BIBVIRT(@05),B'10000000'                            0414 00264000
         BNO   @RF00414                                            0414 00265000
*         CALL VIRTPROC;            /* PERFORM NECESSARY DESTAGES    */ 00266000
         BAL   @14,VIRTPROC                                        0415 00267000
*       CALL CBRELE;                /* RELEASE CONTROL BLOCKS        */ 00268000
@RF00414 BAL   @14,CBRELE                                          0416 00269000
*     END;                                                         0417 00270000
*LTRM200B:                                                         0418 00271000
*   MWASPOOL=WKPOOL;                /* SUBPOOL NUMBER                */ 00272000
@RF00396 DS    0H                                                  0418 00273000
LTRM200B MVI   MWASPOOL,X'FC'                                      0418 00274000
*   RESPECIFY                                                      0419 00275000
*    (R0,                                                          0419 00276000
*     R1) RESTRICTED;                                              0419 00277000
*   GEN(L R1,@SIZDATD);             /* GET SIZE OF AUTOMATICS        */ 00278000
         L R1,@SIZDATD                                                  00279000
*   MWABYTES=R1;                    /* NUMBER OF BYTES TO BE FREED   */ 00280000
         STCM  R1,7,MWABYTES                                       0421 00281000
*   R1=R13;                         /* GET ADDRESS OF MWA            */ 00282000
         LR    R1,R13                                              0422 00283000
*   R0=MWAMNWRD;                    /* GET SUBPOOL NO. AND NO. OF  0423 00284000
*                                      BYTES                         */ 00285000
         L     R0,MWAMNWRD                                         0423 00286000
*   GEN(L R13,4(R13));              /* SAVE ADDR OF PREVIOUS SAVE  0424 00287000
*                                      AREA                          */ 00288000
         L R13,4(R13)                                                   00289000
*   GEN(FREEMAIN R,LV=(R0),A=(R1)); /* FREE MODULE WORK AREA         */ 00290000
         FREEMAIN R,LV=(R0),A=(R1)                                      00291000
*   RESPECIFY                                                      0426 00292000
*    (R0,                                                          0426 00293000
*     R1) UNRESTRICTED;                                            0426 00294000
*ENDLABEL:                          /* EXIT PROCESSING               */ 00295000
*   RESPECIFY                                                      0427 00296000
*     RETCODE RESTRICTED;                                          0427 00297000
ENDLABEL DS    0H                                                  0428 00298000
*   IF CLWTERM=ON THEN              /* IF TERMINATING ERROR          */ 00299000
         TM    CLWTERM(RCORE),B'00010000'                          0428 00300000
         BNO   @RF00428                                            0428 00301000
*     RETCODE=FOUR;                 /* INDICATE TERMINATING ERROR    */ 00302000
         LA    RETCODE,4                                           0429 00303000
*   ELSE                            /* IF NO TERMINATING ERROR       */ 00304000
*     RETCODE=ZERO;                 /* INDICATE NO TERMINATING ERROR */ 00305000
         B     @RC00428                                            0430 00306000
@RF00428 SLR   RETCODE,RETCODE                                     0430 00307000
*   GEN(ST RETCODE,16(R13));        /* PUT RETURN CODE IN CALLER'S 0431 00308000
*                                      REGISTERS SAVE AREA           */ 00309000
@RC00428 DS    0H                                                  0431 00310000
         ST RETCODE,16(R13)                                             00311000
*   RESPECIFY                                                      0432 00312000
*     RETCODE UNRESTRICTED;                                        0432 00313000
*   GEN(LM RET,R12,12(R13));        /* RESTORE REGISTERS             */ 00314000
         LM RET,R12,12(R13)                                             00315000
*   GEN(BR RET);                    /* EXIT MODULE                   */ 00316000
         BR RET                                                         00317000
*INIT200B:                                                         0435 00318000
*                                                                  0435 00319000
*/********************************************************************/ 00320000
*/*                                                                  */ 00321000
*/*      01 FUNCTION - MODULE INITIALIZATION, BUILD MODULE WORK AREA */ 00322000
*/*      01 INPUTS -   1. CORE SIZE FOR AUTOMATICS                   */ 00323000
*/*      01 OUTPUTS -  1. MODULE WORK AREA BUILT AND ENQUEUE         */ 00324000
*/*                       PARAMETER LIST INITIALIZED.                */ 00325000
*/*                                                                  */ 00326000
*/********************************************************************/ 00327000
*                                                                  0435 00328000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0435 00329000
INIT200B DS    0H                                                  0436 00330000
*   RESPECIFY                                                      0436 00331000
*    (R9,                                                          0436 00332000
*     RET,                                                         0436 00333000
*     RETCODE) RESTRICTED;                                         0436 00334000
*   RSTATRTN=IDAR200B;              /* NOTE THE MODULE IN CONTROL. 0437 00335000
*                                                            @Z30SSMD*/ 00336000
         L     @08,CLWCOMWK(,RCORE)                                0437 00337000
         MVC   RSTATRTN(4,@08),@CC03816                            0437 00338000
*   R9=RET;                         /* SAVE RETURN ADDRESS           */ 00339000
         LR    R9,RET                                              0438 00340000
*/********************************************************************/ 00341000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 00342000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 00343000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 00344000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 00345000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 00346000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 00347000
*/********************************************************************/ 00348000
*                                                                  0439 00349000
*   GEN;                                                           0439 00350000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00351000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00352000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00353000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00354000
*                                       IF NO ERROR WAS ENCOUNTERED     00355000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00356000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00357000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00358000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00359000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00360000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00361000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00362000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00363000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00364000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00365000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00366000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00367000
         B     GMN00200                 BYPASS SETTING ERROR            00368000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00369000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00370000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00371000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00372000
*   IF RETCODE^=ZERO THEN           /* GETMAIN SUCCESSFUL            */ 00373000
         LTR   RETCODE,RETCODE                                     0440 00374000
         BZ    @RF00440                                            0440 00375000
*     DO;                                                          0441 00376000
*       GEN(ST R13,4(RETCODE));     /* SAVE PREVIOUS SAVEAREA ADDRESS*/ 00377000
         ST R13,4(RETCODE)                                              00378000
*       R13=RETCODE;                /* POINT TO MODULE WORK AREA     */ 00379000
         LR    R13,RETCODE                                         0443 00380000
*       GEN(USING @DATD,R13);       /* ESTABLISH ADDRESSABILITY      */ 00381000
         USING @DATD,R13                                                00382000
*       CLWGMAIN=ON;                /* INDICATE GETMAIN SUCCESSFUL   */ 00383000
         OI    CLWGMAIN(RCORE),B'00100000'                         0445 00384000
*     END;                                                         0446 00385000
*   ELSE                            /* GETMAIN FAILED                */ 00386000
*     DO;                                                          0447 00387000
         B     @RC00440                                            0447 00388000
@RF00440 DS    0H                                                  0448 00389000
*       PDPACB->ACBERFLG=OPERR136;  /* SET USER ACB ERROR FLAG       */ 00390000
         LA    @08,280                                             0448 00391000
         AL    @08,CLWCOMWK(,RCORE)                                0448 00392000
         L     @05,PDPACB(,@08)                                    0448 00393000
         MVI   ACBERFLG(@05),X'88'                                 0448 00394000
*       PDPCODE=PP20;               /* SET PROBLEM CODE              */ 00395000
         MVI   PDPCODE(@08),X'14'                                  0449 00396000
*       CLWTERM=ON;                 /* INDICATE TERMINATING ERROR    */ 00397000
         OI    CLWTERM(RCORE),B'00010000'                          0450 00398000
*       CALL PROBDT;                /* PROBLEM DETERMINATION       0451 00399000
*                                      PROCESSING                    */ 00400000
         BAL   @14,PROBDT                                          0451 00401000
*       RETURN TO ENDLABEL;         /* EXIT MODULE                   */ 00402000
         LA    @14,ENDLABEL                                        0452 00403000
@EL00002 DS    0H                                                  0452 00404000
@EF00002 DS    0H                                                  0452 00405000
@ER00002 BR    @14                                                 0452 00406000
*     END;                                                         0453 00407000
*   MWAQNAME='SYSVSAM ';            /* SET QNAME FOR ENQ/DEQ         */ 00408000
@RC00440 MVC   MWAQNAME(8),@CC04976                                0454 00409000
*   ENQTCB=TCBJSTCB;                /* GET JOB STEP TCB ADDRESS    0455 00410000
*                                                            @Z30SSMD*/ 00411000
         L     @08,PSATOLD                                         0455 00412000
         L     @08,TCBJSTCB(,@08)                                  0455 00413000
         ST    @08,ENQTCB                                          0455 00414000
*   ENQQNADR=ADDR(MWAQNAME);        /* GET QNAME ADDRESS             */ 00415000
         LA    @08,MWAQNAME                                        0456 00416000
         ST    @08,ENQQNADR                                        0456 00417000
*   ENQRLEN=LENGTH(MWARNAME);       /* LENGTH OF RNAME               */ 00418000
