         L     R14,IIRSAVE2(,RCORE)                                0490 01428000
*   RESPECIFY                                                      0491 01429000
*    (R0) UNRESTRICTED;                                            0491 01430000
*   RESPECIFY                                                      0492 01431000
*    (RLKRETSV,                                                    0492 01432000
*     RGMSP,                                                       0492 01433000
*     RGMLEN,                                                      0492 01434000
*     RFMADDR) UNRESTRICTED;        /*                       @Y021130*/ 01435000
*   END FREEMAIN;                                                  0493 01436000
@EL00011 DS    0H                                                  0493 01437000
