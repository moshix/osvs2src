*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00270000
**                                                                    * 00271000
**   FUNCTION = DELETE ROUTINE                                        * 00272000
**       THIS ROUTINE DELETES THE ISAM INTERFACE PROCESSING AND       * 00273000
**       SYNAD MODULES AND ZEROS THE DCB VECTORS THAT POINT           * 00274000
**       TO THE ROUTINES.  THE ISAM SYNAD ROUTINE SPECIFIED IN THE    * 00275000
**       AMP PARAMETER IS ALSO DELETED.                               * 00276000
**                                                                    * 00277000
**   INPUT =                                                          * 00278000
**       REGISTER 1 - RDCB - DCB POINTER                              * 00279000
**       REGISTER 2 - RIICB - IICB POINTER                            * 00280000
**                                                                    * 00281000
**   OUTPUT =                                                         * 00282000
**       REGISTERS - SAME AS ABOVE                                    * 00283000
**                                                                    * 00284000
**       CONTROL BLOCK FIELDS INITIALIZED                             * 00285000
**         DCBESETL                                                   * 00286000
**         DCBFREED                                                   * 00287000
**         DCBIIGET                                                   * 00288000
**         DCBIIPUT                                                   * 00289000
**         DCBLRAN                                                    * 00290000
**         DCBLWKN                                                    * 00291000
**         DCBSETL                                                    * 00292000
**         DCBSYNAD                                                   * 00293000
**                                                                    * 00294000
**   PROTECTION KEY = SEE BELOW                                       * 00295000
**       KEY ON ENTRY - USER KEY                                      * 00296000
**       KEY ON EXIT - USER KEY                                       * 00297000
**       INTERIM KEYS ASSUMED = NONE                                  * 00298000
**                                                                    * 00299000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00300000
*                                                                  0222 00301000
*DELETRTN:                                                         0222 00302000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0222 00303000
DELETRTN DS    0H                                                  0223 00304000
*   RESPECIFY                                                      0223 00305000
*     RWORK2 RESTRICTED;                                           0223 00306000
*   RESPECIFY                                                      0224 00307000
*     IHADCB BASED(RWORK2);                                        0224 00308000
*   RESPECIFY                                                      0225 00309000
*    (RLKRETSV) RESTRICTED;         /*                       @Y021130*/ 00310000
*   RLKRETSV=R14;                   /* SAVE RET ADDRESS      @Y021130*/ 00311000
         LR    RLKRETSV,R14                                        0226 00312000
*   RWORK2=RDCB;                    /* SET DCB BASE                  */ 00313000
*                                                                  0227 00314000
         LR    RWORK2,RDCB                                         0227 00315000
*   /*****************************************************************/ 00316000
*   /*                                                               */ 00317000
*   /* AUDIT - INDICATE THAT DELETE OF IDAIISM1 WAS ATTEMPTED        */ 00318000
*   /*                                                               */ 00319000
*   /*****************************************************************/ 00320000
*                                                                  0228 00321000
*   AUDIISYN=OFF;                   /* INDICATOR OFF         @Y021130*/ 00322000
         NI    AUDIISYN(RIICB),B'11111101'                         0228 00323000
*   GEN(DELETE EP=IDAIISM1);        /* DELETE II SYNAD               */ 00324000
         DELETE EP=IDAIISM1                                             00325000
*   IF DCBMFGET=ON THEN             /* IF QISAM SCAN MODE THEN       */ 00326000
         TM    DCBMFGET+40(RWORK2),B'01000000'                     0230 00327000
         BNO   @RF00230                                            0230 00328000
*     DO;                                                          0231 00329000
*                                                                  0231 00330000
*       /*************************************************************/ 00331000
*       /*                                                           */ 00332000
*       /* AUDIT - INDICATE THAT DELETE OF IDAIIPM2 WAS ATTEMPTED    */ 00333000
*       /*                                                           */ 00334000
*       /*************************************************************/ 00335000
*                                                                  0232 00336000
*       AUDPRMOD=MODPRDEL;          /* INDICATOR OFF         @Y021130*/ 00337000
         NI    AUDPRMOD(RIICB),B'11110011'                         0232 00338000
*       GEN(DELETE EP=IDAIIPM2);    /* DELETE II QISAM SCAN RTN      */ 00339000
         DELETE EP=IDAIIPM2                                             00340000
*       DCBIIGET=ZERO;              /* CLEAR GET ADDR IN DCB         */ 00341000
         SLR   @15,@15                                             0234 00342000
         STCM  @15,7,DCBIIGET+48(RWORK2)                           0234 00343000
*       DCBSETL=ONE;                /* CLEAR SETL ADDR IN DCB        */ 00344000
         LA    @15,1                                               0235 00345000
         LA    @05,52                                              0235 00346000
         ALR   @05,RWORK2                                          0235 00347000
         ST    @15,DCBSETL(,@05)                                   0235 00348000
*       DCBESETL=ONE;               /* CLEAR ESETL ADDR IN DCB       */ 00349000
         ST    @15,DCBESETL(,@05)                                  0236 00350000
*     END;                                                         0237 00351000
*   ELSE                                                           0238 00352000
*     IF DCBMFPUT=ON THEN           /* IF QISAM PUT                  */ 00353000
         B     @RC00230                                            0238 00354000
@RF00230 TM    DCBMFPUT+40(RWORK2),B'01000000'                     0238 00355000
         BNO   @RF00238                                            0238 00356000
*       DO;                                                        0239 00357000
*                                                                  0239 00358000
*         /***********************************************************/ 00359000
*         /*                                                         */ 00360000
*         /* AUDIT - INDICATE THAT DELETE OF IDAIIPM1 WAS ATTEMPTED  */ 00361000
*         /*                                                         */ 00362000
*         /***********************************************************/ 00363000
*                                                                  0240 00364000
*         AUDPRMOD=MODPRDEL;        /* INDICATOR OFF         @Y021130*/ 00365000
         NI    AUDPRMOD(RIICB),B'11110011'                         0240 00366000
