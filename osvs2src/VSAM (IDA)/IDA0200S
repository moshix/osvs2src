**                                                                    * 01296000
**   FUNCTION = FREE WORK AREAS                                       * 01297000
**       THIS ROUTINE ISSUES A CALL TO THE FREEMAIN ROUTINE TO FREE   * 01298000
**       THE CORE OBTAINED BY THE ISAM INTERFACE OPEN MODULE FOR      * 01299000
**       THE IICB, ACB AND EXLST                                      * 01300000
**                                                                    * 01301000
**   INPUT =                                                          * 01302000
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           * 01303000
**                                                                    * 01304000
**   OUTPUT =                                                         * 01305000
**       REGISTERS - SAME AS ABOVE                                    * 01306000
**                                                                    * 01307000
**       REGISTERS - PASSED TO FREEMAIN SUBROUTINE                    * 01308000
**         RFMADDR - R7 - ADDRESS OF CORE TO BE FREED                 * 01309000
**         RGMSP - R8 - SUBPOOL NUMBER FOR FREEMAIN                   * 01310000
**         RGMLEN - R9 - LENGTH OF CORE FOR FREEMAIN                  * 01311000
**                                                                    * 01312000
**   PROTECTION KEY = SEE BELOW                                       * 01313000
**       KEY ON ENTRY - USER KEY                                      * 01314000
**       KEY ON EXIT - USER KEY                                       * 01315000
**       INTERIM KEYS ASSUMED = NONE                                  * 01316000
**                                                                    * 01317000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01318000
*                                                                  0462 01319000
*FREEWA:                                                           0462 01320000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0462 01321000
FREEWA   DS    0H                                                  0463 01322000
*   RESPECIFY                                                      0463 01323000
*    (RLKRETSV,                                                    0463 01324000
*     RGMSP,                                                       0463 01325000
*     RGMLEN,                                                      0463 01326000
*     RFMADDR) RESTRICTED;          /*                       @Y021130*/ 01327000
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR      @Y021130*/ 01328000
         LR    RLKRETSV,R14                                        0464 01329000
*   RGMLEN=LENGTH(IDAIICB)+         /* INIT LENGTH OF IICB PLUS      */ 01330000
*       LENGTH(IFGACB)+             /* ACB PLUS                      */ 01331000
*       20;                         /* EXIT LIST                     */ 01332000
         LA    RGMLEN,264                                          0465 01333000
*   RGMSP=USERPOOL;                 /* INIT SUBPOOL          @Y021130*/ 01334000
         LA    RGMSP,250                                           0466 01335000
*   RFMADDR=RIICB;                  /* INIT ADDRESS OF IICB,ACB    0467 01336000
*                                                            @Y021130*/ 01337000
         LR    RFMADDR,RIICB                                       0467 01338000
*   DO;                             /* MODESET EXTKEY(DATAMGT)       */ 01339000
*     SPKA(80);                     /* SET PSW KEY                   */ 01340000
         LA    @15,80                                              0469 01341000
         SPKA  0(@15)                                              0469 01342000
*   END;                            /* MODESET EXTKEY(DATAMGT)     0470 01343000
*                                                            @Y021130*/ 01344000
*                                                                  0470 01345000
*   /*****************************************************************/ 01346000
*   /*                                                               */ 01347000
*   /* AUDIT - INDICATE THAT AUDIT NO LONGER ACTIVE          @Y021130*/ 01348000
*   /*                                                               */ 01349000
*   /*****************************************************************/ 01350000
*                                                                  0471 01351000
*   IIAUDPTR=AUDEND;                /* MARK WITH X'FFFFFFFF' @Y021130*/ 01352000
         MVC   IIAUDPTR(4,RCORE),@CF02234                          0471 01353000
*   CALL FREEMAIN;                  /* CALL FREEMAIN                 */ 01354000
         BAL   @14,FREEMAIN                                        0472 01355000
