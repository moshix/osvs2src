*       RESPECIFY                                                  0210 00238000
*         IHADCB BASED(RDCB);       /* USER DCB              @Y021130*/ 00239000
*       DO;                         /* MODESET KEYADDR(DXUKEY)       */ 00240000
*         SPKA(DXUKEY);             /* SET PSW KEY                   */ 00241000
         SLR   @07,@07                                             0212 00242000
         IC    @07,DXUKEY(,RCORE)                                  0212 00243000
         SPKA  0(@07)                                              0212 00244000
*       END;                        /* MODESET KEYADDR(DXUKEY) RESUME   00245000
*                                      USER KEY              @Y021130*/ 00246000
*       RDCB=DXUDCBAD;              /* RESTORE DCB PTR       @Y021130*/ 00247000
         L     RDCB,DXUDCBAD(,RCORE)                               0214 00248000
*     END;                                                         0215 00249000
*   IF AMDNLR>ZERO THEN             /* IF DATA SET HAS RECORDS       */ 00250000
@RF00185 L     @07,ACBAMBL(,RACB)                                  0216 00251000
         L     @07,AMBLDTA(,@07)                                   0216 00252000
         L     @07,AMBDSB(,@07)                                    0216 00253000
         L     @07,AMDNLR(,@07)                                    0216 00254000
         LTR   @07,@07                                             0216 00255000
         BNP   @RF00216                                            0216 00256000
*     DCBSTLOD=ON;                  /* TURN ON RESUME LOAD BIT       */ 00257000
         OI    DCBSTLOD+52(RDCB),B'00100000'                       0217 00258000
