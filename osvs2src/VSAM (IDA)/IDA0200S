**                                                                    * 00660000
**   FUNCTION = CLOSE SYNAD                                           * 00661000
**       THIS ROUTINE IS USED AS A SYNAD EXIT FOR ERRORS OCCURRING    * 00662000
**       DURING CLOSE ON THE ACB.                                     * 00663000
**                                                                    * 00664000
**   INPUT =                                                          * 00665000
**       REGISTER 1 - RPL POINTER (THE RPL THAT VSAM CLOSE BUILDS)    * 00666000
**       REGISTER 2-12 - CONTENTS OF REGISTERS AS THEY EXISTED WHEN   * 00667000
**                       THE CLOSE WAS ISSUED (SEE CLOSEACB ROUTINE). * 00668000
**       REGISTER 13 - SAVE AREA ADDRESS (SAVE AREA PROVIDED BY       * 00669000
**                     VSAM CLOSE FOR RECORD MANAGEMENT).             * 00670000
**       REGISTER 14 - RETURN ADDRESS                                 * 00671000
**       REGISTER 15 - ADDR OF CLOSESYN ROUTINE. NOTE THAT REG 12     * 00672000
**                     CONTAINS THE BASE ADDRESS.                     * 00673000
**                                                                    * 00674000
**   OUTPUT = SEE BELOW                                               * 00675000
**       OUTPUT TO MODULE IDAIISM1 =                                  * 00676000
**            REGISTER 1 - RPL POINTER (ISAM INTERFACE RPL)           * 00677000
**            REGISTER 8 - RWORK3 - RETURN ADDR TO THIS MODULE        * 00678000
**            REGISTER 15 - ADDRESS OF MODULE IDAIISM1                * 00679000
**       OUTPUT ON RETURN TO VSAM CLOSE =                             * 00680000
**            REGISTER 1 - RPL POINTER (RPL THAT CLOSE BUILT)         * 00681000
**            REGISTER 13 - SAVE AREA POINTER THAT VSAM CLOSE PASSED  * 00682000
**            REGISTER 14 - RETURN ADDRESS                            * 00683000
**                                                                    * 00684000
**   PROTECTION KEY = SEE BELOW                                       * 00685000
**       KEY ON ENTRY - USER KEY                                      * 00686000
**       KEY ON EXIT - USER KEY                                       * 00687000
**       INTERIM KEYS ASSUMED = NONE                                  * 00688000
**                                                                    * 00689000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00690000
*                                                                  0322 00691000
*CLOSESYN:                                                         0322 00692000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @YM04657*/ 00693000
CLOSESYN DS    0H                                                  0323 00694000
*   RESPECIFY                                                      0323 00695000
*    (RWORK1,                                                      0323 00696000
*     RWORK2,                                                      0323 00697000
*     RWORK3,                                                      0323 00698000
*     R13,                                                         0323 00699000
*     R15) RESTRICTED;                                             0323 00700000
*   RRPL=R1;                        /* SAVE RPL ADDR PASSED  @YM04657*/ 00701000
         LR    RRPL,R1                                             0324 00702000
*   RWORK1=R14;                     /* SAVE RETURN ADDR      @YM04657*/ 00703000
         LR    RWORK1,R14                                          0325 00704000
*   RWORK2=DXUDCBAD;                /* GET DCB ADDR FROM WORK AREA 0326 00705000
*                                                            @YM04657*/ 00706000
         L     RWORK2,DXUDCBAD(,RCORE)                             0326 00707000
*   RESPECIFY                                                      0327 00708000
*     IHADCB BASED(RWORK2);         /*                       @YM04657*/ 00709000
*                                                                  0327 00710000
*   /*****************************************************************/ 00711000
*   /*                                                               */ 00712000
*   /* THE USER SYNAD EXIT WILL ONLY BE TAKEN FOR QISAM, AN I/O ERROR*/ 00713000
*   /* WAS DETECTED, AND A PREVIOUS EXIT WAS NOT TAKEN DURING CLOSE. */ 00714000
*   /*                                                       @YM04657*/ 00715000
*   /*                                                               */ 00716000
*   /*****************************************************************/ 00717000
*                                                                  0328 00718000
*   IF(DCBMFPUT=ON|DCBMFGET=ON)&RPLERREG='0C'X&DCBEXCLD=OFF THEN   0328 00719000
         LA    @10,40                                              0328 00720000
         ALR   @10,RWORK2                                          0328 00721000
         TM    DCBMFPUT(@10),B'01000000'                           0328 00722000
         BO    @GL00007                                            0328 00723000
         TM    DCBMFGET(@10),B'01000000'                           0328 00724000
         BNO   @RF00328                                            0328 00725000
@GL00007 CLI   RPLERREG(RRPL),12                                   0328 00726000
         BNE   @RF00328                                            0328 00727000
         TM    DCBEXCLD+52(RWORK2),B'00100000'                     0328 00728000
         BNZ   @RF00328                                            0328 00729000
*     DO;                           /*                       @YM04657*/ 00730000
*       R15=ADDR(IIREGSAV);         /* SAVE AREA IN IICB     @YM04657*/ 00731000
         LA    R15,IIREGSAV(,RIICB)                                0330 00732000
*       GEN(STM R14,R12,12(R15));   /* SAVE REGS IN IICB     @YM04657*/ 00733000
         STM R14,R12,12(R15)                                            00734000
*       R13=R15;                    /* USER MUST RETURN REG 13     0332 00735000
*                                                            @YM04657*/ 00736000
         LR    R13,R15                                             0332 00737000
*       IIRPLPTR->RPLFDBK=RPLFDBK;  /* COPY RPL PASSED TO OWN      0333 00738000
*                                                            @YM04657*/ 00739000
         L     @10,IIRPLPTR(,RIICB)                                0333 00740000
         MVC   RPLFDBK(3,@10),RPLFDBK(RRPL)                        0333 00741000
*       IIRPLPTR->RPLERMSA->IDAERMSG=IDAERMSG;/* COPY ERROR MESSAGE     00742000
*                                      TO OWN                @YM04657*/ 00743000
         L     @09,RPLERMSA(,@10)                                  0334 00744000
         L     @05,RPLERMSA(,RRPL)                                 0334 00745000
         MVC   IDAERMSG(128,@09),IDAERMSG(@05)                     0334 00746000
*       R15=RWORK3;                 /* ADDR OF MODULE IDAIISM1     0335 00747000
*                                                            @YM04657*/ 00748000
         LR    R15,RWORK3                                          0335 00749000
*       RWORK3=ADDR(SYNADRET);      /* RETURN TO THIS MODULE FROM  0336 00750000
*                                      USER EXIT             @YM04657*/ 00751000
         LA    RWORK3,SYNADRET                                     0336 00752000
*       R1=IIRPLPTR;                /* PASS OWN RPL          @YM04657*/ 00753000
         LR    R1,@10                                              0337 00754000
*       GO TO SYNAD;                /* GO ENTER MODULE IDAIISM1    0338 00755000
*                                                            @YM04657*/ 00756000
         BR    R15                                                 0338 00757000
*SYNADRET:                                                         0339 00758000
*       GEN(LM R14,R12,12(R13));    /* RESTORE OWN REGS      @YM04657*/ 00759000
SYNADRET LM R14,R12,12(R13)                                             00760000
*       R14=RWORK1;                 /* RETURN ADDR OF VSAM CLOSE   0340 00761000
*                                                            @YM04657*/ 00762000
         LR    R14,RWORK1                                          0340 00763000
*       R1=RRPL;                    /* ADDR OF RPL PASSED    @YM04657*/ 00764000
         LR    R1,RRPL                                             0341 00765000
*     END;                          /* END OF SYNAD EXIT     @YM04657*/ 00766000
*   RESPECIFY                                                      0343 00767000
*     IHADCB BASED(RDCB);           /*                       @YM04657*/ 00768000
*   RESPECIFY                                                      0344 00769000
*    (RWORK1,                                                      0344 00770000
*     RWORK2,                                                      0344 00771000
*     RWORK3,                                                      0344 00772000
*     R13,                                                         0344 00773000
*     R15) UNRESTRICTED;                                           0344 00774000
*   END CLOSESYN;                   /* END CLOSE SYNAD       @YM04657*/ 00775000
@EL00006 DS    0H                                                  0345 00776000
@EF00006 DS    0H                                                  0345 00777000
@ER00006 BR    @14                                                 0345 00778000
*                                                                  0346 00779000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00780000
**   FUNCTION = FREE BUFFERS                                          * 00781000
**       FREE THE CORE OBTAINED BY THE ISAM INTERFACE OPEN            * 00782000
**       MODULE FOR THE RPL, RPL EXTENSION, ERROR MESSAGE AREAS,      * 00783000
**       KEYSAVE AREA AND II BUFFERS.                                 * 00784000
**                                                                    * 00785000
**   INPUT =                                                          * 00786000
**       REGISTER 2 - RIICB - IICB POINTER                            * 00787000
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           * 00788000
**                                                                    * 00789000
**   OUTPUT =                                                         * 00790000
**       REGISTERS - SAME AS ABOVE                                    * 00791000
**                                                                    * 00792000
**       REGISTERS - PASSED TO FREEMAIN SUBROUTINE                    * 00793000
**         RFMADDR - R7 - ADDRESS OF CORE TO BE FREED                 * 00794000
**         RGMSP - R8 - SUBPOOL NUMBER FOR FREEMAIN                   * 00795000
**         RGMLEN - R9 - LENGTH OF CORE FOR FREEMAIN                  * 00796000
**                                                                    * 00797000
