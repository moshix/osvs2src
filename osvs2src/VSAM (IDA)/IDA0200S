         TITLE 'IDA0200S - MVM ISAM INTERFACE CLOSE                    *00001000
                        '                                               00002000
IDA0200S CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0200S  78.038'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0153 00014000
*   /*****************************************************************/ 00015000
*   /*                                                               */ 00016000
*   /* AUDIT - INDICATE AUDIT NOT STARTED                    @Y021130*/ 00017000
*   /*                                                               */ 00018000
*   /*****************************************************************/ 00019000
*                                                                  0154 00020000
*   IIAUDPTR=ZERO;                  /* ZERO IICB PTR         @Y021130*/ 00021000
         SLR   @15,@15                                             0154 00022000
         ST    @15,IIAUDPTR(,RCORE)                                0154 00023000
*   IIRSAVE=R13;                    /* PTR TO SAVE AREA              */ 00024000
         ST    R13,IIRSAVE(,RCORE)                                 0155 00025000
*   RESPECIFY                                                      0156 00026000
*     R13 UNRESTRICTED;             /*                       @Y021130*/ 00027000
*   RDCB=DXUDCBAD;                  /* INIT DCB POINTER TO DCB     0157 00028000
*                                                            @Y021130   00029000
*                                      COMMON CLOSE ISSUES A DEBCHK     00030000
*                                      VERIFY AND STORES THE DEB PTR    00031000
*                                      IN DCBDEBA                    */ 00032000
         L     RDCB,DXUDCBAD(,RCORE)                               0157 00033000
*   RESPECIFY                                                      0158 00034000
*     IHADCB BASED(DXPDCBAD);       /* USE COPY DCB          @Y021130*/ 00035000
*   RDEB=DCBDEBA;                   /* INIT DEB POINTER      @Y021130*/ 00036000
         L     @15,DXPDCBAD(,RCORE)                                0159 00037000
         L     RDEB,DCBDEBA+39(,@15)                               0159 00038000
         LA    RDEB,0(,RDEB)                                       0159 00039000
*   RESPECIFY                                                      0160 00040000
*     IHADCB BASED(RDCB);           /* USER DCB              @Y021130*/ 00041000
*   RIICB=DEBWKPT5;                 /* INIT IICBPOINTER              */ 00042000
*                                                                  0161 00043000
         L     @15,DEBEXPT(,RDEB)                                  0161 00044000
         L     RIICB,DEBWKPT5(,@15)                                0161 00045000
*   /*****************************************************************/ 00046000
*   /*                                                               */ 00047000
*   /* AUDIT - INITIALIZE POINTER TO IICB VIA A POINTER      @Y021130*/ 00048000
*   /* LOCATED IN THE OPEN CLOSE WORK AREA                   @Y021130*/ 00049000
*   /*                                                               */ 00050000
*   /*****************************************************************/ 00051000
*                                                                  0162 00052000
*   IIAUDPTR=RIICB;                 /* BASE FOR AUDIT        @Y021130*/ 00053000
         ST    RIICB,IIAUDPTR(,RCORE)                              0162 00054000
*   DO;                             /* MODESET KEYADDR(DXUKEY)       */ 00055000
*     SPKA(DXUKEY);                 /* SET PSW KEY                   */ 00056000
         SLR   @15,@15                                             0164 00057000
         IC    @15,DXUKEY(,RCORE)                                  0164 00058000
         SPKA  0(@15)                                              0164 00059000
*   END;                            /* MODESET KEYADDR(DXUKEY) ASSUME   00060000
*                                      USER KEY              @Y021130*/ 00061000
*   RACB=IIACBPTR;                  /* INIT ACB POINTER              */ 00062000
         L     RACB,IIACBPTR(,RIICB)                               0166 00063000
*   CALL FLUSHBFR;                  /* FLUSH BUFFERS                 */ 00064000
         BAL   @14,FLUSHBFR                                        0167 00065000
*   CALL CLOSEACB;                  /* CLOSE ACB             @YM04657*/ 00066000
         BAL   @14,CLOSEACB                                        0168 00067000
*   CALL DELETRTN;                  /* DELETE II PROCESSING RTNS   0169 00068000
*                                                            @YM04657*/ 00069000
         BAL   @14,DELETRTN                                        0169 00070000
*   CALL FREEBFRS;                  /* FREE II RPL'S AND BUFFERS     */ 00071000
         BAL   @14,FREEBFRS                                        0170 00072000
*   CALL FREEDEB;                   /* FREE THE II DEB               */ 00073000
         BAL   @14,FREEDEB                                         0171 00074000
*   CALL RESETDCB;                  /* RESET DCB TO PRE-OPEN COND    */ 00075000
         BAL   @14,RESETDCB                                        0172 00076000
*   CALL FREEWA;                    /* FREE II WORK AREAS            */ 00077000
         BAL   @14,FREEWA                                          0173 00078000
*   DO;                             /* MODESET EXTKEY(ZERO)          */ 00079000
*     SPKA(0);                      /* SET PSW KEY                   */ 00080000
         SLR   @15,@15                                             0175 00081000
         SPKA  0(@15)                                              0175 00082000
*   END;                            /* MODESET EXTKEY(ZERO) RESUME 0176 00083000
*                                      SYSTEM KEY FOR RETURN @Y021130*/ 00084000
*   PCOPYDCB=ISAMDCB;               /* REFRESH THE COPY WITH USER DCB   00085000
*                                      AS COMMON CLOSE REFRESHES THE    00086000
*                                      USER DCB FROM THE COPY      0177 00087000
*                                                            @Y021130*/ 00088000
         L     @15,DXPDCBAD(,RCORE)                                0177 00089000
         MVC   PCOPYDCB(236,@15),ISAMDCB+16(RDCB)                  0177 00090000
*   RESPECIFY                                                      0178 00091000
*     R13 RESTRICTED;                                              0178 00092000
*   R13=IIRSAVE;                    /* RESTORE SAVE AREA PTR         */ 00093000
         L     R13,IIRSAVE(,RCORE)                                 0179 00094000
*   RETURN;                                                        0180 00095000
@EL00001 DS    0H                                                  0180 00096000
@EF00001 DS    0H                                                  0180 00097000
@ER00001 LM    @14,@12,12(@13)                                     0180 00098000
         BR    @14                                                 0180 00099000
*                                                                  0181 00100000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00101000
**                                                                    * 00102000
**   FUNCTION = FLUSH BUFFER                                          * 00103000
**       WHEN LOCATE MODE IS BEING USED BY THE ISAM PROBLEM PROGRAM   * 00104000
**       THIS ROUTINE WILL PASS CONTROL TO THE ISAM INTERFACE         * 00105000
**       LOAD ROUTINE TO ISSUE THE LAST PUT MACRO TO VSAM.            * 00106000
**                                                                    * 00107000
**   INPUT =                                                          * 00108000
**       REGISTER 1 - RDCB - DCB POINTER                              * 00109000
**       REGISTER 2 - RIICB - IICB POINTER                            * 00110000
**       REGISTER 4 - RCORE - I/O SUPPORT WORK AREA POINTER           * 00111000
**       REGISTER 6 - RACB - ACB POINTER                              * 00112000
**                                                                    * 00113000
**   OUTPUT =                                                         * 00114000
**       REGISTERS - SAME AS ABOVE                                    * 00115000
**                                                                    * 00116000
**       CONTROL BLOCK FIELDS INITIALIZED                             * 00117000
**         DCBST                                                      * 00118000
**         IIFCLOSE                                                   * 00119000
**         XRBSAV                                                     * 00120000
**         DCBDEBAD (COPY DCB - FORMERLY DXPDEBAD)                    * 00121000
**                                                                    * 00122000
**   PROTECTION KEY = SEE BELOW                                       * 00123000
**       KEY ON ENTRY - USER KEY                                      * 00124000
**       KEY ON EXIT - USER KEY                                       * 00125000
**       INTERIM KEYS ASSUMED = DATA MANAGEMENT KEY 5                 * 00126000
**                              SYSTEM KEY ZERO                       * 00127000
**                                                                    * 00128000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00129000
*                                                                  0181 00130000
*FLUSHBFR:                                                         0181 00131000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0181 00132000
FLUSHBFR DS    0H                                                  0182 00133000
*   RESPECIFY                                                      0182 00134000
*     R15 RESTRICTED;                                              0182 00135000
*   RESPECIFY                                                      0183 00136000
*    (RLKRETSV) RESTRICTED;         /*                       @Y021130*/ 00137000
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR      @Y021130*/ 00138000
         LR    RLKRETSV,R14                                        0184 00139000
*   IF ACBOPEN=ON&                  /* IF ACB STILL OPEN COULD BE  0185 00140000
*                                      PREVIOUSLY CLOSED BY ABEND OR    00141000
*                                      TASK TERM                     */ 00142000
*       DCBMFLCP=ON&                /* AND PUT LOCATE                */ 00143000
*       DCBSTKSQ=ON&                /* NOT THE FIRST PUT ISSUED      */ 00144000
*       DCBEXCD1=ZERO&              /* AND NO ERRORS INDICATED       */ 00145000
*       DCBEXCD2=ZERO THEN          /* IN THE DCB THEN               */ 00146000
         TM    ACBOPEN(RACB),B'00010000'                           0185 00147000
         BNO   @RF00185                                            0185 00148000
         TM    DCBMFLCP+40(RDCB),B'00001000'                       0185 00149000
         BNO   @RF00185                                            0185 00150000
         LA    @07,52                                              0185 00151000
         ALR   @07,RDCB                                            0185 00152000
         TM    DCBSTKSQ(@07),B'01000000'                           0185 00153000
         BNO   @RF00185                                            0185 00154000
         CLI   DCBEXCD1(@07),0                                     0185 00155000
         BNE   @RF00185                                            0185 00156000
         CLI   DCBEXCD2(@07),0                                     0185 00157000
         BNE   @RF00185                                            0185 00158000
*     DO;                                                          0186 00159000
*       IIFCLOSE=ON;                /* TURN ON CLOSE BIT             */ 00160000
         OI    IIFCLOSE(RIICB),B'00010000'                         0187 00161000
*       R14=ADDR(SVRB);             /* GET ADDR OF SVRB              */ 00162000
         L     @07,CVTPTR                                          0188 00163000
         L     @07,CVTTCBDW(,@07)                                  0188 00164000
         L     @07,TCBPTR(,@07)                                    0188 00165000
         L     R14,SVRBPTR(,@07)                                   0188 00166000
*       R15=DCBIIPUT+26;            /* ADDR OF II PUT RTN PLUS     0189 00167000
*                                      DISPLACEMENT TO BRANCH IN PM1    00168000
*                                      AFTER STM INST AT BALR        */ 00169000
         LA    R15,26                                              0189 00170000
         L     @07,DCBIIPUT+47(,RDCB)                              0189 00171000
         LA    @07,0(,@07)                                         0189 00172000
         ALR   R15,@07                                             0189 00173000
*       DO;                         /* MODESET EXTKEY(ZERO)          */ 00174000
*         SPKA(0);                  /* SET PSW KEY                   */ 00175000
         SLR   @07,@07                                             0191 00176000
         SPKA  0(@07)                                              0191 00177000
*       END;                        /* MODESET EXTKEY(ZERO) SET KEY     00178000
*                                      ZERO FOR REG SAVE IN SVRB   0192 00179000
*                                                            @Y021130*/ 00180000
*       GEN SETS(XRBXSAV);                                         0193 00181000
           STM R2,R13,XRBXSAV(R14)      /* SAVE REGS IN EXTEND SAVE A*/ 00182000
*       DO;                         /* MODESET KEYADDR(DXUKEY)       */ 00183000
*         SPKA(DXUKEY);             /* SET PSW KEY                   */ 00184000
         SLR   @07,@07                                             0195 00185000
         IC    @07,DXUKEY(,RCORE)                                  0195 00186000
         SPKA  0(@07)                                              0195 00187000
*       END;                        /* MODESET KEYADDR(DXUKEY) GET 0196 00188000
*                                      INTO USER KEY         @Y021130*/ 00189000
*                                                                  0196 00190000
*       /*************************************************************/ 00191000
*       /*                                                           */ 00192000
*       /* AUDIT - INDICATE PROCESSING MOD IDAIIPM1 HAS BEEN ENTERED */ 00193000
*       /*                                                           */ 00194000
*       /*************************************************************/ 00195000
*                                                                  0197 00196000
*       AUDBFREX=ON;                /* FLAG IN IICB          @Y021130*/ 00197000
         OI    AUDBFREX(RIICB),B'00010000'                         0197 00198000
*       GEN;                                                       0198 00199000
*                                       /*                   @Y021130*/ 00200000
           SYNCH (15)                    /* GO TO II PUT             */ 00201000
*       R14=ADDR(SVRB);             /* GET ADDR OF SVRB              */ 00202000
         L     @07,CVTPTR                                          0199 00203000
         L     @07,CVTTCBDW(,@07)                                  0199 00204000
         L     @07,TCBPTR(,@07)                                    0199 00205000
         L     R14,SVRBPTR(,@07)                                   0199 00206000
*       GEN REFS(XRBXSAV);                                         0200 00207000
*                                                                  0200 00208000
*                                       /*                   @Y021130*/ 00209000
           LM  R2,R13,XRBXSAV(R14)      /* RELOAD REGISTERS          */ 00210000
*       /*************************************************************/ 00211000
*       /*                                                           */ 00212000
*       /* AUDIT - CONTROL RETURNED FROM PROCESSING MODULE IDAIIPM1  */ 00213000
*       /*                                                           */ 00214000
*       /*************************************************************/ 00215000
*                                                                  0201 00216000
*       AUDBFRRT=ON;                /* FLAG IN IICB          @Y021130*/ 00217000
         OI    AUDBFRRT(RIICB),B'00001000'                         0201 00218000
*       RDCB=DXUDCBAD;              /* RESTORE DCB PTR       @Y021130*/ 00219000
         L     RDCB,DXUDCBAD(,RCORE)                               0202 00220000
*       DO;                         /* MODESET EXTKEY(DATAMGT)       */ 00221000
*         SPKA(80);                 /* SET PSW KEY                   */ 00222000
         LA    @07,80                                              0204 00223000
         SPKA  0(@07)                                              0204 00224000
*       END;                        /* MODESET EXTKEY(DATAMGT) DATA     00225000
*                                      MANAGEMENT KEY FOR STORING  0205 00226000
*                                      INTO COPY DCB         @Y021130*/ 00227000
*       GEN;                                                       0206 00228000
           DEBCHK (1),TYPE=VERIFY,AM=ISAM /* VERIFY THE DEB POINTER  */ 00229000
*       RESPECIFY                                                  0207 00230000
*         IHADCB BASED(DXPDCBAD);   /* COPY DCB              @Y021130*/ 00231000
*       RDEB=R1;                    /* INIT DEB POINTER      @Y021130*/ 00232000
         LR    RDEB,R1                                             0208 00233000
*       DCBDEBA=RDEB;               /* STORE VALID DEB PTR IN WA   0209 00234000
*                                      PROTECTED DCB         @Y021130*/ 00235000
