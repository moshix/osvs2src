         TM    AMBLSCRA(@05),B'00001100'                           0446 00384000
         BZ    @RF00446                                            0446 00385000
@RT00446 DS    0H                                                  0447 00386000
*     CLWCNOUP=ON;                  /* INDICATE NO CATALOG UPDATE IN    00387000
*                                      CLW                           */ 00388000
         OI    CLWCNOUP(RCORE),B'01000000'                         0447 00389000
*   IF AMBLDUMY=OFF THEN            /* IF NOT A DUMMY DATA SET       */ 00390000
@RF00446 L     @05,MWAMBLPT                                        0448 00391000
         TM    AMBLDUMY(@05),B'00000001'                           0448 00392000
         BNZ   @RF00448                                            0448 00393000
*     DO;                                                          0449 00394000
*       IF AMBPSDS=ON THEN          /* IF THIS IS PAGE SPACE DATA SET   00395000
