*                                      GLOBAL.               @Z30SSMD*/ 03783000
*                                                                  1404 03784000
         BAL   @14,GSRDUMP                                         1404 03785000
*   /*****************************************************************/ 03786000
*   /*                                                               */ 03787000
*   /* OBTAIN SPACE FOR A P/L FOR IDA0192Y.                  @Y30DSMD*/ 03788000
*   /*                                                               */ 03789000
*   /*****************************************************************/ 03790000
*                                                                  1405 03791000
*   MWASPOOL=WKPOOL;                /* USE PROTECTED SUBPOOL.      1405 03792000
*                                                            @Y30DSMD*/ 03793000
         MVI   MWASPOOL,X'FC'                                      1405 03794000
*   MWABYTES=LENGTH(IDABLPRM);      /* COMPUTE THE LENGTH.   @Y30DSMD*/ 03795000
         MVC   MWABYTES(3),@CF04954+1                              1406 03796000
*   CALL GETCORE;                   /* DO THE GETMAIN.       @Y30DSMD*/ 03797000
         BAL   @14,GETCORE                                         1407 03798000
*   RFY                                                            1408 03799000
*     IDABLPRM BASED(MWADDRES);     /* POINT TO THE P/L.     @Y30DSMD*/ 03800000
*                                                                  1408 03801000
*   /*****************************************************************/ 03802000
*   /*                                                               */ 03803000
*   /* INITIALIZE THE P/L FOR IDA0192Y(DLVRP).               @Y30DSMD*/ 03804000
*   /*                                                               */ 03805000
*   /*****************************************************************/ 03806000
*                                                                  1409 03807000
*   BLPACBID=KACBID;                /* SET ACB ID.           @Y30DSMD*/ 03808000
         L     @15,MWADDRES                                        1409 03809000
