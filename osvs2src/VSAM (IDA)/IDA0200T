         ST    @11,MWADDRES                                        0819 01680000
*   MWABYTES=LENGTH(DEBXTN);        /* NOTE THE SIZE OF THE XTN.   0820 01681000
*                                                            @Y30DSMD*/ 01682000
         MVC   MWABYTES(3),@CF00508+1                              0820 01683000
*   IF MWACAT=OFF THEN              /* IF NOT CATALOG OR SCRA        */ 01684000
         TM    MWACAT,B'10000000'                                  0821 01685000
         BNZ   @RF00821                                            0821 01686000
*     DO;                           /* PROCESS THE NON-CTLG DDEB.  0822 01687000
*                                                            @Y30DSMD*/ 01688000
*       IF MWAGSR=ON|MWACBIC=ON THEN/* IS THIS GSR OR CBIC   @ZA16012*/ 01689000
         TM    MWAGSR,B'10000000'                                  0823 01690000
         BO    @RT00823                                            0823 01691000
         TM    MWACBIC,B'10000000'                                 0823 01692000
         BNO   @RF00823                                            0823 01693000
@RT00823 DS    0H                                                  0824 01694000
*         MWASPOOL=GSRSPOOL;        /* INDICATE GSR SUB-POOL.      0824 01695000
*                                                            @Y30DSMD*/ 01696000
         MVI   MWASPOOL,X'F1'                                      0824 01697000
*       ELSE                        /* IF NOT GSR DATA SET   @Z30SSRL*/ 01698000
*         MWASPOOL=DDEBPOOL;        /* SUBPOOL OF DEB        @Y30DSMD*/ 01699000
