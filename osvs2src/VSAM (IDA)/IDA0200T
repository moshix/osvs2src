*     RET RSTD;                     /* INFORM THE COMPILER.  @Y30DSMD*/ 03722000
*   MWASAVEE=RET;                   /* SAVE OFF THE RETURN ADDRESS.     03723000
*                                                            @Y30DSMD*/ 03724000
         ST    RET,MWASAVEE                                        1391 03725000
*   RFY                                                            1392 03726000
*     RET UNRSTD;                   /* INFORM THE COMPILER.  @Y30DSMD*/ 03727000
*   MWADDRES=MWAVGTT;               /* GET THE ADDRESS TO FREE.    1393 03728000
*                                                            @Y30DSMD*/ 03729000
