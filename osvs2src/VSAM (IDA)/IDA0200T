*         DO;                                                      0453 00409000
*           CLWNWRIT=ON;            /* SUPPRESS WRTBFR               */ 00410000
*           CLWBNOFL=ON;            /* SUPPRESS BUFFER FLUSH         */ 00411000
*           CLWCNOUP=ON;            /* SUPPRESS CATALOG UPDATE       */ 00412000
         OI    CLWNWRIT(RCORE),B'11100000'                         0456 00413000
*         END;                                                     0457 00414000
*       CALL ENQFUNC;               /* CALL ENQUEING FUNCTION        */ 00415000
@RF00452 BAL   @14,ENQFUNC                                         0458 00416000
*       MWAMBLPT=ACBAMBL;           /* GET AMBL POINTED TO BY USER 0459 00417000
*                                      ACB                           */ 00418000
         L     @05,ACBAMBL(,RACB)                                  0459 00419000
         ST    @05,MWAMBLPT                                        0459 00420000
*       IF AMBLPATH=ON THEN         /* IF THIS IS PATH AMBL          */ 00421000
         TM    AMBLPATH(@05),B'10000000'                           0460 00422000
         BNO   @RF00460                                            0460 00423000
*         MWAMBLPT=AMBLXPT;         /* POINT TO BASE AMBL            */ 00424000
         L     @05,AMBLXPT(,@05)                                   0461 00425000
         ST    @05,MWAMBLPT                                        0461 00426000
*       IF AMBLPRIM=ON&             /* PRIMARY AMBL AND              */ 00427000
*           AMBLSCHN=ZERO THEN      /* NO SECONDARY CHAIN            */ 00428000
@RF00460 L     @05,MWAMBLPT                                        0462 00429000
         TM    AMBLPRIM(@05),B'10000000'                           0462 00430000
