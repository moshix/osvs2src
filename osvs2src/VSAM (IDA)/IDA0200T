*   RESPECIFY                                                      0512 00591000
*     RET RESTRICTED;                                              0512 00592000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS OF CALLER */ 00593000
         ST    RET,MWASAVEB                                        0513 00594000
*   RESPECIFY                                                      0514 00595000
*     RET UNRESTRICTED;                                            0514 00596000
*   MWAMBLPT=ACBAMBL;               /* GET AMBL POINTED TO BY USER 0515 00597000
*                                      ACB                           */ 00598000
         L     @11,ACBAMBL(,RACB)                                  0515 00599000
         ST    @11,MWAMBLPT                                        0515 00600000
*   MWADSCNT=ZERO;                  /* TOTAL COUNT OF DATA SETS      */ 00601000
         SLR   @05,@05                                             0516 00602000
         ST    @05,MWADSCNT                                        0516 00603000
*   MWAQNAME='SYSVSAM ';            /* INITIALIZE THE QNAME          */ 00604000
         MVC   MWAQNAME(8),@CC05342                                0517 00605000
*   MWAPAMBL=ZERO;                  /* INITLZ PATH AMBL ADDR @ZA16012*/ 00606000
         SLR   MWAPAMBL,MWAPAMBL                                   0518 00607000
*   IF AMBLPATH=ON THEN             /* IF THIS IS A PATH AMBL        */ 00608000
         TM    AMBLPATH(@11),B'10000000'                           0519 00609000
         BNO   @RF00519                                            0519 00610000
*     DO;                                                          0520 00611000
*       MWADSCNT=MWADSCNT+TWO;      /* TOTAL DATA SET COUNT          */ 00612000
         AL    @05,@CF00096                                        0521 00613000
         ST    @05,MWADSCNT                                        0521 00614000
*       CLWPATH=ON;                 /* INDICATE PATH PROCESSING IN 0522 00615000
*                                      CLW                           */ 00616000
         OI    CLWPATH(RCORE),B'00010000'                          0522 00617000
*       MWAPAMBL=MWAMBLPT;          /* SAVE PATH AMBL ADDRESS        */ 00618000
         LR    MWAPAMBL,@11                                        0523 00619000
*       MWAMBLPT=AMBLXPT;           /* POINT TO BASE DATA SET AMBL   */ 00620000
         L     @11,AMBLXPT(,@11)                                   0524 00621000
         ST    @11,MWAMBLPT                                        0524 00622000
*     END;                                                         0525 00623000
*   MWABAMBL=MWAMBLPT;              /* SAVE POINTER TO BASE AMBL     */ 00624000
@RF00519 L     @11,MWAMBLPT                                        0526 00625000
         ST    @11,MWABAMBL                                        0526 00626000
*   IF BIBUPT^=ZERO THEN            /* UPT EXIST                     */ 00627000
         L     @11,AMBLBIB(,@11)                                   0527 00628000
         L     @11,BIBUPT(,@11)                                    0527 00629000
         LTR   @11,@11                                             0527 00630000
         BZ    @RF00527                                            0527 00631000
*     MWADSCNT=MWADSCNT+UPTNOENT*TWO;/* INCREASE DATA SET COUNT BY 0528 00632000
*                                      NO. OF UPGRADE DS'S           */ 00633000
         SLR   @05,@05                                             0528 00634000
         IC    @05,UPTNOENT(,@11)                                  0528 00635000
         ALR   @05,@05                                             0528 00636000
         AL    @05,MWADSCNT                                        0528 00637000
         ST    @05,MWADSCNT                                        0528 00638000
*   IF AMBLIX=ZERO THEN             /* IF ESDS                       */ 00639000
@RF00527 L     @11,MWAMBLPT                                        0529 00640000
         L     @11,AMBLIX(,@11)                                    0529 00641000
         LTR   @11,@11                                             0529 00642000
         BNZ   @RF00529                                            0529 00643000
*     MWADSCNT=MWADSCNT+ONE;        /* INCREMENT DATA SET COUNT BY 0530 00644000
*                                      ONE                           */ 00645000
         LA    @11,1                                               0530 00646000
         AL    @11,MWADSCNT                                        0530 00647000
         ST    @11,MWADSCNT                                        0530 00648000
*   ELSE                            /* IF KSDS                       */ 00649000
*     MWADSCNT=MWADSCNT+TWO;        /* INCREMENT DATA SET COUNT BY 0531 00650000
*                                      TWO                           */ 00651000
         B     @RC00529                                            0531 00652000
@RF00529 LA    @11,2                                               0531 00653000
         AL    @11,MWADSCNT                                        0531 00654000
         ST    @11,MWADSCNT                                        0531 00655000
*   MWAMBLPT=ACBAMBL;               /* AMBL POINTED TO BY USER ACB   */ 00656000
@RC00529 L     @11,ACBAMBL(,RACB)                                  0532 00657000
         ST    @11,MWAMBLPT                                        0532 00658000
*   MWABYTES=LENGTH(ENQTCB)+MWADSCNT*(LENGTH(ENQENTRY)+LENGTH(ENQRNAME) 00659000
*       );                          /* BYTES REQUIRED FOR ENQ      0533 00660000
*                                      PARAMETER LIST AND RNAME LIST */ 00661000
         L     @11,MWADSCNT                                        0533 00662000
         MH    @11,@CH00585                                        0533 00663000
         LA    @11,4(,@11)                                         0533 00664000
         STCM  @11,7,MWABYTES                                      0533 00665000
*   MWASPOOL=WKPOOL;                /* SUBPOOL NUMBER                */ 00666000
         MVI   MWASPOOL,X'FC'                                      0534 00667000
*   CALL GETCORE;                   /* GETMAIN FOR ENQ PARAMETER LIST*/ 00668000
         BAL   @14,GETCORE                                         0535 00669000
*   MWAENQFG=ON;                    /* INDICATE ENQ PARAMETER LIST 0536 00670000
*                                      BUILT                         */ 00671000
         OI    MWAENQFG,B'10000000'                                0536 00672000
*   MWAENQPT=MWADDRES;              /* ADDRESSABILITY FOR ENQ PARM 0537 00673000
*                                      LIST                          */ 00674000
         L     @11,MWADDRES                                        0537 00675000
         ST    @11,MWAENQPT                                        0537 00676000
*   MWARNPTR=MWADDRES+LENGTH(ENQPARML)+MWADSCNT*(LENGTH(ENQENTRY));/*   00677000
*                                      ADDRESSING FOR RNAME LIST     */ 00678000
         AL    @11,@CF00143                                        0538 00679000
         L     MWARNPTR,MWADSCNT                                   0538 00680000
         MH    MWARNPTR,@CH00033                                   0538 00681000
         ALR   @11,MWARNPTR                                        0538 00682000
         LR    MWARNPTR,@11                                        0538 00683000
*   CALL ENQINIT;                   /* INITIALIZE ENQ PARAMETER LIST */ 00684000
         BAL   @14,ENQINIT                                         0539 00685000
*   RESPECIFY                                                      0540 00686000
*     R1 RESTRICTED;                                               0540 00687000
*   R1=MWAENQPT+LENGTH(ENQTCB);     /* POINT TO ENQ PARAMETER LIST   */ 00688000
         LA    R1,4                                                0541 00689000
         AL    R1,MWAENQPT                                         0541 00690000
*   GEN(ENQ    MF=(E,(R1)));        /* ENQ ON DATA SET       @ZA10878*/ 00691000
         ENQ    MF=(E,(R1))                                             00692000
*   RESPECIFY                                                      0543 00693000
*     RET RESTRICTED;                                              0543 00694000
*   RET=MWASAVEB;                   /* GET RETURN ADDRESS            */ 00695000
         L     RET,MWASAVEB                                        0544 00696000
*   RESPECIFY                                                      0545 00697000
*    (R1,                                                          0545 00698000
*     RET) UNRESTRICTED;                                           0545 00699000
*   END ENQFUNC;                                                   0546 00700000
@EL00004 DS    0H                                                  0546 00701000
@EF00004 DS    0H                                                  0546 00702000
@ER00004 BR    @14                                                 0546 00703000
*ENQINIT:                                                          0547 00704000
*                                                                  0547 00705000
*/********************************************************************/ 00706000
*/*                                                                  */ 00707000
*/*      01   FUNCTION-INITIALIZE ENQ PARAMETER LIST FOR ALL THE     */ 00708000
*/*                    DATA SETS                                     */ 00709000
*/*      01   INPUTS-  1. POINTER TO AMBL IN ACBAMBL                 */ 00710000
*/*      01   OUTPUTS- 1. ENQ PARAMETER LIST INITIALIZED             */ 00711000
*/*                                                                  */ 00712000
*/********************************************************************/ 00713000
*                                                                  0547 00714000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0547 00715000
ENQINIT  DS    0H                                                  0548 00716000
*   RESPECIFY                                                      0548 00717000
*     RET RESTRICTED;                                              0548 00718000
*   MWASAVEC=RET;                   /* SAVE CALLER RETURN ADDRESS    */ 00719000
         ST    RET,MWASAVEC                                        0549 00720000
*   RESPECIFY                                                      0550 00721000
*     RET UNRESTRICTED;                                            0550 00722000
*   MWAEQIDX=ONE;                   /* INDEX FOR ENQ PARAMETER LIST  */ 00723000
         LA    MWAEQIDX,1                                          0551 00724000
*   IF CLWPATH=ON THEN              /* IF PATH PROCESSING            */ 00725000
         TM    CLWPATH(RCORE),B'00010000'                          0552 00726000
         BNO   @RF00552                                            0552 00727000
*     DO;                           /* INITIALIZE PATH CI'S IN ENQ 0553 00728000
*                                      PARAMETER LIST                */ 00729000
*       MWAAMBPT=AMBLDTA;           /* POINT TO DATA AMB OF PATH AMBL*/ 00730000
