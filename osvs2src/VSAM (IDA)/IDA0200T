@RF00823 MVI   MWASPOOL,X'E6'                                      0825 01701000
*       MWAFRKEY=K50;               /* REQUEST KEY 5 FREEMAIN.     0826 01702000
*                                                            @Y30DSMD*/ 01703000
@RC00823 MVI   MWAFRKEY,X'50'                                      0826 01704000
*     END;                          /* END OF NON-CTLG DDEB LOGIC  0827 01705000
*                                      BYPASS.               @Y30DSMD*/ 01706000
*   ELSE                            /* IF CATALOG OR SCRA            */ 01707000
*     MWASPOOL=SCATPOOL;            /* SUBPOOL OF CATALOG    @Z30SSMD*/ 01708000
         B     @RC00821                                            0828 01709000
@RF00821 MVI   MWASPOOL,X'F1'                                      0828 01710000
*   CALL FREECORE;                  /* FREE DEB EXTENTION.   @Y30DSMD*/ 01711000
*                                                                  0829 01712000
