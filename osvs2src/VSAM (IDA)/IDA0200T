*     RET UNRESTRICTED;                                            1073 02534000
*   PDPFUNC=PDF107;                 /* SET FUNCTION CODE             */ 02535000
         L     @14,MWACORAD                                        1074 02536000
         MVI   PDPFUNC+280(@14),X'6B'                              1074 02537000
*   DO MWAINDEX=1 TO UPTNOENT BY 1; /* GO THROUGH UPT                */ 02538000
         LA    @14,1                                               1075 02539000
         ST    @14,MWAINDEX                                        1075 02540000
