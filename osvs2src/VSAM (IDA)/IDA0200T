*   MWABYTES=DEBLNGTH*DWORDLEN;     /* NOTE THE SIZE OF THE BLK.   0831 01726000
*                                                            @Y30DSMD*/ 01727000
         SLR   @15,@15                                             0831 01728000
         IC    @15,DEBLNGTH(,@11)                                  0831 01729000
         SLA   @15,3                                               0831 01730000
         STCM  @15,7,MWABYTES                                      0831 01731000
*   IF MWACAT=OFF THEN              /* IF NOT CATALOG OR SCRA      0832 01732000
*                                                            @Y30DSMD*/ 01733000
         TM    MWACAT,B'10000000'                                  0832 01734000
         BNZ   @RF00832                                            0832 01735000
*     DO;                           /* PROCESS THE NON-CTLG DDEB.  0833 01736000
*                                                            @Y30DSMD*/ 01737000
*       IF MWAGSR=ON|MWACBIC=ON THEN/* IS THIS GSR OR CBIC   @ZA16012*/ 01738000
         TM    MWAGSR,B'10000000'                                  0834 01739000
         BO    @RT00834                                            0834 01740000
         TM    MWACBIC,B'10000000'                                 0834 01741000
         BNO   @RF00834                                            0834 01742000
@RT00834 DS    0H                                                  0835 01743000
*         MWASPOOL=GSRSPOOL;        /* INDICATE GSR SUB-SPOOL.     0835 01744000
*                                                            @Y30DSMD*/ 01745000
         MVI   MWASPOOL,X'F1'                                      0835 01746000
*       ELSE                        /* IF NOT GSR DATA SET   @Z30SSRL*/ 01747000
