*                                                                  1425 03870000
         L     RACB,MWASAVED                                       1425 03871000
*   /*****************************************************************/ 03872000
*   /*                                                               */ 03873000
*   /* FREEMAIN THE P/L FOR IDA0192Y. MWA VARIABLES ARE INTACT.      */ 03874000
*   /*                                                               */ 03875000
*   /*****************************************************************/ 03876000
*                                                                  1426 03877000
*   CALL FREECORE;                  /* FREE THE IDABLPRM.    @Y30DSMD*/ 03878000
*                                                                  1426 03879000
         BAL   @14,FREECORE                                        1426 03880000
*   /*****************************************************************/ 03881000
*   /*                                                               */ 03882000
*   /* ISUUE A MSG TO INFORM THE OPERATOR AND THE USER OF FDLVRP.    */ 03883000
*   /*                                                               */ 03884000
*   /*****************************************************************/ 03885000
*                                                                  1427 03886000
*   CALL FDLMSG;                    /* ISSUE MESSAGE FOR THE FORCE 1427 03887000
*                                      DELETE.               @Z30SSMD*/ 03888000
         BAL   @14,FDLMSG                                          1427 03889000
*   RET=MWASAVEB;                   /* RETURN TO CALLER.     @Y30DSMD*/ 03890000
         L     RET,MWASAVEB                                        1428 03891000
*   RFY                                                            1429 03892000
*     RET UNRSTD;                   /* INFORM THE CIMPILER.  @Y30DSMD*/ 03893000
*   END FDLVRP;                     /* END OF FDLVRP ROUTINE.      1430 03894000
*                                                            @Y30DSMD*/ 03895000
@EL00027 DS    0H                                                  1430 03896000
@EF00027 DS    0H                                                  1430 03897000
@ER00027 BR    @14                                                 1430 03898000
*GSRDUMP:                                                          1431 03899000
*                                                                  1431 03900000
*/********************************************************************/ 03901000
*/*      01 FUNCTION-GSRDUMP. THIS ROUTINE WILL DUMP GSR DATA TO THE */ 03902000
*/*                  SYS1.DUMP DATA SET. THE AMCBS, VSRT AND ALL  SYS*/ 03903000
*/*                  ASSOCIATED GLOBAL STORAGE IS DUMPED. MWADRET IS */ 03904000
*/*                  SET FOR THE FDLMSG ROUTINE IN ORDER TO DETERMINE*/ 03905000
*/*                  WHICH MESSAGE OF SEVERAL POSSIBLES TO ISSUE.    */ 03906000
*/*      01 INPUT-GSR VSRT ASSUMED TO BE POINTED BY MWAVSPTR @Z40ESSR*/ 03907000
*/*      01 OUTPUT-GLABAL DATA IS DUMPED IN UNFORMATTED MODE TO      */ 03908000
*/*                SYS1.DUMP USING  SVC DUMP (SDUMP MACRO).         .*/ 03909000
*/********************************************************************/ 03910000
*                                                                  1431 03911000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* DUMP GSR DATA ROUTINE.        */ 03912000
GSRDUMP  DS    0H                                                  1432 03913000
*   RFY                                                            1432 03914000
*     RET RSTD;                     /* RESERVE RET (R15).    @Z30SSMD*/ 03915000
*   MWASAVEC=RET;                   /* SAVE CALLERS RETURN ADDRESS.     03916000
*                                                            @Z30SSMD*/ 03917000
         ST    RET,MWASAVEC                                        1433 03918000
*   MWADRET=ONE;                    /* INITIALIZE THE SDUMP RETURN 1434 03919000
*                                      CODE.                 @Z30SSMD*/ 03920000
*                                                                  1434 03921000
         MVC   MWADRET(4),@CF00080                                 1434 03922000
*   /*****************************************************************/ 03923000
*   /*                                                               */ 03924000
*   /* DUMP THE AMCBS.                                       @Z30SSMD*/ 03925000
*   /*                                                               */ 03926000
*   /*****************************************************************/ 03927000
*                                                                  1435 03928000
*   MWADADRS=ADDR(AMCBS);           /* POINT TO THE AMCBS.   @Z30SSMD*/ 03929000
         L     @15,CVTPTR                                          1435 03930000
         MVC   MWADADRS(3),CVTCBSP+1(@15)                          1435 03931000
*   MWADLEN=LENGTH(AMCBS);          /* SET THE BYTE COUNT FOR THE  1436 03932000
*                                      DUMP.                 @Z30SSMD*/ 03933000
         LA    MWADLEN,96                                          1436 03934000
*   CALL SDLOAD;                    /* MOVE THE INFO TO THE DUMP DATA   03935000
*                                      SET.                  @Z30SSMD*/ 03936000
*                                                                  1437 03937000
         BAL   @14,SDLOAD                                          1437 03938000
*   /*****************************************************************/ 03939000
*   /*                                                               */ 03940000
*   /* DUMP THE VSRT ITSELF.                                 @Z30SSMD*/ 03941000
*   /*                                                               */ 03942000
*   /*****************************************************************/ 03943000
*                                                                  1438 03944000
*   MWADADRS=ADDR(IDAVSRT);         /* POINT TO THE VSRT.    @Z30SSMD*/ 03945000
         L     @15,MWACBSPT                                        1438 03946000
         L     @15,BWAVSPTR(,@15)                                  1438 03947000
         STCM  @15,7,MWADADRS                                      1438 03948000
*   MWADLEN=VSRTLEN;                /* USE ACTUAL LENGHT IN BYTES. 1439 03949000
*                                                            @Z30SSMD*/ 03950000
         LH    MWADLEN,VSRTLEN(,@15)                               1439 03951000
*   CALL SDLOAD;                    /* MOVE AND DUMP THE DATA.     1440 03952000
*                                                            @Z30SSMD*/ 03953000
*                                                                  1440 03954000
         BAL   @14,SDLOAD                                          1440 03955000
*   /*****************************************************************/ 03956000
*   /*                                                               */ 03957000
*   /* DUMP THE WORK AREA HEADER AND ASSOCIATED CORE.        @Z30SSMD*/ 03958000
*   /*                                                               */ 03959000
*   /*****************************************************************/ 03960000
*                                                                  1441 03961000
*   RFY                                                            1441 03962000
*     IDAWSHD BASED(MWADWSHD);      /* INFORN THE COMPILER OF BASE.  */ 03963000
*   MWADWSHD=VSRTWAH;               /* POINT TO THE FIRST WSHD.    1442 03964000
*                                                            @Z30SSMD*/ 03965000
*                                                                  1442 03966000
         L     @15,MWACBSPT                                        1442 03967000
         L     @15,BWAVSPTR(,@15)                                  1442 03968000
         L     MWADWSHD,VSRTWAH(,@15)                              1442 03969000
*   /*****************************************************************/ 03970000
*   /*                                                               */ 03971000
*   /* DUMP THE WSHD ITSELF.                                 @Z30SSMD*/ 03972000
*   /*                                                               */ 03973000
*   /*****************************************************************/ 03974000
*                                                                  1443 03975000
*   DO WHILE MWADWSHD^=ZERO;        /* LOOP THROUGH THE WSHD CHAIN.     03976000
*                                                            @Z30SSMD*/ 03977000
         B     @DE01443                                            1443 03978000
@DL01443 DS    0H                                                  1444 03979000
*     MWADADRS=ADDR(IDAWSHD);       /* POINT TO A WSHD.      @Z30SSMD*/ 03980000
         STCM  MWADWSHD,7,MWADADRS                                 1444 03981000
*     MWADLEN=WSHDLEN;              /* USE ACTUAL WSHD LENGHT IN   1445 03982000
*                                      BYTES.                @Z30SSMD*/ 03983000
         LH    MWADLEN,WSHDLEN(,MWADWSHD)                          1445 03984000
*     CALL SDLOAD;                  /* DUMP THE DATA.        @Z30SSMD*/ 03985000
*                                                                  1446 03986000
         BAL   @14,SDLOAD                                          1446 03987000
*     /***************************************************************/ 03988000
*     /*                                                             */ 03989000
*     /* DUMP THE CORE ASSOCIATED WITH THE WSHD.             @Z30SSMD*/ 03990000
*     /*                                                             */ 03991000
*     /***************************************************************/ 03992000
*                                                                  1447 03993000
*     DO MWADCTR=WSHDNUS BY-1 TO ONE;/* LOOP THRU SLOTS.     @Z30SSRL*/ 03994000
         LH    MWADCTR,WSHDNUS(,MWADWSHD)                          1447 03995000
         B     @DE01447                                            1447 03996000
@DL01447 DS    0H                                                  1448 03997000
*       MWADADRS=WSHDSAD(MWADCTR);  /* POINT TO ASSOCIATED SLOT    1448 03998000
*                                      CORE.@Z30SSMD                 */ 03999000
         LR    @15,MWADCTR                                         1448 04000000
         SLA   @15,4                                               1448 04001000
         L     @08,WSHDSAD-16(@15,MWADWSHD)                        1448 04002000
         STCM  @08,7,MWADADRS                                      1448 04003000
*       MWADLEN=WSHDSFLN(MWADCTR);  /* GET SIZE IN BYTES OF        1449 04004000
*                                      CORE.@Z30SSMD                 */ 04005000
         L     MWADLEN,WSHDSFLN-17(@15,MWADWSHD)                   1449 04006000
         LA    MWADLEN,0(,MWADLEN)                                 1449 04007000
*       CALL SDLOAD;                /* DUMP THE DATA.        @Z30SSMD*/ 04008000
         BAL   @14,SDLOAD                                          1450 04009000
*     END;                          /* END OF WSHD OWNED CORE LOOP.     04010000
*                                                            @Z30SSMD*/ 04011000
         BCTR  MWADCTR,0                                           1451 04012000
@DE01447 LTR   MWADCTR,MWADCTR                                     1451 04013000
         BP    @DL01447                                            1451 04014000
*     MWADWSHD=WSHDNEXT;            /* GET THE NEXT WSHD IN THE    1452 04015000
*                                      CHAIN.                @Z30SSMD*/ 04016000
         L     @15,WSHDNEXT-1(,MWADWSHD)                           1452 04017000
         LA    @15,0(,@15)                                         1452 04018000
         LR    MWADWSHD,@15                                        1452 04019000
*   END;                            /* END OF WSHD AND RELATED CORE     04020000
*                                      DUMP.                 @Z30SSMD*/ 04021000
@DE01443 LTR   MWADWSHD,MWADWSHD                                   1453 04022000
         BNZ   @DL01443                                            1453 04023000
*   MWADWSHD=VSRTCPAH;              /* POINT TO FIRST CPA WSHD.    1454 04024000
*                                                            @Z30SSMD*/ 04025000
*                                                                  1454 04026000
