         L     @15,BWAVSPTR(,@15)                                  1454 04028000
         L     MWADWSHD,VSRTCPAH(,@15)                             1454 04029000
*   /*****************************************************************/ 04030000
*   /*                                                               */ 04031000
*   /* DUMP THE CPA WSHD AND ASSOCIATED CORE.                @Z30SSMD*/ 04032000
*   /*                                                               */ 04033000
*   /*****************************************************************/ 04034000
*                                                                  1455 04035000
*   DO WHILE MWADWSHD^=ZERO;        /* LOOP THROUGH THE CPAWSHD    1455 04036000
*                                      CHAIN.@Z30SSMD                */ 04037000
         B     @DE01455                                            1455 04038000
@DL01455 DS    0H                                                  1456 04039000
*     MWADADRS=ADDR(IDAWSHD);       /* POINT TO A CPA WSHD.  @Z30SSMD*/ 04040000
         STCM  MWADWSHD,7,MWADADRS                                 1456 04041000
*     MWADLEN=WSHDLEN;              /* USE ACTUAL WSHD LENGHT IN   1457 04042000
*                                      BYTES.                @Z30SSMD*/ 04043000
         LH    MWADLEN,WSHDLEN(,MWADWSHD)                          1457 04044000
*     CALL SDLOAD;                  /* DUMP THE DATA.        @Z30SSMD*/ 04045000
*                                                                  1458 04046000
         BAL   @14,SDLOAD                                          1458 04047000
*     /***************************************************************/ 04048000
*     /*                                                             */ 04049000
*     /* DUMP THE CORE ASSOCIATED WITH THE CPA WSHD.         @Z30SSMD*/ 04050000
*     /*                                                             */ 04051000
*     /***************************************************************/ 04052000
*                                                                  1459 04053000
*     DO MWADCTR=WSHDNUS BY-1 TO ONE;/* LOOP THRU SLOTS.     @Z30SSRL*/ 04054000
         LH    MWADCTR,WSHDNUS(,MWADWSHD)                          1459 04055000
         B     @DE01459                                            1459 04056000
@DL01459 DS    0H                                                  1460 04057000
*       MWADADRS=WSHDSAD(MWADCTR);  /* POINT TO ASSOCIATED SLOT CORE    04058000
*                                                            @Z30SSMD*/ 04059000
         LR    @15,MWADCTR                                         1460 04060000
         SLA   @15,4                                               1460 04061000
         L     @08,WSHDSAD-16(@15,MWADWSHD)                        1460 04062000
         STCM  @08,7,MWADADRS                                      1460 04063000
*       MWADLEN=WSHDSFLN(MWADCTR);  /* GET SIZE IN BYTES OF CORE.  1461 04064000
*                                                            @Z30SSMD*/ 04065000
         L     MWADLEN,WSHDSFLN-17(@15,MWADWSHD)                   1461 04066000
         LA    MWADLEN,0(,MWADLEN)                                 1461 04067000
*       CALL SDLOAD;                /* DUMP THE DATA.        @Z30SSMD*/ 04068000
         BAL   @14,SDLOAD                                          1462 04069000
*     END;                          /* END OF WSHD OWNED CORE LOOP.     04070000
*                                                            @Z30SSMD*/ 04071000
         BCTR  MWADCTR,0                                           1463 04072000
@DE01459 LTR   MWADCTR,MWADCTR                                     1463 04073000
         BP    @DL01459                                            1463 04074000
*     MWADWSHD=WSHDNEXT;            /* GET THE NEXT CPA WSHD IN THE     04075000
*                                      CHAIN.@Z30SSMD                */ 04076000
         L     @15,WSHDNEXT-1(,MWADWSHD)                           1464 04077000
         LA    @15,0(,@15)                                         1464 04078000
         LR    MWADWSHD,@15                                        1464 04079000
*   END;                            /* END OF CPA WSHD AND RELATED 1465 04080000
*                                      CORE DUMP.            @Z30SSMD*/ 04081000
*                                                                  1465 04082000
@DE01455 LTR   MWADWSHD,MWADWSHD                                   1465 04083000
         BNZ   @DL01455                                            1465 04084000
*   /*****************************************************************/ 04085000
*   /*                                                               */ 04086000
*   /* DUMP CORE POINTED TO BY VSRT CSL LIST.                @Z30SSMD*/ 04087000
*   /*                                                               */ 04088000
*   /*****************************************************************/ 04089000
*                                                                  1466 04090000
*   MWADCTR=ONE;                    /* START WITH THE FIRST ENTRY. 1466 04091000
*                                                            @Z30SSMD*/ 04092000
         LA    MWADCTR,1                                           1466 04093000
*   DO WHILE VSRTCSVS(MWADCTR)^=ON; /* LOOP UNTIL END.       @Z30SSMD*/ 04094000
         B     @DE01467                                            1467 04095000
@DL01467 DS    0H                                                  1468 04096000
*     MWADADRS=VSRTCSAD(MWADCTR);   /* LOAD A CSL ENTRY.     @Z30SSMD*/ 04097000
         LR    @15,MWADCTR                                         1468 04098000
         SLA   @15,3                                               1468 04099000
         L     @08,MWACBSPT                                        1468 04100000
         L     @08,BWAVSPTR(,@08)                                  1468 04101000
         ST    @15,@TF00001                                        1468 04102000
         ALR   @15,@08                                             1468 04103000
         MVC   MWADADRS(3),VSRTCSAD-8(@15)                         1468 04104000
*     MWADLEN=VSRTCSLN(MWADCTR);    /* LOAD THE LENGTH.      @Z30SSMD*/ 04105000
         L     @15,@TF00001                                        1469 04106000
         L     MWADLEN,VSRTCSLN-9(@15,@08)                         1469 04107000
         LA    MWADLEN,0(,MWADLEN)                                 1469 04108000
*     IF(MWADADRS^=ZERO&MWADLEN^=ZERO) THEN/* ENTRY NULL?    @Z30SSMD*/ 04109000
         L     @15,MWADADRS-1                                      1470 04110000
         LA    @15,0(,@15)                                         1470 04111000
         LTR   @15,@15                                             1470 04112000
         BZ    @RF01470                                            1470 04113000
         LTR   MWADLEN,MWADLEN                                     1470 04114000
         BZ    @RF01470                                            1470 04115000
*       CALL SDLOAD;                /* IF ANY DATA DUMP IT.  @Z30SSMD*/ 04116000
         BAL   @14,SDLOAD                                          1471 04117000
*     MWADCTR=MWADCTR+ONE;          /* INCREMENT COUNTER BY ONE    1472 04118000
*                                                            @Z30SSRL*/ 04119000
@RF01470 AL    MWADCTR,@CF00080                                    1472 04120000
*   END;                            /* END OF DUMP VSRT CSL LOOP.  1473 04121000
*                                                            @Z30SSMD*/ 04122000
*                                                                  1473 04123000
@DE01467 LR    @15,MWADCTR                                         1473 04124000
         SLA   @15,3                                               1473 04125000
         L     @11,MWACBSPT                                        1473 04126000
         L     @11,BWAVSPTR(,@11)                                  1473 04127000
         ALR   @11,@15                                             1473 04128000
         TM    VSRTCSVS-8(@11),B'01000000'                         1473 04129000
         BNO   @DL01467                                            1473 04130000
*   /*****************************************************************/ 04131000
*   /*                                                               */ 04132000
*   /* AT THIS POINT A BUFFER FLUSH MAY BE NEEDED.           @Z30SSMD*/ 04133000
*   /*                                                               */ 04134000
*   /*****************************************************************/ 04135000
*                                                                  1474 04136000
*   RET=MWASAVEC;                   /* RESTORE THE RETURN ADDRESS. 1474 04137000
*                                                            @Z30SSMD*/ 04138000
         L     RET,MWASAVEC                                        1474 04139000
*   RFY                                                            1475 04140000
*     RET UNRSTD;                   /* INFORM THE COMPILER.  @Z30SSMD*/ 04141000
*   END GSRDUMP;                    /* END OF GSRDUMP ROUTINE.     1476 04142000
*                                                            @Z30SSMD*/ 04143000
@EL00028 DS    0H                                                  1476 04144000
@EF00028 DS    0H                                                  1476 04145000
@ER00028 BR    @14                                                 1476 04146000
*SDLOAD:                                                           1477 04147000
*                                                                  1477 04148000
*/********************************************************************/ 04149000
*/*       01 FUNCTION-MOVE OUTPUT DATA FOR GSRDUMP ROUTINE.         .*/ 04150000
*/*       01 INPUT-MWADADRS POINT TO THE AREA TO BE DUMPED.         .*/ 04151000
*/*               -MWADLEN CONTAINS THE SIZE OF THE AREA.          . */ 04152000
*/*       01 OUTPUT-DATA IS SVC DUMPED TO SYS1.DUMP DATA SET.        */ 04153000
*/********************************************************************/ 04154000
*                                                                  1477 04155000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* DUMP GSR DATA ROUTINE.        */ 04156000
SDLOAD   DS    0H                                                  1478 04157000
*   RFY                                                            1478 04158000
*    (RET,                                                         1478 04159000
*     RETCODE,                                                     1478 04160000
*     R5,                                                          1478 04161000
*     R6) RSTD;                     /* INFORM THE COMPILER.  @Z30SSMD*/ 04162000
*   MWASAVED=R14;                   /* SAVE THE RETURN ADDRESS.    1479 04163000
*                                                            @Z30SSMD*/ 04164000
         ST    R14,MWASAVED                                        1479 04165000
*   MWASAVEE=R5;                    /* SAVE REG(5) IN THIS PROC.   1480 04166000
*                                                            @Z30SSMD*/ 04167000
         ST    R5,MWASAVEE                                         1480 04168000
*   MWASAVEF=R6;                    /* SAVE REG(6) ALSO.     @Z30SSMD*/ 04169000
         ST    R6,MWASAVEF                                         1481 04170000
*   R5=MWADADRS;                    /* GET STARTING ADDRESS. @Z30SSMD*/ 04171000
         L     R5,MWADADRS-1                                       1482 04172000
         LA    R5,0(,R5)                                           1482 04173000
*   R6=R5+MWADLEN-ONE;              /* COMPUTE ENDING ADDRESS.     1483 04174000
*                                                            @Z30SSMD*/ 04175000
         LR    R6,R5                                               1483 04176000
         ALR   R6,MWADLEN                                          1483 04177000
         BCTR  R6,0                                                1483 04178000
*   SDLSTSA=R5;                     /* SET STARTING ADDR IN STORAGE     04179000
*                                      LIST                  @ZA12192*/ 04180000
         STCM  R5,7,SDLSTSA                                        1484 04181000
*   SDLSTEA=R6;                     /* SET ENDING ADDR IN STORAGE  1485 04182000
*                                      LIST                  @ZA12192*/ 04183000
         STCM  R6,7,SDLSTEA                                        1485 04184000
*   SDLSTEOL=ON;                    /* SET END OF LIST INDICATOR   1486 04185000
*                                                            @ZA12192*/ 04186000
         OI    SDLSTEOL,B'10000000'                                1486 04187000
*   MWASDUMP=SDUMPPL;               /* INIT SDUMP PARM LIST  @Z30SSRL*/ 04188000
         MVC   MWASDUMP(72),SDUMPPL                                1487 04189000
*   GEN SETS(RETCODE,RET) REFS(R5,R6);                             1488 04190000
*                                                          /*@Z30SSMD*/ 04191000
         SDUMP LIST=MWASDLST,BUFFER=NO,QUIESCE=YES,BRANCH=NO,          *04192000
               MF=(E,MWASDUMP)         EXECUTE FORM OF SDUMP  @ZA12192  04193000
*   IF MWADRET=ONE THEN             /* HAVE ANY DUMPS BEEN ATTEMPTED?   04194000
*                                                            @Z30SSMD*/ 04195000
         CLC   MWADRET(4),@CF00080                                 1489 04196000
         BNE   @RF01489                                            1489 04197000
*     DO;                           /* IF NOT THEN PROCESS THE SDUMP    04198000
*                                      RETURN CODE HERE.     @Z30SSMD*/ 04199000
*       MWADRET=RETCODE;            /* USE THE CURRENT SDUMP RETURN     04200000
*                                      CODE.                 @Z30SSMD*/ 04201000
         ST    RETCODE,MWADRET                                     1491 04202000
*     END;                          /* END OF RET PROCESSING.      1492 04203000
*                                                            @Z30SSMD*/ 04204000
*   ELSE                            /* IF NOT CONTINUE TESTING     1493 04205000
*                                      STATUS.               @Z30SSMD*/ 04206000
*     IF MWADRET=ZERO THEN          /* HAVE ALL DUMPS BEEN         1493 04207000
*                                      COMPLLETED?          @Z30SSMD*/ 04208000
         B     @RC01489                                            1493 04209000
@RF01489 SLR   @11,@11                                             1493 04210000
         C     @11,MWADRET                                         1493 04211000
         BNE   @RF01493                                            1493 04212000
*       DO;                         /* PROCESS THE SDUMP RETURN CODE    04213000
*                                      HERE.                 @Z30SSMD*/ 04214000
*         IF RETCODE=ZERO THEN      /* STILL DUMPING OK?     @Z30SSMD*/ 04215000
         CR    RETCODE,@11                                         1495 04216000
         BNE   @RF01495                                            1495 04217000
*           MWADRET=ZERO;           /* SET DUMP RET CODE AS ALL OK.     04218000
*                                                            @Z30SSMD*/ 04219000
         ST    @11,MWADRET                                         1496 04220000
*         ELSE                      /* IF THIS DUMP NOT OK CONTINUE     04221000
*                                      TESTING.              @Z30SSMD*/ 04222000
*           IF RETCODE=FOUR|RETCODE=EIGHT THEN/* IS CODE EXPECTED? 1497 04223000
*                                                            @Z30SSMD*/ 04224000
         B     @RC01495                                            1497 04225000
@RF01495 C     RETCODE,@CF00143                                    1497 04226000
         BE    @RT01497                                            1497 04227000
         C     RETCODE,@CF00071                                    1497 04228000
         BNE   @RF01497                                            1497 04229000
@RT01497 DS    0H                                                  1498 04230000
*             MWADRET=FOUR;         /* IF PARTIAL OR NO DUMP THEN NOW   04231000
*                                      PARTIAL.                      */ 04232000
         MVC   MWADRET(4),@CF00143                                 1498 04233000
*       END;                        /* END OF RET PROCESSING GROUP.     04234000
*                                                            @Z30SSMD*/ 04235000
*     ELSE                          /* IF NOT, CONTINUE TESTING.   1500 04236000
*                                                            @Z30SSMD*/ 04237000
*       IF MWADRET=FOUR THEN        /* ANY PARTIAL DUMPS DONE TO THIS   04238000
*                                      POINT?                        */ 04239000
         B     @RC01493                                            1500 04240000
@RF01493 LA    @11,4                                               1500 04241000
         C     @11,MWADRET                                         1500 04242000
         BNE   @RF01500                                            1500 04243000
*         MWADRET=FOUR;             /* STATE IS UNCHANGED BY THIS  1501 04244000
*                                      SDUMP.                @Z30SSMD*/ 04245000
         ST    @11,MWADRET                                         1501 04246000
*       ELSE                        /* IF NONE OF THE ABOVE THE TEST    04247000
*                                      FOR                   @Z30SSMD*/ 04248000
*         IF MWADRET=EIGHT THEN     /* NO DUMP TAKEN YET.    @Z30SSMD*/ 04249000
         B     @RC01500                                            1502 04250000
@RF01500 CLC   MWADRET(4),@CF00071                                 1502 04251000
         BNE   @RF01502                                            1502 04252000
*           DO;                     /* PROCESS THE SDUMP RETURN CODE.   04253000
*                                                            @Z30SSMD*/ 04254000
*             IF RETCODE=ZERO|      /* IF THIS DUMP IS OK THEN     1504 04255000
*                                                            @Z30SSMD*/ 04256000
*                 RETCODE=FOUR THEN /* OR IF PARTIAL DUMP THEN     1504 04257000
*                                                            @Z30SSMD*/ 04258000
         LTR   RETCODE,RETCODE                                     1504 04259000
         BZ    @RT01504                                            1504 04260000
         C     RETCODE,@CF00143                                    1504 04261000
         BNE   @RF01504                                            1504 04262000
@RT01504 DS    0H                                                  1505 04263000
*               MWADRET=FOUR;       /* STATE IS NOW PARTIAL DUMP.  1505 04264000
*                                                            @Z30SSMD*/ 04265000
         MVC   MWADRET(4),@CF00143                                 1505 04266000
*             ELSE                  /* IF NOT ZERO OR FOUR   @Z30SSMD*/ 04267000
*               IF RETCODE=EIGHT THEN/* IS IT EIGHT?         @Z30SSMD*/ 04268000
         B     @RC01504                                            1506 04269000
@RF01504 LA    @11,8                                               1506 04270000
         CR    RETCODE,@11                                         1506 04271000
         BNE   @RF01506                                            1506 04272000
*                 MWADRET=EIGHT;    /* STILL NO DUMPS TAKEN. @Z30SSMD*/ 04273000
         ST    @11,MWADRET                                         1507 04274000
*           END;                    /* END OF RET PROCESSING.      1508 04275000
*                                                            @Z30SSMD*/ 04276000
@RF01506 DS    0H                                                  1508 04277000
@RC01504 DS    0H                                                  1509 04278000
*   R5=MWASAVEE;                    /* RESTORE REG(5).       @Z30SSMD*/ 04279000
@RF01502 DS    0H                                                  1509 04280000
@RC01500 DS    0H                                                  1509 04281000
@RC01493 DS    0H                                                  1509 04282000
@RC01489 L     R5,MWASAVEE                                         1509 04283000
*   R6=MWASAVEF;                    /* RESTORE REG(6).       @Z30SSMD*/ 04284000
         L     R6,MWASAVEF                                         1510 04285000
*   RET=MWASAVED;                   /* RESTORE RETURN ADDRESS.     1511 04286000
*                                                            @Z30SSMD*/ 04287000
         L     RET,MWASAVED                                        1511 04288000
*   RFY                                                            1512 04289000
*    (RET,                                                         1512 04290000
*     R6,                                                          1512 04291000
*     R5) UNRSTD;                   /* KEEP COMPILER UP TO DATE.   1512 04292000
*                                                            @Z30SSMD*/ 04293000
*   END SDLOAD;                     /* END OF SDLOAD ROUTINE.      1513 04294000
*                                                            @Z30SSMD*/ 04295000
@EL00029 DS    0H                                                  1513 04296000
@EF00029 DS    0H                                                  1513 04297000
@ER00029 BR    @14                                                 1513 04298000
*FDLMSG:                                                           1514 04299000
*                                                                  1514 04300000
*/********************************************************************/ 04301000
*/*       01 FUNCTION-THIS ROUTINE CALLS IDA0192P TO ISSUE A MSG     */ 04302000
*/*                   INFORMING THE OPERATOR AND THE USER THAT A     */ 04303000
*/*                   FORCE DELETE  VRP WAS DONE AND THAT NO DUMP, A */ 04304000
*/*                   PARTIAL DUMP OR A FULL DUMP WAS TAKEN.       . */ 04305000
*/*       01 INPUT-MWADRET CONTAINS A RETURN CODE INDICATING SDUMP   */ 04306000
*/*                RETURN CODES.                                    .*/ 04307000
*/*       01 OUTPUT-THE CALL TO IDA0192P WILL RESULT IN AN IEC251    */ 04308000
*/*                 MSG FOR THE USER AND THE SYSTEM OPERATOR.       .*/ 04309000
*/********************************************************************/ 04310000
*                                                                  1514 04311000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* FORCE DELETE MSG ROUTINE.     */ 04312000
FDLMSG   DS    0H                                                  1515 04313000
*   RFY                                                            1515 04314000
*     RET RSTD;                     /* INFORM THE COMPILER.  @Z30SSMD*/ 04315000
*   RFY                                                            1516 04316000
*     R1 RSTD;                      /* INFORM THE COMPILER.  @Z30SSMD*/ 04317000
*   MWASAVEC=R14;                   /* SAVE THE RETURN ADDRESS.    1517 04318000
*                                                            @Z30SSMD*/ 04319000
         ST    R14,MWASAVEC                                        1517 04320000
*   PDPNOTRC=ON;                    /* SET SUPRESS GTF TRACE.      1518 04321000
*                                                            @Z30SSMD*/ 04322000
*   PDPNOMSG=ON;                    /* SET SUPRESS MSG AREA .      1519 04323000
*                                                            @Z30SSMD*/ 04324000
         LA    @11,280                                             1519 04325000
         AL    @11,MWACORAD                                        1519 04326000
         OI    PDPNOTRC(@11),B'00110000'                           1519 04327000
*   PDPCALL=PDPCLOSE;               /* NOTE THE CALLER.      @Z30SSMD*/ 04328000
         MVI   PDPCALL(@11),X'04'                                  1520 04329000
*   PDPFUNC=PDF148;                 /* THIS IS FORCE DLVRP.  @Z30SSMD*/ 04330000
         MVI   PDPFUNC(@11),X'94'                                  1521 04331000
*   IF MWADRET=ZERO THEN            /* ALL DUMPS SUCCESSFUL? @Z30SSMD*/ 04332000
         L     @05,MWADRET                                         1522 04333000
         LTR   @05,@05                                             1522 04334000
         BNZ   @RF01522                                            1522 04335000
*     PDPCODE=PP176;                /* SET MSG CODE.         @Z30SSMD*/ 04336000
         MVI   PDPCODE(@11),X'B0'                                  1523 04337000
*   ELSE                            /* IF NOT TEST ANOTHER CONDITION.   04338000
*                                                            @Z30SSMD*/ 04339000
*     IF MWADRET=FOUR THEN          /* PARTIAL DUMPS?        @Z30SSMD*/ 04340000
         B     @RC01522                                            1524 04341000
@RF01522 CLC   MWADRET(4),@CF00143                                 1524 04342000
         BNE   @RF01524                                            1524 04343000
*       PDPCODE=PP180;              /* SET MSG CODE.         @Z30SSMD*/ 04344000
         L     @11,MWACORAD                                        1525 04345000
         MVI   PDPCODE+280(@11),X'B4'                              1525 04346000
*     ELSE                          /* IF NOT THAT THEN TEST AGAIN.     04347000
*                                                            @Z30SSMD*/ 04348000
*       IF MWADRET=ONE|MWADRET=EIGHT THEN/* NO DUMPS?        @Z30SSMD*/ 04349000
         B     @RC01524                                            1526 04350000
@RF01524 L     @11,MWADRET                                         1526 04351000
         C     @11,@CF00080                                        1526 04352000
         BE    @RT01526                                            1526 04353000
         C     @11,@CF00071                                        1526 04354000
         BNE   @RF01526                                            1526 04355000
@RT01526 DS    0H                                                  1527 04356000
*         PDPCODE=PP184;            /* SET MSG CODE.         @Z30SSMD*/ 04357000
         L     @11,MWACORAD                                        1527 04358000
         MVI   PDPCODE+280(@11),X'B8'                              1527 04359000
*   R1=ADDR(FORCORE);               /* POINT TO WORK AREA.   @Z30SSMD*/ 04360000
@RF01526 DS    0H                                                  1528 04361000
@RC01524 DS    0H                                                  1528 04362000
@RC01522 L     R1,MWACORAD                                         1528 04363000
*   CALL IDA0192P;                  /* ISSUE IEC251 MSG.     @Z30SSMD*/ 04364000
         L     @15,@CV05327                                        1529 04365000
         BALR  @14,@15                                             1529 04366000
*   PDPNOTRC=OFF;                   /* RESET SUPRESS GTF TRACE.    1530 04367000
*                                                            @Z30SSMD*/ 04368000
*   PDPNOMSG=OFF;                   /* RESET SUPRESS MSG AREA .    1531 04369000
*                                                            @Z30SSMD*/ 04370000
         L     @11,MWACORAD                                        1531 04371000
         NI    PDPNOTRC+280(@11),B'11001111'                       1531 04372000
*   RET=MWASAVEC;                   /* RESTORE RETURN ADDRESS.     1532 04373000
*                                                            @Z30SSMD*/ 04374000
         L     RET,MWASAVEC                                        1532 04375000
*   RFY                                                            1533 04376000
*     RET UNRSTD;                   /* INFORM THE COMPILER.  @Z30SSMD*/ 04377000
*   RFY                                                            1534 04378000
*     R1 UNRSTD;                    /* FREE-UP REG(1).       @Z30SSMD*/ 04379000
*   END FDLMSG;                     /* END OF FORCE DELETE MESSAGE 1535 04380000
*                                      ROUTINE.              @Z30SSMD*/ 04381000
@EL00030 DS    0H                                                  1535 04382000
@EF00030 DS    0H                                                  1535 04383000
@ER00030 BR    @14                                                 1535 04384000
@PB00030 DS    0H                                                  1535 04385000
