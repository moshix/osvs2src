*       CALL PARMINIT;              /* INITIALIZE RNAME OF INDEX DS     00742000
*                                      IN ENQ PARAMETER LIST         */ 00743000
         BAL   @14,PARMINIT                                        0557 00744000
*       MWAMBLPT=AMBLXPT;           /* POINT TO BASE AMBL            */ 00745000
         L     @11,MWAMBLPT                                        0558 00746000
         L     @11,AMBLXPT(,@11)                                   0558 00747000
         ST    @11,MWAMBLPT                                        0558 00748000
*     END;                                                         0559 00749000
*   IF AMBLIX^=ZERO THEN            /* IF BASE INDEX DS EXIST (KSDS) */ 00750000
@RF00552 L     @11,MWAMBLPT                                        0560 00751000
