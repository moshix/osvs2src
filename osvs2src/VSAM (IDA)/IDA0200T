         L     @05,MWACORAD                                        0420 00289000
         STC   @15,DXUKEY(,@05)                                    0420 00290000
*   ACBDEB->DEBDCBB=RACB;           /* POINT DEB TO COPY ACB.      0421 00291000
*                                                            @Z30SSMD*/ 00292000
@RF00419 L     @15,ACBDEB-1(,RACB)                                 0421 00293000
         LA    @15,0(,@15)                                         0421 00294000
         STCM  RACB,7,DEBDCBB(@15)                                 0421 00295000
*   AMBLACB=RACB;                   /* POINT AMBL TO COPY ACB.     0422 00296000
*                                                            @Z30SSMD*/ 00297000
         L     @14,MWAMBLPT                                        0422 00298000
         ST    RACB,AMBLACB(,@14)                                  0422 00299000
*   MWAAMBPT=AMBLDTA;               /* ADDRESSABILITY FOR DATA AMB   */ 00300000
         L     @11,AMBLDTA(,@14)                                   0423 00301000
         ST    @11,MWAAMBPT                                        0423 00302000
*   MWADDFRE=OFF;                   /* NOTE THAT DUMMY DEB NOT FREED.   00303000
*                                                            @Y30DSMD*/ 00304000
         NI    MWADDFRE,B'01111111'                                0424 00305000
*   MWAAMBLF=OFF;                   /* NOTE AMBL FROM RACB NOT FREED.   00306000
*                                                            @Y30DSMD*/ 00307000
         NI    MWAAMBLF,B'01111111'                                0425 00308000
*   MWADEBPT=ACBDEB;                /* SET MWADEBPT TO DUMMY-DEB.  0426 00309000
*                                                            @Z30SSMD*/ 00310000
         LR    MWADEBPT,@15                                        0426 00311000
*   RFY                                                            0427 00312000
*     DEB BASED(MWADEBPT-LENGTH(DEB));/* THERE IS NO AVT FOR D-DEB.  */ 00313000
*   DEBXCDCB=ON;                    /* PREVENT TASK CLOSE ON THE   0428 00314000
*                                      DUMMY-DEB.            @Z30SSMD*/ 00315000
         LR    @01,MWADEBPT                                        0428 00316000
         AL    @01,@CF05495                                        0428 00317000
         L     @01,DEBXTNP-28(,@01)                                0428 00318000
         OI    DEBXCDCB(@01),B'10000000'                           0428 00319000
*   RFY                                                            0429 00320000
*     DEB BASED(DEBAPPAD);          /* RE-BASE FOR R-DEB.    @Z30SSMD*/ 00321000
*   IF AMBLUCRA=ON THEN             /* IF UCRA                       */ 00322000
         TM    AMBLUCRA(@14),B'00000100'                           0430 00323000
         BNO   @RF00430                                            0430 00324000
*     MWAUCRA=ON;                   /* INDICATE UCRA IN MWA          */ 00325000
         OI    MWAUCRA,B'10000000'                                 0431 00326000
*   IF AMBLLSR=ON THEN              /* IF THIS IS LSR THEN   @Y30DSMD*/ 00327000
@RF00430 L     @05,MWAMBLPT                                        0432 00328000
         TM    AMBLLSR(@05),B'00100000'                            0432 00329000
         BNO   @RF00432                                            0432 00330000
*     MWALSR=ON;                    /* SET WORK AREA LSR SWITCH.   0433 00331000
*                                                            @Y30DSMD*/ 00332000
         OI    MWALSR,B'10000000'                                  0433 00333000
*   ELSE                            /* IF NOT LSR            @Z30SSRL*/ 00334000
*     IF AMBLGSR=ON THEN            /* IS THIS GSR?          @Y30DSMD*/ 00335000
         B     @RC00432                                            0434 00336000
@RF00432 L     @05,MWAMBLPT                                        0434 00337000
         TM    AMBLGSR(@05),B'01000000'                            0434 00338000
         BNO   @RF00434                                            0434 00339000
*       MWAGSR=ON;                  /* IF SO SET GSR SWITCH. @Y30DSMD*/ 00340000
         OI    MWAGSR,B'10000000'                                  0435 00341000
*   IF AMBLCBIC=ON THEN             /* IF CBIC               @ZA16012*/ 00342000
@RF00434 DS    0H                                                  0436 00343000
@RC00432 L     @05,MWAMBLPT                                        0436 00344000
         TM    AMBLCBIC(@05),B'10000000'                           0436 00345000
         BNO   @RF00436                                            0436 00346000
*     MWACBIC=ON;                   /* INDICATE SO           @ZA16012*/ 00347000
         OI    MWACBIC,B'10000000'                                 0437 00348000
*   IF AMBLCAT=OFF THEN             /* IF NOT CATALOG OR SCRA        */ 00349000
@RF00436 L     @05,MWAMBLPT                                        0438 00350000
         TM    AMBLCAT(@05),B'00000010'                            0438 00351000
         BNZ   @RF00438                                            0438 00352000
*     GEN(DEBCHK (RACB),TYPE=VERIFY,AM=VSAM);/* VERIFY DUMMY DEB     */ 00353000
         DEBCHK (RACB),TYPE=VERIFY,AM=VSAM                              00354000
*   ELSE                            /* IF CATALOG OR SCRA            */ 00355000
*     DO;                                                          0440 00356000
         B     @RC00438                                            0440 00357000
@RF00438 DS    0H                                                  0441 00358000
*       MWACAT=ON;                  /* INDICATE CATALOG IN MWA       */ 00359000
         OI    MWACAT,B'10000000'                                  0441 00360000
*       IF(AMBCREAT=OFF&            /* DATA AMB INDICATE NOT CREATE     00361000
*                                      AND                           */ 00362000
*           ACBSDS=OFF) THEN        /* NOT SYSTEM DATA SET   @Z40WSRL*/ 00363000
         L     @05,MWAAMBPT                                        0442 00364000
         TM    AMBCREAT(@05),B'10000000'                           0442 00365000
         BNZ   @RF00442                                            0442 00366000
         TM    ACBSDS(RACB),B'00000010'                            0442 00367000
         BNZ   @RF00442                                            0442 00368000
*         CLWBNOFL=ON;              /* INDICATE SUPPRESS BUFFER FLUSH*/ 00369000
         OI    CLWBNOFL(RCORE),B'10000000'                         0443 00370000
*     END;                                                         0444 00371000
@RF00442 DS    0H                                                  0445 00372000
*   ACBERFLG=ZERO;                  /* ZERO ACB ERROR FLAG   @Z30SSRL*/ 00373000
@RC00438 MVI   ACBERFLG(RACB),X'00'                                0445 00374000
*   IF(AMBLCAT=ON&AMBCREAT=OFF)|    /* CATALOG AND NOT CREATE OR     */ 00375000
*       AMBLSCRA=ON|AMBLUCRA=ON THEN/* SCRA OR RESET         @Z30SSMD*/ 00376000
         L     @05,MWAMBLPT                                        0446 00377000
         TM    AMBLCAT(@05),B'00000010'                            0446 00378000
         BNO   @GL00005                                            0446 00379000
         L     @05,MWAAMBPT                                        0446 00380000
         TM    AMBCREAT(@05),B'10000000'                           0446 00381000
         BZ    @RT00446                                            0446 00382000
