         TM    AMBLFIX(@05),B'00001000'                            0359 00061000
         BNO   @RF00359                                            0359 00062000
*     CALL PGFRAMBL;                /* PAGE FREE BASE AMBL           */ 00063000
         BAL   @14,PGFRAMBL                                        0360 00064000
