*                                                                  1416 03826000
         MVI   BLPERFLG(@15),X'00'                                 1416 03827000
*   /*****************************************************************/ 03828000
*   /*                                                               */ 03829000
*   /* CALL IDA0192Y TO DO A DLVRP.                          @Y30DSMD*/ 03830000
*   /*                                                               */ 03831000
*   /*****************************************************************/ 03832000
*                                                                  1417 03833000
*   MWASAVED=RACB;                  /* SAVE ACB POINTER.     @Y30DSMD*/ 03834000
         ST    RACB,MWASAVED                                       1417 03835000
*   MWASAVEE=RCORE;                 /* SAVE POINTER TO CLWRK.      1418 03836000
