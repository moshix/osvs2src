         TM    AMBLCAT(@05),B'00000010'                            0362 00071000
         BNO   @RF00362                                            0362 00072000
*     MWASPOOL=CATPOOL;             /* CATALOG SUBPOOL NUMBER        */ 00073000
         MVI   MWASPOOL,X'E7'                                      0363 00074000
*   ELSE                            /* NOT CATALOG OR SCRA   @Z30SSRL*/ 00075000
*     IF AMBLGSR=ON|AMBLCBIC=ON THEN/* IF GSR OR CBIC        @ZA16012*/ 00076000
         B     @RC00362                                            0364 00077000
@RF00362 L     @05,MWAMBLPT                                        0364 00078000
         TM    AMBLGSR(@05),B'01000000'                            0364 00079000
         BO    @RT00364                                            0364 00080000
         TM    AMBLCBIC(@05),B'10000000'                           0364 00081000
         BNO   @RF00364                                            0364 00082000
@RT00364 DS    0H                                                  0365 00083000
*       MWASPOOL=GSRSPOOL;          /* GSR SUBPOOL NUMBER    @Z30SSRL*/ 00084000
         MVI   MWASPOOL,X'F1'                                      0365 00085000
*     ELSE                          /* IF NOT GSR            @Z30SSRL*/ 00086000
*       MWASPOOL=AMBLPOOL;          /* DEFAULT TO SUBPOOL FOR AMBL 0366 00087000
*                                                            @Z30SSRL*/ 00088000
         B     @RC00364                                            0366 00089000
@RF00364 MVI   MWASPOOL,X'FC'                                      0366 00090000
*   MWABYTES=LENGTH(IDAAMBL);       /* LENGTH OF AMBL                */ 00091000
@RC00364 DS    0H                                                  0367 00092000
@RC00362 MVC   MWABYTES(3),@CF05095+1                              0367 00093000
*   CALL FREECORE;                  /* FREE BASE AMBL                */ 00094000
         BAL   @14,FREECORE                                        0368 00095000
*   MWAAMBLF=ON;                    /* INDICATE AMBL FREED   @Z30SSRL*/ 00096000
         OI    MWAAMBLF,B'10000000'                                0369 00097000
*LTRM200T:                                                         0370 00098000
*   CALL TERM200T;                  /* CALL TERMINATION PROCESSING   */ 00099000
LTRM200T BAL   @14,TERM200T                                        0370 00100000
*ENDLABEL:                                                         0371 00101000
*   GEN(LM RET,R12,12(R13));        /* RESTORE REGISTERS             */ 00102000
ENDLABEL LM RET,R12,12(R13)                                             00103000
*   GEN(BR RET);                    /* EXIT MODULE                   */ 00104000
         BR RET                                                         00105000
*INIT200T:                                                         0373 00106000
*                                                                  0373 00107000
*/********************************************************************/ 00108000
*/*                                                                  */ 00109000
*/*      01   FUNCTION-INITIALIZE PARAMETER LIST FOR PROBLEM         */ 00110000
*/*                    DETERMINATION                                 */ 00111000
*/*                    BUILD MODULE WORK AREA (MWA)                  */ 00112000
*/*                    BUILD ACB WORK AREA (CLW)                     */ 00113000
*/*                    ENQ ALL DATA SETS                             */ 00114000
*/*      01   INPUTS-  1. ADDR OF COMMON WORK AREA IN RCORE          */ 00115000
*/*                    2. ADDR OF PREVIOUS SAVE AREA IN REGISTER 13  */ 00116000
*/*                    3. ADDR OF USER ACB IN REGISTER 2             */ 00117000
*/*      01   OUTPUTS- 1. ADDR OF ACB WORK AREA IN RCORE             */ 00118000
*/*                    2. ADDR OF MODULE WORK AREA IN REGISTER 13    */ 00119000
*/*                                                                  */ 00120000
*/********************************************************************/ 00121000
*                                                                  0373 00122000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0373 00123000
INIT200T DS    0H                                                  0374 00124000
*   RESPECIFY                                                      0374 00125000
*    (R11,                                                         0374 00126000
*     RET) RESTRICTED;                                             0374 00127000
*   R11=RET;                        /* SAVE RETURN ADDR IN R11       */ 00128000
         LR    R11,RET                                             0375 00129000
*   RESPECIFY                                                      0376 00130000
*     RET UNRESTRICTED;                                            0376 00131000
*   IDAPDPRM=IDAPDPRM&&IDAPDPRM;    /* ZERO PROBLEM DETERMINATION  0377 00132000
*                                      (PD) PARAMETER LIST           */ 00133000
         LA    @10,280                                             0377 00134000
         ALR   @10,RCORE                                           0377 00135000
         XC    IDAPDPRM(40,@10),IDAPDPRM(@10)                      0377 00136000
*   PDPCALL=PDPCLOSE;               /* INDICATE CALLER IS CLOSE      */ 00137000
         MVI   PDPCALL(@10),X'04'                                  0378 00138000
*   PDPFUNC=PDF101;                 /* SET FUNCTION CODE IN PD LIST  */ 00139000
         MVI   PDPFUNC(@10),X'65'                                  0379 00140000
*   PDPACB=RACB;                    /* SET PTR TO USER ACB FOR PD    */ 00141000
         ST    RACB,PDPACB(,@10)                                   0380 00142000
*   PDPDSTYP=PDPDDNM;               /* INDICATE DD DATA SET NAME USED*/ 00143000
         MVI   PDPDSTYP(@10),X'01'                                 0381 00144000
*   PDPCLSNM=ADDR(JFCBDSNM);        /* SET CLUSTER NAME TO USER DS 0382 00145000
*                                      NAME IN DD STATEMENT          */ 00146000
         LA    @05,100                                             0382 00147000
         ALR   @05,RCORE                                           0382 00148000
         ST    @05,PDPCLSNM(,@10)                                  0382 00149000
*   PDPDSN=ZERO;                    /* ZERO OUT DS NAME              */ 00150000
         SLR   @05,@05                                             0383 00151000
         ST    @05,PDPDSN(,@10)                                    0383 00152000
*   IF ACBAMBL->AMBLDUMY=OFF THEN   /* NOT DUMMY DATA SET            */ 00153000
         L     @05,ACBAMBL(,RACB)                                  0384 00154000
         TM    AMBLDUMY(@05),B'00000001'                           0384 00155000
         BNZ   @RF00384                                            0384 00156000
*     PDPCTACB=ACBAMBL->AMBLDTA->AMBCACB;/* SET PTR TO CATALOG ACB   */ 00157000
         L     @05,AMBLDTA(,@05)                                   0385 00158000
         L     @05,AMBCACB(,@05)                                   0385 00159000
         ST    @05,PDPCTACB(,@10)                                  0385 00160000
*   PDPTIOT=DXTIOTAD;               /* POINT TO CORRECT TIOT ENTRY 0386 00161000
*                                                            @Y30DSMD*/ 00162000
@RF00384 L     @10,DXTIOTAD(,RCORE)                                0386 00163000
         ST    @10,PDPTIOT+280(,RCORE)                             0386 00164000
*/********************************************************************/ 00165000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 00166000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 00167000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 00168000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 00169000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 00170000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 00171000
*/********************************************************************/ 00172000
*                                                                  0387 00173000
*   GEN;                                                           0387 00174000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00175000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00176000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00177000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00178000
*                                       IF NO ERROR WAS ENCOUNTERED     00179000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00180000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00181000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00182000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00183000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00184000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00185000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00186000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00187000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00188000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00189000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00190000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00191000
         B     GMN00200                 BYPASS SETTING ERROR            00192000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00193000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00194000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00195000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00196000
*   RESPECIFY                                                      0388 00197000
*    (R1,                                                          0388 00198000
*     R11,                                                         0388 00199000
*     R12,                                                         0388 00200000
*     R13,                                                         0388 00201000
*     RETCODE) RESTRICTED;                                         0388 00202000
*   IF RETCODE=ZERO THEN            /* IF GETMAIN FAILED             */ 00203000
         LTR   RETCODE,RETCODE                                     0389 00204000
         BNZ   @RF00389                                            0389 00205000
*     DO;                                                          0390 00206000
*       PDPCODE=PP20;               /* SET PROBLEM CODE FOR PD       */ 00207000
         MVI   PDPCODE+280(RCORE),X'14'                            0391 00208000
*       ACBERFLG=OPERR136;          /* SET ACB ERROR FLAG            */ 00209000
         MVI   ACBERFLG(RACB),X'88'                                0392 00210000
*       R1=RCORE;                   /* COPY PTR TO COMMON WORK AREA  */ 00211000
         LR    R1,RCORE                                            0393 00212000
*       RSTATRTN=IDAR192P;          /* NOTE CONTROL GIVEN TO       0394 00213000
*                                      IDA0192P.             @Y30DSMD*/ 00214000
         MVC   RSTATRTN(4,RCORE),@CC04071                          0394 00215000
*       CALL IDA0192P;              /* CALL PROBLEM DETERMINATION    */ 00216000
         L     @15,@CV05327                                        0395 00217000
         BALR  @14,@15                                             0395 00218000
*       RSTATRTN=IDAR200T;          /* NOTE CSECT NOW IN CONTROL.  0396 00219000
*                                                            @Y30DSMD*/ 00220000
         MVC   RSTATRTN(4,RCORE),@CC04085                          0396 00221000
*       RETURN TO ENDLABEL;         /* RETURN TO IDA0200T AND EXIT   */ 00222000
         LA    @14,ENDLABEL                                        0397 00223000
@EL00002 DS    0H                                                  0397 00224000
@EF00002 DS    0H                                                  0397 00225000
@ER00002 BR    @14                                                 0397 00226000
*     END;                                                         0398 00227000
*   GEN(ST R13,4(RETCODE));         /* BACKCHAIN TO PREVIUOS SAVEAREA*/ 00228000
@RF00389 DS    0H                                                  0399 00229000
         ST R13,4(RETCODE)                                              00230000
*   R13=RETCODE;                    /* ADDR OF MODULE WORK AREA TO 0400 00231000
*                                      R13                           */ 00232000
         LR    R13,RETCODE                                         0400 00233000
*   RESPECIFY                                                      0401 00234000
*    (R1,                                                          0401 00235000
*     RETCODE) UNRESTRICTED;                                       0401 00236000
*   GEN(USING @DATD,R13);           /* ESTABLISH ADDRESSABILITY      */ 00237000
         USING @DATD,R13                                                00238000
*   MWASAVEA=R11;                   /* SAVE RETURN ADDR IN MWA       */ 00239000
         ST    R11,MWASAVEA                                        0403 00240000
*   RESPECIFY                                                      0404 00241000
*     R11 UNRESTRICTED;                                            0404 00242000
*   MWACORAD=RCORE;                 /* SAVE COMMON WORK AREA ADDRESS */ 00243000
         ST    RCORE,MWACORAD                                      0405 00244000
*   RESPECIFY                                                      0406 00245000
*     FORCORE BASED(MWACORAD);                                     0406 00246000
*   GEN( LA RACB,0(,RACB));         /* ZERO THE OPTION BYTE. @Y30DSMD*/ 00247000
          LA RACB,0(,RACB)                                              00248000
*   MWARACB=RACB;                   /* SAVE USER ACB ADDRESS         */ 00249000
         ST    RACB,MWARACB                                        0408 00250000
*   MWATIOTP=PDPTIOT;               /* SAVE TIOT DD ENTRY ADDR     0409 00251000
*                                                            @ZA19549*/ 00252000
         L     @05,MWACORAD                                        0409 00253000
         L     @05,PDPTIOT+280(,@05)                               0409 00254000
         ST    @05,MWATIOTP                                        0409 00255000
*   MWABYTES=LENGTH(IDACLWRK);      /* BYTES FOR ACB WORK AREA       */ 00256000
         MVC   MWABYTES(3),@CF04755+1                              0410 00257000
*   MWASPOOL=WKPOOL;                /* SUBPOOL NUMBER                */ 00258000
         MVI   MWASPOOL,X'FC'                                      0411 00259000
*   CALL GETCORE;                   /* BUILD ACB WORK AREA (CLW)     */ 00260000
         BAL   @14,GETCORE                                         0412 00261000
*   MWACBWK=ON;                     /* SET BIT TO INDICATE CLW BUILT */ 00262000
         OI    MWACBWK,B'10000000'                                 0413 00263000
*   RCORE=MWADDRES;                 /* REGISTER 4 GETS ADDR OF CLW   */ 00264000
         L     RCORE,MWADDRES                                      0414 00265000
*   CLWCOMWK=MWACORAD;              /* SAVE COMMON WORK AREA ADDR IN    00266000
*                                      CLW                           */ 00267000
         L     @05,MWACORAD                                        0415 00268000
         ST    @05,CLWCOMWK(,RCORE)                                0415 00269000
*   CLWID='IDACLWRK';               /* SET CLW ID            @Z30SSRL*/ 00270000
         MVC   CLWID(8,RCORE),@CC05330                             0416 00271000
*   MWAMBLPT=ACBAMBL;               /* ADDRESSABILITY FOR AMBL       */ 00272000
         L     @05,ACBAMBL(,RACB)                                  0417 00273000
         ST    @05,MWAMBLPT                                        0417 00274000
*   MWAVGTT=BIBVGTT;                /* MAKE A LOCAL COPY OF VGTT   0418 00275000
*                                      POINTER.              @Z30SSMD*/ 00276000
         L     @01,AMBLBIB(,@05)                                   0418 00277000
         L     @15,BIBVGTT(,@01)                                   0418 00278000
         ST    @15,MWAVGTT                                         0418 00279000
*   IF AMBLGSR=ON|AMBLCBIC=ON THEN  /* IF GSR OR CBIC        @ZA16012*/ 00280000
         TM    AMBLGSR(@05),B'01000000'                            0419 00281000
         BO    @RT00419                                            0419 00282000
         TM    AMBLCBIC(@05),B'10000000'                           0419 00283000
         BNO   @RF00419                                            0419 00284000
@RT00419 DS    0H                                                  0420 00285000
*     DXUKEY=VGTTGSRK;              /* SET USER KEY TO GSR   @ZA16012*/ 00286000
         L     @05,MWAVGTT                                         0420 00287000
