*                                      OCCURS.               #Y30DSMD*/ 03002000
*             IF VSRTASCB=ZERO THEN /* IF THE ASCB PTR HAS BEEN    1198 03003000
*                                      ZEROED,                       */ 03004000
         L     @05,BWAVSPTR(,@05)                                  1198 03005000
         C     @11,VSRTASCB(,@05)                                  1198 03006000
         BNE   @RF01198                                            1198 03007000
*               DO;                 /* THEN THE VSRT WILL BE FORCE 1199 03008000
*                                      DELETED.              #Y30DSMD*/ 03009000
*                 CALL FDLVRP;      /* CALL FORCE DELETE VRP.      1200 03010000
*                                                            #Y30DSMD*/ 03011000
         BAL   @14,FDLVRP                                          1200 03012000
*               END;                /* END OF FORCE DLVRP.   #Y30DSMD*/ 03013000
*           END;                    /* END OF LOGIC FOR            1202 03014000
*                                      VSRTASCB=ZERO.        #Y30DSMD*/ 03015000
@RF01198 DS    0H                                                  1203 03016000
*       END;                        /* END OF GSR PROCESSING.      1203 03017000
*                                                            @Y30DSMD*/ 03018000
*                                                                  1203 03019000
@RF01196 DS    0H                                                  1204 03020000
*   /*****************************************************************/ 03021000
*   /*                                                               */ 03022000
*   /* IF THIS IS NOT CTLG(CTLG,SCRA,UCRA) OR LSR AND IS NOT A DUMMY */ 03023000
*   /* DATASET THEN A VGTT EXISTS FOR THE SPHERE. IT IS DECHAINED AND*/ 03024000
*   /* FREEMAINED.                                                   */ 03025000
*   /*                                                               */ 03026000
*   /*****************************************************************/ 03027000
*                                                                  1204 03028000
*   IF((AMBLCAT=OFF&AMBLLSR=OFF)|   /* NOT CTLG OR LSR       @ZA29854*/ 03029000
*       ACBSDS=ON)&                 /* OR SYSTEM DATA SET    @ZA29854*/ 03030000
*       AMBLDUMY=OFF THEN           /* AND NOT DUMMY D/S     @ZA29854*/ 03031000
@RF01184 DS    0H                                                  1204 03032000
@RC01177 L     @11,MWAMBLPT                                        1204 03033000
         TM    AMBLCAT(@11),B'00000010'                            1204 03034000
         BNZ   @GL00036                                            1204 03035000
         TM    AMBLLSR(@11),B'00100000'                            1204 03036000
         BZ    @GL00035                                            1204 03037000
@GL00036 TM    ACBSDS(RACB),B'00000010'                            1204 03038000
         BNO   @RF01204                                            1204 03039000
@GL00035 L     @11,MWAMBLPT                                        1204 03040000
         TM    AMBLDUMY(@11),B'00000001'                           1204 03041000
         BNZ   @RF01204                                            1204 03042000
*     DO;                           /* PROCESS THE VGTT.     #Y30DSMD*/ 03043000
*       CALL REMVGTT;               /* REMOVE VGTT.          #Y30DSMD*/ 03044000
         BAL   @14,REMVGTT                                         1206 03045000
*       CALL FREVGTT;               /* THEN FREEMAIN IT.     #Y30DSMD*/ 03046000
         BAL   @14,FREVGTT                                         1207 03047000
*     END;                          /* END OF SPHERE VGTT PROCESSING.   03048000
*                                                            #Y30DSMD*/ 03049000
*   RESPECIFY                                                      1209 03050000
*     RET RESTRICTED;                                              1209 03051000
@RF01204 DS    0H                                                  1210 03052000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 03053000
         L     RET,MWASAVEA                                        1210 03054000
*   RESPECIFY                                                      1211 03055000
*     RET UNRESTRICTED;                                            1211 03056000
*   END CLSPHERE;                                                  1212 03057000
@EL00017 DS    0H                                                  1212 03058000
@EF00017 DS    0H                                                  1212 03059000
@ER00017 BR    @14                                                 1212 03060000
*FREESPHR:                                                         1213 03061000
*                                                                  1213 03062000
*/********************************************************************/ 03063000
*/*                                                                  */ 03064000
*/*      01   FUNCTION-FREE SPHERE OR PROTECTED SPHERE BLOCK         */ 03065000
*/*      01   INPUTS-  1. POINTER TO SPHERE OR PROTECTED SPHERE BLOCK*/ 03066000
*/*                       IN MWAHEBPT                                */ 03067000
*/*      01   OUTPUTS- 1. CONTROL BLOCKS FREED                       */ 03068000
*/*                                                                  */ 03069000
*/* IN VS2 THIS ROUTINE IS MODIFIED TO DETERMINE THE KEY OF THE CORE */ 03070000
*/* FROM THE HEB ENTRY BEING PROCESSED. IF THE HEBKEY5 FIELD IS ON   */ 03071000
*/* THEN A MODESET TO KEY50 IS DONE PRIOR TO THE FREEMAIN. IF       .*/ 03072000
*/* HEBGSRKT IS ON THEN A MODESET TO GSRKEY. IF THE SUBPOOL  @Z40ESSR*/ 03073000
*/* IS 250 THEN NEITHER FIELD SHUOLD HAVE BEEN SET. IF NEITHER     . */ 03074000
*/* HEBKEY5 NOR HEBGSRKT IS ON THEN FREEMAIN MUST BE DONE IN @Z40ESSR*/ 03075000
*/* KEY ZERO. THIS ROUTINE PASSES THE KEY TO THE FREECORE PROCEDURE  */ 03076000
*/* IN FIELD MWAFRKEY.                                       @Y30DSMD*/ 03077000
*/*                                                                 .*/ 03078000
*/********************************************************************/ 03079000
*                                                                  1213 03080000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1213 03081000
FREESPHR DS    0H                                                  1214 03082000
*   RESPECIFY                                                      1214 03083000
*     RET RESTRICTED;                                              1214 03084000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 03085000
         ST    RET,MWASAVEB                                        1215 03086000
*   RESPECIFY                                                      1216 03087000
*     RET UNRESTRICTED;                                            1216 03088000
*   DO WHILE MWAHEBPT^=ZERO;        /* LOOP THRUGH SPHERE BLOCK CHAIN*/ 03089000
         B     @DE01217                                            1217 03090000
@DL01217 DS    0H                                                  1218 03091000
*     MWADDRES=HEBBLKPT;            /* ADDR OF SPHERE BLOCK TO BE  1218 03092000
*                                      FREED                         */ 03093000
         L     @07,HEBBLKPT(,MWAHEBPT)                             1218 03094000
         ST    @07,MWADDRES                                        1218 03095000
*     MWASPOOL=HEBSP;               /* SUBPOOL OF SPHERE BLOCKS      */ 03096000
         IC    @07,HEBSP(,MWAHEBPT)                                1219 03097000
         STC   @07,MWASPOOL                                        1219 03098000
*     MWABYTES=HEBLN;               /* LENGTH OF SPHERE BLOCK        */ 03099000
         MVC   MWABYTES(3),HEBLN(MWAHEBPT)                         1220 03100000
*     MWAFRKEY=ZERO;                /* SET DEFAULT TO NO MODESET   1221 03101000
*                                      NEEDED.               @Z30SSMD*/ 03102000
         MVI   MWAFRKEY,X'00'                                      1221 03103000
*     IF HEBKEY5=ON THEN            /* SPECIAL KEY 5 CORE?   @Z30SSMD*/ 03104000
         TM    HEBKEY5(MWAHEBPT),B'01000000'                       1222 03105000
         BNO   @RF01222                                            1222 03106000
*       MWAFRKEY=K50;               /* INDICATE MODESET TO KEY FIVE     03107000
*                                      NEEDED.               @Z30SSMD*/ 03108000
         MVI   MWAFRKEY,X'50'                                      1223 03109000
*     ELSE                          /* IF NOT FIVE TEST FOR SEVEN. 1224 03110000
*                                                            @Z30SSMD*/ 03111000
*       IF HEBGSRKT=ON THEN         /* IS CORE IN GSR KEY?   @Z40ESSR*/ 03112000
         B     @RC01222                                            1224 03113000
@RF01222 TM    HEBGSRKT(MWAHEBPT),B'00100000'                      1224 03114000
         BNO   @RF01224                                            1224 03115000
*         MWAFRKEY=HEBGSRK;         /* PASS GSR KEY          @Z40ESSR*/ 03116000
         IC    @07,HEBGSRK(,MWAHEBPT)                              1225 03117000
         STC   @07,MWAFRKEY                                        1225 03118000
*     MWAHEBPT=HEBELCHN;            /* POINT TO NEXT SPHERE BLOCK    */ 03119000
@RF01224 DS    0H                                                  1226 03120000
@RC01222 L     MWAHEBPT,HEBELCHN(,MWAHEBPT)                        1226 03121000
*     IF MWAPSBSW=ON THEN           /* PSB BEING FREED       @Z40WSRL*/ 03122000
         TM    MWAPSBSW,B'10000000'                                1227 03123000
         BNO   @RF01227                                            1227 03124000
*       VGTTPSB=MWAHEBPT;           /* IF SO NOTE THE NEXT PSB ADDRS.   03125000
*                                                            @Y30DSMD*/ 03126000
         L     @07,MWAVGTT                                         1228 03127000
         ST    MWAHEBPT,VGTTPSB(,@07)                              1228 03128000
*     CALL FREECORE;                /* FREE THE SPHERE BLOCK @Z40WSRL*/ 03129000
@RF01227 BAL   @14,FREECORE                                        1229 03130000
*   END;                                                           1230 03131000
@DE01217 LTR   MWAHEBPT,MWAHEBPT                                   1230 03132000
         BNZ   @DL01217                                            1230 03133000
*   MWAPSBSW=OFF;                   /* RESET PSB SWITCH NOW THAT ALL    03134000
*                                      ARE DONE.             @Y30DSMD*/ 03135000
         NI    MWAPSBSW,B'01111111'                                1231 03136000
*   RESPECIFY                                                      1232 03137000
*     RET RESTRICTED;                                              1232 03138000
*   RET=MWASAVEB;                   /* GET RETURN ADDRESS            */ 03139000
         L     RET,MWASAVEB                                        1233 03140000
*   RESPECIFY                                                      1234 03141000
*     RET UNRESTRICTED;                                            1234 03142000
*   END FREESPHR;                                                  1235 03143000
@EL00018 DS    0H                                                  1235 03144000
@EF00018 DS    0H                                                  1235 03145000
@ER00018 BR    @14                                                 1235 03146000
*VMTPROC:                                                          1236 03147000
*                                                                  1236 03148000
*/********************************************************************/ 03149000
*/*                                                                  */ 03150000
*/*      01   FUNCTION-DECREMENT UCB USE COUNTS AND FREE VMT'S       */ 03151000
*/*      01   INPUTS-  1. POINTER TO FIRST VMT IN BIBVMT             */ 03152000
*/*      01   OUTPUTS- 1. VMT'S FREED AND UCB USE COUNTS DECREMENTED */ 03153000
*/*                                                                  */ 03154000
*/********************************************************************/ 03155000
*                                                                  1236 03156000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1236 03157000
VMTPROC  DS    0H                                                  1237 03158000
*   RESPECIFY                                                      1237 03159000
*     RET RESTRICTED;                                              1237 03160000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 03161000
         ST    RET,MWASAVEB                                        1238 03162000
*   RESPECIFY                                                      1239 03163000
*     RET UNRESTRICTED;                                            1239 03164000
*   MWAVMTPT=BIBVMT;                /* POINT TO FIRST VMT            */ 03165000
         L     @05,MWAMBLPT                                        1240 03166000
         L     @01,AMBLBIB(,@05)                                   1240 03167000
         L     MWAVMTPT,BIBVMT(,@01)                               1240 03168000
*   PDPFUNC=PDF108;                 /* SET FUNCTION CODE FOR PD      */ 03169000
         L     @08,MWACORAD                                        1241 03170000
         MVI   PDPFUNC+280(@08),X'6C'                              1241 03171000
*   IF AMBLCAT=ON&AMBLSCRA=OFF&AMBLSDS=OFF THEN/* IF THIS IS CATALOG    03172000
*                                                            @Z40WSRL*/ 03173000
         TM    AMBLCAT(@05),B'00000010'                            1242 03174000
         BNO   @RF01242                                            1242 03175000
         TM    AMBLSCRA(@05),B'00011000'                           1242 03176000
         BNZ   @RF01242                                            1242 03177000
*     DO;                                                          1243 03178000
*       IF TIOEFSRT=VMTUCB(1) THEN  /* TIOT UCB MATCH VMT UCB        */ 03179000
         L     @08,VMTUCB(,MWAVMTPT)                               1244 03180000
         L     @05,MWATIOTP                                        1244 03181000
         L     @05,TIOEFSRT-1(,@05)                                1244 03182000
         LA    @05,0(,@05)                                         1244 03183000
         CR    @08,@05                                             1244 03184000
         BNE   @RF01244                                            1244 03185000
*         DO;                                                      1245 03186000
*           MWAUCBPT=VMTUCB(1);     /* POINT TO UCB                  */ 03187000
         LR    MWAUCBPT,@08                                        1246 03188000
*           CALL DCRUCBCT;          /* DECREMENT UCB USE COUNT       */ 03189000
         BAL   @14,DCRUCBCT                                        1247 03190000
*         END;                                                     1248 03191000
*       MWADDRES=MWAVMTPT;          /* ADDRESS OF VMT TO BE FREED    */ 03192000
@RF01244 ST    MWAVMTPT,MWADDRES                                   1249 03193000
*       MWABYTES=LENGTH(IDAVMT)+VMTNOVOL*LENGTH(VMTVOL);/* BYTES NO. */ 03194000
         LH    @08,VMTNOVOL(,MWAVMTPT)                             1250 03195000
         SLA   @08,4                                               1250 03196000
         LA    @08,16(,@08)                                        1250 03197000
         STCM  @08,7,MWABYTES                                      1250 03198000
*       MWASPOOL=CATPOOL;           /* SUBPOOL NUMBER FOR VMT        */ 03199000
         MVI   MWASPOOL,X'E7'                                      1251 03200000
*       CALL FREECORE;              /* FREE VMT                      */ 03201000
         BAL   @14,FREECORE                                        1252 03202000
*     END;                                                         1253 03203000
*   ELSE                            /* NOT CATALOG                   */ 03204000
*     DO WHILE MWAVMTPT^=ZERO;      /* IF VMT EXIST                  */ 03205000
         B     @RC01242                                            1254 03206000
@RF01242 B     @DE01254                                            1254 03207000
@DL01254 DS    0H                                                  1255 03208000
*       IF AMBLSCRA=OFF&            /* NOT SCRA AND          @Z30SSMD*/ 03209000
*           AMBLUCRA=OFF THEN       /* NOT UCRA THEN         @Z30SSMD*/ 03210000
         L     @08,MWAMBLPT                                        1255 03211000
         TM    AMBLSCRA(@08),B'00001100'                           1255 03212000
         BNZ   @RF01255                                            1255 03213000
*         DO MWAINDEX=1 TO VMTNOVOL;/* LOOP THROUGH ENTRIES          */ 03214000
         LA    @08,1                                               1256 03215000
         B     @DE01256                                            1256 03216000
@DL01256 DS    0H                                                  1257 03217000
*           IF VMTVLSER(MWAINDEX)^=MWAVLSER THEN/* VALID VOL SER NO. */ 03218000
         SLA   @08,4                                               1257 03219000
         ST    @08,@TF00001                                        1257 03220000
         ALR   @08,MWAVMTPT                                        1257 03221000
         CLC   VMTVLSER-16(6,@08),MWAVLSER                         1257 03222000
         BE    @RF01257                                            1257 03223000
*             DO;                                                  1258 03224000
*               MWAUCBPT=VMTUCB(MWAINDEX);/* POINT TO UCB            */ 03225000
         L     @08,@TF00001                                        1259 03226000
         L     MWAUCBPT,VMTUCB-16(@08,MWAVMTPT)                    1259 03227000
*               CALL DCRUCBCT;      /* DECREMENT UCB USE COUNT       */ 03228000
         BAL   @14,DCRUCBCT                                        1260 03229000
*             END;                                                 1261 03230000
*         END;                                                     1262 03231000
@RF01257 LA    @08,1                                               1262 03232000
         AL    @08,MWAINDEX                                        1262 03233000
@DE01256 ST    @08,MWAINDEX                                        1262 03234000
         CH    @08,VMTNOVOL(,MWAVMTPT)                             1262 03235000
         BNH   @DL01256                                            1262 03236000
*       MWADDRES=MWAVMTPT;          /* ADDRESS OF VMT TO BE FREED    */ 03237000
@RF01255 ST    MWAVMTPT,MWADDRES                                   1263 03238000
*       MWABYTES=LENGTH(IDAVMT)+VMTNOVOL*LENGTH(VMTVOL);/* BYTES TO     03239000
*                                      BE FREED                      */ 03240000
         LH    @08,VMTNOVOL(,MWAVMTPT)                             1264 03241000
         SLA   @08,4                                               1264 03242000
         LA    @08,16(,@08)                                        1264 03243000
         STCM  @08,7,MWABYTES                                      1264 03244000
*       IF AMBLCAT=ON THEN          /* IF CATALOG OR SCRA            */ 03245000
         L     @08,MWAMBLPT                                        1265 03246000
         TM    AMBLCAT(@08),B'00000010'                            1265 03247000
         BNO   @RF01265                                            1265 03248000
*         MWASPOOL=CATPOOL;         /* SUBPOOL NUMBER FOR CATALOG    */ 03249000
         MVI   MWASPOOL,X'E7'                                      1266 03250000
*       ELSE                        /* NOT CATALOG OR SCRA   @Z30SSRL*/ 03251000
*         IF AMBLGSR=ON|AMBLCBIC=ON THEN/* IS THIS GSR OR CBIC     1267 03252000
*                                                            @ZA16012*/ 03253000
         B     @RC01265                                            1267 03254000
@RF01265 L     @08,MWAMBLPT                                        1267 03255000
         TM    AMBLGSR(@08),B'01000000'                            1267 03256000
         BO    @RT01267                                            1267 03257000
         TM    AMBLCBIC(@08),B'10000000'                           1267 03258000
         BNO   @RF01267                                            1267 03259000
@RT01267 DS    0H                                                  1268 03260000
*           MWASPOOL=GSRSPOOL;      /* YES, USE GLOBAL CORE. @Y30DSMD*/ 03261000
         MVI   MWASPOOL,X'F1'                                      1268 03262000
*         ELSE                      /* NOT CATALOG OR SCRA OR GSR.   */ 03263000
*           MWASPOOL=VMTPOOL;       /* SUBPOOL NUMBER FOR VMT        */ 03264000
         B     @RC01267                                            1269 03265000
@RF01267 MVI   MWASPOOL,X'FC'                                      1269 03266000
*       MWAVMTPT=VMTNXT;            /* POINT TO NEXT VMT             */ 03267000
@RC01267 DS    0H                                                  1270 03268000
@RC01265 L     MWAVMTPT,VMTNXT(,MWAVMTPT)                          1270 03269000
*       CALL FREECORE;              /* FREE VMT'S                    */ 03270000
         BAL   @14,FREECORE                                        1271 03271000
*     END;                                                         1272 03272000
@DE01254 LTR   MWAVMTPT,MWAVMTPT                                   1272 03273000
         BNZ   @DL01254                                            1272 03274000
*   RESPECIFY                                                      1273 03275000
*     RET RESTRICTED;                                              1273 03276000
@RC01242 DS    0H                                                  1274 03277000
*   RET=MWASAVEB;                   /* GET RETURN ADDRESS            */ 03278000
         L     RET,MWASAVEB                                        1274 03279000
*   RESPECIFY                                                      1275 03280000
*     RET UNRESTRICTED;                                            1275 03281000
*   END VMTPROC;                                                   1276 03282000
@EL00019 DS    0H                                                  1276 03283000
@EF00019 DS    0H                                                  1276 03284000
@ER00019 BR    @14                                                 1276 03285000
*DCRUCBCT:                                                         1277 03286000
*                                                                  1277 03287000
*/********************************************************************/ 03288000
*/*                                                                  */ 03289000
*/*      01   FUNCTION-DECREMENT UCB USE COUNT                       */ 03290000
*/*      01   INPUTS-  1. POINTER TO UCB IN MWAUCBPT                 */ 03291000
*/*      01   OUTPUTS- 1. UCB USE COUNT DECREMENTED                  */ 03292000
*/*                                                                  */ 03293000
*/********************************************************************/ 03294000
*                                                                  1277 03295000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1277 03296000
DCRUCBCT DS    0H                                                  1278 03297000
*   RESPECIFY                                                      1278 03298000
*    (R6,                                                          1278 03299000
*     R7,                                                          1278 03300000
*     R8,                                                          1278 03301000
*     RET) RESTRICTED;                                             1278 03302000
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */ 03303000
         ST    RET,MWASAVEC                                        1279 03304000
*   RET=MWAUCBPT;                   /* GET POINTER TO UCB            */ 03305000
         LR    RET,MWAUCBPT                                        1280 03306000
*   IF UCBDMCT^='00'X THEN          /* IF UCB USE COUNT IS NOT ZERO  */ 03307000
         CLI   UCBDMCT(MWAUCBPT),X'00'                             1281 03308000
         BE    @RF01281                                            1281 03309000
*     DO;                                                          1282 03310000
*       GEN;                                                       1283 03311000
*                                  /*DECREMENT UCB USE COUNT         */ 03312000
THREE    EQU   3                       CONSTANT OF THREE                03313000
LABEL2   L     R6,UCBDMCT-THREE(RET)   LOAD FWORD WITH UCB USE COUNT    03314000
         SR    R7,R7                   ZERO REGISTER 7                  03315000
         IC    R7,UCBDMCT(RET)         GET UCB USE COUNT                03316000
         BCTR  R7,R0                   DECREMENT USE COUNT BY ONE       03317000
         L     R8,UCBDMCT-THREE(RET)   GET FWORD WITH UCB USE COUNT     03318000
         SRL   R8,8                    SHIFT OUT OLD USE COUNT          03319000
         SLL   R8,8                    SHIFT BACK TO ORIGINAL POSITION  03320000
         OR    R7,R8                   LOGICALLY OR IN NEW USE COUNT    03321000
         CS    R6,R7,UCBDMCT-THREE(RET) COMPARE AND SWAP                03322000
         BC    7,LABEL2                IF UNSUCCESSFUL TRY AGAIN        03323000
*     END;                                                         1284 03324000
*   RET=MWASAVEC;                   /* GET RETURN ADDRESS            */ 03325000
@RF01281 L     RET,MWASAVEC                                        1285 03326000
*   RESPECIFY                                                      1286 03327000
*    (R6,                                                          1286 03328000
*     R7,                                                          1286 03329000
*     R8,                                                          1286 03330000
*     RET) UNRESTRICTED;                                           1286 03331000
*   END DCRUCBCT;                                                  1287 03332000
@EL00020 DS    0H                                                  1287 03333000
@EF00020 DS    0H                                                  1287 03334000
@ER00020 BR    @14                                                 1287 03335000
*FREECORE:                                                         1288 03336000
*                                                                  1288 03337000
*/********************************************************************/ 03338000
*/*                                                                  */ 03339000
*/*      01   FUNCTION-FREEMAIN CORE STORAGE                         */ 03340000
*/*      01   INPUTS-  1. SUBPOOL NUMBER IN MWASPOOL                 */ 03341000
*/*                    2. NUMBER OF BYTES TO BE FREED IN MWABYTES    */ 03342000
*/*                    3. ADDRESS OF CORE TO BE FREED IN MWADDRES    */ 03343000
*/*      01   OUTPUTS- 1. CORE STORAGE FREED                         */ 03344000
*/*                                                                  */ 03345000
*/* IN VS2 THIS ROUTINE IS MODIFIED TO HANDLE KEY 0 TO 7     @Z40ESSR*/ 03346000
*/* CORE. THE ROUTINE WILL CHECK MWAFRKEY TO DETERMINE WHETHER OR    */ 03347000
*/* NOT A MODESET IS NEEDED PRIOR TO THE FREEMAIN. THE DEFAULT IS TO */ 03348000
*/* REMAIN IN KEY ZERO WHEN FREEMAINING. THIS ROUTINE WILL SET THE   */ 03349000
*/* FIELD MWAFRKEY TO ZERO AFTER THE SUCCESSFUL FREEMAIN TO INSURE   */ 03350000
*/* THE DEFAULT OF ZERO.                                     @Y30DSMD*/ 03351000
*/*                                                                 .*/ 03352000
*/********************************************************************/ 03353000
*                                                                  1288 03354000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1288 03355000
FREECORE DS    0H                                                  1289 03356000
*   RESPECIFY                                                      1289 03357000
*    (R0,                                                          1289 03358000
*     R1,                                                          1289 03359000
*     RET) RESTRICTED;                                             1289 03360000
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */ 03361000
         ST    RET,MWASAVEC                                        1290 03362000
*   RESPECIFY                                                      1291 03363000
*     RET UNRESTRICTED;                                            1291 03364000
*   IF MWAFRKEY<=K70 THEN           /* IF MODESET IS REQUIRED THEN 1292 03365000
*                                                            @Z40ESSR*/ 03366000
         CLI   MWAFRKEY,X'70'                                      1292 03367000
         BH    @RF01292                                            1292 03368000
*     DO;                           /*                       @Z40ESSR*/ 03369000
*       R1=MWAFRKEY;                /* LOAD KEY INTO REG     @Z40ESSR*/ 03370000
         SLR   R1,R1                                               1294 03371000
         IC    R1,MWAFRKEY                                         1294 03372000
*       SPKA(R1);                   /* MODESET TO SYSTEM KEY @Z40ESSR*/ 03373000
         SPKA  0(R1)                                               1295 03374000
*     END;                          /*                       @Z40ESSR*/ 03375000
*   R1=MWADDRES;                    /* ADDRESS OF CORE TO BE FREED   */ 03376000
@RF01292 L     R1,MWADDRES                                         1297 03377000
*   R0=MWAMNWRD;                    /* SUBPOOL NO. AND BYTES TO BE 1298 03378000
*                                      FREE                          */ 03379000
         L     R0,MWAMNWRD                                         1298 03380000
*   GEN(FREEMAIN R,LV=(R0),A=(R1)); /* FREEMAIN                      */ 03381000
         FREEMAIN R,LV=(R0),A=(R1)                                      03382000
*   GEN( MODESET EXTKEY=ZERO);      /* FORCE KEY ZERO.       @Z30SSMD*/ 03383000
