         TITLE 'IDA0200T - VSAM CLOSE ACB                              *00001000
                        '                                               00002000
IDA0200T CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0200T  78.199'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @12,4095(,@03)                                      0001 00012000
         USING @PSTART,@03                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
JSCBDISP EQU 188                                                        00015000
JSCBAOS1 EQU  72                                                        00016000
JSCBAOS2 EQU 192                                                        00017000
RBPRFXLN EQU 64                                                         00018000
RBPRFLNA EQU 32                                                         00019000
PRBLEN   EQU 136                                                        00020000
SIRBLEN  EQU 200                                                        00021000
TIRBLEN  EQU 136                                                        00022000
IRBLEN   EQU 128                                                        00023000
SVRBLEN  EQU 224                                                        00024000
         LA RACB,0(RACB)                                                00025000
*   RSTATRTN=IDAR200T;              /* NOTE CSECT IN CONTROL @Z30SSRL*/ 00026000
         MVC   RSTATRTN(4,RCORE),@CC04085                          0346 00027000
*   CALL INIT200T;                  /* PERFORM MODULE INITIALIZATION */ 00028000
         BAL   @14,INIT200T                                        0347 00029000
*   GEN(USING @DATD,R13);           /* ESTABLISH ADDRESSABILITY      */ 00030000
         USING @DATD,R13                                                00031000
*   IF CLWDUMMY=OFF THEN            /* IF NOT DUMMY DATA SET         */ 00032000
         TM    CLWDUMMY(RCORE),B'00000100'                         0349 00033000
         BNZ   @RF00349                                            0349 00034000
*     DO;                                                          0350 00035000
*       IF CLWBNOFL=OFF&            /* BUFFER FLUSH NOT SUPPRESSED 0351 00036000
*                                      AND                           */ 00037000
*           AMBLFSTP=OFF THEN       /* NOT FAST PATH PROCESSING (ICI)*/ 00038000
