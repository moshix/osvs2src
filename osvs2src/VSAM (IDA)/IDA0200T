@EL00011 DS    0H                                                  0846 01774000
@EF00011 DS    0H                                                  0846 01775000
@ER00011 BR    @14                                                 0846 01776000
*DECHNDEB:                                                         0847 01777000
*                                                                  0847 01778000
*/********************************************************************/ 01779000
*/*                                                                  */ 01780000
*/*      01   FUNCTION-DECHAIN DEB AND REMOVE DEB ENTRY FROM         */ 01781000
*/*                    THE DEB TABLE                                 */ 01782000
*/*      01   INPUTS-  1. POINTER TO DEB IN MWADEBPT                 */ 01783000
*/*      01   OUTPUTS- 1. DEB DECHAINED AND REMOVED FROM DEB TABLE   */ 01784000
*/*                                                                  */ 01785000
*/********************************************************************/ 01786000
*                                                                  0847 01787000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0847 01788000
DECHNDEB DS    0H                                                  0848 01789000
*   RESPECIFY                                                      0848 01790000
*    (R1,                                                          0848 01791000
*     R6,                                                          0848 01792000
*     R7,                                                          0848 01793000
*     RET) RESTRICTED;                                             0848 01794000
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */ 01795000
         ST    RET,MWASAVEC                                        0849 01796000
*   RESPECIFY                                                      0850 01797000
*     RET UNRESTRICTED;                                            0850 01798000
*   R6=MWADEBPT;                    /* SAVE POINTER TO DEB FOR     0851 01799000
*                                      DECHAIN                       */ 01800000
         LR    R6,MWADEBPT                                         0851 01801000
*   CALL LOCKUP;                    /* SERIALIZE THE PORCESS.      0852 01802000
*                                                            @Y30DSMD*/ 01803000
         BAL   @14,LOCKUP                                          0852 01804000
*   RFY                                                            0853 01805000
*     TCB BASED(DEBTCBAD);          /* DIRECT THE COMPILER.  @Y30DSMD*/ 01806000
*   IF TCBDEB=MWADEBPT THEN         /* IS MY DEB THE FIRST ON THE  0854 01807000
*                                      CHAIN?                        */ 01808000
         L     @11,DEBTCBAD(,MWADEBPT)                             0854 01809000
         C     MWADEBPT,TCBDEB(,@11)                               0854 01810000
         BNE   @RF00854                                            0854 01811000
*     DO;                           /* PROCESS THE FIRST DEB ON THE     01812000
*                                      CHAIN.                @Y30DSMD*/ 01813000
*       MWARDEB=MWADEBPT;           /* SET UP OUR DEB PTR.   @Y30DSMD*/ 01814000
         LR    MWARDEB,MWADEBPT                                    0856 01815000
*       MWANDEB=DEBDEBB;            /* SET UP NEXT DEB PTR.  @Y30DSMD*/ 01816000
         L     MWANDEB,DEBDEBB-1(,MWADEBPT)                        0857 01817000
         LA    MWANDEB,0(,MWANDEB)                                 0857 01818000
*       CS(MWARDEB,MWANDEB,TCBDEB); /* MAKE NEXT DEB THE FIRST.      */ 01819000
         CS    MWARDEB,@09,TCBDEB(@11)                             0858 01820000
*       BC(EIGHT,DECHND1);          /* GOTO DECHND1 IF SUCCESSFUL  0859 01821000
*                                                            @Z30SSRL*/ 01822000
         BC    8,DECHND1                                           0859 01823000
*     END;                          /* END OF FIRST DEB PROCESSING.     01824000
*                                                            @Y30DSMD*/ 01825000
*   MWADEBPT=DEBTCBAD+FWORDLEN;     /* LOAD PTR TO 1ST DEB IN      0861 01826000
*                                      DEB-CHAIN                     */ 01827000
@RF00854 LA    @11,4                                               0861 01828000
         AL    @11,DEBTCBAD(,MWADEBPT)                             0861 01829000
         LR    MWADEBPT,@11                                        0861 01830000
*   DO WHILE(DEBDEBB^=R6&DEBDEBB^=ZERO);/* IF NO MATCH FOUND         */ 01831000
         B     @DE00862                                            0862 01832000
@DL00862 DS    0H                                                  0863 01833000
*     MWADEBPT=DEBDEBB;             /* POINT TO NEXT DEB             */ 01834000
         L     @11,DEBDEBB-1(,MWADEBPT)                            0863 01835000
         LA    @11,0(,@11)                                         0863 01836000
         LR    MWADEBPT,@11                                        0863 01837000
*   END;                                                           0864 01838000
@DE00862 L     @11,DEBDEBB-1(,MWADEBPT)                            0864 01839000
         LA    @11,0(,@11)                                         0864 01840000
         CR    @11,R6                                              0864 01841000
         BE    @DC00862                                            0864 01842000
         LTR   @11,@11                                             0864 01843000
         BNZ   @DL00862                                            0864 01844000
@DC00862 DS    0H                                                  0865 01845000
*   IF DEBDEBB^=ZERO THEN           /* IF DEB MATCH FOUND            */ 01846000
         L     @11,DEBDEBB-1(,MWADEBPT)                            0865 01847000
         LA    @11,0(,@11)                                         0865 01848000
         LTR   @11,@11                                             0865 01849000
         BZ    @RF00865                                            0865 01850000
*     DEBDEBB=DEBDEBB->DEBDEBB;     /* DECHAIN THE DEB               */ 01851000
         L     @11,DEBDEBB-1(,@11)                                 0866 01852000
         LA    @11,0(,@11)                                         0866 01853000
         STCM  @11,7,DEBDEBB(MWADEBPT)                             0866 01854000
*DECHND1:                                                          0867 01855000
*   CALL UNLOCK;                    /* RELEASE THE SERIALIZATION.  0867 01856000
*                                                            @Y30DSMD*/ 01857000
@RF00865 DS    0H                                                  0867 01858000
DECHND1  BAL   @14,UNLOCK                                          0867 01859000
*   MWADEBPT=R6;                    /* COPY DEB POINTER              */ 01860000
         LR    MWADEBPT,R6                                         0868 01861000
*   R1=DEBDCBB;                     /* POINT TO CURRENT DEB          */ 01862000
         L     R1,DEBDCBB-1(,MWADEBPT)                             0869 01863000
         LA    R1,0(,R1)                                           0869 01864000
*   IF R1^=ZERO THEN                /* DOES THEN DUMMY DEB EXIST?  0870 01865000
*                                                            @Y30DSMD*/ 01866000
         LTR   R1,R1                                               0870 01867000
         BZ    @RF00870                                            0870 01868000
*     GEN(DEBCHK (1),TYPE=DELETE,AM=VSAM);/* DELETE DEB FROM DEB   0871 01869000
*                                      TABLE                         */ 01870000
         DEBCHK (1),TYPE=DELETE,AM=VSAM                                 01871000
*   RESPECIFY                                                      0872 01872000
*     RET RESTRICTED;                                              0872 01873000
@RF00870 DS    0H                                                  0873 01874000
*   RET=MWASAVEC;                   /* GET RETURN ADDRESS            */ 01875000
         L     RET,MWASAVEC                                        0873 01876000
*   RESPECIFY                                                      0874 01877000
*    (R1,                                                          0874 01878000
*     R6,                                                          0874 01879000
*     R7,                                                          0874 01880000
*     RET) UNRESTRICTED;                                           0874 01881000
*   RFY                                                            0875 01882000
*     TCB BASED(TCBPTR);            /* DIRECT THE COMPILER.  @Z30SSMD*/ 01883000
*   END DECHNDEB;                                                  0876 01884000
@EL00012 DS    0H                                                  0876 01885000
