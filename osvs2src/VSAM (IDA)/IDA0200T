*   MWABYTES=VGTTSIZE;              /* GET THE SIZE OF THE VGTT.   1394 03732000
*                                                            @Y30DSMD*/ 03733000
         MVC   MWABYTES(3),VGTTSIZE+1(@11)                         1394 03734000
*   MWASPOOL=VGTTSP;                /* GET THE SUBPOOL.      @Y30DSMD*/ 03735000
