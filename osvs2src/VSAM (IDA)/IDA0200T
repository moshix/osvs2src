         LTR   @05,@05                                             0462 00433000
         BNZ   @RF00462                                            0462 00434000
*         CLWSPHCL=ON;              /* INDICATE SPHERE CLOSE         */ 00435000
         OI    CLWSPHCL(RCORE),B'00001000'                         0463 00436000
*     END;                                                         0464 00437000
*   ELSE                            /* IF A DUMMY DATA SET           */ 00438000
*     DO;                           /* DO DUMMY INITIALIZATION       */ 00439000
         B     @RC00448                                            0465 00440000
@RF00448 DS    0H                                                  0466 00441000
*       CLWDUMMY=ON;                /* INDICATE DUMMY DATA SET IN CLW*/ 00442000
*       CLWSPHCL=ON;                /* INDICATE SPHERE CLOSE @ZA26774*/ 00443000
         OI    CLWDUMMY(RCORE),B'00001100'                         0467 00444000
*       MWABAMBL=MWAMBLPT;          /* INIT BASE AMBL PTR            */ 00445000
         L     @05,MWAMBLPT                                        0468 00446000
         ST    @05,MWABAMBL                                        0468 00447000
*     END;                                                         0469 00448000
*   MWAMBLPT=ACBAMBL;               /* POINT TO USER ACB AMBL        */ 00449000
@RC00448 L     @05,ACBAMBL(,RACB)                                  0470 00450000
         ST    @05,MWAMBLPT                                        0470 00451000
*   RESPECIFY                                                      0471 00452000
*     RET RESTRICTED;                                              0471 00453000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 00454000
         L     RET,MWASAVEA                                        0472 00455000
*   RESPECIFY                                                      0473 00456000
*     RET UNRESTRICTED;                                            0473 00457000
*   END INIT200T;                                                  0474 00458000
         B     @EL00002                                            0474 00459000
*GETCORE:                                                          0475 00460000
*                                                                  0475 00461000
*/********************************************************************/ 00462000
*/*                                                                  */ 00463000
*/*      01   FUNCTION-ISSUE GETMAIN TO OBTAIN CORE STORAGE          */ 00464000
*/*      01   INPUTS-  1. SUBPOOL NUMBER IN MWASPOOL                 */ 00465000
*/*                    2. NUMBER OF BYTES REQUIRED IN MWABYTES       */ 00466000
*/*      01   OUTPUTS- 1. ADDRESS OF GOTTEN CORE IN MWADDRES         */ 00467000
*/*                    2. IF GETMAIN FAILED, PROBLEM DETERM IS CALLED*/ 00468000
*/*      THIS ROUTINE HAS BEEN ALTERED FOR VS2. IF MWAFRKEY IS SET   */ 00469000
*/*      TO SYS KEY THIS RTN WILL MODESET TO SYS KEY BEFORE  @Z40ESSR*/ 00470000
*/*      DOING THE GETMAIN. THE DEFAULT IS TO GETMAIN IN KEY ZERO.   */ 00471000
*/*      MWAFRKEY IS RESET TO K00 FOR EACH CALL TO ENFORCE KEY ZERO. */ 00472000
*/*                                                                  */ 00473000
*/********************************************************************/ 00474000
*                                                                  0475 00475000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0475 00476000
GETCORE  DS    0H                                                  0476 00477000
*   RESPECIFY                                                      0476 00478000
*    (R1,                                                          0476 00479000
*     R6,                                                          0476 00480000
*     R7,                                                          0476 00481000
*     R8,                                                          0476 00482000
*     R9,                                                          0476 00483000
*     RET) RESTRICTED;              /*                       @Z30SSMD*/ 00484000
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */ 00485000
         ST    RET,MWASAVEC                                        0477 00486000
*   RESPECIFY                                                      0478 00487000
*     RET UNRESTRICTED;                                            0478 00488000
*   IF MWAFRKEY<=K70 THEN           /* IF IN SYSTEM KEY THEN @Z40ESSR*/ 00489000
         CLI   MWAFRKEY,X'70'                                      0479 00490000
         BH    @RF00479                                            0479 00491000
*     DO;                           /* MODESET TO SYSTEM KEY @Z40ESSR*/ 00492000
*       R6=MWAFRKEY;                /* LOAD REG 6 WITH SYS KEY     0481 00493000
*                                                            @Z40ESSR*/ 00494000
         SLR   R6,R6                                               0481 00495000
         IC    R6,MWAFRKEY                                         0481 00496000
*       SPKA(R6);                   /* MODESET TO SYSTEM KEY @Z40ESSR*/ 00497000
         SPKA  0(R6)                                               0482 00498000
*     END;                          /*                       @Z40ESSR*/ 00499000
*   R6=MWASPOOL;                    /* SUBPOOL NUMBER                */ 00500000
@RF00479 SLR   R6,R6                                               0484 00501000
         IC    R6,MWASPOOL                                         0484 00502000
*   R7=MWABYTES;                    /* NUMBER OF BYTES REQUIRED      */ 00503000
         L     R7,MWABYTES-1                                       0485 00504000
         LA    R7,0(,R7)                                           0485 00505000
*   GEN(GETMAIN RC,LV=(R7),SP=(R6))                                0486 00506000
*   SETS(RETCODE,R1);               /* ISSUE GETMAIN         @Z30SSMD*/ 00507000
         GETMAIN RC,LV=(R7),SP=(R6)                                     00508000
*   GEN( MODESET EXTKEY=ZERO  FORCE KEY 0);/* RETURN TO KEY 0      0487 00509000
*                                                            @Z30SSMD*/ 00510000
          MODESET EXTKEY=ZERO  FORCE KEY 0                              00511000
*   RESPECIFY                                                      0488 00512000
*     RETCODE RESTRICTED;                                          0488 00513000
*   MWAFRKEY=K00;                   /* RESET TO FORCE KEY ZERO     0489 00514000
*                                      DEFAULT.              @Z30SSMD*/ 00515000
         MVI   MWAFRKEY,X'00'                                      0489 00516000
*   IF RETCODE=ZERO THEN            /* GETMAIN SUCCESSFUL            */ 00517000
         LTR   RETCODE,RETCODE                                     0490 00518000
         BNZ   @RF00490                                            0490 00519000
*     DO;                           /* PROCESS THE STORAGE.  @Z30SSMD*/ 00520000
*       MWADDRES=R1;                /* SAVE THE ADDRESS OF THE     0492 00521000
*                                      STORAGE.              @Z30SSMD*/ 00522000
         ST    R1,MWADDRES                                         0492 00523000
*       R6=MWADDRES;                /* ADDRESS OF GOTTEN CORE        */ 00524000
         L     @14,MWADDRES                                        0493 00525000
         LR    R6,@14                                              0493 00526000
*       R9=ZERO;                    /* ZERO PAD BYTE                 */ 00527000
         SLR   R9,R9                                               0494 00528000
*       GEN(MVCL R6,R8)                                            0495 00529000
*       NOSETS;                     /* ZERO GOTTEN CORE              */ 00530000
         MVCL R6,R8                                                     00531000
*       RESPECIFY                                                  0496 00532000
*        (R6,                                                      0496 00533000
*         R7,                                                      0496 00534000
*         R8,                                                      0496 00535000
*         R9) UNRESTRICTED;                                        0496 00536000
*     END;                          /* END OF LOGIC FOR GETMAIN    0497 00537000
*                                      RETCODE=ZERO.         @Z30SSMD*/ 00538000
*   ELSE                            /* IF GETMAIN FAILED             */ 00539000
*     DO;                           /* IF GETMAIN FAILED, CALL     0498 00540000
*                                      IDA0192P.             @Z30SSMD*/ 00541000
         B     @RC00490                                            0498 00542000
@RF00490 DS    0H                                                  0499 00543000
*       ACBERFLG=OPERR136;          /* SET USER ACB ERROR FLAG       */ 00544000
         MVI   ACBERFLG(RACB),X'88'                                0499 00545000
*       R1=MWACORAD;                /* POINT TO COMMON WORK AREA     */ 00546000
         L     @14,MWACORAD                                        0500 00547000
         LR    R1,@14                                              0500 00548000
*       PDPCODE=PP20;               /* SET PROBLEM CODE              */ 00549000
         MVI   PDPCODE+280(@14),X'14'                              0501 00550000
*       RSTATRTN=IDAR192P;          /* NOTE WHICH CSECT IN CONTROL.     00551000
*                                                            @Y30DSMD*/ 00552000
         MVC   RSTATRTN(4,@14),@CC04071                            0502 00553000
*       CALL IDA0192P;              /* CALL PROBLEM DETERMINATION    */ 00554000
         L     @15,@CV05327                                        0503 00555000
         BALR  @14,@15                                             0503 00556000
*       RSTATRTN=IDAR200T;          /* NOTE WHICH CSECT IS IN      0504 00557000
*                                      CONTROL.              @Y30DSMD*/ 00558000
         L     @14,MWACORAD                                        0504 00559000
         MVC   RSTATRTN(4,@14),@CC04085                            0504 00560000
*       RETURN TO LTRM200T;         /* TERMINATION PROCESSING        */ 00561000
         LA    @14,LTRM200T                                        0505 00562000
@EL00003 DS    0H                                                  0505 00563000
@EF00003 DS    0H                                                  0505 00564000
@ER00003 BR    @14                                                 0505 00565000
*     END;                          /* END OF LOGIC IF GETMAIN     0506 00566000
*                                      FAILED.               @Z30SSMD*/ 00567000
*   RESPECIFY                                                      0507 00568000
*     RET RESTRICTED;                                              0507 00569000
@RC00490 DS    0H                                                  0508 00570000
*   RET=MWASAVEC;                   /* GET RETURN ADDRESS            */ 00571000
         L     RET,MWASAVEC                                        0508 00572000
*   RESPECIFY                                                      0509 00573000
*    (R1,                                                          0509 00574000
*     RET,                                                         0509 00575000
*     RETCODE) UNRESTRICTED;        /*                       @Z30SSMD*/ 00576000
*   END GETCORE;                                                   0510 00577000
         B     @EL00003                                            0510 00578000
*ENQFUNC:                                                          0511 00579000
*                                                                  0511 00580000
*/********************************************************************/ 00581000
*/*                                                                  */ 00582000
*/*      01   FUNCTION-BUILD ENQ PARAMETER LIST AND ISSUE ENQ'S      */ 00583000
*/*      01   INPUTS-  1. POINTER TO AMBL IN ACBAMBL                 */ 00584000
*/*      01   OUTPUTS- 1. ENQ ALL DATA SETS CONNECTED TO USER ACB    */ 00585000
*/*                                                                  */ 00586000
