@DL01075 DS    0H                                                  1076 02542000
*     CLWPRMCL=ON;                  /* INDICATE PRIMARY CLOSE        */ 02543000
         OI    CLWPRMCL(RCORE),B'10000000'                         1076 02544000
*     RACB=UPTRPL(MWAINDEX)->RPLDACB;/* POINT TO UPGRADE ACB         */ 02545000
         MH    @14,@CH00033                                        1077 02546000
         L     @11,MWAMBLPT                                        1077 02547000
         L     @11,AMBLBIB(,@11)                                   1077 02548000
         L     @11,BIBUPT(,@11)                                    1077 02549000
         L     @05,UPTRPL-12(@14,@11)                              1077 02550000
         L     RACB,RPLDACB(,@05)                                  1077 02551000
*     PDPDSTYP=PDPUPGR;             /* INDICATE UPGRADE DATA SET     */ 02552000
         L     @05,MWACORAD                                        1078 02553000
         MVI   PDPDSTYP+280(@05),X'04'                             1078 02554000
*     IF UPTF1NOP(MWAINDEX)=OFF THEN/* AIX IS OPENNED                */ 02555000
         ALR   @11,@14                                             1079 02556000
         TM    UPTF1NOP-12(@11),B'00010000'                        1079 02557000
         BNZ   @RF01079                                            1079 02558000
*       DO;                                                        1080 02559000
*         RSTATRTN=IDAR200B;        /* NOTE THE TRANSFER OF CSECTS.     02560000
*                                                            @Y30DSMD*/ 02561000
         MVC   RSTATRTN(4,@05),@CC04083                            1081 02562000
*         ACBBUSY=ON;               /* INDICATE ACB BUSY     @XM02092*/ 02563000
         OI    ACBBUSY(RACB),B'00000001'                           1082 02564000
*         CALL IDA0200B;            /* CLOSE UPGRADE DATA SET        */ 02565000
         L     @15,@CV05403                                        1083 02566000
         BALR  @14,@15                                             1083 02567000
*         ACBBUSY=OFF;              /* RESET BUSY BIT        @XM02092*/ 02568000
         NI    ACBBUSY(RACB),B'11111110'                           1084 02569000
*         RSTATRTN=IDAR200T;        /* NOTE THE TRANSFER OF CSECTS.     02570000
