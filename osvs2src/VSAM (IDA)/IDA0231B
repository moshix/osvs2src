*/*                                                                  */ 00998000
*/*      01 FUNCTION - PROBLEM DETERMINATION AND WRITE ERROR MESSAGES*/ 00999000
*/*      01 INPUTS -   PROBLEM DETERMINATION PARAMETER LIST          */ 01000000
*/*      01 OUTPUTS -  PROBLEM MESSAGES WRITTEN BY IDA0192P          */ 01001000
*/*                                                                  */ 01002000
*/********************************************************************/ 01003000
*                                                                  0566 01004000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0566 01005000
PROBDT   DS    0H                                                  0567 01006000
*   RESPECIFY                                                      0567 01007000
*    (R6,                                                          0567 01008000
*     RET) RESTRICTED;                                             0567 01009000
*   R6=RET;                         /* SAVE RETURN ADDRESS           */ 01010000
         LR    R6,RET                                              0568 01011000
*   RESPECIFY                                                      0569 01012000
*     RET UNRESTRICTED;                                            0569 01013000
*   IF CLWGMAIN=ON THEN             /* MODULE WORK AREA EXIST        */ 01014000
         TM    CLWGMAIN(RCORE),B'00100000'                         0570 01015000
         BNO   @RF00570                                            0570 01016000
*     DO;                                                          0571 01017000
*       MWASAVED=R6;                /* SAVE RETURN CODE              */ 01018000
         LR    MWASAVED,R6                                         0572 01019000
*       MWACATID=CLC;               /* SET-UP CALLER ID              */ 01020000
         MVI   MWACATID,X'30'                                      0573 01021000
*       MWACI=AMBLDTA->AMBDDSN;     /* CI NUMBER OF DATA AMB         */ 01022000
         L     @11,ACBAMBL(,RACB)                                  0574 01023000
         L     @11,AMBLDTA(,@11)                                   0574 01024000
         MVC   MWACI(3),AMBDDSN(@11)                               0574 01025000
*       MWACACB=AMBLDTA->AMBCACB;   /* CATALOG ACB ADDRESS           */ 01026000
         L     @11,AMBCACB(,@11)                                   0575 01027000
         ST    @11,MWACACB                                         0575 01028000
*       RESPECIFY                                                  0576 01029000
*        (R1,                                                      0576 01030000
*         RETCODE) RESTRICTED;                                     0576 01031000
*       R1=ADDR(MWACATPM);          /* R1 PT TO PARM LIST FOR      0577 01032000
*                                      IDA0192C                      */ 01033000
         LA    R1,MWACATPM                                         0577 01034000
*       RSTATRTN=IDAR192C;          /* NOTE CALL TO 192C             */ 01035000
         L     @14,CLWCOMWK(,RCORE)                                0578 01036000
         MVC   RSTATRTN(4,@14),@CC04105                            0578 01037000
*       CALL IDA0192C;              /* OBTAIN CLUSTER NAME FOR CI IN    01038000
*                                      DATA AMB                      */ 01039000
         L     @15,@CV04580                                        0579 01040000
         BALR  @14,@15                                             0579 01041000
*       RSTATRTN=IDAR231B;          /* NOTE 231B IN CONTROL          */ 01042000
         L     @14,CLWCOMWK(,RCORE)                                0580 01043000
         MVC   RSTATRTN(4,@14),@CC04131                            0580 01044000
*       IF RETCODE=ZERO THEN        /* NO ERROR                      */ 01045000
         LTR   RETCODE,RETCODE                                     0581 01046000
         BNZ   @RF00581                                            0581 01047000
*         PDPCLSNM=ADDR(MWACLSNM);  /* GET ADDRESS OF CLUSTER NAME   */ 01048000
         LA    @11,MWACLSNM                                        0582 01049000
         ST    @11,PDPCLSNM+280(,@14)                              0582 01050000
*       ELSE                        /* IF ERROR ENCOUNTERED          */ 01051000
*         PDPDSTYP=PDPDDNM;         /* INDICATE DDNAME USED INSTEAD  */ 01052000
         B     @RC00581                                            0583 01053000
@RF00581 L     @14,CLWCOMWK(,RCORE)                                0583 01054000
         MVI   PDPDSTYP+280(@14),X'01'                             0583 01055000
*       IF CLWAMBPT^=ZERO THEN      /* IF ERROR RELATED TO A DATA SET*/ 01056000
@RC00581 L     @14,CLWAMBPT(,RCORE)                                0584 01057000
         LTR   @14,@14                                             0584 01058000
         BZ    @RF00584                                            0584 01059000
*         DO;                                                      0585 01060000
*           MWACATID=CLD;           /* SET CALLER ID                 */ 01061000
         MVI   MWACATID,X'28'                                      0586 01062000
*           MWACI=CLWAMBPT->AMBDDSN;/* GET DATA SET CI NUMBER        */ 01063000
         MVC   MWACI(3),AMBDDSN(@14)                               0587 01064000
*           R1=ADDR(MWACATPM);      /* R1 PT TO PARM LIST FOR      0588 01065000
*                                      IDA0192C                      */ 01066000
         LA    R1,MWACATPM                                         0588 01067000
*           RSTATRTN=IDAR192C;      /* NOTE CALL TO 192C             */ 01068000
         L     @14,CLWCOMWK(,RCORE)                                0589 01069000
         MVC   RSTATRTN(4,@14),@CC04105                            0589 01070000
*           CALL IDA0192C;          /* GET DATA SET NAME             */ 01071000
         L     @15,@CV04580                                        0590 01072000
         BALR  @14,@15                                             0590 01073000
*           RSTATRTN=IDAR231B;      /* NOTE 231B IN CONTROL          */ 01074000
         L     @14,CLWCOMWK(,RCORE)                                0591 01075000
         MVC   RSTATRTN(4,@14),@CC04131                            0591 01076000
*           IF RETCODE=ZERO THEN    /* DATA SET OBTAINED             */ 01077000
         LTR   RETCODE,RETCODE                                     0592 01078000
         BNZ   @RF00592                                            0592 01079000
*             DO;                                                  0593 01080000
*               MWADSN=MWACLSNM;    /* MOVE DATA SET NAME TO AMB WORK   01081000
*                                      AREA                          */ 01082000
         L     @11,CLWAMBPT(,RCORE)                                0594 01083000
         L     @11,AMBWKA(,@11)                                    0594 01084000
         MVC   MWADSN(44,@11),MWACLSNM                             0594 01085000
*               PDPDSN=ADDR(MWADSN);/* INITIALIZE DATA SET NAME    0595 01086000
*                                      ADDRESS IN PROBLEM          0595 01087000
*                                      DETERMINATION PARM LIST       */ 01088000
         ST    @11,PDPDSN+280(,@14)                                0595 01089000
*             END;                                                 0596 01090000
*         END;                                                     0597 01091000
@RF00592 DS    0H                                                  0598 01092000
*       CALL ERRORFLG;              /* DETERMINE CORRECT ACBERFLG    */ 01093000
@RF00584 BAL   @14,ERRORFLG                                        0598 01094000
*       R6=MWASAVED;                /* GET RETURN ADDRESS            */ 01095000
         LR    R6,MWASAVED                                         0599 01096000
*     END;                                                         0600 01097000
*   R1=CLWCOMWK;                    /* POINT TO COMMON WORK AREA     */ 01098000
@RF00570 L     @14,CLWCOMWK(,RCORE)                                0601 01099000
         LR    R1,@14                                              0601 01100000
*   RSTATRTN=IDAR192P;              /* NOTE CALL TO 192P             */ 01101000
         MVC   RSTATRTN(4,@14),@CC04115                            0602 01102000
*   CALL IDA0192P;                  /* CALL PROBLEM DETERMINATION    */ 01103000
         L     @15,@CV04591                                        0603 01104000
         BALR  @14,@15                                             0603 01105000
*   RSTATRTN=IDAR231B;              /* NOTE 231B IN CONTROL          */ 01106000
         L     @14,CLWCOMWK(,RCORE)                                0604 01107000
         MVC   RSTATRTN(4,@14),@CC04131                            0604 01108000
*   PDPSFI=ZERO;                    /* ZERO POINTER TO CATALOG SFI   */ 01109000
         SLR   @11,@11                                             0605 01110000
         LA    @09,280                                             0605 01111000
         ALR   @09,@14                                             0605 01112000
         ST    @11,PDPSFI(,@09)                                    0605 01113000
*   PDPCLSNM=ADDR(JFCBDSNM);        /* RESET CLUSTER NAME TO DDNAME  */ 01114000
         AL    @14,@CF04149                                        0606 01115000
         ST    @14,PDPCLSNM(,@09)                                  0606 01116000
*   PDPDSN=ZERO;                    /* RESET DATA SET NAME TO ZERO   */ 01117000
         ST    @11,PDPDSN(,@09)                                    0607 01118000
*   RESPECIFY                                                      0608 01119000
*     RET RESTRICTED;                                              0608 01120000
*   RET=R6;                         /* GET RETURN ADDRESS            */ 01121000
         LR    RET,R6                                              0609 01122000
*   RESPECIFY                                                      0610 01123000
*    (R1,                                                          0610 01124000
*     R6,                                                          0610 01125000
*     RET,                                                         0610 01126000
*     RETCODE) UNRESTRICTED;                                       0610 01127000
*   END PROBDT;                                                    0611 01128000
@EL00009 DS    0H                                                  0611 01129000
@EF00009 DS    0H                                                  0611 01130000
@ER00009 BR    @14                                                 0611 01131000
*ERRORFLG:                                                         0612 01132000
*                                                                  0612 01133000
*/********************************************************************/ 01134000
*/*                                                                  */ 01135000
*/*    01 FUNCTION - THIS ROUTINE SELECT THE CORRECT ERROR FLAG FOR  */ 01136000
*/*                  THE ACBERFLG                                    */ 01137000
*/*    01 INPUT -    1. PROBLEM DETERMINATION PROBLEM CODE (MWAPCODE)*/ 01138000
*/*                  2. PROBLEM DETERMINATION FUNCTION CODE(MWAPFUNC)*/ 01139000
*/*                  3. PROBLEM DET SUB FUNCTION CODE (MWAPSFC)      */ 01140000
*/*    01 OUTPUT -   1. CURRENT ERROR IS SET                         */ 01141000
*/*                  2. ACBERFLG IS SET IF CURRENT ERROR IS GREATER  */ 01142000
*/*                                                                  */ 01143000
*/********************************************************************/ 01144000
*                                                                  0612 01145000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0612 01146000
ERRORFLG DS    0H                                                  0613 01147000
*   RESPECIFY                                                      0613 01148000
*     RET RESTRICTED;                                              0613 01149000
*   MWASAVEE=RET;                   /* SAVE RETURN ADDRESS           */ 01150000
         LR    MWASAVEE,RET                                        0614 01151000
*   RESPECIFY                                                      0615 01152000
*     RET UNRESTRICTED;                                            0615 01153000
*   PDPFUNC=MWAPFUNC;               /* GET FUNCTION CODE             */ 01154000
         LA    @11,280                                             0616 01155000
         AL    @11,CLWCOMWK(,RCORE)                                0616 01156000
         IC    @06,MWAPFUNC                                        0616 01157000
         STC   @06,PDPFUNC(,@11)                                   0616 01158000
*   PDPCODE=MWAPCODE;               /* GET PROBLEM CODE              */ 01159000
         SLR   @06,@06                                             0617 01160000
         IC    @06,MWAPCODE                                        0617 01161000
         STC   @06,PDPCODE(,@11)                                   0617 01162000
*   PDPSFC=MWAPSFC;                 /* GET SUB FUNCTION CODE         */ 01163000
         IC    @15,MWAPSFC                                         0618 01164000
         STC   @15,PDPSFC(,@11)                                    0618 01165000
*   IF ACBERFLG<IDAERMAP(PDPCODE) THEN/* IF CURRENT ERROR IS GREATER */ 01166000
         SLR   @11,@11                                             0619 01167000
         IC    @11,IDAERMAP-1(@06)                                 0619 01168000
         SLR   @06,@06                                             0619 01169000
         IC    @06,ACBERFLG(,RACB)                                 0619 01170000
         CR    @11,@06                                             0619 01171000
         BNH   @RF00619                                            0619 01172000
*     ACBERFLG=IDAERMAP(PDPCODE);   /* SET HIGHEST ERROR CODE        */ 01173000
         STC   @11,ACBERFLG(,RACB)                                 0620 01174000
*   RESPECIFY                                                      0621 01175000
*     RET RESTRICTED;                                              0621 01176000
@RF00619 DS    0H                                                  0622 01177000
*   RET=MWASAVEE;                   /* RESTORE RETURN ADDRESS        */ 01178000
         LR    RET,MWASAVEE                                        0622 01179000
*   RESPECIFY                                                      0623 01180000
*     RET UNRESTRICTED;                                            0623 01181000
*   END ERRORFLG;                                                  0624 01182000
@EL00010 DS    0H                                                  0624 01183000
@EF00010 DS    0H                                                  0624 01184000
@ER00010 BR    @14                                                 0624 01185000
*GETCORE:                                                          0625 01186000
*                                                                  0625 01187000
*/********************************************************************/ 01188000
*/*                                                                  */ 01189000
*/*      01   FUNCTION-ISSUE GETMAIN'S TO OBTAIN CORE STORAGE        */ 01190000
*/*      01   INPUTS-  SUBPOOL NUMBER IN MWASPOOL                    */ 01191000
*/*                    NUMBER OF BYTES REQUIRED IN MWABYTES          */ 01192000
*/*      01   OUTPUTS- ADDRESS OF GOTTEN CORE IN MWADDRES            */ 01193000
*/*                    IF GETMAIN FAILED, PROBLEM DETERM IS CALLED   */ 01194000
*/*                                                                  */ 01195000
*/********************************************************************/ 01196000
*                                                                  0625 01197000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0625 01198000
GETCORE  DS    0H                                                  0626 01199000
*   RESPECIFY                                                      0626 01200000
*    (R0,                                                          0626 01201000
*     R1,                                                          0626 01202000
*     R6,                                                          0626 01203000
*     R7,                                                          0626 01204000
*     RET,                                                         0626 01205000
*     RETCODE) RESTRICTED;                                         0626 01206000
*   MWASAVEC=RET;                   /* SAVE RETURN ADDRESS           */ 01207000
         LR    MWASAVEC,RET                                        0627 01208000
*   R6=MWASPOOL;                    /* SUBPOOL NUMBER                */ 01209000
         SLR   R6,R6                                               0628 01210000
         IC    R6,MWASPOOL                                         0628 01211000
*   R7=MWABYTES;                    /* NUMBER OF BYTES REQUIRED      */ 01212000
         L     R7,MWABYTES-1                                       0629 01213000
         LA    R7,0(,R7)                                           0629 01214000
*   GEN(GETMAIN RC,LV=(R7),SP=(R6)) /* REGISTER CONDITIONAL GETMAIN  */ 01215000
*   SETS(R1,RETCODE);                                              0630 01216000
         GETMAIN RC,LV=(R7),SP=(R6)                                     01217000
*   IF RETCODE=ZERO THEN            /* GETMAIN SUCCESSFUL            */ 01218000
         LTR   RETCODE,RETCODE                                     0631 01219000
         BNZ   @RF00631                                            0631 01220000
*     DO;                                                          0632 01221000
*       MWADDRES=R1;                /* ADDRESS OF GOTTEN CORE        */ 01222000
         ST    R1,MWADDRES                                         0633 01223000
*       R6=R1;                      /* ADDRESS OF GOTTEN CORE FOR  0634 01224000
*                                      MVCL                          */ 01225000
         LR    R6,R1                                               0634 01226000
*       R1=ZERO;                    /* ZERO PAD BYTE                 */ 01227000
         SLR   R1,R1                                               0635 01228000
*       GEN(MVCL R6,R0)                                            0636 01229000
*       NOSETS;                     /* ZERO GOTTEN CORE              */ 01230000
         MVCL R6,R0                                                     01231000
*     END;                                                         0637 01232000
*   ELSE                            /* IF GETMAIN FAILED             */ 01233000
*     DO;                                                          0638 01234000
         B     @RC00631                                            0638 01235000
@RF00631 DS    0H                                                  0639 01236000
*       MWAPCODE=PP20;              /* SET PROBLEM CODE              */ 01237000
         MVI   MWAPCODE,X'14'                                      0639 01238000
*       CALL PROBDT;                /* CALL PROBLEM DETERMINATION    */ 01239000
         BAL   @14,PROBDT                                          0640 01240000
*     END;                                                         0641 01241000
*   RET=MWASAVEC;                   /* GET RETURN ADDRESS            */ 01242000
@RC00631 LR    RET,MWASAVEC                                        0642 01243000
*   RESPECIFY                                                      0643 01244000
*    (R0,                                                          0643 01245000
*     R1,                                                          0643 01246000
*     R6,                                                          0643 01247000
*     R7,                                                          0643 01248000
*     RET,                                                         0643 01249000
*     RETCODE) UNRESTRICTED;                                       0643 01250000
*   END GETCORE;                                                   0644 01251000
@EL00011 DS    0H                                                  0644 01252000
@EF00011 DS    0H                                                  0644 01253000
@ER00011 BR    @14                                                 0644 01254000
*FREECORE:                                                         0645 01255000
*                                                                  0645 01256000
*/********************************************************************/ 01257000
*/*                                                                  */ 01258000
*/*      01 FUNCTION - FREE MODULE WORK AREA                         */ 01259000
*/*      01 INPUTS -   ADDRESS OF MWA IN MWADDRES                    */ 01260000
*/*                    NUMBER OF BYTES TO BE FREED IN MWABYTES       */ 01261000
*/*                    SUBPOOL NUMBER IN MWASPOOL                    */ 01262000
*/*      01 OUTPUTS -  CORE STORAGE FREED                            */ 01263000
*/*                                                                  */ 01264000
*/********************************************************************/ 01265000
*                                                                  0645 01266000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0645 01267000
FREECORE DS    0H                                                  0646 01268000
*   RESPECIFY                                                      0646 01269000
*    (R0,                                                          0646 01270000
*     R1,                                                          0646 01271000
*     RET) RESTRICTED;                                             0646 01272000
*   MWASAVEB=RET;                   /* SAVE RETURN ADDRESS           */ 01273000
         ST    RET,MWASAVEB                                        0647 01274000
*   LM(R0,R1,MWAMNPRM);             /* SUBPOOL NO., LENGTH, AND    0648 01275000
*                                      ADDRESS OF CORE TO BE FREED   */ 01276000
         LM    R0,R1,MWAMNPRM                                      0648 01277000
*   GEN(FREEMAIN R,LV=(0),A=(1));   /* FREEMAIN                      */ 01278000
         FREEMAIN R,LV=(0),A=(1)                                        01279000
*   RET=MWASAVEB;                   /* GET RETURN ADDRESS            */ 01280000
         L     RET,MWASAVEB                                        0650 01281000
*   RESPECIFY                                                      0651 01282000
*    (R0,                                                          0651 01283000
*     R1,                                                          0651 01284000
*     RET) UNRESTRICTED;                                           0651 01285000
*   END FREECORE;                                                  0652 01286000
@EL00012 DS    0H                                                  0652 01287000
@EF00012 DS    0H                                                  0652 01288000
@ER00012 BR    @14                                                 0652 01289000
@PB00012 DS    0H                                                  0652 01290000
