*                                                                  0523 00883000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0523 00884000
UPSMF    DS    0H                                                  0524 00885000
*   RESPECIFY                                                      0524 00886000
*    (R1,                                                          0524 00887000
*     R6,                                                          0524 00888000
*     RET,                                                         0524 00889000
*     RETCODE) RESTRICTED;                                         0524 00890000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 00891000
         ST    RET,MWASAVEA                                        0525 00892000
*   RESPECIFY                                                      0526 00893000
*     RET UNRESTRICTED;                                            0526 00894000
*   RESPECIFY                                                      0527 00895000
*     IGGCAXWA BASED(R6);           /* ADDRESSABILITY FOR CATALOG  0527 00896000
*                                      WORK AREA                     */ 00897000
*   MWARCNBR=MWACLOSE;              /* SET RECORD NUMBER             */ 00898000
         MVI   MWARCNBR,X'40'                                      0528 00899000
*   MWARCLOS=ON;                    /* INDICATE CALL FROM CLOSE      */ 00900000
