*   MWACATID=CLM;                   /* INDICATE CLOSE                */ 00779000
@RF00478 MVI   MWACATID,X'08'                                      0484 00780000
*   MWASFIPT=ADDR(CLWSFI);          /* POINT TO SFI AREA IN CLW      */ 00781000
         LA    @14,CLWSFI(,RCORE)                                  0485 00782000
