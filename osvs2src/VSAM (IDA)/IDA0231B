         L     @15,@CV04585                                        0538 00926000
         BALR  @14,@15                                             0538 00927000
*   RSTATRTN=IDAR231B;              /* NOTE 231B IN CONTROL          */ 00928000
         L     @14,CLWCOMWK(,RCORE)                                0539 00929000
         MVC   RSTATRTN(4,@14),@CC04131                            0539 00930000
*   IF RETCODE^=ZERO THEN           /* IF ERROR ENCOUNTERED          */ 00931000
