*       MWAOUTBT=ON;                /* INDICATE OUTPUT TO CATALOG    */ 00773000
         L     @14,AMBWKA(,@14)                                    0481 00774000
         OI    MWAOUTBT(@14),B'10000000'                           0481 00775000
*       MWAOPEN='80'X;              /* SET OPEN INDICATOR            */ 00776000
