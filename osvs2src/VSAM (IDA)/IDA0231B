         ST    @14,CLWAMBPT(,RCORE)                                0549 00953000
*           MWARCDSN=ADDR(MWADSN);  /* POINT TO DATA SET NAME        */ 00954000
         L     @14,AMBWKA(,@14)                                    0550 00955000
         ST    @14,MWARCDSN                                        0550 00956000
*           RSTATRTN=IDAR192S;      /* NOTE CALL TO 192S             */ 00957000
         L     @14,CLWCOMWK(,RCORE)                                0551 00958000
         MVC   RSTATRTN(4,@14),@CC04117                            0551 00959000
*           CALL IDA0192S;          /* WRITE SMF RECORDS             */ 00960000
         L     @15,@CV04585                                        0552 00961000
         BALR  @14,@15                                             0552 00962000
*           RSTATRTN=IDAR231B;      /* NOTE 231B IN CONTROL          */ 00963000
         L     @14,CLWCOMWK(,RCORE)                                0553 00964000
         MVC   RSTATRTN(4,@14),@CC04131                            0553 00965000
*           IF RETCODE^=ZERO THEN   /* IF ERROR ENCOUNTED            */ 00966000
         LTR   RETCODE,RETCODE                                     0554 00967000
         BZ    @RF00554                                            0554 00968000
*             DO;                                                  0555 00969000
*               MWAPDPRM=RETCODE;   /* SAVE RETURN CODE              */ 00970000
         ST    RETCODE,MWAPDPRM                                    0556 00971000
*               CALL PROBDT;        /* CALL PROBLEM DETERMINATION    */ 00972000
         BAL   @14,PROBDT                                          0557 00973000
*             END;                                                 0558 00974000
*         END;                                                     0559 00975000
@RF00554 DS    0H                                                  0560 00976000
*     END;                                                         0560 00977000
@RF00546 DS    0H                                                  0561 00978000
*   RESPECIFY                                                      0561 00979000
*     IGGCAXWA BASED(ACBUAPTR);                                    0561 00980000
