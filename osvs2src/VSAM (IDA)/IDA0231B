*   R1=ADDR(MWACATPM);              /* POINT TO IDA0192C PARAMETER 0502 00822000
*                                      LIST                          */ 00823000
         LA    R1,MWACATPM                                         0502 00824000
*   R5=CLWAMBPT;                    /* ADDRESS OF AMB                */ 00825000
         L     R5,CLWAMBPT(,RCORE)                                 0503 00826000
*   RSTATRTN=IDAR192C;              /* NOTE CALL TO 192C             */ 00827000
         L     @14,CLWCOMWK(,RCORE)                                0504 00828000
         MVC   RSTATRTN(4,@14),@CC04105                            0504 00829000
*   CALL IDA0192C;                  /* UPDATE CATALOG FOR AMB        */ 00830000
         L     @15,@CV04580                                        0505 00831000
         BALR  @14,@15                                             0505 00832000
*   RSTATRTN=IDAR231B;              /* NOTE 231B IN CONTROL          */ 00833000
         L     @14,CLWCOMWK(,RCORE)                                0506 00834000
         MVC   RSTATRTN(4,@14),@CC04131                            0506 00835000
*   IF RETCODE^=ZERO THEN           /* IF ERROR DURING CATALOG UPDATE*/ 00836000
         LTR   RETCODE,RETCODE                                     0507 00837000
