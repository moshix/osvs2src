         LA    @09,0(,@09)                                         0509 00842000
         ST    @09,PDPSFI+280(,@14)                                0509 00843000
*       MWAPDPRM=RETCODE;           /* SAVE RETURN CODE              */ 00844000
         ST    RETCODE,MWAPDPRM                                    0510 00845000
*       CALL PROBDT;                /* CALL PROBLEM DETERMINATION    */ 00846000
         BAL   @14,PROBDT                                          0511 00847000
*     END;                                                         0512 00848000
*   RESPECIFY                                                      0513 00849000
*    (R1,                                                          0513 00850000
*     R5) UNRESTRICTED;                                            0513 00851000
@RF00507 DS    0H                                                  0514 00852000
*   IF UCB2OPT2=ON THEN             /* CATALOG ON SHARE DEVICE       */ 00853000
         TM    UCB2OPT2(MWAUCBPT),B'00100000'                      0514 00854000
