*   IF AMBLWRIT=ON THEN             /* IF ACB IS OPENNED FOR OUTPUT  */ 00764000
         L     @14,ACBAMBL(,RACB)                                  0478 00765000
         TM    AMBLWRIT(@14),B'00100000'                           0478 00766000
         BNO   @RF00478                                            0478 00767000
*     DO;                                                          0479 00768000
*       AMDSTSP=MWATIMES;           /* UPDATE TIMESTAMP IN AMDSB     */ 00769000
         L     @14,CLWAMBPT(,RCORE)                                0480 00770000
         L     @09,AMBDSB(,@14)                                    0480 00771000
