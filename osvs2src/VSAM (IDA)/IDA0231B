         TITLE 'IDA0231B - VS2 VSAM TCLOSE CLUSTER                     *00001000
                        '                                               00002000
IDA0231B CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0231B  78.144'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@03                                         0001 00013000
JSCBDISP EQU 188                                                        00014000
JSCBAOS1 EQU  72                                                        00015000
JSCBAOS2 EQU 192                                                        00016000
RBPRFXLN EQU 64                                                         00017000
RBPRFLNA EQU 32                                                         00018000
PRBLEN   EQU 136                                                        00019000
SIRBLEN  EQU 200                                                        00020000
TIRBLEN  EQU 136                                                        00021000
IRBLEN   EQU 128                                                        00022000
SVRBLEN  EQU 200                                                        00023000
*                                      USE                           */ 00024000
*   RSTATRTN=IDAR231B;              /* NOTE 231B IN CONTROL          */ 00025000
         L     @05,CLWCOMWK(,RCORE)                                0240 00026000
         MVC   RSTATRTN(4,@05),@CC04131                            0240 00027000
*   PDPFUNC=PDF157;                 /* TCLOSE AMBL                   */ 00028000
         MVI   PDPFUNC+280(@05),X'9D'                              0241 00029000
*/********************************************************************/ 00030000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 00031000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 00032000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 00033000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 00034000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 00035000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 00036000
*/********************************************************************/ 00037000
*                                                                  0242 00038000
*   GEN;                                                           0242 00039000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00040000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00041000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00042000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00043000
*                                       IF NO ERROR WAS ENCOUNTERED     00044000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00045000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00046000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00047000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00048000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00049000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00050000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00051000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00052000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00053000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00054000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00055000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00056000
         B     GMN00200                 BYPASS SETTING ERROR            00057000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00058000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00059000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00060000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00061000
*   RESPECIFY                                                      0243 00062000
*     RETCODE RESTRICTED;                                          0243 00063000
*   IF RETCODE=ZERO THEN            /* GETMAIN FAILED                */ 00064000
         LTR   RETCODE,RETCODE                                     0244 00065000
         BNZ   @RF00244                                            0244 00066000
*     DO;                                                          0245 00067000
*       PDPCODE=PP20;               /* SET PROBLEM CODE              */ 00068000
         L     @14,CLWCOMWK(,RCORE)                                0246 00069000
         MVI   PDPCODE+280(@14),X'14'                              0246 00070000
*       ACBERFLG=OPERR136;          /* SET ACB ERROR FLAG            */ 00071000
         MVI   ACBERFLG(RACB),X'88'                                0247 00072000
*       CLWTERM=ON;                 /* INDICATE TERMINATING ERROR    */ 00073000
         OI    CLWTERM(RCORE),B'00010000'                          0248 00074000
*       CALL PROBDT;                /* PROBLEM DETERMINATION         */ 00075000
         BAL   @14,PROBDT                                          0249 00076000
*     END;                                                         0250 00077000
*   ELSE                            /* GETMAIN SUCCESSFUL            */ 00078000
*     DO;                                                          0251 00079000
         B     @RC00244                                            0251 00080000
@RF00244 DS    0H                                                  0252 00081000
*       CLWGMAIN=ON;                /* INDICATE MODULE WORK AREA   0252 00082000
*                                      BUILT                         */ 00083000
         OI    CLWGMAIN(RCORE),B'00100000'                         0252 00084000
*       RETCODE->SABWRD=R13;        /* PUT SAVE AREA ADDRESS IN MWA  */ 00085000
         ST    R13,SABWRD(,RETCODE)                                0253 00086000
*       R13=RETCODE;                /* MWA ADDRESS INTO REGISTER 13  */ 00087000
         LR    R13,RETCODE                                         0254 00088000
*       GEN(USING @DATD,R13);       /* ADDRESSABILITY FOR AUTOMATICS */ 00089000
         USING @DATD,R13                                                00090000
*       CALL VALCHECK;              /* VALIDITY CHECK AMBL AND DEB'S */ 00091000
         BAL   @14,VALCHECK                                        0256 00092000
*       IF CLWNWRIT=OFF THEN        /* NOT SUPPRESS WRTBFR           */ 00093000
         TM    CLWNWRIT(RCORE),B'00100000'                         0257 00094000
         BNZ   @RF00257                                            0257 00095000
*         DO;                                                      0258 00096000
*           CALL WRITBUFR;          /* WRITE OUT DATA SET BUFFERS    */ 00097000
         BAL   @14,WRITBUFR                                        0259 00098000
*           IF MWAPCODE^=ZERO THEN  /* ERROR IN WRTBFR       @Z30SSRL*/ 00099000
         CLI   MWAPCODE,0                                          0260 00100000
         BE    @RF00260                                            0260 00101000
*             CLWCNOUP=ON;          /* SUPPRESS CATALOG UPDATE       */ 00102000
         OI    CLWCNOUP(RCORE),B'01000000'                         0261 00103000
*         END;                                                     0262 00104000
@RF00260 DS    0H                                                  0263 00105000
*       CLWAMBPT=AMBLDTA;           /* POINT TO DATA AMB             */ 00106000
@RF00257 L     @14,ACBAMBL(,RACB)                                  0263 00107000
         L     @05,AMBLDTA(,@14)                                   0263 00108000
         ST    @05,CLWAMBPT(,RCORE)                                0263 00109000
*       IF BIBVIRT=ON&AMDWAIT=ON THEN/* CHECK FOR VIRTUAL DEVICE     */ 00110000
         L     @14,AMBLBIB(,@14)                                   0264 00111000
         TM    BIBVIRT(@14),B'10000000'                            0264 00112000
         BNO   @RF00264                                            0264 00113000
         L     @14,AMBDSB(,@05)                                    0264 00114000
         TM    AMDWAIT(@14),B'00010000'                            0264 00115000
         BNO   @RF00264                                            0264 00116000
*         CALL VIRTPROC;            /* FORCE DESTAGE IF VIRTUAL    0265 00117000
*                                      DEVICE                        */ 00118000
         BAL   @14,VIRTPROC                                        0265 00119000
*       CLWAMBPT=ZERO;              /* ZERO AMB POINTER IN CLW       */ 00120000
@RF00264 SLR   @14,@14                                             0266 00121000
         ST    @14,CLWAMBPT(,RCORE)                                0266 00122000
*       IF CLWCNOUP=OFF THEN        /* NOT SUPPRESS CATALOG UPDATE   */ 00123000
         TM    CLWCNOUP(RCORE),B'01000000'                         0267 00124000
         BNZ   @RF00267                                            0267 00125000
*         CALL UPCATACB;            /* UPDATE CATALOG                */ 00126000
         BAL   @14,UPCATACB                                        0268 00127000
*       IF CVTSMCA^=ZERO&           /* SMCA EXIST                    */ 00128000
*           SMCADSA=ON&             /* DATA SET INFORMATION REQUIRED */ 00129000
*           (SMCAUSER=ON&SMCAMAN=ON)&/* SMF RECORDING                */ 00130000
*           TCBTCT^=ZERO THEN       /* TCT EXIST                     */ 00131000
@RF00267 SLR   @14,@14                                             0269 00132000
         L     @05,FLCCVT                                          0269 00133000
         L     @05,CVTSMCA(,@05)                                   0269 00134000
         CR    @05,@14                                             0269 00135000
         BE    @RF00269                                            0269 00136000
         TM    SMCADSA(@05),B'00010000'                            0269 00137000
         BNO   @RF00269                                            0269 00138000
         TM    SMCAUSER(@05),B'11000000'                           0269 00139000
         BNO   @RF00269                                            0269 00140000
         L     @05,PSATOLD                                         0269 00141000
         C     @14,TCBTCT(,@05)                                    0269 00142000
         BE    @RF00269                                            0269 00143000
*         CALL UPSMF;               /* WRITE SMF RECORD 64           */ 00144000
         BAL   @14,UPSMF                                           0270 00145000
*TERM231B:                          /* TERMINATION PROCESSING        */ 00146000
*       RESPECIFY                                                  0271 00147000
*        (R0,                                                      0271 00148000
*         R1) RESTRICTED;                                          0271 00149000
@RF00269 DS    0H                                                  0271 00150000
TERM231B DS    0H                                                  0272 00151000
*       GEN(L R0,@SIZDATD);         /* MODULE WORK AREA SIZE AND SP  */ 00152000
         L R0,@SIZDATD                                                  00153000
*       R1=R13;                     /* ADDRESS OF MODULE WORK AREA   */ 00154000
         LR    R1,R13                                              0273 00155000
*       R13=SABWRD;                 /* ADDRESS OF PREVIOUS SAVEAREA  */ 00156000
         L     R13,SABWRD(,R13)                                    0274 00157000
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE MODULE WORK AREA        */ 00158000
         FREEMAIN R,LV=(0),A=(1)                                        00159000
*       RESPECIFY                                                  0276 00160000
*        (R0,                                                      0276 00161000
*         R1) UNRESTRICTED;                                        0276 00162000
*     END;                                                         0277 00163000
*   IF CLWTERM=ON THEN              /* TERMINATING ERROR             */ 00164000
@RC00244 TM    CLWTERM(RCORE),B'00010000'                          0278 00165000
         BNO   @RF00278                                            0278 00166000
*     RETCODE=FOUR;                 /* RETURN TO IDA0231T WITH R15=4 */ 00167000
         LA    RETCODE,4                                           0279 00168000
*   ELSE                            /* NO TERMINATING ERROR          */ 00169000
*     RETCODE=ZERO;                 /* RETURN TO IDA0231T WITH R15=0 */ 00170000
         B     @RC00278                                            0280 00171000
@RF00278 SLR   RETCODE,RETCODE                                     0280 00172000
*   SACODE=RETCODE;                 /* SET RETURN CODE IN SAVE AREA  */ 00173000
@RC00278 ST    RETCODE,SACODE(,R13)                                0281 00174000
*   LM(RET,R12,SAREGS);             /* RELOAD CALLERS REGISTERS      */ 00175000
         LM    RET,R12,SAREGS(R13)                                 0282 00176000
*   GEN(RETURN);                    /* RETURN TO CALLER              */ 00177000
         RETURN                                                         00178000
*VALCHECK:                                                         0284 00179000
*                                                                  0284 00180000
*/********************************************************************/ 00181000
*/*                                                                  */ 00182000
*/*      01 FUNCTION - VALIDITY CHECK AMBL AND AMB DEB.              */ 00183000
*/*      01 INPUTS   - POINTER TO AMBL IN MWAMBLPT.                  */ 00184000
*/*      01 OUTPUTS -  AMBL AND AMB DEB'S WERE CHECKED TO BE VALID   */ 00185000
*/*                                                                  */ 00186000
*/********************************************************************/ 00187000
*                                                                  0284 00188000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0284 00189000
VALCHECK DS    0H                                                  0285 00190000
*   RESPECIFY                                                      0285 00191000
*     RET RESTRICTED;                                              0285 00192000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 00193000
         ST    RET,MWASAVEA                                        0286 00194000
*   RESPECIFY                                                      0287 00195000
*     RET UNRESTRICTED;                                            0287 00196000
*   MWAPFUNC=PDF111;                /* SET FUNCTION CODE             */ 00197000
         MVI   MWAPFUNC,X'6F'                                      0288 00198000
*   MWAVATPT=JSCBSHR;               /* POINT TO THE VAT              */ 00199000
         L     @14,PSATOLD                                         0289 00200000
         L     @14,TCBJSCB(,@14)                                   0289 00201000
         L     @14,JSCBSHR(,@14)                                   0289 00202000
         ST    @14,MWAVATPT                                        0289 00203000
*   DO WHILE AMBLVRT^=VATVRT&VATNEXT^=0;/* FIND CORRECT VAT EXTENSION*/ 00204000
         B     @DE00290                                            0290 00205000
@DL00290 DS    0H                                                  0291 00206000
*     MWAVATPT=VATNEXT;             /* POINT TO NEXT EXTENSION       */ 00207000
         L     @14,MWAVATPT                                        0291 00208000
         L     @14,VATNEXT(,@14)                                   0291 00209000
         ST    @14,MWAVATPT                                        0291 00210000
*   END;                                                           0292 00211000
@DE00290 L     @14,ACBAMBL(,RACB)                                  0292 00212000
         L     @05,MWAVATPT                                        0292 00213000
         CLC   AMBLVRT(1,@14),VATVRT(@05)                          0292 00214000
         BE    @DC00290                                            0292 00215000
         L     @14,VATNEXT(,@05)                                   0292 00216000
         LTR   @14,@14                                             0292 00217000
         BNZ   @DL00290                                            0292 00218000
@DC00290 DS    0H                                                  0293 00219000
*   IF AMBLENO<=VATENO&             /* VALID USER AMBL               */ 00220000
*       ACBAMBL=VATAMBL(AMBLENO) THEN/* CHECK AMBL AGAINST VAT       */ 00221000
         L     @14,ACBAMBL(,RACB)                                  0293 00222000
         L     @05,MWAVATPT                                        0293 00223000
         CLC   AMBLENO(1,@14),VATENO(@05)                          0293 00224000
         BH    @RF00293                                            0293 00225000
         SLR   @11,@11                                             0293 00226000
         IC    @11,AMBLENO(,@14)                                   0293 00227000
         SLA   @11,2                                               0293 00228000
         C     @14,VATAMBL-4(@11,@05)                              0293 00229000
         BNE   @RF00293                                            0293 00230000
*     DO;                           /* IF AMBL IS VALID              */ 00231000
*       RESPECIFY                                                  0295 00232000
*         R1 RESTRICTED;                                           0295 00233000
*       R1=AMBLDTA;                 /* POINT TO DATA AMB             */ 00234000
         L     R1,AMBLDTA(,@14)                                    0296 00235000
*       GEN(DEBCHK (1),TYPE=VERIFY,AM=VSAM);/* VERIFY DATA DEB       */ 00236000
         DEBCHK (1),TYPE=VERIFY,AM=VSAM                                 00237000
*       IF AMBLIX^=ZERO THEN        /* IF KSDS                       */ 00238000
         L     @14,ACBAMBL(,RACB)                                  0298 00239000
         L     @14,AMBLIX(,@14)                                    0298 00240000
         LTR   @14,@14                                             0298 00241000
         BZ    @RF00298                                            0298 00242000
*         DO;                                                      0299 00243000
*           R1=AMBLIX;              /* POINT TO INDEX AMB            */ 00244000
         LR    R1,@14                                              0300 00245000
*           GEN(DEBCHK (1),TYPE=VERIFY,AM=VSAM);/* VERIFY INDEX DEB  */ 00246000
         DEBCHK (1),TYPE=VERIFY,AM=VSAM                                 00247000
*         END;                                                     0302 00248000
*       RESPECIFY                                                  0303 00249000
*         R1 UNRESTRICTED;                                         0303 00250000
@RF00298 DS    0H                                                  0304 00251000
*     END;                                                         0304 00252000
*   ELSE                            /* IF AMBL IS INVALID            */ 00253000
*     DO;                                                          0305 00254000
         B     @RC00293                                            0305 00255000
@RF00293 DS    0H                                                  0306 00256000
*       CLWTERM=ON;                 /* INDICATE TERMINATING ERROR    */ 00257000
         OI    CLWTERM(RCORE),B'00010000'                          0306 00258000
*       MWAPCODE=PP78;              /* PROBLEM CODE FOR INVALID AMBL */ 00259000
         MVI   MWAPCODE,X'4E'                                      0307 00260000
*       CALL PROBDT;                /* GET PROBLEM MESSAGE           */ 00261000
         BAL   @14,PROBDT                                          0308 00262000
*       RETURN TO TERM231B;         /* GO TO TERM231B                */ 00263000
         LA    @14,TERM231B                                        0309 00264000
@EL00002 DS    0H                                                  0309 00265000
@EF00002 DS    0H                                                  0309 00266000
@ER00002 BR    @14                                                 0309 00267000
*     END;                                                         0310 00268000
*   MWAVATPT=JSCBSHR;               /* RESTORE POINTER TO VAT        */ 00269000
@RC00293 L     @14,PSATOLD                                         0311 00270000
         L     @14,TCBJSCB(,@14)                                   0311 00271000
         L     @14,JSCBSHR(,@14)                                   0311 00272000
         ST    @14,MWAVATPT                                        0311 00273000
*   RESPECIFY                                                      0312 00274000
*     RET RESTRICTED;                                              0312 00275000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 00276000
         L     RET,MWASAVEA                                        0313 00277000
*   RESPECIFY                                                      0314 00278000
*     RET UNRESTRICTED;                                            0314 00279000
*   END VALCHECK;                                                  0315 00280000
         B     @EL00002                                            0315 00281000
*WRITBUFR:                                                         0316 00282000
*                                                                  0316 00283000
*/********************************************************************/ 00284000
*/*                                                                  */ 00285000
*/*      01 FUNCTION - INTERFACE WITH RECORD MANAGEMENT TO WRITE OUT */ 00286000
*/*                    BUFFERS. IF WRTBFR FAILS FOR PLH NOT AVAIL-   */ 00287000
*/*                    ABLE, THEN INTERFACE WITH IDA0192Y TO OBTAIN  */ 00288000
*/*                    AND RELEASE A DISCONNECTED PLH FOR THIS       */ 00289000
*/*                    REQUEST, AND REISSUE THE WRTBFR.  PROBLEM     */ 00290000
*/*                    DETERMINATION IS CALLED IF ANY ERRORS OCCUR.  */ 00291000
*/*      01 INPUTS -   POINTER TO VAT IN JSCBSHR                     */ 00292000
*/*      01 OUTPUTS -  BUFFERS FOR AMBL WRITTEN OUT                  */ 00293000
*/*                    MWAPCODE IS SET                               */ 00294000
*/*                                                                  */ 00295000
*/********************************************************************/ 00296000
*                                                                  0316 00297000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0316 00298000
WRITBUFR DS    0H                                                  0317 00299000
*   RESPECIFY                                                      0317 00300000
*     RET RESTRICTED;                                              0317 00301000
*   MWASAVEA=RET;                   /* SAVE RETURN ADDRESS           */ 00302000
         ST    RET,MWASAVEA                                        0318 00303000
*   RESPECIFY                                                      0319 00304000
*     RET UNRESTRICTED;                                            0319 00305000
*   PDPFUNC=PDF161;                 /* SET FUNCTION CODE             */ 00306000
         L     @14,CLWCOMWK(,RCORE)                                0320 00307000
         MVI   PDPFUNC+280(@14),X'A1'                              0320 00308000
*   CLWAMBPT=AMBLDTA;               /* POINT TO DATA AMB     @Z30SSRL*/ 00309000
         L     @14,ACBAMBL(,RACB)                                  0321 00310000
         L     @14,AMBLDTA(,@14)                                   0321 00311000
         ST    @14,CLWAMBPT(,RCORE)                                0321 00312000
*   MWAPCODE=ZERO;                  /* CLEAR MWAPCODE        @Z30SSRL*/ 00313000
         MVI   MWAPCODE,X'00'                                      0322 00314000
*   MWABYTES=LENGTH(IFGRPL)+LENGTH(MWABUFSA);/* BUILD DUMMY RPL AND     00315000
*                                      REGISTER SAVE AREA            */ 00316000
         MVC   MWABYTES(3),@CF04163+1                              0323 00317000
*   MWASPOOL=USERPOOL;              /* UNPROTECTED SUBPOOL FOR SYNCH */ 00318000
         MVI   MWASPOOL,X'FA'                                      0324 00319000
*   CALL GETCORE;                   /* CALL ROUTINE TO GET STORAGE   */ 00320000
         BAL   @14,GETCORE                                         0325 00321000
*   IF RETCODE=ZERO THEN            /* BUILD DUMMY RPL SUCCESSFUL  0326 00322000
*                                                            @Z30SSRL*/ 00323000
         LTR   RETCODE,RETCODE                                     0326 00324000
         BNZ   @RF00326                                            0326 00325000
*     DO;                                                          0327 00326000
*       MWARPLPT=MWADDRES+LENGTH(MWABUFSA);/* ADDRESSABILITY FOR RPL */ 00327000
         LA    @14,72                                              0328 00328000
         AL    @14,MWADDRES                                        0328 00329000
         ST    @14,MWARPLPT                                        0328 00330000
*       IF RACB=DXPACBAD THEN       /* IF THIS IS THE COPIED ACB     */ 00331000
         L     @05,CLWCOMWK(,RCORE)                                0329 00332000
         L     @11,DXPACBAD-1(,@05)                                0329 00333000
         LA    @11,0(,@11)                                         0329 00334000
         CR    RACB,@11                                            0329 00335000
         BNE   @RF00329                                            0329 00336000
*         RPLDACB=DXUACBAD;         /* POINT RPL TO THE USERS ACB    */ 00337000
         L     @05,DXUACBAD-1(,@05)                                0330 00338000
         LA    @05,0(,@05)                                         0330 00339000
         ST    @05,RPLDACB(,@14)                                   0330 00340000
*       ELSE                        /* IF NOT THE COPY               */ 00341000
*         RPLDACB=RACB;             /* USE THE DUMMY OR UPGRADE ACB  */ 00342000
         B     @RC00329                                            0331 00343000
@RF00329 L     @14,MWARPLPT                                        0331 00344000
         ST    RACB,RPLDACB(,@14)                                  0331 00345000
*       CALL WRBUFFER;              /* CALL ROUTINE TO ISSUE WRTBFR  */ 00346000
@RC00329 BAL   @14,WRBUFFER                                        0332 00347000
*       IF RETCODE=MWALERR&RPLFDBK=MWANOPLH THEN/* IF PLH NOT      0333 00348000
*                                      AVAILABLE                     */ 00349000
         C     RETCODE,@CF00049                                    0333 00350000
         BNE   @RF00333                                            0333 00351000
         L     @14,MWARPLPT                                        0333 00352000
         CLC   RPLFDBK(3,@14),@CF00115+1                           0333 00353000
         BNE   @RF00333                                            0333 00354000
*         DO;                       /* GET ONE FROM IDA0192Y         */ 00355000
*           MWACLWRK=RCORE;         /* SAVE POINTER TO ACB WORKAREA  */ 00356000
         LR    MWACLWRK,RCORE                                      0335 00357000
*           RCORE=ADDR(FORCORE);    /* GET POINTER TO COMMON WORKAREA*/ 00358000
         L     RCORE,CLWCOMWK(,RCORE)                              0336 00359000
*           BLPFLAGS=ZERO;          /* CLEAR PARM LIST FLAGS @Z30SSRL*/ 00360000
         SLR   @07,@07                                             0337 00361000
         STH   @07,BLPFLAGS(,@14)                                  0337 00362000
*           BLPFLSR=ON;             /* INDICATE LSR REQUEST          */ 00363000
*           BLPFBLD=ON;             /* INDICATE BUILD REQUEST        */ 00364000
         OI    BLPFLSR(@14),B'01010000'                            0339 00365000
*           IF VSRTIOBF=ON THEN     /* IF IOMBS FIXED THEN           */ 00366000
         L     @07,MWAVATPT                                        0340 00367000
         L     @07,VATVPTR(,@07)                                   0340 00368000
         TM    VSRTIOBF(@07),B'00100000'                           0340 00369000
         BNO   @RF00340                                            0340 00370000
*             BLPFIOBF=ON;          /* INDICATE IOMBS ARE FIXED      */ 00371000
         OI    BLPFIOBF(@14),B'00000100'                           0341 00372000
*           IF VSRTBFRF=ON THEN     /* IF BUFFERS FIXED THEN         */ 00373000
@RF00340 L     @14,MWAVATPT                                        0342 00374000
         L     @14,VATVPTR(,@14)                                   0342 00375000
         TM    VSRTBFRF(@14),B'00010000'                           0342 00376000
         BNO   @RF00342                                            0342 00377000
*             BLPFBFRF=ON;          /* INDICATE BUFFERS ARE FIXED    */ 00378000
         L     @14,MWARPLPT                                        0343 00379000
         OI    BLPFBFRF(@14),B'00000010'                           0343 00380000
*           BLPFIOSR=ON;            /* INDICATE I/O SUPPORT REQUEST  */ 00381000
@RF00342 L     @14,MWARPLPT                                        0344 00382000
         OI    BLPFIOSR(@14),B'00000010'                           0344 00383000
*           MWAACBPT=RACB;          /* SAVE CURRENT ACB ADDRESS      */ 00384000
         LR    MWAACBPT,RACB                                       0345 00385000
