         TITLE 'IDA0231T - VS2 VSAM TCLOSE ACB                         *00001000
                        '                                               00002000
IDA0231T CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0231T  78.151'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@03                                         0001 00013000
JSCBDISP EQU 188                                                        00014000
JSCBAOS1 EQU  72                                                        00015000
JSCBAOS2 EQU 192                                                        00016000
RBPRFXLN EQU 64                                                         00017000
RBPRFLNA EQU 32                                                         00018000
PRBLEN   EQU 136                                                        00019000
SIRBLEN  EQU 200                                                        00020000
TIRBLEN  EQU 136                                                        00021000
IRBLEN   EQU 128                                                        00022000
SVRBLEN  EQU 224                                                        00023000
*                                                                  0225 00024000
*/********************************************************************/ 00025000
*/*                                                                  */ 00026000
*/*                     MODULE IDA0231T                              */ 00027000
*/*                                                                  */ 00028000
*/********************************************************************/ 00029000
*                                                                  0225 00030000
*   IF AMBLDUMY=OFF THEN            /* IF NOT A DUMMY DATA SET     0225 00031000
*                                                            @ZA17534*/ 00032000
         L     @11,ACBAMBL(,RACB)                                  0225 00033000
         TM    AMBLDUMY(@11),B'00000001'                           0225 00034000
         BNZ   @RF00225                                            0225 00035000
*     DO;                           /* THEN CONTINUE NORMAL PROC   0226 00036000
*                                                            @ZA17534*/ 00037000
*       RESPECIFY                                                  0227 00038000
*         R12 UNRESTRICTED;         /* FREE BASE REGISTER FOR GENERAL   00039000
*                                      USE                           */ 00040000
*       RSTATRTN=IDAR231T;          /* NOTE 231T IN CONTROL          */ 00041000
         MVC   RSTATRTN(4,RCORE),@CC03388                          0228 00042000
*       IF(AMBLLSR=OFF&AMBLGSR=OFF)|/* IF (NEITHER GSR NOR LSR) OR   */ 00043000
*           (AMBLLSR=ON&BIBCKPT=ON)|/* (LSR AND CHECK POINT) OR    0229 00044000
*                                                            @Z30SSMD*/ 00045000
*           BIBREST=ON THEN         /* RESTART THEN          @Z30SSMD*/ 00046000
         TM    AMBLLSR(@11),B'01100000'                            0229 00047000
         BZ    @RT00229                                            0229 00048000
         L     @11,ACBAMBL(,RACB)                                  0229 00049000
         TM    AMBLLSR(@11),B'00100000'                            0229 00050000
         BNO   @GL00001                                            0229 00051000
         L     @11,AMBLBIB(,@11)                                   0229 00052000
         TM    BIBCKPT(@11),B'00100000'                            0229 00053000
         BO    @RT00229                                            0229 00054000
@GL00001 L     @11,ACBAMBL(,RACB)                                  0229 00055000
         L     @11,AMBLBIB(,@11)                                   0229 00056000
         TM    BIBREST(@11),B'01000000'                            0229 00057000
         BNO   @RF00229                                            0229 00058000
@RT00229 DS    0H                                                  0230 00059000
*         DO;                       /* PERFORM T-CLOSE PROCESSING    */ 00060000
*           CALL INIT231T;          /* TCLOSE INITIALIZATION         */ 00061000
         BAL   @14,INIT231T                                        0231 00062000
*           GEN(USING @DATD,R13);   /* ESTABLISH ADDRESSABILITY      */ 00063000
         USING @DATD,R13                                                00064000
*           RESPECIFY                                              0233 00065000
*             RETCODE RESTRICTED;                                  0233 00066000
*           IF AMBLLSR=OFF|BIBREST=ON THEN/* IF RESTART OR NOT LSR   */ 00067000
         L     @14,ACBAMBL(,RACB)                                  0234 00068000
         TM    AMBLLSR(@14),B'00100000'                            0234 00069000
         BZ    @RT00234                                            0234 00070000
         L     @14,AMBLBIB(,@14)                                   0234 00071000
         TM    BIBREST(@14),B'01000000'                            0234 00072000
         BNO   @RF00234                                            0234 00073000
@RT00234 DS    0H                                                  0235 00074000
*             CLWNWRIT=ON;          /* SUPPRESS WRITE BUFFER         */ 00075000
         OI    CLWNWRIT(RCORE),B'00100000'                         0235 00076000
*           IF BIBREST=ON|AMBLLSR=ON THEN/* IF RESTART OR LSR      0236 00077000
*                                                            @Z30SSMD*/ 00078000
@RF00234 L     @14,ACBAMBL(,RACB)                                  0236 00079000
         L     @05,AMBLBIB(,@14)                                   0236 00080000
         TM    BIBREST(@05),B'01000000'                            0236 00081000
         BO    @RT00236                                            0236 00082000
         TM    AMBLLSR(@14),B'00100000'                            0236 00083000
         BNO   @RF00236                                            0236 00084000
@RT00236 DS    0H                                                  0237 00085000
*             RETCODE=ZERO;         /* BYPASS FLUSH/QUIESCE PROCEDURE*/ 00086000
         SLR   RETCODE,RETCODE                                     0237 00087000
*           ELSE                    /* OTHERWISE, NORMAL T-CLOSE     */ 00088000
*             CALL FLQUIS;          /* COMPLETE I/O                  */ 00089000
         B     @RC00236                                            0238 00090000
@RF00236 BAL   @14,FLQUIS                                          0238 00091000
*           IF RETCODE=ZERO THEN    /* NO I/O ERROR                  */ 00092000
@RC00236 LTR   RETCODE,RETCODE                                     0239 00093000
         BNZ   @RF00239                                            0239 00094000
*             DO;                                                  0240 00095000
*               IF CLWPATH=ON THEN  /* IF PATH PROCESSING            */ 00096000
         TM    CLWPATH(RCORE),B'00010000'                          0241 00097000
         BNO   @RF00241                                            0241 00098000
*                 CALL TCLSPATH;    /* TCLOSE PATH AIX               */ 00099000
         BAL   @14,TCLSPATH                                        0242 00100000
*               CALL TCLSBASE;      /* TCLOSE BASE ESDS OR KSDS      */ 00101000
@RF00241 BAL   @14,TCLSBASE                                        0243 00102000
*               IF BIBUPT^=ZERO THEN/* UPGRADE TABLE EXIST           */ 00103000
         L     @14,ACBAMBL(,RACB)                                  0244 00104000
         L     @14,AMBLBIB(,@14)                                   0244 00105000
         L     @14,BIBUPT(,@14)                                    0244 00106000
         LTR   @14,@14                                             0244 00107000
         BZ    @RF00244                                            0244 00108000
*                 CALL TCLSUPGR;    /* TCLOSE UPGRADE AIX'S          */ 00109000
         BAL   @14,TCLSUPGR                                        0245 00110000
*             END;                                                 0246 00111000
*           ELSE                    /* IF I/O ERROR EXIST            */ 00112000
*             DO;                                                  0247 00113000
         B     @RC00239                                            0247 00114000
@RF00239 DS    0H                                                  0248 00115000
*               RESPECIFY                                          0248 00116000
*                 R1 RESTRICTED;                                   0248 00117000
*               PDPCODE=PP22;       /* SET I/O ERROR PROBLEM CODE    */ 00118000
         MVI   PDPCODE+280(RCORE),X'16'                            0249 00119000
*               ACBERFLG=OPERR184;  /* SET ACB ERROR FLAG            */ 00120000
         MVI   ACBERFLG(RACB),X'B8'                                0250 00121000
*               R1=MWACORAD;        /* POINT TO COMMON WORK AREA     */ 00122000
         L     R1,MWACORAD                                         0251 00123000
*               RSTATRTN=IDAR192P;  /* NOTE CALL TO 192P             */ 00124000
         MVC   RSTATRTN(4,RCORE),@CC03370                          0252 00125000
*               CALL IDA0192P;      /* CALL PROBLEM DETERMINATION    */ 00126000
         L     @15,@CV03796                                        0253 00127000
         BALR  @14,@15                                             0253 00128000
*               RSTATRTN=IDAR231T;  /* NOTE 231T IN CONTROL          */ 00129000
         MVC   RSTATRTN(4,RCORE),@CC03388                          0254 00130000
*               RESPECIFY                                          0255 00131000
*                 R1 UNRESTRICTED;                                 0255 00132000
*             END;                                                 0256 00133000
*           RESPECIFY                                              0257 00134000
*             RETCODE UNRESTRICTED;                                0257 00135000
@RC00239 DS    0H                                                  0258 00136000
*EXIT:                                                             0258 00137000
*           IF MWAENQFG=ON THEN     /* ENQ WAS ISSUED                */ 00138000
EXIT     TM    MWAENQFG,B'10000000'                                0258 00139000
         BNO   @RF00258                                            0258 00140000
*             DO;                                                  0259 00141000
*               RESPECIFY                                          0260 00142000
*                 R1 RESTRICTED;                                   0260 00143000
*               R1=MWAENQPT+LENGTH(ENQTCB);/* POINT TO ENQ PARAMETER    00144000
*                                      LIST                          */ 00145000
         LA    R1,4                                                0261 00146000
         AL    R1,MWAENQPT                                         0261 00147000
*               GEN(DEQ  MF=(E,(1)));/* ISSUE DEQ            @ZA10878*/ 00148000
         DEQ  MF=(E,(1))                                                00149000
*               RESPECIFY                                          0263 00150000
*                 R1 UNRESTRICTED;                                 0263 00151000
*               MWADDRES=MWAENQPT;  /* GET ADDR OF ENQ/DEQ PARM LIST */ 00152000
         L     @05,MWAENQPT                                        0264 00153000
         ST    @05,MWADDRES                                        0264 00154000
*               MWASPOOL=WKPOOL;    /* GET SUBPOOL NUMBER            */ 00155000
         MVI   MWASPOOL,X'FC'                                      0265 00156000
*               MWABYTES=LENGTH(ENQTCB)+MWADSCNT*(LENGTH(ENQENTRY)+     00157000
*                   LENGTH(ENQRNAME));/* NUMBER OF BYTES TO BE FREED */ 00158000
         L     @05,MWADSCNT                                        0266 00159000
         MH    @05,@CH01363                                        0266 00160000
         LA    @05,4(,@05)                                         0266 00161000
         STCM  @05,7,MWABYTES                                      0266 00162000
*               CALL FREECORE;      /* FREE ENQ/DEQ AND RNAME PARM 0267 00163000
*                                      LIST                          */ 00164000
         BAL   @14,FREECORE                                        0267 00165000
*             END;                                                 0268 00166000
*           IF MWACLWID=ON THEN     /* ACB WORK AREA (CLW) EXIST     */ 00167000
@RF00258 TM    MWACLWID,B'10000000'                                0269 00168000
         BNO   @RF00269                                            0269 00169000
*             DO;                                                  0270 00170000
*               MWADDRES=RCORE;     /* ADDRESS OF CLW TO BE FREED    */ 00171000
         ST    RCORE,MWADDRES                                      0271 00172000
*               MWABYTES=LENGTH(IDACLWRK);/* SIZE OF ACB WORK AREA   */ 00173000
         MVC   MWABYTES(3),@CF03190+1                              0272 00174000
*               MWASPOOL=WKPOOL;    /* SUBPOOL NUMBER                */ 00175000
         MVI   MWASPOOL,X'FC'                                      0273 00176000
*               CALL FREECORE;      /* FREE ACB WORK AREA            */ 00177000
         BAL   @14,FREECORE                                        0274 00178000
*             END;                                                 0275 00179000
*           RESPECIFY                                              0276 00180000
*            (R0,                                                  0276 00181000
*             R1) RESTRICTED;                                      0276 00182000
@RF00269 DS    0H                                                  0277 00183000
*           GEN(L R0,@SIZDATD);     /* CORE SIZE FOR AUTOMATICS      */ 00184000
         L R0,@SIZDATD                                                  00185000
*           R1=R13;                 /* ADDRESS OF MODULE WORK AREA   */ 00186000
         LR    R1,R13                                              0278 00187000
*           R13=SABWRD;             /* GET ADDR OF PREVIOUS SAVE AREA*/ 00188000
         L     R13,SABWRD(,R13)                                    0279 00189000
*           GEN(FREEMAIN R,LV=(0),A=(1));/* FREE MODULE WORK AREA    */ 00190000
         FREEMAIN R,LV=(0),A=(1)                                        00191000
*           RESPECIFY                                              0281 00192000
*            (R0,                                                  0281 00193000
*             R1) UNRESTRICTED;                                    0281 00194000
*         END;                                                     0282 00195000
*     END;                          /*                       @ZA17534*/ 00196000
@RF00229 DS    0H                                                  0284 00197000
*ENDLABEL:                                                         0284 00198000
*   ACBEXFG=ON;                     /* RESET ACB LOCK BIT TO OFF   0284 00199000
*                                      BIT'1'                        */ 00200000
@RF00225 DS    0H                                                  0284 00201000
ENDLABEL DS    0H                                                  0285 00202000
*   ACBIOSFG=OFF;                   /* RESET ACB BUSY BIT            */ 00203000
         OI    ACBEXFG(RACB),B'00000010'                           0285 00204000
         NI    ACBIOSFG(RACB),B'11111110'                          0285 00205000
*   LM(RET,R12,SAREGS);             /* RELOAD CALLERS REGISTERS      */ 00206000
         LM    RET,R12,SAREGS(R13)                                 0286 00207000
*   GEN(RETURN);                    /* EXIT MODULE                   */ 00208000
         RETURN                                                         00209000
*INIT231T:                                                         0288 00210000
*                                                                  0288 00211000
*/********************************************************************/ 00212000
*/*                                                                  */ 00213000
*/*      01 FUNCTION - MODULE INITIALIZATION, BUILD MODULE WORK AREA */ 00214000
*/*                    BUILD ACB WORK AREA AND CHECK DUMMY DEB       */ 00215000
*/*      01 INPUTS -   1. COMMON WORK AREA                           */ 00216000
*/*                    2. SIZE OF THE AUTOMATICS                     */ 00217000
*/*      01 OUTPUTS -  1. DUMMY DEB CHECKED                          */ 00218000
*/*                    2. MWA AND CLW BUILT                          */ 00219000
*/*                                                                  */ 00220000
*/********************************************************************/ 00221000
*                                                                  0288 00222000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0288 00223000
INIT231T DS    0H                                                  0289 00224000
*   RESPECIFY                                                      0289 00225000
*    (R11,                                                         0289 00226000
*     RET) RESTRICTED;                                             0289 00227000
*   R11=RET;                        /* SAVE RETURN ADDRESS           */ 00228000
         LR    R11,RET                                             0290 00229000
*   RESPECIFY                                                      0291 00230000
*     RET UNRESTRICTED;                                            0291 00231000
*   IDAPDPRM=IDAPDPRM&&IDAPDPRM;    /* ZERO PROBLEM DETERMINATION  0292 00232000
*                                      PARAMETER LIST                */ 00233000
         LA    @05,280                                             0292 00234000
         ALR   @05,RCORE                                           0292 00235000
         XC    IDAPDPRM(40,@05),IDAPDPRM(@05)                      0292 00236000
*   PDPCALL=PDPTCLOS;               /* IDENTIFY CALLER AS TCLOSE     */ 00237000
         MVI   PDPCALL(@05),X'0C'                                  0293 00238000
*   PDPFUNC=PDF151;                 /* IDA0231T MODULE INITIALIZATION*/ 00239000
         MVI   PDPFUNC(@05),X'97'                                  0294 00240000
*   PDPACB=DXUACBAD;                /* ADDRESS OF USER ACB           */ 00241000
         L     @15,DXUACBAD-1(,RCORE)                              0295 00242000
         LA    @15,0(,@15)                                         0295 00243000
         ST    @15,PDPACB(,@05)                                    0295 00244000
*   PDPTIOT=TCBTIO+ACBTIOT;         /* POINT TO CORRECT TIOT ENTRY   */ 00245000
         LH    @15,ACBTIOT(,RACB)                                  0296 00246000
         L     @14,PSATOLD                                         0296 00247000
         AL    @15,TCBTIO(,@14)                                    0296 00248000
         ST    @15,PDPTIOT(,@05)                                   0296 00249000
*   PDPDSTYP=PDPDDNM;               /* INDICATE DD DATA SET NAME USED*/ 00250000
         MVI   PDPDSTYP(@05),X'01'                                 0297 00251000
*   PDPCLSNM=ADDR(JFCBDSNM);        /* SET CLUSTER NAME TO DD      0298 00252000
*                                      STATEMENT DATA SET NAME       */ 00253000
         LA    @15,100                                             0298 00254000
         ALR   @15,RCORE                                           0298 00255000
         ST    @15,PDPCLSNM(,@05)                                  0298 00256000
*   PDPDSN=ZERO;                    /* SET DATA SET NAME ADDR TO ZERO*/ 00257000
         SLR   @15,@15                                             0299 00258000
         ST    @15,PDPDSN(,@05)                                    0299 00259000
*   PDPCTACB=AMBLDTA->AMBCACB;      /* SET POINTER TO CATALOG ACB    */ 00260000
         L     @01,ACBAMBL(,RACB)                                  0300 00261000
         L     @01,AMBLDTA(,@01)                                   0300 00262000
         L     @15,AMBCACB(,@01)                                   0300 00263000
         ST    @15,PDPCTACB(,@05)                                  0300 00264000
*/********************************************************************/ 00265000
*/*           IDAGMAIN - GETS CORE STORAGE FOR AUTOMATICS.           */ 00266000
*/*                      FOR VS2 IDAGMAIN WILL MOVE THE      @Z30SSRL*/ 00267000
*/*                      ID AND DATE OF THE MODULE TO A      @Z30SSRL*/ 00268000
*/*                      LOCATION DIRECTLY AFTER THE PLS     @Z30SSRL*/ 00269000
*/*                      GENERATED SAVE AREA.                @Z30SSRL*/ 00270000
*/*           DATE OF LAST CHANGE = 3 DECEMBER 74            @Z30SSRL*/ 00271000
*/********************************************************************/ 00272000
*                                                                  0301 00273000
*   GEN;                                                           0301 00274000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00275000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00276000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00277000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00278000
*                                       IF NO ERROR WAS ENCOUNTERED     00279000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00280000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00281000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00282000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00283000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00284000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00285000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00286000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00287000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00288000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00289000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00290000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00291000
         B     GMN00200                 BYPASS SETTING ERROR            00292000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00293000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00294000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00295000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00296000
*   RESPECIFY                                                      0302 00297000
*     RETCODE RESTRICTED;                                          0302 00298000
*   IF RETCODE=ZERO THEN            /* GETMAIN FAILED                */ 00299000
         LTR   RETCODE,RETCODE                                     0303 00300000
         BNZ   @RF00303                                            0303 00301000
*     DO;                                                          0304 00302000
*       RESPECIFY                                                  0305 00303000
*         R1 RESTRICTED;                                           0305 00304000
*       PDPCODE=PP20;               /* SET PROBLEM CODE              */ 00305000
         MVI   PDPCODE+280(RCORE),X'14'                            0306 00306000
*       ACBERFLG=OPERR136;          /* SET ACB ERROR FLAG            */ 00307000
         MVI   ACBERFLG(RACB),X'88'                                0307 00308000
*       R1=RCORE;                   /* POINT TO COMMON WORK AREA     */ 00309000
         LR    R1,RCORE                                            0308 00310000
*       RSTATRTN=IDAR192P;          /* NOTE CALL TO 192P             */ 00311000
         MVC   RSTATRTN(4,RCORE),@CC03370                          0309 00312000
*       CALL IDA0192P;              /* CALL PROBLEM DETERMINATION    */ 00313000
         L     @15,@CV03796                                        0310 00314000
         BALR  @14,@15                                             0310 00315000
*       RSTATRTN=IDAR231T;          /* NOTE 231T IN CONTROL          */ 00316000
         MVC   RSTATRTN(4,RCORE),@CC03388                          0311 00317000
*       RETURN TO ENDLABEL;         /* GO TO ENDLABEL                */ 00318000
         LA    @14,ENDLABEL                                        0312 00319000
@EL00002 DS    0H                                                  0312 00320000
@EF00002 DS    0H                                                  0312 00321000
@ER00002 BR    @14                                                 0312 00322000
*       RESPECIFY                                                  0313 00323000
*         R1 UNRESTRICTED;                                         0313 00324000
*     END;                                                         0314 00325000
*   RETCODE->SABWRD=R13;            /* STORE SAVEAREA ADDRESS IN MWA */ 00326000
@RF00303 ST    R13,SABWRD(,RETCODE)                                0315 00327000
*   R13=RETCODE;                    /* ADDRESS OF MWA INTO REGISTER     00328000
*                                      13                            */ 00329000
         LR    R13,RETCODE                                         0316 00330000
*   RESPECIFY                                                      0317 00331000
*     RETCODE UNRESTRICTED;                                        0317 00332000
*   GEN(USING @DATD,R13);           /* ESTABLISH ADDRESSABILITY      */ 00333000
         USING @DATD,R13                                                00334000
*   MWARACB=RACB;                   /* MWA SAVE AREA FOR USER ACB  0319 00335000
*                                      ADDR                          */ 00336000
         ST    RACB,MWARACB                                        0319 00337000
*   MWASAVEA=R11;                   /* SAVE RETURN ADDRESS IN MWA    */ 00338000
         ST    R11,MWASAVEA                                        0320 00339000
*   RESPECIFY                                                      0321 00340000
*     R11 UNRESTRICTED;                                            0321 00341000
*   MWACORAD=RCORE;                 /* SAVE COMMON WORK AREA ADDRESS */ 00342000
         ST    RCORE,MWACORAD                                      0322 00343000
*   RESPECIFY                                                      0323 00344000
*     FORCORE BASED(MWACORAD);      /* RE-BASE COMMON WORK AREA      */ 00345000
*   MWABYTES=LENGTH(IDACLWRK);      /* NUMBER OF BYTES REQUIRED      */ 00346000
         MVC   MWABYTES(3),@CF03190+1                              0324 00347000
*   MWASPOOL=WKPOOL;                /* SUBPOOL NUMBER                */ 00348000
         MVI   MWASPOOL,X'FC'                                      0325 00349000
*   CALL GETCORE;                   /* BUILD ACB WORK AREA (CLW)     */ 00350000
         BAL   @14,GETCORE                                         0326 00351000
*   RCORE=MWADDRES;                 /* CLW ADDRESS INTO RCORE        */ 00352000
         L     RCORE,MWADDRES                                      0327 00353000
*   CLWCOMWK=MWACORAD;              /* SAVE COMMON WORK AREA ADDRESS */ 00354000
         L     @11,MWACORAD                                        0328 00355000
         ST    @11,CLWCOMWK(,RCORE)                                0328 00356000
*   CLWID='IDACLWRK';               /* SET ACB WORK AREA ID  @Z30SSRL*/ 00357000
         MVC   CLWID(8,RCORE),@CC03808                             0329 00358000
*   MWACLWID=ON;                    /* INDICATE CLW BUILD SUCCESSFUL */ 00359000
         OI    MWACLWID,B'10000000'                                0330 00360000
*   RACB=DXPACBAD;                  /* PT TO COPIED ACB FOR        0331 00361000
*                                      PROCESSING                    */ 00362000
         L     RACB,DXPACBAD-1(,@11)                               0331 00363000
         LA    RACB,0(,RACB)                                       0331 00364000
*   RFY                                                            0332 00365000
*     R1 RSTD;                      /* USED IN DEBCHK                */ 00366000
*   R1=DXUACBAD;                    /* LOAD ACB ADDRESS              */ 00367000
         L     R1,DXUACBAD-1(,@11)                                 0333 00368000
         LA    R1,0(,R1)                                           0333 00369000
*   GEN(DEBCHK (1),TYPE=VERIFY,AM=VSAM);/* VERIFY DUMMY DEB          */ 00370000
         DEBCHK (1),TYPE=VERIFY,AM=VSAM                                 00371000
*   RFY                                                            0335 00372000
*     R1 UNRSTD;                                                   0335 00373000
*   ACBERFLG=ZERO;                  /* ZERO ACB ERROR FLAG   @Z30SSRL*/ 00374000
         MVI   ACBERFLG(RACB),X'00'                                0336 00375000
*   IF AMBLPATH=ON THEN             /* IF PATH PROCESSING    @ZA17534*/ 00376000
         L     @11,ACBAMBL(,RACB)                                  0337 00377000
         TM    AMBLPATH(@11),B'10000000'                           0337 00378000
         BNO   @RF00337                                            0337 00379000
*     CLWPATH=ON;                   /* INDICATE PATH PROCESSING      */ 00380000
         OI    CLWPATH(RCORE),B'00010000'                          0338 00381000
*   CALL ENQFUNC;                   /* ISSUE ENQ BUSY FOR DATA SETS  */ 00382000
@RF00337 BAL   @14,ENQFUNC                                         0339 00383000
*   RESPECIFY                                                      0340 00384000
*     RET RESTRICTED;                                              0340 00385000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 00386000
         L     RET,MWASAVEA                                        0341 00387000
