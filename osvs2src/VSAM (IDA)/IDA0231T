         L     @14,UPTRPL-12(@14,@05)                              0563 01151000
         L     @14,RPLDACB(,@14)                                   0563 01152000
         LR    RACB,@14                                            0563 01153000
*     ACBBUSY=ON;                   /* TURN ON ACB BUSY BIT  @Z30SSRL*/ 01154000
         OI    ACBBUSY(RACB),B'00000001'                           0564 01155000
*     CALL IDA0231B;                /* TCLOSE UPGRADE DATA SETS      */ 01156000
         L     @15,@CV03834                                        0565 01157000
         BALR  @14,@15                                             0565 01158000
*     RSTATRTN=IDAR231T;            /* NOTE 231T IN CONTROL          */ 01159000
         L     @14,MWACORAD                                        0566 01160000
         MVC   RSTATRTN(4,@14),@CC03388                            0566 01161000
*     ACBBUSY=OFF;                  /* RESET ACB BUSY BIT    @Z30SSRL*/ 01162000
         NI    ACBBUSY(RACB),B'11111110'                           0567 01163000
*     RACB=MWARACB;                 /* RESTORE USER ACB              */ 01164000
         L     RACB,MWARACB                                        0568 01165000
*     IF RETCODE^=ZERO THEN         /* TERMINATING ERROR             */ 01166000
         LTR   RETCODE,RETCODE                                     0569 01167000
         BZ    @RF00569                                            0569 01168000
*       RETURN TO EXIT;             /* GO TO EXIT                    */ 01169000
         LA    @14,EXIT                                            0570 01170000
@EL00009 DS    0H                                                  0570 01171000
@EF00009 DS    0H                                                  0570 01172000
@ER00009 BR    @14                                                 0570 01173000
*   END;                                                           0571 01174000
@RF00569 LA    @14,1                                               0571 01175000
         AL    @14,MWAINDEX                                        0571 01176000
@DE00562 ST    @14,MWAINDEX                                        0571 01177000
         L     @05,ACBAMBL(,RACB)                                  0571 01178000
         L     @05,AMBLBIB(,@05)                                   0571 01179000
         L     @05,BIBUPT(,@05)                                    0571 01180000
         SLR   @11,@11                                             0571 01181000
         IC    @11,UPTNOENT(,@05)                                  0571 01182000
         CR    @14,@11                                             0571 01183000
         BNH   @DL00562                                            0571 01184000
*   PDPDSTYP=PDPDDNM;               /* RESET DATA SET TYPE TO DD NAME*/ 01185000
         L     @14,MWACORAD                                        0572 01186000
         MVI   PDPDSTYP+280(@14),X'01'                             0572 01187000
*   RESPECIFY                                                      0573 01188000
*     RET RESTRICTED;                                              0573 01189000
*   RET=MWASAVEA;                   /* GET RETURN ADDRESS            */ 01190000
         L     RET,MWASAVEA                                        0574 01191000
*   RESPECIFY                                                      0575 01192000
*    (RET,                                                         0575 01193000
*     RETCODE) UNRESTRICTED;                                       0575 01194000
*   END TCLSUPGR;                                                  0576 01195000
         B     @EL00009                                            0576 01196000
*GETCORE:                                                          0577 01197000
*                                                                  0577 01198000
*/********************************************************************/ 01199000
*/*                                                                  */ 01200000
*/*      01 FUNCTION - ISSUE GETMAIN TO OBTAIN CORE STORAGE          */ 01201000
*/*      01 INPUTS -   1. NUMBER OF BYTES REQUIRED IN MWABYTES       */ 01202000
*/*                    2. SUBPOOL NUMBER IN MWASPOOL                 */ 01203000
*/*      01 OUTPUTS -  1. CORE STORAGE ADDRESS IN MWADDRES           */ 01204000
*/*                                                                  */ 01205000
*/********************************************************************/ 01206000
*                                                                  0577 01207000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0577 01208000
GETCORE  DS    0H                                                  0578 01209000
*   RESPECIFY                                                      0578 01210000
*    (R0,                                                          0578 01211000
*     R1,                                                          0578 01212000
*     R6,                                                          0578 01213000
*     R7,                                                          0578 01214000
*     RET,                                                         0578 01215000
*     RETCODE) RESTRICTED;                                         0578 01216000
*   IF MWAFRKEY<=K70 THEN           /* IN SYSTEM KEY         @ZA16012*/ 01217000
         CLI   MWAFRKEY,X'70'                                      0579 01218000
         BH    @RF00579                                            0579 01219000
*     DO;                           /* YES                   @ZA16012*/ 01220000
*       R6=MWAFRKEY;                /* LOAD REG WITH SYS KEY @ZA16012*/ 01221000
         SLR   R6,R6                                               0581 01222000
         IC    R6,MWAFRKEY                                         0581 01223000
*       SPKA(R6);                   /* MODESET TO SYS KEY    @ZA16012*/ 01224000
         SPKA  0(R6)                                               0582 01225000
*     END;                          /*                       @ZA16012*/ 01226000
*   R6=MWASPOOL;                    /* SUBPOOL NUMBER                */ 01227000
@RF00579 SLR   R6,R6                                               0584 01228000
         IC    R6,MWASPOOL                                         0584 01229000
*   R7=MWABYTES;                    /* NUMBER OF BYTES REQUIRED      */ 01230000
         L     R7,MWABYTES-1                                       0585 01231000
         LA    R7,0(,R7)                                           0585 01232000
*   GEN(GETMAIN RC,LV=(R7),SP=(R6)) /* ISSUE GETMAIN                 */ 01233000
*   SETS(RETCODE);                  /* SETS R15 WITH RETURN CODE     */ 01234000
         GETMAIN RC,LV=(R7),SP=(R6)                                     01235000
*   GEN(MODESET EXTKEY=ZERO);       /* RETURN TO KEY ZERO    @ZA16012*/ 01236000
         MODESET EXTKEY=ZERO                                            01237000
*   MWAFRKEY=K00;                   /* FORCE KEY ZERO        @ZA16012*/ 01238000
         MVI   MWAFRKEY,X'00'                                      0588 01239000
*   IF RETCODE=ZERO THEN            /* GETMAIN SUCCESSFUL            */ 01240000
         LTR   RETCODE,RETCODE                                     0589 01241000
         BNZ   @RF00589                                            0589 01242000
*     DO;                                                          0590 01243000
*       MWADDRES=R1;                /* SAVE ADDRESS OF GOTTEN CORE   */ 01244000
         ST    R1,MWADDRES                                         0591 01245000
*       R6=MWADDRES;                /* ADDRESS OF GOTTEN CORE        */ 01246000
         L     @11,MWADDRES                                        0592 01247000
         LR    R6,@11                                              0592 01248000
*       R1=ZERO;                    /* ZERO PAD BYTE                 */ 01249000
         SLR   R1,R1                                               0593 01250000
*       GEN(MVCL R6,R0)                                            0594 01251000
*       NOSETS;                     /* ZERO GOTTEN CORE              */ 01252000
         MVCL R6,R0                                                     01253000
*     END;                                                         0595 01254000
*   ELSE                            /* IF GETMAIN FAILED             */ 01255000
*     DO;                                                          0596 01256000
         B     @RC00589                                            0596 01257000
@RF00589 DS    0H                                                  0597 01258000
*       ACBERFLG=OPERR136;          /* SET ACB ERROR FLAG            */ 01259000
         MVI   ACBERFLG(RACB),X'88'                                0597 01260000
*       R1=MWACORAD;                /* POINT TO COMMON WORK AREA     */ 01261000
         L     @05,MWACORAD                                        0598 01262000
         LR    R1,@05                                              0598 01263000
*       PDPCODE=PP20;               /* SET PROBLEM CODE              */ 01264000
         MVI   PDPCODE+280(@05),X'14'                              0599 01265000
*       RSTATRTN=IDAR192P;          /* NOTE CALL TO 192P             */ 01266000
         MVC   RSTATRTN(4,@05),@CC03370                            0600 01267000
*       CALL IDA0192P;              /* CALL PROBLEM DETERMINATION    */ 01268000
         L     @15,@CV03796                                        0601 01269000
         BALR  @14,@15                                             0601 01270000
*       RSTATRTN=IDAR231T;          /* NOTE 231T IN CONTROL          */ 01271000
         L     @05,MWACORAD                                        0602 01272000
         MVC   RSTATRTN(4,@05),@CC03388                            0602 01273000
*       RETURN TO EXIT;             /* EXIT MODULE                   */ 01274000
         LA    @14,EXIT                                            0603 01275000
@EL00010 DS    0H                                                  0603 01276000
@EF00010 DS    0H                                                  0603 01277000
@ER00010 BR    @14                                                 0603 01278000
*     END;                                                         0604 01279000
*   RESPECIFY                                                      0605 01280000
*    (R0,                                                          0605 01281000
*     R1,                                                          0605 01282000
*     R6,                                                          0605 01283000
*     R7,                                                          0605 01284000
*     RET,                                                         0605 01285000
*     RETCODE) UNRESTRICTED;                                       0605 01286000
*   END GETCORE;                                                   0606 01287000
         B     @EL00010                                            0606 01288000
*FREECORE:                                                         0607 01289000
*                                                                  0607 01290000
*/********************************************************************/ 01291000
*/*                                                                  */ 01292000
*/*      01 FUNCTION - FREE CORE STORAGE PREVIOUSLY OBTAINED         */ 01293000
*/*      01 INPUTS -   1. ADDRESS OF CORE TO BE FREED IN MWADDRES    */ 01294000
*/*                    2. NUMBER OF BYTES TO BE FREED IN MWABYTES    */ 01295000
*/*                    3. SUBPOOL NUMBER IN MWASPOOL                 */ 01296000
*/*      01 OUTPUTS -  1. CORE STORAGE FREED                         */ 01297000
*/*                                                                  */ 01298000
*/********************************************************************/ 01299000
*                                                                  0607 01300000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0607 01301000
FREECORE DS    0H                                                  0608 01302000
*   RESPECIFY                                                      0608 01303000
*    (R0,                                                          0608 01304000
*     R1,                                                          0608 01305000
*     RET) RESTRICTED;                                             0608 01306000
*   IF MWAFRKEY<=K70 THEN           /* IS MODESET REQUIRED   @ZA16012*/ 01307000
         CLI   MWAFRKEY,X'70'                                      0609 01308000
         BH    @RF00609                                            0609 01309000
*     DO;                           /* YES                   @ZA16012*/ 01310000
*       R1=MWAFRKEY;                /* LOAD KEY INTO REG     @ZA16012*/ 01311000
         SLR   R1,R1                                               0611 01312000
         IC    R1,MWAFRKEY                                         0611 01313000
*       SPKA(R1);                   /* MODESET TO SYSTEM KEY@ZA16012 */ 01314000
         SPKA  0(R1)                                               0612 01315000
*     END;                          /*                       @ZA16012*/ 01316000
*   LM(R0,R1,MWAMNPRM);             /* SUBPOOL, LENGTH, AND ADDRESS  */ 01317000
@RF00609 LM    R0,R1,MWAMNPRM                                      0614 01318000
*   GEN(FREEMAIN R,LV=(0),A=(1));   /* FREEMAIN                      */ 01319000
         FREEMAIN R,LV=(0),A=(1)                                        01320000
*   GEN(MODESET EXTKEY=ZERO);       /* FORCE KEY ZERO        @ZA16012*/ 01321000
         MODESET EXTKEY=ZERO                                            01322000
*   MWAFRKEY=K00;                   /* RESET FREEMAIN SWITCH@ZA16012 */ 01323000
         MVI   MWAFRKEY,X'00'                                      0617 01324000
*   RESPECIFY                                                      0618 01325000
*    (R0,                                                          0618 01326000
*     R1,                                                          0618 01327000
*     RET) UNRESTRICTED;                                           0618 01328000
*   END FREECORE;                                                  0619 01329000
@EL00011 DS    0H                                                  0619 01330000
@EF00011 DS    0H                                                  0619 01331000
@ER00011 BR    @14                                                 0619 01332000
@PB00011 DS    0H                                                  0619 01333000
*   END IDA0231T                                                   0620 01334000
