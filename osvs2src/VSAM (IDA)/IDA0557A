*                                                                  1890 05532000
*   R6=CTGPLSZ+(4*CTGFLSZ);         /* FIND LENGTH OF LISTS  @ZA17050*/ 05533000
         L     R6,CTGFLSZ                                          1890 05534000
         SLA   R6,2                                                1890 05535000
         AL    R6,CTGPLSZ                                          1890 05536000
*   R6=R6+(3*LENGTH(CTGFIELD));     /*                       @ZA17050*/ 05537000
         AL    R6,@CF00118                                         1891 05538000
*   CALL CATSETUP;                  /* INITIALIZE CPL.       @ZA13505*/ 05539000
         BAL   @14,CATSETUP                                        1892 05540000
*   CTGNOFLD=1;                     /* ONE CHAIN OF FIELD LISTS.     */ 05541000
