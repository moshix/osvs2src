*           GPR15F,                                                0505 00761000
*           GPR00F) RESTRICTED;                                    0505 00762000
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */ 00763000
         SLR   GPR01F,GPR01F                                       0506 00764000
*         GPR15F=0;                 /* RC-TYPE GETMAIN               */ 00765000
         SLR   GPR15F,GPR15F                                       0507 00766000
*         GPR00F=CATWSIZ;           /* LENGTH REQUESTED              */ 00767000
         LA    GPR00F,512                                          0508 00768000
*         GPR15F=GPR15F|((WKPOOL)*256);/* SP IN BYTE 2               */ 00769000
         O     GPR15F,@CF05636                                     0509 00770000
*         SVC(120);                 /* RC/RU-FORM OF GETMAIN         */ 00771000
         SVC   120                                                 0510 00772000
*         RESPECIFY                                                0511 00773000
*          (GPR01F,                                                0511 00774000
*           GPR15F,                                                0511 00775000
*           GPR00F) UNRESTRICTED;                                  0511 00776000
*       END;                        /* GETMAIN (RC) LV(CATWSIZ)    0512 00777000
*                                      SP(WKPOOL) Z30SSJH            */ 00778000
*       RFY                                                        0513 00779000
*        (R1,                                                      0513 00780000
*         R15) RSTD;                /* RESTRICT GETMAIN REGS @Z30SSJH*/ 00781000
*       IF R15^=0 THEN              /* GETMAIN ERROR.                */ 00782000
         LTR   R15,R15                                             0514 00783000
         BZ    @RF00514                                            0514 00784000
*         DO;                                                      0515 00785000
*           RFY                                                    0516 00786000
*             R15 UNRSTD;           /* DROP REG RESTRICTION  @Z30SSJH*/ 00787000
*           EOVPDCDE=PP20;          /* SET INTERNAL PD CODE  @Y30DSJH*/ 00788000
         MVI   EOVPDCDE(RWORK),X'14'                               0517 00789000
*           EOVERR=MNTERR;          /* SET EOV ERROR.                */ 00790000
         MVI   EOVERR(RWORK),X'04'                                 0518 00791000
*         END;                                                     0519 00792000
*       ELSE                        /* GETMAIN SUCCESSFUL.           */ 00793000
*         DO;                                                      0520 00794000
         B     @RC00514                                            0520 00795000
@RF00514 DS    0H                                                  0521 00796000
*           CATWPTR=R1;             /* SAVE CATALOG WORKAREA ADDRESS    00797000
*                                                            @Z30SSJH*/ 00798000
         ST    R1,CATWPTR(,RWORK)                                  0521 00799000
*           RFY                                                    0522 00800000
*             R1 UNRSTD;            /* DROP REG RESTRICTION  @Z30SSJH*/ 00801000
*           CATWSZ=CATWSIZ;         /* SAVE CATALOG WORKAREA SIZE.   */ 00802000
         MVC   CATWSZ(2,RWORK),@CH00475                            0523 00803000
*         END;                                                     0524 00804000
*     END;                                                         0525 00805000
@RC00514 DS    0H                                                  0526 00806000
*   RFY                                                            0526 00807000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 00808000
@RC00323 DS    0H                                                  0527 00809000
*   RET=EOVSAV1;                    /* RESTORE RETURN ADDRESS.       */ 00810000
         L     RET,EOVSAV1(,RWORK)                                 0527 00811000
*   RFY                                                            0528 00812000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 00813000
*   END EOVINIT;                                                   0529 00814000
@EL00002 DS    0H                                                  0529 00815000
@EF00002 DS    0H                                                  0529 00816000
@ER00002 BR    @14                                                 0529 00817000
*AMBCHK:                                                           0530 00818000
*                                                                  0530 00819000
*   /*****************************************************************/ 00820000
*   /*                                                               */ 00821000
*   /* AMBCHK - VALIDITY CHECK THE AMB PASSED IF THE CALLER IS NOT   */ 00822000
*   /* THE SYSTEM. IF THE RBPSW KEY IS 0-7 THEN THE CALLER IS SYSTEM */ 00823000
*   /* CODE, BYPASS THE VALIDITY TEST. IN ORDER TO BE VALID, THE     */ 00824000
*   /* BLOCK ADDRESS PASSED MUST BE CONTAINED IN EITHER 'AMBLDTA' OR */ 00825000
*   /* 'AMBLIX' OF A PRIMARY AMBL CHAINED OFF THE JSCB FOR THE ACTIVE*/ 00826000
*   /* TCB. A SIMILAR TEST IS MADE BY ABP(IGC121). IDA0192A (VSAM    */ 00827000
*   /* OPEN) DOES THE CHAINING OF THE ABOVE BLOCKS.                  */ 00828000
*   /*                                                               */ 00829000
*   /*****************************************************************/ 00830000
*                                                                  0530 00831000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @Y021139*/ 00832000
AMBCHK   DS    0H                                                  0531 00833000
*   RFY                                                            0531 00834000
*     RET RSTD;                     /* INFORM THE COMPILER   @Y021139*/ 00835000
*   EOVSAV2=RET;                    /* SAVE RETURN ADDRESS.  @Y021139*/ 00836000
         ST    RET,EOVSAV2(,RWORK)                                 0532 00837000
*   RFY                                                            0533 00838000
*     RET UNRSTD;                   /* RELEASE R14.          @Y021139*/ 00839000
*   RFY                                                            0534 00840000
*     R15 RSTD;                     /* RESTRICT REGISTER     @Y021139*/ 00841000
*   R15=DXUKEY;                     /* GET KEY OF EOV CALLER @Y021139*/ 00842000
         L     @10,EOVRCORE(,RWORK)                                0535 00843000
         SLR   R15,R15                                             0535 00844000
         IC    R15,DXUKEY(,@10)                                    0535 00845000
*   R15=R15&KEYMASK;                /* DROP LOW FOUR BITS    @Y021139*/ 00846000
         N     R15,@CF00089                                        0536 00847000
*   IF R15^<KEY8 THEN               /* IF CALLER NOT SYSTEM, @Y021139*/ 00848000
         C     R15,@CF00087                                        0537 00849000
         BL    @RF00537                                            0537 00850000
*     DO;                           /* VALIDITY CHECK AMB    @Y021139*/ 00851000
*       EOVLOOP1=OFF;               /* INITLOOP SW           @ZA16063*/ 00852000
         NI    EOVLOOP1(RWORK),B'01111111'                         0539 00853000
*       RFY                                                        0540 00854000
*         IDAAMBL BASED(EOVAMBL1);  /* INFORM COMPILER       @ZA16063*/ 00855000
*       RFY                                                        0541 00856000
*         IDAVAT BASED(I);          /* ADDRESSABILITY TO VAT @ZA16063*/ 00857000
*       I=JSCBSHR;                  /* ADDRESS OF 1ST VAT    @ZA16063*/ 00858000
         L     @14,PSATOLD                                         0542 00859000
         L     @14,TCBJSCB(,@14)                                   0542 00860000
         L     @14,JSCBSHR(,@14)                                   0542 00861000
         ST    @14,I(,RWORK)                                       0542 00862000
*       DO WHILE I^=0&EOVLOOP1=OFF; /* CHK ALL VATS          @ZA16063*/ 00863000
         B     @DE00543                                            0543 00864000
@DL00543 DS    0H                                                  0544 00865000
*         DO K=1 TO VATENO WHILE EOVLOOP1=OFF;/* CHECK ALL ENTRIES IN   00866000
*                                      EACH VAT              @ZA16063*/ 00867000
         LA    @14,1                                               0544 00868000
         B     @DE00544                                            0544 00869000
@DL00544 TM    EOVLOOP1(RWORK),B'10000000'                         0544 00870000
         BNZ   @DC00544                                            0544 00871000
*           EOVAMBL1=VATAMBL(K);    /* AMBL BASE             @ZA16063*/ 00872000
         L     @14,K(,RWORK)                                       0545 00873000
         SLA   @14,2                                               0545 00874000
         L     @10,I(,RWORK)                                       0545 00875000
         L     @14,VATAMBL-4(@14,@10)                              0545 00876000
         ST    @14,EOVAMBL1(,RWORK)                                0545 00877000
*           IF EOVAMBL1^=0&         /* ENTRY NOT ZERO        @ZA16063*/ 00878000
*               (AMBLDTA=EOVAMBPT|  /* DATA AMB MATCHES      @ZA16063*/ 00879000
*               AMBLIX=EOVAMBPT) THEN/* INDEX AMB MATCHES    @ZA16063*/ 00880000
         LTR   @14,@14                                             0546 00881000
         BZ    @RF00546                                            0546 00882000
         L     @10,EOVAMBPT(,RWORK)                                0546 00883000
         C     @10,AMBLDTA(,@14)                                   0546 00884000
         BE    @RT00546                                            0546 00885000
         C     @10,AMBLIX(,@14)                                    0546 00886000
         BNE   @RF00546                                            0546 00887000
@RT00546 DS    0H                                                  0547 00888000
*             EOVLOOP1=ON;          /* SET EXIT FLAG         @ZA16063*/ 00889000
         OI    EOVLOOP1(RWORK),B'10000000'                         0547 00890000
*         END;                                                     0548 00891000
@RF00546 LA    @14,1                                               0548 00892000
         AL    @14,K(,RWORK)                                       0548 00893000
@DE00544 ST    @14,K(,RWORK)                                       0548 00894000
         L     @10,I(,RWORK)                                       0548 00895000
         SLR   @07,@07                                             0548 00896000
         IC    @07,VATENO(,@10)                                    0548 00897000
         CR    @14,@07                                             0548 00898000
         BNH   @DL00544                                            0548 00899000
@DC00544 DS    0H                                                  0549 00900000
*         I=VATNEXT;                /* NEXT VAT              @ZA16063*/ 00901000
         L     @14,I(,RWORK)                                       0549 00902000
         L     @14,VATNEXT(,@14)                                   0549 00903000
         ST    @14,I(,RWORK)                                       0549 00904000
*       END;                        /*                       @ZA16063*/ 00905000
@DE00543 L     @14,I(,RWORK)                                       0550 00906000
         LTR   @14,@14                                             0550 00907000
         BZ    @DC00543                                            0550 00908000
         TM    EOVLOOP1(RWORK),B'10000000'                         0550 00909000
         BZ    @DL00543                                            0550 00910000
@DC00543 DS    0H                                                  0551 00911000
*       IF EOVLOOP1=OFF THEN        /* OFF= NOT FOUND        @ZA16063*/ 00912000
         TM    EOVLOOP1(RWORK),B'10000000'                         0551 00913000
         BNZ   @RF00551                                            0551 00914000
*         R15=NZERO;                /* ERROR CODE            @ZA16063*/ 00915000
         LA    R15,4                                               0552 00916000
*       ELSE                        /* ELSE FOUND            @ZA16063*/ 00917000
*         R15=0;                    /* RETURN CODE 0         @ZA16063*/ 00918000
         B     @RC00551                                            0553 00919000
@RF00551 SLR   R15,R15                                             0553 00920000
*       RFY                                                        0554 00921000
*         IDAVAT BASED(JSCBSHR);    /* RESET TO 1ST VAT      @ZA16063*/ 00922000
@RC00551 DS    0H                                                  0555 00923000
*     END;                          /* END OF CHECK IF NOT SYSTEM  0555 00924000
*                                      CALLER.               @Y021139*/ 00925000
*   ELSE                            /* CALLER IS THE SYSTEM  @Z30SSJH*/ 00926000
*     R15=ZERO;                     /* IND CHK IS A SUCCESS  @Z30SSJH*/ 00927000
         B     @RC00537                                            0556 00928000
@RF00537 SLR   R15,R15                                             0556 00929000
*   RFY                                                            0557 00930000
*     RET RSTD;                     /* RESERVE RET REGISTER. @Y021139*/ 00931000
@RC00537 DS    0H                                                  0558 00932000
*   RET=EOVSAV2;                    /* RESTORE RET REGISTER. @Y021139*/ 00933000
         L     RET,EOVSAV2(,RWORK)                                 0558 00934000
*   RFY                                                            0559 00935000
*     RET UNRSTD;                   /* RELEASE RET REGISTER. @Y021139*/ 00936000
*   RETURN CODE(R15);               /* RETURN TO CALLER      @Y021139*/ 00937000
@EL00003 DS    0H                                                  0560 00938000
@EF00003 DS    0H                                                  0560 00939000
@ER00003 BR    @14                                                 0560 00940000
*   RFY                                                            0561 00941000
*     IDAAMBL BASED(AMBPAMBL);      /* RESET THE COMPILER.   @Y021139*/ 00942000
*   RFY                                                            0562 00943000
*    (R15,                                                         0562 00944000
*     RBPTR) UNRSTD;                /* DROP REG RESTRICTIONS @Y021139*/ 00945000
*   END AMBCHK;                     /* END OF PROC AMBCHK.   @Y021139*/ 00946000
*LOCKUP:                                                           0564 00947000
*                                                                  0564 00948000
*   /*****************************************************************/ 00949000
*   /*                                                               */ 00950000
*   /* LOCKUP IOMB'S - THIS ROUTINE LOCKS ALL DATA & INDEX IOMB'S OF */ 00951000
*   /* THE CLUSTER BY SETTING IOMLOCK. IF ALL IOMB'S CAN'T BE LOCKED,*/ 00952000
*   /* THEN THE IOMB'S THAT WERE LOCKED BY EOV ARE UNLOCKED. THE     */ 00953000
*   /* FAILING IOMB IS PASSED TO RECORD MANAGEMENT IN AMBEOVPT.      */ 00954000
*   /*                                                               */ 00955000
*   /*****************************************************************/ 00956000
*                                                                  0564 00957000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0564 00958000
LOCKUP   DS    0H                                                  0565 00959000
*   RFY                                                            0565 00960000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 00961000
*   EOVSAV2=RET;                    /* SAVE RETURN ADDRESS.          */ 00962000
         ST    RET,EOVSAV2(,RWORK)                                 0566 00963000
*   RFY                                                            0567 00964000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 00965000
*   R6=ADDR(AMBLCOMP);              /* POINT TO EOV LOCK.            */ 00966000
         L     @10,EOVAMBPT(,RWORK)                                0568 00967000
         L     R6,AMBPAMBL(,@10)                                   0568 00968000
         LA    R6,AMBLCOMP(,R6)                                    0568 00969000
*   GEN;                                                           0569 00970000
         TS    0(R6)                   TEST AND SET AMBL EOV LOCK       00971000
         BC    8,TSOK                  EOV LOCK WAS 0                   00972000
         LA    R5,512                  NON-ZERO IN REGISTER 5           00973000
         B     TSEND                   BR TO CHECK TEST AND SET         00974000
TSOK     SR    R5,R5                   ZERO REGISTER 5                  00975000
TSEND    EQU   *                                                        00976000
*   IF R5^=0 THEN                   /* AMBL EOV LOCK WAS ON.         */ 00977000
         LTR   R5,R5                                               0570 00978000
         BZ    @RF00570                                            0570 00979000
*     DO;                                                          0571 00980000
*       EOVERR=REDRVERR;            /* SET EOV ERROR-REDRIVE REQ.    */ 00981000
         MVI   EOVERR(RWORK),X'0C'                                 0572 00982000
*     END;                                                         0573 00983000
*   ELSE                            /* AMBL EOV LOCK WAS OFF.        */ 00984000
*     DO;                                                          0574 00985000
         B     @RC00570                                            0574 00986000
@RF00570 DS    0H                                                  0575 00987000
*       CALL GETLOCK;               /* OBTAIN THE LOCAL LOCK @Z30SSJH*/ 00988000
