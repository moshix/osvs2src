         BNL   *+6                                                      01897000
         LR    @07,@10                                             0828 01898000
         ST    @07,EOVHIRBA(,RWORK)                                0828 01899000
*     IF RBAREQ=ON THEN             /* RBA REQUEST.                  */ 01900000
*                                                                  0829 01901000
         TM    RBAREQ(RWORK),B'10000000'                           0829 01902000
         BNO   @RF00829                                            0829 01903000
*       /*************************************************************/ 01904000
*       /*                                                           */ 01905000
*       /* TEST IF REQUESTED RBA IS EQUAL TO HI ALLOCATED RBA.       */ 01906000
*       /*                                                           */ 01907000
*       /*************************************************************/ 01908000
*                                                                  0830 01909000
*       IF RBA=ARDERBA THEN         /* RBA'S ARE EQUAL.              */ 01910000
         L     @07,EOVEOVPT(,RWORK)                                0830 01911000
         ICM   @15,15,RBA(@07)                                     0830 01912000
         CLR   @10,@15                                             0830 01913000
         BNE   @RF00830                                            0830 01914000
*         EOVARDB1=RARDB;           /* SAVE ARDB ADDR. IN WORKAREA.  */ 01915000
         ST    RARDB,EOVARDB1(,RWORK)                              0831 01916000
*       ELSE                                                       0832 01917000
*         ;                         /* RBA'S NOT EQUAL.              */ 01918000
@RF00830 DS    0H                                                  0833 01919000
*     ELSE                          /* KEY REQUEST.                  */ 01920000
*       DO;                                                        0833 01921000
*                                                                  0833 01922000
         B     @RC00829                                            0833 01923000
@RF00829 DS    0H                                                  0834 01924000
*         /***********************************************************/ 01925000
*         /*                                                         */ 01926000
*         /* TEST IF REQUESTED KEY IS GTE ARDB LOW KEY.              */ 01927000
*         /*                                                         */ 01928000
*         /***********************************************************/ 01929000
*                                                                  0834 01930000
*         IF INKEY(1:EOVKEYLN)=>ARDKEYS(1:EOVKEYLN) THEN/* REQUESTED    01931000
*                                      KEY IS GTE.           @Z30SSJH*/ 01932000
         L     @10,EOVEOVPT(,RWORK)                                0834 01933000
         LH    @07,EOVKEYLN(,RWORK)                                0834 01934000
