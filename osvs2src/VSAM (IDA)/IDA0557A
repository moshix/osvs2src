*       EOVNMEXT=EOVNMEXT-1;        /* DECREMENT CURRENT NUMBER OF 1411 03858000
*                                      EXTENTS.                      */ 03859000
         SLR   @10,@10                                             1411 03860000
         IC    @10,EOVNMEXT(,RWORK)                                1411 03861000
         BCTR  @10,0                                               1411 03862000
         STC   @10,EOVNMEXT(,RWORK)                                1411 03863000
*     RDEBXT=RDEBXT+LENGTH(DEBDASD);/* POINT TO NEXT EXTENT.         */ 03864000
@RF01410 AL    RDEBXT,@CF00121                                     1412 03865000
*   END;                                                           1413 03866000
*                                                                  1413 03867000
         LA    @10,1                                               1413 03868000
