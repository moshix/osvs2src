*                                                                  1023 02599000
*   IF EOVNOSCN=ON THEN             /* IF SECONDARY EXTENTS NOT    1023 02600000
*                                      ALLOWED               @ZA17050*/ 02601000
@RC01013 TM    EOVNOSCN(RWORK),B'00010000'                         1023 02602000
         BNO   @RF01023                                            1023 02603000
*     CATRC=ERRNOSPC;               /* SIMULATE NO SPACE     @ZA17050*/ 02604000
         MVC   CATRC(2,RWORK),@CH00331                             1024 02605000
*   ELSE                            /*                       @ZA17050*/ 02606000
*                                                                  1025 02607000
*     /***************************************************************/ 02608000
*     /*                                                             */ 02609000
*     /* CATALOG UPDATE TO ALLOCATE SPACE.                           */ 02610000
*     /*                                                             */ 02611000
*     /***************************************************************/ 02612000
*                                                                  1025 02613000
*     CALL CATALC;                                                 1025 02614000
*                                                                  1025 02615000
         B     @RC01023                                            1025 02616000
@RF01023 BAL   @14,CATALC                                          1025 02617000
*   /*****************************************************************/ 02618000
*   /*                                                               */ 02619000
*   /*                                                               */ 02620000
*   /*****************************************************************/ 02621000
*                                                                  1026 02622000
*   IF CATRC^=0 THEN                /* CATALOG ERROR.                */ 02623000
@RC01023 LH    @10,CATRC(,RWORK)                                   1026 02624000
         N     @10,@CF05661                                        1026 02625000
         LTR   @10,@10                                             1026 02626000
         BZ    @RF01026                                            1026 02627000
*     DO;                                                          1027 02628000
*                                                                  1027 02629000
*       /*************************************************************/ 02630000
*       /*                                                           */ 02631000
*       /* TEST IF VOLUME OUT OF SPACE.                              */ 02632000
*       /*                                                           */ 02633000
*       /*************************************************************/ 02634000
*                                                                  1028 02635000
*       IF CATRC=ERRNOSPC THEN      /* VOLUME OUT OF SPACE.          */ 02636000
         C     @10,@CF00331                                        1028 02637000
         BNE   @RF01028                                            1028 02638000
*         DO;                                                      1029 02639000
*           CALL VOLSW;             /* CALL VOLUME SWITCH ROUTINE.   */ 02640000
         BAL   @14,VOLSW                                           1030 02641000
*           EOVFUNC=PDF203;         /* SET FUNCTION CODE - ALLOCATE     02642000
*                                      SPACE                 @Y30DSJH*/ 02643000
         MVI   EOVFUNC(RWORK),X'CB'                                1031 02644000
*         END;                                                     1032 02645000
*       ELSE                        /* CATALOG ERROR.                */ 02646000
*         DO;                                                      1033 02647000
         B     @RC01028                                            1033 02648000
@RF01028 DS    0H                                                  1034 02649000
*           CALL CATERR;            /* SET PROBLEM CODE.             */ 02650000
         BAL   @14,CATERR                                          1034 02651000
*           EOVERR=ALCERR;          /* SET UNABLE TO ALLOCATE ERROR. */ 02652000
         MVI   EOVERR(RWORK),X'08'                                 1035 02653000
*         END;                                                     1036 02654000
*     END;                                                         1037 02655000
*   ELSE                                                           1038 02656000
*     ;                             /* NO CATALOG ERROR.             */ 02657000
@RF01026 DS    0H                                                  1039 02658000
*   RFY                                                            1039 02659000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 02660000
@RC01026 DS    0H                                                  1040 02661000
*   RET=EOVSAV1;                    /* RESTORE RETURN ADDRESS.       */ 02662000
         L     RET,EOVSAV1(,RWORK)                                 1040 02663000
*   RFY                                                            1041 02664000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 02665000
*   END ALLOCSPC;                                                  1042 02666000
@EL00015 DS    0H                                                  1042 02667000
@EF00015 DS    0H                                                  1042 02668000
@ER00015 BR    @14                                                 1042 02669000
*VOLSW:                                                            1043 02670000
*                                                                  1043 02671000
*   /*****************************************************************/ 02672000
*   /*                                                               */ 02673000
*   /* VOLUME SWITCH - SWITCHES TO OVERFLOW OR CANDIDATE VOLUME AND  */ 02674000
*   /* ALLOCATES SPACE.                                              */ 02675000
*   /*                                                               */ 02676000
*   /*****************************************************************/ 02677000
*                                                                  1043 02678000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1043 02679000
VOLSW    DS    0H                                                  1044 02680000
*   RFY                                                            1044 02681000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 02682000
*   EOVSAV2=RET;                    /* SAVE RETURN ADDRESS.          */ 02683000
         ST    RET,EOVSAV2(,RWORK)                                 1045 02684000
*   RFY                                                            1046 02685000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 02686000
*   EOVFUNC=PDF204;                 /* SET FUNCT CD - VOL SW @Y30DSJH*/ 02687000
         MVI   EOVFUNC(RWORK),X'CC'                                1047 02688000
*   EOVVOLSW=ON;                    /* SET VOLUME SWITCH INDICATION. */ 02689000
*   EOVCAND=OFF;                    /* TURN OFF CANDIDATE VOLUME   1049 02690000
*                                      SWITCH.                       */ 02691000
         OI    EOVVOLSW(RWORK),B'00100000'                         1049 02692000
         NI    EOVCAND(RWORK),B'10111111'                          1049 02693000
*   RARDB=EOVARDB1;                 /* BASE ARDB.                    */ 02694000
         L     RARDB,EOVARDB1(,RWORK)                              1050 02695000
*   EOVRRNO1='0000'X;               /* ZERO REL REP NO FOR CATLOCNC.    02696000
*                                                            @XM01563*/ 02697000
*                                                                  1051 02698000
         MVC   EOVRRNO1(2,RWORK),@CB05490                          1051 02699000
*   /*****************************************************************/ 02700000
*   /*                                                               */ 02701000
*   /* TEST IF ARDB INDICATES KEY RANGE.                             */ 02702000
*   /*                                                               */ 02703000
*   /*****************************************************************/ 02704000
*                                                                  1052 02705000
*   IF ARDKR=ON THEN                /* KEY RANGE.                    */ 02706000
         TM    ARDKR(RARDB),B'10000000'                            1052 02707000
         BNO   @RF01052                                            1052 02708000
*     DO;                                                          1053 02709000
*                                                                  1053 02710000
*       /*************************************************************/ 02711000
*       /*                                                           */ 02712000
*       /* SEARCH ARDB'S FOR HIGHEST OVERFLOW VOLUME (HIGH-ALLOCATED */ 02713000
*       /* RBA).                                                     */ 02714000
*       /*                                                           */ 02715000
*       /*************************************************************/ 02716000
*                                                                  1054 02717000
*       RARDB=AMDPARDB;             /* POINT TO FIRST ARDB.  @XM01563*/ 02718000
         L     @10,EOVAMBPT(,RWORK)                                1054 02719000
         L     @10,AMBDSB(,@10)                                    1054 02720000
         L     RARDB,AMDPARDB(,@10)                                1054 02721000
*       EOVRBAWK=0;                 /* ZERO RBA SAVE FIELD.  @XM01563*/ 02722000
         SLR   @10,@10                                             1055 02723000
         ST    @10,EOVRBAWK(,RWORK)                                1055 02724000
*       DO WHILE RARDB^=0;          /* LOOP THROUGH ARDB'S.  @XM01563*/ 02725000
         B     @DE01056                                            1056 02726000
@DL01056 DS    0H                                                  1057 02727000
*         IF ARDUOVFL=ON THEN       /* KEY RANGE USING OVERFLOW.   1057 02728000
*                                                            @XM01563*/ 02729000
         TM    ARDUOVFL(RARDB),B'00010000'                         1057 02730000
         BNO   @RF01057                                            1057 02731000
*           DO;                     /*                       @XM01563*/ 02732000
*             EOVOVFSW=ON;          /* SET OVERFLOW VOLUME EXISTS  1059 02733000
*                                      SWITCH.               @XM01563*/ 02734000
         OI    EOVOVFSW(RWORK),B'10000000'                         1059 02735000
*             IF ARDERBA>EOVRBAWK THEN/* THIS OVERFLOW VOLUME HAS A     02736000
*                                      HIGHER HI-ALLOCATED RBA.    1060 02737000
*                                                            @XM01563*/ 02738000
         L     @10,ARDERBA(,RARDB)                                 1060 02739000
         CL    @10,EOVRBAWK(,RWORK)                                1060 02740000
         BNH   @RF01060                                            1060 02741000
*               DO;                 /*                       @XM01563*/ 02742000
*                 EOVRBAWK=ARDERBA; /* SAVE ARDB HI-ALLOCATED RBA. 1062 02743000
*                                                            @XM01563*/ 02744000
         ST    @10,EOVRBAWK(,RWORK)                                1062 02745000
*                 EOVVOLWK=ARDVOLSR;/* SAVE ARDB VOLUME SERIAL     1063 02746000
*                                      NUMBER.               @XM01563*/ 02747000
         MVC   EOVVOLWK(6,RWORK),ARDVOLSR(RARDB)                   1063 02748000
*               END;                /*                       @XM01563*/ 02749000
*           END;                    /*                       @XM01563*/ 02750000
@RF01060 DS    0H                                                  1066 02751000
*         RARDB=ARDNPTR;            /* POINT TO NEXT ARDB.   @XM01563*/ 02752000
@RF01057 L     RARDB,ARDNPTR(,RARDB)                               1066 02753000
*       END;                        /*                       @XM01563*/ 02754000
@DE01056 LTR   RARDB,RARDB                                         1067 02755000
         BNZ   @DL01056                                            1067 02756000
*       IF EOVOVFSW=ON THEN         /* AN OVERFLOW VOLUME EXISTS.  1068 02757000
*                                                            @XM01563*/ 02758000
         TM    EOVOVFSW(RWORK),B'10000000'                         1068 02759000
         BNO   @RF01068                                            1068 02760000
*         CATVOLS=EOVVOLWK;         /* USE VOLUME SERIAL OF HIGHEST     02761000
*                                      OVERFLOW VOLUME.      @XM01563*/ 02762000
         MVC   CATVOLS(6,RWORK),EOVVOLWK(RWORK)                    1069 02763000
*       ELSE                        /* NO OVERFLOW VOLUME EXISTS.  1070 02764000
*                                                            @XM01563*/ 02765000
*         CALL CATLOCNC;            /* CALL LOCATE NEXT CANDIDATE. 1070 02766000
*                                                            @XM01563*/ 02767000
