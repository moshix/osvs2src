         BNE   @RF01387                                            1387 03768000
*                     DO;                                          1388 03769000
*                       EOVARDB2->ARDEOD=OFF;/* TURN OFF END OF DATA    03770000
*                                      IN OLD ARDB.                  */ 03771000
         L     @10,EOVARDB2(,RWORK)                                1389 03772000
         NI    ARDEOD(@10),B'11110111'                             1389 03773000
*                       EOVARDB1->ARDEOD=ON;/* TURN ON END OF DATA IN   03774000
*                                      CURRENT ARDB.                 */ 03775000
         L     @07,EOVARDB1(,RWORK)                                1390 03776000
         OI    ARDEOD(@07),B'00001000'                             1390 03777000
*                       EOVARDB1->ARDHRBA=EOVARDB2->ARDERBA;/* SET 1391 03778000
*                                      HI-USED RBA TO HI-ALLOCATED 1391 03779000
*                                      RBA OF EOD ARDB.              */ 03780000
