         TM    AMBTYPE(@10),B'01000000'                            0629 01219000
         BNO   @RF00629                                            0629 01220000
*         EOVAMBPT=AMBLDTA;         /* YES, SET AMB PTR TO DATA AMB  */ 01221000
         L     @10,AMBPAMBL(,@10)                                  0630 01222000
         L     @10,AMBLDTA(,@10)                                   0630 01223000
         ST    @10,EOVAMBPT(,RWORK)                                0630 01224000
*       ELSE                        /* NO, THIS IS A DATA AMB        */ 01225000
*         EOVAMBPT=AMBLIX;          /* SET AMB PTR TO INDEX AMB      */ 01226000
         B     @RC00629                                            0631 01227000
