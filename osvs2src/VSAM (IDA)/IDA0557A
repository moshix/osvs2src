*             /*                                                     */ 02814000
*             /*******************************************************/ 02815000
*                                                                  1083 02816000
*             CALL CATALC;                                         1083 02817000
         BAL   @14,CATALC                                          1083 02818000
*             IF EOVCAND=ON THEN    /* CANDIDATE VOLUME.     @XM01563*/ 02819000
         TM    EOVCAND(RWORK),B'01000000'                          1084 02820000
         BNO   @RF01084                                            1084 02821000
*               EOVRRNO1=CTGWKA->CATRRNO1;/* SAVE REL REP NO FOR   1085 02822000
