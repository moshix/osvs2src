         L     @10,EOVAMBPT(,RWORK)                                0605 01126000
         L     @07,AMBPAMBL(,@10)                                  0605 01127000
         OI    AMBLWAIT(@07),B'10000000'                           0605 01128000
*           AMBLCOMP='00'X;         /* CLEAR AMBL EOV LOCK.          */ 01129000
         MVI   AMBLCOMP(@07),X'00'                                 0606 01130000
*           EOVERR=IOBLKERR;        /* SET EOV ERROR.                */ 01131000
         MVI   EOVERR(RWORK),X'0C'                                 0607 01132000
*           AMBXEVPT=EOVIOBF;       /* PASS BACK FAILING IOMB TO RCD    01133000
*                                      MGMT.                 @Y021139*/ 01134000
         L     @10,AMBEOVPT(,@10)                                  0608 01135000
         L     @07,EOVIOBF(,RWORK)                                 0608 01136000
         ST    @07,AMBXEVPT(,@10)                                  0608 01137000
*         END;                                                     0609 01138000
         LA    @10,1                                               0609 01139000
         AL    @10,K(,RWORK)                                       0609 01140000
         ST    @10,K(,RWORK)                                       0609 01141000
         C     @10,@CF00126                                        0609 01142000
         BNH   @DL00591                                            0609 01143000
@DC00591 DS    0H                                                  0610 01144000
*       CALL FREELOCK;              /* FREE THE LOCAL LOCK   @Z30SSJH*/ 01145000
@RF00590 BAL   @14,FREELOCK                                        0610 01146000
*     END;                                                         0611 01147000
*   RFY                                                            0612 01148000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 01149000
@RC00570 DS    0H                                                  0613 01150000
*   RET=EOVSAV2;                    /* RESTORE RETURN ADDRESS.       */ 01151000
         L     RET,EOVSAV2(,RWORK)                                 0613 01152000
*   RFY                                                            0614 01153000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 01154000
*   END LOCKUP;                                                    0615 01155000
@EL00004 DS    0H                                                  0615 01156000
@EF00004 DS    0H                                                  0615 01157000
@ER00004 BR    @14                                                 0615 01158000
*CHKPTRTN:                                                         0616 01159000
*                                                                  0616 01160000
*   /*****************************************************************/ 01161000
*   /*                                                               */ 01162000
*   /* EOV CHECKPOINT - THIS ROUTINE RECORDS THE STATUS OF THE VSAM  */ 01163000
*   /* CONTROL BLOCKS THAT CAN BE MODIFIED BY EOV AT ENTRY TO EOV. IF*/ 01164000
*   /* AN ERROR IS DETECTED DURING EOV PROCESSING, THE INFORMATION   */ 01165000
*   /* RECORDED BY THIS ROUTINE IS USED TO RESET THE VSAM CONTROL    */ 01166000
*   /* BLOCKS TO THE WAY THEY WERE AT ENTRY TO EOV.                  */ 01167000
*   /*                                                               */ 01168000
*   /*****************************************************************/ 01169000
*                                                                  0616 01170000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0616 01171000
CHKPTRTN DS    0H                                                  0617 01172000
*   RFY                                                            0617 01173000
*     TIOENTRY BASED(R5);           /*                       @YM04608*/ 01174000
*   RFY                                                            0618 01175000
*     UCBOB BASED(TIOEFSRT);                                       0618 01176000
*   RFY                                                            0619 01177000
*     IDAVVOLL BASED(ADDR(CHKDATA));                               0619 01178000
*   RFY                                                            0620 01179000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 01180000
*   EOVSAV2=RET;                    /* SAVE RETURN ADDRESS           */ 01181000
         ST    RET,EOVSAV2(,RWORK)                                 0621 01182000
*   RFY                                                            0622 01183000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 01184000
*                                                                  0622 01185000
*   /*****************************************************************/ 01186000
*   /*                                                               */ 01187000
*   /* COUNT THE NUMBER OF VOLUMES MOUNTED AND VERIFIED AND THE      */ 01188000
*   /* NUMBER OF ARDB'S THAT EXIST FOR THE DATA SET. IF SEQUENCE SET */ 01189000
*   /* WITH DATA, THE NUMBER OF ARDB'S FOR BOTH DATA AND INDEX ARE   */ 01190000
*   /* COUNTED.                                                      */ 01191000
*   /*                                                               */ 01192000
*   /*****************************************************************/ 01193000
*                                                                  0623 01194000
*   R5=DXTIOTAD;                    /* SET PTR TO TIOT ENTRY @Z30SSJH*/ 01195000
         L     @10,EOVRCORE(,RWORK)                                0623 01196000
         L     R5,DXTIOTAD(,@10)                                   0623 01197000
*   EOVNVOL1=(TIOELNGH-16)/4;       /* CALCULATE NUMBER OF UCB PTR'S    01198000
*                                      IN TIOT ENTRY                 */ 01199000
         SLR   @00,@00                                             0624 01200000
         IC    @00,TIOELNGH(,R5)                                   0624 01201000
         SL    @00,@CF00121                                        0624 01202000
         SRDA  @00,32                                              0624 01203000
         D     @00,@CF00079                                        0624 01204000
         STH   @01,EOVNVOL1(,RWORK)                                0624 01205000
*   CALL ARDBCNT;                   /* COUNT NUMBER OF ARDBS         */ 01206000
         BAL   @14,ARDBCNT                                         0625 01207000
*   EOVNARD1=EOVCPWA2;              /* SAVE NUMBER OF ARDB'S         */ 01208000
