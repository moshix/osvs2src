@RF01068 BAL   @14,CATLOCNC                                        1070 02769000
*       RARDB=EOVARDB1;             /* BASE ARDB.            @XM01563*/ 02770000
@RC01068 L     RARDB,EOVARDB1(,RWORK)                              1071 02771000
*       ARDUOVFL=ON;                /* SET KEY RANGE USING OVERFLOW.    02772000
*                                                            @XM01563*/ 02773000
         OI    ARDUOVFL(RARDB),B'00010000'                         1072 02774000
*     END;                          /*                       @XM01563*/ 02775000
*   ELSE                            /* NOT KEY RANGE.                */ 02776000
*     CALL CATLOCNC;                /* CATALOG LOCATE NEXT CANDIDATE    02777000
*                                      VOLUME.                       */ 02778000
         B     @RC01052                                            1074 02779000
@RF01052 BAL   @14,CATLOCNC                                        1074 02780000
*   IF EOVERR=0 THEN                /* NO EOV ERROR.                 */ 02781000
@RC01052 CLI   EOVERR(RWORK),0                                     1075 02782000
         BNE   @RF01075                                            1075 02783000
*     DO;                                                          1076 02784000
*                                                                  1076 02785000
*       /*************************************************************/ 02786000
*       /*                                                           */ 02787000
*       /* LOOP FOR VOLUME SWITCHING.                                */ 02788000
*       /*                                                           */ 02789000
*       /*************************************************************/ 02790000
*                                                                  1077 02791000
*       EOVDOSW=OFF;                /* TURN OFF DO SWITCH.           */ 02792000
         NI    EOVDOSW(RWORK),B'11101111'                          1077 02793000
