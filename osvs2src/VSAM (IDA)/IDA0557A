         NI    AMBLWAIT(@10),B'01111111'                           0576 00993000
*       EOVIOBF=0;                  /* CLEAR FAILNG IOMB POINTER     */ 00994000
         SLR   @10,@10                                             0577 00995000
         ST    @10,EOVIOBF(,RWORK)                                 0577 00996000
*       DO K=1 TO 2 WHILE EOVPLHDR(K)^=0;/* CHK ALL PLH'S    @ZA07547*/ 00997000
         LA    @10,1                                               0578 00998000
         ST    @10,K(,RWORK)                                       0578 00999000
@DL00578 L     @10,K(,RWORK)                                       0578 01000000
         SLA   @10,2                                               0578 01001000
         L     @10,EOVPLHDR-4(@10,RWORK)                           0578 01002000
         LTR   @10,@10                                             0578 01003000
         BZ    @DC00578                                            0578 01004000
*         EOVPLHPT=ADDR(IDAPLHDR)+LENGTH(IDAPLHDR);/* POINT TO FIRST    01005000
*                                      PLH ENTRY.                    */ 01006000
         AL    @10,@CF00121                                        0579 01007000
         ST    @10,EOVPLHPT(,RWORK)                                0579 01008000
*         DO I=1 TO PLHCNT WHILE EOVIOBF=0;/* SCAN PLH ENTRIES.      */ 01009000
         LA    @10,1                                               0580 01010000
         B     @DE00580                                            0580 01011000
@DL00580 SLR   @10,@10                                             0580 01012000
         C     @10,EOVIOBF(,RWORK)                                 0580 01013000
         BNE   @DC00580                                            0580 01014000
*           EOVIOBPT=PLHDIOB;       /* POINT TO DATA IOMB            */ 01015000
         L     @07,EOVPLHPT(,RWORK)                                0581 01016000
         L     @07,PLHDIOB(,@07)                                   0581 01017000
         ST    @07,EOVIOBPT(,RWORK)                                0581 01018000
*           IF IOMAMB^=0&(IOMAMB=AMBLDTA|IOMAMB=AMBLIX) THEN/*     0582 01019000
*                                                            @Y30DSJH*/ 01020000
         L     @07,IOMAMB(,@07)                                    0582 01021000
