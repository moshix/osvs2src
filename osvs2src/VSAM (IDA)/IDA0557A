*   RFY                                                            1428 03950000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 03951000
*   EOVSAV3=RET;                    /* SAVE RETURN ADDRESS.          */ 03952000
         ST    RET,EOVSAV3(,RWORK)                                 1429 03953000
*   RFY                                                            1430 03954000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 03955000
*                                                                  1430 03956000
*   /*****************************************************************/ 03957000
*   /*                                                               */ 03958000
*   /* FIND SIZE OF NEW EDB AND GETMAIN FOR IT.                      */ 03959000
*   /*                                                               */ 03960000
*   /*****************************************************************/ 03961000
*                                                                  1431 03962000
*   R5=LENGTH(IDAEDBHD)+(LENGTH(IDAEDB)*(EOVNMEXT+CATNMEXT));      1431 03963000
         SLR   R5,R5                                               1431 03964000
         IC    R5,EOVNMEXT(,RWORK)                                 1431 03965000
         LH    @10,CATNMEXT(,RWORK)                                1431 03966000
         N     @10,@CF05661                                        1431 03967000
         ALR   R5,@10                                              1431 03968000
         MH    R5,@CH00151                                         1431 03969000
         AL    R5,@CF00048                                         1431 03970000
*   IF AMBGSR=ON THEN               /* IS THIS GSR DATA SET  @Z30SSJH*/ 03971000
         L     @10,EOVAMBPT(,RWORK)                                1432 03972000
         TM    AMBGSR(@10),B'00100000'                             1432 03973000
         BNO   @RF01432                                            1432 03974000
*     R6=GSRPOOL;                   /* GET NEW EDB IN SP 241 @Z30SSJH*/ 03975000
         LA    R6,241                                              1433 03976000
*   ELSE                            /* NO, CONTINUE TESTS    @Z30SSJH*/ 03977000
*     IF AMBLCAT=ON THEN            /* CATALOG AMB ?                 */ 03978000
         B     @RC01432                                            1434 03979000
@RF01432 L     @10,EOVAMBPT(,RWORK)                                1434 03980000
         L     @10,AMBPAMBL(,@10)                                  1434 03981000
         TM    AMBLCAT(@10),B'00000010'                            1434 03982000
         BNO   @RF01434                                            1434 03983000
*       R6=CATPOOL;                 /* GET NEW EDB IN SP 231 @Z30SSJH*/ 03984000
         LA    R6,231                                              1435 03985000
*     ELSE                          /* NOT GSR OR CATALOG    @Z30SSJH*/ 03986000
*       R6=EDBPOOL;                 /* GET NEW EDB IN SP 252 @Z30SSJH*/ 03987000
         B     @RC01434                                            1436 03988000
@RF01434 LA    R6,252                                              1436 03989000
*   DO;                             /* GETMAIN (RC) LV(R5) SP(R6)    */ 03990000
@RC01434 DS    0H                                                  1437 03991000
@RC01432 DS    0H                                                  1438 03992000
*     RESPECIFY                                                    1438 03993000
*      (GPR01F,                                                    1438 03994000
*       GPR15F,                                                    1438 03995000
*       GPR00F) RESTRICTED;                                        1438 03996000
*     GPR01F=0;                     /* REG 1 MUST BE ZERO            */ 03997000
         SLR   GPR01F,GPR01F                                       1439 03998000
*     GPR15F=0;                     /* RC-TYPE GETMAIN               */ 03999000
         SLR   GPR15F,GPR15F                                       1440 04000000
*     GPR00F=R5;                    /* LENGTH REQUESTED              */ 04001000
         LR    GPR00F,R5                                           1441 04002000
*     GPR15F=GPR15F|((R6)*256);     /* SP IN BYTE 2                  */ 04003000
         LR    @14,R6                                              1442 04004000
         SLA   @14,8                                               1442 04005000
         OR    GPR15F,@14                                          1442 04006000
*     SVC(120);                     /* RC/RU-FORM OF GETMAIN         */ 04007000
         SVC   120                                                 1443 04008000
*     RESPECIFY                                                    1444 04009000
*      (GPR01F,                                                    1444 04010000
*       GPR15F,                                                    1444 04011000
*       GPR00F) UNRESTRICTED;                                      1444 04012000
*   END;                            /* GETMAIN (RC) LV(R5) SP(R6) GET   04013000
*                                      NEW EDB               @Z30SSJH*/ 04014000
*   RFY                                                            1446 04015000
*    (R1,                                                          1446 04016000
*     R15) RSTD;                    /* RESTRICT GETMAIN REGS @Z30SSJH*/ 04017000
*   IF R15^=0 THEN                  /* GETMAIN ERROR.                */ 04018000
         LTR   R15,R15                                             1447 04019000
         BZ    @RF01447                                            1447 04020000
*     DO;                                                          1448 04021000
*       RFY                                                        1449 04022000
*         R15 UNRSTD;               /* DROP REG RESTRICTION  @Z30SSJH*/ 04023000
*       EOVPDCDE=PP20;              /* SET PROBLEM CODE      @Y30DSJH*/ 04024000
         MVI   EOVPDCDE(RWORK),X'14'                               1450 04025000
*       EOVERR=MNTERR;              /* SET EOV ERROR                 */ 04026000
         MVI   EOVERR(RWORK),X'04'                                 1451 04027000
*     END;                                                         1452 04028000
*   ELSE                            /* GETMAIN SUCCESSFUL.           */ 04029000
*     DO;                                                          1453 04030000
         B     @RC01447                                            1453 04031000
@RF01447 DS    0H                                                  1454 04032000
*       EOVNEWCB=R1;                /* SAVE ADDR OF NEW EDB  @Z30SSJH*/ 04033000
         ST    R1,EOVNEWCB(,RWORK)                                 1454 04034000
*       CBLNEDB=R1;                 /* SAVE ADDR OF NEW EDB  @Z30SSJH*/ 04035000
         L     @10,EOVCBLPT(,RWORK)                                1455 04036000
         ST    R1,CBLNEDB(,@10)                                    1455 04037000
*       R6=R1;                      /* POINT TO GOTTEN CORE  @Z30SSJH*/ 04038000
         LR    R6,R1                                               1456 04039000
*       RFY                                                        1457 04040000
*         R1 UNRSTD;                /* DROP REG RESTRICTION  @Z30SSJH*/ 04041000
*       IF AMBLFIX=ON THEN          /* WAS FASTPATH OPT SPEC @Z30SSJH*/ 04042000
         L     @10,EOVAMBPT(,RWORK)                                1458 04043000
