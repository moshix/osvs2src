*           RDEBXT=ADDR(DEBBASND);  /* POINT TO FIRST DEB EXTENT     */ 04073000
         L     RDEBXT,AMBDEBPT(,@10)                               1465 04074000
         LA    RDEBXT,DEBBASND(,RDEBXT)                            1465 04075000
*           EOVMCTR=0;              /* ZERO M COUNTER.               */ 04076000
         MVI   EOVMCTR(RWORK),X'00'                                1466 04077000
*           DO I=1 TO DEBNMEXT;     /* SCAN DEB ENTRIES.             */ 04078000
         LA    @10,1                                               1467 04079000
         B     @DE01467                                            1467 04080000
@DL01467 DS    0H                                                  1468 04081000
*             IF DEBUCBA^=EOVSUCB&DEBUCBA^=EOVDUCB THEN/* DEB UCB NOT   04082000
*                                      EQUAL TO SUCCESS AND NOT EQUAL   04083000
*                                      TO DISMOUNT UCB.              */ 04084000
         L     @10,DEBUCBA-1(,RDEBXT)                              1468 04085000
         LA    @10,0(,@10)                                         1468 04086000
         C     @10,EOVSUCB(,RWORK)                                 1468 04087000
