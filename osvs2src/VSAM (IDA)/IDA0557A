*                         EOVDOSW=ON;/* STOP DO LOOP.                */ 02922000
         OI    EOVDOSW(RWORK),B'00010000'                          1119 02923000
*                     END;                                         1120 02924000
@RF01118 DS    0H                                                  1121 02925000
*           END;                                                   1121 02926000
@RC01110 DS    0H                                                  1121 02927000
@RC01104 DS    0H                                                  1121 02928000
@RC01098 DS    0H                                                  1121 02929000
@RC01086 DS    0H                                                  1122 02930000
*       END;                                                       1122 02931000
@RC01080 DS    0H                                                  1122 02932000
@DE01078 TM    EOVDOSW(RWORK),B'00010000'                          1122 02933000
         BZ    @DL01078                                            1122 02934000
*     END;                                                         1123 02935000
*   RFY                                                            1124 02936000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 02937000
@RF01075 DS    0H                                                  1125 02938000
*   RET=EOVSAV2;                    /* RESTORE RETURN ADDRESS.       */ 02939000
         L     RET,EOVSAV2(,RWORK)                                 1125 02940000
*   RFY                                                            1126 02941000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 02942000
*   END VOLSW;                                                     1127 02943000
@EL00016 DS    0H                                                  1127 02944000
@EF00016 DS    0H                                                  1127 02945000
@ER00016 BR    @14                                                 1127 02946000
*CATALC:                                                           1128 02947000
*                                                                  1128 02948000
*   /*****************************************************************/ 02949000
*   /*                                                               */ 02950000
*   /* CATALOG ALLOCATE SPACE ---- THIS ROUTINE INTERFACES WITH      */ 02951000
*   /* CATALOG TO ALLOCATE SPACE ON THE CURRENT VOLUME. THE FIRST    */ 02952000
*   /* FIELD LIST BUILT IS FOR THE FIELD NAME UCBADDR. IF THE DATA   */ 02953000
*   /* SET HAS KEY RANGES, 2 MORE FIELD LISTS ARE BUILT TO CONTAIN   */ 02954000
*   /* THE LOW & HIGH KEYS OF THE KEY RANGE.                         */ 02955000
*   /*                                                               */ 02956000
*   /*****************************************************************/ 02957000
*                                                                  1128 02958000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1128 02959000
CATALC   DS    0H                                                  1129 02960000
*   RFY                                                            1129 02961000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 02962000
*   EOVSAV3=RET;                    /* SAVE RETURN ADDRESS.          */ 02963000
         ST    RET,EOVSAV3(,RWORK)                                 1130 02964000
*   RFY                                                            1131 02965000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 02966000
*                                                                  1131 02967000
*   /*****************************************************************/ 02968000
*   /*                                                               */ 02969000
*   /* BUILD CATALOG PARAMETER LIST.                                 */ 02970000
*   /*                                                               */ 02971000
*   /*****************************************************************/ 02972000
*                                                                  1132 02973000
*   R6=CTGPLSZ+(3*CTGFLSZ);         /* FIND LENGTH OF PARAMETER      */ 02974000
         L     R6,CTGFLSZ                                          1132 02975000
         MH    R6,@CH00128                                         1132 02976000
         AL    R6,CTGPLSZ                                          1132 02977000
*   R6=R6+(2*LENGTH(CTGFIELD));     /* LISTS.                        */ 02978000
         AL    R6,@CF00048                                         1133 02979000
*   CALL CATSETUP;                  /* INITIALIZE PARM LIST  @ZA13505*/ 02980000
         BAL   @14,CATSETUP                                        1134 02981000
*   IF EOVSSWIX=ON&AMBTYPE=AMBDATA THEN/* SEQ SET/DATA & REQ. AMB IS    02982000
*                                      DATA.                 @ZA08325*/ 02983000
         TM    EOVSSWIX(RWORK),B'01000000'                         1135 02984000
         BNO   @RF01135                                            1135 02985000
         L     @10,EOVAMBPT(,RWORK)                                1135 02986000
         TM    AMBTYPE(@10),B'01000000'                            1135 02987000
         BNZ   @RF01135                                            1135 02988000
*     DO;                           /*                       @XA02392*/ 02989000
*       EOVAMBPT=AMBLIX;            /* BASE AMB ON INDEX AMB @XA02392*/ 02990000
         L     @10,AMBPAMBL(,@10)                                  1137 02991000
         L     @10,AMBLIX(,@10)                                    1137 02992000
         ST    @10,EOVAMBPT(,RWORK)                                1137 02993000
*       CATWANEX=AMDNEXT;           /* PASS NO. OF INDEX EXTENTS TO     02994000
