         IC    @15,DEBNMEXT(,@07)                                  1413 03874000
         CR    @10,@15                                             1413 03875000
         BNH   @DL01409                                            1413 03876000
*   /*****************************************************************/ 03877000
*   /*                                                               */ 03878000
*   /* SCAN THE DEB ENTRIES, IF DISMOUNT UCB IS NOT 0 AND NOT EQUAL  */ 03879000
*   /* TO SUCCESS UCB, DECREMENT THE NUMBER EXTENTS FOR EACH ENTRY   */ 03880000
*   /* WHOSE UCB IS EQUAL TO THE DISMOUNT UCB.                       */ 03881000
*   /*                                                               */ 03882000
*   /*****************************************************************/ 03883000
*                                                                  1414 03884000
*   IF EOVDUCB^=0&EOVDUCB^=EOVSUCB THEN/* DISMOUNT UCB NOT 0 & NOT 1414 03885000
*                                      EQUAL TO SUCCESS UCB.         */ 03886000
         L     @10,EOVDUCB(,RWORK)                                 1414 03887000
         LTR   @10,@10                                             1414 03888000
         BZ    @RF01414                                            1414 03889000
         C     @10,EOVSUCB(,RWORK)                                 1414 03890000
         BE    @RF01414                                            1414 03891000
*     DO;                                                          1415 03892000
*       RDEBXT=ADDR(DEBBASND);      /* POINT TO FIRST DEB EXTENT     */ 03893000
         L     @10,EOVAMBPT(,RWORK)                                1416 03894000
         L     RDEBXT,AMBDEBPT(,@10)                               1416 03895000
         LA    RDEBXT,DEBBASND(,RDEBXT)                            1416 03896000
*       DO I=1 TO DEBNMEXT;         /* SCAN DEB EXTENTS              */ 03897000
         LA    @10,1                                               1417 03898000
         B     @DE01417                                            1417 03899000
@DL01417 DS    0H                                                  1418 03900000
*         IF DEBUCBA=EOVDUCB THEN   /* SAME UCB                      */ 03901000
         L     @10,DEBUCBA-1(,RDEBXT)                              1418 03902000
         LA    @10,0(,@10)                                         1418 03903000
         C     @10,EOVDUCB(,RWORK)                                 1418 03904000
         BNE   @RF01418                                            1418 03905000
*           EOVNMEXT=EOVNMEXT-1;    /* DECREMENT CURRENT NO. OF    1419 03906000
*                                      EXTENTS.                      */ 03907000
         SLR   @10,@10                                             1419 03908000
         IC    @10,EOVNMEXT(,RWORK)                                1419 03909000
         BCTR  @10,0                                               1419 03910000
         STC   @10,EOVNMEXT(,RWORK)                                1419 03911000
*         RDEBXT=RDEBXT+LENGTH(DEBDASD);/* POINT TO NEXT EXTENT      */ 03912000
@RF01418 AL    RDEBXT,@CF00121                                     1420 03913000
*       END;                                                       1421 03914000
         LA    @10,1                                               1421 03915000
         AL    @10,I(,RWORK)                                       1421 03916000
@DE01417 ST    @10,I(,RWORK)                                       1421 03917000
         L     @07,EOVAMBPT(,RWORK)                                1421 03918000
         L     @07,AMBDEBPT(,@07)                                  1421 03919000
         SLR   @15,@15                                             1421 03920000
         IC    @15,DEBNMEXT(,@07)                                  1421 03921000
         CR    @10,@15                                             1421 03922000
         BNH   @DL01417                                            1421 03923000
*     END;                                                         1422 03924000
*   RFY                                                            1423 03925000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 03926000
@RF01414 DS    0H                                                  1424 03927000
*   RET=EOVSAV3;                    /* RESTORE RETURN ADDRESS.       */ 03928000
         L     RET,EOVSAV3(,RWORK)                                 1424 03929000
*   RFY                                                            1425 03930000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 03931000
*   END CALCDEB;                                                   1426 03932000
@EL00022 DS    0H                                                  1426 03933000
@EF00022 DS    0H                                                  1426 03934000
@ER00022 BR    @14                                                 1426 03935000
*BLDEDB:                                                           1427 03936000
*                                                                  1427 03937000
*   /*****************************************************************/ 03938000
*   /*                                                               */ 03939000
*   /* BUILD EDB - THIS ROUTINE BUILDS A NEW EDB. ALL CORRESPONDING  */ 03940000
*   /* EXTENTS IN THE DEB THAT ARE NOT MARKED DELETED ARE COPIED FROM*/ 03941000
*   /* THE OLD EDB TO THE NEW EDB. THEN ALL EXTENTS RETRIEVED FROM   */ 03942000
*   /* THE CATALOG FOR THE VOLUME BEING PROCESSED ARE ADDED TO THE   */ 03943000
*   /* NEW EDB.                                                      */ 03944000
*   /*                                                               */ 03945000
*   /*****************************************************************/ 03946000
*                                                                  1427 03947000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1427 03948000
