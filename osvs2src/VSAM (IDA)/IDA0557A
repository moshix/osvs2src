*ARDBSCH:                                                          0821 01857000
*                                                                  0821 01858000
*   /*****************************************************************/ 01859000
*   /*                                                               */ 01860000
*   /* SEARCH ARDB'S - THIS ROUTINE SEARCHES FOR THE ARDB TO BE      */ 01861000
*   /* ALLOCATED SPACE BY COMPARING THE REQUESTED RBA TO THE HIGH    */ 01862000
*   /* ALLOCATED RBA FOR AN EQUAL CONDITION. THE SERIAL NO. IN THE   */ 01863000
*   /* ARDB IS THEN USED TO MOUNT THE VOLUME.                        */ 01864000
*   /*                                                               */ 01865000
*   /*****************************************************************/ 01866000
*                                                                  0821 01867000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0821 01868000
ARDBSCH  DS    0H                                                  0822 01869000
*   RFY                                                            0822 01870000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 01871000
*   EOVSAV2=RET;                    /* SAVE RETURN ADDRESS.          */ 01872000
         ST    RET,EOVSAV2(,RWORK)                                 0823 01873000
*   RFY                                                            0824 01874000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 01875000
*   RARDB=AMDPARDB;                 /* POINT TO FIRST ARDB.          */ 01876000
*                                                                  0825 01877000
         L     @10,EOVAMBPT(,RWORK)                                0825 01878000
         L     @10,AMBDSB(,@10)                                    0825 01879000
         L     RARDB,AMDPARDB(,@10)                                0825 01880000
*   /*****************************************************************/ 01881000
*   /*                                                               */ 01882000
*   /* SEARCH ARDB'S BY REQUESTED RBA OR KEY.                        */ 01883000
*   /*                                                               */ 01884000
*   /*****************************************************************/ 01885000
*                                                                  0826 01886000
*   EOVHIRBA=0;                     /* INSURE ZERO           @ZA17050*/ 01887000
         SLR   @10,@10                                             0826 01888000
         ST    @10,EOVHIRBA(,RWORK)                                0826 01889000
*   DO WHILE(RARDB^=0);             /* CHECK ALL ARDB'S      @ZA17050*/ 01890000
         B     @DE00827                                            0827 01891000
@DL00827 DS    0H                                                  0828 01892000
*     EOVHIRBA=MAX(EOVHIRBA,ARDERBA);/* FIND HI RBA          @ZA17050*/ 01893000
         L     @10,ARDERBA(,RARDB)                                 0828 01894000
         L     @07,EOVHIRBA(,RWORK)                                0828 01895000
