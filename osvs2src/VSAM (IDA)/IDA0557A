         L     @10,CVTSMCA(,@10)                                   1857 05393000
         LTR   @10,@10                                             1857 05394000
         BZ    @RF01857                                            1857 05395000
         TM    SMCAUSER(@10),B'11000000'                           1857 05396000
         BNO   @RF01857                                            1857 05397000
         TM    SMCADSA(@10),B'00010000'                            1857 05398000
         BNO   @RF01857                                            1857 05399000
         L     @10,PSATOLD                                         1857 05400000
         L     @10,TCBTCTB-1(,@10)                                 1857 05401000
         LA    @10,0(,@10)                                         1857 05402000
         LTR   @10,@10                                             1857 05403000
         BZ    @RF01857                                            1857 05404000
*     DO;                                                          1858 05405000
*       EOVSMFPL=EOVSMFPL&&EOVSMFPL;/* CLR SMF PARAMETER LIST        */ 05406000
         XC    EOVSMFPL(24,RWORK),EOVSMFPL(RWORK)                  1859 05407000
*       SMFDSNAM=EOVLOCDS(1);       /* POINT TO DATA SET NAME.       */ 05408000
         L     @10,EOVLOCDS(,RWORK)                                1860 05409000
         ST    @10,SMFDSNAM(,RWORK)                                1860 05410000
*       SMFCTNAM=ADDR(CAXCNAM);     /* POINT TO CATALOG DSNAME.      */ 05411000
