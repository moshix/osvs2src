*DSCTLBLK:                                                         1367 03701000
*                                                                  1367 03702000
*   /*****************************************************************/ 03703000
*   /*                                                               */ 03704000
*   /* BUILD DEB AND EDB'S - THIS ROUTINE DOES A CATALOG LOCATE FOR  */ 03705000
*   /* THE EXTENTS ON THE CURRENT VOLUME. GETMAIN IS ISSUED FOR THE  */ 03706000
*   /* NEW DEB AND THE OLD DEB IS MOVED TO THE NEW DEB AND CHAINED ON*/ 03707000
*   /* THE TCB DEB CHAIN. IF THE EOV REQUEST WAS 'MOUNT SPECIFIED    */ 03708000
*   /* VOLUME', AN EDB & DEB ENTRY IS BUILT FOR EACH EXISTING        */ 03709000
*   /* PHYSICAL EXTENT. IF THE REQUEST CAUSED SPACE ALLOCATION, THEN */ 03710000
*   /* AN EDB & DEB ENTRY IS BUILT FOR EACH NEW PHYSICAL EXTENT. THE */ 03711000
*   /* OLD DEB IS FREED WITH FREEMAIN.                               */ 03712000
*   /*                                                               */ 03713000
*   /*****************************************************************/ 03714000
*                                                                  1367 03715000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1367 03716000
DSCTLBLK DS    0H                                                  1368 03717000
*   RFY                                                            1368 03718000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 03719000
*   EOVSAV2=RET;                    /* SAVE RETURN ADDRESS.          */ 03720000
         ST    RET,EOVSAV2(,RWORK)                                 1369 03721000
*   RFY                                                            1370 03722000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 03723000
*   CALL CATLOCXT;                  /* CALL CATALOG LOCATE EXTENTS,     03724000
*                                      MUST BE DONE BEFORE CONTROL 1371 03725000
*                                      BLOCKS ARE MODIFIED           */ 03726000
         BAL   @14,CATLOCXT                                        1371 03727000
*   IF EOVERR=0 THEN                /* NO EOV ERROR.                 */ 03728000
         CLI   EOVERR(RWORK),0                                     1372 03729000
         BNE   @RF01372                                            1372 03730000
*     DO;                                                          1373 03731000
*       CALL CALCDEB;               /* CALCULATE NO. DEB ENTRIES     */ 03732000
         BAL   @14,CALCDEB                                         1374 03733000
*       CALL BLDEDB;                /* BUILD EDB                     */ 03734000
         BAL   @14,BLDEDB                                          1375 03735000
*       IF EOVERR=0 THEN            /* NO EOV ERROR                  */ 03736000
         CLI   EOVERR(RWORK),0                                     1376 03737000
         BNE   @RF01376                                            1376 03738000
*         DO;                                                      1377 03739000
*           CALL BLDDEB;            /* BUILD DEB                     */ 03740000
         BAL   @14,BLDDEB                                          1378 03741000
*           IF EOVERR=0 THEN        /* NO EOV ERROR                  */ 03742000
         CLI   EOVERR(RWORK),0                                     1379 03743000
         BNE   @RF01379                                            1379 03744000
*             DO;                                                  1380 03745000
*               IF AMBLCAT=OFF THEN /* NOT CATALOG AMB               */ 03746000
         L     @10,EOVAMBPT(,RWORK)                                1381 03747000
         L     @10,AMBPAMBL(,@10)                                  1381 03748000
         TM    AMBLCAT(@10),B'00000010'                            1381 03749000
         BNZ   @RF01381                                            1381 03750000
*                 CALL TCBDEBCN;    /* CHAIN DEB ON TCB              */ 03751000
         BAL   @14,TCBDEBCN                                        1382 03752000
*               ELSE                /* CATALOG AMB           @Z30SSJH*/ 03753000
*                 AMBDEBAD=EOVNEWCB;/* PT AMB TO NEW DEB     @Z30SSJH*/ 03754000
         B     @RC01381                                            1383 03755000
