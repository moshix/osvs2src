         BNE   @RF01460                                            1460 04051000
*         DO;                       /* CONTINUE IF NO ERRORS @Z30SSJH*/ 04052000
*           R6->IDAEDBHD=IDAEDBHD;  /* MOVE EDB FIXED SECTION.       */ 04053000
*                                                                  1462 04054000
         L     @10,EOVAMBPT(,RWORK)                                1462 04055000
         L     @15,AMBEDB(,@10)                                    1462 04056000
         MVC   IDAEDBHD(8,R6),IDAEDBHD(@15)                        1462 04057000
*           /*********************************************************/ 04058000
*           /*                                                       */ 04059000
*           /* SCAN DEB AND EDB ENTRIES. IF THE DEB ENTRY HASN'T BEEN*/ 04060000
*           /* DELETED, MOVE THE OLD EDB ENTRY TO THE NEW EDB.       */ 04061000
*           /*                                                       */ 04062000
*           /*********************************************************/ 04063000
*                                                                  1463 04064000
*           R6=R6+LENGTH(IDAEDBHD); /* POINT TO FIRST ENTRY IN NEW 1463 04065000
*                                      EDB.                          */ 04066000
