         STH   @10,EOVNARD1(,RWORK)                                0626 01210000
*   IF EOVSSWIX=ON THEN             /* TEST FOR SEQUENCE     @ZA08325   01211000
*                                      SET WITH DATA         @ZA08325*/ 01212000
         TM    EOVSSWIX(RWORK),B'01000000'                         0627 01213000
         BNO   @RF00627                                            0627 01214000
*     DO;                           /* COUNT NO. OF ARDB'S FOR OTHER    01215000
*                                      DATA SET IN CLUSTER           */ 01216000
*       IF AMBTYPE=AMBINDEX THEN    /* IS THIS AN INDEX AMB          */ 01217000
