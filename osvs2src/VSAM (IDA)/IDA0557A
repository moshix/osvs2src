*     DO;                                                          1814 05239000
*       CALL CATERR;                /* SET PROBLEM CODE              */ 05240000
         BAL   @14,CATERR                                          1815 05241000
*       EOVERR=MNTERR;              /* SET UNABLE TO MOUNT ERROR.    */ 05242000
         MVI   EOVERR(RWORK),X'04'                                 1816 05243000
*     END;                                                         1817 05244000
*   ELSE                            /* NO CATLOG ERROR.              */ 05245000
*     DO;                                                          1818 05246000
         B     @RC01813                                            1818 05247000
@RF01813 DS    0H                                                  1819 05248000
*       RCATF=CTGFIELD(1);          /* POINT TO FIRST FIELD LIST   1819 05249000
*                                                            @ZA27118*/ 05250000
*                                                                  1819 05251000
