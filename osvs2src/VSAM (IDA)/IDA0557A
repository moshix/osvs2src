         L     @10,EOVCPWA2(,RWORK)                                0633 01237000
         STH   @10,EOVNARD2(,RWORK)                                0633 01238000
*     END;                                                         0634 01239000
*   ELSE                                                           0635 01240000
*     ;                             /* NOT SEQUENCE SET WITH DATA    */ 01241000
*                                                                  0635 01242000
@RF00627 DS    0H                                                  0636 01243000
*   /*****************************************************************/ 01244000
*   /*                                                               */ 01245000
*   /* DETERMINE THE SIZE OF THE CHECKPOINT WORK AREA, OBTAIN        */ 01246000
*   /* STORAGE, AND INITIALIZE FIXED FIELDS IN THE WORK AREA.        */ 01247000
*   /*                                                               */ 01248000
