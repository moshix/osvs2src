*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1833 05302000
CALCXT   DS    0H                                                  1834 05303000
*   RFY                                                            1834 05304000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 05305000
*   EOVSAV4=RET;                    /* SAVE RETURN ADDRESS.          */ 05306000
         ST    RET,EOVSAV4(,RWORK)                                 1835 05307000
*   RFY                                                            1836 05308000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 05309000
*   RCATF=CTGFIELD(1);              /* POINT TO EXTENT FIELD LIST.   */ 05310000
