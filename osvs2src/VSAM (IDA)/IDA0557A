         BCTR  @15,0                                               0834 01936000
         EX    @15,@SC05657                                        0834 01937000
         BL    @RF00834                                            0834 01938000
*           DO;                                                    0835 01939000
*             R6=ADDR(IDAARDB)+EOVKEYLN;/* ADJUST ARDB POINTER FOR 0836 01940000
*                                      2ND KEY.                      */ 01941000
*                                                                  0836 01942000
         ALR   @07,RARDB                                           0836 01943000
         LR    R6,@07                                              0836 01944000
*             /*******************************************************/ 01945000
*             /*                                                     */ 01946000
*             /* TEST IF REQUESTED KEY IS LTE ARDB HIGH KEY.         */ 01947000
*             /*                                                     */ 01948000
*             /*******************************************************/ 01949000
*                                                                  0837 01950000
*             IF INKEY(1:EOVKEYLN)<=R6->ARDKEYS(1:EOVKEYLN) THEN/* 0837 01951000
*                                      REQUESTED KEY IS LTE.         */ 01952000
         EX    @15,@SC05659                                        0837 01953000
         BH    @RF00837                                            0837 01954000
*               EOVARDB1=RARDB;     /* SAVE ARDB ADDRESS IN WORKAREA.*/ 01955000
         ST    RARDB,EOVARDB1(,RWORK)                              0838 01956000
*           END;                                                   0839 01957000
@RF00837 DS    0H                                                  0840 01958000
*       END;                                                       0840 01959000
@RF00834 DS    0H                                                  0841 01960000
*     RARDB=ARDNPTR;                /* POINT TO NEXT ARDB.           */ 01961000
@RC00829 L     RARDB,ARDNPTR(,RARDB)                               0841 01962000
*   END;                                                           0842 01963000
@DE00827 SLR   @10,@10                                             0842 01964000
         CR    RARDB,@10                                           0842 01965000
         BNE   @DL00827                                            0842 01966000
*   IF EOVARDB1=0 THEN              /* ARDB NOT FOUND.               */ 01967000
         C     @10,EOVARDB1(,RWORK)                                0843 01968000
         BNE   @RF00843                                            0843 01969000
*     DO;                                                          0844 01970000
*       EOVPDCDE=PP104;             /* SET INTERNAL PD CODE  @Y30DSJH*/ 01971000
         MVI   EOVPDCDE(RWORK),X'68'                               0845 01972000
*       EOVERR=ALCERR;              /* SET UNABLE TO ALLOCATE ERROR. */ 01973000
         MVI   EOVERR(RWORK),X'08'                                 0846 01974000
*     END;                                                         0847 01975000
*   ELSE                            /* SAVE SERIAL NO.IN CAT @ZA09453*/ 01976000
*     CATVOLS=EOVARDB1->ARDVOLSR;   /* FIELD                         */ 01977000
*                                                                  0848 01978000
         B     @RC00843                                            0848 01979000
@RF00843 L     @10,EOVARDB1(,RWORK)                                0848 01980000
         MVC   CATVOLS(6,RWORK),ARDVOLSR(@10)                      0848 01981000
*   /*****************************************************************/ 01982000
*   /*                                                               */ 01983000
*   /* CHECK TO SEE THAT THE NEW EXTENT WILL NOT CAUSE THE HI ALLOC  */ 01984000
*   /* RBA TO RAP AROUND                                     @ZA17050*/ 01985000
*   /*                                                               */ 01986000
*   /*****************************************************************/ 01987000
*                                                                  0849 01988000
*   CALL CATLOCDS;                  /* GET EXTENT INFORMATION      0849 01989000
*                                                            @ZA17050*/ 01990000
@RC00843 BAL   @14,CATLOCDS                                        0849 01991000
*   EOVPALOC=EOVLOCDS(2)->AQTY;     /* GET PRIMARY ALLOC QTY @ZA17050*/ 01992000
