*             /*                                                     */ 02829000
*             /* TEST IF CATALOG ERROR.                              */ 02830000
*             /*                                                     */ 02831000
*             /*******************************************************/ 02832000
*                                                                  1086 02833000
*             IF CATRC=0 THEN       /* NO CATALOG ERROR.             */ 02834000
@RF01084 LH    @10,CATRC(,RWORK)                                   1086 02835000
         N     @10,@CF05661                                        1086 02836000
         LTR   @10,@10                                             1086 02837000
         BNZ   @RF01086                                            1086 02838000
*               DO;                                                1087 02839000
*                 RCATW=ADDR(CATWORK);/* POINT TO CATALOG PL.        */ 02840000
         L     @10,RCATW(,RWORK)                                   1088 02841000
         ST    @10,RCATW(,RWORK)                                   1088 02842000
*                 EOVRRNO=CTGWKA->CATRRNO;/* SAVE REL. REP. NOS.     */ 02843000
         L     @10,CTGWKA(,@10)                                    1089 02844000
         MVC   EOVRRNO(4,RWORK),CATRRNO(@10)                       1089 02845000
*                 CALL CATUPDVO;    /* CALL CATALOG UPDATE VOLUME  1090 02846000
*                                      OCCURRENCE.                   */ 02847000
         BAL   @14,CATUPDVO                                        1090 02848000
*                 IF EOVERR=0 THEN  /* NO EOV ERROR.                 */ 02849000
         CLI   EOVERR(RWORK),0                                     1091 02850000
         BNE   @RF01091                                            1091 02851000
*                   DO;                                            1092 02852000
*                     EOVARDB1->ARDVOLSR=CATVOLS;/* UPDATE ARDB WITH    02853000
*                                      CURRENT VOLUME SERIAL.        */ 02854000
         L     @10,EOVARDB1(,RWORK)                                1093 02855000
         MVC   ARDVOLSR(6,@10),CATVOLS(RWORK)                      1093 02856000
*                     EOVARDB1->ARDRELNO=EOVRRNO1;/* UPDATE ARDB REL.   02857000
*                                      REP. NO.                      */ 02858000
         LH    @07,EOVRRNO1(,RWORK)                                1094 02859000
         N     @07,@CF05661                                        1094 02860000
         STH   @07,ARDRELNO(,@10)                                  1094 02861000
*                   END;                                           1095 02862000
*                 EOVDOSW=ON;       /* STOP DO LOOP.                 */ 02863000
@RF01091 OI    EOVDOSW(RWORK),B'00010000'                          1096 02864000
*               END;                                               1097 02865000
*             ELSE                  /* CATALOG ERROR.                */ 02866000
*               IF CATRC^=ERRNOSPC THEN/* NOT OUT OF SPACE.          */ 02867000
         B     @RC01086                                            1098 02868000
@RF01086 CLC   CATRC(2,RWORK),@CH00331                             1098 02869000
         BE    @RF01098                                            1098 02870000
*                 DO;                                              1099 02871000
*                   CALL CATERR;    /* SET PROBLEM CODE.             */ 02872000
         BAL   @14,CATERR                                          1100 02873000
*                   EOVERR=ALCERR;  /* SET UNABLE TO ALLOCATE ERROR. */ 02874000
         MVI   EOVERR(RWORK),X'08'                                 1101 02875000
*                   EOVDOSW=ON;     /* STOP DO LOOP.                 */ 02876000
         OI    EOVDOSW(RWORK),B'00010000'                          1102 02877000
*                 END;                                             1103 02878000
*               ELSE                /* OUT OF SPACE.                 */ 02879000
*                 IF AMDORDER=OFF THEN/* VOLUMES NOT ORDERED.        */ 02880000
         B     @RC01098                                            1104 02881000
@RF01098 L     @10,EOVAMBPT(,RWORK)                                1104 02882000
         L     @10,AMBDSB(,@10)                                    1104 02883000
         TM    AMDORDER(@10),B'00001000'                           1104 02884000
         BNZ   @RF01104                                            1104 02885000
*                   DO;                                            1105 02886000
*                     CALL CATLOCNC;/* CATALOG LOCATE NEXT CANDIDATE    02887000
*                                      VOLUME.                       */ 02888000
         BAL   @14,CATLOCNC                                        1106 02889000
*                     IF EOVERR^=0 THEN/* EOV ERROR.                 */ 02890000
         CLI   EOVERR(RWORK),0                                     1107 02891000
         BE    @RF01107                                            1107 02892000
*                       EOVDOSW=ON; /* STOP DO LOOP.                 */ 02893000
         OI    EOVDOSW(RWORK),B'00010000'                          1108 02894000
*                   END;                                           1109 02895000
*                 ELSE              /* VOLUMES ORDERED.              */ 02896000
*                   IF EOVCAND=ON THEN/* LAST VOLUME WAS A CANDIDATE    02897000
*                                      VOLUME.                       */ 02898000
         B     @RC01104                                            1110 02899000
@RF01104 TM    EOVCAND(RWORK),B'01000000'                          1110 02900000
         BNO   @RF01110                                            1110 02901000
*                     DO;                                          1111 02902000
*                       EOVPDCDE=PP100;/* SET PROB CODE      @Y30DSJH*/ 02903000
         MVI   EOVPDCDE(RWORK),X'64'                               1112 02904000
*                       EOVERR=ALCERR;/* SET UNABLE TO ALLOCATE    1113 02905000
*                                      ERROR.                        */ 02906000
         MVI   EOVERR(RWORK),X'08'                                 1113 02907000
*                       EOVDOSW=ON; /* STOP DO LOOP.                 */ 02908000
         OI    EOVDOSW(RWORK),B'00010000'                          1114 02909000
*                     END;                                         1115 02910000
*                   ELSE            /* LAST VOLUME WASN'T A        1116 02911000
