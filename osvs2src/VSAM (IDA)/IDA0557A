         SLR   @15,@10                                             1393 03794000
         ST    @15,AMDASPA(,@07)                                   1393 03795000
*                     END;                                         1394 03796000
*                 END;                                             1395 03797000
@RF01387 DS    0H                                                  1396 03798000
*             END;                                                 1396 03799000
@RF01384 DS    0H                                                  1397 03800000
*         END;                                                     1397 03801000
@RF01379 DS    0H                                                  1398 03802000
*     END;                                                         1398 03803000
@RF01376 DS    0H                                                  1399 03804000
*   RFY                                                            1399 03805000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 03806000
@RF01372 DS    0H                                                  1400 03807000
*   RET=EOVSAV2;                    /* RESTORE RETURN ADDRESS.       */ 03808000
         L     RET,EOVSAV2(,RWORK)                                 1400 03809000
*   RFY                                                            1401 03810000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 03811000
*   END DSCTLBLK;                                                  1402 03812000
@EL00021 DS    0H                                                  1402 03813000
@EF00021 DS    0H                                                  1402 03814000
@ER00021 BR    @14                                                 1402 03815000
*CALCDEB:                                                          1403 03816000
*                                                                  1403 03817000
*   /*****************************************************************/ 03818000
*   /*                                                               */ 03819000
*   /* CALCULATE DEB ENTRIES - THIS ROUTINE CALCULATES THE NUMBER OF */ 03820000
*   /* DEB ENTRIES WHOSE VOLUME IS NOT DISMOUNTED.                   */ 03821000
*   /*                                                               */ 03822000
*   /*****************************************************************/ 03823000
*                                                                  1403 03824000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1403 03825000
CALCDEB  DS    0H                                                  1404 03826000
*   RFY                                                            1404 03827000
*     RET RSTD;                     /* RESTRICT RETURN REG   @Z30SSJH*/ 03828000
*   EOVSAV3=RET;                    /* SAVE RETURN ADDRESS.          */ 03829000
         ST    RET,EOVSAV3(,RWORK)                                 1405 03830000
*   RFY                                                            1406 03831000
*     RET UNRSTD;                   /* DROP REG RESTRICTION  @Z30SSJH*/ 03832000
*                                                                  1406 03833000
*   /*****************************************************************/ 03834000
*   /*                                                               */ 03835000
*   /* SCAN DEB ENTRIES - DECREMENT THE NUMBER OF EXTENTS FOR EACH   */ 03836000
*   /* ENTRY WHOSE UCB IS EQUAL TO THE SUCCESS UCB.                  */ 03837000
*   /*                                                               */ 03838000
*   /*****************************************************************/ 03839000
*                                                                  1407 03840000
*   EOVNMEXT=DEBNMEXT;              /* INIT. CURRENT NO. OF DEB    1407 03841000
*                                      EXTENTS.                      */ 03842000
