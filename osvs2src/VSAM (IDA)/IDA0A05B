*                 DO;               /* IF NOT EQUAL,                 */ 02333000
*                   RSRETCD2=RSERR198;/* SET THE RESTART RETURN CODE */ 02334000
         L     @05,RSTWAPTR                                        0931 02335000
         MVI   RSRETCD2(@05),X'C6'                                 0931 02336000
*                   CALL ERREXIT;   /* ISSUE MESSAGE AND EXIT        */ 02337000
         BAL   @14,ERREXIT                                         0932 02338000
*                 END;              /* OTHERWISE, NO ERROR, SO       */ 02339000
*               ARDHRBA=MWAHURBA;   /* RESTORE HIGH USED RBA AND     */ 02340000
@RF00929 L     @05,ARDBPTR                                         0934 02341000
