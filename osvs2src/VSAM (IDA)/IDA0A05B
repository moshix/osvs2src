*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 03329000
         SLA   @05,2                                               1197 03330000
         ST    RET,RETSAVE-4(@05)                                  1197 03331000
*   RESPECIFY                                                      1198 03332000
*     RET UNRESTRICTED;                                            1198 03333000
*   ACBEXLST=MWAACBEX;              /* RESTORE ACB EXIT LIST FIELD   */ 03334000
         L     @05,MWAACBEX                                        1199 03335000
         ST    @05,ACBEXLST(,RACB)                                 1199 03336000
*   ACBAMBL->AMBLQ=MWAAMBLQ;        /* RESTORE THE AMBL QUALIFIER    */ 03337000
         L     @05,ACBAMBL(,RACB)                                  1200 03338000
         MVC   AMBLQ(1,@05),MWAAMBLQ                               1200 03339000
*   ACBAMBL->AMBLFLG1=MWAFLG1;      /* RESTORE THE AMBL FLAGS        */ 03340000
         MVC   AMBLFLG1(1,@05),MWAFLG1                             1201 03341000
*   DO LC11=ONE TO AMBLNUM BY ONE;  /* RESTORE ALL AMBS              */ 03342000
         LR    LC11,@15                                            1202 03343000
         B     @DE01202                                            1202 03344000
@DL01202 DS    0H                                                  1203 03345000
*     IF LC11=ONE THEN              /* IF THIS IS THE FIRST PASS     */ 03346000
         C     LC11,@CF00053                                       1203 03347000
         BNE   @RF01203                                            1203 03348000
*       DO;                         /* PREPARE SAVED DATA AMB & RST. */ 03349000
*         ADDR(MWAAMBD)->AMBDEBPT=AMBLDTA->AMBDEBPT;/* SAVE DEB PTR  */ 03350000
