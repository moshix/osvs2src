         AL    @14,MWASP250                                        1029 02689000
         XC    IFGRPL(76,@14),IFGRPL(@14)                          1029 02690000
*   RPLDIR=ON;                      /* INDICATE DIRECT PROCESSING    */ 02691000
         OI    RPLDIR(@14),B'01000000'                             1030 02692000
*   RPLCNV=ON;                      /* CONTROL INTERVAL ACCESS       */ 02693000
*   RPLUPD=ON;                      /* UPDATE                        */ 02694000
         OI    RPLCNV(@14),B'00100010'                             1032 02695000
*   RPLDACB=ADDR(IFGACB);           /* SET ADDRESS OF ACB            */ 02696000
