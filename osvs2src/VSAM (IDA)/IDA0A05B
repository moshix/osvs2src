*   RESPECIFY                                                      0892 02107000
*     RET UNRESTRICTED;                                            0892 02108000
*   END BLDOPEN;                                                   0893 02109000
@EL00010 DS    0H                                                  0893 02110000
@EF00010 DS    0H                                                  0893 02111000
@ER00010 BR    @14                                                 0893 02112000
*MODTEST:                                                          0894 02113000
*                                                                  0894 02114000
*/********************************************************************/ 02115000
*/*                                                                  */ 02116000
*/*   FUNCTION - DETERMINES IF TEST FOR MODIFICATION WAS REQUESTED   */ 02117000
*/*              AND, IF IT WAS, COMPARES AMDSB STATISTICS WITH      */ 02118000
*/*              THOSE IN THE CATALOG FOR CLUSTERS OPEN FOR OUTPUT.  */ 02119000
*/*   INPUT    - A POINTER TO THE CURRENT AMBL IN AMBLPTR            */ 02120000
*/*   OUTPUT   - IF TEST REQUIRED AND A MISMATCH IS FOUND, VSAM      */ 02121000
*/*              RESTART IS EXITED WITH A RETURN CODE OF 196.        */ 02122000
*/*                                                                  */ 02123000
*/********************************************************************/ 02124000
*                                                                  0894 02125000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0894 02126000
MODTEST  DS    0H                                                  0895 02127000
*   RESPECIFY                                                      0895 02128000
*     RET RESTRICTED;                                              0895 02129000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 02130000
         LA    @15,1                                               0896 02131000
         AL    @15,RETCONT                                         0896 02132000
         ST    @15,RETCONT                                         0896 02133000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 02134000
*                                                                  0897 02135000
         SLA   @15,2                                               0897 02136000
         ST    RET,RETSAVE-4(@15)                                  0897 02137000
*   /*****************************************************************/ 02138000
*   /*                                                               */ 02139000
*   /* IF THE ACBCRNCK OPTION BIT IS OFF IN EITHER THE ACB OR THE    */ 02140000
*   /* DUMMY ACB AND THE CLUSTER IS OPEN FOR OUTPUT, PERFORM THE     */ 02141000
*   /* MODIFICATION TEST.                                            */ 02142000
*   /*                                                               */ 02143000
*   /*****************************************************************/ 02144000
*                                                                  0898 02145000
*   IF((BIBDACB^=ZERO&ACBCRNCK=OFF)|ACBCRNCK=OFF)&AMBLWRIT=ON THEN 0898 02146000
         L     @15,OPWBIB(,RCORE)                                  0898 02147000
         L     @15,BIBDACB(,@15)                                   0898 02148000
         LTR   @15,@15                                             0898 02149000
         BZ    @GL00008                                            0898 02150000
         TM    ACBCRNCK(RACB),B'10000000'                          0898 02151000
         BZ    @GL00007                                            0898 02152000
@GL00008 TM    ACBCRNCK(RACB),B'10000000'                          0898 02153000
         BNZ   @RF00898                                            0898 02154000
@GL00007 L     @15,AMBLPTR                                         0898 02155000
         TM    AMBLWRIT(@15),B'00100000'                           0898 02156000
         BNO   @RF00898                                            0898 02157000
*     IF NRC=OFF&NCK=OFF THEN       /* CHECK RESTART TIME OPTIONS    */ 02158000
         LA    @15,MWAFCORE                                        0899 02159000
         TM    NRC+100(@15),B'01010000'                            0899 02160000
         BNZ   @RF00899                                            0899 02161000
*       DO LC7=ONE TO AMBLNUM BY ONE;/* CHECK ALL AMDSBS             */ 02162000
         LA    @15,1                                               0900 02163000
         ST    @15,LC7                                             0900 02164000
         B     @DE00900                                            0900 02165000
@DL00900 DS    0H                                                  0901 02166000
*         AMBPTR=AMBPTRS(LC7);      /* POINT AT CURRENT AMB          */ 02167000
*                                                                  0901 02168000
         LR    @11,@15                                             0901 02169000
         SLA   @11,2                                               0901 02170000
         L     @01,AMBLPTR                                         0901 02171000
         L     @11,AMBPTRS-4(@11,@01)                              0901 02172000
         ST    @11,AMBPTR                                          0901 02173000
*         /***********************************************************/ 02174000
*         /*                                                         */ 02175000
*         /* ESTABLISH ADDRESSIBILITY FOR SAVED AMDSTAT FIELDS       */ 02176000
*         /*                                                         */ 02177000
*         /***********************************************************/ 02178000
*                                                                  0902 02179000
*         AMDSBPTR=ADDR(MWASTAT(LC7))-LENGTH(IDAAMDSB)+LENGTH(AMDSTAT); 02180000
         MH    @15,@CH02711                                        0902 02181000
         LA    AMDSBPTR,MWASTAT-48(@15)                            0902 02182000
         AL    AMDSBPTR,@CF05428                                   0902 02183000
*         IF AMDNLR^=AMDSBPTR->AMDNLR|/* COMPARE LOGICAL RECORD CNT  */ 02184000
*             AMDIREC^=AMDSBPTR->AMDIREC|/* AND INSERTED RECORD CNT  */ 02185000
*             AMDDELR^=AMDSBPTR->AMDDELR|/* AND DELETED RECORD COUNT */ 02186000
*             AMDUPR^=AMDSBPTR->AMDUPR THEN/* AND UPDATED RECORD CNT */ 02187000
         L     @15,AMBDSB(,@11)                                    0903 02188000
         CLC   AMDNLR(4,@15),AMDNLR(AMDSBPTR)                      0903 02189000
         BNE   @RT00903                                            0903 02190000
         CLC   AMDIREC(4,@15),AMDIREC(AMDSBPTR)                    0903 02191000
         BNE   @RT00903                                            0903 02192000
         CLC   AMDDELR(4,@15),AMDDELR(AMDSBPTR)                    0903 02193000
         BNE   @RT00903                                            0903 02194000
         CLC   AMDUPR(4,@15),AMDUPR(AMDSBPTR)                      0903 02195000
         BE    @RF00903                                            0903 02196000
@RT00903 DS    0H                                                  0904 02197000
*           DO;                     /* IF ANY COUNT HAS CHANGED      */ 02198000
*             RSRETCD2=RSERR196;    /* SET RETURN CODE TO 196        */ 02199000
         L     @15,RSTWAPTR                                        0905 02200000
         MVI   RSRETCD2(@15),X'C4'                                 0905 02201000
*             CALL ERREXIT;         /* ISSUE MESSAGE AND EXIT        */ 02202000
         BAL   @14,ERREXIT                                         0906 02203000
*           END;                                                   0907 02204000
*       END;                                                       0908 02205000
@RF00903 LA    @15,1                                               0908 02206000
         AL    @15,LC7                                             0908 02207000
         ST    @15,LC7                                             0908 02208000
@DE00900 L     @05,AMBLPTR                                         0908 02209000
         CH    @15,AMBLNUM(,@05)                                   0908 02210000
         BNH   @DL00900                                            0908 02211000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 02212000
@RF00899 DS    0H                                                  0909 02213000
@RF00898 L     @05,RETCONT                                         0909 02214000
         LR    @15,@05                                             0909 02215000
         SLA   @15,2                                               0909 02216000
         L     RET,RETSAVE-4(@15)                                  0909 02217000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 02218000
         BCTR  @05,0                                               0910 02219000
         ST    @05,RETCONT                                         0910 02220000
*   RESPECIFY                                                      0911 02221000
*     RET UNRESTRICTED;                                            0911 02222000
*   END MODTEST;                                                   0912 02223000
@EL00011 DS    0H                                                  0912 02224000
@EF00011 DS    0H                                                  0912 02225000
@ER00011 BR    @14                                                 0912 02226000
*REPOSITN:                                                         0913 02227000
*                                                                  0913 02228000
*/********************************************************************/ 02229000
*/*                                                                  */ 02230000
*/*   FUNCTION - DETERMINES IF REPOSITIONING WAS REQUESTED AND, IF   */ 02231000
*/*              IT WAS, DETERMINES IF THE REQUEST IS VALID.  IF THE */ 02232000
*/*              REQUEST IS VALID, REPOSITIONING IS PERFORMED.  IF   */ 02233000
*/*              THE CLUSTER IS A KSDS IN CREATE MODE, REPOSITIONING */ 02234000
*/*              IS PERFORMED.  IF REPOSITIONING WAS NOT REQUESTED,  */ 02235000
*/*              A VERIFY IS ISSUED.                                 */ 02236000
*/*   INPUT    - A POINTER TO THE CURRENT AMBL IN AMBLPTR            */ 02237000
*/*              A POINTER TO THE CURRENT VCRT IN VCRTPTR            */ 02238000
*/*   OUTPUT   - IF REPOSITIONING IS REQUIRED, THE HIGH USED RBA IS  */ 02239000
*/*              RESET ITS THE VALUE AT CHECKPOINT TIME.             */ 02240000
*/*                                                                  */ 02241000
*/********************************************************************/ 02242000
*                                                                  0913 02243000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0913 02244000
REPOSITN DS    0H                                                  0914 02245000
*   RESPECIFY                                                      0914 02246000
*     RET RESTRICTED;                                              0914 02247000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 02248000
         LA    @15,1                                               0915 02249000
         AL    @15,RETCONT                                         0915 02250000
         ST    @15,RETCONT                                         0915 02251000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 02252000
         SLA   @15,2                                               0916 02253000
         ST    RET,RETSAVE-4(@15)                                  0916 02254000
*   RESPECIFY                                                      0917 02255000
*     RET UNRESTRICTED;                                            0917 02256000
*   AMBPTR=AMBLDTA;                 /* POINT TO DATA AMB FOR DS TYPE */ 02257000
         L     @05,AMBLPTR                                         0918 02258000
         L     @05,AMBLDTA(,@05)                                   0918 02259000
         ST    @05,AMBPTR                                          0918 02260000
*   ARDBPTR=AMDPARDB;               /* POINT TO FIRST ARDB           */ 02261000
         L     @01,AMBDSB(,@05)                                    0919 02262000
         L     @15,AMDPARDB(,@01)                                  0919 02263000
         ST    @15,ARDBPTR                                         0919 02264000
*   IF AMBCREAT=ON&ARDHRBA^=ZERO|   /* IF CREATE MODE AND NOT NULL   */ 02265000
*       ACBCRNRE=OFF&               /* OR REPOSITIONING REQUESTED    */ 02266000
*       AMDDST=OFF&                 /* AND IF NOT A KSDS             */ 02267000
*       AMDRRDS=OFF&                /* AND NOT AN RRDS, SO AN ESDS,  */ 02268000
*       AMBLWRIT=ON&                /* AND OPEN FOR OUTPUT           */ 02269000
*       ARDHRBA^=ZERO&              /* AND OBJECT IS NOT EMPTY AND   */ 02270000
*       BIBUPT=ZERO THEN            /* IMMEDIATE UPGRADE NOT OPEN    */ 02271000
