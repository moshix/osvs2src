*   BLPBFLST(1)=ON;                 /* SET THE LAST ENTRY INDICATOR  */ 00655000
         L     @14,BLPBUFLP(,@14)                                  0483 00656000
         OI    BLPBFLST(@14),B'10000000'                           0483 00657000
*   BLPBFLCT(1)=THREE;              /* SET THE BUFFER COUNT TO THREE */ 00658000
         MVC   BLPBFLCT(2,@14),@CH00032                            0484 00659000
*   AMBLPTR=VATPAMBL;               /* POINT TO FIRST PRIMARY AMBL   */ 00660000
         L     @05,PSATOLD                                         0485 00661000
