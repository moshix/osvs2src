         LA    @05,OPWSFI(,RCORE)                                  0313 00141000
         STCM  @05,7,MWASFIPT                                      0313 00142000
*       MWAQNAME='SYSVSAM ';        /* INITIALIZE QNAME      @ZA04523*/ 00143000
         MVC   MWAQNAME(8),@CC05328                                0314 00144000
*       DO LC0=TWO TO ONE BY-ONE;   /* INIT BOTH ENQ ENTRIES @ZA04523*/ 00145000
         LA    LC0,2                                               0315 00146000
@DL00315 DS    0H                                                  0316 00147000
*         MWARNIND(LC0)='B';        /* SET BUSY IN RNAME     @ZA04523*/ 00148000
