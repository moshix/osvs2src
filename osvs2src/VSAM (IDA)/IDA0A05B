@EF00009 DS    0H                                                  0763 01673000
@ER00009 BR    @14                                                 0763 01674000
*BLDOPEN:                                                          0764 01675000
*                                                                  0764 01676000
*/********************************************************************/ 01677000
*/*                                                                  */ 01678000
*/*   FUNCTION - UPDATE THE CONTROL BLOCK STRUCTURE FOR VCRT OPEN    */ 01679000
*/*              ENTRIES IF THEY ARE NOT IN THE IMMEDIATE UPGRADE    */ 01680000
*/*              SET.  PERFORM REPOSITIONING FOR KSDS CREATE MODE    */ 01681000
*/*              OR ESDS OUTPUT WITH REPOSITIONING SPECIFIED.        */ 01682000
*/*   INPUT    - A POINTER TO THE CURRENT VCRT IN VCRTPTR            */ 01683000
*/*   OUTPUT   - A COMPLETED CONTROL BLOCK STRUCTURE FOR THE VCRT    */ 01684000
*/*              OPEN ENTRIES.                                       */ 01685000
*/*                                                                  */ 01686000
*/********************************************************************/ 01687000
*                                                                  0764 01688000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0764 01689000
BLDOPEN  DS    0H                                                  0765 01690000
*   RESPECIFY                                                      0765 01691000
*     RET RESTRICTED;                                              0765 01692000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 01693000
         LA    @15,1                                               0766 01694000
         L     @05,RETCONT                                         0766 01695000
         ALR   @05,@15                                             0766 01696000
         ST    @05,RETCONT                                         0766 01697000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 01698000
         SLA   @05,2                                               0767 01699000
         ST    RET,RETSAVE-4(@05)                                  0767 01700000
*   RESPECIFY                                                      0768 01701000
*     RET UNRESTRICTED;                                            0768 01702000
*   DO LC5=ONE TO VCROPNCT BY ONE;  /* PROCESS ALL OPEN ENTRIES      */ 01703000
         ST    @15,LC5                                             0769 01704000
         B     @DE00769                                            0769 01705000
@DL00769 DS    0H                                                  0770 01706000
*     AMBLPTR=VCRAMBL(LC5);         /* POINT TO OPEN ENTRY AMBL      */ 01707000
         SLA   @15,3                                               0770 01708000
         L     @05,VCRTPTR                                         0770 01709000
         L     @05,VCROPN(,@05)                                    0770 01710000
         AL    @05,@CF05432                                        0770 01711000
         L     @05,VCRAMBL-4(@15,@05)                              0770 01712000
         ST    @05,AMBLPTR                                         0770 01713000
*     RACB=AMBLACB;                 /* ADDRESSIBILITY FOR ACB        */ 01714000
         L     @15,AMBLACB(,@05)                                   0771 01715000
         LR    RACB,@15                                            0771 01716000
*     OPWUACB=AMBLACB;              /* POINT TO ACB TO POST ACBERFLG */ 01717000
         ST    @15,OPWUACB(,RCORE)                                 0772 01718000
*     OPWCSTRN=CMBNST;              /* NUMBER OF STRINGS FOR CLUSTER */ 01719000
*                                                                  0773 01720000
         L     @01,AMBLCMB(,@05)                                   0773 01721000
         IC    @00,CMBNST(,@01)                                    0773 01722000
         STC   @00,OPWCSTRN(,RCORE)                                0773 01723000
*     /***************************************************************/ 01724000
*     /*                                                             */ 01725000
*     /* IF THE AMBL IS NOT A BASE AMBL, AND THEREFORE A PATH AMBL,  */ 01726000
*     /* SET UP THE OPEN CONTROL BLOCKS FOR OPENING AN AIX PATH      */ 01727000
*     /*                                                             */ 01728000
*     /***************************************************************/ 01729000
*                                                                  0774 01730000
*     IF AMBLBASE=OFF THEN          /* IF IS THIS A PATH ENTRY       */ 01731000
