         L     @05,AMDPARDB(,@05)                                  0941 02367000
         ST    @05,ARDBPTR                                         0941 02368000
*             DO UNTIL ARDBPTR=ZERO;/* SCAN ALL ARDBS FOR HI LEVEL   */ 02369000
@DL00942 DS    0H                                                  0943 02370000
*               IF ARDHLI=ON THEN   /* IS ARDB FOR HI LEVEL INDEX    */ 02371000
         L     @05,ARDBPTR                                         0943 02372000
         TM    ARDHLI(@05),B'01000000'                             0943 02373000
         BNO   @RF00943                                            0943 02374000
*                 IF ARDVOLSR^=MWAVOLSR THEN/* COMPARE VOLUME SERIAL */ 02375000
         L     @01,ENTRYPTR                                        0944 02376000
         CLC   ARDVOLSR(6,@05),MWAVOLSR(@01)                       0944 02377000
         BE    @RF00944                                            0944 02378000
*                   DO;             /* IF VOLUME SERIAL HAS CHANGED  */ 02379000
*                     RSRETCD2=RSERR198;/* IND A NEW VOLUME IN ARDB  */ 02380000
         L     @05,RSTWAPTR                                        0946 02381000
         MVI   RSRETCD2(@05),X'C6'                                 0946 02382000
*                     CALL ERREXIT; /* ISSUE MESSAGE AND EXIT        */ 02383000
         BAL   @14,ERREXIT                                         0947 02384000
*                   END;                                           0948 02385000
*                 ELSE              /* OTHERWISE,                    */ 02386000
*                   DO;                                            0949 02387000
         B     @RC00944                                            0949 02388000
@RF00944 DS    0H                                                  0950 02389000
*                     ARDHRBA=MWAHURBA;/* RESTORE HIGH USED RBA      */ 02390000
         L     @05,ENTRYPTR                                        0950 02391000
         L     @05,MWAHURBA(,@05)                                  0950 02392000
         L     @01,ARDBPTR                                         0950 02393000
         ST    @05,ARDHRBA(,@01)                                   0950 02394000
*                     ARDBPTR=ZERO; /* SET ARDB POINTER TO ZERO      */ 02395000
         SLR   @05,@05                                             0951 02396000
         ST    @05,ARDBPTR                                         0951 02397000
*                   END;                                           0952 02398000
*               ELSE                /* NOT FOR HI LEVEL INDEX        */ 02399000
*                 ARDBPTR=ARDNPTR;  /* POINT TO NEXT ARDB            */ 02400000
         B     @RC00943                                            0953 02401000
@RF00943 L     @05,ARDBPTR                                         0953 02402000
         L     @05,ARDNPTR(,@05)                                   0953 02403000
         ST    @05,ARDBPTR                                         0953 02404000
*             END;                                                 0954 02405000
@RC00943 DS    0H                                                  0954 02406000
@DE00942 L     @05,ARDBPTR                                         0954 02407000
         LTR   @05,@05                                             0954 02408000
         BNZ   @DL00942                                            0954 02409000
*           END;                                                   0955 02410000
*       END;                                                       0956 02411000
@RC00924 LA    @05,1                                               0956 02412000
         AL    @05,LC8                                             0956 02413000
         ST    @05,LC8                                             0956 02414000
@DE00922 L     @01,AMBLPTR                                         0956 02415000
         CH    @05,AMBLNUM(,@01)                                   0956 02416000
         BNH   @DL00922                                            0956 02417000
*       CALL IOSAVE;                /* SAVE USER CONTROL BLOCKS      */ 02418000
         BAL   @14,IOSAVE                                          0957 02419000
*       IF AMBSPEED=OFF THEN        /* SPEED OPTION NOT REQUESTED    */ 02420000
         L     @05,AMBPTR                                          0958 02421000
         TM    AMBSPEED(@05),B'00001000'                           0958 02422000
         BNZ   @RF00958                                            0958 02423000
*         CALL PREFRMAT;            /* PREFORMAT THE CLUSTER         */ 02424000
         BAL   @14,PREFRMAT                                        0959 02425000
