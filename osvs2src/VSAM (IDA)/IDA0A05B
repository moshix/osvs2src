         BNO   @RF00882                                            0882 02081000
*       DO;                         /*                       @ZA04523*/ 02082000
*         ENQEND(1)=OFF;            /* END OFF IN 1ST ENTRY  @ZA04523*/ 02083000
         NI    ENQEND,B'01111111'                                  0884 02084000
*         CALL BLDUPGRD;            /* BUILD THE UPGRADE STRUCTURE   */ 02085000
         BAL   @14,BLDUPGRD                                        0885 02086000
*       END;                        /*                       @ZA04523*/ 02087000
*   END;                                                           0887 02088000
@RF00882 LA    @15,1                                               0887 02089000
         AL    @15,LC5                                             0887 02090000
         ST    @15,LC5                                             0887 02091000
