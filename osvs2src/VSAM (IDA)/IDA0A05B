         TITLE 'IDA0A05B - VS2 VSAM RESTART                            *00001000
                        '                                               00002000
IDA0A05B CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0A05B  78.174'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @12,4095(,@03)                                      0001 00012000
         USING @PSTART,@03                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
JSCBDISP EQU 188                                                        00015000
JSCBAOS1 EQU  72                                                        00016000
JSCBAOS2 EQU 192                                                        00017000
RBPRFXLN EQU 64                                                         00018000
RBPRFLNA EQU 32                                                         00019000
PRBLEN   EQU 136                                                        00020000
SIRBLEN  EQU 200                                                        00021000
TIRBLEN  EQU 136                                                        00022000
IRBLEN   EQU 128                                                        00023000
SVRBLEN  EQU 224                                                        00024000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00025000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00026000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00027000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00028000
*                                       IF NO ERROR WAS ENCOUNTERED     00029000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00030000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00031000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00032000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00033000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00034000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00035000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00036000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00037000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00038000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00039000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00040000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00041000
         B     GMN00200                 BYPASS SETTING ERROR            00042000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00043000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00044000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00045000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00046000
*   R11=RETCODE;                    /* SAVE ADDRESS OF MODULE W.A.   */ 00047000
         LR    R11,RETCODE                                         0284 00048000
*   R0=(LENGTH(SP250WA)+SEVEN)&-DWORDLEN;/* LENGTH UNPROTECTED W.A.  */ 00049000
         LA    R0,152                                              0285 00050000
*   GEN(GETMAIN RC,LV=(0),SP=250);  /* GET CORE FOR UNPROTECTED W.A. */ 00051000
         GETMAIN RC,LV=(0),SP=250                                       00052000
*   IF RETCODE^=ZERO|R11=ZERO THEN  /* IF NO CORE AVAILABLE          */ 00053000
         SLR   @14,@14                                             0287 00054000
         CR    RETCODE,@14                                         0287 00055000
         BNE   @RT00287                                            0287 00056000
         CR    R11,@14                                             0287 00057000
         BNE   @RF00287                                            0287 00058000
@RT00287 DS    0H                                                  0288 00059000
*     SAREG(1)->RSRETCD2=RSERR195;  /* SET RESTART RETURN CODE       */ 00060000
         L     @14,SAREG(,R13)                                     0288 00061000
         MVI   RSRETCD2(@14),X'C3'                                 0288 00062000
*                                                                  0289 00063000
*   /*****************************************************************/ 00064000
*   /*                                                               */ 00065000
*   /* NO ERRORS WERE DETECTED OBTAINING THE MODULE WORK AREAS, SO   */ 00066000
*   /* CONTINUE WITH MODULE INITIALIZATION                           */ 00067000
*   /*                                                               */ 00068000
*   /*****************************************************************/ 00069000
*                                                                  0289 00070000
*   ELSE                            /* NO ERROR                      */ 00071000
*     DO;                           /* MAINLINE                      */ 00072000
         B     @RC00287                                            0289 00073000
@RF00287 DS    0H                                                  0290 00074000
*       R11->SABWRD=R13;            /* CHAIN THE SAVE AREA           */ 00075000
         ST    R13,SABWRD(,R11)                                    0290 00076000
*       R13=R11;                    /* LOAD PTR TO AUTOMATICS        */ 00077000
         LR    R13,R11                                             0291 00078000
*       GEN(USING @DATD,R13);       /* GET BASE FOR AUTOMATICS       */ 00079000
         USING @DATD,R13                                                00080000
*       MWASP250=R1;                /* SAVE THE POINTER TO THE UWA   */ 00081000
         ST    R1,MWASP250                                         0293 00082000
*       RSTWAPTR=SABWRD->SAREG(1);  /* POINTER TO RESTART WORK AREA  */ 00083000
         L     @14,SABWRD(,R13)                                    0294 00084000
         L     @14,SAREG(,@14)                                     0294 00085000
         ST    @14,RSTWAPTR                                        0294 00086000
*       RETCONT=ZERO;               /* INIT ARRAY CONTROL            */ 00087000
         SLR   @14,@14                                             0295 00088000
         ST    @14,RETCONT                                         0295 00089000
*       SAVBASEB=RBASEB;            /* SAVE SECOND BASE REGISTER     */ 00090000
         ST    RBASEB,SAVBASEB                                     0296 00091000
*       CALL RECOVERY;              /* EST. RECOVERY ENVIRONMENT     */ 00092000
         BAL   @14,RECOVERY                                        0297 00093000
*       TBLPTR=ADDR(ARDBTAB(1));    /* ADDRESSIBILITY FOR ARDBTBL    */ 00094000
         LA    @14,ARDBTAB                                         0298 00095000
         ST    @14,TBLPTR                                          0298 00096000
*       ADDR(SP250SA)->SABWRD=ADDR(SAVAREA);/* PT UWA SA TO SAVEAREA */ 00097000
         L     @14,MWASP250                                        0299 00098000
         ST    R13,SABWRD(,@14)                                    0299 00099000
*       RESPECIFY                                                  0300 00100000
*        (RETCODE,                                                 0300 00101000
*         R11,                                                     0300 00102000
*         R1,                                                      0300 00103000
*         R0) UNRESTRICTED;                                        0300 00104000
*                                                                  0300 00105000
*       /*************************************************************/ 00106000
*       /*                                                           */ 00107000
