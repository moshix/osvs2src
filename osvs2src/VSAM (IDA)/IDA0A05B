*       GEN(CLOSE MF=(E,MWACLPRM),TYPE=T);/* T-CLOSE TO UPDATE CTLG  */ 02463000
         CLOSE MF=(E,MWACLPRM),TYPE=T                                   02464000
*       LM(RACB,R12,XRBESA);        /* RESTORE RESTARTS REGISTERS    */ 02465000
         L     @05,PSATOLD                                         0973 02466000
         L     @05,TCBRBP(,@05)                                    0973 02467000
         LM    RACB,R12,XRBESA(@05)                                0973 02468000
*       RESPECIFY                                                  0974 02469000
*         R1 UNRESTRICTED;                                         0974 02470000
*       IF ACBERFLG>OPERR128 THEN   /* IF ANY ERRORS ON THE T-CLOSE  */ 02471000
         CLI   ACBERFLG(RACB),128                                  0975 02472000
         BNH   @RF00975                                            0975 02473000
*         DO;                       /* THEN                          */ 02474000
*           RSRETCD2=ACBERFLG/FOUR+ERRBASE;/* SET THE ERROR CODE AND */ 02475000
         SLR   @05,@05                                             0977 02476000
         IC    @05,ACBERFLG(,RACB)                                 0977 02477000
         SRL   @05,2                                               0977 02478000
         AL    @05,@CF02132                                        0977 02479000
         L     @01,RSTWAPTR                                        0977 02480000
         STC   @05,RSRETCD2(,@01)                                  0977 02481000
*           CALL ERREXIT;           /* ISSUE THE MESSAGE AND EXIT    */ 02482000
         BAL   @14,ERREXIT                                         0978 02483000
*         END;                                                     0979 02484000
*     END;                          /* END OF REPOSITIONING CODE     */ 02485000
*                                                                  0981 02486000
*   /*****************************************************************/ 02487000
*   /*                                                               */ 02488000
*   /* WE HAVE DETERMINED THAT REPOSITIONING IS NOT REQUIRED, SO THE */ 02489000
*   /* VERIFY ROUTINE WILL BE CALLED IF OPENED FOR OUTPUT AND THE    */ 02490000
*   /* CLUSTER HAS NOT ALREADY BEEN PROCESSED AS PART OF THE UPGRADE */ 02491000
*   /* SET.                                                          */ 02492000
*   /*                                                               */ 02493000
*   /*****************************************************************/ 02494000
*                                                                  0981 02495000
*   ELSE                                                           0981 02496000
*     DO;                           /* VERIFY AND DEQUEUE    @ZA04523*/ 02497000
         B     @RC00920                                            0981 02498000
@RF00920 DS    0H                                                  0982 02499000
*       IF AMBPUG=OFF&              /* IF AMB NOT IN UPGRADE         */ 02500000
*           AMBLWRIT=ON THEN        /* AND OPEN FOR OUTPUT           */ 02501000
         L     @05,AMBPTR                                          0982 02502000
         TM    AMBPUG(@05),B'10000000'                             0982 02503000
         BNZ   @RF00982                                            0982 02504000
         L     @05,AMBLPTR                                         0982 02505000
         TM    AMBLWRIT(@05),B'00100000'                           0982 02506000
         BNO   @RF00982                                            0982 02507000
*         CALL VERIFY;              /* ISSUE A VERIFY ON THE CLUSTER */ 02508000
         BAL   @14,VERIFY                                          0983 02509000
*       RESPECIFY                                                  0984 02510000
*         R1 RESTRICTED;            /*                       @ZA04523*/ 02511000
@RF00982 DS    0H                                                  0985 02512000
*       R1=ADDR(MWENQPRM);          /* PT TO ENQ PARM LIST   @ZA04523*/ 02513000
         LA    R1,MWENQPRM                                         0985 02514000
*       GEN(DEQ  (,,,,),MF=(E,(1)));/* DEQUEUE CLUSTER       @ZA04523*/ 02515000
         DEQ  (,,,,),MF=(E,(1))                                         02516000
*     END;                          /* END NO REPOSITION PROCESSING  */ 02517000
*   RESPECIFY                                                      0988 02518000
*     RET RESTRICTED;                                              0988 02519000
@RC00920 DS    0H                                                  0989 02520000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 02521000
         L     @05,RETCONT                                         0989 02522000
         LR    @15,@05                                             0989 02523000
         SLA   @15,2                                               0989 02524000
         L     RET,RETSAVE-4(@15)                                  0989 02525000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 02526000
         BCTR  @05,0                                               0990 02527000
         ST    @05,RETCONT                                         0990 02528000
*   RESPECIFY                                                      0991 02529000
*    (R1,                                                          0991 02530000
*     RET) UNRESTRICTED;                                           0991 02531000
*   END REPOSITN;                                                  0992 02532000
@EL00012 DS    0H                                                  0992 02533000
@EF00012 DS    0H                                                  0992 02534000
@ER00012 BR    @14                                                 0992 02535000
*PREFRMAT:                                                         0993 02536000
*                                                                  0993 02537000
*/********************************************************************/ 02538000
*/*                                                                  */ 02539000
*/*   FUNCTION - PREFORMATS ALL CONTROL INTERVALS IN THE CONTROL     */ 02540000
*/*              AREA ASSOCIATED WITH THE CHECKPOINTED HIGH USED RBA */ 02541000
*/*              PLUS THE FIRST CI IN THE NEXT CA BY INVOKING THE    */ 02542000
*/*              PREFORMAT FUNCTION OF RECORD MANAGEMENT.            */ 02543000
*/*   INPUT    - A POINTER TO THE CURRENT AMBL IN AMBLPTR            */ 02544000
*/*   OUTPUT   - THE CONTROL AREA IS PREFORMATTED                    */ 02545000
*/*                                                                  */ 02546000
*/********************************************************************/ 02547000
*                                                                  0993 02548000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0993 02549000
PREFRMAT DS    0H                                                  0994 02550000
*   RESPECIFY                                                      0994 02551000
*     RET RESTRICTED;                                              0994 02552000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 02553000
         LA    @15,1                                               0995 02554000
         AL    @15,RETCONT                                         0995 02555000
         ST    @15,RETCONT                                         0995 02556000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 02557000
         SLA   @15,2                                               0996 02558000
         ST    RET,RETSAVE-4(@15)                                  0996 02559000
*   RESPECIFY                                                      0997 02560000
*     RET UNRESTRICTED;                                            0997 02561000
*   AMBPTR=ADDR(MWAAMBD);           /* POINT TO DATA AMB             */ 02562000
         LA    @05,MWAAMBD                                         0998 02563000
         ST    @05,AMBPTR                                          0998 02564000
*   IFGRPL=IFGRPL&&IFGRPL;          /* CLEAR THE RPL                 */ 02565000
         LA    @15,72                                              0999 02566000
         AL    @15,MWASP250                                        0999 02567000
         XC    IFGRPL(76,@15),IFGRPL(@15)                          0999 02568000
*   RPLDACB=ADDR(IFGACB);           /* SET THE ADDRESS OF THE ACB    */ 02569000
         ST    RACB,RPLDACB(,@15)                                  1000 02570000
*   IF AMDDST=ON|AMDRRDS=ON THEN    /* IF A KSDS OR RRDS,    @ZA03934*/ 02571000
         L     @01,AMBDSB(,@05)                                    1001 02572000
         TM    AMDDST(@01),B'10000010'                             1001 02573000
         BZ    @RF01001                                            1001 02574000
*     RPLDDDD=BUFCORBA;             /* PREFORMAT FROM BUFC OUTPUT RBA   02575000
*                                                            @ZA03934*/ 02576000
         L     @05,AMBPH(,@05)                                     1002 02577000
         L     @05,PLHDBUFC+16(,@05)                               1002 02578000
         L     @05,BUFCORBA(,@05)                                  1002 02579000
         ST    @05,RPLDDDD(,@15)                                   1002 02580000
*   ELSE                            /* OTHERWISE,                    */ 02581000
*     RPLDDDD=(AMDPARDB->ARDHRBA)-AMDCINV;/* PREFORMAT FROM HIGH USED   02582000
*                                      RBA MINUS LENGTH OF ONE CI    */ 02583000
         B     @RC01001                                            1003 02584000
@RF01001 L     @05,AMBPTR                                          1003 02585000
         L     @05,AMBDSB(,@05)                                    1003 02586000
         L     @01,AMDPARDB(,@05)                                  1003 02587000
         L     @15,ARDHRBA(,@01)                                   1003 02588000
         SL    @15,AMDCINV(,@05)                                   1003 02589000
         L     @05,MWASP250                                        1003 02590000
         ST    @15,RPLDDDD+72(,@05)                                1003 02591000
*   RESPECIFY                                                      1004 02592000
*    (R0,                                                          1004 02593000
*     R1,                                                          1004 02594000
*     R15) RESTRICTED;                                             1004 02595000
@RC01001 DS    0H                                                  1005 02596000
*   R0=RPLPFMTD;                    /* INDICATE PREFORMAT REQUEST    */ 02597000
         LA    R0,8                                                1005 02598000
*   R1=ADDR(IFGRPL);                /* POINT TO RPL FOR R/M          */ 02599000
         L     @14,MWASP250                                        1006 02600000
         LA    R1,72                                               1006 02601000
         ALR   R1,@14                                              1006 02602000
*   R15=BIBINTRF;                   /* POINT TO INTERFACE ROUTINE    */ 02603000
         L     @05,OPWBIB(,RCORE)                                  1007 02604000
         L     R15,BIBINTRF(,@05)                                  1007 02605000
*   STM(RACB,R13,XRBESA);           /* SAVE REGISTERS IN RB SAVEAREA */ 02606000
         L     @05,PSATOLD                                         1008 02607000
         L     @05,TCBRBP(,@05)                                    1008 02608000
         STM   RACB,R13,XRBESA(@05)                                1008 02609000
*   R13=ADDR(SP250SA);              /* PT TO USER SUBPOOL SAVE AREA  */ 02610000
         LR    R13,@14                                             1009 02611000
*   GEN(SYNCH (15));                /* GO TO R/M IN USERS KEY        */ 02612000
