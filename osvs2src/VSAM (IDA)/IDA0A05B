*/********************************************************************/ 00745000
*/*                                                                  */ 00746000
*/*   FUNCTION - RELEASES THE HEADER ELEMENTS FOR THE DEBS AND EDBS  */ 00747000
*/*              IF THE SPHERE WAS OPENED WITH THE LSR OPTION.       */ 00748000
*/*   INPUT    - THE VCRT IN VCRTPTR                                 */ 00749000
*/*   OUTPUT   - HEADER ELEMENTS ARE CLEARED AND PUT ON THE FREE Q.  */ 00750000
*/*                                                                  */ 00751000
*/********************************************************************/ 00752000
*                                                                  0507 00753000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0507 00754000
FREEHEBS DS    0H                                                  0508 00755000
*   RESPECIFY                                                      0508 00756000
*     RET RESTRICTED;                                              0508 00757000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 00758000
         LA    @15,1                                               0509 00759000
         L     @05,RETCONT                                         0509 00760000
         ALR   @05,@15                                             0509 00761000
         ST    @05,RETCONT                                         0509 00762000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 00763000
         SLA   @05,2                                               0510 00764000
         ST    RET,RETSAVE-4(@05)                                  0510 00765000
*   RESPECIFY                                                      0511 00766000
*     RET UNRESTRICTED;                                            0511 00767000
*                                                                  0511 00768000
*   /*****************************************************************/ 00769000
*   /*                                                               */ 00770000
*   /* RELEASE HEADER ELEMENTS FOR ALL UPGRADE ENTRIES               */ 00771000
*   /*                                                               */ 00772000
*   /*****************************************************************/ 00773000
*                                                                  0512 00774000
*   DO LC1=1 TO VCRUPGCT BY 1;      /* PROCESS ALL UPGRADE ENTRIES   */ 00775000
         ST    @15,LC1                                             0512 00776000
         B     @DE00512                                            0512 00777000
@DL00512 DS    0H                                                  0513 00778000
*     AMBLPTR=VCRUAMBL(LC1);        /* ADDRESSIBILITY FOR AMBL       */ 00779000
         SLA   @15,3                                               0513 00780000
         L     @05,VCRTPTR                                         0513 00781000
         L     @05,VCRUPG(,@05)                                    0513 00782000
         AL    @05,@CF05432                                        0513 00783000
         L     @05,VCRUAMBL-4(@15,@05)                             0513 00784000
         ST    @05,AMBLPTR                                         0513 00785000
*     DO WHILE CMBDEBPT^=ZERO;      /* PROCESS DEB HEADER ELEMENTS   */ 00786000
         B     @DE00514                                            0514 00787000
@DL00514 DS    0H                                                  0515 00788000
*       HDRELPTR=CMBDEBPT;          /* POINT TO DEB HEADER ELEMENT   */ 00789000
         L     @01,AMBLPTR                                         0515 00790000
         L     @15,AMBLCMB(,@01)                                   0515 00791000
         L     HDRELPTR,CMBDEBPT(,@15)                             0515 00792000
*       CMBDEBPT=HEBELCHN;          /* UPDATE POINTER IN CMB         */ 00793000
         L     @14,HEBELCHN(,HDRELPTR)                             0516 00794000
         ST    @14,CMBDEBPT(,@15)                                  0516 00795000
*       HEBHDREL=HEBHDREL&&HEBHDREL;/* CLEAR THE HEADER ELEMENT      */ 00796000
         XC    HEBHDREL(24,HDRELPTR),HEBHDREL(HDRELPTR)            0517 00797000
*       HEBELCHN=BIBHEBFQ;          /* CHAIN TO FREE QUEUE ELEMENT   */ 00798000
         L     @15,OPWBIB(,RCORE)                                  0518 00799000
         L     @14,BIBHEBFQ(,@15)                                  0518 00800000
         ST    @14,HEBELCHN(,HDRELPTR)                             0518 00801000
*       BIBHEBFQ=ADDR(HEBHDREL);    /* PUT AT TOP OF FREE QUEUE      */ 00802000
         ST    HDRELPTR,BIBHEBFQ(,@15)                             0519 00803000
*     END;                          /* END DEB HEADER PROCESSING     */ 00804000
@DE00514 L     @05,AMBLPTR                                         0520 00805000
         L     @05,AMBLCMB(,@05)                                   0520 00806000
         L     @05,CMBDEBPT(,@05)                                  0520 00807000
         LTR   @05,@05                                             0520 00808000
         BNZ   @DL00514                                            0520 00809000
*     DO WHILE CMBEDBPT^=ZERO;      /* PROCESS EDB HEADER ELEMENTS   */ 00810000
         B     @DE00521                                            0521 00811000
@DL00521 DS    0H                                                  0522 00812000
*       HDRELPTR=CMBEDBPT;          /* POINT TO EDB HEADER ELEMENT   */ 00813000
         L     @01,AMBLPTR                                         0522 00814000
         L     @15,AMBLCMB(,@01)                                   0522 00815000
         L     HDRELPTR,CMBEDBPT(,@15)                             0522 00816000
*       CMBEDBPT=HEBELCHN;          /* UPDATE POINTER IN CMB         */ 00817000
         L     @14,HEBELCHN(,HDRELPTR)                             0523 00818000
         ST    @14,CMBEDBPT(,@15)                                  0523 00819000
*       HEBHDREL=HEBHDREL&&HEBHDREL;/* CLEAR THE HEADER ELEMENT      */ 00820000
         XC    HEBHDREL(24,HDRELPTR),HEBHDREL(HDRELPTR)            0524 00821000
*       HEBELCHN=BIBHEBFQ;          /* CHAIN TO FREE QUEUE ELEMENT   */ 00822000
         L     @15,OPWBIB(,RCORE)                                  0525 00823000
         L     @14,BIBHEBFQ(,@15)                                  0525 00824000
         ST    @14,HEBELCHN(,HDRELPTR)                             0525 00825000
*       BIBHEBFQ=ADDR(HEBHDREL);    /* PUT AT TOP OF FREE QUEUE      */ 00826000
         ST    HDRELPTR,BIBHEBFQ(,@15)                             0526 00827000
*     END;                          /* END EDB HEADER PROCESSING     */ 00828000
@DE00521 L     @05,AMBLPTR                                         0527 00829000
         L     @05,AMBLCMB(,@05)                                   0527 00830000
         L     @05,CMBEDBPT(,@05)                                  0527 00831000
         LTR   @05,@05                                             0527 00832000
         BNZ   @DL00521                                            0527 00833000
*   END;                                                           0528 00834000
         LA    @15,1                                               0528 00835000
         AL    @15,LC1                                             0528 00836000
         ST    @15,LC1                                             0528 00837000
@DE00512 L     @05,VCRTPTR                                         0528 00838000
         CH    @15,VCRUPGCT(,@05)                                  0528 00839000
         BNH   @DL00512                                            0528 00840000
*                                                                  0529 00841000
*   /*****************************************************************/ 00842000
*   /*                                                               */ 00843000
*   /* RELEASE HEADER ELEMENTS FOR ALL OPEN ENTRIES                  */ 00844000
*   /*                                                               */ 00845000
*   /*****************************************************************/ 00846000
*                                                                  0529 00847000
*   DO LC1=ONE TO VCROPNCT BY ONE;  /* PROCESS ALL OPEN ENTRIES      */ 00848000
         LA    @05,1                                               0529 00849000
         ST    @05,LC1                                             0529 00850000
         B     @DE00529                                            0529 00851000
@DL00529 DS    0H                                                  0530 00852000
*     AMBLPTR=VCRAMBL(LC1);         /* ADDRESSIBILITY FOR AMBL       */ 00853000
         SLA   @05,3                                               0530 00854000
         L     @01,VCRTPTR                                         0530 00855000
         L     @01,VCROPN(,@01)                                    0530 00856000
         AL    @01,@CF05432                                        0530 00857000
         L     @05,VCRAMBL-4(@05,@01)                              0530 00858000
         ST    @05,AMBLPTR                                         0530 00859000
*     DO WHILE CMBDEBPT^=ZERO;      /* PROCESS DEB HEADER ELEMENTS   */ 00860000
         B     @DE00531                                            0531 00861000
@DL00531 DS    0H                                                  0532 00862000
*       HDRELPTR=CMBDEBPT;          /* POINT TO DEB HEADER ELEMENT   */ 00863000
         L     @01,AMBLPTR                                         0532 00864000
         L     @15,AMBLCMB(,@01)                                   0532 00865000
         L     HDRELPTR,CMBDEBPT(,@15)                             0532 00866000
*       CMBDEBPT=HEBELCHN;          /* UPDATE POINTER IN CMB         */ 00867000
         L     @14,HEBELCHN(,HDRELPTR)                             0533 00868000
         ST    @14,CMBDEBPT(,@15)                                  0533 00869000
*       HEBHDREL=HEBHDREL&&HEBHDREL;/* CLEAR THE HEADER ELEMENT      */ 00870000
         XC    HEBHDREL(24,HDRELPTR),HEBHDREL(HDRELPTR)            0534 00871000
*       HEBELCHN=BIBHEBFQ;          /* CHAIN TO FREE QUEUE ELEMENT   */ 00872000
         L     @15,OPWBIB(,RCORE)                                  0535 00873000
         L     @14,BIBHEBFQ(,@15)                                  0535 00874000
         ST    @14,HEBELCHN(,HDRELPTR)                             0535 00875000
*       BIBHEBFQ=ADDR(HEBHDREL);    /* PUT AT TOP OF FREE QUEUE      */ 00876000
         ST    HDRELPTR,BIBHEBFQ(,@15)                             0536 00877000
*     END;                          /* END DEB HEADER PROCESSING     */ 00878000
@DE00531 L     @05,AMBLPTR                                         0537 00879000
         L     @05,AMBLCMB(,@05)                                   0537 00880000
         L     @05,CMBDEBPT(,@05)                                  0537 00881000
         LTR   @05,@05                                             0537 00882000
         BNZ   @DL00531                                            0537 00883000
*     DO WHILE CMBEDBPT^=ZERO;      /* PROCESS EDB HEADER ELEMENTS   */ 00884000
         B     @DE00538                                            0538 00885000
@DL00538 DS    0H                                                  0539 00886000
*       HDRELPTR=CMBEDBPT;          /* POINT TO EDB HEADER ELEMENT   */ 00887000
         L     @01,AMBLPTR                                         0539 00888000
         L     @15,AMBLCMB(,@01)                                   0539 00889000
         L     HDRELPTR,CMBEDBPT(,@15)                             0539 00890000
*       CMBEDBPT=HEBELCHN;          /* UPDATE POINTER IN CMB         */ 00891000
         L     @14,HEBELCHN(,HDRELPTR)                             0540 00892000
         ST    @14,CMBEDBPT(,@15)                                  0540 00893000
*       HEBHDREL=HEBHDREL&&HEBHDREL;/* CLEAR THE HEADER ELEMENT      */ 00894000
         XC    HEBHDREL(24,HDRELPTR),HEBHDREL(HDRELPTR)            0541 00895000
*       HEBELCHN=BIBHEBFQ;          /* CHAIN TO FREE QUEUE ELEMENT   */ 00896000
         L     @15,OPWBIB(,RCORE)                                  0542 00897000
         L     @14,BIBHEBFQ(,@15)                                  0542 00898000
         ST    @14,HEBELCHN(,HDRELPTR)                             0542 00899000
*       BIBHEBFQ=ADDR(HEBHDREL);    /* PUT AT TOP OF FREE QUEUE      */ 00900000
         ST    HDRELPTR,BIBHEBFQ(,@15)                             0543 00901000
*     END;                          /* END EDB HEADER PROCESSING     */ 00902000
@DE00538 L     @05,AMBLPTR                                         0544 00903000
         L     @05,AMBLCMB(,@05)                                   0544 00904000
         L     @05,CMBEDBPT(,@05)                                  0544 00905000
         LTR   @05,@05                                             0544 00906000
         BNZ   @DL00538                                            0544 00907000
*   END;                                                           0545 00908000
         LA    @05,1                                               0545 00909000
         AL    @05,LC1                                             0545 00910000
         ST    @05,LC1                                             0545 00911000
@DE00529 L     @01,VCRTPTR                                         0545 00912000
         CH    @05,VCROPNCT(,@01)                                  0545 00913000
         BNH   @DL00529                                            0545 00914000
*   RESPECIFY                                                      0546 00915000
*     RET RESTRICTED;                                              0546 00916000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 00917000
         L     @05,RETCONT                                         0547 00918000
         LR    @15,@05                                             0547 00919000
         SLA   @15,2                                               0547 00920000
         L     RET,RETSAVE-4(@15)                                  0547 00921000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 00922000
         BCTR  @05,0                                               0548 00923000
         ST    @05,RETCONT                                         0548 00924000
*   RESPECIFY                                                      0549 00925000
*     RET UNRESTRICTED;                                            0549 00926000
*   END FREEHEBS;                   /*                       @ZA04230*/ 00927000
@EL00005 DS    0H                                                  0550 00928000
@EF00005 DS    0H                                                  0550 00929000
@ER00005 BR    @14                                                 0550 00930000
*BLDVGTT:                                                          0551 00931000
*                                                                  0551 00932000
*/********************************************************************/ 00933000
*/*                                                                  */ 00934000
*/*   FUNCTION - BUILDS THE VGTT FOR THE SPHERE AND CONNECTS IT TO   */ 00935000
*/*              THE BIB, AND TO THE ASCB FOR THE MEMORY             */ 00936000
*/*   INPUT    - THE PRIMARY AMBL IN PAMBLPTR                        */ 00937000
*/*   OUTPUT   - THE VGTT INITIALIZED FOR THE SPHERE                 */ 00938000
*/*                                                                  */ 00939000
*/********************************************************************/ 00940000
*                                                                  0551 00941000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0551 00942000
BLDVGTT  DS    0H                                                  0552 00943000
*   RESPECIFY                                                      0552 00944000
*     RET RESTRICTED;                                              0552 00945000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 00946000
         LA    @15,1                                               0553 00947000
         AL    @15,RETCONT                                         0553 00948000
         ST    @15,RETCONT                                         0553 00949000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 00950000
         SLA   @15,2                                               0554 00951000
         ST    RET,RETSAVE-4(@15)                                  0554 00952000
*   RESPECIFY                                                      0555 00953000
*     RET UNRESTRICTED;                                            0555 00954000
*   OPWVMSP=PRTSPPL;                /* GET VGTT FROM SUBPOOL 241     */ 00955000
         MVI   OPWVMSP(RCORE),X'F1'                                0556 00956000
*   OPWVMLNG=(LENGTH(IDAVGTT)+DWORDLEN-1)&-DWORDLEN;/* LNGTH OF VGTT */ 00957000
         MVC   OPWVMLNG(3,RCORE),@CF00348+1                        0557 00958000
*   OPWVMTYP=DIRCTREQ;              /* INDICATE A DIRECT REQUEST     */ 00959000
         MVI   OPWVMTYP(RCORE),X'85'                               0558 00960000
*   OPWVMFLG=ZERO;                  /* CLEAR THE FLAG BYTE           */ 00961000
         MVI   OPWVMFLG(RCORE),X'00'                               0559 00962000
*   OPWVMNSL=ON;                    /* DO NOT BUILD A CSL ENTRY      */ 00963000
         OI    OPWVMNSL(RCORE),B'00001000'                         0560 00964000
*   CALL IDA0192M;                  /* CALL THE VIRTUAL STORAGE MGR  */ 00965000
         L     @15,@CV05282                                        0561 00966000
         BALR  @14,@15                                             0561 00967000
*   RSTATRTN=IDARA05B;              /* NOTE IDA0A05B BACK IN CONTROL */ 00968000
         LA    @05,MWAFCORE                                        0562 00969000
         MVC   RSTATRTN(4,@05),@CC05018                            0562 00970000
*   IF OPWVMADR=ZERO THEN           /* SEE IF STORAGE WAS OBTAINED   */ 00971000
         L     @05,OPWVMADR(,RCORE)                                0563 00972000
         LTR   @05,@05                                             0563 00973000
         BNZ   @RF00563                                            0563 00974000
*     DO;                           /* IF NOT,                       */ 00975000
*       RSRETCD2=RSERR195;          /* INDICATE NO CORE AVAILABLE    */ 00976000
         L     @05,RSTWAPTR                                        0565 00977000
         MVI   RSRETCD2(@05),X'C3'                                 0565 00978000
*       CALL ERREXIT;               /* AND RETURN TO VS RESTART      */ 00979000
         BAL   @14,ERREXIT                                         0566 00980000
*     END;                                                         0567 00981000
*   VGTTID=VGTTNAME;                /* INITIALIZE ID                 */ 00982000
@RF00563 L     @05,OPWVMADR(,RCORE)                                0568 00983000
         MVC   VGTTID(4,@05),@CC04941                              0568 00984000
*   VGTTOPEN=ON;                    /* INDICATE SPHERE VGTT          */ 00985000
         OI    VGTTOPEN(@05),B'00001000'                           0569 00986000
*   VGTTSP=OPWVMSP;                 /* INITIALIZE SUBPOOL IN VGTT    */ 00987000
         IC    @15,OPWVMSP(,RCORE)                                 0570 00988000
         STC   @15,VGTTSP(,@05)                                    0570 00989000
*   VGTTSIZE=OPWVMLNG;              /* INITIALIZE LENGTH IN VGTT     */ 00990000
         L     @15,OPWVMLNG-1(,RCORE)                              0571 00991000
         LA    @15,0(,@15)                                         0571 00992000
         ST    @15,VGTTSIZE(,@05)                                  0571 00993000
*   DO UNTIL CSWORD1=VGTTNEXT;      /* EQUAL IF CS SUCCESSFUL        */ 00994000
@DL00572 DS    0H                                                  0573 00995000
*     VGTTNEXT=ASCBVGTT;            /* GET ADDRESS OF TOP VGTT       */ 00996000
         L     @07,OPWVMADR(,RCORE)                                0573 00997000
         L     @15,PSAAOLD                                         0573 00998000
         L     @14,ASCBVGTT(,@15)                                  0573 00999000
         ST    @14,VGTTNEXT(,@07)                                  0573 01000000
*     CSWORD1=VGTTNEXT;             /* INITIALIZE COMPARE OPERAND    */ 01001000
         LR    CSWORD1,@14                                         0574 01002000
*     CSWORD2=ADDR(IDAVGTT);        /* INITIALIZE REPLACE OPERAND    */ 01003000
         LR    CSWORD2,@07                                         0575 01004000
*     CS(CSWORD1,CSWORD2,ASCBVGTT); /* COMPARE AND SWAP              */ 01005000
         CS    CSWORD1,@06,ASCBVGTT(@15)                           0576 01006000
*   END;                            /* DO AGAIN IF NOT SUCCESSFUL    */ 01007000
@DE00572 L     @15,OPWVMADR(,RCORE)                                0577 01008000
         C     CSWORD1,VGTTNEXT(,@15)                              0577 01009000
         BNE   @DL00572                                            0577 01010000
*   VGTTBIB=ADDR(IDABIB);           /* VGTT POINTS TO THE BIB        */ 01011000
         L     @05,OPWBIB(,RCORE)                                  0578 01012000
         ST    @05,VGTTBIB(,@15)                                   0578 01013000
*   BIBVGTT=ADDR(IDAVGTT);          /* BIB POINTS BACK TO THE VGTT   */ 01014000
         ST    @15,BIBVGTT(,@05)                                   0579 01015000
*   RESPECIFY                                                      0580 01016000
*     RET RESTRICTED;                                              0580 01017000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 01018000
         L     @05,RETCONT                                         0581 01019000
         LR    @15,@05                                             0581 01020000
         SLA   @15,2                                               0581 01021000
         L     RET,RETSAVE-4(@15)                                  0581 01022000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 01023000
         BCTR  @05,0                                               0582 01024000
         ST    @05,RETCONT                                         0582 01025000
*   RESPECIFY                                                      0583 01026000
*     RET UNRESTRICTED;                                            0583 01027000
*   END BLDVGTT;                                                   0584 01028000
@EL00006 DS    0H                                                  0584 01029000
@EF00006 DS    0H                                                  0584 01030000
@ER00006 BR    @14                                                 0584 01031000
*BLDPSBLK:                                                         0585 01032000
*                                                                  0585 01033000
*/********************************************************************/ 01034000
*/*                                                                  */ 01035000
*/*   FUNCTION - REBUILDS THE CMB HEB CHAINS FOR THE SPHERE WITH THE */ 01036000
*/*              EXCEPTION OF THE EDB AND DEB ENTRIES, AND THE FIXED */ 01037000
*/*              PROTECTED STRING BLOCKS (UPGRADE AND NON-UPGRADE)   */ 01038000
*/*   INPUT    - THE CURRENT VCRT IN VCRTPTR                         */ 01039000
*/*   OUTPUT   - THE HEB ELEMENTS FOR THE SPHERE INITIALIZED         */ 01040000
*/*                                                                  */ 01041000
*/********************************************************************/ 01042000
*                                                                  0585 01043000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0585 01044000
BLDPSBLK DS    0H                                                  0586 01045000
*   RESPECIFY                                                      0586 01046000
*     RET RESTRICTED;                                              0586 01047000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 01048000
         LA    @15,1                                               0587 01049000
         AL    @15,RETCONT                                         0587 01050000
         ST    @15,RETCONT                                         0587 01051000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 01052000
         SLA   @15,2                                               0588 01053000
         ST    RET,RETSAVE-4(@15)                                  0588 01054000
*   RESPECIFY                                                      0589 01055000
*     RET UNRESTRICTED;                                            0589 01056000
*   BIBHEBPT=ZERO;                  /* ZERO BIB HEB POINTER  @ZA04523*/ 01057000
         SLR   @07,@07                                             0590 01058000
         L     @05,OPWBIB(,RCORE)                                  0590 01059000
         ST    @07,BIBHEBPT(,@05)                                  0590 01060000
*   BIBPRSPH=ZERO;                  /* ZERO BIB PSB POINTER  @ZA04523*/ 01061000
         ST    @07,BIBPRSPH(,@05)                                  0591 01062000
*   ELEMNUM=HEBNEL+ONE;             /* FORCE BLDHEB TO OBTAIN A HEB  */ 01063000
*                                                                  0592 01064000
         LA    ELEMNUM,17                                          0592 01065000
*   /*****************************************************************/ 01066000
*   /*                                                               */ 01067000
*   /* REBUILD SPHERE BLOCK HEADER ELEMENTS                          */ 01068000
*   /*                                                               */ 01069000
*   /*****************************************************************/ 01070000
*                                                                  0593 01071000
*   LC1=ZERO;                       /* ZERO C1 TO TRIGGER SHPERE BLD */ 01072000
         ST    @07,LC1                                             0593 01073000
*   BIBSPHPT=ZERO;                  /* CLEAR OLD HEB POINTER IN BIB  */ 01074000
         ST    @07,BIBSPHPT(,@05)                                  0594 01075000
*   HEBSAPTR=VCRSPHPT;              /* ADDRESSIBILITY FOR HEB SAVE   */ 01076000
         L     @05,VCRTPTR                                         0595 01077000
         L     @05,VCRSPHPT(,@05)                                  0595 01078000
         ST    @05,HEBSAPTR                                        0595 01079000
*   IF VCRSPHPT^=ZERO THEN          /* IF THERE IS A SPHERE BLOCK    */ 01080000
         CR    @05,@07                                             0596 01081000
         BE    @RF00596                                            0596 01082000
*     CALL BLDHEBS;                 /* THEN REBUILD HEADER ELEMENTS  */ 01083000
*                                                                  0597 01084000
         BAL   @14,BLDHEBS                                         0597 01085000
*   /*****************************************************************/ 01086000
*   /*                                                               */ 01087000
*   /* REBUILD HEADER ELEMENTS FOR ALL UPGRADE ENTRIES               */ 01088000
*   /*                                                               */ 01089000
*   /*****************************************************************/ 01090000
*                                                                  0598 01091000
*   DO LC1=1 TO VCRUPGCT BY 1;      /* PROCESS ALL UPGRADE ENTRIES   */ 01092000
@RF00596 LA    @07,1                                               0598 01093000
         ST    @07,LC1                                             0598 01094000
         B     @DE00598                                            0598 01095000
@DL00598 DS    0H                                                  0599 01096000
*     HEBSAPTR=VCRUHEBS(LC1);       /* ADDRESSIBILITY FOR HEB SAVE   */ 01097000
         SLA   @07,3                                               0599 01098000
         L     @05,VCRTPTR                                         0599 01099000
         L     @05,VCRUPG(,@05)                                    0599 01100000
         LA    @01,0(@07,@05)                                      0599 01101000
         AL    @01,@CF05420                                        0599 01102000
         L     @15,VCRUHEBS(,@01)                                  0599 01103000
         ST    @15,HEBSAPTR                                        0599 01104000
*     AMBLPTR=VCRUAMBL(LC1);        /* ADDRESSIBILITY FOR AMBL       */ 01105000
         AL    @05,@CF05432                                        0600 01106000
         L     @07,VCRUAMBL-4(@07,@05)                             0600 01107000
         ST    @07,AMBLPTR                                         0600 01108000
*     CMBPTRS=CMBPTRS&&CMBPTRS;     /* ZERO POINTERS TO HDR ELEMENTS */ 01109000
         L     @07,AMBLCMB(,@07)                                   0601 01110000
         XC    CMBPTRS(56,@07),CMBPTRS(@07)                        0601 01111000
*     DO UNTIL HEBSAPTR=ZERO;       /* PROCESS ALL HEB SAVE HEADERS  */ 01112000
@DL00602 DS    0H                                                  0603 01113000
*       IF VCRHFREL=OFF THEN        /* SEE IF CORE IS TO BE FREED    */ 01114000
         L     @07,HEBSAPTR                                        0603 01115000
         TM    VCRHFREL(@07),B'01000000'                           0603 01116000
