*                                                                  1035 02702000
*   /*****************************************************************/ 02703000
*   /*                                                               */ 02704000
*   /* RESET THE INDEX CONTROL INTERVALS                             */ 02705000
*   /*                                                               */ 02706000
*   /*****************************************************************/ 02707000
*                                                                  1035 02708000
*   DO LC9=ONE TO VCRIDXCT BY ONE;  /* PROCESS ALL INDEX ENTRIES     */ 02709000
         ST    @15,LC9                                             1035 02710000
         B     @DE01035                                            1035 02711000
@DL01035 DS    0H                                                  1036 02712000
*     RPLARG=ADDR(ICWCRBA);         /* POINT TO THE RBA TO UPDATE    */ 02713000
