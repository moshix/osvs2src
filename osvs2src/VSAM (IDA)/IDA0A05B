*         HEBID=IDXHEB;             /* INITIALIZE HEB HEADER ID      */ 01319000
         MVI   HEBID(@07),B'00010011'                              0656 01320000
*         HEBLEN=LENGTH(IDAHEB);    /* SET LENGTH IN HEADER          */ 01321000
