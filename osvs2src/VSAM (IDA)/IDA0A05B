         L     @07,RETCONT                                         0638 01268000
         ALR   @07,@15                                             0638 01269000
         ST    @07,RETCONT                                         0638 01270000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 01271000
         SLA   @07,2                                               0639 01272000
         ST    RET,RETSAVE-4(@07)                                  0639 01273000
*   RESPECIFY                                                      0640 01274000
*     RET UNRESTRICTED;                                            0640 01275000
*   DO LC2=ONE TO VCRHNENT BY ONE;  /* PROCESS ALL HEADER ELEMENTS   */ 01276000
*                                                                  0641 01277000
         ST    @15,LC2                                             0641 01278000
         B     @DE00641                                            0641 01279000
@DL00641 DS    0H                                                  0642 01280000
*     /***************************************************************/ 01281000
*     /*                                                             */ 01282000
*     /* OBTAIN A NEW HEB IF THERE ARE NO FREE HEADER ELEMENTS       */ 01283000
*     /*                                                             */ 01284000
*     /***************************************************************/ 01285000
*                                                                  0642 01286000
*     IF ELEMNUM>HEBNEL THEN        /* IS A HEADER ELEMENT AVAILABLE */ 01287000
         CL    ELEMNUM,@CF00050                                    0642 01288000
         BNH   @RF00642                                            0642 01289000
*       DO;                         /* NO, OBTAIN AND INITIALIZE ONE */ 01290000
*         OPWVMLNG=LENGTH(IDAHEB);  /* SET REQUEST LENGTH            */ 01291000
         MVC   OPWVMLNG(3,RCORE),@CF05426+1                        0644 01292000
*         OPWVMTYP=PRSPHBLK;        /* PROTECTED SPHERE BLOCK REQ    */ 01293000
         MVI   OPWVMTYP(RCORE),X'81'                               0645 01294000
*         OPWVMFLG=ZERO;            /* CLEAR THE FLAG BYTE           */ 01295000
         MVI   OPWVMFLG(RCORE),X'00'                               0646 01296000
*         OPWVMNSL=ON;              /* NO CSL WANTED                 */ 01297000
         OI    OPWVMNSL(RCORE),B'00001000'                         0647 01298000
*         CALL IDA0192M;            /* CALL STORAGE MANAGER          */ 01299000
         L     @15,@CV05282                                        0648 01300000
