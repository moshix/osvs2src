*   BLPBUFL(1)=BLPBUFL(1)&&BLPBUFL(1);/* CLEAR BUFFER LIST   @ZA33504*/ 00643000
         XC    BLPBUFL(8,@05),BLPBUFL(@05)                         0479 00644000
*   IF MWAVPTR^=ZERO THEN           /* IF AN LSR POOL EXISTS         */ 00645000
         L     @05,MWAVPTR                                         0480 00646000
         LTR   @05,@05                                             0480 00647000
         BZ    @RF00480                                            0480 00648000
*     BLPKEYLN=MWAVPTR->VSRTKL;     /* SET THE KEY LENGTH            */ 00649000
         IC    @11,VSRTKL(,@05)                                    0481 00650000
         STC   @11,BLPKEYLN(,@14)                                  0481 00651000
*   BLPSTRNO=ONE;                   /* GET ONE STRING                */ 00652000
