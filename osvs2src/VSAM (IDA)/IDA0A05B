*             CALL ERREXIT;         /* AND RETURN TO VS RESTART      */ 01313000
         BAL   @14,ERREXIT                                         0653 01314000
*           END;                                                   0654 01315000
*         HEBPTR=OPWVMADR;          /* UPDATE CURRENT HEB PTR        */ 01316000
@RF00650 L     @07,OPWVMADR(,RCORE)                                0655 01317000
