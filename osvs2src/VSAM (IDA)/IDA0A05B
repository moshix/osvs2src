         BZ    @RF00962                                            0962 02439000
*           CALL PUTRTN;            /* CALL THE CI PUT ROUTINE       */ 02440000
         BAL   @14,PUTRTN                                          0963 02441000
*       CALL IORESTOR;              /* RESTORE USER CONTROL BLOCKS   */ 02442000
@RF00962 DS    0H                                                  0964 02443000
@RC00960 BAL   @14,IORESTOR                                        0964 02444000
*       MWACLPRM=ADDR(IFGACB);      /* POINT TO ACB FOR REPOSITION   */ 02445000
         ST    RACB,MWACLPRM                                       0965 02446000
*       MWALASTE=ON;                /* LAST ENTRY IN PARAMETER LIST  */ 02447000
         OI    MWALASTE,B'10000000'                                0966 02448000
*       RESPECIFY                                                  0967 02449000
*         R1 RESTRICTED;                                           0967 02450000
*       R1=ADDR(MWENQPRM);          /* PT TO ENQ PARM LIST   @ZA04523*/ 02451000
         LA    R1,MWENQPRM                                         0968 02452000
*       GEN(DEQ  (,,,,),MF=(E,(1)));/* DEQUEUE CLUSTER       @ZA04523*/ 02453000
         DEQ  (,,,,),MF=(E,(1))                                         02454000
*       STM(RACB,R12,XRBESA);       /* SAVE RESTARTS REGISTERS       */ 02455000
         L     @05,PSATOLD                                         0970 02456000
         L     @05,TCBRBP(,@05)                                    0970 02457000
