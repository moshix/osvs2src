@EF00008 DS    0H                                                  0680 01406000
@ER00008 BR    @14                                                 0680 01407000
*BLDUPGRD:                                                         0681 01408000
*                                                                  0681 01409000
*/********************************************************************/ 01410000
*/*                                                                  */ 01411000
*/*   FUNCTION - VALIDITY CHECK THE IMMEDIATE UPGRADE SET AND        */ 01412000
*/*              REBUILD THE ARDBS, DEBS, EDBS, AND GLOBAL BLOCKS    */ 01413000
*/*              FOR THE IMMEDIATE UPGRADE AMBLS. THIS ROUTINE IS    */ 01414000
*/*              ENTERED ONCE FOR EACH VCRT.                         */ 01415000
*/*   INPUT    - A POINTER TO THE CURRENT VCRT IN VCRTPTR            */ 01416000
*/*   OUTPUT   - A COMPLETE IMMEDIATE UPGRADE STRUCTURE              */ 01417000
*/*                                                                  */ 01418000
*/********************************************************************/ 01419000
*                                                                  0681 01420000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0681 01421000
BLDUPGRD DS    0H                                                  0682 01422000
*   RESPECIFY                                                      0682 01423000
*     RET RESTRICTED;                                              0682 01424000
*   OPWBASE=OFF;                    /* TURN OFF OPEN BASE INDICATOR  */ 01425000
         L     @15,OPWCURPT(,RCORE)                                0683 01426000
         NI    OPWBASE(@15),B'01111111'                            0683 01427000
*   IF VCRUPGCT=ZERO THEN           /* SEE IF UPGRADE SET AT CHECKPT */ 01428000
         L     @15,VCRTPTR                                         0684 01429000
         LH    @05,VCRUPGCT(,@15)                                  0684 01430000
         LTR   @05,@05                                             0684 01431000
         BNZ   @RF00684                                            0684 01432000
*     IF OPWNOUPG=ZERO THEN         /* NO, SEE IF THERE IS ONE NOW   */ 01433000
         CLI   OPWNOUPG(RCORE),0                                   0685 01434000
         BE    @RT00685                                            0685 01435000
*       RETURN;                     /* NO, RETURN TO MAINLINE        */ 01436000
*     ELSE                          /* THERE IS AN UPGRADE SET       */ 01437000
*       IF VCRAMBL(ONE)->AMBLWRIT=ON THEN/* IF OPEN FOR OUTPUT       */ 01438000
         L     @15,VCROPN(,@15)                                    0687 01439000
         L     @15,VCRAMBL(,@15)                                   0687 01440000
         TM    AMBLWRIT(@15),B'00100000'                           0687 01441000
         BNO   @RF00687                                            0687 01442000
*         DO;                                                      0688 01443000
*           RSRETCD2=RSERR194;      /* SET ERROR CODE AND            */ 01444000
         L     @15,RSTWAPTR                                        0689 01445000
         MVI   RSRETCD2(@15),X'C2'                                 0689 01446000
*           CALL ERREXIT;           /* EXIT THE MODULE               */ 01447000
         BAL   @14,ERREXIT                                         0690 01448000
*         END;                                                     0691 01449000
*       ELSE                        /* OTHERWISE,                    */ 01450000
*         RETURN;                   /* RETURN TO MAINLINE            */ 01451000
*   IF VCRUPGCT^=OPWNOUPG THEN      /* NUMBER OF UGRADE CHANGED      */ 01452000
@RF00684 L     @15,VCRTPTR                                         0693 01453000
         LH    @15,VCRUPGCT(,@15)                                  0693 01454000
         SLR   @05,@05                                             0693 01455000
         IC    @05,OPWNOUPG(,RCORE)                                0693 01456000
         CR    @15,@05                                             0693 01457000
         BE    @RF00693                                            0693 01458000
*     DO;                           /* YES                           */ 01459000
*       RSRETCD2=RSERR194;          /* SET ERROR CODE AND            */ 01460000
         L     @15,RSTWAPTR                                        0695 01461000
         MVI   RSRETCD2(@15),X'C2'                                 0695 01462000
*       CALL ERREXIT;               /* EXIT THE MODULE               */ 01463000
         BAL   @14,ERREXIT                                         0696 01464000
*     END;                                                         0697 01465000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 01466000
@RF00693 LA    @15,1                                               0698 01467000
         L     @05,RETCONT                                         0698 01468000
         ALR   @05,@15                                             0698 01469000
         ST    @05,RETCONT                                         0698 01470000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 01471000
         SLA   @05,2                                               0699 01472000
         ST    RET,RETSAVE-4(@05)                                  0699 01473000
*   RESPECIFY                                                      0700 01474000
*     RET UNRESTRICTED;                                            0700 01475000
*   VCRUPGSW=ON;                    /* SET UPGRADE IN PROCESS SWITCH */ 01476000
         L     @05,VCRTPTR                                         0701 01477000
         OI    VCRUPGSW(@05),B'10000000'                           0701 01478000
*   OPWCSTRN=ONE;                   /* ONE STRING FOR UPGRADE        */ 01479000
         MVI   OPWCSTRN(RCORE),X'01'                               0702 01480000
*   DO LC3=ONE TO VCRUPGCT BY ONE;  /* PROCESS VCRT UPGRADE ENTRIES  */ 01481000
         ST    @15,LC3                                             0703 01482000
         B     @DE00703                                            0703 01483000
@DL00703 DS    0H                                                  0704 01484000
*     AMBLPTR=VCRUAMBL(LC3);        /* POINT TO CURRENT AMBL         */ 01485000
         LR    @05,@15                                             0704 01486000
         SLA   @05,3                                               0704 01487000
         L     @01,VCRTPTR                                         0704 01488000
