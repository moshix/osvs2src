         L     @05,JSCBSHR(,@05)                                   0485 00663000
         L     @05,VATPAMBL(,@05)                                  0485 00664000
         ST    @05,AMBLPTR                                         0485 00665000
*   BLPBUFSZ(1)=PAGE;               /* INITIZLIZE SIZE       @ZA08304*/ 00666000
         MVC   BLPBUFSZ(4,@14),@CF05067                            0486 00667000
*   DO WHILE AMBLPTR^=ZERO;         /* SEARCH ALL VCRTS      @ZA08304*/ 00668000
*                                                                  0487 00669000
         B     @DE00487                                            0487 00670000
@DL00487 DS    0H                                                  0488 00671000
*     /***************************************************************/ 00672000
*     /*                                                             */ 00673000
*     /* GET LARGEST CI SIZE FROM THE VRCTS AND USE AS BUFFER SIZE   */ 00674000
*     /*                                                             */ 00675000
*     /***************************************************************/ 00676000
*                                                                  0488 00677000
*     BLPBUFSZ(1)=MAX(BLPBUFSZ(1),AMBLBIB->BIBVCRT->VCRCISIZ);     0488 00678000
         LA    @14,MWABLPRM                                        0488 00679000
         L     @14,BLPBUFLP(,@14)                                  0488 00680000
         L     @05,AMBLPTR                                         0488 00681000
