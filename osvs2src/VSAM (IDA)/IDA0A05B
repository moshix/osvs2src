*       CALL ERREXIT;               /* ISSUE MESSAGE AND EXIT        */ 03017000
         BAL   @14,ERREXIT                                         1128 03018000
*     END;                                                         1129 03019000
*   CALL IORESTOR;                  /* RESTORE CONTROL BLOCK STATUS  */ 03020000
@RF01125 BAL   @14,IORESTOR                                        1130 03021000
*   RESPECIFY                                                      1131 03022000
*     RET RESTRICTED;                                              1131 03023000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 03024000
