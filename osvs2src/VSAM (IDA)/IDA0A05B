         BNO   @GL00013                                            0920 02273000
         L     @05,ARDHRBA(,@15)                                   0920 02274000
         LTR   @05,@05                                             0920 02275000
         BNZ   @RT00920                                            0920 02276000
@GL00013 TM    ACBCRNRE(RACB),B'01000000'                          0920 02277000
         BNZ   @RF00920                                            0920 02278000
         L     @05,AMBPTR                                          0920 02279000
         L     @05,AMBDSB(,@05)                                    0920 02280000
         TM    AMDDST(@05),B'10000010'                             0920 02281000
         BNZ   @RF00920                                            0920 02282000
         L     @05,AMBLPTR                                         0920 02283000
         TM    AMBLWRIT(@05),B'00100000'                           0920 02284000
         BNO   @RF00920                                            0920 02285000
         SLR   @05,@05                                             0920 02286000
         L     @01,ARDBPTR                                         0920 02287000
         CL    @05,ARDHRBA(,@01)                                   0920 02288000
         BE    @RF00920                                            0920 02289000
         L     @01,OPWBIB(,RCORE)                                  0920 02290000
         C     @05,BIBUPT(,@01)                                    0920 02291000
         BNE   @RF00920                                            0920 02292000
@RT00920 DS    0H                                                  0921 02293000
*     DO;                                                          0921 02294000
*                                                                  0921 02295000
*       /*************************************************************/ 02296000
*       /*                                                           */ 02297000
*       /* IT HAS BEEN DETERMINED THAT REPOSITIONING IS REQUIRED, SO */ 02298000
*       /* CHECK THE ARDBS TO SEE IF THE CLUSTER HAS EXTENDED TO     */ 02299000
*       /* ANOTHER VOLUME AND PERFORM REPOSITIONING FUNCTIONS.       */ 02300000
*       /*                                                           */ 02301000
*       /*************************************************************/ 02302000
*                                                                  0922 02303000
*       DO LC8=ONE TO AMBLNUM BY ONE;/* PROCESS THE AMBS             */ 02304000
         LA    @05,1                                               0922 02305000
         ST    @05,LC8                                             0922 02306000
         B     @DE00922                                            0922 02307000
@DL00922 DS    0H                                                  0923 02308000
*         AMBPTR=AMBPTRS(LC8);      /* ADDRESSIBILITY TO AMB         */ 02309000
         LR    @15,@05                                             0923 02310000
         SLA   @15,2                                               0923 02311000
         L     @01,AMBLPTR                                         0923 02312000
         L     @15,AMBPTRS-4(@15,@01)                              0923 02313000
         ST    @15,AMBPTR                                          0923 02314000
*         IF LC8=ONE THEN           /* IF DATA AMB                   */ 02315000
         C     @05,@CF00053                                        0924 02316000
         BNE   @RF00924                                            0924 02317000
*           DO;                                                    0925 02318000
*             ENTRYPTR=ADDR(ARDBTBL(1));/* INIT PT TO ARDBTBL ENTRY  */ 02319000
         L     @05,TBLPTR                                          0926 02320000
