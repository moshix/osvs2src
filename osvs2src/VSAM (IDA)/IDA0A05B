         STM   RACB,R13,XRBESA(@14)                                1120 02996000
*   R13=ADDR(SP250SA);              /* POINT REG 13 AT USER SP S.A.  */ 02997000
         LR    R13,@05                                             1121 02998000
*   GEN(SYNCH (15));                /* SYNCH TO INTERFACE ROUTINE    */ 02999000
         SYNCH (15)                                                     03000000
*   LM(RACB,R13,XRBESA);            /* RESTORE RESTARTS REGISTERS    */ 03001000
         L     @14,PSATOLD                                         1123 03002000
         L     @14,TCBRBP(,@14)                                    1123 03003000
         LM    RACB,R13,XRBESA(@14)                                1123 03004000
*   RESPECIFY                                                      1124 03005000
*    (R0,                                                          1124 03006000
*     R1,                                                          1124 03007000
*     R15) UNRESTRICTED;                                           1124 03008000
*   IF RPLRTNCD^=ZERO THEN          /* IF ANY ERRORS ON VERIFY       */ 03009000
         L     @05,MWASP250                                        1125 03010000
         CLI   RPLRTNCD+72(@05),0                                  1125 03011000
         BE    @RF01125                                            1125 03012000
*     DO;                                                          1126 03013000
*       RSRETCD2=RSERR182;          /* INDICATE VERIFY ERROR         */ 03014000
         L     @05,RSTWAPTR                                        1127 03015000
