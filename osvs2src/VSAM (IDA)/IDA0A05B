         L     @05,AMDPARDB(,@05)                                  0927 02324000
         ST    @05,ARDBPTR                                         0927 02325000
*             DO UNTIL ARDBPTR=ZERO;/* COMPARE ALL ARDBS             */ 02326000
@DL00928 DS    0H                                                  0929 02327000
*               IF ARDVOLSR^=MWAVOLSR THEN/* COMPARE VOLUME SERIAL   */ 02328000
