@RF00797 SLR   @05,@05                                             0802 01803000
         ST    @05,OPWCAMBL(,RCORE)                                0802 01804000
*       END;                                                       0803 01805000
*                                                                  0803 01806000
*     /***************************************************************/ 01807000
*     /*                                                             */ 01808000
*     /* IF THE AMBL IS THE BASE AMBL, IT IS THE FIRST ENTRY OF THE  */ 01809000
*     /* OPEN ENTRIES. A CALL TO BLDUPGRD WILL BE MADE ON THIS PASS. */ 01810000
*     /*                                                             */ 01811000
*     /***************************************************************/ 01812000
*                                                                  0804 01813000
*     ELSE                          /* AMBL IS FOR THE BASE CLUSTER  */ 01814000
*       DO;                                                        0804 01815000
         B     @RC00774                                            0804 01816000
@RF00774 DS    0H                                                  0805 01817000
*         OPWCURPT=ADDR(OPWBSECL);  /* POINT TO BASE CLUSTER         */ 01818000
         LA    @05,OPWBSECL(,RCORE)                                0805 01819000
