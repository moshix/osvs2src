*     DO;                                                          0497 00717000
*       RSRETCD2=RSERR199;          /* SET ERROR CODE IN RSTWA       */ 00718000
         L     @14,RSTWAPTR                                        0498 00719000
         MVI   RSRETCD2(@14),X'C7'                                 0498 00720000
*       CALL ERREXIT;               /* EXIT THE MODULE               */ 00721000
         BAL   @14,ERREXIT                                         0499 00722000
*     END;                                                         0500 00723000
*   RCORE=ADDR(MWAOPWRK);           /* POINT RCORE AT OPWRK          */ 00724000
@RF00496 LA    RCORE,MWAOPWRK                                      0501 00725000
*   RESPECIFY                                                      0502 00726000
*     RET RESTRICTED;                                              0502 00727000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 00728000
         L     @05,RETCONT                                         0503 00729000
         LR    @11,@05                                             0503 00730000
