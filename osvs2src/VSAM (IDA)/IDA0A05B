@EF00016 DS    0H                                                  1135 03039000
@ER00016 BR    @14                                                 1135 03040000
*IOSAVE:                                                           1136 03041000
*                                                                  1136 03042000
*/********************************************************************/ 03043000
*/*                                                                  */ 03044000
*/*   FUNCTION - SAVES INFORMATION FROM THE USER CONTROL BLOCKS. THE */ 03045000
*/*              AMBS ARE SAVED IN ENTIRETY AND THEN MODIFIED TO     */ 03046000
*/*              OPERATE WITH THE RESOURCE POOL CREATED FOR RESTART. */ 03047000
*/*   INPUT    - A POINTER TO THE CURRENT AMBL IN AMBLPTR            */ 03048000
*/*   OUTPUT   - A RESTART STRUCTURE FOR THE CLUSTER FOR ISSUING R/M */ 03049000
*/*              REQUESTS AGAINST THE RESTART RESOURCE POOL.         */ 03050000
*/*                                                                  */ 03051000
*/********************************************************************/ 03052000
*                                                                  1136 03053000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1136 03054000
IOSAVE   DS    0H                                                  1137 03055000
*   RESPECIFY                                                      1137 03056000
*     RET RESTRICTED;                                              1137 03057000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 03058000
         LA    @15,1                                               1138 03059000
         L     @05,RETCONT                                         1138 03060000
         ALR   @05,@15                                             1138 03061000
         ST    @05,RETCONT                                         1138 03062000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 03063000
         SLA   @05,2                                               1139 03064000
         ST    RET,RETSAVE-4(@05)                                  1139 03065000
*   RESPECIFY                                                      1140 03066000
*     RET UNRESTRICTED;                                            1140 03067000
*                                                                  1140 03068000
*   /*****************************************************************/ 03069000
*   /*                                                               */ 03070000
*   /* SAVE FIELDS FROM CONTROL BLOCKS SUBJECT TO CHANGE             */ 03071000
*   /*                                                               */ 03072000
*   /*****************************************************************/ 03073000
*                                                                  1141 03074000
*   MWAACBEX=ACBEXLST;              /* SAVE THE ACB EXIT LIST FIELD  */ 03075000
         L     @05,ACBEXLST(,RACB)                                 1141 03076000
         ST    @05,MWAACBEX                                        1141 03077000
*   ACBEXLST=ZERO;                  /* CLEAR THE ACB EXLST FIELD     */ 03078000
         SLR   @05,@05                                             1142 03079000
         ST    @05,ACBEXLST(,RACB)                                 1142 03080000
*   MWAAMBLQ=ACBAMBL->AMBLQ;        /* SAVE THE AMBL QUALIFIER       */ 03081000
         L     @05,ACBAMBL(,RACB)                                  1143 03082000
         MVC   MWAAMBLQ(1),AMBLQ(@05)                              1143 03083000
*   ACBAMBL->AMBLLSR=ON;            /* SET THE AMBL LSR FLAG         */ 03084000
         OI    AMBLLSR(@05),B'00100000'                            1144 03085000
*   MWAFLG1=ACBAMBL->AMBLFLG1;      /* SAVE THE FLAGS FROM THE AMBL  */ 03086000
         MVC   MWAFLG1(1),AMBLFLG1(@05)                            1145 03087000
*   ACBAMBL->AMBLFLG1=ZERO;         /* RESET THE FLAGS AND           */ 03088000
         MVI   AMBLFLG1(@05),X'00'                                 1146 03089000
*   ACBAMBL->AMBLFULL=ON;           /* INDICATE FULL ACCESS OK       */ 03090000
*   ACBAMBL->AMBLCINV=ON;           /* INDICATE CINV ACCESS OK       */ 03091000
         OI    AMBLFULL(@05),B'11000000'                           1148 03092000
*   DO LC10=ONE TO AMBLNUM BY ONE;  /* SETUP ALL AMBS FOR LSR        */ 03093000
*                                                                  1149 03094000
         LR    LC10,@15                                            1149 03095000
         B     @DE01149                                            1149 03096000
@DL01149 DS    0H                                                  1150 03097000
*     /***************************************************************/ 03098000
*     /*                                                             */ 03099000
*     /* SAVE THE ENTIRE AMB AND PREPARE IT FOR LSR PROCESSING       */ 03100000
*     /*                                                             */ 03101000
*     /***************************************************************/ 03102000
*                                                                  1150 03103000
*     AMBPTR=AMBPTRS(LC10);         /* POINT TO CURRENT AMB          */ 03104000
         LR    @06,LC10                                            1150 03105000
         SLA   @06,2                                               1150 03106000
         L     @05,AMBLPTR                                         1150 03107000
         L     @06,AMBPTRS-4(@06,@05)                              1150 03108000
         ST    @06,AMBPTR                                          1150 03109000
*     IF LC10=ONE THEN              /* IF THIS IS THE FIRST PASS     */ 03110000
