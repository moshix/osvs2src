*         BIBHEBPT=OPWVMADR;        /* CHAIN THIS HEB OFF THE BIB    */ 01327000
         ST    @07,BIBHEBPT(,@05)                                  0659 01328000
*         HEBCNT=HEBNEL;            /* SET NUMBER OF ELEMENTS IN HEB */ 01329000
         MVC   HEBCNT(2,@07),@CH00050                              0660 01330000
