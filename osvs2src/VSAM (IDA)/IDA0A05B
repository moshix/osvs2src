         MVC   MWALEN(3),@CF02149+1                                0389 00373000
*       MWAPTR=ADDR(SP250WA);       /* GET ADDRESS UNPROTECTED W.A.  */ 00374000
         L     @05,MWASP250                                        0390 00375000
         ST    @05,MWAPTR                                          0390 00376000
*       LM(R0,R1,MWAFREMN);         /* LOAD THE FREEMAIN REGISTERS   */ 00377000
         LM    R0,R1,MWAFREMN                                      0391 00378000
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE UNPROTECTED WORK AREA   */ 00379000
         FREEMAIN R,LV=(0),A=(1)                                        00380000
*       GEN(L R0,@SIZDATD);         /* LOAD R0 WITH SP & SIZE OF MWA */ 00381000
         L R0,@SIZDATD                                                  00382000
*       R1=R13;                     /* LOAD REG 1 WITH ADDRESS OF MWA*/ 00383000
         LR    R1,R13                                              0394 00384000
*       R13=SABWRD;                 /* POINT R13 AT PREVIOUS SAVE    */ 00385000
         L     R13,SABWRD(,R13)                                    0395 00386000
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE MODULE WORK AREA        */ 00387000
         FREEMAIN R,LV=(0),A=(1)                                        00388000
*       GEN(ESTAE 0);               /* RELEASE RECOVERY ENVIRONMENT  */ 00389000
         ESTAE 0                                                        00390000
*     END;                                                         0398 00391000
*   LM(RET,R12,SAREGS);             /* RESTORE CALLERS REGS          */ 00392000
@RC00287 LM    RET,R12,SAREGS(R13)                                 0399 00393000
*   GEN(BR RET);                    /* RETURN TO CALLER              */ 00394000
         BR RET                                                         00395000
*   RESPECIFY                                                      0401 00396000
*    (R0,                                                          0401 00397000
*     R1) UNRESTRICTED;                                            0401 00398000
*ERREXIT:                                                          0402 00399000
*                                                                  0402 00400000
*/********************************************************************/ 00401000
*/*                                                                  */ 00402000
*/*   FUNCTION - ISSUES A WRITE-TO-OPERATOR TO IDENTIFY THE DD       */ 00403000
*/*              ON WHICH THE ERROR OCCURRED AND EXITS THE MODULE.   */ 00404000
*/*   INPUT    - A POINTER TO THE TIOT ENTRY IN OPWTIOT              */ 00405000
*/*   OUTPUT   - MESSAGE IHJ009I IS ISSUED FOR ALL ERRORS EXCEPT 195 */ 00406000
*/*              (NOT ENOUGH STORAGE), 199 (BLDVRP FAILED), AND      */ 00407000
*/*              240 (UNABLE TO ESTABLISH ESTAE EXIT).               */ 00408000
*/*              THE MODULE IS EXITED                                */ 00409000
*/*                                                                  */ 00410000
*/********************************************************************/ 00411000
*                                                                  0402 00412000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0402 00413000
ERREXIT  DS    0H                                                  0403 00414000
*   RESPECIFY                                                      0403 00415000
*     RET RESTRICTED;                                              0403 00416000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 00417000
         LA    @15,1                                               0404 00418000
         AL    @15,RETCONT                                         0404 00419000
         ST    @15,RETCONT                                         0404 00420000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 00421000
         SLA   @15,2                                               0405 00422000
         ST    RET,RETSAVE-4(@15)                                  0405 00423000
*   RESPECIFY                                                      0406 00424000
*     RET UNRESTRICTED;                                            0406 00425000
*   RBASEB=SAVBASEB;                /* MAKE SURE BASE REG 12 IS OK   */ 00426000
         L     RBASEB,SAVBASEB                                     0407 00427000
*   IF RSRETCD2^=RSERR195&          /* IF WE AREN'T SHORT OF STORAGE */ 00428000
*       RSRETCD2^=RSERR199&         /* AND NOT A BLDVRP ERROR        */ 00429000
*       RSRETCD2<RSERR240 THEN      /* AND NOT A RECOVERY ERROR      */ 00430000
         L     @05,RSTWAPTR                                        0408 00431000
         CLI   RSRETCD2(@05),195                                   0408 00432000
         BE    @RF00408                                            0408 00433000
         CLI   RSRETCD2(@05),199                                   0408 00434000
         BE    @RF00408                                            0408 00435000
         CLI   RSRETCD2(@05),240                                   0408 00436000
         BNL   @RF00408                                            0408 00437000
*     DO;                           /* THEN                          */ 00438000
*       MSGAREA=MESSAGE;            /* MOVE IN MESSAGE SKELETON      */ 00439000
         MVC   MSGAREA(33),MESSAGE                                 0410 00440000
*       MSGDDNM=TIOEDDNM;           /* MOVE IN DDNAME                */ 00441000
         L     @05,OPWTIOT(,RCORE)                                 0411 00442000
         MVC   MSGDDNM(8),TIOEDDNM(@05)                            0411 00443000
*       RESPECIFY                                                  0412 00444000
*         R1 RESTRICTED;                                           0412 00445000
*       R1=ADDR(MSGAREA);           /* POINT TO MSG FOR WTO MACRO    */ 00446000
         LA    R1,MSGAREA                                          0413 00447000
*       GEN(WTO MF=(E,(1)));        /* TELL WHICH DD HAD THE ERROR   */ 00448000
         WTO MF=(E,(1))                                                 00449000
*       RESPECIFY                                                  0415 00450000
*         R1 UNRESTRICTED;                                         0415 00451000
*     END;                                                         0416 00452000
*   IF RSRETCD2^=RSERR240 THEN      /* IF ESTAE WAS ESTABLISHED      */ 00453000
@RF00408 L     @05,RSTWAPTR                                        0417 00454000
         CLI   RSRETCD2(@05),240                                   0417 00455000
         BE    @RF00417                                            0417 00456000
*     GEN(ESTAE 0);                 /* RELEASE RECOVERY ENVIRONMENT  */ 00457000
         ESTAE 0                                                        00458000
*   R13=SABWRD;                     /* POINT TO PREVIOUS SAVE AREA   */ 00459000
@RF00417 L     R13,SABWRD(,R13)                                    0419 00460000
*   LM(RET,R12,SAREGS);             /* RELOAD REGISTERS              */ 00461000
         LM    RET,R12,SAREGS(R13)                                 0420 00462000
*   GEN(BR RET);                    /* EXIT THE MODULE               */ 00463000
         BR RET                                                         00464000
*   END ERREXIT;                                                   0422 00465000
@EL00002 DS    0H                                                  0422 00466000
@EF00002 DS    0H                                                  0422 00467000
@ER00002 BR    @14                                                 0422 00468000
*RECOVERY:                                                         0423 00469000
*                                                                  0423 00470000
*/********************************************************************/ 00471000
*/*                                                                  */ 00472000
*/*   FUNCTION - ESTABLISH THE RECOVERY ENVIRONMENT FOR IDA0A05B.    */ 00473000
*/*   INPUT    - THE MODULE WORKAREA WITH MWAFCORE DEFINED           */ 00474000
*/*   OUTPUT   - THE ESTAE IS ISSUE WITH MWAFCORE AS THE PARAMETER   */ 00475000
*/*                                                                  */ 00476000
*/********************************************************************/ 00477000
*                                                                  0423 00478000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0423 00479000
RECOVERY DS    0H                                                  0424 00480000
*   RESPECIFY                                                      0424 00481000
*    (R0,                                                          0424 00482000
*     R1,                                                          0424 00483000
*     RET,                                                         0424 00484000
*     RETCODE) RESTRICTED;                                         0424 00485000
*   IDAESPL=ESTAEPRM;               /* COPY SKELETON INTO WORK AREA  */ 00486000
         LA    @05,MWAFCORE                                        0425 00487000
         MVC   IDAESPL(16,@05),ESTAEPRM                            0425 00488000
*   R1=ADDR(IDAESPL);               /* PUT PTR TO PARM LIST IN REG 1 */ 00489000
         LA    R1,IDAESPL(,@05)                                    0426 00490000
*   GEN(ESTAE PARAM=MWAFCORE,MF=(E,(1)));/* ISSUE ESTAE MACRO        */ 00491000
         ESTAE PARAM=MWAFCORE,MF=(E,(1))                                00492000
*   IF RETCODE^=ZERO THEN           /* CHECK THE RETURN CODE         */ 00493000
         LTR   RETCODE,RETCODE                                     0428 00494000
         BZ    @RF00428                                            0428 00495000
*     DO;                           /* IF NOT ZERO                   */ 00496000
*       RSRETCD2=RSERR240;          /* SET THE RESTART RETURN CODE   */ 00497000
         L     @05,RSTWAPTR                                        0430 00498000
         MVI   RSRETCD2(@05),X'F0'                                 0430 00499000
*       CALL ERREXIT;               /* AND EXIT THE MODULE           */ 00500000
         BAL   @14,ERREXIT                                         0431 00501000
*     END;                                                         0432 00502000
*   IDABASER=RBASEA;                /* SAVE BASE REG FOR RECOVERY    */ 00503000
@RF00428 LA    @05,MWAFCORE                                        0433 00504000
         ST    RBASEA,IDABASER(,@05)                               0433 00505000
*   IDADATAR=R13;                   /* SAVE DATA REG FOR RECOVERY    */ 00506000
         ST    R13,IDADATAR(,@05)                                  0434 00507000
*   IDARETRY=ADDR(ERREXIT);         /* RETRY ROUTINE IS ERREXIT      */ 00508000
         LA    @11,ERREXIT                                         0435 00509000
         ST    @11,IDARETRY(,@05)                                  0435 00510000
*   IDACRWAP=ADDR(RSTWA);           /* SAVE ADDR OF RESTART WORKAREA */ 00511000
         L     @11,RSTWAPTR                                        0436 00512000
         ST    @11,IDACRWAP(,@05)                                  0436 00513000
*   IDARRSTR=ON;                    /* RESTART IN PROGRESS           */ 00514000
         OI    IDARRSTR(@05),B'01000000'                           0437 00515000
*   RSTATRTN=IDARA05B;              /* INDICATE IDA0A05B IN CONTROL  */ 00516000
         MVC   RSTATRTN(4,@05),@CC05018                            0438 00517000
*   RESPECIFY                                                      0439 00518000
*    (R0,                                                          0439 00519000
*     R1,                                                          0439 00520000
*     RET,                                                         0439 00521000
*     RETCODE) UNRESTRICTED;                                       0439 00522000
*   END RECOVERY;                                                  0440 00523000
@EL00003 DS    0H                                                  0440 00524000
@EF00003 DS    0H                                                  0440 00525000
@ER00003 BR    @14                                                 0440 00526000
*BLDVSRP:                                                          0441 00527000
*                                                                  0441 00528000
*/********************************************************************/ 00529000
*/*                                                                  */ 00530000
*/*   FUNCTION - DETERMINES IF A VSRT FOR LSR EXISTS AND INTERFACES  */ 00531000
*/*              WITH IDA0192Y TO BUILD THE GLOBAL CONTROL BLOCKS    */ 00532000
*/*              THAT WERE LOST AT STEP TERMINATION.  THE VATVSRT    */ 00533000
*/*              FIELD IS SAVED AND A SHARED RESOURCE POOL IS BUILT  */ 00534000
*/*              FOR RESTART PROCESSING.                             */ 00535000
*/*   INPUT    - POINTER TO THE VSRT IN VATVPTR                      */ 00536000
*/*   OUTPUT   - GLOBAL STORAGE IS BUILT FOR LOCAL SHARED RESOURCES  */ 00537000
*/*              A SHARED RESOURCE POOL IS BUILT FOR RESTART         */ 00538000
*/*                                                                  */ 00539000
*/********************************************************************/ 00540000
*                                                                  0441 00541000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0441 00542000
BLDVSRP  DS    0H                                                  0442 00543000
*   RESPECIFY                                                      0442 00544000
*     RET RESTRICTED;                                              0442 00545000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 00546000
         LA    @15,1                                               0443 00547000
         AL    @15,RETCONT                                         0443 00548000
         ST    @15,RETCONT                                         0443 00549000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 00550000
         SLA   @15,2                                               0444 00551000
         ST    RET,RETSAVE-4(@15)                                  0444 00552000
*   RESPECIFY                                                      0445 00553000
*     RET UNRESTRICTED;                                            0445 00554000
*   RCORE=ADDR(FORCORE);            /* RCORE PTS TO FORCORE FOR 192Y */ 00555000
         LA    RCORE,MWAFCORE                                      0446 00556000
*   RACB=ADDR(IDABLPRM);            /* POINT REG 2 AT THE PARM LIST  */ 00557000
         LA    @05,MWABLPRM                                        0447 00558000
         LR    RACB,@05                                            0447 00559000
*   IF VATVPTR^=ZERO THEN           /* DOES A VSRT EXIST FOR LSR     */ 00560000
         L     @01,PSATOLD                                         0448 00561000
         L     @01,TCBJSCB(,@01)                                   0448 00562000
         L     @01,JSCBSHR(,@01)                                   0448 00563000
         L     @15,VATVPTR(,@01)                                   0448 00564000
         LTR   @15,@15                                             0448 00565000
         BZ    @RF00448                                            0448 00566000
*     DO;                                                          0449 00567000
*                                                                  0449 00568000
*       /*************************************************************/ 00569000
*       /*                                                           */ 00570000
*       /* INITIALIZE THE PARAMETER LIST AND CALL IDA0192Y           */ 00571000
*       /*                                                           */ 00572000
*       /*************************************************************/ 00573000
*                                                                  0450 00574000
*       BLPACBID=BLPACBCD;          /* SET THE ID BYTE               */ 00575000
         MVI   BLPACBID(@05),X'A0'                                 0450 00576000
*       BLPACBST=BLPSTCD;           /* SET THE SUBTYPE FIELD         */ 00577000
         MVI   BLPACBST(@05),X'11'                                 0451 00578000
*       BLPFDBDC=ON;                /* INDICATE SHARED RESOURCE REQ  */ 00579000
*       BLPFBLD=ON;                 /* INDICATE BUILD REQUEST        */ 00580000
*       BLPFLSR=ON;                 /* INDICATE LSR                  */ 00581000
         OI    BLPFDBDC(@05),B'11010000'                           0454 00582000
*       BLPFRSTR=ON;                /* THIS IS A RESTART REQUEST     */ 00583000
         OI    BLPFRSTR(@05),B'00000001'                           0455 00584000
*       BLPDSORG=BLPDSOCD;          /* SET THE DSORG FIELD FOR VSAM  */ 00585000
         MVI   BLPDSORG(@05),X'08'                                 0456 00586000
*       BLPOFLGS=BLPOFLCD;          /* SET THE OFLAGS FIELD          */ 00587000
         MVI   BLPOFLGS(@05),X'02'                                 0457 00588000
*       CALL IDA0192Y;              /* CALL THE BLDVRP ROUTINE       */ 00589000
         L     @15,@CV05283                                        0458 00590000
         BALR  @14,@15                                             0458 00591000
*       RESPECIFY                                                  0459 00592000
*         RETCODE RESTRICTED;       /* SAVE THE RETURN CODE          */ 00593000
*       RSTATRTN=IDARA05B;          /* NOTE IDA0A05B IN CONTROL      */ 00594000
         LA    @14,MWAFCORE                                        0460 00595000
         MVC   RSTATRTN(4,@14),@CC05018                            0460 00596000
*       MWARETCD=RETCODE;           /* SAVE RETURN CODE FOR ANALYSIS */ 00597000
         ST    RETCODE,MWARETCD                                    0461 00598000
*       IF RETCODE^=ZERO THEN       /* IF ANY ERRORS IN BLDVRP       */ 00599000
         LTR   RETCODE,RETCODE                                     0462 00600000
         BZ    @RF00462                                            0462 00601000
*         DO;                                                      0463 00602000
*           RSRETCD2=RSERR199;      /* SET ERROR CODE IN RSTWA       */ 00603000
         L     @14,RSTWAPTR                                        0464 00604000
         MVI   RSRETCD2(@14),X'C7'                                 0464 00605000
*           CALL ERREXIT;           /* EXIT THE MODULE               */ 00606000
         BAL   @14,ERREXIT                                         0465 00607000
*         END;                                                     0466 00608000
*     END;                          /* END RESTORING RESOURCE POOL   */ 00609000
@RF00462 DS    0H                                                  0468 00610000
*                                                                  0468 00611000
*   /*****************************************************************/ 00612000
*   /*                                                               */ 00613000
*   /* ISSUE A BLDVRP REQUEST FOR THE RESTART SHARED RESOURCE POOL   */ 00614000
*   /*                                                               */ 00615000
*   /*****************************************************************/ 00616000
*                                                                  0468 00617000
*   MWAVSRT=VATVSRT;                /* SAVE THE VATVSRT FIELD        */ 00618000
@RF00448 L     @14,PSATOLD                                         0468 00619000
         L     @14,TCBJSCB(,@14)                                   0468 00620000
         L     @14,JSCBSHR(,@14)                                   0468 00621000
         MVC   MWAVSRT(8),VATVSRT(@14)                             0468 00622000
*   VATVSRT=VATVSRT&&VATVSRT;       /* CLEAR VATVSRT FIELD           */ 00623000
         XC    VATVSRT(8,@14),VATVSRT(@14)                         0469 00624000
*   IDABLPRM=IDABLPRM&&IDABLPRM;    /* CLEAR THE PARAMETER LIST      */ 00625000
         LA    @14,MWABLPRM                                        0470 00626000
         XC    IDABLPRM(52,@14),IDABLPRM(@14)                      0470 00627000
*   BLPACBID=BLPACBCD;              /* SET THE ID BYTE               */ 00628000
         MVI   BLPACBID(@14),X'A0'                                 0471 00629000
*   BLPACBST=BLPSTCD;               /* SET THE SUBTYPE FIELD         */ 00630000
         MVI   BLPACBST(@14),X'11'                                 0472 00631000
*   BLPFDBDC=ON;                    /* A DBDC REQUEST                */ 00632000
*   BLPFBLD=ON;                     /* A BUILD REQUEST               */ 00633000
*   BLPFLSR=ON;                     /* FOR LOCAL SHARED RESOURCES    */ 00634000
         OI    BLPFDBDC(@14),B'11010000'                           0475 00635000
*   BLPDSORG=BLPDSOCD;              /* SET THE DSORG FIELD FOR VSAM  */ 00636000
         MVI   BLPDSORG(@14),X'08'                                 0476 00637000
*   BLPOFLGS=BLPOFLCD;              /* SET THE OFLAGS FIELD          */ 00638000
         MVI   BLPOFLGS(@14),X'02'                                 0477 00639000
