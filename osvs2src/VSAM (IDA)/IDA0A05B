*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 03744000
         SLA   @15,2                                               1306 03745000
         ST    RET,RETSAVE-4(@15)                                  1306 03746000
*   RESPECIFY                                                      1307 03747000
*     RET UNRESTRICTED;                                            1307 03748000
*   RESPECIFY                                                      1308 03749000
*     DEBXTN BASED(DEBXTNP);        /* POINT TO REAL DEB EXTENSION   */ 03750000
*   RDEB=TCBDEB;                    /* GET ADDRESS OF FIRST DEB      */ 03751000
         L     @01,PSATOLD                                         1309 03752000
         L     RDEB,TCBDEB(,@01)                                   1309 03753000
*   PRIORDEB=ZERO;                  /* SET PREVIOUS DEB PTR TO ZERO  */ 03754000
         SLR   PRIORDEB,PRIORDEB                                   1310 03755000
*   DO UNTIL RDEB=ZERO;             /* SCAN ALL DEBS                 */ 03756000
@DL01311 DS    0H                                                  1312 03757000
*     IF DEBAMTYP=VSAMDEB&          /* IF THE DEB IS A VSAM DEB AND  */ 03758000
*         DEBDCBAD->AMBID^=IDXAMB&  /* IF IT IS NOT AN AMB AND       */ 03759000
*         PRIORDEB^=ZERO THEN       /* IF IT IS NOT AT HEAD OF CHAIN */ 03760000
         LR    @10,RDEB                                            1312 03761000
         SL    @10,@CF02693                                        1312 03762000
         CLI   DEBAMTYP(@10),X'01'                                 1312 03763000
         BNE   @RF01312                                            1312 03764000
         L     @01,DEBDCBAD(,RDEB)                                 1312 03765000
         CLI   AMBID(@01),B'01000000'                              1312 03766000
         BE    @RF01312                                            1312 03767000
         LTR   PRIORDEB,PRIORDEB                                   1312 03768000
         BZ    @RF01312                                            1312 03769000
*       DO;                         /* PROCESS A DEB FOR AN ACB      */ 03770000
*         PRIORDEB->DEBDEBB=DEBDEBB;/* POINT PREVIOUS DEB AT NEXT    */ 03771000
         L     @15,DEBDEBB-1(,RDEB)                                1314 03772000
         LA    @15,0(,@15)                                         1314 03773000
         STCM  @15,7,DEBDEBB(PRIORDEB)                             1314 03774000
*         DEBDEBB=TCBDEB;           /* POINT THIS DEB AT HEAD DEB    */ 03775000
         L     @14,PSATOLD                                         1315 03776000
         MVC   DEBDEBB(3,RDEB),TCBDEB+1(@14)                       1315 03777000
*         TCBDEB=RDEB;              /* POINT TCB AT THIS DEB         */ 03778000
         ST    RDEB,TCBDEB(,@14)                                   1316 03779000
*         DEBXCDCB=OFF;             /* TURN OFF DEB NO PROCESS BIT   */ 03780000
         L     @10,DEBXTNP(,@10)                                   1317 03781000
         NI    DEBXCDCB(@10),B'01111111'                           1317 03782000
*         RDEB=PRIORDEB->DEBDEBB;   /* POINT TO NEXT DEB IN CHAIN    */ 03783000
         LR    RDEB,@15                                            1318 03784000
*       END;                                                       1319 03785000
*     ELSE                          /* NOT A VSAM ACB                */ 03786000
*       DO;                                                        1320 03787000
         B     @RC01312                                            1320 03788000
@RF01312 DS    0H                                                  1321 03789000
*         PRIORDEB=RDEB;            /* SAVE DEB POINTER FOR CHAINING */ 03790000
         LR    PRIORDEB,RDEB                                       1321 03791000
*         RDEB=DEBDEBB;             /* POINT TO THE NEXT DEB         */ 03792000
         L     @10,DEBDEBB-1(,RDEB)                                1322 03793000
         LA    @10,0(,@10)                                         1322 03794000
         LR    RDEB,@10                                            1322 03795000
*       END;                                                       1323 03796000
*   END;                                                           1324 03797000
@RC01312 DS    0H                                                  1324 03798000
@DE01311 LTR   RDEB,RDEB                                           1324 03799000
         BNZ   @DL01311                                            1324 03800000
*   RESPECIFY                                                      1325 03801000
*     RET RESTRICTED;                                              1325 03802000
*   RET=RETSAVE(RETCONT);           /* LOAD RETURN ADDRESS           */ 03803000
         L     @10,RETCONT                                         1326 03804000
         LR    @05,@10                                             1326 03805000
         SLA   @05,2                                               1326 03806000
         L     RET,RETSAVE-4(@05)                                  1326 03807000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 03808000
         BCTR  @10,0                                               1327 03809000
         ST    @10,RETCONT                                         1327 03810000
*   RESPECIFY                                                      1328 03811000
*    (RDEB,                                                        1328 03812000
*     RET) UNRESTRICTED;                                           1328 03813000
*   END FIXDEBS;                                                   1329 03814000
@EL00021 DS    0H                                                  1329 03815000
@EF00021 DS    0H                                                  1329 03816000
@ER00021 BR    @14                                                 1329 03817000
*BLDVMT:                                                           1330 03818000
*                                                                  1330 03819000
*/********************************************************************/ 03820000
*/*                                                                  */ 03821000
*/*   FUNCTION - LOCATES THE DSAB FOR THE CURRENT ACB AND CALLS      */ 03822000
*/*              IDA0192F FOR THE VOLUME MOUNT FUNCTION.             */ 03823000
*/*   INPUT    - POINTER TO THE OPW IN RCORE.                        */ 03824000
*/*              POINTER TO THE TIOT ENTRY IN OPWTIOT.               */ 03825000
*/*   OUTPUT   - VMTS ARE BUILT AND VOLUMES ARE MOUNTED.             */ 03826000
*/*              THE POINTER TO THE DSAB IS PLACED IN DXDSABPT.      */ 03827000
*/*                                                                  */ 03828000
*/********************************************************************/ 03829000
*                                                                  1330 03830000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1330 03831000
BLDVMT   DS    0H                                                  1331 03832000
*   RESPECIFY                                                      1331 03833000
*     RET RESTRICTED;                                              1331 03834000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 03835000
         LA    @15,1                                               1332 03836000
         AL    @15,RETCONT                                         1332 03837000
         ST    @15,RETCONT                                         1332 03838000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 03839000
         SLA   @15,2                                               1333 03840000
         ST    RET,RETSAVE-4(@15)                                  1333 03841000
*   RESPECIFY                                                      1334 03842000
*     RET UNRESTRICTED;                                            1334 03843000
*   OPWBCON=ZERO;                   /* RESET CONNECT TO BASE         */ 03844000
         SLR   @05,@05                                             1335 03845000
         ST    @05,OPWBCON(,RCORE)                                 1335 03846000
*   AMBLPTR=PAMBLPTR;               /* POINT TO PRIMARY AMBL         */ 03847000
         L     @05,PAMBLPTR                                        1336 03848000
         ST    @05,AMBLPTR                                         1336 03849000
*   DO UNTIL AMBLSCHN=ZERO;         /* MOUNT VOLUMES FOR ALL ACBS    */ 03850000
@DL01337 DS    0H                                                  1338 03851000
*     DSABPTR=QDBFELMP;             /* GET PTR TO FIRST DSAB         */ 03852000
         L     @05,PSATOLD                                         1338 03853000
         L     @05,TCBJSCB(,@05)                                   1338 03854000
         L     @05,JSCDSABQ(,@05)                                  1338 03855000
         L     @05,QDBFELMP(,@05)                                  1338 03856000
         ST    @05,DSABPTR                                         1338 03857000
*     DO WHILE OPWTIOT^=DSABTIOT;   /* GET CORRECT DSAB              */ 03858000
         B     @DE01339                                            1339 03859000
@DL01339 DS    0H                                                  1340 03860000
*       IF DSABPTR=ZERO THEN        /* CHECK FOR END OF CHN          */ 03861000
         L     @05,DSABPTR                                         1340 03862000
         LTR   @05,@05                                             1340 03863000
         BNZ   @RF01340                                            1340 03864000
*         DO;                       /* IF NO DSAB FOR ACB,           */ 03865000
*           RSRETCD2=RSERR191;      /* SET ERROR CODE AND            */ 03866000
         L     @05,RSTWAPTR                                        1342 03867000
         MVI   RSRETCD2(@05),X'BF'                                 1342 03868000
*           CALL ERREXIT;           /* EXIT MODULE                   */ 03869000
         BAL   @14,ERREXIT                                         1343 03870000
*         END;                                                     1344 03871000
*       DSABPTR=DSABFCHN;           /* POINT TO NEXT DSAB            */ 03872000
@RF01340 L     @05,DSABPTR                                         1345 03873000
         L     @05,DSABFCHN(,@05)                                  1345 03874000
         ST    @05,DSABPTR                                         1345 03875000
*     END;                                                         1346 03876000
@DE01339 L     @05,DSABPTR                                         1346 03877000
         CLC   OPWTIOT(4,RCORE),DSABTIOT(@05)                      1346 03878000
         BNE   @DL01339                                            1346 03879000
*     DXDSABAD=DSABPTR;             /* INIT DSAB POINTER IN FORCORE  */ 03880000
         LA    @15,MWAFCORE                                        1347 03881000
         ST    @05,DXDSABAD(,@15)                                  1347 03882000
*     RESPECIFY                                                    1348 03883000
*       RETCODE RESTRICTED;                                        1348 03884000
*     CALL IDA0192F;                /* BUILD VMT AND MOUNT VOLUMES   */ 03885000
         L     @15,@CV05281                                        1349 03886000
         BALR  @14,@15                                             1349 03887000
*     RSTATRTN=IDARA05B;            /* NOTE A05B BACK IN CONTROL     */ 03888000
         LA    @14,MWAFCORE                                        1350 03889000
         MVC   RSTATRTN(4,@14),@CC05018                            1350 03890000
*     MWARETCD=RETCODE;             /* SAVE RETURN CODE FOR ANALYSIS */ 03891000
         ST    RETCODE,MWARETCD                                    1351 03892000
*     IF RETCODE^=ZERO THEN         /* CHECK RETURN CODE FOR ERRORS  */ 03893000
         LTR   RETCODE,RETCODE                                     1352 03894000
         BZ    @RF01352                                            1352 03895000
*       DO;                         /* ANY ERRORS THEN               */ 03896000
*         RSRETCD2=RSERR191;        /* SET RETURN CODE               */ 03897000
         L     @14,RSTWAPTR                                        1354 03898000
         MVI   RSRETCD2(@14),X'BF'                                 1354 03899000
*         CALL ERREXIT;             /* TAKE ERROR EXIT FROM MODULE   */ 03900000
         BAL   @14,ERREXIT                                         1355 03901000
*       END;                                                       1356 03902000
*     RESPECIFY                                                    1357 03903000
*       RETCODE UNRESTRICTED;                                      1357 03904000
@RF01352 DS    0H                                                  1358 03905000
*     IF AMBLSCHN^=ZERO THEN        /* ANY MORE SECONDARY AMBLS      */ 03906000
         SLR   @05,@05                                             1358 03907000
         L     @01,AMBLPTR                                         1358 03908000
         L     @15,AMBLSCHN(,@01)                                  1358 03909000
         CR    @15,@05                                             1358 03910000
         BE    @RF01358                                            1358 03911000
*       DO;                         /* YES                           */ 03912000
*         AMBLPTR=AMBLSCHN;         /* POINT TO THE NEXT SECONDARY   */ 03913000
         ST    @15,AMBLPTR                                         1360 03914000
*         IF AMBLXPT=ZERO THEN      /* BASE OPENED DIRECTLY          */ 03915000
         C     @05,AMBLXPT(,@15)                                   1361 03916000
         BNE   @RF01361                                            1361 03917000
*           RACB=AMBLACB;           /* YES, GET ACB ADDRESS          */ 03918000
         L     RACB,AMBLACB(,@15)                                  1362 03919000
*         ELSE                      /* NO,                           */ 03920000
*           RACB=AMBLXPT->AMBLACB;  /* ACB ADDRESS FROM PATH AMBL    */ 03921000
         B     @RC01361                                            1363 03922000
@RF01361 L     @05,AMBLPTR                                         1363 03923000
         L     @05,AMBLXPT(,@05)                                   1363 03924000
         L     RACB,AMBLACB(,@05)                                  1363 03925000
*         OPWUACB=ADDR(IFGACB);     /* PUT ACB ADDRESS IN OPW        */ 03926000
@RC01361 ST    RACB,OPWUACB(,RCORE)                                1364 03927000
*         OPWTIOT=TCBTIO+ACBTIOT;   /* GET ADDRESS OF TIOT ENTRY     */ 03928000
         LH    @15,ACBTIOT(,RACB)                                  1365 03929000
         L     @14,PSATOLD                                         1365 03930000
         AL    @15,TCBTIO(,@14)                                    1365 03931000
         ST    @15,OPWTIOT(,RCORE)                                 1365 03932000
*         JFCBPTR=TIOEJFCB;         /* GET ADDRESS OF JFCB           */ 03933000
         SLR   JFCBPTR,JFCBPTR                                     1366 03934000
         ICM   JFCBPTR,7,TIOEJFCB(@15)                             1366 03935000
*         DXJBF=JFCB;               /* GET JFCB FROM LSQA            */ 03936000
         LA    @15,MWAFCORE                                        1367 03937000
         MVC   DXJBF(176,@15),JFCB(JFCBPTR)                        1367 03938000
*       END;                                                       1368 03939000
*     OPWBCON=PAMBLPTR;             /* INDICATE CONNECT TO BASE      */ 03940000
@RF01358 L     @05,PAMBLPTR                                        1369 03941000
         ST    @05,OPWBCON(,RCORE)                                 1369 03942000
*   END;                                                           1370 03943000
@DE01337 L     @05,AMBLPTR                                         1370 03944000
         L     @05,AMBLSCHN(,@05)                                  1370 03945000
         LTR   @05,@05                                             1370 03946000
         BNZ   @DL01337                                            1370 03947000
*   RESPECIFY                                                      1371 03948000
*     RET RESTRICTED;                                              1371 03949000
*   RET=RETSAVE(RETCONT);           /* LOAD RETURN ADDRESS           */ 03950000
         L     @05,RETCONT                                         1372 03951000
         LR    @15,@05                                             1372 03952000
         SLA   @15,2                                               1372 03953000
         L     RET,RETSAVE-4(@15)                                  1372 03954000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 03955000
         BCTR  @05,0                                               1373 03956000
         ST    @05,RETCONT                                         1373 03957000
*   RESPECIFY                                                      1374 03958000
*     RET UNRESTRICTED;                                            1374 03959000
*   END BLDVMT;                                                    1375 03960000
@EL00022 DS    0H                                                  1375 03961000
@EF00022 DS    0H                                                  1375 03962000
@ER00022 BR    @14                                                 1375 03963000
@PB00022 DS    0H                                                  1375 03964000
