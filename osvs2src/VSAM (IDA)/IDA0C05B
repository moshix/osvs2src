         TITLE 'IDA0C05B - VS2 VSAM RESTART, SSCR AND DEB PROCESSING   *00001000
                        '                                               00002000
IDA0C05B CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0C05B  75.197'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@03                                         0001 00013000
JSCBDISP EQU 188                                                        00014000
JSCBAOS1 EQU  72                                                        00015000
JSCBAOS2 EQU 192                                                        00016000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00017000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00018000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00019000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00020000
*                                       IF NO ERROR WAS ENCOUNTERED     00021000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00022000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00023000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00024000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00025000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00026000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00027000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00028000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00029000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00030000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00031000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00032000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00033000
         B     GMN00200                 BYPASS SETTING ERROR            00034000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00035000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00036000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00037000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00038000
*   SACODE=ZERO;                    /* ZERO RETURN CODE              */ 00039000
         SLR   @14,@14                                             0142 00040000
         ST    @14,SACODE(,R13)                                    0142 00041000
*   R1=SAREG(1);                    /* GET POINTER TO VS RESTART W A */ 00042000
         L     R1,SAREG(,R13)                                      0143 00043000
*   IF R15=ZERO THEN                /* IF NO CORE AVAILABLE          */ 00044000
         CR    R15,@14                                             0144 00045000
         BNE   @RF00144                                            0144 00046000
*     R1->RSRETCD2=RSERR195;        /* SET RESTART RETURN CODE       */ 00047000
         MVI   RSRETCD2(R1),X'C3'                                  0145 00048000
*   ELSE                            /* NO ERROR                      */ 00049000
*     DO;                           /* MAINLINE                      */ 00050000
         B     @RC00144                                            0146 00051000
@RF00144 DS    0H                                                  0147 00052000
*       R15->SABWRD=R13;            /* CHAIN THE SAVE AREA           */ 00053000
         ST    R13,SABWRD(,R15)                                    0147 00054000
*       R13=R15;                    /* LOAD PTR TO AUTOMATICS        */ 00055000
         LR    R13,R15                                             0148 00056000
*       GEN(USING @DATD,R13);       /* GET BASE FOR AUTOMATICS       */ 00057000
         USING @DATD,R13                                                00058000
*       RSTWAPTR=R1;                /* POINTER TO RESTART WORK AREA  */ 00059000
         ST    R1,RSTWAPTR                                         0150 00060000
*       RETCONT=ZERO;               /* INIT RETURN ADR INDEX @ZA04523*/ 00061000
         SLR   RETCONT,RETCONT                                     0151 00062000
*       CALL RECOVERY;              /* ESTABLISH RECOVERY ENVIRONMNT */ 00063000
         BAL   @14,RECOVERY                                        0152 00064000
*       RETCONT=ZERO;               /* INIT ARRAY CONTROL            */ 00065000
         SLR   RETCONT,RETCONT                                     0153 00066000
*       IF SSCRSSID^=SSCRVSAM|      /* IF NOT A VSAM SSCR OR         */ 00067000
*           SSCRFCHN^=ZERO|         /* IF NOT THE ONLY VSAM SSCR OR  */ 00068000
*           SSCRDATL^=LENGTH(SSCRVATP)|/* IF THE DATA LENGTH BAD OR  */ 00069000
*           VATID^=IDXVAT THEN      /* IF DATA DOES NOT POINT TO VAT */ 00070000
         L     @14,RSTWAPTR                                        0154 00071000
         L     @14,RSSSCR(,@14)                                    0154 00072000
         CLI   SSCRSSID(@14),X'01'                                 0154 00073000
         BNE   @RT00154                                            0154 00074000
         L     @12,SSCRFCHN(,@14)                                  0154 00075000
         LTR   @12,@12                                             0154 00076000
         BNZ   @RT00154                                            0154 00077000
         CLC   SSCRDATL(2,@14),@CH00089                            0154 00078000
         BNE   @RT00154                                            0154 00079000
         L     @14,SSCRVATP(,@14)                                  0154 00080000
         CLI   VATID(@14),B'00010001'                              0154 00081000
         BE    @RF00154                                            0154 00082000
@RT00154 DS    0H                                                  0155 00083000
*         RSRETCD2=RSERR243;        /* INDICATE THE SSCR IS BAD      */ 00084000
         L     @14,RSTWAPTR                                        0155 00085000
         MVI   RSRETCD2(@14),X'F3'                                 0155 00086000
*       ELSE                        /* OTHERWISE,                    */ 00087000
*         DO;                       /* SSCR RECORD LOOKS GOOD        */ 00088000
         B     @RC00154                                            0156 00089000
@RF00154 DS    0H                                                  0157 00090000
*           VATSTPNO=JSCBSTEP;      /* STEP NO. IN VAT PTR   @ZA04523*/ 00091000
         L     @14,RSTWAPTR                                        0157 00092000
         L     @14,RSSSCR(,@14)                                    0157 00093000
         L     @12,PSATOLD                                         0157 00094000
         L     @12,TCBJSCB(,@12)                                   0157 00095000
         IC    @05,JSCBSTEP(,@12)                                  0157 00096000
         STC   @05,VATSTPNO(,@14)                                  0157 00097000
*           JSCBSHR=ADDR(IDAVAT);   /* INITIALIZE PTR TO VAT IN JSCB */ 00098000
         L     @14,SSCRVATP(,@14)                                  0158 00099000
         ST    @14,JSCBSHR(,@12)                                   0158 00100000
*           CALL FIXDEBS;           /* PREPARE VSAM DEBS FOR RESTART */ 00101000
         BAL   @14,FIXDEBS                                         0159 00102000
*         END;                                                     0160 00103000
*       GEN(L R0,@SIZDATD);         /* LOAD R0 WITH SP & SIZE OF MWA */ 00104000
@RC00154 DS    0H                                                  0161 00105000
         L R0,@SIZDATD                                                  00106000
*       R1=R13;                     /* LOAD REG 1 WITH ADDRESS OF MWA*/ 00107000
         LR    R1,R13                                              0162 00108000
*       R13=SABWRD;                 /* POINT R13 AT PREVIOUS SAVE    */ 00109000
         L     R13,SABWRD(,R13)                                    0163 00110000
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE MODULE WORK AREA        */ 00111000
         FREEMAIN R,LV=(0),A=(1)                                        00112000
*       GEN(ESTAE 0);               /* RELEASE RECOVERY ENVIRONMENT  */ 00113000
         ESTAE 0                                                        00114000
*     END;                                                         0166 00115000
*   RESPECIFY                                                      0167 00116000
*     RET RESTRICTED;                                              0167 00117000
@RC00144 DS    0H                                                  0168 00118000
*   LM(RET,R12,SAREGS);             /* RESTORE CALLERS REGS          */ 00119000
         LM    RET,R12,SAREGS(R13)                                 0168 00120000
*   R15=R1->RSRETCD2;               /* SET THE RETURN CODE IN REG 15 */ 00121000
         SLR   R15,R15                                             0169 00122000
         IC    R15,RSRETCD2(,R1)                                   0169 00123000
*   GEN(BR RET);                    /* RETURN TO CALLER              */ 00124000
         BR RET                                                         00125000
*   RESPECIFY                                                      0171 00126000
*    (R0,                                                          0171 00127000
*     R1,                                                          0171 00128000
*     RET,                                                         0171 00129000
*     R15) UNRESTRICTED;                                           0171 00130000
*RECOVERY:                                                         0172 00131000
*                                                                  0172 00132000
*/********************************************************************/ 00133000
*/*                                                                  */ 00134000
*/*   FUNCTION - ESTABLISH THE RECOVERY ENVIRONMENT FOR IDA0C05B.    */ 00135000
*/*   INPUT    - THE MODULE WORKAREA WITH MWAFCORE DEFINED           */ 00136000
*/*   OUTPUT   - THE ESTAE IS ISSUE WITH MWAFCORE AS THE PARAMETER   */ 00137000
*/*                                                                  */ 00138000
*/********************************************************************/ 00139000
*                                                                  0172 00140000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0172 00141000
RECOVERY DS    0H                                                  0173 00142000
*   RESPECIFY                                                      0173 00143000
*    (R0,                                                          0173 00144000
*     R1,                                                          0173 00145000
*     RET,                                                         0173 00146000
*     RETCODE) RESTRICTED;                                         0173 00147000
*   IDAESPL=ESTAEPRM;               /* COPY SKELETON INTO WORK AREA  */ 00148000
         LA    @12,MWAFCORE                                        0174 00149000
         MVC   IDAESPL(16,@12),ESTAEPRM                            0174 00150000
*   R1=ADDR(IDAESPL);               /* PUT PTR TO PARM LIST IN REG 1 */ 00151000
         LA    R1,IDAESPL(,@12)                                    0175 00152000
*   GEN(ESTAE PARAM=MWAFCORE,MF=(E,(1)));/* ISSUE ESTAE MACRO        */ 00153000
         ESTAE PARAM=MWAFCORE,MF=(E,(1))                                00154000
*   IF RETCODE^=ZERO THEN           /* CHECK THE RETURN CODE         */ 00155000
         LTR   RETCODE,RETCODE                                     0177 00156000
         BZ    @RF00177                                            0177 00157000
*     DO;                           /* IF NOT ZERO                   */ 00158000
*       RSRETCD2=RSERR240;          /* SET THE RESTART RETURN CODE   */ 00159000
         L     @12,RSTWAPTR                                        0179 00160000
         MVI   RSRETCD2(@12),X'F0'                                 0179 00161000
*       CALL ERREXIT;               /* AND EXIT THE MODULE   @ZA04523*/ 00162000
         BAL   @14,ERREXIT                                         0180 00163000
*     END;                                                         0181 00164000
*   IDABASER=RBASEA;                /* SAVE BASE REG FOR RECOVERY    */ 00165000
@RF00177 LA    @12,MWAFCORE                                        0182 00166000
         ST    RBASEA,IDABASER(,@12)                               0182 00167000
*   IDADATAR=R13;                   /* SAVE DATA REG FOR RECOVERY    */ 00168000
         ST    R13,IDADATAR(,@12)                                  0183 00169000
*   IDARETRY=ADDR(ERREXIT);         /* RETRY ROUTINE IS ERREXIT      */ 00170000
         LA    @02,ERREXIT                                         0184 00171000
         ST    @02,IDARETRY(,@12)                                  0184 00172000
*   IDACRWAP=ADDR(RSTWA);           /* SAVE ADDR OF RESTART WORKAREA */ 00173000
         L     @02,RSTWAPTR                                        0185 00174000
         ST    @02,IDACRWAP(,@12)                                  0185 00175000
*   IDARRSTR=ON;                    /* RESTART IN PROCESS            */ 00176000
         OI    IDARRSTR(@12),B'01000000'                           0186 00177000
*   RSTATRTN=IDARC05B;              /* INDICATE IDA0C05B IN CONTROL  */ 00178000
         MVC   RSTATRTN(4,@12),@CC02236                            0187 00179000
*   RESPECIFY                                                      0188 00180000
*    (R0,                                                          0188 00181000
*     R1,                                                          0188 00182000
*     RET,                                                         0188 00183000
*     RETCODE) UNRESTRICTED;                                       0188 00184000
*   END RECOVERY;                                                  0189 00185000
@EL00002 DS    0H                                                  0189 00186000
@EF00002 DS    0H                                                  0189 00187000
@ER00002 BR    @14                                                 0189 00188000
*FIXDEBS:                                                          0190 00189000
*                                                                  0190 00190000
*/********************************************************************/ 00191000
*/*                                                                  */ 00192000
*/*   FUNCTION - FREE THE DEBS OBTAINED FOR THE AMBS BY VS RESTART   */ 00193000
*/*              AND MARK THE DEBS FOR THE VSAM ACBS BY SETTING A    */ 00194000
*/*              BIT IN THE EXTENSION TO PREVENT VSAM CLOSE FROM     */ 00195000
*/*              BEING CALLED IN THE EVENT OF ABNORMAL TERMINATION.  */ 00196000
*/*   INPUT    - POINTER TO THE RSTWA IN RSTWAPTR                    */ 00197000
*/*   OUTPUT   - DEBS FOR VSAM AMBS ARE REMOVED FROM THE DEB CHAIN   */ 00198000
*/*              AND FREED, WHILE THE DEBXCDCB BIT IS SET IN THE     */ 00199000
*/*              EXTENSION OF DEBS FOR VSAM ACBS.                    */ 00200000
*/*                                                                  */ 00201000
*/********************************************************************/ 00202000
*                                                                  0190 00203000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0190 00204000
FIXDEBS  DS    0H                                                  0191 00205000
*   RESPECIFY                                                      0191 00206000
*    (RDEB,                                                        0191 00207000
*     RET) RESTRICTED;                                             0191 00208000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 00209000
         AL    RETCONT,@CF00052                                    0192 00210000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 00211000
         LR    @15,RETCONT                                         0193 00212000
         SLA   @15,2                                               0193 00213000
         ST    RET,RETSAVE-4(@15)                                  0193 00214000
*   RESPECIFY                                                      0194 00215000
*     RET UNRESTRICTED;                                            0194 00216000
*   RDEB=RSINT;                     /* GET ADDRESS OF FIRST DEB      */ 00217000
         L     @12,RSTWAPTR                                        0195 00218000
         L     RDEB,RSINT(,@12)                                    0195 00219000
*   PRIORDEB=ZERO;                  /* SET PREVIOUS DEB PTR TO ZERO  */ 00220000
         SLR   PRIORDEB,PRIORDEB                                   0196 00221000
*   DO UNTIL RDEB=ZERO;             /* SCAN ALL DEBS                 */ 00222000
@DL00197 DS    0H                                                  0198 00223000
*     IF DEBAMTYP=VSAMDEB THEN      /* IF THE DEB IS A VSAM DEB      */ 00224000
         LR    @12,RDEB                                            0198 00225000
         AL    @12,@CF02399                                        0198 00226000
         CLI   DEBAMTYP-33(@12),X'01'                              0198 00227000
         BNE   @RF00198                                            0198 00228000
*       IF AMBID=IDXAMB THEN        /* DETERMINE WHICH IT IS         */ 00229000
         L     @12,DEBDCBAD(,RDEB)                                 0199 00230000
         CLI   AMBID(@12),B'01000000'                              0199 00231000
         BNE   @RF00199                                            0199 00232000
*         CALL DEBFREE;             /* FREE THE DEB IF IT IS AN AMB  */ 00233000
         BAL   @14,DEBFREE                                         0200 00234000
*       ELSE                        /* OTHERWISE,                    */ 00235000
*         DO;                       /* FOR VSAM ACB OWNED DEB        */ 00236000
         B     @RC00199                                            0201 00237000
@RF00199 DS    0H                                                  0202 00238000
*           DEBXCDCB=ON;            /* PREVENT A COMPLETE CLOSE      */ 00239000
         LR    @12,RDEB                                            0202 00240000
         AL    @12,@CF02400                                        0202 00241000
         L     @12,DEBXTNP-28(,@12)                                0202 00242000
         OI    DEBXCDCB(@12),B'10000000'                           0202 00243000
*           PRIORDEB=RDEB;          /* SAVE DEB POINTER FOR CHAINING */ 00244000
         LR    PRIORDEB,RDEB                                       0203 00245000
*           RDEB=DEBDEBB;           /* POINT TO THE NEXT DEB         */ 00246000
         L     @12,DEBDEBB-1(,RDEB)                                0204 00247000
         LA    @12,0(,@12)                                         0204 00248000
         LR    RDEB,@12                                            0204 00249000
*         END;                      /* END OF PROCESSING DEB FOR ACB */ 00250000
*     ELSE                          /* OTHERWISE,                    */ 00251000
*       DO;                         /* FOR NON-VSAM DEBS             */ 00252000
         B     @RC00198                                            0206 00253000
@RF00198 DS    0H                                                  0207 00254000
*         PRIORDEB=RDEB;            /* SAVE DEB POINTER FOR CHAINING */ 00255000
         LR    PRIORDEB,RDEB                                       0207 00256000
*         RDEB=DEBDEBB;             /* POINT TO THE NEXT DEB         */ 00257000
         L     @12,DEBDEBB-1(,RDEB)                                0208 00258000
         LA    @12,0(,@12)                                         0208 00259000
         LR    RDEB,@12                                            0208 00260000
*       END;                        /* END PROCESSING NON-VSAM DEBS  */ 00261000
*   END;                                                           0210 00262000
@RC00198 DS    0H                                                  0210 00263000
@DE00197 LTR   RDEB,RDEB                                           0210 00264000
         BNZ   @DL00197                                            0210 00265000
*   RESPECIFY                                                      0211 00266000
*     RET RESTRICTED;                                              0211 00267000
*   RET=RETSAVE(RETCONT);           /* LOAD RETURN ADDRESS           */ 00268000
         LR    @12,RETCONT                                         0212 00269000
         SLA   @12,2                                               0212 00270000
         L     RET,RETSAVE-4(@12)                                  0212 00271000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 00272000
         BCTR  RETCONT,0                                           0213 00273000
*   RESPECIFY                                                      0214 00274000
*     RET UNRESTRICTED;                                            0214 00275000
*   END FIXDEBS;                                                   0215 00276000
@EL00003 DS    0H                                                  0215 00277000
@EF00003 DS    0H                                                  0215 00278000
@ER00003 BR    @14                                                 0215 00279000
*DEBFREE:                                                          0216 00280000
*                                                                  0216 00281000
*/********************************************************************/ 00282000
*/*                                                                  */ 00283000
*/*   FUNCTION - FREE THE DEBS OBTAINED FOR THE AMBS BY VS RESTART   */ 00284000
*/*              AND CHAIN THE NEXT DEB IN ITS PLACE.                */ 00285000
*/*   INPUT    - POINTER TO THE DEB TO BE FREED IN RDEB              */ 00286000
*/*   OUTPUT   - THE DEB AND EXTENSION ARE FREED                     */ 00287000
*/*              RDEB POINTS TO THE NEXT DEB TO BE PROCESSED         */ 00288000
*/*                                                                  */ 00289000
*/********************************************************************/ 00290000
*                                                                  0216 00291000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0216 00292000
DEBFREE  DS    0H                                                  0217 00293000
*   RESPECIFY                                                      0217 00294000
*     RET RESTRICTED;                                              0217 00295000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 00296000
         AL    RETCONT,@CF00052                                    0218 00297000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 00298000
         LR    @15,RETCONT                                         0219 00299000
         SLA   @15,2                                               0219 00300000
         ST    RET,RETSAVE-4(@15)                                  0219 00301000
*   RESPECIFY                                                      0220 00302000
*     RET UNRESTRICTED;                                            0220 00303000
*   FREMNSP=RDEBPOOL;               /* SPECIFY DEB SUBPOOL           */ 00304000
*                                                                  0221 00305000
         MVI   FREMNSP,X'E6'                                       0221 00306000
*   /*****************************************************************/ 00307000
*   /*                                                               */ 00308000
*   /* FREE THE DEB EXTENSION                                        */ 00309000
*   /*                                                               */ 00310000
*   /*****************************************************************/ 00311000
*                                                                  0222 00312000
*   FREMNLEN=DEBXLNGH;              /* GET LENGTH OF EXTENSION       */ 00313000
         LR    @12,RDEB                                            0222 00314000
         AL    @12,@CF02400                                        0222 00315000
         L     @12,DEBXTNP-28(,@12)                                0222 00316000
         LH    @10,DEBXLNGH(,@12)                                  0222 00317000
         STCM  @10,7,FREMNLEN                                      0222 00318000
*   FREMNADR=DEBXTNP;               /* GET ADDRESS OF EXTENSION      */ 00319000
         ST    @12,FREMNADR                                        0223 00320000
*   LM(R0,R1,FREMNLST);             /* LOAD THE FREEMAIN REGISTERS   */ 00321000
         LM    R0,R1,FREMNLST                                      0224 00322000
*   GEN(MODESET EXTKEY=DATAMGT);    /* GET IN KEY TO ISSUE FREEMAIN  */ 00323000
         MODESET EXTKEY=DATAMGT                                         00324000
*   GEN(FREEMAIN R,LV=(0),A=(1));   /* FREE THE EXTENSION            */ 00325000
         FREEMAIN R,LV=(0),A=(1)                                        00326000
*   GEN(MODESET EXTKEY=ZERO);       /* RESUME EXECUTION IN KEY ZERO  */ 00327000
*                                                                  0227 00328000
         MODESET EXTKEY=ZERO                                            00329000
*   /*****************************************************************/ 00330000
*   /*                                                               */ 00331000
*   /* RECHAIN THE NEXT DEB AND FREE THIS DEB                        */ 00332000
*   /*                                                               */ 00333000
*   /*****************************************************************/ 00334000
*                                                                  0228 00335000
*   FREMNLEN=DEBLNGTH*DWORDLEN;     /* GET LENGTH OF DEB             */ 00336000
         LR    @12,RDEB                                            0228 00337000
         SL    @12,@CF02279                                        0228 00338000
         SLR   @10,@10                                             0228 00339000
         IC    @10,DEBLNGTH(,@12)                                  0228 00340000
         SLA   @10,3                                               0228 00341000
         STCM  @10,7,FREMNLEN                                      0228 00342000
*   FREMNADR=ADDR(DEB);             /* GET ADDRESS OF DEB            */ 00343000
         ST    @12,FREMNADR                                        0229 00344000
*   RDEB=DEBDEBB;                   /* GET THE ADDRESS OF NEXT DEB   */ 00345000
         L     @12,DEBDEBB-1(,RDEB)                                0230 00346000
         LA    @12,0(,@12)                                         0230 00347000
         LR    RDEB,@12                                            0230 00348000
*   IF PRIORDEB=ZERO THEN           /* IF THIS IS FIRST DEB ON CHAIN */ 00349000
         LTR   PRIORDEB,PRIORDEB                                   0231 00350000
         BNZ   @RF00231                                            0231 00351000
*     RSINT=RDEB;                   /* CHAIN TO RESTART WORK AREA    */ 00352000
         L     @12,RSTWAPTR                                        0232 00353000
         ST    RDEB,RSINT(,@12)                                    0232 00354000
*   ELSE                            /* OTHERWISE,                    */ 00355000
*     PRIORDEB->DEBDEBB=RDEB;       /* CHAIN TO PREVIOUS DEB         */ 00356000
         B     @RC00231                                            0233 00357000
@RF00231 STCM  RDEB,7,DEBDEBB(PRIORDEB)                            0233 00358000
*   LM(R0,R1,FREMNLST);             /* LOAD FREEMAIN REGISTERS       */ 00359000
@RC00231 LM    R0,R1,FREMNLST                                      0234 00360000
*   GEN(MODESET EXTKEY=DATAMGT);    /* GET IN KEY TO ISSUE FREEMAIN  */ 00361000
         MODESET EXTKEY=DATAMGT                                         00362000
*   GEN(FREEMAIN R,LV=(0),A=(1));   /* FREE THE DEB                  */ 00363000
         FREEMAIN R,LV=(0),A=(1)                                        00364000
*   GEN(MODESET EXTKEY=ZERO);       /* RESUME EXECUTION IN KEY ZERO  */ 00365000
         MODESET EXTKEY=ZERO                                            00366000
*   RESPECIFY                                                      0238 00367000
*     RET RESTRICTED;                                              0238 00368000
*   RET=RETSAVE(RETCONT);           /* LOAD RETURN ADDRESS           */ 00369000
         LR    @12,RETCONT                                         0239 00370000
         SLA   @12,2                                               0239 00371000
         L     RET,RETSAVE-4(@12)                                  0239 00372000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 00373000
         BCTR  RETCONT,0                                           0240 00374000
*   RESPECIFY                                                      0241 00375000
*    (RDEB,                                                        0241 00376000
*     RET) UNRESTRICTED;                                           0241 00377000
*   END DEBFREE;                                                   0242 00378000
@EL00004 DS    0H                                                  0242 00379000
@EF00004 DS    0H                                                  0242 00380000
@ER00004 BR    @14                                                 0242 00381000
*ERREXIT:                                                          0243 00382000
*                                                                  0243 00383000
*/********************************************************************/ 00384000
*/*                                                                  */ 00385000
*/*   FUNCTION - PERFORMS ERROR EXIT AND ESTAE RETRY ROUTINE         */ 00386000
*/*              FUNCTIONS.                                          */ 00387000
*/*   INPUT    - A POINTER TO THE RESTART WORK AREA IN IDACRWAP      */ 00388000
*/*   OUTPUT   - THE MODULE IS EXITED                                */ 00389000
*/*                                                                  */ 00390000
