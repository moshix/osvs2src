         TITLE 'IDA0C06C - VS2 VSAM CHECKPOINT, MAIN ROUTINE           *00001000
                        '                                               00002000
IDA0C06C CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDA0C06C  78.151'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @03,0                                               0001 00011000
@PSTART  LA    @12,4095(,@03)                                      0001 00012000
         USING @PSTART,@03                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
JSCBDISP EQU 188                                                        00015000
JSCBAOS1 EQU  72                                                        00016000
JSCBAOS2 EQU 192                                                        00017000
RBPRFXLN EQU 64                                                         00018000
RBPRFLNA EQU 32                                                         00019000
PRBLEN   EQU 136                                                        00020000
SIRBLEN  EQU 200                                                        00021000
TIRBLEN  EQU 136                                                        00022000
IRBLEN   EQU 128                                                        00023000
SVRBLEN  EQU 224                                                        00024000
         L     @00,@SIZDATD             LOAD LENGTH OF AUTOMATICS       00025000
         GETMAIN RC,LV=(0),SP=0         ISSUE GETMAIN IN SUBPOOL ZERO   00026000
         LTR   @15,@15                  TEST RETURN CODE FOR ERROR      00027000
         BNZ   GMN00100                 BRANCH IF ERROR ENCOUNTERED     00028000
*                                       IF NO ERROR WAS ENCOUNTERED     00029000
         LR    @00,@01                  ADDRESS OF GOTTEN AREA IN R0    00030000
         LR    @14,@01                  SAVE THE ADDRESS IN REGISTER 14 00031000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00032000
         MVCL  @00,@14                  ZERO THE GOTTEN AREA, R15=0     00033000
         LR    @15,@14                  ADDRESS OF AREA IN REGISTER 15  00034000
         ST    @15,8(,@13)              FORWARD CHAIN THIS SAVE AREA    00035000
         L     @01,@SIZDATD             LENGTH OF GOTTEN AREA           00036000
         ST    @01,88(,@15)             STORE LENGTH OF AREA            00037000
         LR    @14,@03                  LOAD BASE REGISTER OF MODULE    00038000
         LA    @01,23                   LOAD NEGATIVE OFFSET BYTE NO.   00039000
         SR    @14,@01                  NEGITIVE OFFSET 23 BYTES        00040000
         MVC   72(16,@15),0(@14)        GET MODULE NAME AND DATE        00041000
         B     GMN00200                 BYPASS SETTING ERROR            00042000
GMN00100 SR    @15,@15                  REGISTER 15 = 0 INDICATES ERROR 00043000
GMN00200 EQU   *                        R15 CONTAINS ADDRESS OR ZERO    00044000
*        REGISTER 15 CONTAINS EITHER THE ADDRESS OF GOTTEN CORE OR      00045000
*        ZERO IF THE GETMAIN WAS UNSUCCESSFUL.                          00046000
*   R11=RETCODE;                    /* SAVE ADDRESS OF MODULE W.A.   */ 00047000
         LR    R11,RETCODE                                         0237 00048000
*   R0=(LENGTH(SP250WA)+SEVEN)&-DWORDLEN;/* LENGTH UNPROTECTED W.A.  */ 00049000
         LA    R0,152                                              0238 00050000
*   IF RETCODE^=ZERO THEN           /* IF MODULE WORK AREA GOTTEN    */ 00051000
         LTR   RETCODE,RETCODE                                     0239 00052000
         BZ    @RF00239                                            0239 00053000
*     GEN(GETMAIN RC,LV=(0),SP=250);/* GET CORE FOR UNPROTECTED W.A. */ 00054000
         GETMAIN RC,LV=(0),SP=250                                       00055000
*   IF RETCODE^=ZERO|R11=ZERO THEN  /* IF NO CORE AVAILABLE          */ 00056000
@RF00239 SLR   @14,@14                                             0241 00057000
         CR    RETCODE,@14                                         0241 00058000
         BNE   @RT00241                                            0241 00059000
         CR    R11,@14                                             0241 00060000
         BNE   @RF00241                                            0241 00061000
@RT00241 DS    0H                                                  0242 00062000
*     DO;                                                          0242 00063000
*       SAREG(1)->CKMSGCD1=ZERO;    /* INIT TO INDICATE ERROR MSG  0243 00064000
*                                      IHJ000I TO BE ISSUED  @ZA14803*/ 00065000
         L     @14,SAREG(,R13)                                     0243 00066000
         MVI   CKMSGCD1(@14),X'00'                                 0243 00067000
*       SAREG(1)->CKMSGCD2=CKMSG003;/* SET CHECKPOINT MESSAGE CODE   */ 00068000
         MVI   CKMSGCD2(@14),X'03'                                 0244 00069000
*       SAREG(1)->CKRETCD2=CHKERR08;/* SET CHECKPOINT RETURN CODE    */ 00070000
         MVI   CKRETCD2(@14),X'08'                                 0245 00071000
*       RCORE=ZERO;                 /* IND THE WORKARES NOT OBTAINED */ 00072000
         SLR   RCORE,RCORE                                         0246 00073000
*     END;                                                         0247 00074000
*   ELSE                            /* NO ERROR                      */ 00075000
*     DO;                           /* MAINLINE                      */ 00076000
         B     @RC00241                                            0248 00077000
@RF00241 DS    0H                                                  0249 00078000
*       R11->SABWRD=R13;            /* CHAIN THE SAVE AREA           */ 00079000
         ST    R13,SABWRD(,R11)                                    0249 00080000
*       R13=R11;                    /* LOAD PTR TO AUTOMATICS        */ 00081000
         LR    R13,R11                                             0250 00082000
*       GEN(USING @DATD,R13);       /* GET BASE FOR AUTOMATICS       */ 00083000
         USING @DATD,R13                                                00084000
*       MWASP250=R1;                /* SAVE THE POINTER TO THE UWA   */ 00085000
         ST    R1,MWASP250                                         0252 00086000
*       RETCONT=ZERO;               /* INIT ARRAY CONTROL            */ 00087000
         SLR   @14,@14                                             0253 00088000
         ST    @14,RETCONT                                         0253 00089000
*       CHKWAPTR=SABWRD->SAREG(1);  /* PTR TO CHECKPOINT WORK AREA   */ 00090000
         L     @14,SABWRD(,R13)                                    0254 00091000
         L     @14,SAREG(,@14)                                     0254 00092000
         ST    @14,CHKWAPTR                                        0254 00093000
*       SAVBASEB=RBASEB;            /* SAVE SECOND BASE REGISTER     */ 00094000
         ST    RBASEB,SAVBASEB                                     0255 00095000
*       CKMSGCD1=ZERO;              /* INIT TO INDICATE ERROR MSG  0256 00096000
*                                      IHJ000I TO BE ISSUED  @ZA14803*/ 00097000
         MVI   CKMSGCD1(@14),X'00'                                 0256 00098000
*       CALL RECOVERY;              /* EST. RECOVERY ENVIRONMENT     */ 00099000
         BAL   @14,RECOVERY                                        0257 00100000
*       ADDR(SP250SA)->SABWRD=ADDR(SAVAREA);/* PT UWA SA TO SAVEAREA */ 00101000
         L     @14,MWASP250                                        0258 00102000
         ST    R13,SABWRD(,@14)                                    0258 00103000
*       CKMSGCD2=ZERO;              /* INIT MSG ERROR CODE TO ZERO   */ 00104000
         L     @14,CHKWAPTR                                        0259 00105000
         MVI   CKMSGCD2(@14),X'00'                                 0259 00106000
*       RESPECIFY                   /* FREE REGS FOR COMPILER OPT    */ 00107000
*        (RETCODE,                                                 0260 00108000
*         R11,                                                     0260 00109000
*         RCORE,                                                   0260 00110000
*         R1,                                                      0260 00111000
*         R0) UNRESTRICTED;                                        0260 00112000
*                                                                  0260 00113000
*       /*************************************************************/ 00114000
*       /*                                                           */ 00115000
*       /* INITIALIZE MASK FOR SAVING THE HEADER ELEMENTS            */ 00116000
*       /*                                                           */ 00117000
*       /*************************************************************/ 00118000
*                                                                  0261 00119000
*       RESPECIFY                                                  0261 00120000
*         IDACMB BASED(ADDR(ABSLOC));/* TEMP RESET LOCATION OF CMB   */ 00121000
*       BLDMASK(1)=ONE;             /* SET FIRST BYTE OF MASK TO ONE */ 00122000
         MVI   BLDMASK,X'01'                                       0262 00123000
*       MVC(BLDMASK(2:16),BLDMASK(1:15));/* PROPAGATE, THEN ZERO THE */ 00124000
         MVC   BLDMASK+1(15),BLDMASK                               0263 00125000
*       BLDMASK((ADDR(CMBFSTPT)-ADDR(CMBPTRS))/4+ONE)=ZERO;/* FX STR */ 00126000
         SLR   @02,@02                                             0264 00127000
         STC   @02,BLDMASK+4                                       0264 00128000
*       BLDMASK((ADDR(CMBUFSPT)-ADDR(CMBPTRS))/4+ONE)=ZERO;/* UPG FS */ 00129000
         STC   @02,BLDMASK+5                                       0265 00130000
*       BLDMASK((ADDR(CMBDEBPT)-ADDR(CMBPTRS))/4+ONE)=ZERO;/* DEB BL */ 00131000
         STC   @02,BLDMASK+8                                       0266 00132000
*       BLDMASK((ADDR(CMBFXDPT)-ADDR(CMBPTRS))/4+ONE)=ZERO;/* FIXED     00133000
*                                      BLOCK POINTER         @ZA04718*/ 00134000
*                                                                  0267 00135000
         STC   @02,BLDMASK+12                                      0267 00136000
*       /*************************************************************/ 00137000
*       /*                                                           */ 00138000
*       /* INITIALIZE MASK FOR SETTING THE VCRHFREL FLAG IN VCRHEBSA */ 00139000
*       /*                                                           */ 00140000
*       /*************************************************************/ 00141000
*                                                                  0268 00142000
*       BLDMASK((ADDR(CMBEDBPT)-ADDR(CMBPTRS))/4+ONE)=TWO;/* EDB BLK */ 00143000
         MVI   BLDMASK+9,X'02'                                     0268 00144000
*       RESPECIFY                                                  0269 00145000
*         IDACMB BASED(AMBLCMB);    /* BASE CMB ON POINTER IN AMBL   */ 00146000
*                                                                  0270 00147000
*       /*************************************************************/ 00148000
*       /*                                                           */ 00149000
*       /* BUILD THE CHECKPOINT CONTROL BLOCKS FOR ALL OPEN CLUSTERS */ 00150000
*       /*                                                           */ 00151000
*       /*************************************************************/ 00152000
*                                                                  0270 00153000
*       IF JSCBSHR^=ZERO&VATPAMBL^=ZERO THEN/* VSAM DATA SETS OPEN   */ 00154000
         L     @01,PSATOLD                                         0270 00155000
         L     @01,TCBJSCB(,@01)                                   0270 00156000
         L     @15,JSCBSHR(,@01)                                   0270 00157000
         CR    @15,@02                                             0270 00158000
         BE    @RF00270                                            0270 00159000
         L     @15,VATPAMBL(,@15)                                  0270 00160000
         CR    @15,@02                                             0270 00161000
         BE    @RF00270                                            0270 00162000
*         DO;                                                      0271 00163000
*           PAMBLPTR=VATPAMBL;      /* GET PTR TO FIRST P-AMBL       */ 00164000
         ST    @15,PAMBLPTR                                        0272 00165000
*           CALL SAVEBUF;           /* GET A SAVE AREA FOR BUFFERS   */ 00166000
         BAL   @14,SAVEBUF                                         0273 00167000
*           DO UNTIL PAMBLPTR=0;    /* PROCESS ALL PRIMARY AMBLS     */ 00168000
@DL00274 DS    0H                                                  0275 00169000
*             AMBLPTR=PAMBLPTR;     /* ADDRESSIBILITY FOR AMBL       */ 00170000
         L     @02,PAMBLPTR                                        0275 00171000
         ST    @02,AMBLPTR                                         0275 00172000
*             BIBCKPT=ON;           /* CHECKPOINT PROCESSING SPHERE  */ 00173000
         L     @02,AMBLBIB(,@02)                                   0276 00174000
         OI    BIBCKPT(@02),B'00100000'                            0276 00175000
*             CALL BLDVCRT;         /* BUILD THE VCRT                */ 00176000
         BAL   @14,BLDVCRT                                         0277 00177000
*             CALL HEBSAVE;         /* SAVE HEBS FOR LOCAL STORAGE   */ 00178000
         BAL   @14,HEBSAVE                                         0278 00179000
*             BIBCKPT=OFF;          /* CHECKPOINT OF SPHERE COMPLETE */ 00180000
         L     @02,AMBLPTR                                         0279 00181000
         L     @02,AMBLBIB(,@02)                                   0279 00182000
         NI    BIBCKPT(@02),B'11011111'                            0279 00183000
*             PAMBLPTR=PAMBLPTR->AMBLPCHN;/* POINT TO NEXT P-AMBL    */ 00184000
         L     @02,PAMBLPTR                                        0280 00185000
         L     @02,AMBLPCHN(,@02)                                  0280 00186000
         ST    @02,PAMBLPTR                                        0280 00187000
*           END;                                                   0281 00188000
@DE00274 L     @02,PAMBLPTR                                        0281 00189000
         LTR   @02,@02                                             0281 00190000
         BNZ   @DL00274                                            0281 00191000
*         END;                                                     0282 00192000
*ERREXIT:                                                          0283 00193000
*                                                                  0283 00194000
*       /*************************************************************/ 00195000
*       /*                                                           */ 00196000
*       /* CLEANUP PROCESSING FOR IDA0C06C                           */ 00197000
*       /*                                                           */ 00198000
*       /*************************************************************/ 00199000
*                                                                  0283 00200000
*       RESPECIFY                                                  0283 00201000
*         RCORE RESTRICTED;                                        0283 00202000
@RF00270 DS    0H                                                  0283 00203000
ERREXIT  DS    0H                                                  0284 00204000
*       RBASEB=SAVBASEB;            /* MAKE SURE BASE REG 12 IS OK   */ 00205000
         L     RBASEB,SAVBASEB                                     0284 00206000
*       IF PFSAVEIO=ON THEN                                        0285 00207000
         TM    PFSAVEIO,B'10000000'                                0285 00208000
         BNO   @RF00285                                            0285 00209000
*         CALL IORESTOR;                                           0286 00210000
         BAL   @14,IORESTOR                                        0286 00211000
*       RCORE=ADDR(FORCORE);        /* FORCORE POINTER FOR CLEANUP   */ 00212000
@RF00285 LA    RCORE,MWAFCORE                                      0287 00213000
*       IF CKMSGCD2^=ZERO THEN      /* CHECK FOR ANY ERROR CONDITION */ 00214000
         L     @05,CHKWAPTR                                        0288 00215000
         CLI   CKMSGCD2(@05),0                                     0288 00216000
         BE    @RF00288                                            0288 00217000
*         CALL IDACI96C;            /* YES, REMOVE VSAM CHECKPT CORE */ 00218000
         L     @15,@CV04136                                        0289 00219000
         BALR  @14,@15                                             0289 00220000
*       ELSE                        /* NO ERRORS                     */ 00221000
*         CKRETCD2=ZERO;            /* SET THE RETURN CODE TO ZERO   */ 00222000
         B     @RC00288                                            0290 00223000
@RF00288 L     @05,CHKWAPTR                                        0290 00224000
         MVI   CKRETCD2(@05),X'00'                                 0290 00225000
*       IF CKMSGCD2^=CKMSG240 THEN  /* IF ESTAE WAS ESTABLISHED      */ 00226000
@RC00288 L     @05,CHKWAPTR                                        0291 00227000
         CLI   CKMSGCD2(@05),240                                   0291 00228000
         BE    @RF00291                                            0291 00229000
*         GEN(ESTAE 0);             /* RELEASE RECOVERY ENVIRONMENT  */ 00230000
         ESTAE 0                                                        00231000
*     END;                                                         0293 00232000
@RF00291 DS    0H                                                  0294 00233000
*   RESPECIFY                                                      0294 00234000
*    (R0,                                                          0294 00235000
*     R1,                                                          0294 00236000
*     R11) RESTRICTED;                                             0294 00237000
@RC00241 DS    0H                                                  0295 00238000
*   IF RCORE^=ZERO THEN             /* WERE ALL WORK AREAS OBTAINED  */ 00239000
         LTR   RCORE,RCORE                                         0295 00240000
         BZ    @RF00295                                            0295 00241000
*     DO;                           /* YES, FREE THE UNPROTECTED WA  */ 00242000
*       MWASP=USERPOOL;             /* SET USER SUBPOOL              */ 00243000
         MVI   MWASP,X'FA'                                         0297 00244000
*       MWALEN=(LENGTH(SP250WA)+SEVEN)&-DWORDLEN;/* LENGTH OF UWA    */ 00245000
         MVC   MWALEN(3),@CF01679+1                                0298 00246000
*       MWAPTR=ADDR(SP250WA);       /* GET ADDRESS UNPROTECTED W.A.  */ 00247000
         L     @10,MWASP250                                        0299 00248000
         ST    @10,MWAPTR                                          0299 00249000
*       LM(R0,R1,MWAFREMN);         /* LOAD THE FREEMAIN REGISTERS   */ 00250000
         LM    R0,R1,MWAFREMN                                      0300 00251000
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE UNPROTECTED WORK AREA   */ 00252000
         FREEMAIN R,LV=(0),A=(1)                                        00253000
*     END;                                                         0302 00254000
*   GEN(L R0,@SIZDATD);             /* LOAD R0 WITH SP & SIZE OF MWA */ 00255000
@RF00295 DS    0H                                                  0303 00256000
         L R0,@SIZDATD                                                  00257000
*   IF RCORE=ZERO THEN              /* WERE ALL WORK AREAS OBTAINED? */ 00258000
         LTR   RCORE,RCORE                                         0304 00259000
         BNZ   @RF00304                                            0304 00260000
*     R1=R11;                       /* NO, FREE MWA IF OBTAINED      */ 00261000
         LR    R1,R11                                              0305 00262000
*   ELSE                            /* WORK AREAS GOTTEN SO          */ 00263000
*     DO;                           /* PREPARE TO RETURN             */ 00264000
         B     @RC00304                                            0306 00265000
@RF00304 DS    0H                                                  0307 00266000
*       R1=R13;                     /* LOAD REG 1 WITH ADDRESS OF MWA*/ 00267000
         LR    R1,R13                                              0307 00268000
*       R13=SABWRD;                 /* POINT R13 AT PREVIOUS SAVE    */ 00269000
         L     R13,SABWRD(,R13)                                    0308 00270000
*     END;                                                         0309 00271000
*   IF R1^=ZERO THEN                /* IF THERE IS A MWA TO FREE     */ 00272000
@RC00304 LTR   R1,R1                                               0310 00273000
         BZ    @RF00310                                            0310 00274000
*     GEN(FREEMAIN R,LV=(0),A=(1)); /* FREE MODULE WORK AREA         */ 00275000
         FREEMAIN R,LV=(0),A=(1)                                        00276000
*   LM(RET,R12,SAREGS);             /* RESTORE CALLERS REGS          */ 00277000
@RF00310 LM    RET,R12,SAREGS(R13)                                 0312 00278000
*   GEN(BR RET);                    /* RETURN TO CALLER              */ 00279000
         BR RET                                                         00280000
*   RESPECIFY                                                      0314 00281000
*    (R0,                                                          0314 00282000
*     R1,                                                          0314 00283000
*     RCORE,                                                       0314 00284000
*     R11) UNRESTRICTED;                                           0314 00285000
*RECOVERY:                                                         0315 00286000
*                                                                  0315 00287000
*/********************************************************************/ 00288000
*/*                                                                  */ 00289000
*/*   FUNCTION - ESTABLISH THE RECOVERY ENVIRONMENT FOR IDA0C06C.    */ 00290000
*/*   INPUT    - THE MODULE WORKAREA WITH MWAFCORE DEFINED           */ 00291000
*/*   OUTPUT   - THE ESTAE IS ISSUE WITH MWAFCORE AS THE PARAMETER   */ 00292000
*/*                                                                  */ 00293000
*/********************************************************************/ 00294000
*                                                                  0315 00295000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0315 00296000
RECOVERY DS    0H                                                  0316 00297000
*   RESPECIFY                                                      0316 00298000
*    (R0,                                                          0316 00299000
*     R1,                                                          0316 00300000
*     RET,                                                         0316 00301000
*     RETCODE) RESTRICTED;                                         0316 00302000
*   IDAESPL=ESTAEPRM;               /* COPY SKELETON INTO WORK AREA  */ 00303000
         LA    @02,MWAFCORE                                        0317 00304000
         MVC   IDAESPL(16,@02),ESTAEPRM                            0317 00305000
*   R1=ADDR(IDAESPL);               /* PUT PTR TO PARM LIST IN REG 1 */ 00306000
         LA    R1,IDAESPL(,@02)                                    0318 00307000
*   GEN(ESTAE PARAM=MWAFCORE,MF=(E,(1)));/* ISSUE ESTAE MACRO        */ 00308000
         ESTAE PARAM=MWAFCORE,MF=(E,(1))                                00309000
*   IF RETCODE^=ZERO THEN           /* CHECK THE RETURN CODE         */ 00310000
         LTR   RETCODE,RETCODE                                     0320 00311000
         BZ    @RF00320                                            0320 00312000
*     DO;                           /* IF NOT ZERO                   */ 00313000
*       CKMSGCD2=CKMSG240;          /* SET THE CHECKPOINT MSG CODE   */ 00314000
         L     @02,CHKWAPTR                                        0322 00315000
         MVI   CKMSGCD2(@02),X'F0'                                 0322 00316000
*       CKRETCD2=CHKERR0C;          /* SET CHECKPOINT RETURN CODE    */ 00317000
         MVI   CKRETCD2(@02),X'0C'                                 0323 00318000
*       RETURN TO ERREXIT;          /* AND EXIT THE MODULE           */ 00319000
         LA    @14,ERREXIT                                         0324 00320000
@EL00002 DS    0H                                                  0324 00321000
@EF00002 DS    0H                                                  0324 00322000
@ER00002 BR    @14                                                 0324 00323000
*     END;                                                         0325 00324000
*   IDABASER=RBASEA;                /* SAVE BASE REG FOR RECOVERY    */ 00325000
@RF00320 LA    @02,MWAFCORE                                        0326 00326000
         ST    RBASEA,IDABASER(,@02)                               0326 00327000
*   IDADATAR=R13;                   /* SAVE DATA REG FOR RECOVERY    */ 00328000
         ST    R13,IDADATAR(,@02)                                  0327 00329000
*   IDARETRY=ADDR(ERREXIT);         /* RETRY ROUTINE IS ERREXIT      */ 00330000
         LA    @11,ERREXIT                                         0328 00331000
         ST    @11,IDARETRY(,@02)                                  0328 00332000
*   IDACRWAP=ADDR(CHKWA);           /* SAVE ADDR OF CHKPT WORK AREA  */ 00333000
         L     @11,CHKWAPTR                                        0329 00334000
         ST    @11,IDACRWAP(,@02)                                  0329 00335000
*   IDARCKPT=ON;                    /* INDICATE CHECKPOINT           */ 00336000
         OI    IDARCKPT(@02),B'10000000'                           0330 00337000
*   RSTATRTN=IDARC06C;              /* NOTE IDA0C06C IN CONTROL      */ 00338000
         MVC   RSTATRTN(4,@02),@CC03962                            0331 00339000
*   RESPECIFY                                                      0332 00340000
*    (R0,                                                          0332 00341000
*     R1,                                                          0332 00342000
*     RET,                                                         0332 00343000
*     RETCODE) UNRESTRICTED;                                       0332 00344000
*   END RECOVERY;                                                  0333 00345000
         B     @EL00002                                            0333 00346000
*BLDVCRT:                                                          0334 00347000
*                                                                  0334 00348000
*/********************************************************************/ 00349000
*/*                                                                  */ 00350000
*/*   FUNCTION - BUILDS THE VCRT FOR A SPHERE AND CALLS THE          */ 00351000
*/*              APPROPRIATE ROUTINES TO OBTAIN REPOSITIONING        */ 00352000
*/*              INFORMATION WHEN REQUESTED.                         */ 00353000
*/*   INPUT    - POINTER TO THE PRIMARY AMBL IN AMBLPTR              */ 00354000
*/*   OUTPUT   - VCRT IS BUILT                                       */ 00355000
*/*              REPOSITIONING INFORMATION IS SAVED                  */ 00356000
*/*                                                                  */ 00357000
*/********************************************************************/ 00358000
*                                                                  0334 00359000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0334 00360000
BLDVCRT  DS    0H                                                  0335 00361000
*   RESPECIFY                                                      0335 00362000
*     RET RESTRICTED;                                              0335 00363000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 00364000
         LA    @15,1                                               0336 00365000
         AL    @15,RETCONT                                         0336 00366000
         ST    @15,RETCONT                                         0336 00367000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 00368000
         SLA   @15,2                                               0337 00369000
         ST    RET,RETSAVE-4(@15)                                  0337 00370000
*   RESPECIFY                                                      0338 00371000
*     RET UNRESTRICTED;                                            0338 00372000
*   VCRCPFLP=ON;                    /* REQUEST FOR CORE PLUS EXCESS  */ 00373000
         OI    VCRCPFLP,B'10000000'                                0339 00374000
*   VCRCLENG=LENGTH(IDAVCRT);       /* SPECIFY MINIMUM LENGTH        */ 00375000
         MVC   VCRCLENG(4),@CF02254                                0340 00376000
*   VCRCPOOL=WKPOOL;                /* SPECIFY SUBPOOL               */ 00377000
         MVI   VCRCPOOL,X'FC'                                      0341 00378000
*   CALL GETCORE;                   /* GET CORE FOR THE VCRT         */ 00379000
         BAL   @14,GETCORE                                         0342 00380000
*   BIBVCRT=VCRCADDR;               /* CONNECT THE VCRT TO THE BIB   */ 00381000
         L     @02,VCRCADDR                                        0343 00382000
         L     @15,AMBLPTR                                         0343 00383000
         L     @01,AMBLBIB(,@15)                                   0343 00384000
         ST    @02,BIBVCRT(,@01)                                   0343 00385000
*   VCRTPTR=VCRCADDR;               /* VCRT ADDRESSIBILITY           */ 00386000
         ST    @02,VCRTPTR                                         0344 00387000
*   VCRCOREH=VCRCHEAD;              /* POINT TO HEAD CORE BLOCK      */ 00388000
         L     @14,VCRCHEAD                                        0345 00389000
         ST    @14,VCRCOREH(,@02)                                  0345 00390000
*   VCRID=IDXVCRT;                  /* SET VCRT ID BYTE              */ 00391000
