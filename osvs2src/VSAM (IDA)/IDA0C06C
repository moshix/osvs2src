*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 01750000
         BCTR  @02,0                                               0651 01751000
         ST    @02,RETCONT                                         0651 01752000
*   RESPECIFY                                                      0652 01753000
*     RET UNRESTRICTED;                                            0652 01754000
*BLDHEBS:                                                          0653 01755000
*                                                                  0653 01756000
*/********************************************************************/ 01757000
*/*                                                                  */ 01758000
*/*   FUNCTION - BUILDS THE HEB SAVE AREA HEADER FOR THE CURRENT CMB */ 01759000
*/*              POINTER AND SAVES THE HEADER ELEMENTS.              */ 01760000
*/*   INPUT    - THE CURRENT HEB POINTER IN HEBPTR                   */ 01761000
*/*              THE CURRENT HEADER POINTER OR ZERO IN HEBSAPTR      */ 01762000
*/*              THE RELATIVE CMB ENTRY NUMBER IN C2                 */ 01763000
*/*              THE VCRUPGSW SET FOR PROCESSING UPGRADE ENTRIES     */ 01764000
*/*              THE VCRT ENTRY NUMBER IN C1                         */ 01765000
*/*   OUTPUT   - THE HEB ELEMENTS FOR THE CMB ENTRY ARE SAVED AND    */ 01766000
*/*              CHAINED TO EITHER THE PREVIOUS SAVE HEADER OR THE   */ 01767000
*/*              APPROPRIATE VCRT ENTRY FIELD.                       */ 01768000
*/*                                                                  */ 01769000
*/********************************************************************/ 01770000
*                                                                  0653 01771000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0653 01772000
         B     @PB00011                                            0653 01773000
BLDHEBS  DS    0H                                                  0654 01774000
*   RESPECIFY                                                      0654 01775000
*     RET RESTRICTED;                                              0654 01776000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 01777000
         LA    @15,1                                               0655 01778000
         AL    @15,RETCONT                                         0655 01779000
         ST    @15,RETCONT                                         0655 01780000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 01781000
         SLA   @15,2                                               0656 01782000
         ST    RET,RETSAVE-4(@15)                                  0656 01783000
*   RESPECIFY                                                      0657 01784000
*     RET UNRESTRICTED;                                            0657 01785000
*   IF VCRCLENG<LENGTH(VCRHHDR)+LENGTH(HEBHDREL) THEN/* IF NO CORE   */ 01786000
         LA    @06,32                                              0658 01787000
         C     @06,VCRCLENG                                        0658 01788000
         BNH   @RF00658                                            0658 01789000
*     DO;                           /* THEN                          */ 01790000
*       VCRCLENG=LENGTH(VCRHHDR)+LENGTH(HEBHDREL);/* SET MIN LENGTH  */ 01791000
         ST    @06,VCRCLENG                                        0660 01792000
*       VCRCPFLP=ON;                /* INDICATE REQUEST TYPE         */ 01793000
         OI    VCRCPFLP,B'10000000'                                0661 01794000
*       CALL GETCORE;               /* GET IT                        */ 01795000
         BAL   @14,GETCORE                                         0662 01796000
*     END;                          /* GOT IT                        */ 01797000
*                                                                  0663 01798000
*   /*****************************************************************/ 01799000
*   /*                                                               */ 01800000
*   /* GET AND INITIALIZE THE HEB SAVE AREA HEADER                   */ 01801000
*   /*                                                               */ 01802000
*   /*****************************************************************/ 01803000
*                                                                  0664 01804000
