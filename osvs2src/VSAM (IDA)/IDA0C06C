*                                                                  0646 01728000
         BAL   @14,BLDHEBS                                         0646 01729000
*   /*****************************************************************/ 01730000
*   /*                                                               */ 01731000
*   /* GIVE BACK ANY UNUSED CORE IN THE CURRENT SUBPOOL 252 BLOCK    */ 01732000
*   /*                                                               */ 01733000
*   /*****************************************************************/ 01734000
*                                                                  0647 01735000
*   CUR252C->VCRCPTRA=VCRCADDR;     /* SET FIRST AVAILABLE BYTE FLD  */ 01736000
@RF00645 L     @02,CUR252C                                         0647 01737000
