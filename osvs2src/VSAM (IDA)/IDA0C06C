         SLR   @11,@14                                             0348 00398000
         ST    @11,VCRCLENG                                        0348 00399000
*   VCRCADDR=VCRCADDR+LENGTH(IDAVCRT);/* ADDRESS OF AVAILABLE CORE   */ 00400000
         ALR   @02,@14                                             0349 00401000
         ST    @02,VCRCADDR                                        0349 00402000
*   CKRETCD2=CHKERR08;              /* PREPARE FOR ENVIRONMENTAL ERR */ 00403000
         L     @02,CHKWAPTR                                        0350 00404000
         MVI   CKRETCD2(@02),X'08'                                 0350 00405000
*   IF AMBLGSR=ON THEN              /* WAS THE GSR OPTION SPECIFIED  */ 00406000
