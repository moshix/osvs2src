*         RET=VCROPN;               /* GET PTR TO OPEN ENTRIES       */ 00653000
         L     @02,VCRTPTR                                         0403 00654000
         L     RET,VCROPN(,@02)                                    0403 00655000
*         R1=VCROPNCT*LENGTH(VCRENTRY);/* TOTAL LENGTH OF ENTRIES    */ 00656000
         LH    R1,VCROPNCT(,@02)                                   0404 00657000
         SLA   R1,3                                                0404 00658000
*         R15=R1;                   /* ALSO IN REGISTER 15 FOR MVCL  */ 00659000
         LR    R15,R1                                              0405 00660000
*         VCRCLENG=VCRCLENG-R1;     /* UPDATE LENGTH OF AVAIL CORE   */ 00661000
         L     @11,VCRCLENG                                        0406 00662000
         SLR   @11,R1                                              0406 00663000
