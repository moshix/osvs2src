         STH   ENTRYCNT,VCRIDXCT(,@07)                             0517 01165000
*   VCRCPFEL=ON;                    /* GET EXACT LENGTH REQUESTED    */ 01166000
         OI    VCRCPFEL,B'01000000'                                0518 01167000
*   VCRCLENG=LENGTH(VCRINDEX)*ENTRYCNT;/* SET LENGTH THIS REQUEST    */ 01168000
         LR    @07,ENTRYCNT                                        0519 01169000
         SLA   @07,3                                               0519 01170000
         ST    @07,VCRCLENG                                        0519 01171000
*   CALL GETCORE;                   /* GET THE STORAGE               */ 01172000
         BAL   @14,GETCORE                                         0520 01173000
*   VCRIDX=VCRCADDR;                /* INDEX ENTRIES START ADDRESS   */ 01174000
         L     @02,VCRCADDR                                        0521 01175000
         L     @01,VCRTPTR                                         0521 01176000
         ST    @02,VCRIDX(,@01)                                    0521 01177000
*   CALL IOSAVE;                    /* SAVE STATUS OF USER BLOCKS    */ 01178000
         BAL   @14,IOSAVE                                          0522 01179000
*   IF VCRIDXCT=ZERO THEN           /* IF THERE ARE NO ACTIVE ICWAS  */ 01180000
         SLR   @02,@02                                             0523 01181000
         L     @15,VCRTPTR                                         0523 01182000
         CH    @02,VCRIDXCT(,@15)                                  0523 01183000
         BNE   @RF00523                                            0523 01184000
*     VCRIDX=ZERO;                  /* ZERO PTR TO INDEX ENTRIES     */ 01185000
         ST    @02,VCRIDX(,@15)                                    0524 01186000
*   ELSE                            /* OTHERWISE, ACTIVE ICWAS SO    */ 01187000
*     VCRICWA(ONE)=AMBIWA;          /* PUT ADDRESS 1ST ICWA IN ENTRY */ 01188000
         B     @RC00523                                            0525 01189000
@RF00523 L     @02,VCRTPTR                                         0525 01190000
         L     @02,VCRIDX(,@02)                                    0525 01191000
         L     @01,AMBPTR                                          0525 01192000
         L     @15,AMBIWA(,@01)                                    0525 01193000
         ST    @15,VCRICWA(,@02)                                   0525 01194000
*   DO C1=ONE TO VCRIDXCT BY ONE;   /* PROCESS ALL ACTIVE ICWAS      */ 01195000
@RC00523 LA    @02,1                                               0526 01196000
         ST    @02,C1                                              0526 01197000
         B     @DE00526                                            0526 01198000
@DL00526 DS    0H                                                  0527 01199000
*     IF C1<VCRIDXCT THEN           /* IF THIS NOT LAST INDEX ENTRY  */ 01200000
         L     @15,VCRTPTR                                         0527 01201000
         CH    @02,VCRIDXCT(,@15)                                  0527 01202000
         BNL   @RF00527                                            0527 01203000
*       VCRICWA(C1+ONE)=ICWCHN;     /* PTR NEXT ICWA IN NEXT ENTRY   */ 01204000
         SLA   @02,3                                               0528 01205000
         L     @15,VCRIDX(,@15)                                    0528 01206000
