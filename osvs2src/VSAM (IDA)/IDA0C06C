*   VCRUPGSW=OFF;                   /* PROCESSING OPEN ENTRIES       */ 01551000
         L     @02,VCRTPTR                                         0613 01552000
         NI    VCRUPGSW(@02),B'01111111'                           0613 01553000
*   DO C1=ONE TO VCROPNCT BY ONE;   /* PROCESS ALL OPEN ENTRIES      */ 01554000
         ST    @15,C1                                              0614 01555000
         B     @DE00614                                            0614 01556000
@DL00614 DS    0H                                                  0615 01557000
*     AMBLPTR=VCRAMBL(C1);          /* ADDRESSIBILITY FOR AMBL       */ 01558000
