*   RESPECIFY                                                      0385 00580000
*     RET UNRESTRICTED;                                            0385 00581000
*   END BLDVCRT;                                                   0386 00582000
         B     @EL00003                                            0386 00583000
*BLDOPEN:                                                          0387 00584000
*                                                                  0387 00585000
*/********************************************************************/ 00586000
*/*                                                                  */ 00587000
*/*   FUNCTION - BUILDS AN OPEN ENTRY FOR EVERY PRIMARY AMBL IN THE  */ 00588000
*/*              SPHERE.                                             */ 00589000
*/*   INPUT    - A POINTER TO THE CURRENT VCRT IN VCRTPTR            */ 00590000
*/*              A POINTER TO THE PRIMARY AMBL IN AMBLPTR            */ 00591000
*/*   OUTPUT   - THE VCRT OPEN ENTRIES ARE BUILT                     */ 00592000
*/*              THE VCROUT BIT IS SET IF ANY AMBL IS OPEN FOR WRITE */ 00593000
*/*                                                                  */ 00594000
*/********************************************************************/ 00595000
*                                                                  0387 00596000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0387 00597000
BLDOPEN  DS    0H                                                  0388 00598000
*   RESPECIFY                                                      0388 00599000
*     RET RESTRICTED;                                              0388 00600000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 00601000
         LA    @15,1                                               0389 00602000
         L     @07,RETCONT                                         0389 00603000
         ALR   @07,@15                                             0389 00604000
         ST    @07,RETCONT                                         0389 00605000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 00606000
         SLA   @07,2                                               0390 00607000
         ST    RET,RETSAVE-4(@07)                                  0390 00608000
*   RESPECIFY                                                      0391 00609000
*     RET UNRESTRICTED;                                            0391 00610000
*   ENTRYCNT=ONE;                   /* INITIALIZE THE ENTRY COUNT    */ 00611000
         LR    ENTRYCNT,@15                                        0392 00612000
*   VCROPN=VCRCADDR;                /* POINT TO THE FIRST OPEN ENTRY */ 00613000
         L     @07,VCRCADDR                                        0393 00614000
         L     @02,VCRTPTR                                         0393 00615000
         ST    @07,VCROPN(,@02)                                    0393 00616000
*   DO UNTIL AMBLPTR=ZERO;          /* PROCESS ALL AMBLS FOR BASE    */ 00617000
*                                                                  0394 00618000
@DL00394 DS    0H                                                  0395 00619000
*     /***************************************************************/ 00620000
*     /*                                                             */ 00621000
*     /* IF INSUFFICIENT VCRT OPEN ENTRIES IN THE CURRENT STORAGE    */ 00622000
*     /* BLOCK, GET AND INITIALIZE A NEW BLOCK WITH THE ENTRIES      */ 00623000
*     /* ALREADY BUILT.                                              */ 00624000
*     /*                                                             */ 00625000
*     /***************************************************************/ 00626000
*                                                                  0395 00627000
*     IF VCRCLENG<TWO*LENGTH(VCRENTRY) THEN/* ROOM FOR TWO ENTRIES?  */ 00628000
         LA    @07,16                                              0395 00629000
         C     @07,VCRCLENG                                        0395 00630000
         BNH   @RF00395                                            0395 00631000
*       DO;                         /* IF NOT, GET MORE CORE         */ 00632000
*         VCRCPFLP=ON;              /* GET EVERYTHING IN THE BLOCK   */ 00633000
         OI    VCRCPFLP,B'10000000'                                0397 00634000
*         VCRCLENG=(VCRCADDR-VCROPN)+TWO*LENGTH(VCRENTRY);/* MIN LEN */ 00635000
         L     @02,VCRCADDR                                        0398 00636000
