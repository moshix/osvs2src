*         VCROPNCT=ENTRYCNT;        /* SET THE OPEN ENTRY COUNT      */ 00727000
         STH   ENTRYCNT,VCROPNCT(,@15)                             0423 00728000
*         ENTRYCNT=ENTRYCNT+ONE;    /* INCREMENT THE CURRENT COUNT   */ 00729000
         AL    ENTRYCNT,@CF00053                                   0424 00730000
*         VCRCADDR=VCRCADDR+LENGTH(VCRENTRY);/* NEXT AVAILABLE BYTE  */ 00731000
         LA    @07,8                                               0425 00732000
         L     @02,VCRCADDR                                        0425 00733000
         ALR   @02,@07                                             0425 00734000
         ST    @02,VCRCADDR                                        0425 00735000
*         VCRCLENG=VCRCLENG-LENGTH(VCRENTRY);/* LNGTH AVAILABLE CORE */ 00736000
         LCR   @07,@07                                             0426 00737000
         AL    @07,VCRCLENG                                        0426 00738000
         ST    @07,VCRCLENG                                        0426 00739000
*       END;                                                       0427 00740000
*     IF AMBLWRIT=ON THEN           /* IF AMBL IS OPEN FOR OUTPUT    */ 00741000
@RF00420 L     @07,AMBLPTR                                         0428 00742000
         TM    AMBLWRIT(@07),B'00100000'                           0428 00743000
         BNO   @RF00428                                            0428 00744000
*       VCROUT=ON;                  /* THEN SET VCROUT ON            */ 00745000
         L     @07,VCRTPTR                                         0429 00746000
