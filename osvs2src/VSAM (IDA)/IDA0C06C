         AL    @01,@CF04232                                        0528 01208000
         L     @01,VCRICWA(,@01)                                   0528 01209000
         L     @14,ICWCHN(,@01)                                    0528 01210000
         ST    @14,VCRICWA(@02,@15)                                0528 01211000
*     VCRCLENG=AMDCINV;             /* GET CONTROL INTERVAL LENGTH   */ 01212000
@RF00527 L     @02,AMBPTR                                          0529 01213000
         L     @02,AMBDSB(,@02)                                    0529 01214000
         L     @02,AMDCINV(,@02)                                   0529 01215000
         ST    @02,VCRCLENG                                        0529 01216000
*     VCRCPOOL=USERPOOL;            /* GET FROM SUBPOOL 252 - USERS  */ 01217000
         MVI   VCRCPOOL,X'FA'                                      0530 01218000
*     VCRCPFEL=ON;                  /* GET EXACT LENGTH              */ 01219000
         OI    VCRCPFEL,B'01000000'                                0531 01220000
*     CALL GETCORE;                 /* GET THE INDEX CI BUFFER       */ 01221000
         BAL   @14,GETCORE                                         0532 01222000
*     VCRBUFPT(C1)=VCRCADDR;        /* PUT BUFFER ADDRESS IN ENTRY   */ 01223000
         L     @02,C1                                              0533 01224000
         SLA   @02,3                                               0533 01225000
         L     @01,VCRTPTR                                         0533 01226000
         L     @01,VCRIDX(,@01)                                    0533 01227000
         L     @00,VCRCADDR                                        0533 01228000
         AL    @01,@CF04237                                        0533 01229000
         ST    @00,VCRBUFPT-4(@02,@01)                             0533 01230000
*     RPLAREA=VCRBUFPT(C1);         /* POINT TO BUFFER AREA          */ 01231000
         L     @02,MWASP250                                        0534 01232000
         ST    @00,RPLAREA+72(,@02)                                0534 01233000
*     CALL IDXGET;                  /* CALL ROUTINE TO GET INDEX CI  */ 01234000
         BAL   @14,IDXGET                                          0535 01235000
*   END;                                                           0536 01236000
         LA    @02,1                                               0536 01237000
         AL    @02,C1                                              0536 01238000
         ST    @02,C1                                              0536 01239000
@DE00526 L     @01,VCRTPTR                                         0536 01240000
         CH    @02,VCRIDXCT(,@01)                                  0536 01241000
         BNH   @DL00526                                            0536 01242000
*   CALL IORESTOR;                  /* RESTORE USER BLOCKS           */ 01243000
         BAL   @14,IORESTOR                                        0537 01244000
*   RESPECIFY                                                      0538 01245000
*     RET RESTRICTED;                                              0538 01246000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 01247000
         L     @02,RETCONT                                         0539 01248000
         LR    @15,@02                                             0539 01249000
         SLA   @15,2                                               0539 01250000
         L     RET,RETSAVE-4(@15)                                  0539 01251000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 01252000
         BCTR  @02,0                                               0540 01253000
         ST    @02,RETCONT                                         0540 01254000
*   RESPECIFY                                                      0541 01255000
*     RET UNRESTRICTED;                                            0541 01256000
*   END BLDINDEX;                                                  0542 01257000
@EL00007 DS    0H                                                  0542 01258000
@EF00007 DS    0H                                                  0542 01259000
@ER00007 BR    @14                                                 0542 01260000
*GETRTN:                                                           0543 01261000
*                                                                  0543 01262000
*/********************************************************************/ 01263000
*/*                                                                  */ 01264000
*/*   FUNCTION - READS THE CURRENT CI INTO A BUFFER OBTAINED         */ 01265000
*/*              DYNAMICALLY AND POINTED TO BY THE VCRT. THE BUFFER  */ 01266000
*/*              WILL BE REWRITTEN IF A CHECKPOINT IS REQUESTED.     */ 01267000
*/*   INPUT    - A POINTER TO THE PRIMARY AMBL IN PAMBLPTR           */ 01268000
*/*              A POINTER TO THE DATA AMB IN AMBPTR                 */ 01269000
*/*              A POINTER TO THE CURRENT VCRT IN VCRTPTR            */ 01270000
*/*   OUTPUT   - CURRENT CI SAVED IN CHECKPOINT/RESTART BUFFER       */ 01271000
*/*                                                                  */ 01272000
*/********************************************************************/ 01273000
*                                                                  0543 01274000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0543 01275000
GETRTN   DS    0H                                                  0544 01276000
*   RESPECIFY                                                      0544 01277000
*     RET RESTRICTED;                                              0544 01278000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 01279000
         LA    @15,1                                               0545 01280000
         AL    @15,RETCONT                                         0545 01281000
         ST    @15,RETCONT                                         0545 01282000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 01283000
         SLA   @15,2                                               0546 01284000
         ST    RET,RETSAVE-4(@15)                                  0546 01285000
*   RESPECIFY                                                      0547 01286000
*     RET UNRESTRICTED;                                            0547 01287000
*                                                                  0547 01288000
*   /*****************************************************************/ 01289000
*   /*                                                               */ 01290000
*   /* GET THE BUFFER FOR SAVING THE CONTROL INTERVAL                */ 01291000
*   /*                                                               */ 01292000
*   /*****************************************************************/ 01293000
*                                                                  0548 01294000
*   VCRCPFEL=ON;                    /* EXACT LENGTH REQUEST          */ 01295000
         OI    VCRCPFEL,B'01000000'                                0548 01296000
*   VCRCPOOL=USERPOOL;              /* USERS SUBPOOL 250             */ 01297000
         MVI   VCRCPOOL,X'FA'                                      0549 01298000
*   VCRCLENG=AMDCINV;               /* LENGTH OF CONTROL INTERVAL    */ 01299000
         L     @02,AMBPTR                                          0550 01300000
         L     @02,AMBDSB(,@02)                                    0550 01301000
         L     @02,AMDCINV(,@02)                                   0550 01302000
         ST    @02,VCRCLENG                                        0550 01303000
*   CALL GETCORE;                   /* GET THE CORE                  */ 01304000
         BAL   @14,GETCORE                                         0551 01305000
*   VCRRBUF=VCRCADDR;               /* PUT THE ADDRESS IN THE VCRT   */ 01306000
         L     @02,VCRCADDR                                        0552 01307000
         L     @01,VCRTPTR                                         0552 01308000
         ST    @02,VCRRBUF(,@01)                                   0552 01309000
*   CALL IOSAVE;                    /* SAVE STATUS OF USER BLOCKS    */ 01310000
         BAL   @14,IOSAVE                                          0553 01311000
*   CINVRBA=ARDHRBA-AMDCINV;        /* RBA OF CONTROL INTERVAL       */ 01312000
         L     @02,AMBPTR                                          0554 01313000
         L     @02,AMBDSB(,@02)                                    0554 01314000
         L     @01,AMDPARDB(,@02)                                  0554 01315000
         L     @15,ARDHRBA(,@01)                                   0554 01316000
         SL    @15,AMDCINV(,@02)                                   0554 01317000
         ST    @15,CINVRBA                                         0554 01318000
*   RPLARG=ADDR(CINVRBA);           /* RBA ARGUMENT                  */ 01319000
         L     @02,MWASP250                                        0555 01320000
         LA    @15,72                                              0555 01321000
         ALR   @15,@02                                             0555 01322000
         LA    @14,CINVRBA                                         0555 01323000
         ST    @14,RPLARG(,@15)                                    0555 01324000
*   RPLAREA=VCRRBUF;                /* PT TO BUFFER AREA             */ 01325000
*                                                                  0556 01326000
         L     @01,VCRTPTR                                         0556 01327000
         L     @14,VCRRBUF(,@01)                                   0556 01328000
         ST    @14,RPLAREA(,@15)                                   0556 01329000
*   /*****************************************************************/ 01330000
*   /*                                                               */ 01331000
*   /* INITIALIZE REGISTERS FOR GET REQUEST AND SYNCH TO THE RECORD  */ 01332000
*   /* MANAGEMENT INTERFACE ROUTINE.                                 */ 01333000
*   /*                                                               */ 01334000
*   /*****************************************************************/ 01335000
*                                                                  0557 01336000
*   RESPECIFY                                                      0557 01337000
*    (R0,                                                          0557 01338000
*     R1,                                                          0557 01339000
*     R15) RESTRICTED;                                             0557 01340000
*   R0=RPLGET;                      /* INDICATE GET REQUEST          */ 01341000
         ST    @15,@TF00001                                        0558 01342000
         SLR   R0,R0                                               0558 01343000
*   R1=ADDR(IFGRPL);                /* GET POINTER TO RPL            */ 01344000
         L     R1,@TF00001                                         0559 01345000
*   R15=BIBINTRF;                   /* GET PT TO INTERFACE ROUTINE   */ 01346000
         L     @14,AMBLPTR                                         0560 01347000
         L     @14,AMBLBIB(,@14)                                   0560 01348000
         L     R15,BIBINTRF(,@14)                                  0560 01349000
*   STM(RACB,R13,XRBESA);           /* SAVE REGISTERS IN RB SAVEAREA */ 01350000
         L     @14,PSATOLD                                         0561 01351000
         L     @14,TCBRBP(,@14)                                    0561 01352000
         STM   RACB,R13,XRBESA(@14)                                0561 01353000
*   R13=ADDR(SP250SA);              /* GET POINTER TO SAVE AREA      */ 01354000
         LR    R13,@02                                             0562 01355000
*   GEN(SYNCH (15));                /* ISSUE SYNCH MACRO             */ 01356000
         SYNCH (15)                                                     01357000
*   LM(RACB,R13,XRBESA);            /* RESTORE CHECKPOINTS REGISTERS */ 01358000
         L     @14,PSATOLD                                         0564 01359000
         L     @14,TCBRBP(,@14)                                    0564 01360000
         LM    RACB,R13,XRBESA(@14)                                0564 01361000
*   RESPECIFY                                                      0565 01362000
*    (R0,                                                          0565 01363000
*     R1,                                                          0565 01364000
*     R15) UNRESTRICTED;                                           0565 01365000
*   CALL IORESTOR;                  /* RESTORE USER BLOCKS           */ 01366000
         BAL   @14,IORESTOR                                        0566 01367000
*   IF RPLRTNCD^=ZERO THEN          /* IF ANY ERRORS                 */ 01368000
         LA    @02,72                                              0567 01369000
         AL    @02,MWASP250                                        0567 01370000
         CLI   RPLRTNCD(@02),0                                     0567 01371000
         BE    @RF00567                                            0567 01372000
*     DO;                           /* THEN                          */ 01373000
*       CKMSGCD2=CKMSG042;          /* INDICATE REPOSITIONING ERROR  */ 01374000
         L     @15,CHKWAPTR                                        0569 01375000
         MVI   CKMSGCD2(@15),X'2A'                                 0569 01376000
*       IF RPLRTNCD=RPLERR0C THEN   /* IF A PHYSICAL ERROR THEN      */ 01377000
         CLI   RPLRTNCD(@02),12                                    0570 01378000
         BNE   @RF00570                                            0570 01379000
*         CKRETCD2=CHKERR0C;        /* SET RETURN CODE FOR I/O ERROR */ 01380000
         MVI   CKRETCD2(@15),X'0C'                                 0571 01381000
*       ELSE                        /* OTHERWISE A LOGICAL ERROR     */ 01382000
*         CKRETCD2=CHKERR08;        /* SET ENVIRONMENTAL RETURN CODE */ 01383000
         B     @RC00570                                            0572 01384000
@RF00570 L     @02,CHKWAPTR                                        0572 01385000
         MVI   CKRETCD2(@02),X'08'                                 0572 01386000
*     END;                                                         0573 01387000
@RC00570 DS    0H                                                  0574 01388000
*   RESPECIFY                                                      0574 01389000
*     RET RESTRICTED;                                              0574 01390000
@RF00567 DS    0H                                                  0575 01391000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 01392000
         L     @02,RETCONT                                         0575 01393000
         LR    @15,@02                                             0575 01394000
         SLA   @15,2                                               0575 01395000
         L     RET,RETSAVE-4(@15)                                  0575 01396000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 01397000
         BCTR  @02,0                                               0576 01398000
         ST    @02,RETCONT                                         0576 01399000
*   RESPECIFY                                                      0577 01400000
*     RET UNRESTRICTED;                                            0577 01401000
*   END GETRTN;                                                    0578 01402000
@EL00008 DS    0H                                                  0578 01403000
@EF00008 DS    0H                                                  0578 01404000
@ER00008 BR    @14                                                 0578 01405000
*IDXGET:                                                           0579 01406000
*                                                                  0579 01407000
*/********************************************************************/ 01408000
*/*                                                                  */ 01409000
*/*   FUNCTION - READS THE CURRENT CI FOR THE INDEX LEVEL ASSOCIATED */ 01410000
