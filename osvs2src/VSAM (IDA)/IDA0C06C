         ST    @02,VCRCLENG                                        0398 00640000
*         VCRCPOOL=WKPOOL;          /* PROTECTED SUBPOOL             */ 00641000
         MVI   VCRCPOOL,X'FC'                                      0399 00642000
*         CALL GETCORE;             /* GO GET THE CORE               */ 00643000
         BAL   @14,GETCORE                                         0400 00644000
*         RESPECIFY                                                0401 00645000
*          (R0,                                                    0401 00646000
*           R1,                                                    0401 00647000
*           RET,                                                   0401 00648000
*           R15) RESTRICTED;                                       0401 00649000
*         R0=VCRCADDR;              /* GET ADDRESS OF NEW CORE       */ 00650000
         L     @07,VCRCADDR                                        0402 00651000
