*   ELSE                            /* IF NOT THE FIRST HEADER       */ 01839000
*     VCRHNEXT=VCRCADDR;            /* CHAIN TO THE PREVIOUS HEADER  */ 01840000
         B     @RC00664                                            0670 01841000
@RF00664 L     @06,VCRCADDR                                        0670 01842000
         ST    @06,VCRHNEXT(,HEBSAPTR)                             0670 01843000
*   HEBSAPTR=VCRCADDR;              /* ADDRESSIBILITY FOR THIS HDR   */ 01844000
@RC00664 L     @06,VCRCADDR                                        0671 01845000
         LR    HEBSAPTR,@06                                        0671 01846000
*   VCRCADDR=VCRCADDR+LENGTH(VCRHHDR);/* POINT TO NEXT AVAIL BYTE    */ 01847000
         LA    @02,8                                               0672 01848000
         ALR   @06,@02                                             0672 01849000
         ST    @06,VCRCADDR                                        0672 01850000
*   VCRCLENG=VCRCLENG-LENGTH(VCRHHDR);/* DECREMENT AVAILABLE LENGTH  */ 01851000
