*         VCRCPFLP=ON;              /* INDICATE REQUEST TYPE         */ 01873000
         OI    VCRCPFLP,B'10000000'                                0681 01874000
*         CALL GETCORE;             /* GET THE CORE                  */ 01875000
         BAL   @14,GETCORE                                         0682 01876000
*         VCRHNEXT=VCRCADDR;        /* CHAIN PREVIOUS HEADER TO NEW  */ 01877000
         L     @06,VCRCADDR                                        0683 01878000
         ST    @06,VCRHNEXT(,HEBSAPTR)                             0683 01879000
*         HEBSAPTR=VCRCADDR;        /* ADDRESSIBILITY FOR NEW HEADER */ 01880000
         LR    HEBSAPTR,@06                                        0684 01881000
*         VCRHFCON=ON;              /* PREV HDR A CONTINUATION HDR   */ 01882000
         OI    VCRHFCON(HEBSAPTR),B'10000000'                      0685 01883000
*         VCRCADDR=VCRCADDR+LENGTH(VCRHHDR);/* PT TO NEXT AVAIL BYTE */ 01884000
         LA    @02,8                                               0686 01885000
         ALR   @06,@02                                             0686 01886000
         ST    @06,VCRCADDR                                        0686 01887000
*         VCRCLENG=VCRCLENG-LENGTH(VCRHHDR);/* DECREMNT AVAIL LENGTH */ 01888000
         LCR   @02,@02                                             0687 01889000
         AL    @02,VCRCLENG                                        0687 01890000
         ST    @02,VCRCLENG                                        0687 01891000
*         IF BLDMASK(C2)=TWO THEN   /* IF CORE TO BE FREED AT RESTRT */ 01892000
         SLR   @06,@06                                             0688 01893000
         IC    @06,BLDMASK-1(C2)                                   0688 01894000
         C     @06,@CF00092                                        0688 01895000
         BNE   @RF00688                                            0688 01896000
*           VCRHFREL=ON;            /* SET RELEASE BIT ON            */ 01897000
         OI    VCRHFREL(HEBSAPTR),B'01000000'                      0689 01898000
*         VCRHCID=C2;               /* SET THE RELATIVE ENTRY NUMBER */ 01899000
@RF00688 STC   C2,VCRHCID(,HEBSAPTR)                               0690 01900000
*       END;                        /* END GETTING NEW HEADER        */ 01901000
*     VCRCADDR=VCRCADDR+LENGTH(HEBHDREL);/* POINT TO NEXT AVAIL BYTE */ 01902000
@RF00678 LA    @06,24                                              0692 01903000
         L     @02,VCRCADDR                                        0692 01904000
         ALR   @02,@06                                             0692 01905000
         ST    @02,VCRCADDR                                        0692 01906000
*     VCRCLENG=VCRCLENG-LENGTH(HEBHDREL);/* DECREMENT AVAIL LENGTH   */ 01907000
         L     @02,VCRCLENG                                        0693 01908000
         SLR   @02,@06                                             0693 01909000
         ST    @02,VCRCLENG                                        0693 01910000
*     VCRHNENT=VCRHNENT+ONE;        /* INCREMENT COUNT OF ELEMENTS   */ 01911000
         LA    @02,1                                               0694 01912000
         AH    @02,VCRHNENT(,HEBSAPTR)                             0694 01913000
         STH   @02,VCRHNENT(,HEBSAPTR)                             0694 01914000
*     VCRHENT(VCRHNENT)=HEBHDREL;   /* COPY HEB ELEMENT TO SAVE AREA */ 01915000
         LR    @01,@02                                             0695 01916000
         MR    @00,@06                                             0695 01917000
         ALR   @01,HEBSAPTR                                        0695 01918000
         AL    @01,@CF04239                                        0695 01919000
         MVC   VCRHENT-8(24,@01),HEBHDREL(HEBPTR)                  0695 01920000
*     HEBPTR=HEBELCHN;              /* POINT TO NEXT HEB ELEMENT     */ 01921000
         L     HEBPTR,HEBELCHN(,HEBPTR)                            0696 01922000
*   END;                            /* END OF SAVING HEB ELEMENT CHN */ 01923000
@DE00677 LTR   HEBPTR,HEBPTR                                       0697 01924000
         BNZ   @DL00677                                            0697 01925000
*   RESPECIFY                                                      0698 01926000
*     RET RESTRICTED;                                              0698 01927000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 01928000
         L     @02,RETCONT                                         0699 01929000
         LR    @15,@02                                             0699 01930000
         SLA   @15,2                                               0699 01931000
         L     RET,RETSAVE-4(@15)                                  0699 01932000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 01933000
         BCTR  @02,0                                               0700 01934000
         ST    @02,RETCONT                                         0700 01935000
*   RESPECIFY                                                      0701 01936000
*     RET UNRESTRICTED;                                            0701 01937000
*   END BLDHEBS;                                                   0702 01938000
@EL00011 DS    0H                                                  0702 01939000
@EF00011 DS    0H                                                  0702 01940000
@ER00011 BR    @14                                                 0702 01941000
*   END HEBSAVE;                    /*                       @ZA04230*/ 01942000
@EL00010 DS    0H                                                  0703 01943000
@EF00010 DS    0H                                                  0703 01944000
@ER00010 BR    @14                                                 0703 01945000
*GETCORE:                                                          0704 01946000
*                                                                  0704 01947000
*/********************************************************************/ 01948000
*/*                                                                  */ 01949000
*/*   FUNCTION - OBTAINS CORE FOR CHECKPOINT/RESTART CONTROL BLOCKS  */ 01950000
*/*              IN THE FORMAT DESCRIBED BY THE VCRCORE STRUCTURE.   */ 01951000
*/*              THE REQUESTOR MAY ASK FOR A SPECIFIC AMOUNT OF CORE */ 01952000
*/*              OR FOR A NON-SPECIFIC AMOUNT WITH THE MINIMUM SIZE  */ 01953000
*/*              SPECIFIED NOT TO EXCEED 4K LESS THE CORE HEADER.    */ 01954000
*/*   INPUT    - SUBPOOL IN VCRCPOOL (SUBPOOL 250 OR 252 ONLY)       */ 01955000
*/*              LENGTH IN VCRCLENG                                  */ 01956000
*/*              REQUEST TYPE IN VCRCPFLG (SPECIFIC OR NON-SPECIFIC) */ 01957000
*/*   OUTPUT   - ADDRESS OF CORE IN VCRCADDR                         */ 01958000
*/*              LENGTH IN VCRCLENG                                  */ 01959000
*/*              VCRCPFLG IS CLEARED                                 */ 01960000
*/*              ADDRESS OF CURRENT CORE BLOCK IN CUR250C OR CUR252C */ 01961000
*/*              ADDRESS OF LAST CORE BLOCK ON CHAIN IN CURCORE      */ 01962000
*/*                                                                  */ 01963000
*/********************************************************************/ 01964000
*                                                                  0704 01965000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0704 01966000
GETCORE  DS    0H                                                  0705 01967000
*   RESPECIFY                                                      0705 01968000
*    (R0,                                                          0705 01969000
*     R1,                                                          0705 01970000
*     RET,                                                         0705 01971000
*     R15) RESTRICTED;                                             0705 01972000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 01973000
         LA    @02,1                                               0706 01974000
         AL    @02,RETCONT                                         0706 01975000
         ST    @02,RETCONT                                         0706 01976000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 01977000
         SLA   @02,2                                               0707 01978000
         ST    RET,RETSAVE-4(@02)                                  0707 01979000
*   VCRCLENG=(VCRCLENG+SEVEN)&-DWORDLEN;/* LENGTH TO DOUBLEWORD BDY  */ 01980000
         LA    @02,7                                               0708 01981000
         AL    @02,VCRCLENG                                        0708 01982000
         N     @02,@CF04232                                        0708 01983000
         ST    @02,VCRCLENG                                        0708 01984000
*   IF VCRCPOOL=USERPOOL THEN       /* IF REQUEST FOR SUBPOOL 250    */ 01985000
         CLI   VCRCPOOL,250                                        0709 01986000
         BNE   @RF00709                                            0709 01987000
*     R1=CUR250C;                   /* POINT TO CURRENT 250 BLOCK    */ 01988000
         L     R1,CUR250C                                          0710 01989000
*   ELSE                            /* ELSE, MUST BE 252 REQUEST     */ 01990000
*     R1=CUR252C;                   /* POINT TO CURRENT 252 BLOCK    */ 01991000
*                                                                  0711 01992000
         B     @RC00709                                            0711 01993000
@RF00709 L     R1,CUR252C                                          0711 01994000
*   /*****************************************************************/ 01995000
*   /*                                                               */ 01996000
*   /* DETERMINE IF ENOUGH CORE IS AVAILABLE TO PROCESS THE CALLERS  */ 01997000
*   /* REQUEST. IF NOT, GET AND INITIALIZE A NEW CORE BLOCK.         */ 01998000
*   /*                                                               */ 01999000
*   /*****************************************************************/ 02000000
*                                                                  0712 02001000
*   IF R1=ZERO|VCRCLENA<VCRCLENG THEN/* IF CORE IS NOT AVAILABLE     */ 02002000
@RC00709 LTR   R1,R1                                               0712 02003000
         BZ    @RT00712                                            0712 02004000
         L     @02,VCRCLENA(,R1)                                   0712 02005000
         C     @02,VCRCLENG                                        0712 02006000
         BNL   @RF00712                                            0712 02007000
@RT00712 DS    0H                                                  0713 02008000
*     DO;                           /* GET A NEW CORE BLOCK          */ 02009000
*       IF VCRCLENG>PAGE-LENGTH(VCRCHDR) THEN/* IF LNG GT PAGE SIZE  */ 02010000
         L     @02,VCRCLENG                                        0714 02011000
         C     @02,@CF04233                                        0714 02012000
         BNH   @RF00714                                            0714 02013000
*         CURLEN=VCRCLENG+LENGTH(VCRCHDR);/* GET CORE FOR LNG+HEADER */ 02014000
         AL    @02,@CF00118                                        0715 02015000
         ST    @02,CURLEN                                          0715 02016000
*       ELSE                        /* IF LENGTH LESS THAN PAGE SIZE */ 02017000
*         CURLEN=PAGE;              /* GET CORE FOR PAGE SIZE        */ 02018000
         B     @RC00714                                            0716 02019000
@RF00714 MVC   CURLEN(4),@CF04010                                  0716 02020000
*       R0=CURLEN;                  /* LOAD GETMN REG ZERO W/ LENGTH */ 02021000
@RC00714 L     R0,CURLEN                                           0717 02022000
*       RET=VCRCPOOL;               /* LOAD RETURN REG WITH SUBPOOL  */ 02023000
         SLR   RET,RET                                             0718 02024000
         IC    RET,VCRCPOOL                                        0718 02025000
*       GEN(GETMAIN RC,LV=(0),SP=(RET));/* ISSUE THE GETMAIN         */ 02026000
         GETMAIN RC,LV=(0),SP=(RET)                                     02027000
*       IF R15^=ZERO THEN           /* IF THERE WAS A GETMAIN ERROR  */ 02028000
         LTR   R15,R15                                             0720 02029000
         BZ    @RF00720                                            0720 02030000
*         DO;                                                      0721 02031000
*           CKRETCD2=CHKERR08;      /* SET THE RETURN CODE           */ 02032000
         L     @02,CHKWAPTR                                        0722 02033000
         MVI   CKRETCD2(@02),X'08'                                 0722 02034000
*           CKMSGCD2=CKMSG003;      /* SET THE MESSAGE CODE          */ 02035000
         MVI   CKMSGCD2(@02),X'03'                                 0723 02036000
*           RETURN TO ERREXIT;      /* EXIT CHECKPOINT               */ 02037000
         LA    @14,ERREXIT                                         0724 02038000
@EL00012 DS    0H                                                  0724 02039000
@EF00012 DS    0H                                                  0724 02040000
@ER00012 BR    @14                                                 0724 02041000
*         END;                                                     0725 02042000
*       IF CURCORE=ZERO THEN        /* IF THIS IS FIRST CORE BLOCK   */ 02043000
@RF00720 L     @02,CURCORE                                         0726 02044000
         LTR   @02,@02                                             0726 02045000
         BNZ   @RF00726                                            0726 02046000
*         VCRCHEAD=R1;              /* POINT TO IT - HEAD OF CHAIN   */ 02047000
         ST    R1,VCRCHEAD                                         0727 02048000
*       ELSE                        /* IF NOT FIRST,                 */ 02049000
*         CURCORE->VCRCNEXT=R1;     /* CHAIN TO THE PREVIOUS BLOCK   */ 02050000
         B     @RC00726                                            0728 02051000
@RF00726 L     @02,CURCORE                                         0728 02052000
         ST    R1,VCRCNEXT(,@02)                                   0728 02053000
*       CURCORE=R1;                 /* MAINTAIN PTR TO CURRENT BLOCK */ 02054000
@RC00726 ST    R1,CURCORE                                          0729 02055000
*       R0=R1;                      /* LOAD R0 AS RECEIVER FOR MVCL  */ 02056000
         LR    R0,R1                                               0730 02057000
*       R1=CURLEN;                  /* LOAD R1 WITH LENGTH OF MOVE   */ 02058000
         L     R1,CURLEN                                           0731 02059000
*       MVCL(R0,RET);               /* R15 IS ZERO, SO PAD WITH ZERO */ 02060000
         MVCL  R0,RET                                              0732 02061000
*       RESPECIFY                                                  0733 02062000
*        (R0,                                                      0733 02063000
*         RET,                                                     0733 02064000
*         R15) UNRESTRICTED;                                       0733 02065000
*                                                                  0733 02066000
*       /*************************************************************/ 02067000
*       /*                                                           */ 02068000
*       /* INITIALIZE THE VCRCORE HEADER                             */ 02069000
*       /*                                                           */ 02070000
*       /*************************************************************/ 02071000
*                                                                  0734 02072000
*       R1=CURCORE;                 /* VCRCORE ADDRESSIBILITY        */ 02073000
         L     R1,CURCORE                                          0734 02074000
*       VCRCNM=VCRCNAME;            /* SET VCRCORE NAME IN HEADER    */ 02075000
         MVC   VCRCNM(8,R1),@CC03178                               0735 02076000
*       VCRCSP=VCRCPOOL;            /* SET VCRCORE SUBPOOL IN HEADER */ 02077000
         IC    @02,VCRCPOOL                                        0736 02078000
         STC   @02,VCRCSP(,R1)                                     0736 02079000
*       VCRCLEN=CURLEN;             /* SET VCRCORE LENGTH IN HEADER  */ 02080000
         L     @02,CURLEN                                          0737 02081000
         STCM  @02,7,VCRCLEN(R1)                                   0737 02082000
*       VCRCPTRA=ADDR(VCRCDATA);    /* SET PTR TO 1ST AVAILABLE BYTE */ 02083000
         LA    @00,VCRCDATA(,R1)                                   0738 02084000
         ST    @00,VCRCPTRA(,R1)                                   0738 02085000
*       VCRCLENA=CURLEN-LENGTH(VCRCHDR);/* SET AVAILABLE LENGTH      */ 02086000
         SL    @02,@CF00118                                        0739 02087000
         ST    @02,VCRCLENA(,R1)                                   0739 02088000
*       IF VCRCPOOL=USERPOOL THEN   /* IF THE SUBPOOL IS 250         */ 02089000
         CLI   VCRCPOOL,250                                        0740 02090000
         BNE   @RF00740                                            0740 02091000
*         CUR250C=ADDR(VCRCORE);    /* UPDATE THE PTR IN CUR250C     */ 02092000
         ST    R1,CUR250C                                          0741 02093000
*       ELSE                        /* IF IT IS SUBPOOL 252          */ 02094000
*         CUR252C=ADDR(VCRCORE);    /* UPDATE THE PTR IN CUR252C     */ 02095000
         B     @RC00740                                            0742 02096000
@RF00740 ST    R1,CUR252C                                          0742 02097000
*     END;                                                         0743 02098000
*                                                                  0743 02099000
@RC00740 DS    0H                                                  0744 02100000
*   /*****************************************************************/ 02101000
*   /*                                                               */ 02102000
*   /* CORE IS AVAILABLE, SO PROCESS CALLERS REQUEST                 */ 02103000
*   /*                                                               */ 02104000
*   /*****************************************************************/ 02105000
*                                                                  0744 02106000
*   IF VCRCPFLP=ON THEN             /* IF REQUEST IS FOR MAX LENGTH  */ 02107000
@RF00712 TM    VCRCPFLP,B'10000000'                                0744 02108000
         BNO   @RF00744                                            0744 02109000
*     DO;                                                          0745 02110000
*       VCRCADDR=VCRCPTRA;          /* POINT TO FIRST AVAILABLE BYTE */ 02111000
         L     @02,VCRCPTRA(,R1)                                   0746 02112000
         ST    @02,VCRCADDR                                        0746 02113000
*       VCRCPTRA=VCRCPTRA+VCRCLENA; /* UPDATE NEXT BYTE POINTER      */ 02114000
         L     @15,VCRCLENA(,R1)                                   0747 02115000
         ALR   @02,@15                                             0747 02116000
         ST    @02,VCRCPTRA(,R1)                                   0747 02117000
*       VCRCLENG=VCRCLENA;          /* PASS BACK THE LENGTH AND      */ 02118000
         ST    @15,VCRCLENG                                        0748 02119000
*       VCRCLENA=ZERO;              /* ZERO THE AVAILABLE LENGTH FLD */ 02120000
         SLR   @02,@02                                             0749 02121000
         ST    @02,VCRCLENA(,R1)                                   0749 02122000
*     END;                                                         0750 02123000
*   ELSE                            /* IF EXACT LENGTH REQUESTED     */ 02124000
*     DO;                                                          0751 02125000
         B     @RC00744                                            0751 02126000
@RF00744 DS    0H                                                  0752 02127000
*       VCRCADDR=VCRCPTRA;          /* GET THE ADDRESS OF NEXT BYTE  */ 02128000
         L     @02,VCRCPTRA(,R1)                                   0752 02129000
         ST    @02,VCRCADDR                                        0752 02130000
*       VCRCPTRA=VCRCPTRA+VCRCLENG; /* UPDATE POINTER TO NEXT BYTE   */ 02131000
         L     @15,VCRCLENG                                        0753 02132000
         ALR   @02,@15                                             0753 02133000
         ST    @02,VCRCPTRA(,R1)                                   0753 02134000
*       VCRCLENA=VCRCLENA-VCRCLENG; /* DECREASE AVAILABLE LENGTH     */ 02135000
         LCR   @15,@15                                             0754 02136000
         AL    @15,VCRCLENA(,R1)                                   0754 02137000
         ST    @15,VCRCLENA(,R1)                                   0754 02138000
*     END;                                                         0755 02139000
*   VCRCPFLG=ZERO;                  /* CLEAR GETCORE FLAG BYTE       */ 02140000
@RC00744 MVI   VCRCPFLG,X'00'                                      0756 02141000
*   RESPECIFY                                                      0757 02142000
*     RET RESTRICTED;                                              0757 02143000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 02144000
         L     @02,RETCONT                                         0758 02145000
         LR    @15,@02                                             0758 02146000
         SLA   @15,2                                               0758 02147000
         L     RET,RETSAVE-4(@15)                                  0758 02148000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 02149000
         BCTR  @02,0                                               0759 02150000
         ST    @02,RETCONT                                         0759 02151000
*   RESPECIFY                                                      0760 02152000
*    (R1,                                                          0760 02153000
*     RET) UNRESTRICTED;                                           0760 02154000
*   END GETCORE;                                                   0761 02155000
         B     @EL00012                                            0761 02156000
*SAVEBUF:                                                          0762 02157000
*                                                                  0762 02158000
*/********************************************************************/ 02159000
*/*                                                                  */ 02160000
*/*   FUNCTION - OBTAINS CORE FOR A BUFFER SAVE AREA AFTER           */ 02161000
*/*              DETERMINING THE LARGEST BUFFER SIZE REQUIRED.       */ 02162000
*/*   INPUT    - POINTER TO THE FIRST PRIMARY AMBL IN PAMBLPTR       */ 02163000
*/*   OUTPUT   - ADDRESS OF CORE IN SAVEBUFP                         */ 02164000
*/*                                                                  */ 02165000
*/********************************************************************/ 02166000
*                                                                  0762 02167000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0762 02168000
SAVEBUF  DS    0H                                                  0763 02169000
*   RESPECIFY                                                      0763 02170000
*     RET RESTRICTED;                                              0763 02171000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 02172000
         LA    @15,1                                               0764 02173000
         AL    @15,RETCONT                                         0764 02174000
         ST    @15,RETCONT                                         0764 02175000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 02176000
         SLA   @15,2                                               0765 02177000
         ST    RET,RETSAVE-4(@15)                                  0765 02178000
*   RESPECIFY                                                      0766 02179000
*     RET UNRESTRICTED;                                            0766 02180000
*   AMBLPTR=PAMBLPTR;               /* ADDRESSIBILITY TO THE AMBL    */ 02181000
         L     @02,PAMBLPTR                                        0767 02182000
         ST    @02,AMBLPTR                                         0767 02183000
*   VCRCLENG=ZERO;                  /* INITIALIZE THE LENGTH FIELD   */ 02184000
         SLR   @02,@02                                             0768 02185000
         ST    @02,VCRCLENG                                        0768 02186000
*   DO UNTIL AMBLPTR=ZERO;          /* LOOP THROUGH ALL PRIMARIES    */ 02187000
@DL00769 DS    0H                                                  0770 02188000
*     AMBPTR=AMBLDTA;               /* POINT TO THE DATA AMB         */ 02189000
         L     @02,AMBLPTR                                         0770 02190000
         L     @15,AMBLDTA(,@02)                                   0770 02191000
         ST    @15,AMBPTR                                          0770 02192000
*     IF BIBUPT=ZERO THEN           /* IF UPGRADE PATH IS NOT OPEN   */ 02193000
         L     @01,AMBLBIB(,@02)                                   0771 02194000
         L     @14,BIBUPT(,@01)                                    0771 02195000
         LTR   @14,@14                                             0771 02196000
         BNZ   @RF00771                                            0771 02197000
*       IF AMBCREAT=ON THEN         /* AND THIS IS CREATE MODE       */ 02198000
         TM    AMBCREAT(@15),B'10000000'                           0772 02199000
         BNO   @RF00772                                            0772 02200000
*         IF AMDDST=ON THEN         /* FOR A KSDS THEN               */ 02201000
         L     @01,AMBDSB(,@15)                                    0773 02202000
         TM    AMDDST(@01),B'10000000'                             0773 02203000
         BNO   @RF00773                                            0773 02204000
*           AMBPTR=AMBLIX;          /* POINT TO THE INDEX AMB        */ 02205000
         L     @02,AMBLIX(,@02)                                    0774 02206000
         ST    @02,AMBPTR                                          0774 02207000
*         ELSE                      /* IF NOT A KSDS                 */ 02208000
*           AMBPTR=ZERO;            /* ZERO THE AMB POINTER          */ 02209000
         B     @RC00773                                            0775 02210000
@RF00773 SLR   @02,@02                                             0775 02211000
         ST    @02,AMBPTR                                          0775 02212000
*       ELSE                        /* IF NOT CREATE MODE            */ 02213000
*         IF AMDDST=ON THEN         /* AND THIS IS IS A KSDS         */ 02214000
         B     @RC00772                                            0776 02215000
@RF00772 L     @02,AMBPTR                                          0776 02216000
         L     @02,AMBDSB(,@02)                                    0776 02217000
         TM    AMDDST(@02),B'10000000'                             0776 02218000
         BNO   @RF00776                                            0776 02219000
*           AMBPTR=ZERO;            /* THEN ZERO THE AMB POINTER     */ 02220000
         SLR   @02,@02                                             0777 02221000
         ST    @02,AMBPTR                                          0777 02222000
*         ELSE                                                     0778 02223000
*           ;                       /* NOT A KSDS                    */ 02224000
@RF00776 DS    0H                                                  0779 02225000
*     ELSE                          /* IF AN UPGRADE IS OPEN         */ 02226000
*       AMBPTR=ZERO;                /* THEN ZERO THE AMB POINTER     */ 02227000
         B     @RC00771                                            0779 02228000
@RF00771 SLR   @02,@02                                             0779 02229000
         ST    @02,AMBPTR                                          0779 02230000
*     IF AMBPTR^=ZERO THEN          /* IF THE AMBPTR IS NOT ZERO     */ 02231000
@RC00771 L     @02,AMBPTR                                          0780 02232000
         LTR   @02,@02                                             0780 02233000
         BZ    @RF00780                                            0780 02234000
*       VCRCLENG=MAX(VCRCLENG,AMDCINV);/* THEN SET THE LENGTH        */ 02235000
         L     @02,AMBDSB(,@02)                                    0781 02236000
         L     @15,VCRCLENG                                        0781 02237000
         L     @02,AMDCINV(,@02)                                   0781 02238000
         CR    @15,@02                                             0781 02239000
         BNL   *+6                                                      02240000
         LR    @15,@02                                             0781 02241000
         ST    @15,VCRCLENG                                        0781 02242000
*     AMBLPTR=AMBLPCHN;             /* POINT TO NEXT PRIMARY AMBL    */ 02243000
@RF00780 L     @02,AMBLPTR                                         0782 02244000
         L     @02,AMBLPCHN(,@02)                                  0782 02245000
         ST    @02,AMBLPTR                                         0782 02246000
*   END;                            /* END OF PRIMARY AMBL LOOP      */ 02247000
@DE00769 SLR   @02,@02                                             0783 02248000
         C     @02,AMBLPTR                                         0783 02249000
         BNE   @DL00769                                            0783 02250000
*   IF VCRCLENG^=ZERO THEN          /* IF A SAVE AREA IS NEEDED      */ 02251000
         C     @02,VCRCLENG                                        0784 02252000
         BE    @RF00784                                            0784 02253000
*     DO;                           /* THEN                          */ 02254000
*       VCRCPFEL=ON;                /* EXACT LENGTH REQUEST          */ 02255000
         OI    VCRCPFEL,B'01000000'                                0786 02256000
*       VCRCPOOL=WKPOOL;            /* WORK SUBPOOL - 252            */ 02257000
         MVI   VCRCPOOL,X'FC'                                      0787 02258000
*       CALL GETCORE;               /* GET THE SAVE AREA             */ 02259000
         BAL   @14,GETCORE                                         0788 02260000
*       SAVEBUFP=VCRCADDR;          /* SAVE THE POINTER TO IT        */ 02261000
         L     @02,VCRCADDR                                        0789 02262000
         ST    @02,SAVEBUFP                                        0789 02263000
*     END;                                                         0790 02264000
*   RESPECIFY                                                      0791 02265000
*     RET RESTRICTED;                                              0791 02266000
@RF00784 DS    0H                                                  0792 02267000
*   RET=RETSAVE(RETCONT);           /* GET RETURN ADDRESS            */ 02268000
         L     @02,RETCONT                                         0792 02269000
         LR    @15,@02                                             0792 02270000
         SLA   @15,2                                               0792 02271000
         L     RET,RETSAVE-4(@15)                                  0792 02272000
*   RETCONT=RETCONT-ONE;            /* DECREMENT CONTROL BY ONE      */ 02273000
         BCTR  @02,0                                               0793 02274000
         ST    @02,RETCONT                                         0793 02275000
*   RESPECIFY                                                      0794 02276000
*     RET UNRESTRICTED;                                            0794 02277000
*   END SAVEBUF;                                                   0795 02278000
@EL00013 DS    0H                                                  0795 02279000
@EF00013 DS    0H                                                  0795 02280000
@ER00013 BR    @14                                                 0795 02281000
*IOSAVE:                                                           0796 02282000
*                                                                  0796 02283000
*/********************************************************************/ 02284000
*/*                                                                  */ 02285000
*/*   FUNCTION - SAVES THE STATUS OF THE USERS CONTROL BLOCKS BEFORE */ 02286000
*/*              ANY I/O IS DONE AND INITIALIZES FOR CI PROCESSING.  */ 02287000
*/*   INPUT    - POINTER TO THE AMBL IN AMBLPTR                      */ 02288000
*/*              POINTER TO THE AMB IN AMBPTR                        */ 02289000
*/*   OUTPUT   - CONTROL BLOCKS ARE SAVED                            */ 02290000
*/*                                                                  */ 02291000
*/********************************************************************/ 02292000
*                                                                  0796 02293000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0796 02294000
IOSAVE   DS    0H                                                  0797 02295000
*   RESPECIFY                                                      0797 02296000
*    (R0,                                                          0797 02297000
*     R1,                                                          0797 02298000
*     RET,                                                         0797 02299000
*     R15) RESTRICTED;                                             0797 02300000
*   RETCONT=RETCONT+ONE;            /* BUMP CONTROL BY ONE           */ 02301000
         LA    @02,1                                               0798 02302000
         AL    @02,RETCONT                                         0798 02303000
         ST    @02,RETCONT                                         0798 02304000
*   RETSAVE(RETCONT)=RET;           /* SAVE RETURN ADDRESS           */ 02305000
         SLA   @02,2                                               0799 02306000
         ST    RET,RETSAVE-4(@02)                                  0799 02307000
*   IF ADDR(SAVEBUFR)=ZERO THEN     /* IF NO BUFFER SAVE AREA        */ 02308000
         L     @02,SAVEBUFP                                        0800 02309000
         LTR   @02,@02                                             0800 02310000
         BNZ   @RF00800                                            0800 02311000
*     DO;                           /* THEN                          */ 02312000
*       CKMSGCD2=CKMSG042;          /* REPOSITIONING ERROR           */ 02313000
         L     @02,CHKWAPTR                                        0802 02314000
         MVI   CKMSGCD2(@02),X'2A'                                 0802 02315000
*       CKRETCD2=CHKERR08;          /* ENVIRONMENTAL ERROR           */ 02316000
         MVI   CKRETCD2(@02),X'08'                                 0803 02317000
*       RETURN TO ERREXIT;          /* EXIT                          */ 02318000
         LA    @14,ERREXIT                                         0804 02319000
@EL00014 DS    0H                                                  0804 02320000
@EF00014 DS    0H                                                  0804 02321000
@ER00014 BR    @14                                                 0804 02322000
*     END;                                                         0805 02323000
*                                                                  0805 02324000
*   /*****************************************************************/ 02325000
*   /*                                                               */ 02326000
*   /* SAVE STATUS OF USERS CONTROL BLOCKS BEFORE USING              */ 02327000
*   /*                                                               */ 02328000
*   /*****************************************************************/ 02329000
*                                                                  0806 02330000
