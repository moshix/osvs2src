*   IF BLDMASK(C2)=TWO THEN         /* IF CORE TO BE FREED AT RESTRT */ 01855000
         SLR   @06,@06                                             0674 01856000
         IC    @06,BLDMASK-1(C2)                                   0674 01857000
         C     @06,@CF00092                                        0674 01858000
         BNE   @RF00674                                            0674 01859000
*     VCRHFREL=ON;                  /* SET RELEASE BIT ON            */ 01860000
         OI    VCRHFREL(HEBSAPTR),B'01000000'                      0675 01861000
*   VCRHCID=C2;                     /* SET THE RELATIVE ENTRY NUMBER */ 01862000
@RF00674 STC   C2,VCRHCID(,HEBSAPTR)                               0676 01863000
*   DO UNTIL HEBPTR=ZERO;           /* SAVE ALL THE HEADER ELEMENTS  */ 01864000
@DL00677 DS    0H                                                  0678 01865000
*     IF VCRCLENG<LENGTH(HEBHDREL) THEN/* IF NOT ENOUGH CORE         */ 01866000
         L     @06,VCRCLENG                                        0678 01867000
         C     @06,@CF00118                                        0678 01868000
         BNL   @RF00678                                            0678 01869000
*       DO;                         /* THEN                          */ 01870000
