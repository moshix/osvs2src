*                             CPAWCPE=CPAWCPE+EIGHT;               0433 00924000
         ALR   @01,@15                                             0433 00925000
         ST    @01,CPAWCPE(,RCPA)                                  0433 00926000
*                             CCWCC=WCKD;/* WRITE DATA FIELD         */ 00927000
         MVI   CCWCC(@01),B'00011101'                              0434 00928000
*                             IF CPABLKSZ=F4K THEN                 0435 00929000
         CLC   CPABLKSZ(4,RCPA),@CF00096                           0435 00930000
         BNE   @RF00435                                            0435 00931000
*                               CCWDA=CPAIDAL+(R2-ONE)*TWELVE;/*   0436 00932000
*                                                            @Z30SSLC*/ 00933000
