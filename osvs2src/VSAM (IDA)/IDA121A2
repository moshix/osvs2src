*                             CCWFLG=CZERO;/* CLEAR FLAGS            */ 00949000
@RC00435 L     @01,CPAWCPE(,RCPA)                                  0438 00950000
         MVI   CCWFLG(@01),B'00000000'                             0438 00951000
*                             CCWCCB=ON;/* COMMAND CHAIN             */ 00952000
