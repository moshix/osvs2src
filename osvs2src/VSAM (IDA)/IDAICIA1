*     END;                                                         0545 01311000
*   IF RDEB^=ZERO THEN              /* IF DEB THEN FREE IT           */ 01312000
@RF00530 LTR   RDEB,RDEB                                           0546 01313000
         BZ    @RF00546                                            0546 01314000
*     DO;                                                          0547 01315000
*       DO;                         /* MODESET EXTKEY(DATAMGT)       */ 01316000
*         SPKA(80);                 /* SET PSW KEY                   */ 01317000
         LA    @15,80                                              0549 01318000
         SPKA  0(@15)                                              0549 01319000
*       END;                        /* MODESET EXTKEY(DATAMGT) KEY OF   01320000
*                                      DEB                           */ 01321000
