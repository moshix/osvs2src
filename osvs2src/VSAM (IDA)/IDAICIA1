*       DCBWKPT5=ONE;               /* CLEAR DCB WORK AREA PTR       */ 01227000
         ST    @10,DCBWKPT5(,@05)                                  0519 01228000
*       DCBWKPT6=ONE;               /* CLEAR DCB WORK AREA PTR       */ 01229000
*                                                                  0520 01230000
         ST    @10,DCBWKPT6(,@05)                                  0520 01231000
*       /*************************************************************/ 01232000
*       /*                                                           */ 01233000
*       /* SET DCB OPEN FLAGS                                        */ 01234000
*       /*                                                           */ 01235000
*       /*************************************************************/ 01236000
*                                                                  0521 01237000
*       DCBOFLGS=ZERO;              /* CLEAR OFLGS                   */ 01238000
         MVI   DCBOFLGS(@15),X'00'                                 0521 01239000
*       DCBOFOPN=OFF;               /* OPEN BIT OFF - 0              */ 01240000
*       DCBOFIOF=OFF;               /* BUSY BIT OFF - 0              */ 01241000
*       DCBOFUEX=ON;                /* LOCK BIT OFF - 1              */ 01242000
         OI    DCBOFUEX(@15),B'00000010'                           0524 01243000
         NI    DCBOFOPN(@15),B'11101110'                           0524 01244000
*     END;                          /* END CLOSE RESET               */ 01245000
*   END DCBRESET;                                                  0526 01246000
