*         IHADCB BASED(R3);         /* RESUME USER DCB DESCRIPTION   */ 01199000
*       DCBMACR=RWORK1->DCBMACRF;   /* MACRF FROM COPY DCB           */ 01200000
         ALR   @15,R3                                              0508 01201000
         MVC   DCBMACR(2,@15),DCBMACRF(RWORK1)                     0508 01202000
*       DCBIFLG=RWORK1->DCBIFLGS;   /* IFLGS FROM COPY DCB           */ 01203000
         MVC   DCBIFLG(1,@15),DCBIFLGS(RWORK1)                     0509 01204000
*       RESPECIFY                                                  0510 01205000
*         RTIOT RESTRICTED;                                        0510 01206000
*       RTIOT=DXTIOTAD;             /* LOCATE TIOT DD ENTRY          */ 01207000
         L     RTIOT,DXTIOTAD(,RCORE)                              0511 01208000
*       DCBDDNAM=TIOEDDNM;          /* RESTORE DDNAME IN DCB         */ 01209000
         MVC   DCBDDNAM(8,@15),TIOEDDNM(RTIOT)                     0512 01210000
*       RESPECIFY                                                  0513 01211000
*         RTIOT UNRESTRICTED;                                      0513 01212000
*       DCBDEVT=ZERO;               /* CLEAR DEVICE TYPE             */ 01213000
         MVI   DCBDEVT+16(R3),X'00'                                0514 01214000
*       DCBNCRHI=ZERO;              /* CLEAR DCBNCRHI                */ 01215000
         SLR   @10,@10                                             0515 01216000
         LA    @05,52                                              0515 01217000
         ALR   @05,R3                                              0515 01218000
         STH   @10,DCBNCRHI(,@05)                                  0515 01219000
*       DCBRORG2=ZERO;              /* CLEAR DCB REORGANIZATION      */ 01220000
         STH   @10,DCBRORG2(,@05)                                  0516 01221000
*       DCBSVCXA=ONE;               /* CLEAR SVC EXIT LIST PTR       */ 01222000
         LA    @10,1                                               0517 01223000
         STCM  @10,7,DCBSVCXA+28(R3)                               0517 01224000
*       DCBWKPT1=ONE;               /* CLEAR DCB WORK AREA PTR       */ 01225000
