         TITLE 'IDAICIA1 - MVM ISAM INTERFACE FORCE CLOSE EXECUTOR     *00001000
                        '                                               00002000
IDAICIA1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDAICIA1  78.102'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*   IIRSAVE=R13;                    /* SAVE AREA ADDR SAVED          */ 00014000
         ST    R13,IIRSAVE(,RCORE)                                 0170 00015000
*   RESPECIFY                                                      0171 00016000
*     R13 UNRESTRICTED;                                            0171 00017000
*   R3=DXUDCBAD;                    /* GET USER DCB ADDR             */ 00018000
         L     R3,DXUDCBAD(,RCORE)                                 0172 00019000
*   DO;                             /* MODESET EXTKEY(DATAMGT)       */ 00020000
*     SPKA(80);                     /* SET PSW KEY                   */ 00021000
         LA    @15,80                                              0174 00022000
         SPKA  0(@15)                                              0174 00023000
*   END;                            /* MODESET EXTKEY(DATAMGT) GET 0175 00024000
*                                      INTO DATAMGT KEY              */ 00025000
*   CALL AUDITVER;                  /* DETERMINES IF AUDIT VALID     */ 00026000
         BAL   @14,AUDITVER                                        0176 00027000
*   DO;                             /* MODESET EXTKEY(ZERO)          */ 00028000
*     SPKA(0);                      /* SET PSW KEY                   */ 00029000
         SLR   @15,@15                                             0178 00030000
         SPKA  0(@15)                                              0178 00031000
*   END;                            /* MODESET EXTKEY(ZERO) KEY ZERO    00032000
*                                      FOR DUMP/RECORDING            */ 00033000
*                                                                  0179 00034000
*   /*****************************************************************/ 00035000
*   /*                                                               */ 00036000
*   /* IF OPEN MODULE IDA0192I WAS IN CONTROL THEN PERFORM FORCE     */ 00037000
*   /* CLOSE ON PARTIALLY OPENED DCB. THIS IS DETERMINED BY TESTING A*/ 00038000
*   /* BIT SETTING IN FIELD DXATEXC1 IN THE OPEN/CLOSE WORK AREA AND */ 00039000
*   /* IS SET BY MODULE IDA0192A PRIOR TO GIVING CONTROL TO EITHER   */ 00040000
*   /* IDA0192I OR IDA0200S DURING THE OPEN OR CLOSE PROCESS         */ 00041000
*   /* RESPECTIVELY.                                                 */ 00042000
*   /*                                                               */ 00043000
*   /*****************************************************************/ 00044000
*                                                                  0180 00045000
*   IF IDARIIOP=ON THEN                                            0180 00046000
         TM    IDARIIOP(RCORE),B'00100000'                         0180 00047000
         BNO   @RF00180                                            0180 00048000
*     CALL DRECOPEN;                /* DUMP AND RECORD AREAS FOR OPEN   00049000
*                                      PROCESSING                    */ 00050000
*                                                                  0181 00051000
         BAL   @14,DRECOPEN                                        0181 00052000
*   /*****************************************************************/ 00053000
*   /*                                                               */ 00054000
*   /* OTHERWISE ERROR OCCURRED DURING THE NORMAL CLOSE OF THE DCB   */ 00055000
*   /* WHICH IS THEREFORE IN A STATE OF PARTIAL CLOSED STATUS.       */ 00056000
*   /* IDA0200S WAS IN CONTROL AT TIME THE ERROR WAS DETECTED.       */ 00057000
*   /*                                                               */ 00058000
*   /*****************************************************************/ 00059000
*                                                                  0182 00060000
*   ELSE                                                           0182 00061000
*     CALL DRECLOSE;                /* DUMP AND RECORD AREAS FOR   0182 00062000
*                                      CLOSE PROCESSING              */ 00063000
         B     @RC00180                                            0182 00064000
@RF00180 BAL   @14,DRECLOSE                                        0182 00065000
*   DO;                             /* MODESET KEYADDR(DXUKEY)       */ 00066000
@RC00180 DS    0H                                                  0184 00067000
*     SPKA(DXUKEY);                 /* SET PSW KEY                   */ 00068000
         SLR   @15,@15                                             0184 00069000
         IC    @15,DXUKEY(,RCORE)                                  0184 00070000
         SPKA  0(@15)                                              0184 00071000
*   END;                            /* MODESET KEYADDR(DXUKEY) RESUME   00072000
*                                      USER KEY                      */ 00073000
*   IF RIICB=ZERO|RWORK1=ZERO THEN  /* IF AUDIT NOT VALID THEN       */ 00074000
         SLR   @15,@15                                             0186 00075000
         CR    RIICB,@15                                           0186 00076000
         BE    @RT00186                                            0186 00077000
         CR    RWORK1,@15                                          0186 00078000
         BE    @RT00186                                            0186 00079000
*     GO TO EXIT;                   /* OR CONTINUATION NOT DESIRED 0187 00080000
*                                      BYPASS FORCE CLOSE            */ 00081000
*   CALL DELETRTN;                  /* DELETE ROUTINES LOADED        */ 00082000
         BAL   @14,DELETRTN                                        0188 00083000
*   CALL CLOSEACB;                  /* CLOSE ACB IF STILL OPEN       */ 00084000
         BAL   @14,CLOSEACB                                        0189 00085000
*   CALL DEBUNCHN;                  /* REMOVE DEB FROM DEB CHAIN     */ 00086000
         BAL   @14,DEBUNCHN                                        0190 00087000
*   CALL DCBRESET;                  /* RESET DCB TO PRE-OPEN STATUS  */ 00088000
         BAL   @14,DCBRESET                                        0191 00089000
*   CALL FREERSRC;                  /* FREE CORE OBTAINED            */ 00090000
         BAL   @14,FREERSRC                                        0192 00091000
*EXIT:                                                             0193 00092000
*   DO;                             /* MODESET EXTKEY(ZERO)          */ 00093000
EXIT     DS    0H                                                  0194 00094000
*     SPKA(0);                      /* SET PSW KEY                   */ 00095000
         SLR   @15,@15                                             0194 00096000
         SPKA  0(@15)                                              0194 00097000
*   END;                            /* MODESET EXTKEY(ZERO) ASSUME 0195 00098000
*                                      SYSTEM KEY ZERO               */ 00099000
*   PCOPYDCB=ISAMDCB;               /* SET DCB COPY TO USER DCB      */ 00100000
         L     @15,DXPDCBAD(,RCORE)                                0196 00101000
         MVC   PCOPYDCB(236,@15),ISAMDCB+16(R3)                    0196 00102000
*   RESPECIFY                                                      0197 00103000
*     R13 RESTRICTED;                                              0197 00104000
*   R13=IIRSAVE;                    /* RETRIEVE SAVE AREA ADDR       */ 00105000
         L     R13,IIRSAVE(,RCORE)                                 0198 00106000
*   RETURN;                         /* RETURN TO COMMON ESTAE        */ 00107000
@EL00001 DS    0H                                                  0199 00108000
@EF00001 DS    0H                                                  0199 00109000
@ER00001 LM    @14,@12,12(@13)                                     0199 00110000
         BR    @14                                                 0199 00111000
*                                                                  0200 00112000
*   /*****************************************************************/ 00113000
*   /*                                                               */ 00114000
*   /* THE PURPOSE OF THIS ROUTINE IS TO DETERMINE IF AUDITING HAD   */ 00115000
*   /* BEEN INITIATED OR TERMINATED.                                 */ 00116000
*   /*                                                               */ 00117000
*   /*****************************************************************/ 00118000
*                                                                  0200 00119000
*AUDITVER:                                                         0200 00120000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0200 00121000
*                                                                  0200 00122000
AUDITVER DS    0H                                                  0201 00123000
*   /*****************************************************************/ 00124000
*   /*                                                               */ 00125000
*   /* IF CLOSE MODULE IDA0200S WAS IN CONTROL THEN IT MUST BE       */ 00126000
*   /* DETERMINED WHETHER OR NOT THE AUDIT POINTER HAS BEEN          */ 00127000
*   /* INITIALIZED OR IF THE AUDIT HAS ALREADY ENDED. IF OPEN MODULE */ 00128000
*   /* IDA0192I WAS IN CONTROL THEN AUDIT HAS NOT STARTED IF AUDIT   */ 00129000
*   /* POINTER IS ZERO AND THE AUDIT IS THEREFORE NOT VALID ONLY IF  */ 00130000
*   /* ZERO.                                                         */ 00131000
*   /*                                                               */ 00132000
*   /*****************************************************************/ 00133000
*                                                                  0201 00134000
*   IF IDARIICL=ON THEN             /* IF CLOSE IN CONTROL THEN DO   */ 00135000
         TM    IDARIICL(RCORE),B'01000000'                         0201 00136000
         BNO   @RF00201                                            0201 00137000
*     DO;                                                          0202 00138000
*       IF DXATEXC2=AUDEND THEN     /* END OF AUDIT IS INDICATED BY     00139000
*                                      X'FFFFFFFF' IN AUDIT PTR      */ 00140000
         CLC   DXATEXC2(4,RCORE),@CB02545                          0203 00141000
         BNE   @RF00203                                            0203 00142000
*         IIAUDPTR=ZERO;            /* MARK AUDIT NOT VALID          */ 00143000
         SLR   @15,@15                                             0204 00144000
         ST    @15,IIAUDPTR(,RCORE)                                0204 00145000
*       ELSE                        /* AUDIT PTR MAY NOT HAVE BEEN 0205 00146000
*                                      ESTABLISHED AT TIME OF ERROR  */ 00147000
*         IF IIAUDPTR=ZERO THEN     /* IF ZERO AUDIT STARTED BUT     */ 00148000
         B     @RC00203                                            0205 00149000
@RF00203 SLR   @15,@15                                             0205 00150000
         C     @15,IIAUDPTR(,RCORE)                                0205 00151000
         BNE   @RF00205                                            0205 00152000
*           DO;                     /* POINTER NOT ESTABLISHED TO  0206 00153000
*                                      IICB                          */ 00154000
*             IF RDEB^=ZERO THEN    /* IF DEB IS AVAILABLE THEN      */ 00155000
         CR    RDEB,@15                                            0207 00156000
         BE    @RF00207                                            0207 00157000
*               IIAUDPTR=DEBWKPT5;  /* GET IICB ADDR FROM DEB        */ 00158000
         L     @15,DEBEXPT(,RDEB)                                  0208 00159000
         L     @15,DEBWKPT5(,@15)                                  0208 00160000
         ST    @15,IIAUDPTR(,RCORE)                                0208 00161000
*             ELSE                  /* OTHERWISE IICB NOT AVAILABLE  */ 00162000
*               IIAUDPTR=ZERO;      /* MARK AUDIT INVALID            */ 00163000
         B     @RC00207                                            0209 00164000
@RF00207 SLR   @15,@15                                             0209 00165000
         ST    @15,IIAUDPTR(,RCORE)                                0209 00166000
*           END;                                                   0210 00167000
@RC00207 DS    0H                                                  0211 00168000
*     END;                          /* END CLOSE AUDIT VALIDATION    */ 00169000
@RF00205 DS    0H                                                  0211 00170000
@RC00203 DS    0H                                                  0212 00171000
*   RIICB=IIAUDPTR;                 /* GET ADDRESS OF IICB WHICH   0212 00172000
*                                      CONTAINS THE AUDIT INFORMATION   00173000
*                                      AND IF ZERO THEN AUDIT IS   0212 00174000
*                                      INVALID                       */ 00175000
@RF00201 L     RIICB,IIAUDPTR(,RCORE)                              0212 00176000
*   END AUDITVER;                                                  0213 00177000
@EL00002 DS    0H                                                  0213 00178000
@EF00002 DS    0H                                                  0213 00179000
@ER00002 BR    @14                                                 0213 00180000
*                                                                  0214 00181000
*   /*****************************************************************/ 00182000
*   /*                                                               */ 00183000
*   /* THE PURPOSE OF THIS ROUTINE IS TO DETERMINE HOW MUCH DUMPING  */ 00184000
*   /* AND RECORDING SHOULD BE DONE WHEN AN ERROR HAS OCCURRED DURING*/ 00185000
*   /* OPEN PROCESSING.                                              */ 00186000
*   /*                                                               */ 00187000
*   /*****************************************************************/ 00188000
*                                                                  0214 00189000
*DRECOPEN:                                                         0214 00190000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0214 00191000
DRECOPEN DS    0H                                                  0215 00192000
*   RESPECIFY                                                      0215 00193000
*    (R5,                                                          0215 00194000
*     RLKRETSV) RESTRICTED;                                        0215 00195000
*   RWORK1=FORCECLS;                /* ASSUME FORCE CLOSE TO CONTINUE*/ 00196000
         LA    @15,1                                               0216 00197000
         LR    RWORK1,@15                                          0216 00198000
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR              */ 00199000
*                                                                  0217 00200000
         LR    RLKRETSV,R14                                        0217 00201000
*   /*****************************************************************/ 00202000
*   /*                                                               */ 00203000
*   /* MACHINE CHECK***RECORD=IGNORE,SDUMP=NO,DUMP=IGNORE,CONTINUE   */ 00204000
*   /*                                                               */ 00205000
*   /*****************************************************************/ 00206000
*                                                                  0218 00207000
*   IF SDWAMCHK=ON THEN             /* IF MACHINE CHECK THEN NO      */ 00208000
         TM    SDWAMCHK(R7),B'10000000'                            0218 00209000
         BO    @RT00218                                            0218 00210000
*     RETURN;                       /* RECORDING                     */ 00211000
*                                                                  0219 00212000
*   /*****************************************************************/ 00213000
*   /*                                                               */ 00214000
*   /* AUDIT NOT STARTED***RECORD=YES,SDUMP=YES,DUMP=IGNORE, DO NOT  */ 00215000
*   /* CONTINUE WITH FORCE CLOSE                                     */ 00216000
*   /*                                                               */ 00217000
*   /*****************************************************************/ 00218000
*                                                                  0220 00219000
*   IF RIICB=ZERO THEN              /* IICB NOT OBTAINED             */ 00220000
         LTR   RIICB,RIICB                                         0220 00221000
         BNZ   @RF00220                                            0220 00222000
*     DO;                           /* SET RECOVERY PROCESSING       */ 00223000
*       R5=OPSDUMP1;                /* PARM FOR SDUMP SUBROUTINE     */ 00224000
         LR    R5,@15                                              0222 00225000
*       CALL SDUMPSUB;              /* PERFORM RECORDING             */ 00226000
         BAL   @14,SDUMPSUB                                        0223 00227000
*     END;                                                         0224 00228000
*   ELSE                                                           0225 00229000
*                                                                  0225 00230000
*     /***************************************************************/ 00231000
*     /*                                                             */ 00232000
*     /* USER ERROR***ABEND03B OR ERROR IN DCB EXIT ROUTINE***RECORD=*/ 00233000
*     /* IGNORE,SDUMP=NO,DUMP=YES,DO NOT CONTINUE WITH FORCE CLOSE   */ 00234000
*     /*                                                             */ 00235000
*     /***************************************************************/ 00236000
*                                                                  0225 00237000
*     IF(SDWASVCD=ON&               /* IF ABEND ISSUED AND           */ 00238000
*         SDWACMPC=ABEND3B)|        /* AND ABEND IS 03B, OR          */ 00239000
*         (SDWANRBE=ON&             /* ERROR IN LOWER LEVEL SVRB AND */ 00240000
*         AUDDCBEX=ON&              /* DCB EXIT WAS TAKEN BUT        */ 00241000
*         AUDDCBRT=OFF) THEN        /* DID NOT RETURN BEFORE ERROR   */ 00242000
         B     @RC00220                                            0225 00243000
@RF00220 TM    SDWASVCD(R7),B'00010000'                            0225 00244000
         BNO   @GL00002                                            0225 00245000
         CLC   SDWACMPC(3,R7),@CB02622                             0225 00246000
         BE    @RT00225                                            0225 00247000
@GL00002 TM    SDWANRBE(R7),B'01000000'                            0225 00248000
         BNO   @RF00225                                            0225 00249000
         TM    AUDDCBEX(RIICB),B'00100000'                         0225 00250000
         BNO   @RF00225                                            0225 00251000
         TM    AUDDCBRT(RIICB),B'00010000'                         0225 00252000
         BNZ   @RF00225                                            0225 00253000
@RT00225 DS    0H                                                  0226 00254000
*       CALL USERDUMP;              /* SET UP USER DUMP              */ 00255000
         BAL   @14,USERDUMP                                        0226 00256000
*     ELSE                                                         0227 00257000
*                                                                  0227 00258000
*       /*************************************************************/ 00259000
*       /*                                                           */ 00260000
*       /* VSAM OPEN IN CONTROL***RECORD=IGNORE,SDUMP=NO,DUMP=       */ 00261000
*       /* IGNORE, CONTINUE WITH FORCE CLOSE PROCESSING              */ 00262000
*       /*                                                           */ 00263000
*       /*************************************************************/ 00264000
*                                                                  0227 00265000
*       IF(SDWANRBE=ON&             /* LOWER LEVEL RB IN CONTROL AND */ 00266000
*           AUDACBOP=ON&            /* OPEN ISSUED ON ACB BUT        */ 00267000
*           AUDACBRO=OFF) THEN      /* NO RETURN FROM ACB OPEN       */ 00268000
         B     @RC00225                                            0227 00269000
@RF00225 TM    SDWANRBE(R7),B'01000000'                            0227 00270000
         BNO   @RF00227                                            0227 00271000
         TM    AUDACBOP(RIICB),B'10000000'                         0227 00272000
         BNO   @RF00227                                            0227 00273000
         TM    AUDACBRO(RIICB),B'01000000'                         0227 00274000
         BNZ   @RF00227                                            0227 00275000
*         DO;                       /* CONTINUE FORCE CLOSE          */ 00276000
*           R14=RLKRETSV;           /* RESTORE RETURN ADDR           */ 00277000
         LR    R14,RLKRETSV                                        0229 00278000
*           RETURN;                 /* AND RETURN                    */ 00279000
@EL00003 DS    0H                                                  0230 00280000
@EF00003 DS    0H                                                  0230 00281000
@ER00003 BR    @14                                                 0230 00282000
*         END;                                                     0231 00283000
*       ELSE                                                       0232 00284000
*                                                                  0232 00285000
*         /***********************************************************/ 00286000
*         /*                                                         */ 00287000
*         /* ERROR OCCURED IN IDA0192I***RECORD=YES,DUMP=NO,         */ 00288000
*         /* SDUMP=YES,CONTINUE WITH FORCE CLOSE PROCESS             */ 00289000
*         /*                                                         */ 00290000
*         /***********************************************************/ 00291000
*                                                                  0232 00292000
*         DO;                                                      0232 00293000
@RF00227 DS    0H                                                  0233 00294000
*           R5=OPSDUMP2;            /* PARM FOR SDUMP SUBROUTINE     */ 00295000
         LA    R5,2                                                0233 00296000
*           CALL SDUMPSUB;          /* PERFORM SDUMP SETUP           */ 00297000
         BAL   @14,SDUMPSUB                                        0234 00298000
*         END;                                                     0235 00299000
*   R14=RLKRETSV;                   /* RESTORE RETURN ADDR           */ 00300000
@RC00225 DS    0H                                                  0236 00301000
@RC00220 LR    R14,RLKRETSV                                        0236 00302000
*   RESPECIFY                                                      0237 00303000
*    (R5,                                                          0237 00304000
*     RLKRETSV) UNRESTRICTED;                                      0237 00305000
*   END DRECOPEN;                                                  0238 00306000
         B     @EL00003                                            0238 00307000
*                                                                  0239 00308000
*   /*****************************************************************/ 00309000
*   /*                                                               */ 00310000
*   /* THE PURPOSE OF THIS ROUTINE IS TO DETERMINE HOW MUCH DUMPING  */ 00311000
*   /* AND RECORDING SHOULD BE DONE WHEN AN ERROR HAS OCCURRED DURING*/ 00312000
*   /* CLOSE PROCESSING.                                             */ 00313000
*   /*                                                               */ 00314000
*   /*****************************************************************/ 00315000
*                                                                  0239 00316000
*DRECLOSE:                                                         0239 00317000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0239 00318000
DRECLOSE DS    0H                                                  0240 00319000
*   RESPECIFY                                                      0240 00320000
*    (R5,                                                          0240 00321000
*     RLKRETSV) RESTRICTED;                                        0240 00322000
*   RWORK1=FORCECLS;                /* ASSUME FORCE CLOSE TO CONTINUE*/ 00323000
         LA    RWORK1,1                                            0241 00324000
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR              */ 00325000
*                                                                  0242 00326000
         LR    RLKRETSV,R14                                        0242 00327000
*   /*****************************************************************/ 00328000
*   /*                                                               */ 00329000
*   /* MACHINE CHECK***SDUMP=NO,RECORD=IGNORE,DUMP=IGNORE,CONTINUE   */ 00330000
*   /*                                                               */ 00331000
*   /*****************************************************************/ 00332000
*                                                                  0243 00333000
*   IF SDWAMCHK=ON THEN             /* RETURN IF MACHINE CHECK       */ 00334000
         TM    SDWAMCHK(R7),B'10000000'                            0243 00335000
         BO    @RT00243                                            0243 00336000
*     RETURN;                       /* PERFORM NO RECORDING          */ 00337000
*                                                                  0244 00338000
*   /*****************************************************************/ 00339000
*   /*                                                               */ 00340000
*   /* AUDIT ENDED OR UNAVAILABLE***SDUMP=YES,RECORD=YES,            */ 00341000
*   /* DUMP=IGNORE, DO NOT CONTINUE WITH FORCE CLOSE                 */ 00342000
*   /*                                                               */ 00343000
*   /*****************************************************************/ 00344000
*                                                                  0245 00345000
*   IF RIICB=ZERO THEN              /* IICB UNAVAILABLE              */ 00346000
         LTR   RIICB,RIICB                                         0245 00347000
         BNZ   @RF00245                                            0245 00348000
*     DO;                           /* SET RECOVERY PROCESSING       */ 00349000
*       R5=CLSDUMP1;                /* PARM FOR SDUMP SUBROUTINE     */ 00350000
         LA    R5,3                                                0247 00351000
*       CALL SDUMPSUB;              /* PERFORM SDUMP SETUP           */ 00352000
         BAL   @14,SDUMPSUB                                        0248 00353000
*     END;                                                         0249 00354000
*   ELSE                                                           0250 00355000
*                                                                  0250 00356000
*     /***************************************************************/ 00357000
*     /*                                                             */ 00358000
*     /* USER ERROR***ABEND031***REOCRD=IGNORE,SDUMP=NO,DUMP=YES, DO */ 00359000
*     /* NOT CONTINUE WITH FORCE CLOSE                               */ 00360000
*     /*                                                             */ 00361000
*     /***************************************************************/ 00362000
*                                                                  0250 00363000
*     IF SDWASVCD=ON&               /* SVC 13 ISSUED(ABEND)          */ 00364000
*         SDWACMPC=ABEND031 THEN    /* AND ABEND IS 031 FROM CLOSE   */ 00365000
         B     @RC00245                                            0250 00366000
@RF00245 TM    SDWASVCD(R7),B'00010000'                            0250 00367000
         BNO   @RF00250                                            0250 00368000
         CLC   SDWACMPC(3,R7),@CB02624                             0250 00369000
         BNE   @RF00250                                            0250 00370000
*       CALL USERDUMP;              /* SETUP FOR USER DUMP           */ 00371000
         BAL   @14,USERDUMP                                        0251 00372000
*     ELSE                                                         0252 00373000
*                                                                  0252 00374000
*       /*************************************************************/ 00375000
*       /*                                                           */ 00376000
*       /* VSAM CLOSE IN CONTROL***RECORD=IGNORE, SDUMP=NO,          */ 00377000
*       /* DUMP=IGNORE, CONTINUE WITH FORCE CLOSE PROCESSING         */ 00378000
*       /*                                                           */ 00379000
*       /*************************************************************/ 00380000
*                                                                  0252 00381000
*       IF(SDWANRBE=ON&             /* LOWER LEVEL RB IN CONTROL AND */ 00382000
*           AUDACBCL=ON&            /* A CLOSE ISSUED ON ACB AND     */ 00383000
*           AUDACBRC=OFF) THEN      /* AND CLOSE DID NOT RETURN      */ 00384000
         B     @RC00250                                            0252 00385000
@RF00250 TM    SDWANRBE(R7),B'01000000'                            0252 00386000
         BNO   @RF00252                                            0252 00387000
         TM    AUDACBCL(RIICB),B'01000000'                         0252 00388000
         BNO   @RF00252                                            0252 00389000
         TM    AUDACBRC(RIICB),B'00100000'                         0252 00390000
         BNZ   @RF00252                                            0252 00391000
*         DO;                       /* CONTINUE CLOSE                */ 00392000
*           R14=RLKRETSV;           /* RESTORE RET ADDR              */ 00393000
         LR    R14,RLKRETSV                                        0254 00394000
*           RETURN;                 /* AND RETURN                    */ 00395000
@EL00004 DS    0H                                                  0255 00396000
@EF00004 DS    0H                                                  0255 00397000
@ER00004 BR    @14                                                 0255 00398000
*         END;                                                     0256 00399000
*       ELSE                                                       0257 00400000
*                                                                  0257 00401000
*         /***********************************************************/ 00402000
*         /*                                                         */ 00403000
*         /* MODULE IDAIIPM1 IN CONTROL***SDUMP=YES, DUMP=IGNORE,    */ 00404000
*         /* RECORD=YES, CONTINUE WITH FORCE CLOSE PROCESSING        */ 00405000
*         /*                                                         */ 00406000
*         /***********************************************************/ 00407000
*                                                                  0257 00408000
*         IF(SDWANRBE=ON&           /* LOWER LEVEL RB IN CONTROL     */ 00409000
*             AUDBFREX=ON&          /* AND SYNCH TO PM1 ISSUED       */ 00410000
*             AUDBFRRT=OFF) THEN    /* BUT DID NOT RETURN            */ 00411000
@RF00252 TM    SDWANRBE(R7),B'01000000'                            0257 00412000
         BNO   @RF00257                                            0257 00413000
         TM    AUDBFREX(RIICB),B'00010000'                         0257 00414000
         BNO   @RF00257                                            0257 00415000
         TM    AUDBFRRT(RIICB),B'00001000'                         0257 00416000
         BNZ   @RF00257                                            0257 00417000
*           DO;                     /* SET RECOVERY PROCESSING       */ 00418000
*             R5=CLSDUMP2;          /* PARM FOR SDUMP SUBROUTINE     */ 00419000
         LA    R5,4                                                0259 00420000
*             CALL SDUMPSUB;        /* PERFORM SDUMP SETUP           */ 00421000
         BAL   @14,SDUMPSUB                                        0260 00422000
*           END;                                                   0261 00423000
*         ELSE                                                     0262 00424000
*                                                                  0262 00425000
*           /*********************************************************/ 00426000
*           /*                                                       */ 00427000
*           /* MODULE IDA0200S IN CONTROL***RECORD=YES, DUMP=IGNORE, */ 00428000
