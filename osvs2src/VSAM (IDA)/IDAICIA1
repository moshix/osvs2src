*   /*****************************************************************/ 01251000
*   /*                                                               */ 01252000
*   /* THE PURPOSE OF THIS ROUTINE IS TO FREE UP THE CORE WHICH HAD  */ 01253000
*   /* BEEN OBTAINED PRIOR TO THE TIME OF ERROR.                     */ 01254000
*   /*                                                               */ 01255000
*   /*****************************************************************/ 01256000
*                                                                  0527 01257000
*FREERSRC:                                                         0527 01258000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0527 01259000
FREERSRC DS    0H                                                  0528 01260000
*   RESPECIFY                                                      0528 01261000
*     RLKRETSV RESTRICTED;                                         0528 01262000
*   RLKRETSV=R14;                   /* SAVE RETURN ADDR              */ 01263000
         LR    RLKRETSV,R14                                        0529 01264000
*   IF RDEB^=ZERO&                  /* IF DEB IS PRESENT AND         */ 01265000
*       DEBXTNP^=ZERO&              /* DEB EXTENSION FILLED IN AND   */ 01266000
*       AUDDEBXF=OFF THEN           /* NO ATTEMPT HAS BEEN MADE TO   */ 01267000
         SLR   @15,@15                                             0530 01268000
         CR    RDEB,@15                                            0530 01269000
         BE    @RF00530                                            0530 01270000
         LR    @05,RDEB                                            0530 01271000
         AL    @05,@CF02738                                        0530 01272000
         L     @05,DEBXTNP-28(,@05)                                0530 01273000
         CR    @05,@15                                             0530 01274000
         BE    @RF00530                                            0530 01275000
         TM    AUDDEBXF(RIICB),B'00000100'                         0530 01276000
         BNZ   @RF00530                                            0530 01277000
*     DO;                           /* FREE EXTENSION - THEN FREE IT */ 01278000
*       DO;                         /* MODESET EXTKEY(DATAMGT)       */ 01279000
*         SPKA(80);                 /* SET PSW KEY                   */ 01280000
         LA    @15,80                                              0533 01281000
         SPKA  0(@15)                                              0533 01282000
*       END;                        /* MODESET EXTKEY(DATAMGT) FOR 0534 01283000
*                                      DEBPOOL                       */ 01284000
*       DO;                         /* FREEMAIN(RC)                0535 01285000
*                                      LV(LENGTH(DEBXTN))A(DEBXTN) 0535 01286000
*                                      SP(DEBPOOL)                   */ 01287000
*         RESPECIFY                                                0536 01288000
*          (GPR01F,                                                0536 01289000
*           GPR15F,                                                0536 01290000
*           GPR00F) RESTRICTED;                                    0536 01291000
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */ 01292000
         SLR   GPR01F,GPR01F                                       0537 01293000
*         GPR15F=1;                 /* SET TYPE                      */ 01294000
         LA    GPR15F,1                                            0538 01295000
*         GPR00F=LENGTH(DEBXTN);    /* LENGTH REQUESTED              */ 01296000
         LA    GPR00F,32                                           0539 01297000
*         GPR01F=ADDR(DEBXTN);      /* SET REGISTER ONE              */ 01298000
