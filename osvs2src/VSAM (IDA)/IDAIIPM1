         TITLE 'IDAIIPM1 - MVM ISAM INTERFACE LOAD                     *00001000
                       '                                                00002000
IDAIIPM1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDAIIPM1  73.230'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@11                                         0001 00013000
*   RDCB=R1;                        /* ESTABLISH DCB BASE            */ 00014000
*   RIICB=DEBWKPT5;                 /* PTR TO IICB                   */ 00015000
         L     @12,DCBDEBAD+40(,RDCB)                              0143 00016000
         L     @12,DEBEXPT(,@12)                                   0143 00017000
         L     RIICB,DEBWKPT5(,@12)                                0143 00018000
*   IF IIFCLOSE=ON THEN             /* IF ENTRY FROM CLOSE           */ 00019000
         TM    IIFCLOSE(RIICB),B'00010000'                         0144 00020000
         BNO   @RF00144                                            0144 00021000
*     RWORK3=R14;                   /* SAVE RETURN REG               */ 00022000
         LR    RWORK3,R14                                          0145 00023000
*   ELSE                                                           0146 00024000
*     DO;                                                          0146 00025000
         B     @RC00144                                            0146 00026000
@RF00144 DS    0H                                                  0147 00027000
*       IIREGBC=R13;                /* KEEP PTR TO USER SAVE         */ 00028000
         ST    R13,IIREGBC(,RIICB)                                 0147 00029000
*       R15=ADDR(IIREGSAV);         /* HOLD II SAVE ADDR             */ 00030000
         LA    R15,IIREGSAV(,RIICB)                                0148 00031000
*       GEN(ST   R15,8(0,R13));     /* KEEP IT IN USER SAVE          */ 00032000
         ST   R15,8(0,R13)                                              00033000
*     END;                                                         0150 00034000
*   R13=ADDR(IIREGSAV);             /* POINTER TO II SAVE            */ 00035000
@RC00144 LA    R13,IIREGSAV(,RIICB)                                0151 00036000
*   RRPL=IIRPLPTR;                  /* ESTABLISH RPL BASE            */ 00037000
         L     RRPL,IIRPLPTR(,RIICB)                               0152 00038000
*   DCBEXCD1=CZERO;                 /* TURN OFF ERROR FLAGS          */ 00039000
         LA    @12,52                                              0153 00040000
         AR    @12,RDCB                                            0153 00041000
         MVI   DCBEXCD1(@12),B'00000000'                           0153 00042000
*   DCBEXCD2=CZERO;                 /* TURN OFF ERROR FLAGS          */ 00043000
         MVI   DCBEXCD2(@12),B'00000000'                           0154 00044000
*   RWORK2=R0;                      /* HOLD BFR PTR (PM)             */ 00045000
         LR    RWORK2,R0                                           0155 00046000
*   IF DCBMFLCP=ON THEN             /* IF LOCATE MODE                */ 00047000
         TM    DCBMFLCP+40(RDCB),B'00001000'                       0156 00048000
         BNO   @RF00156                                            0156 00049000
*     RWORK2=RPLIIBFR;              /* INIT BUFFER POINTER           */ 00050000
         L     RWORK2,RPLIIBFR+76(,RRPL)                           0157 00051000
*   RPLAREA=RWORK2;                 /* INIT RPL AREA PTR             */ 00052000
@RF00156 ST    RWORK2,RPLAREA(,RRPL)                               0158 00053000
*   IF DCBRECL='01'B THEN           /* IF VARIABLE LENGTH            */ 00054000
         LA    @12,DCBIOBAD+16(,RDCB)                              0159 00055000
         TM    DCBRECL(@12),B'01000000'                            0159 00056000
         BNO   @RF00159                                            0159 00057000
         TM    DCBRECL(@12),B'10000000'                            0159 00058000
         BNZ   @RF00159                                            0159 00059000
*     DO;                           /* THEN ADD RDW LENGTH (4)       */ 00060000
*       RWORK2=RWORK2+LENGTH(RDWWORD);                             0161 00061000
         AH    RWORK2,@CH00199                                     0161 00062000
*       RPLAREA=RWORK2;             /* ADJUST RPLAREA PAST RDW       */ 00063000
         ST    RWORK2,RPLAREA(,RRPL)                               0162 00064000
*     END;                                                         0163 00065000
*   IF DCBMFLCP=ON&                 /* IF LOCATE MODE AND THIS       */ 00066000
*       DCBSTKSQ=OFF THEN           /* IS THE FIRST PUT              */ 00067000
@RF00159 TM    DCBMFLCP+40(RDCB),B'00001000'                       0164 00068000
         BNO   @RF00164                                            0164 00069000
         LA    @12,52                                              0164 00070000
         AR    @12,RDCB                                            0164 00071000
         TM    DCBSTKSQ(@12),B'01000000'                           0164 00072000
         BNZ   @RF00164                                            0164 00073000
*     DO;                                                          0165 00074000
*       DCBSTKSQ=ON;                /* TURN ON FIRST PASS (KEY       */ 00075000
         OI    DCBSTKSQ(@12),B'01000000'                           0166 00076000
*                                   /* SEQUENCE CHECKING) BIT        */ 00077000
*       IISAVLRL=DCBLRECL;          /* SAVE DCBLRECL FOR COMPARE WITH   00078000
*                                      RDW LENGTH            @Y021130*/ 00079000
         MVC   IISAVLRL(2,RIICB),DCBLRECL(@12)                     0167 00080000
*       GOTO EXIT;                  /* RETURN TO CALLER              */ 00081000
         B     EXIT                                                0168 00082000
*     END;                                                         0169 00083000
*   IF DCBRKP=ZERO THEN             /* IF RKP=0, THEN ADJUST PTR     */ 00084000
@RF00164 ICM   @12,3,DCBRKP+52(RDCB)                               0170 00085000
         BNZ   @RF00170                                            0170 00086000
*     RWORK2=RWORK2+DCBKEYLE;       /* TO FIRST DATA BYTE            */ 00087000
         SR    @12,@12                                             0171 00088000
         IC    @12,DCBKEYLE+16(,RDCB)                              0171 00089000
         AR    RWORK2,@12                                          0171 00090000
*                                   /*                               */ 00091000
*                                   /* TEST FOR DELETE CODE          */ 00092000
*                                   /*                               */ 00093000
*   IF(DCBRECL='10'B&               /* IF FIXED LENGTH RECORDS,      */ 00094000
*       DCBRECBR=OFF&               /* UNBLOCKED,                    */ 00095000
*       DCBRKP=ZERO)|               /* RKP = 0 OR                    */ 00096000
*       (DCBRECL='01'B&             /* VARIABLE LENGTH RECORDS &     */ 00097000
*       DCBRKP=4)THEN               /* RKP = 4 THEN CONTINUE         */ 00098000
@RF00170 LA    @12,DCBIOBAD+16(,RDCB)                              0172 00099000
         TM    DCBRECL(@12),B'10000000'                            0172 00100000
         BNO   @GL00002                                            0172 00101000
         TM    DCBRECL(@12),B'01000000'                            0172 00102000
         BNZ   @GL00002                                            0172 00103000
         TM    DCBRECBR(@12),B'00010000'                           0172 00104000
         BNZ   @GL00002                                            0172 00105000
         ICM   @12,3,DCBRKP+52(RDCB)                               0172 00106000
         BZ    @RT00172                                            0172 00107000
@GL00002 LA    @12,DCBIOBAD+16(,RDCB)                              0172 00108000
         TM    DCBRECL(@12),B'01000000'                            0172 00109000
         BNO   @RF00172                                            0172 00110000
         TM    DCBRECL(@12),B'10000000'                            0172 00111000
         BNZ   @RF00172                                            0172 00112000
         CLC   DCBRKP+52(2,RDCB),@CH00199                          0172 00113000
         BE    @RT00172                                            0172 00114000
*     GO TO NEXT;                   /* BECAUSE OPTCD=L IS INVALID    */ 00115000
*   ELSE                                                           0174 00116000
*     DO;                           /* DELETE OPTION IS VALID        */ 00117000
@RF00172 DS    0H                                                  0175 00118000
*       IF DCBOPTL=ON&              /* IF OPTCD=LI -(DUMMY RCDS      */ 00119000
*           DCBOPTI=ON&             /* ARE NOT WRITTEN)              */ 00120000
*           DATABYT1=DELCODE THEN   /* AND FIRST BYTE IS FF          */ 00121000
         TM    DCBOPTL+52(RDCB),B'00010010'                        0175 00122000
         BNO   @RF00175                                            0175 00123000
         CLI   DATABYT1(RWORK2),B'11111111'                        0175 00124000
         BE    @RT00175                                            0175 00125000
*         GO TO EXIT;               /* RETURN W/O ISSUING PUT        */ 00126000
*     END;                                                         0177 00127000
@RF00175 DS    0H                                                  0178 00128000
*NEXT:                                                             0178 00129000
*   RPLARG=RPLAREA+AMDRKP;          /* INIT RPL PTR TO KEY           */ 00130000
NEXT     L     @12,RPLAREA(,RRPL)                                  0178 00131000
         L     @10,IIACBPTR(,RIICB)                                0178 00132000
         L     @10,ACBAMBL(,@10)                                   0178 00133000
         L     @10,AMBLDTA(,@10)                                   0178 00134000
         L     @10,AMBDSB(,@10)                                    0178 00135000
         LH    @10,AMDRKP(,@10)                                    0178 00136000
         AR    @10,@12                                             0178 00137000
         ST    @10,RPLARG(,RRPL)                                   0178 00138000
*   IF DCBRECL='01'B&               /* IF VARIABLE LEN RECORDS &,    */ 00139000
*       ((DCBMFMVP=ON&RDWLEN>IIMAXLRL)/* PUT MOVE AND RDW LGTH       */ 00140000
*                                   /* > MAX ALLOWED         @Y021130*/ 00141000
*       |(DCBMFLCP=ON&              /* OR, PUT LOCATE AND            */ 00142000
*       (RDWLEN>IISAVLRL|           /* RDWLEN > PREVIOSLY STATED     */ 00143000
*       DCBLRECL>IIMAXLRL)))THEN    /* OR NEXT REC WILL      @Y021130*/ 00144000
         LA    @10,DCBIOBAD+16(,RDCB)                              0179 00145000
         TM    DCBRECL(@10),B'01000000'                            0179 00146000
         BNO   @RF00179                                            0179 00147000
         TM    DCBRECL(@10),B'10000000'                            0179 00148000
         BNZ   @RF00179                                            0179 00149000
         TM    DCBMFMVP+40(RDCB),B'00010000'                       0179 00150000
         BNO   @GL00008                                            0179 00151000
         AL    @12,@CF01209                                        0179 00152000
         CLC   RDWLEN(2,@12),IIMAXLRL(RIICB)                       0179 00153000
         BH    @RT00179                                            0179 00154000
@GL00008 TM    DCBMFLCP+40(RDCB),B'00001000'                       0179 00155000
         BNO   @RF00179                                            0179 00156000
         L     @12,RPLAREA(,RRPL)                                  0179 00157000
         AL    @12,@CF01209                                        0179 00158000
         CLC   RDWLEN(2,@12),IISAVLRL(RIICB)                       0179 00159000
         BH    @RT00179                                            0179 00160000
         CLC   DCBLRECL+52(2,RDCB),IIMAXLRL(RIICB)                 0179 00161000
         BNH   @RF00179                                            0179 00162000
@RT00179 DS    0H                                                  0180 00163000
*                                   /* EXCEED MAX ALLOWED            */ 00164000
*     DO;                           /* THEN                          */ 00165000
*       DCBEXLTH=ON;                /* TURN ON DCB EXCEPTION CODE    */ 00166000
         OI    DCBEXLTH+52(RDCB),B'00001000'                       0181 00167000
*                                   /* BIT FOR LENGTH ERROR          */ 00168000
*                                                                  0182 00169000
*/* **************************************************************** */ 00170000
*/*                                                                  */ 00171000
*/*   THIS SYNAD EXIT WILL BE ENTERED FOR THE FOLLOWING CONDITIONS   */ 00172000
*/*                                                                  */ 00173000
*/*   A.  LENGTH ERROR - IF VARIABLE LENGTH RECORDS AND THE          */ 00174000
*/*                      RDW LENGTH > DCBLRECL                       */ 00175000
*/*                                                                  */ 00176000
*/*   B.  SEQUENCE CHECK - ENTRY DURING RESUME LOAD PROCESSING ONLY  */ 00177000
*/*                                                                  */ 00178000
*/*   C.  INVALID REQUEST - NO AVAILABLE PLACEHOLDERS                */ 00179000
*/*                         (USER MAY BE SHARING THE DCB)            */ 00180000
*/*                                                                  */ 00181000
*/* **************************************************************** */ 00182000
*                                                                  0182 00183000
*SYNADEXT:                                                         0182 00184000
*       IF DCBSYNAD=ONE THEN        /* IF USER SYNAD NOT SPECIFD     */ 00185000
SYNADEXT CLC   DCBSYNAD+52(4,RDCB),@CF00087                        0182 00186000
         BNE   @RF00182                                            0182 00187000
*         DO;                       /* ABEND(ABEND31) DUMP SYSTEM    */ 00188000
*           RESPECIFY                                              0184 00189000
*            (GPR01F)RESTRICTED;                                   0184 00190000
*           GPR01F=((ABEND31)&'00000FFF'X)*4096;/* COMP CODE IN BITS    00191000
*                                      8-19                          */ 00192000
         L     GPR01F,@CF01200                                     0185 00193000
*           GPR01F=GPR01F|'80000000'X;/* DUMP/STEP/DUMPOPTS FLAG     */ 00194000
         O     GPR01F,@CF01196                                     0186 00195000
*           SVC(13);                /* ISSUE ABEND SVC               */ 00196000
         SVC   13                                                  0187 00197000
*           RESPECIFY                                              0188 00198000
*            (GPR01F)UNRESTRICTED;                                 0188 00199000
*         END;                      /* ABEND(ABEND31) DUMP SYSTEM    */ 00200000
*                                   /* X'031' ABEND CODE     @Y021130*/ 00201000
*       ELSE                                                       0190 00202000
*         DO;                                                      0190 00203000
         B     @RC00182                                            0190 00204000
@RF00182 DS    0H                                                  0191 00205000
*           R15=DCBSYNAD;           /* INIT R15 - SYNAD ADDR         */ 00206000
         LA    @12,52                                              0191 00207000
         AR    @12,RDCB                                            0191 00208000
         L     R15,DCBSYNAD(,@12)                                  0191 00209000
*           R13=IIREGBC;            /* INIT R13                      */ 00210000
         L     R13,IIREGBC(,RIICB)                                 0192 00211000
*           IF DCBEXSEQ=OFF THEN    /* ERROR OTHER THAN SEQ CHECK    */ 00212000
         TM    DCBEXSEQ(@12),B'10000000'                           0193 00213000
         BNZ   @RF00193                                            0193 00214000
*             DO;                                                  0194 00215000
*               GEN(L R0,20(R13));  /* RELOAD REGISTER ZERO          */ 00216000
         L R0,20(R13)                                                   00217000
*             END;                  /* IF AN ISAM SYNAD ROUTINE      */ 00218000
*                                   /* WAS SPECIFIED IN AMP PARM     */ 00219000
*           IF IISAMSYN(1:4)^=ZERO THEN                            0197 00220000
@RF00193 ICM   @12,15,IISAMSYN(RIICB)                              0197 00221000
         BZ    @RF00197                                            0197 00222000
*             R1=IIDCBPTR;          /* THEN SET REG 1 = DCB          */ 00223000
         L     R1,IIDCBPTR(,RIICB)                                 0198 00224000
*           ELSE                                                   0199 00225000
*             R1=ZERO;              /* OTHERWISE REG 1 = ZERO        */ 00226000
         B     @RC00197                                            0199 00227000
@RF00197 SR    R1,R1                                               0199 00228000
*           GEN;                                                   0200 00229000
@RC00197 DS    0H                                                  0200 00230000
               LM R2,R12,28(R13)           LOAD USER REGSISTERS         00231000
               L  R14,12(R13)              RESTORE USER RETURN ADDR     00232000
               BR R15                                                   00233000
*         END;                                                     0201 00234000
*     END;                                                         0202 00235000
*   ELSE                            /* SET RPL RECORD LENGTH         */ 00236000
*     DO;                                                          0203 00237000
         B     @RC00179                                            0203 00238000
@RF00179 DS    0H                                                  0204 00239000
*       IISAVLRL=DCBLRECL;          /* SAVE CURRENT REC LGTH       0204 00240000
*                                      SPECIFIED             @Y021130*/ 00241000
         MVC   IISAVLRL(2,RIICB),DCBLRECL+52(RDCB)                 0204 00242000
*       IF DCBRECL='01'B THEN       /* IF VARIABLE LENGTH RECORDS    */ 00243000
         LA    @12,DCBIOBAD+16(,RDCB)                              0205 00244000
         TM    DCBRECL(@12),B'01000000'                            0205 00245000
         BNO   @RF00205                                            0205 00246000
         TM    DCBRECL(@12),B'10000000'                            0205 00247000
         BNZ   @RF00205                                            0205 00248000
*         RPLRLEN=RDWLEN-LENGTH(RDWWORD);                          0206 00249000
         LA    @12,4                                               0206 00250000
         L     @10,RPLAREA(,RRPL)                                  0206 00251000
         SR    @10,@12                                             0206 00252000
         LCR   @12,@12                                             0206 00253000
         AH    @12,RDWLEN(,@10)                                    0206 00254000
         ST    @12,RPLRLEN(,RRPL)                                  0206 00255000
*     END;                                                         0207 00256000
@RF00205 DS    0H                                                  0208 00257000
*                                                                  0208 00258000
*/* **************************************************************** */ 00259000
*/*                                                                  */ 00260000
*/*                 RESUME LOAD PROCESSING                           */ 00261000
*/*                                                                  */ 00262000
*/*   KEY SEQUENCE CHECKING IS PERFORMED BY THIS ROUTINE UNTIL A     */ 00263000
*/*   KEY IS RECEIVED THAT IS HIGHER THAN THE HIGHEST KEY IN THE     */ 00264000
*/*   DATA SET.  THIS IS DONE BY ISSUING A VSAM GET FOR A KEY        */ 00265000
*/*   THAT IS GREATER THAN OR EQUAL TO THE KEY GIVEN IN THE USER'S   */ 00266000
*/*   PUT REQUEST.                                                   */ 00267000
*/*                                                                  */ 00268000
*/* **************************************************************** */ 00269000
*                                                                  0208 00270000
*   IF DCBSTLOD=ON THEN             /* IF RESUME LOAD SEQUENCE       */ 00271000
@RC00179 TM    DCBSTLOD+52(RDCB),B'00100000'                       0208 00272000
         BNO   @RF00208                                            0208 00273000
*     DO;                           /* CHECKING IS NECESSARY         */ 00274000
*       RPLSAVE=RPLAREA;            /* SAVE PTR TO RECORD (PUT)      */ 00275000
         LA    @12,76                                              0210 00276000
         AR    @12,RRPL                                            0210 00277000
         MVC   RPLSAVE(4,@12),RPLAREA(RRPL)                        0210 00278000
*       RPLAREA=ADDR(RPLSAVE2);     /* PT TO 4 BYTE FIELD FOR LOC    */ 00279000
         LA    @12,RPLSAVE2(,@12)                                  0211 00280000
         ST    @12,RPLAREA(,RRPL)                                  0211 00281000
*       RPLDIR=ON;                  /* TURN ON DIRECT BIT IN RPL     */ 00282000
*       RPLSEQ=OFF;                 /* TURN OFF SEQ BIT IN RPL       */ 00283000
*       RPLLOC=ON;                  /* TURN ON LOCATE MODE BIT       */ 00284000
*       RPLKGE=ON;                  /* SET SRCH KEY GRTR/EQUAL BT    */ 00285000
         OI    RPLDIR(RRPL),B'11000100'                            0215 00286000
         NI    RPLSEQ(RRPL),B'11011111'                            0215 00287000
*       GEN(GET RPL=(RRPL));        /* ISSUE VSAM GET LOCATE         */ 00288000
         GET RPL=(RRPL)                                                 00289000
*       R1=IIDCBPTR;                /* RESTORE DCB PTR               */ 00290000
         L     R1,IIDCBPTR(,RIICB)                                 0217 00291000
*       IF DCBEXNKY=OFF THEN        /* IF RECORD WAS FOUND THEN      */ 00292000
         LA    @12,52                                              0218 00293000
         AR    @12,RDCB                                            0218 00294000
         TM    DCBEXNKY(@12),B'10000000'                           0218 00295000
         BNZ   @RF00218                                            0218 00296000
*         DO;                       /* KEY FROM PUT IS LOWER         */ 00297000
*                                   /* THAN OR EQUAL TO THE KEY      */ 00298000
*                                   /* FROM THE GET - SET THE        */ 00299000
*           R0=ADDR(RLGETKEY);      /* PTR TO HIGHER KEY IN REG 0    */ 00300000
         L     @10,IIACBPTR(,RIICB)                                0220 00301000
         L     @10,ACBAMBL(,@10)                                   0220 00302000
         L     @10,AMBLDTA(,@10)                                   0220 00303000
         L     @10,AMBDSB(,@10)                                    0220 00304000
         LH    R0,AMDRKP(,@10)                                     0220 00305000
         A     R0,RPLSAVE2+76(,RRPL)                               0220 00306000
*           DCBEXSEQ=ON;            /* SEQ CHK ERROR CODE IN DCB     */ 00307000
         OI    DCBEXSEQ(@12),B'10000000'                           0221 00308000
*           RPLERREG=CZERO;         /* CLEAR RPL ERROR CODE          */ 00309000
         MVI   RPLERREG(RRPL),X'00'                                0222 00310000
*           GOTO SYNADEXT;          /* GO TO ISAM SYNAD RTN          */ 00311000
         B     SYNADEXT                                            0223 00312000
*         END;                                                     0224 00313000
*       ELSE                                                       0225 00314000
*         ;                         /* ISSUE PUT - NO RECD FOUND     */ 00315000
@RF00218 DS    0H                                                  0226 00316000
*       DCBEXCD1=CZERO;             /* CLEAR DCB EXCEPTION CODE      */ 00317000
         LA    @12,52                                              0226 00318000
         AR    @12,RDCB                                            0226 00319000
         MVI   DCBEXCD1(@12),B'00000000'                           0226 00320000
*       DCBEXCD2=CZERO;             /* CLEAR DCB EXCEPTION CODE      */ 00321000
         MVI   DCBEXCD2(@12),B'00000000'                           0227 00322000
*       RPLERREG=CZERO;             /* CLEAR RPL ERROR CODE          */ 00323000
         MVI   RPLERREG(RRPL),X'00'                                0228 00324000
*                                   /*                               */ 00325000
*                                   /* PREPARE TO ISSUE PUT AFTER    */ 00326000
*                                   /* SUCCESSFUL RESUME LOAD        */ 00327000
*                                   /* SEQUENCE CHECK                */ 00328000
*                                   /*                               */ 00329000
*       DCBSTLOD=OFF;               /* RESET STATUS BIT FOR          */ 00330000
         NI    DCBSTLOD(@12),B'11011111'                           0229 00331000
*                                   /* RESUMPTION OF LOADING         */ 00332000
*       RPLDIR=OFF;                 /* TURN OFF DIRECT BIT IN RPL    */ 00333000
*       RPLSEQ=ON;                  /* TURN ON SEQ BIT IN RPL        */ 00334000
*       RPLLOC=OFF;                 /* SET RPL TO MOVE MODE          */ 00335000
*       RPLKGE=OFF;                 /* SET RPL TO SRCH EQUAL         */ 00336000
         OI    RPLSEQ(RRPL),B'00100000'                            0233 00337000
         NI    RPLDIR(RRPL),B'00111011'                            0233 00338000
*       RPLAREA=RPLSAVE;            /* RESTORE RPL AREA PTR          */ 00339000
         MVC   RPLAREA(4,RRPL),RPLSAVE+76(RRPL)                    0234 00340000
*     END;                                                         0235 00341000
*   ELSE                                                           0236 00342000
*     ;                             /* INITIAL LOAD                  */ 00343000
@RF00208 DS    0H                                                  0237 00344000
*                                                                  0237 00345000
*/* **************************************************************** */ 00346000
*/*                                                                  */ 00347000
*/*   THIS CODE WILL BE ENTERED FOR INITIAL LOAD OR A VALID RESUME   */ 00348000
*/*   LOAD CONDITION (THE KEY TO BE ADDED IS HIGHER THAN THE HIGHEST */ 00349000
*/*   EXISTING KEY)                                                  */ 00350000
*/*                                                                  */ 00351000
*/* **************************************************************** */ 00352000
*                                                                  0237 00353000
*   GEN(PUT  RPL=(RRPL));           /* ISSUE VSAM PUT                */ 00354000
         PUT  RPL=(RRPL)                                                00355000
*   R1=IIDCBPTR;                    /* RESTORE DCB POINTER           */ 00356000
         L     R1,IIDCBPTR(,RIICB)                                 0238 00357000
*   IF RPLERRCD=RPLNOPLH THEN       /* IF NO PLH AVAILABLE THEN      */ 00358000
         CLI   RPLERRCD(RRPL),64                                   0239 00359000
         BNE   @RF00239                                            0239 00360000
*     DO;                           /* (USER COULD BE SHARING)       */ 00361000
*       DCBEXINV=ON;                /* SET INVALID REQUEST           */ 00362000
         OI    DCBEXINV+52(RDCB),B'00010000'                       0241 00363000
*       RPLERREG=CZERO;             /* CLEAR RPL ERROR CODES         */ 00364000
         MVI   RPLERREG(RRPL),X'00'                                0242 00365000
*       GO TO SYNADEXT;             /* GO TO SYNAD EXIT              */ 00366000
         B     SYNADEXT                                            0243 00367000
*     END;                                                         0244 00368000
*   DCBSTKSQ=ON;                    /* TURN ON KEY SEQ CHK BIT       */ 00369000
@RF00239 OI    DCBSTKSQ+52(RDCB),B'01000000'                       0245 00370000
*   IF DCBMFLCP=ON THEN             /* IF PUT LOCATE                 */ 00371000
         TM    DCBMFLCP+40(RDCB),B'00001000'                       0246 00372000
         BNO   @RF00246                                            0246 00373000
*                                   /* SAVE KEY IN WORK AREA         */ 00374000
*     KEYAREA(1:DCBKEYLE)=PUTLKEY(1:DCBKEYLE);                     0247 00375000
         L     @12,IIKEYPT(,RIICB)                                 0247 00376000
         SR    @10,@10                                             0247 00377000
