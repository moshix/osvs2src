*           RPLUPD=ON;              /* INDICATE UPDATE               */ 00838000
         OI    RPLUPD(RRPL),B'00000010'                            0370 00839000
*           DECEXDUP=OFF;           /* CLEAR ERROR FLAG              */ 00840000
         NI    DECEXDUP(RDECB),B'11111110'                         0371 00841000
*           R1=ADDR(IFGRPL);        /* POINT TO RPL                  */ 00842000
         LR    R1,RRPL                                             0372 00843000
*           RFY                                                    0373 00844000
*            (R2,                                                  0373 00845000
*             R3,                                                  0373 00846000
*             R4,                                                  0373 00847000
*             R5,                                                  0373 00848000
*             R6,                                                  0373 00849000
*             R7,                                                  0373 00850000
*             R8,                                                  0373 00851000
*             R9,                                                  0373 00852000
*             R10,                                                 0373 00853000
*             R12) RSTD;            /* GUARD REGS                    */ 00854000
*           GEN(LM    R2,R10,TWENTY8(R13));/* RESTORE USER REGS      */ 00855000
         LM    R2,R10,TWENTY8(R13)                                      00856000
*           RFY                                                    0375 00857000
*             IFGRPL BASED(R1);     /* SET NEW RPL BASE              */ 00858000
*           R12=R12SAVE;            /* SET USER REG 12 ISSUE VSAM GET*/ 00859000
