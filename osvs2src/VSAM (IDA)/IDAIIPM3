*         R7,                                                      0428 01055000
*         R8,                                                      0428 01056000
*         R9,                                                      0428 01057000
*         R10,                                                     0428 01058000
*         R12) UNRSTD;              /* DROP REGISTERS                */ 01059000
*       RRPL=R1;                    /* RESET RPL BASE                */ 01060000
         LR    RRPL,R1                                             0429 01061000
*       RFY                                                        0430 01062000
*         IFGRPL BASED(RRPL);       /* REDEFINE RPL BASE             */ 01063000
*       RDECB=RPLDECB;              /* SET DECB BASE                 */ 01064000
         LA    @12,76                                              0431 01065000
         ALR   @12,RRPL                                            0431 01066000
         L     RDECB,RPLDECB(,@12)                                 0431 01067000
*       R01SAVE=RDECB;              /* RESTORE REG 1         @XM01688   01068000
*                                      R11 AND R14 MUST BE RESTORED     01069000
*                                      PRIOR TO FREE- ING THE RPL  0432 01070000
*                                                            @ZA01397*/ 01071000
