         TITLE 'IDAIIPM3--MVM ISAM INTERFACE BISAM                     *00001000
                        '                                               00002000
IDAIIPM3 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
ENTWRITE DS    0H                                                  0001 00008000
         ENTRY ENTWRITE                                                 00009000
ENTREAD  DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00209-ENTREAD)                                    00013000
         ENTRY ENTREAD                                                  00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @11,0                                               0001 00017000
@PSTART  DS    0H                                                  0001 00018000
         USING @PSTART,@11                                         0001 00019000
         AH    @15,4(,@15)                                         0001 00020000
         BR    @15                                                 0001 00021000
@EP00001 DS    0H                                                  0002 00022000
*                                                                  0186 00023000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/ 00024000
*/*                                                                  */ 00025000
*/*   FUNCTION = CHECK ROUTINE                                       */ 00026000
*/*      THIS ROUTINE TAKES THE SYNAD EXIT IF AN ERROR IS INDICATED  */ 00027000
*/*      IN THE DECB.  IF NO ISAM SYNAD ROUTINE IS SPECIFIED, THEN   */ 00028000
*/*      ISSUE A SYSTEM 001 ABEND.  IF NO ERROR IS INDICATED, THIS   */ 00029000
*/*      ROUTINE RETURNS TO THE ISAM PROGRAM.                        */ 00030000
*/*                                                                  */ 00031000
*/*   INPUT =                                                        */ 00032000
*/*      REGISTER 1 = DECB POINTER                                   */ 00033000
*/*                                                                  */ 00034000
*/*   OUTPUT =                                                       */ 00035000
*/*      USER REGISTERS                                              */ 00036000
*/*                                                                  */ 00037000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/ 00038000
*                                                                  0186 00039000
*   IF DECEXRNF=ON|                 /* IF ANY ERROR OCCURRED,        */ 00040000
*       DECEXRLC=ON|DECEXNSP=ON|DECEXINV=ON|DECEXERR=ON|DECEXUBK=ON|    00041000
*       DECEXDUP=ON THEN                                           0186 00042000
         TM    DECEXRNF(RDECB),B'11111101'                         0186 00043000
         BZ    @RF00186                                            0186 00044000
*     DO;                           /* PREPARE FOR SYNAD ENTRY       */ 00045000
*       RFY                                                        0188 00046000
*         R0 RSTD;                  /* GET PARM REG                  */ 00047000
*       R0=RDECB;                   /* SET SYNADAF INPUT             */ 00048000
         LR    R0,RDECB                                            0189 00049000
*       IF DCBSYNAD=ONE THEN        /* IF NO SYNAD IS HERE,          */ 00050000
         L     @15,DECBDCBA(,RDECB)                                0190 00051000
         CLC   DCBSYNAD+52(4,@15),@CF00087                         0190 00052000
         BNE   @RF00190                                            0190 00053000
*         DO;                       /* ABEND(ABENDCD) DUMP SYSTEM    */ 00054000
*           RESPECIFY                                              0192 00055000
*            (GPR01F) RESTRICTED;                                  0192 00056000
*           GPR01F=((ABENDCD)&'00000FFF'X)*4096;/* COMP CODE IN BITS    00057000
*                                      8-19                          */ 00058000
         L     GPR01F,@CF01264                                     0193 00059000
*           GPR01F=GPR01F|'80000000'X;/* DUMP/STEP/DUMPOPTS FLAG     */ 00060000
         O     GPR01F,@CF01265                                     0194 00061000
*           SVC(13);                /* ISSUE ABEND SVC               */ 00062000
         SVC   13                                                  0195 00063000
*           RESPECIFY                                              0196 00064000
*            (GPR01F) UNRESTRICTED;                                0196 00065000
*         END;                      /* ABEND(ABENDCD) DUMP SYSTEM  0197 00066000
*                                      X'001' ABEND          @Y021130*/ 00067000
*       RFY                                                        0198 00068000
*        (R14,                                                     0198 00069000
*         R15) RSTD;                /* GET LINKAGE REGISTERS         */ 00070000
@RF00190 DS    0H                                                  0199 00071000
*       R14=R14SAVE;                /* SET RETURN ADDRESS            */ 00072000
         L     R14,R14SAVE(,R13)                                   0199 00073000
*       R15=DCBSYNAD;               /* POINT TO SYNAD ROUTINE        */ 00074000
         L     @12,DECBDCBA(,RDECB)                                0200 00075000
         L     R15,DCBSYNAD+52(,@12)                               0200 00076000
*       IF IISAMSYN(ONE)^=CZERO THEN/* IF LOADED SYNAD               */ 00077000
         L     @10,DCBDEBAD+40(,@12)                               0201 00078000
         L     @10,DEBEXPT(,@10)                                   0201 00079000
         L     @10,DEBWKPT5(,@10)                                  0201 00080000
         CLI   IISAMSYN(@10),B'00000000'                           0201 00081000
         BE    @RF00201                                            0201 00082000
*         R1=ADDR(IHADCB);          /* PASS DCB ADDR IN R1           */ 00083000
         LR    R1,@12                                              0202 00084000
*       GEN(LM    R2,R12,TWENTY8(R13));/* RESTORE USER REGS          */ 00085000
@RF00201 DS    0H                                                  0203 00086000
         LM    R2,R12,TWENTY8(R13)                                      00087000
*       GOTO SYNAD;                 /* TAKE SYNAD EXIT               */ 00088000
         BR    R15                                                 0204 00089000
*       RFY                                                        0205 00090000
*         R0 UNRSTD;                /* RELEASE PARM REG              */ 00091000
*       RFY                                                        0206 00092000
*        (R14,                                                     0206 00093000
*         R15) UNRSTD;              /* DROP REGISTERS                */ 00094000
*     END;                          /* END OF ERROR CODE             */ 00095000
*   RETURN;                         /* EXIT FROM CHECK               */ 00096000
@EL00001 DS    0H                                                  0208 00097000
@EF00001 DS    0H                                                  0208 00098000
@ER00001 LM    @14,@12,12(@13)                                     0208 00099000
         BR    @14                                                 0208 00100000
*                                                                  0209 00101000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/ 00102000
*/*                                                                  */ 00103000
*/*   FUNCTION = READ/WRITE ROUTINE                                  */ 00104000
*/*      THIS ROUTINE TRANSLATES A BISAM READ K, READ KU, WRITE K,   */ 00105000
*/*      WRITE KN, OR WRITE KN (REPLACE) INTO A VSAM GET, PUT, OR    */ 00106000
*/*      ERASE.                                                      */ 00107000
*/*                                                                  */ 00108000
*/*   INPUT =                                                        */ 00109000
*/*      REGISTER 1 = DECB POINTER                                   */ 00110000
*/*                   THE DECB CONTAINS THE REQUEST CODE, RECORD     */ 00111000
*/*                   LENGTH (WRITE KEY REPLACE), DCB ADDRESS,       */ 00112000
*/*                   AREA ADDRESS, KEY ADDRESS, AND THE READ FOR    */ 00113000
*/*                   UPDATE FLAG.                                   */ 00114000
*/*                                                                  */ 00115000
*/*   OUTPUT =                                                       */ 00116000
*/*      A VSAM REQUEST IS ISSUED AND A CODE IS SET IN THE DECB.     */ 00117000
*/*                                                                  */ 00118000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/ 00119000
*                                                                  0209 00120000
*ENTREAD:                           /* READ ENTRY                    */ 00121000
*ENTWRITE:                          /* WRITE KN ENTRY                */ 00122000
*   ENTRY;                                                         0209 00123000
@EP00209 DS    0H                                                  0210 00124000
*   GEN(LA    RDECB,@00(RDECB));    /* CLEAR HIGH ORDER BYTE         */ 00125000
         LA    RDECB,@00(RDECB)                                         00126000
*   DECBECB=ZERO;                   /* CLEAR ECB                     */ 00127000
         SLR   @12,@12                                             0211 00128000
         ST    @12,DECBECB(,RDECB)                                 0211 00129000
*   DECBEXC1=CZERO;                 /* CLEAR EXCEPTION FLAGS         */ 00130000
*                                                                  0212 00131000
         MVI   DECBEXC1(RDECB),B'00000000'                         0212 00132000
*   /*****************************************************************/ 00133000
*   /*                                                               */ 00134000
*   /* IF THE PREVIOUS REQUEST ISSUED AGAINST THIS DECB WAS A READ   */ 00135000
*   /* FOR UPDATE, LOOK FOR THE RPL PREVIOUSLY ASSIGNED (AUTOMATIC   */ 00136000
*   /* FREEDBUF IS IMPLIED). OTHERWISE, ASSIGN A NEW RPL.            */ 00137000
*   /*                                                               */ 00138000
*   /*****************************************************************/ 00139000
*                                                                  0213 00140000
*   IF DECEXRKU=ON THEN             /* IF READ FOR UPDATE THEN       */ 00141000
         TM    DECEXRKU(RDECB),B'00000001'                         0213 00142000
         BNO   @RF00213                                            0213 00143000
*     DO;                           /* GO                            */ 00144000
*       CALL FINDORPL;              /* LOOK FOR OLD RPL.             */ 00145000
         BAL   @14,FINDORPL                                        0215 00146000
*       IF RRPL=ZERO&               /* IF NO RPL WAS FOUND,          */ 00147000
*           DECBTYP2^=WRITEK THEN   /* IGNORE UPDATE FLAG (IF        */ 00148000
         LTR   RRPL,RRPL                                           0216 00149000
         BNZ   @RF00216                                            0216 00150000
         CLI   DECBTYP2(RDECB),B'00001000'                         0216 00151000
         BE    @RF00216                                            0216 00152000
*         CALL FINDRPL;             /* NOT WRITE K) AND GET A        */ 00153000
         BAL   @14,FINDRPL                                         0217 00154000
*     END;                          /* NEW RPL INSTEAD.              */ 00155000
*   ELSE                                                           0219 00156000
*     CALL FINDRPL;                 /* IF NOT UPDATE, GET NEW        */ 00157000
         B     @RC00213                                            0219 00158000
@RF00213 BAL   @14,FINDRPL                                         0219 00159000
*   IF RRPL=ZERO                    /* IF NO RPL IS AVAILABLE        */ 00160000
*     THEN                                                         0220 00161000
@RC00213 LTR   RRPL,RRPL                                           0220 00162000
         BNZ   @RF00220                                            0220 00163000
*     DO;                           /* SET INVALID REQUEST           */ 00164000
*       DECEXINV=ON;                /* SET INVALID REQUEST           */ 00165000
         OI    DECEXINV(RDECB),B'00010000'                         0222 00166000
*ERRORX:                                                           0223 00167000
*       R1=ADDR(DECBECB);           /* POINT TO ECB                  */ 00168000
ERRORX   DS    0H                                                  0224 00169000
*       GEN(POST  (1));             /* POST ECB COMPLETE             */ 00170000
         POST  (1)                                                      00171000
*       RETURN;                     /* EXIT                          */ 00172000
         B     @EL00001                                            0225 00173000
*     END;                          /* END OF INVALID REQUEST        */ 00174000
*                                                                  0227 00175000
*   /*****************************************************************/ 00176000
*   /*                                                               */ 00177000
*   /* SAVE THE BASE AND RETURN REGISTERS IN THE RPL EXTENSION ALL   */ 00178000
*   /* OTHER USER REGISTERS WILL BE RESTORED PRIOR TO A VSAM REQUEST */ 00179000
*   /* SO THAT VSAM WILL STORE THE SAME REGISTERS BACK INTO THE USER */ 00180000
*   /* AREA. THIS ALLOWS THE ISAM INTERFACE TO AVOID OBTAINING A     */ 00181000
*   /* SAVEAREA FOR EACH RPL.                                        */ 00182000
*   /*                                                               */ 00183000
*   /*****************************************************************/ 00184000
*                                                                  0227 00185000
*   RPLSAVE=R14SAVE;                /* SAVE USER RETURN POINT        */ 00186000
@RF00220 LA    @12,76                                              0227 00187000
         ALR   @12,RRPL                                            0227 00188000
         L     @10,R14SAVE(,R13)                                   0227 00189000
         ST    @10,RPLSAVE(,@12)                                   0227 00190000
*   RPLSAVE2=R11SAVE;               /* SAVE USER REGISTER 11         */ 00191000
*                                                                  0228 00192000
         L     @10,R11SAVE(,R13)                                   0228 00193000
         ST    @10,RPLSAVE2(,@12)                                  0228 00194000
*   /*****************************************************************/ 00195000
*   /*                                                               */ 00196000
*   /* ALL REQUESTS EXCEPT FOR READ KEY OR WRITE KEY NEW             */ 00197000
*   /* (NON-REPLACE) REQUIRE A GET FOR UPDATE.                       */ 00198000
*   /*                                                               */ 00199000
*   /*****************************************************************/ 00200000
*                                                                  0229 00201000
*   IF DECBTYP2=READK|              /* IF A READ KEY                 */ 00202000
*       (DECBTYP2=WRITEKN&          /* OR A NON-REPLACE              */ 00203000
*       (DECBLGTH=ZERO|             /* WRITE KN                      */ 00204000
*       DCBRECL^='01'B)) THEN       /*                               */ 00205000
         CLI   DECBTYP2(RDECB),B'10000000'                         0229 00206000
         BE    @RT00229                                            0229 00207000
         CLI   DECBTYP2(RDECB),B'00000100'                         0229 00208000
         BNE   @RF00229                                            0229 00209000
         LH    @12,DECBLGTH(,RDECB)                                0229 00210000
         LTR   @12,@12                                             0229 00211000
         BZ    @RT00229                                            0229 00212000
         L     @12,DECBDCBA(,RDECB)                                0229 00213000
         TM    DCBRECL+28(@12),B'01000000'                         0229 00214000
         BNO   @RT00229                                            0229 00215000
         TM    DCBRECL+28(@12),B'10000000'                         0229 00216000
         BZ    @RF00229                                            0229 00217000
@RT00229 DS    0H                                                  0230 00218000
*     RPLUPD=OFF;                   /* SET OFF UPDATE                */ 00219000
         NI    RPLUPD(RRPL),B'11111101'                            0230 00220000
*   ELSE                                                           0231 00221000
*     RPLUPD=ON;                    /* USE UPDATE OTHERWISE          */ 00222000
         B     @RC00229                                            0231 00223000
@RF00229 OI    RPLUPD(RRPL),B'00000010'                            0231 00224000
*   RPLARG=DECBKEY;                 /* POINT RPL TO KEY              */ 00225000
*                                                                  0232 00226000
@RC00229 L     @12,DECBKEY(,RDECB)                                 0232 00227000
         ST    @12,RPLARG(,RRPL)                                   0232 00228000
*   /*****************************************************************/ 00229000
*   /*                                                               */ 00230000
*   /* DETERMINE IF A VSAM GET SHOULD BE ISSUED.                     */ 00231000
*   /*                                                               */ 00232000
*   /*****************************************************************/ 00233000
*                                                                  0233 00234000
*   IF^(DECBTYP2=WRITEK&            /* IF STAND-ALONE                */ 00235000
*       DECEXRKU=ON)&               /* WRITE,                        */ 00236000
*       ^(DECBTYP2=WRITEKN&         /* READ K, READKU, OR            */ 00237000
*       (DECBLGTH=ZERO|             /* WKR                           */ 00238000
*       DCBRECL^='01'B|             /* (NOT WRITE KN)                */ 00239000
*       DECEXRKU=ON)) THEN          /* STAND-ALONE (REPLACE),        */ 00240000
         CLI   DECBTYP2(RDECB),B'00001000'                         0233 00241000
         BNE   @GL00005                                            0233 00242000
         TM    DECEXRKU(RDECB),B'00000001'                         0233 00243000
         BO    @RF00233                                            0233 00244000
@GL00005 CLI   DECBTYP2(RDECB),B'00000100'                         0233 00245000
         BNE   @RT00233                                            0233 00246000
         LH    @12,DECBLGTH(,RDECB)                                0233 00247000
         LTR   @12,@12                                             0233 00248000
         BZ    @RF00233                                            0233 00249000
         L     @12,DECBDCBA(,RDECB)                                0233 00250000
         TM    DCBRECL+28(@12),B'01000000'                         0233 00251000
         BNO   @RF00233                                            0233 00252000
         TM    DCBRECL+28(@12),B'10000000'                         0233 00253000
         BNZ   @RF00233                                            0233 00254000
         TM    DECEXRKU(RDECB),B'00000001'                         0233 00255000
         BO    @RF00233                                            0233 00256000
@RT00233 DS    0H                                                  0234 00257000
*     DO;                           /* RECORD MUST BE READ           */ 00258000
*       IF DECBARS=ON THEN          /* IF DYNAMIC BUFFERING          */ 00259000
         TM    DECBARS(RDECB),B'00000001'                          0235 00260000
         BNO   @RF00235                                            0235 00261000
*         DECBAREA=RPLIIBFR-SIXTEEN;/* POINT TO DYNAMIC BFR SET ADDR    00262000
*                                      FOR VSAM GET                  */ 00263000
         L     @12,RPLIIBFR+76(,RRPL)                              0236 00264000
         SL    @12,@CF00168                                        0236 00265000
         ST    @12,DECBAREA(,RDECB)                                0236 00266000
*       IF DCBRKP=ZERO&             /* THE USER BUFFER CANNOT        */ 00267000
*           DCBRECBR=OFF|           /* BE USED FOR A GET IF          */ 00268000
*           (DECBTYP2=WRITEK&       /* RKP=0 & UNBLOCKED (NO         */ 00269000
*           DECEXRKU=OFF)|          /* ROOM FOR KEY) OR IF           */ 00270000
*           (DECBTYP2=WRITEKN&      /* EITHER A STAND-ALONE          */ 00271000
*           DECBLGTH^=ZERO) THEN    /* WRITE OR WRITE KEY RE-        */ 00272000
@RF00235 L     @12,DECBDCBA(,RDECB)                                0237 00273000
         LH    @10,DCBRKP+52(,@12)                                 0237 00274000
         LTR   @10,@10                                             0237 00275000
         BNZ   @GL00011                                            0237 00276000
         TM    DCBRECBR+28(@12),B'00010000'                        0237 00277000
         BZ    @RT00237                                            0237 00278000
@GL00011 CLI   DECBTYP2(RDECB),B'00001000'                         0237 00279000
         BNE   @GL00010                                            0237 00280000
         TM    DECEXRKU(RDECB),B'00000001'                         0237 00281000
         BZ    @RT00237                                            0237 00282000
@GL00010 CLI   DECBTYP2(RDECB),B'00000100'                         0237 00283000
         BNE   @RF00237                                            0237 00284000
         LH    @12,DECBLGTH(,RDECB)                                0237 00285000
         LTR   @12,@12                                             0237 00286000
         BZ    @RF00237                                            0237 00287000
@RT00237 DS    0H                                                  0238 00288000
*         DO;                       /* PLACE (NEW RECORD             */ 00289000
*           RPLAREA=RPLIIBFR;       /* WOULD BE OVERLAID             */ 00290000
         L     @12,RPLIIBFR+76(,RRPL)                              0239 00291000
         ST    @12,RPLAREA(,RRPL)                                  0239 00292000
*         END;                      /* END OF SPECIAL CASE           */ 00293000
*       ELSE                        /* COME HERE IF USER AREA        */ 00294000
*         DO;                       /* HAS BUFFER POINTER            */ 00295000
         B     @RC00237                                            0241 00296000
@RF00237 DS    0H                                                  0242 00297000
*           IF DCBRECL='01'B THEN   /* IF VLR, INCLUDE RDW IN        */ 00298000
         L     @12,DECBDCBA(,RDECB)                                0242 00299000
         TM    DCBRECL+28(@12),B'01000000'                         0242 00300000
         BNO   @RF00242                                            0242 00301000
         TM    DCBRECL+28(@12),B'10000000'                         0242 00302000
         BNZ   @RF00242                                            0242 00303000
*             RPLAREA=DECBAREA+TWENTY;/* RECORD PREFIX.              */ 00304000
         LA    @12,20                                              0243 00305000
         AL    @12,DECBAREA(,RDECB)                                0243 00306000
         ST    @12,RPLAREA(,RRPL)                                  0243 00307000
*           ELSE                                                   0244 00308000
*             RPLAREA=DECBAREA+SIXTEEN;/* FLR--STANDARD ISAM PFX     */ 00309000
         B     @RC00242                                            0244 00310000
@RF00242 LA    @12,16                                              0244 00311000
         AL    @12,DECBAREA(,RDECB)                                0244 00312000
         ST    @12,RPLAREA(,RRPL)                                  0244 00313000
*         END;                      /* END OF READ IN DECBAREA       */ 00314000
@RC00242 DS    0H                                                  0246 00315000
*       IF(DECBTYP2=READK|          /* IF READ K OR          @YM05335*/ 00316000
*           DECBTYP2=READKU)&       /* READ KU AND           @YM05335*/ 00317000
*           (DCBRECBR=ON|           /* BLOCKED RCDS OR@YM05335       */ 00318000
*           DCBRKP>ZERO) THEN       /* RKP > 0 THEN          @YM05335*/ 00319000
@RC00237 CLI   DECBTYP2(RDECB),B'10000000'                         0246 00320000
         BE    @GL00015                                            0246 00321000
         CLI   DECBTYP2(RDECB),B'00100000'                         0246 00322000
         BNE   @RF00246                                            0246 00323000
@GL00015 L     @12,DECBDCBA(,RDECB)                                0246 00324000
         TM    DCBRECBR+28(@12),B'00010000'                        0246 00325000
         BO    @RT00246                                            0246 00326000
         LH    @12,DCBRKP+52(,@12)                                 0246 00327000
         LTR   @12,@12                                             0246 00328000
         BNP   @RF00246                                            0246 00329000
@RT00246 DS    0H                                                  0247 00330000
*         RPLKGE=ON;                /* SEARCH KEY EQUAL/HIGH @YM05335*/ 00331000
         OI    RPLKGE(RRPL),B'00000100'                            0247 00332000
*                                                                  0248 00333000
*       /*************************************************************/ 00334000
*       /*                                                           */ 00335000
*       /* ISSUE VSAM GET (NUP OR UPD).                              */ 00336000
*       /*                                                           */ 00337000
*       /*************************************************************/ 00338000
*                                                                  0248 00339000
*       R1=ADDR(IFGRPL);            /* POINT TO RPL                  */ 00340000
@RF00246 LR    R1,RRPL                                             0248 00341000
*       RFY                                                        0249 00342000
*        (R2,                                                      0249 00343000
*         R3,                                                      0249 00344000
*         R4,                                                      0249 00345000
*         R5,                                                      0249 00346000
*         R6,                                                      0249 00347000
*         R7,                                                      0249 00348000
*         R8,                                                      0249 00349000
*         R9,                                                      0249 00350000
*         R10,                                                     0249 00351000
*         R12) RSTD;                /* INDICATE REG CHANGE           */ 00352000
*       GEN(LM    R2,R10,TWENTY8(R13));/* RESTORE USER REGS          */ 00353000
         LM    R2,R10,TWENTY8(R13)                                      00354000
*       RFY                                                        0251 00355000
*         IFGRPL BASED(R1);         /* SET NEW RPL BASE              */ 00356000
*       R12=R12SAVE;                /* SET TO USER R12 ISSUE VSAM GET*/ 00357000
         L     R12,R12SAVE(,R13)                                   0252 00358000
*       GEN(GET   RPL=(1));         /*                               */ 00359000
         GET   RPL=(1)                                                  00360000
*       RFY                                                        0254 00361000
*        (R2,                                                      0254 00362000
*         R3,                                                      0254 00363000
*         R4,                                                      0254 00364000
*         R5,                                                      0254 00365000
*         R6,                                                      0254 00366000
*         R7,                                                      0254 00367000
*         R8,                                                      0254 00368000
*         R9,                                                      0254 00369000
*         R10,                                                     0254 00370000
*         R12) UNRSTD;              /* DROP REGISTERS                */ 00371000
*       RRPL=R1;                    /* RESET RPL BASE                */ 00372000
         LR    RRPL,R1                                             0255 00373000
*       RFY                                                        0256 00374000
*         IFGRPL BASED(RRPL);       /* DEFINE RPL BASE               */ 00375000
*       RPLKGE=OFF;                 /* TURN OFF HIGH/EQ SEARCH     0257 00376000
*                                                            @YM05335*/ 00377000
         NI    RPLKGE(RRPL),B'11111011'                            0257 00378000
*       RDECB=RPLDECB;              /* SET DECB BASE                 */ 00379000
         L     RDECB,RPLDECB+76(,RRPL)                             0258 00380000
*       IF RPLERRCD=RPLNOPLH        /* IF NO PLACEHOLDER,            */ 00381000
*         THEN                                                     0259 00382000
         CLI   RPLERRCD(RRPL),64                                   0259 00383000
         BNE   @RF00259                                            0259 00384000
*         DECEXINV=ON;              /* SET INVALID REQUEST           */ 00385000
         OI    DECEXINV(RDECB),B'00010000'                         0260 00386000
*                                                                  0261 00387000
*       /*************************************************************/ 00388000
*       /*                                                           */ 00389000
*       /* FOR A SUCCESSFUL ISAM READ SET THE LOGICAL RECORD POINTER */ 00390000
*       /* AND, FOR VARIABLE LENGTH RECORDS, SET THE RECORD          */ 00391000
*       /* DESCRIPTOR WORD. IF A DATA ONLY RETRIEVAL WAS DONE, MOVE  */ 00392000
*       /* DATA TO THE USER AREA IF NECESSARY.                       */ 00393000
*       /*                                                           */ 00394000
*       /*************************************************************/ 00395000
*                                                                  0261 00396000
*       IF DECBEXC1=CZERO&          /* IF NO ERRORS AND              */ 00397000
*           (DECBTYP2=READK|        /* IF A READ WITH OR WITH-       */ 00398000
*           DECBTYP2=READKU) THEN   /* OUT UPDATE, SET LOGR.         */ 00399000
@RF00259 CLI   DECBEXC1(RDECB),B'00000000'                         0261 00400000
