*       RFY                                                        0454 01128000
*         RWORK1 UNRSTD;            /* DROP WORK REG                 */ 01129000
*     END;                          /* END OF POST DECBECB           */ 01130000
*   RETURN;                         /* END OF READ/WRITE CODE        */ 01131000
         B     @EL00001                                            0456 01132000
*                                                                  0457 01133000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/ 01134000
*/*                                                                  */ 01135000
*/*   FUNCTION = FINDRPL                                             */ 01136000
*/*      FIND AN AVAILABLE RPL ON THE II RPL CHAIN POINTED TO BY THE */ 01137000
*/*      IICB AND THE RPLE.                                          */ 01138000
*/*                                                                  */ 01139000
*/*   INPUT =                                                        */ 01140000
*/*      REGISTER 1 = DECB POINTER                                   */ 01141000
*/*                                                                  */ 01142000
*/*   OUTPUT =                                                       */ 01143000
*/*      RRPL = ZERO IF NO AVAILABLE RPL                             */ 01144000
*/*      RRPL = AVAILABLE RPL ADDRESS                                */ 01145000
*/*                                                                  */ 01146000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/ 01147000
*                                                                  0457 01148000
*FINDRPL:                                                          0457 01149000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* FIND AN AVAILABLE RPL         */ 01150000
FINDRPL  DS    0H                                                  0458 01151000
*   RFY                                                            0458 01152000
*     R14 RSTD;                     /* SAVE RETURN ADDR              */ 01153000
*   RRPL=IIRPLPTR;                  /* POINT TO FIRST RPL            */ 01154000
         L     @15,DECBDCBA(,RDECB)                                0459 01155000
         L     @15,DCBDEBAD+40(,@15)                               0459 01156000
         L     @15,DEBEXPT(,@15)                                   0459 01157000
         L     @15,DEBWKPT5(,@15)                                  0459 01158000
         L     RRPL,IIRPLPTR(,@15)                                 0459 01159000
*   DO WHILE RRPL^=ZERO;            /* SEARCH RPL CHAIN              */ 01160000
         B     @DE00460                                            0460 01161000
@DL00460 DS    0H                                                  0461 01162000
*     TS(RPLIITSB);                 /* SEE IF BUSY                   */ 01163000
         TS    RPLIITSB+76(RRPL)                                   0461 01164000
*     BC(8,RPLHERE);                /* BRANCH IF NOT BUSY            */ 01165000
         BC    8,RPLHERE                                           0462 01166000
*     RRPL=RPLRPLPT;                /* GO TO NEXT RPL                */ 01167000
         L     @15,RPLRPLPT+76(,RRPL)                              0463 01168000
         LR    RRPL,@15                                            0463 01169000
*   END;                            /* END OF RPL SEARCH             */ 01170000
@DE00460 LTR   RRPL,RRPL                                           0464 01171000
         BNZ   @DL00460                                            0464 01172000
*   RETURN;                         /* RETURN (RRPL = 0)             */ 01173000
@EL00002 DS    0H                                                  0465 01174000
@EF00002 DS    0H                                                  0465 01175000
@ER00002 BR    @14                                                 0465 01176000
*RPLHERE:                                                          0466 01177000
*   RPLDECB=RDECB;                  /* POINT RPL TO DECB             */ 01178000
RPLHERE  ST    RDECB,RPLDECB+76(,RRPL)                             0466 01179000
*   RETURN;                         /* RETURN TO CALLER              */ 01180000
         B     @EL00002                                            0467 01181000
*   END FINDRPL;                    /* END OF FINDRPL RTN            */ 01182000
*                                                                  0469 01183000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/ 01184000
*/*                                                                  */ 01185000
*/*   FUNCTION = FINDORPL                                            */ 01186000
*/*      FIND THE RPL THAT WAS USED FOR THE READ FOR UPDATE SO THAT  */ 01187000
*/*      IT CAN BE USED FOR THE COMPLEMENTARY WRITE FOR UPDATE.      */ 01188000
*/*                                                                  */ 01189000
*/*   INPUT =                                                        */ 01190000
*/*      REGISTER 1 = DECB POINTER                                   */ 01191000
*/*                                                                  */ 01192000
*/*   OUTPUT =                                                       */ 01193000
*/*      RRPL = ZERO IF THE RPL IS NOT FOUND                         */ 01194000
