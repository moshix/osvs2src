         LH    @10,DCBRKP(,@12)                                    0392 00923000
         LTR   @10,@10                                             0392 00924000
         BNZ   @RF00392                                            0392 00925000
*                     DO;           /* ADJACENT TO EACH OTHER        */ 00926000
*                       R5=DCBLRECL;/* GET DATA LENGTH               */ 00927000
         LH    R5,DCBLRECL(,@12)                                   0394 00928000
*                       R7=R5;      /* PUT IN BOTH ODD REGS          */ 00929000
         LR    R7,R5                                               0395 00930000
