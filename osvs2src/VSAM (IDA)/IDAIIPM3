*       R14SAVE=RPLSAVE;            /* RESTORE USER RETURN REG       */ 01076000
         L     @10,RPLSAVE(,@12)                                   0434 01077000
         ST    @10,R14SAVE(,R13)                                   0434 01078000
*       RPLIITSB=CZERO;             /* MAKE RPL AVAILABLE            */ 01079000
         MVI   RPLIITSB(@12),B'00000000'                           0435 01080000
*       DECEXRKU=OFF;               /* SET OFF UPDATE BIT            */ 01081000
         NI    DECEXRKU(RDECB),B'11111110'                         0436 01082000
*     END;                          /* END OF NON-READ KU CODE       */ 01083000
*   ELSE                            /* SUCCESSFUL READ               */ 01084000
*     DO;                           /* FOR UPDATE                    */ 01085000
         B     @RC00420                                            0438 01086000
@RF00420 DS    0H                                                  0439 01087000
*       DECEXRKU=ON;                /* INDICATE READ KU              */ 01088000
         OI    DECEXRKU(RDECB),B'00000001'                         0439 01089000
*       R01SAVE=RDECB;              /* RESTORE R1            @ZA01395*/ 01090000
         ST    RDECB,R01SAVE(,R13)                                 0440 01091000
*       R14SAVE=RPLSAVE;            /* RESTORE USER RETURN REG       */ 01092000
         LA    @12,76                                              0441 01093000
         ALR   @12,RRPL                                            0441 01094000
         L     @10,RPLSAVE(,@12)                                   0441 01095000
