*             R10,                                                 0360 00797000
*             R12) UNRSTD;          /* DROP REGISTERS                */ 00798000
*           RRPL=R1;                /* RESET RPL BASE                */ 00799000
         LR    RRPL,R1                                             0361 00800000
*           RFY                                                    0362 00801000
*             IFGRPL BASED(RRPL);   /* REDEFINE RPL BASE             */ 00802000
*         END;                      /* END OF PUT CODE               */ 00803000
*       RDECB=RPLDECB;              /* SET DECB BASE                 */ 00804000
@RC00332 L     RDECB,RPLDECB+76(,RRPL)                             0364 00805000
*       IF RPLERRCD=RPLNOPLH THEN   /* IF NO PLACEHOLDER,            */ 00806000
         CLI   RPLERRCD(RRPL),64                                   0365 00807000
         BNE   @RF00365                                            0365 00808000
*         DECEXINV=ON;              /* SET INVALID REQUEST           */ 00809000
         OI    DECEXINV(RDECB),B'00010000'                         0366 00810000
*                                                                  0367 00811000
*       /*************************************************************/ 00812000
*       /*                                                           */ 00813000
*       /* IF THE INSERT IS A DUPLICATE, READ THE OLD RECORD FOR     */ 00814000
*       /* UPDATE TO SEE IF IT WAS DELETED. IF IT WAS DELETED, DO AN */ 00815000
*       /* UPDATE PUT. OTHERWISE, SET DUPLICATE RECORD ERROR AGAIN.  */ 00816000
*       /*                                                           */ 00817000
*       /*************************************************************/ 00818000
*                                                                  0367 00819000
*       IF DECEXDUP=ON&             /* IF A DUPLICATE RECORD         */ 00820000
*           DCBOPTL=ON THEN         /* BUT DELETE OPTION ON          */ 00821000
@RF00365 TM    DECEXDUP(RDECB),B'00000001'                         0367 00822000
