*                       GEN(MVCL  R4,R6);/* MOVE TO IIBFR+KEYLEN     */ 00934000
         MVCL  R4,R6                                                    00935000
*                     END;          /* END OF RKP = 0 CODE           */ 00936000
*                   ELSE            /* HERE IF RKP NON-ZERO          */ 00937000
*                     IF DCBRECL='01'B THEN/* IF VLR, ALLOW FOR RDW  */ 00938000
         B     @RC00392                                            0399 00939000
@RF00392 L     @12,DECBDCBA(,RDECB)                                0399 00940000
