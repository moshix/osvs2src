*                   RFY                                            0250 00250000
*                     IHADCB BASED(R15);/* CHANGE DCB BASE           */ 00251000
*                   RRPL=TCBRBP;    /* POINT TO SYNCH SVRB           */ 00252000
         L     @12,DCBDEBAD+40(,R15)                               0251 00253000
         L     @12,TCBPTR(,@12)                                    0251 00254000
