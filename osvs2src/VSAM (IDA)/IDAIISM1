*                       ELSE        /*                               */ 00121000
*                         IF RPLERRCD=RPLEODER THEN/*        @OZ02676*/ 00122000
         B     @RC00210                                            0212 00123000
@RF00210 CLI   RPLERRCD(RRPL),4                                    0212 00124000
         BNE   @RF00212                                            0212 00125000
*                           DO;     /* CALL II EODAD         @OZ02676*/ 00126000
*                             RFY                                  0214 00127000
*                               R15 RSTD;/* GET REG          @OZ02676*/ 00128000
*                             RFY                                  0215 00129000
*                               IFGACB BASED(IIACBPTR);/* PTR      0215 00130000
*                                                            @OZ02676*/ 00131000
*                             RFY                                  0216 00132000
*                               IFGEXLST BASED(ACBEXLST);/* PT     0216 00133000
*                                                            @OZ02676*/ 00134000
*                             R15=EXLEODP;/* POINT TO EXLST  @OZ02676*/ 00135000
         L     @12,IIACBPTR(,RIICB)                                0217 00136000
         L     @12,ACBEXLST(,@12)                                  0217 00137000
         ICM   R15,15,EXLEODP(@12)                                 0217 00138000
*                             R1=RRPL;/* POINT TO RPL        @OZ02676*/ 00139000
