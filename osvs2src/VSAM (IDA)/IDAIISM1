         OI    DECEXDUP(RDECB),B'00000001'                         0295 00403000
*             ELSE                  /* IF NOT ABLE TO EXTEND DATA SET   00404000
*                                      THEN SET                      */ 00405000
*               IF RPLERRCD=RPLNOEXT THEN                          0296 00406000
         B     @RC00294                                            0296 00407000
@RF00294 CLI   RPLERRCD(RRPL),28                                   0296 00408000
         BNE   @RF00296                                            0296 00409000
*                 DECEXNSP=ON;      /* SPACE NOT FOUND IN DECB       */ 00410000
         OI    DECEXNSP(RDECB),B'00100000'                         0297 00411000
*               ELSE                /* IF VSAM FOUND EXCLUSIVE USE 0298 00412000
*                                      CONFLICT                      */ 00413000
*                 IF RPLERRCD=RPLEXCL|/* NO KEY RANGE FOR INSERT     */ 00414000
*                     RPLERRCD=RPLNOKR|/* OR KEY CHANGE ATTEMPT      */ 00415000
*                     RPLERRCD=RPLKEYCH THEN                       0298 00416000
         B     @RC00296                                            0298 00417000
@RF00296 CLI   RPLERRCD(RRPL),20                                   0298 00418000
         BE    @RT00298                                            0298 00419000
         CLI   RPLERRCD(RRPL),36                                   0298 00420000
         BE    @RT00298                                            0298 00421000
         CLI   RPLERRCD(RRPL),96                                   0298 00422000
         BNE   @RF00298                                            0298 00423000
@RT00298 DS    0H                                                  0299 00424000
*                   DECEXINV=ON;    /* SET DECB INVALID REQ          */ 00425000
