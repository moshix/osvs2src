*                             GEN(BR 15);/* EXIT TO EODAD    @OZ02676*/ 00141000
         BR 15                                                          00142000
*                           END;    /* END EODAD CALL        @OZ02676*/ 00143000
*                         ELSE      /* TRY ANOTHER           @OZ02676   00144000
*                                      CALL READ EXCLUSIVE OR        */ 00145000
*                           IF RPLERRCD=RPLEXCL|/* REQUEST AFTER EOD    00146000
*                                      OR                            */ 00147000
*                               RPLERRCD=RPLNOKR|/* OR KEY CHANGE TRY   00148000
*                                      A                             */ 00149000
*                               RPLERRCD=RPLKEYCH THEN             0221 00150000
         B     @RC00212                                            0221 00151000
