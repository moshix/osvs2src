*                   IF IISAMSYN(ONE)=CZERO THEN/* IF SYNAD NOT LOADED*/ 00270000
@RF00253 CLI   IISAMSYN(RIICB),B'00000000'                         0255 00271000
         BNE   @RF00255                                            0255 00272000
*                     R1=ZERO;      /* ZERO R1                       */ 00273000
         SLR   R1,R1                                               0256 00274000
*                   ELSE                                           0257 00275000
*                     R1=R15;       /* OTHERWISE, DCB ADDR           */ 00276000
*                                                                  0257 00277000
