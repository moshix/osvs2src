*               DO;                 /* THEN GET OR PUTX ERROR        */ 00800000
*                 IF DCBEXOER=ON THEN/* IF UNCORRECTABLE OUTPUT      */ 00801000
         TM    DCBEXOER(@08),B'00000100'                           0394 00802000
         BNO   @RF00394                                            0394 00803000
*                   EROPER=PUTX;    /* ERROR, SET PUTX CODE.         */ 00804000
         MVC   EROPER(6,R1),PUTX                                   0395 00805000
*                 ELSE                                             0396 00806000
*                   EROPER=GET;     /* OTHERWISE, SET GET.           */ 00807000
         B     @RC00394                                            0396 00808000
@RF00394 MVC   EROPER(6,R1),GET                                    0396 00809000
*               END;                /* END OF NON-SETL ERROR         */ 00810000
*             ELSE                                                 0398 00811000
*               EROPER=SETL;        /* SET BAD SETL                  */ 00812000
         B     @RC00392                                            0398 00813000
@RF00392 MVC   EROPER(6,R1),SETL                                   0398 00814000
*         END;                      /* END OF SCAN CODE              */ 00815000
*       ELSE                                                       0400 00816000
*         EROPER=PUT;               /* IF LOAD MODE, SET PUT         */ 00817000
         B     @RC00389                                            0400 00818000
@RF00389 MVC   EROPER(6,R1),PUT                                    0400 00819000
*       IF AUDACBCL=ON&AUDACBRC=OFF THEN                           0401 00820000
@RC00389 L     @12,DCBDEBAD+40(,R2)                                0401 00821000
