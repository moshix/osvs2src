*                     R0=ADDR(PLHKEY);/* SET HIGH KEY PTR            */ 00238000
         L     @12,RPLPLHPT(,RRPL)                                 0246 00239000
         LA    R0,PLHKEY(,@12)                                     0246 00240000
*                   ELSE                                           0247 00241000
*                     R0=ZERO;      /* NO BUFFER ADDR                */ 00242000
         B     @RC00245                                            0247 00243000
@RF00245 SLR   R0,R0                                               0247 00244000
*                   RFY                                            0248 00245000
*                     R15 RESTRICTED;/* GET REGISTER                 */ 00246000
@RC00245 DS    0H                                                  0249 00247000
