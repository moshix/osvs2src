*             DO;                   /* COME HERE.                    */ 00220000
*               IF DCBEXCLD=ON THEN /* WAS SYNAD ENTERED             */ 00221000
         TM    DCBEXCLD(@12),B'00100000'                           0238 00222000
         BO    @RT00238                                            0238 00223000
*                 GO TO EXIT;       /* BEFORE? YES, EXIT.            */ 00224000
*               ELSE                /* FIRST TIME CLOSE ERROR        */ 00225000
*                 DO;                                              0240 00226000
*                   R14=RWORK3;     /* GET CLOSE RET ADDR            */ 00227000
         LR    R14,RWORK3                                          0241 00228000
*                   RFY                                            0242 00229000
