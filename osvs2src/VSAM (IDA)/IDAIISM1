*                   DCBEXCLD=ON;    /* INDICATE ERROR IN CLOSE       */ 00231000
         OI    DCBEXCLD(@12),B'00100000'                           0243 00232000
*                   RFY                                            0244 00233000
*                     R0 RESTRICTED;/* GET REGISTER                  */ 00234000
*                   IF DCBEXSEQ=ON THEN/* SEQUENCE CHK ERROR?        */ 00235000
         TM    DCBEXSEQ(@12),B'10000000'                           0245 00236000
