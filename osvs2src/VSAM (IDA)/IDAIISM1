         BNE   @RF00406                                            0406 00843000
@RT00406 DS    0H                                                  0407 00844000
*         EROPER=READ;              /* READ OPERATION.               */ 00845000
         MVC   EROPER(6,R1),READ                                   0407 00846000
*       ELSE                                                       0408 00847000
*         EROPER=WRITE;             /* OTHERWISE, SET WRITE.         */ 00848000
         B     @RC00406                                            0408 00849000
@RF00406 MVC   EROPER(6,R1),WRITE                                  0408 00850000
*     END;                          /* END OF BISAM CODE             */ 00851000
@RC00406 DS    0H                                                  0410 00852000
*   R0SAVE=NOBFRAD;                 /* INDICATE NO BUFFER ADDR       */ 00853000
