*     END;                          /* END OF QISAM CODE             */ 00831000
*   ELSE                            /* HERE FOR BISAM                */ 00832000
*     DO;                           /*                               */ 00833000
         B     @RC00386                                            0404 00834000
@RF00386 DS    0H                                                  0405 00835000
*       ERDAAM=BISAM;               /* SET BISAM CODE                */ 00836000
         MVC   ERDAAM(6,R1),BISAM                                  0405 00837000
*       IF DECBTYP2=READK|          /* IF A READ KEY OR READ         */ 00838000
*           DECBTYP2=READKU THEN    /* KEY FOR UPDATE, SET A         */ 00839000
