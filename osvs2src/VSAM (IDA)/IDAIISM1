*                     DECEXRLC=ON;  /* SET DECB RCD LENGTH CHK       */ 00433000
         OI    DECEXRLC(RDECB),B'01000000'                         0301 00434000
*                   ELSE                                           0302 00435000
*                     DECEXUBK=ON;  /* ELSE UNKNOWN ERROR            */ 00436000
