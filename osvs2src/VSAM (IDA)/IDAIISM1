*         DO;                       /* ABEND(QABCODE) DUMP SYSTEM    */ 00347000
@RF00234 DS    0H                                                  0279 00348000
*           RESPECIFY                                              0279 00349000
*            (GPR01F) RESTRICTED;                                  0279 00350000
*           GPR01F=((QABCODE)&'00000FFF'X)*4096;/* COMP CODE IN BITS    00351000
*                                      8-19                          */ 00352000
         L     GPR01F,@CF02043                                     0280 00353000
*           GPR01F=GPR01F|'80000000'X;/* DUMP/STEP/DUMPOPTS FLAG     */ 00354000
