*FALLVGTT:                                                         0265 00231000
*                                                                  0265 00232000
*   /*****************************************************************/ 00233000
*   /*                                                               */ 00234000
*   /* FALLVGTT - LOCATE ALL VGTT BLOCKS EXCEPT RESTART .    @Z30SSMD*/ 00235000
*   /* FREMAIN CORE BASED ON THE TYPE OF VGTT. 1) LSR VGTT IS        */ 00236000
*   /* CONTIGUOUS TO THE CORE IT DESCRIBES. 2) AN OPEN VGTT IS THE   */ 00237000
*   /* ANCHOR FOR THE PSB WHICH CONTAINS HEB ELEMENTS. RANGE CHECK   */ 00238000
*   /* THESE ADDRESSES AND THEN CONDITIONAL FREEMAIN USING THE       */ 00239000
*   /* ADDRESS IN THE HEB ELEMENT. USE THE GDA FOR THE END OF THE    */ 00240000
*   /* PRIVATE AREA ( START OF GLOBAL). 3) CTLG VGTT BLOCKS POINT TO */ 00241000
*   /* THE BIB, CSL, AND PSB. FREEMAIN BASED ON THESE ANCHORS. IF    */ 00242000
*   /* CVTAMFF ='04'X THEN THIS PROCEDURE IS A NOP. THIS ROUTINE     */ 00243000
*   /* ADOPTS A DEFENSIVE POSTURE BY : 1) REMOVING THE ENTIRE VGTT   */ 00244000
*   /* CHAIN FROM THE ASCB TO PREVENT BAD DATA FROM CAUSING          */ 00245000
*   /* REPETITIVE PROGRAM CHECKS DURING TERMINATION. 2) VERIFYING    */ 00246000
*   /* THAT THE VGTT ABOUT TO BE PROCESSED HAS THE CORRECT CONTROL   */ 00247000
*   /* BLOCK ID ('VGTT'). IF A VGTT POINTS TO ITSELF IT IS PROCESSED */ 00248000
*   /* ONLY ONCE.                                                    */ 00249000
*   /*                                                               */ 00250000
*   /*****************************************************************/ 00251000
*                                                                  0265 00252000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0265 00253000
FALLVGTT DS    0H                                                  0266 00254000
*   RFY                                                            0266 00255000
*     IDAVGTT BASED(VGTTPTR);       /* INFORM PL/S OF BASE.          */ 00256000
*   RFY                                                            0267 00257000
*     R0 RSTD;                      /* RESERVE R0.           @Z30SSMD*/ 00258000
*   RFY                                                            0268 00259000
*     R1 RSTD;                      /* RESERVE R1.           @Z30SSMD*/ 00260000
*   RFY                                                            0269 00261000
*     R14 RSTD;                     /* RESERVE R14.          @Z30SSMD*/ 00262000
*   RSAVR14(RSAVCNTR)=R14;          /* SAVE RETURN ADDRESS.  @Z30SSMD*/ 00263000
         LR    @15,RSAVCNTR                                        0270 00264000
         SLA   @15,2                                               0270 00265000
         ST    R14,RSAVR14-4(@15)                                  0270 00266000
*   RSAVCNTR=RSAVCNTR+ONE;          /* POINT TO NEXT SAVEAREA.     0271 00267000
*                                                            @Z30SSMD*/ 00268000
         LA    RSAVCNTR,1(,RSAVCNTR)                               0271 00269000
*   RFY                                                            0272 00270000
*     R14 UNRSTD;                   /* RELEASE R14.          @Z30SSMD*/ 00271000
*   RSAVR1=R1;                      /* SAVE R1 FROM DAMAGE.          */ 00272000
         ST    R1,RSAVR1                                           0273 00273000
*   VGTTPTR=ASCBVGTT;               /* GET THE FIRST VGTT.   @Z30SSMD*/ 00274000
         L     @12,ASCBAD                                          0274 00275000
         L     @11,ASCBVGTT(,@12)                                  0274 00276000
         LR    VGTTPTR,@11                                         0274 00277000
*   MWANVGTT=ASCBVGTT;              /* INITIALIZE LOCAL CHAIN HEAD.     00278000
*                                                            @Z30SSMD*/ 00279000
         ST    @11,MWANVGTT                                        0275 00280000
*   ASCBVGTT=ZERO;                  /* INDICATE PROCESSING COMPLETED.   00281000
*                                                            @Z30SSMD*/ 00282000
         SLR   @11,@11                                             0276 00283000
         ST    @11,ASCBVGTT(,@12)                                  0276 00284000
*   DO WHILE(VGTTPTR^=ZERO&         /* SCAN VGTT CHAIN,      @Z30SSMD*/ 00285000
*         VGTTID=VGTTNAME);         /* AND PROCESS IF VGTT.  @Z30SSMD*/ 00286000
         B     @DE00277                                            0277 00287000
@DL00277 DS    0H                                                  0278 00288000
*     CALL LOCKUP;                  /* SERIALIZE THIS PROCESS.     0278 00289000
*                                                            @Z30SSMD*/ 00290000
         BAL   @14,LOCKUP                                          0278 00291000
*     CALL VDECHAIN;                /* DECHAIN THE VGTT.     @Z30SSMD*/ 00292000
         BAL   @14,VDECHAIN                                        0279 00293000
*     CALL UNLOCK;                  /* RELEASE THIS PROCESS. @Z30SSMD*/ 00294000
         BAL   @14,UNLOCK                                          0280 00295000
*     IF VGTTSDS=ON|VGTTCBIC=ON THEN/* IF SYSTEM DATA SET OR @ZA16012*/ 00296000
         TM    VGTTSDS(VGTTPTR),B'10000100'                        0281 00297000
         BZ    @RF00281                                            0281 00298000
*       DO;                         /* CBIC                  @ZA16012*/ 00299000
*         IF VGTTBIB^=ZERO&BIBCSL^=ZERO THEN/* IF BIB OR CSL EXIST 0283 00300000
*                                                            @Z40WSRL*/ 00301000
         SLR   @12,@12                                             0283 00302000
         L     @11,VGTTBIB(,VGTTPTR)                               0283 00303000
         CR    @11,@12                                             0283 00304000
         BE    @RF00283                                            0283 00305000
         C     @12,BIBCSL(,@11)                                    0283 00306000
         BE    @RF00283                                            0283 00307000
*           CALL FCTLG;             /* FREEMAIN VIA THE CSL  @Z40WSRL*/ 00308000
         BAL   @14,FCTLG                                           0284 00309000
*         ELSE                      /* BIB OR CSL DOES NOT EXIST   0285 00310000
*                                                            @Z40WSRL*/ 00311000
*           CALL FOPEN;             /* FREEMAIN VIA THE PSB PTR IN 0285 00312000
*                                      VGTT                  @Z40WSRL*/ 00313000
         B     @RC00283                                            0285 00314000
@RF00283 BAL   @14,FOPEN                                           0285 00315000
*       END;                        /*                       @Z40WSRL*/ 00316000
*     ELSE                          /* NOT SYSTEM DATA SET VGTT    0287 00317000
*                                                            @Z40WSRL*/ 00318000
*       DO;                         /*                       @Z40WSRL*/ 00319000
         B     @RC00281                                            0287 00320000
@RF00281 DS    0H                                                  0288 00321000
*         IF VGTTOPEN=ON THEN       /* IS THIS AN OPEN VGTT? @Z30SSMD*/ 00322000
         TM    VGTTOPEN(VGTTPTR),B'00001000'                       0288 00323000
         BNO   @RF00288                                            0288 00324000
*           CALL FOPEN;             /* FREEMAIN THAT TYPE.   @Z30SSMD*/ 00325000
         BAL   @14,FOPEN                                           0289 00326000
*         ELSE                      /* IF NOT THAT TYPE TEST FOR   0290 00327000
*                                      OTHERS.               @Z30SSMD*/ 00328000
*           IF VGTTLSR=ON THEN      /* IS IT A LSR VGTT?     @Z30SSMD*/ 00329000
         B     @RC00288                                            0290 00330000
@RF00288 TM    VGTTLSR(VGTTPTR),B'00100000'                        0290 00331000
         BNO   @RF00290                                            0290 00332000
*             CALL FLSR;            /* IF SO, FREE THAT TYPE.      0291 00333000
*                                                            @Z30SSMD*/ 00334000
         BAL   @14,FLSR                                            0291 00335000
*           ELSE                    /* IF NOT THAT TYPE CONTINUE   0292 00336000
*                                      TESTING.              @Z30SSMD*/ 00337000
*             IF VGTTCTLG=ON THEN   /* IS IT A CTLG VGTT?    @Z30SSMD*/ 00338000
         B     @RC00290                                            0292 00339000
@RF00290 TM    VGTTCTLG(VGTTPTR),B'00010000'                       0292 00340000
         BNO   @RF00292                                            0292 00341000
*               CALL FCTLG;         /* YES, FREE THAT TYPE.  @Z30SSMD*/ 00342000
         BAL   @14,FCTLG                                           0293 00343000
*             ELSE                  /* COULD IT BE A GSR VGTT?     0294 00344000
*                                                            @Z30SSMD*/ 00345000
*               IF VGTTGSR=ON THEN  /* CHECK FOR A GSR VGTT. @Z30SSMD*/ 00346000
         B     @RC00292                                            0294 00347000
@RF00292 TM    VGTTGSR(VGTTPTR),B'01000000'                        0294 00348000
         BNO   @RF00294                                            0294 00349000
*                 CALL FCTLG;       /* FREE UP GLOBAL CORE.  @Z30SSMD*/ 00350000
         BAL   @14,FCTLG                                           0295 00351000
*               ELSE                /* OTHERWISE IGNORE THIS VGTT. 0296 00352000
*                                                            @Z30SSRL*/ 00353000
*                 ;                                                0296 00354000
@RF00294 DS    0H                                                  0297 00355000
*       END;                        /*                       @Z40WSRL*/ 00356000
@RC00292 DS    0H                                                  0297 00357000
@RC00290 DS    0H                                                  0297 00358000
@RC00288 DS    0H                                                  0298 00359000
*     VGTTPTR=MWANVGTT;             /* PICK UP THE NEXT VGTT ON THE     00360000
*                                      CHAIN                 @Z30SSMD*/ 00361000
@RC00281 L     VGTTPTR,MWANVGTT                                    0298 00362000
*   END;                            /* END OF VGTT SCAN GROUP.     0299 00363000
*                                                            @Z30SSMD*/ 00364000
@DE00277 LTR   VGTTPTR,VGTTPTR                                     0299 00365000
         BZ    @DC00277                                            0299 00366000
         CLC   VGTTID(4,VGTTPTR),@CC02363                          0299 00367000
         BE    @DL00277                                            0299 00368000
@DC00277 DS    0H                                                  0300 00369000
*   RFY                                                            0300 00370000
*     R14 RSTD;                     /* RESERVE R14.                  */ 00371000
*   R1=RSAVR1;                      /* RESTORE R1.                   */ 00372000
         L     R1,RSAVR1                                           0301 00373000
*   RSAVCNTR=RSAVCNTR-ONE;          /* POINT TO THIS SAVEAREA.     0302 00374000
*                                                            @Z30SSMD*/ 00375000
         BCTR  RSAVCNTR,0                                          0302 00376000
*   R14=RSAVR14(RSAVCNTR);          /* GET RETURN ADDRESS.   @Z30SSMD*/ 00377000
         LR    @12,RSAVCNTR                                        0303 00378000
         SLA   @12,2                                               0303 00379000
         L     R14,RSAVR14-4(@12)                                  0303 00380000
*   RFY                                                            0304 00381000
*     R14 UNRSTD;                   /* RELEASE R14.                  */ 00382000
*   RFY                                                            0305 00383000
*     R0 UNRSTD;                    /* RELEASE R0.                   */ 00384000
*   RFY                                                            0306 00385000
*     R1 UNRSTD;                    /* RELEASE R1.                   */ 00386000
*   RETURN;                         /* TAKE EXIT.                    */ 00387000
@EL00005 DS    0H                                                  0307 00388000
@EF00005 DS    0H                                                  0307 00389000
@ER00005 BR    @14                                                 0307 00390000
*   END FALLVGTT;                   /* END OF PROC, FALLVGTT.        */ 00391000
*LOCKUP:                                                           0309 00392000
*                                                                  0309 00393000
*   /*****************************************************************/ 00394000
