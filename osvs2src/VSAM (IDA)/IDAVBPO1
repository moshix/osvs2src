*   RFY                                                            0109 00047000
*    (R2,                                                          0109 00048000
*     R4) UNRSTD;                                                  0109 00049000
*                                                                  0110 00050000
*   /*****************************************************************/ 00051000
*   /*                                                               */ 00052000
*   /* IF THIS IS A NEW DATASET THEN ACQUIRE AND INITIALIZE DSPCT HDR*/ 00053000
*   /* AND GET A LOGICAL GROUP NUMBER(LGN) WHICH IDENTIFIES THIS VIO */ 00054000
*   /* DATASET.                                                      */ 00055000
*   /*                                                               */ 00056000
*   /*****************************************************************/ 00057000
*                                                                  0110 00058000
*   IF VBPOHPTR=ZERO THEN                                          0110 00059000
         LTR   @10,@10                                             0110 00060000
         BNZ   @RF00110                                            0110 00061000
*     DO;                                                          0111 00062000
*                                                                  0111 00063000
*       /*************************************************************/ 00064000
*       /*                                                           */ 00065000
*       /* FIND MAXIMUM DATASET SIZE USING INPUT PARM LIST           */ 00066000
*       /*                                                           */ 00067000
*       /*************************************************************/ 00068000
*                                                                  0112 00069000
*       RFY                                                        0112 00070000
*        (DSSIZE,                                                  0112 00071000
*         HDRSIZE) RSTD;                                           0112 00072000
*                                                                  0112 00073000
*/* IF DATASET SIZE IS >4 BILLION BYTES THEN IT IS EQUAL TO 1MILL PGS*/ 00074000
*                                                                  0113 00075000
*       IF VBPODSSZ>DSBYTLIM THEN                                  0113 00076000
         CLC   VBPODSSZ(4,R1),DSBYTLIM                             0113 00077000
         BNH   @RF00113                                            0113 00078000
*         DSSIZE=DSPGLIM;           /* CONVERT DATASET SIZE INTO   0114 00079000
*                                      PAGES USING INPUT PARM LIST 0114 00080000
*                                                            @Z40WSSR*/ 00081000
         L     DSSIZE,@CF02059                                     0114 00082000
*/*  ELSE CALCULATE REQUIRED SIZE                                    */ 00083000
*                                                                  0115 00084000
*       ELSE                                                       0115 00085000
*         DO;                                                      0115 00086000
         B     @RC00113                                            0115 00087000
@RF00113 DS    0H                                                  0116 00088000
*           DSSIZE=(VBPODSSZ+FOUR095)/PAGESIZE;                    0116 00089000
         LA    DSSIZE,4095                                         0116 00090000
         AL    DSSIZE,VBPODSSZ(,R1)                                0116 00091000
         SRL   DSSIZE,12                                           0116 00092000
*           DSSIZE=DSSIZE+((DSSIZE+TEN22)/TEN23);/* DATA SET SIZE  0117 00093000
*                                      INCLUDES MAP PAGES ALSO       */ 00094000
         LA    @06,1022                                            0117 00095000
         ALR   @06,DSSIZE                                          0117 00096000
         LR    @09,@06                                             0117 00097000
         SLR   @08,@08                                             0117 00098000
         D     @08,@CF02051                                        0117 00099000
         ALR   DSSIZE,@09                                          0117 00100000
*         END;                                                     0118 00101000
*                                                                  0118 00102000
*       /*************************************************************/ 00103000
*       /*                                                           */ 00104000
*       /* FIND DSPCT HEADER SIZE BASED ON THE MAX. SIZE OF DATASET  */ 00105000
*       /*                                                           */ 00106000
*       /*************************************************************/ 00107000
*                                                                  0119 00108000
*       HDRSIZE=LENGTH(VBPHFIX)+(((DSSIZE+TEN23)/TEN24)*FOUR);/* FIND   00109000
*                                      SIZE OF DSPCT HEADER          */ 00110000
@RC00113 LA    @04,1023                                            0119 00111000
         ALR   @04,DSSIZE                                          0119 00112000
         SRL   @04,10                                              0119 00113000
         SLL   @04,2                                               0119 00114000
         AL    @04,@CF02112                                        0119 00115000
         LR    HDRSIZE,@04                                         0119 00116000
*       HDRSIZE=(((HDRSIZE+SEVEN)/EIGHT));/* ROUND IT TO A DOUBLE  0120 00117000
*                                      WORD                          */ 00118000
         LA    @08,7                                               0120 00119000
         ALR   @08,HDRSIZE                                         0120 00120000
         SRL   @08,3                                               0120 00121000
         LR    HDRSIZE,@08                                         0120 00122000
*       HDRSIZE=HDRSIZE*EIGHT;                                     0121 00123000
*                                                                  0121 00124000
         SLL   HDRSIZE,3                                           0121 00125000
*       /*************************************************************/ 00126000
*       /*                                                           */ 00127000
*       /* GETMAIN DSPCT HEADER                                      */ 00128000
*       /*                                                           */ 00129000
*       /*************************************************************/ 00130000
*                                                                  0122 00131000
*       RFY                                                        0122 00132000
*        (R4,                                                      0122 00133000
*         R7,                                                      0122 00134000
*         R10) RSTD;                                               0122 00135000
*       R10=R1;                     /* SAVE INPUT PARM LIST ADDR IN     00136000
*                                      REG 10                        */ 00137000
         LR    R10,R1                                              0123 00138000
*       RFY                                                        0124 00139000
*         IDAVOP1 BASED(R10);                                      0124 00140000
*       R4=JSCBACT->JSCBTCBP;       /* PASS INITIATOR TCB    @VS07802*/ 00141000
         L     @06,PSATOLD                                         0125 00142000
         L     @06,TCBJSCB(,@06)                                   0125 00143000
         L     @06,JSCBACT(,@06)                                   0125 00144000
         L     R4,JSCBTCBP(,@06)                                   0125 00145000
*       R7=ADDR(ASCB);              /* REG 7 HAS ASCB ADDR   @Z40WSSR*/ 00146000
         L     R7,PSAAOLD                                          0126 00147000
*       DO;                         /* GETMAIN (RU) LV(HDRSIZE)    0127 00148000
*                                      SP(JSCBACT->JSCBSWSP) KEY(1)     00149000
*                                      BRANCH                        */ 00150000
*         RESPECIFY                                                0128 00151000
*          (GPR01F,                                                0128 00152000
*           GPR03F,                                                0128 00153000
*           GPR00F) RESTRICTED;                                    0128 00154000
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */ 00155000
         SLR   GPR01F,GPR01F                                       0129 00156000
*         GPR03F=2;                 /* RU-TYPE GETMAIN               */ 00157000
         LA    GPR03F,2                                            0130 00158000
*         GPR00F=HDRSIZE;           /* LENGTH REQUESTED              */ 00159000
         LR    GPR00F,HDRSIZE                                      0131 00160000
*         GPR03F=GPR03F|((JSCBACT->JSCBSWSP)*256);/* SP IN BYTE 2    */ 00161000
         SLR   @08,@08                                             0132 00162000
         IC    @08,JSCBSWSP(,@06)                                  0132 00163000
         SLA   @08,8                                               0132 00164000
         OR    GPR03F,@08                                          0132 00165000
*         GPR03F=GPR03F|((1)*65536*16);/* KEY IN BYTE 1              */ 00166000
         O     GPR03F,@CF02113                                     0133 00167000
*         CALL CVTCRMN;             /* BRANCH ENTRY POINT            */ 00168000
         L     @06,FLCCVT                                          0134 00169000
         L     @15,CVTCRMN(,@06)                                   0134 00170000
         BALR  @14,@15                                             0134 00171000
*         RESPECIFY                                                0135 00172000
*          (GPR01F,                                                0135 00173000
*           GPR03F,                                                0135 00174000
*           GPR00F) UNRESTRICTED;                                  0135 00175000
*       END;                        /* GETMAIN (RU) LV(HDRSIZE)    0136 00176000
*                                      SP(JSCBACT->JSCBSWSP) KEY(1)     00177000
*                                      BRANCH                        */ 00178000
*       RFY                                                        0137 00179000
*        (R4,                                                      0137 00180000
*         R7) UNRSTD;                                              0137 00181000
*       RFY                                                        0138 00182000
*         VBPDSPCT BASED(R1);                                      0138 00183000
*                                                                  0138 00184000
*       /*************************************************************/ 00185000
*       /*                                                           */ 00186000
*       /* CLEAR DSPCT HEADER                                @Z40WSSR*/ 00187000
*       /*                                                           */ 00188000
*       /*************************************************************/ 00189000
*                                                                  0139 00190000
*       RFY                                                        0139 00191000
