*                                                                  0145 00215000
*       /*************************************************************/ 00216000
*       /*                                                           */ 00217000
*       /* SET PROPER LINKAGE TO CALLER VIA DSPCT HDR        @Z40WSSR*/ 00218000
*       /*                                                           */ 00219000
*       /*************************************************************/ 00220000
*                                                                  0146 00221000
*       VBPHFRAR=ADDR(RDWA);        /* STORE RECV. RTN PARM. ADDR IN    00222000
*                                      DSPCT HDR                     */ 00223000
         ST    R11,VBPHFRAR(,R1)                                   0146 00224000
*       RDWAVBPH=ADDR(VBPDSPCT);    /* SAVE DSPCT HDR ADDR IN RECV.     00225000
*                                      RTN. PARM                     */ 00226000
