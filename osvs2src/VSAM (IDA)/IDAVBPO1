*       /*************************************************************/ 00261000
*       /*                                                           */ 00262000
*       /* INITIALIZE DSPCT HEADER                                   */ 00263000
*       /*                                                           */ 00264000
*       /*************************************************************/ 00265000
*                                                                  0160 00266000
*       VBPHNAME=DSPCBLKN;          /* MOVE DSPCT BLOCK ACRONYM    0160 00267000
*                                                            @Z40WSSR*/ 00268000
         MVC   VBPHNAME(4,R11),DSPCBLKN                            0160 00269000
*       VBPHLEN=HDRSIZE;            /* SET HEADER SIZE               */ 00270000
