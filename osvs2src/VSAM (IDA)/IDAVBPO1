*   RDWA32=VOPENFLG;                /* SET IDAVBPO1 FLAG IN RTM COMM.   00039000
*                                      AREA                  @Z40WSSR*/ 00040000
         MVI   RDWA32(R11),X'01'                                   0107 00041000
*   RDWAVBPH=VBPOHPTR;              /* MOVE DSPCT HDR ADDR IN SDWA 0108 00042000
