*       /*************************************************************/ 00375000
*       /*                                                           */ 00376000
*       /* SAVE THE LOGICAL GROUP #(LGN) IN THE DSPCT HEADER         */ 00377000
*       /*                                                           */ 00378000
*       /*************************************************************/ 00379000
*                                                                  0193 00380000
*       VBPHLGN=ACALGN;             /* STORE LGN IN DSPCT HEADER     */ 00381000
         MVC   VBPHLGN(8,R11),ACALGN(R1)                           0193 00382000
*       RFY                                                        0194 00383000
*         R1 UNRSTD;                                               0194 00384000
*                                                                  0194 00385000
*       /*************************************************************/ 00386000
*       /*                                                           */ 00387000
*       /* DERIVE DSPAGEID GENERATOR FROM LGN, TO IDENTIFY THIS VIO  */ 00388000
*       /* DATASET TO RSM. IF LGN IS WITHIN LIMITS THEN DSPAGEID =   */ 00389000
*       /* LGN                                                       */ 00390000
*       /*                                                           */ 00391000
*       /*************************************************************/ 00392000
*                                                                  0195 00393000
*       IF VBPHLGID<RECLMLIM THEN                                  0195 00394000
         CLC   VBPHLGID(4,R11),@CF02061                            0195 00395000
         BNL   @RF00195                                            0195 00396000
*         VBPHDSG=VBPHLPG2;         /* SET DSPAGEID GENERATOR        */ 00397000
*                                                                  0196 00398000
         LH    @10,VBPHLPG2(,R11)                                  0196 00399000
         N     @10,@CF02117                                        0196 00400000
         STH   @10,VBPHDSG(,R11)                                   0196 00401000
*       /*************************************************************/ 00402000
*       /*                                                           */ 00403000
*       /* ELSE IF LGN IS BEYOND LIMITS THEN DSPAGEID = 0            */ 00404000
*       /*                                                           */ 00405000
*       /*************************************************************/ 00406000
*                                                                  0197 00407000
*       VBPHNMP=ONE;                /* FOR A NEW DATASET ACQUIRE ONE    00408000
*                                      MAP PAGE              @Z40WSSR*/ 00409000
@RF00195 MVC   VBPHNMP(2,R11),@CH00085                             0197 00410000
*       VBPHMMP=(DSSIZE+TEN23)/TEN24;/* SET MAX SIZE OF MAP FIELD IN    00411000
*                                      HDR                           */ 00412000
         LA    @00,1023                                            0198 00413000
         ALR   @00,DSSIZE                                          0198 00414000
         SRL   @00,10                                              0198 00415000
         STH   @00,VBPHMMP(,R11)                                   0198 00416000
*       RFY                                                        0199 00417000
*        (DSSIZE) UNRSTD;                                          0199 00418000
*       CALL GETDSPX;               /* BUILD DSPCT EXTENSION @Z40WSSR*/ 00419000
         BAL   @14,GETDSPX                                         0200 00420000
*     END;                                                         0201 00421000
*                                                                  0202 00422000
*   /*****************************************************************/ 00423000
*   /*                                                               */ 00424000
