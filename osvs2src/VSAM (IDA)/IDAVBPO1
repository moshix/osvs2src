         TITLE 'IDAVBPO1 VBP OPEN MODULE                               *00001000
                        '                                               00002000
IDAVBPO1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(24)                                             0001 00007000
         DC    C'IDAVBPO1 ZA31846 4/18/78'                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
JSCBDISP EQU 188                                                        00014000
JSCBAOS1 EQU  72                                                        00015000
JSCBAOS2 EQU 192                                                        00016000
*   R2=ADDR(IDAVBPR1);              /* LOAD RECOVERY ROUTINE ADDR IN    00017000
*                                      REG 1                         */ 00018000
         L     R2,@CV00073                                         0103 00019000
*   GEN CODE REFS(R2,PSACSTK,FRRSCURR,PSA,FRRS,FRRSENTR,PSALSFCC,  0104 00020000
*       FRRSELEN,FRRSLAST) SETS(FRRSCURR,FRRSFRRA,R11,FRRSPARM) NOEXIT  00021000
*       NOFLOWS;                                                   0104 00022000
*                                                                  0104 00023000
  SETFRR  A,FRRAD=(2),PARMAD=(11),WRKREGS=(11,4)                        00024000
*   /*****************************************************************/ 00025000
*   /*                                                               */ 00026000
*   /* FILL IN RELEVENT DATA IN PARM BLOCK FOR FUNC. RECV. RTN.      */ 00027000
*   /*                                                               */ 00028000
*   /*****************************************************************/ 00029000
*                                                                  0105 00030000
*   RFY                                                            0105 00031000
*     RDWA BASED(R11);                                             0105 00032000
*                                                                  0105 00033000
*/*  RTM COMM. AREA IS BASED ON REG 11                               */ 00034000
*                                                                  0106 00035000
*   RDWAINPM=ADDR(IDAVOP1);         /* SET INPUT PARM LIST ADDR IN 0106 00036000
*                                      RTM COMM. AREA                */ 00037000
