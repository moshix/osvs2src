         IC    @03,JSCBSWSP(,@10)                                  0167 00292000
         STC   @03,VBPHSPUL(,R11)                                  0167 00293000
*       VBPHTCB=JSCBACT->JSCBTCBP;  /* SAVE INITIATORS TCB ADDR FOR     00294000
*                                      LATER GETMAINING AND        0168 00295000
*                                      FREEMAINING THE DSPCT MAP   0168 00296000
*                                                            @VS07802*/ 00297000
*                                                                  0168 00298000
         L     @10,JSCBTCBP(,@10)                                  0168 00299000
         ST    @10,VBPHTCB(,R11)                                   0168 00300000
*       /*************************************************************/ 00301000
*       /*                                                           */ 00302000
*       /* ASSIGN LOGICAL GROUP                               #(LGN).*/ 00303000
*       /* THIS IDENTIFIES ALL THE PAGES BELONGING TO THIS DATASET   */ 00304000
*       /* THAT WILL BE WRITTEN FROM THE WINDOW INTO THE PAGING      */ 00305000
*       /* SPACE. DSPCT MAP IS ALSO WRITTEN OUT AND IS A PART OF THIS*/ 00306000
*       /* LOGICAL GROUP                                     @Z40WSSR*/ 00307000
*       /*                                                           */ 00308000
*       /*************************************************************/ 00309000
*                                                                  0169 00310000
*       RFY                                                        0169 00311000
*         R1 RSTD;                                                 0169 00312000
*       R1=ADDR(SECSAVE);           /*                       @Z40WSSR*/ 00313000
         LA    R1,SECSAVE(,R11)                                    0170 00314000
*       ACAASID=ASCBASID;           /* SET ASID IN PARM LIST         */ 00315000
         L     @10,PSAAOLD                                         0171 00316000
         LH    @10,ASCBASID(,@10)                                  0171 00317000
         N     @10,@CF02117                                        0171 00318000
         STH   @10,ACAASID(,R1)                                    0171 00319000
*       ACAMAXPN=DSSIZE;            /* SET DATASET SIZE IN PARM LIST */ 00320000
         ST    DSSIZE,ACAMAXPN(,R1)                                0172 00321000
*       ACAFLG1=HEX00;              /* SET ASSIGN LG OPTIONS IN PARM    00322000
*                                      LIST                          */ 00323000
         MVI   ACAFLG1(R1),B'00000000'                             0173 00324000
*       RFY                                                        0174 00325000
*         R1 UNRSTD;                                               0174 00326000
*       DO;                         /* ILRCALL                     0175 00327000
*                                      ACA(ADDR(ACA))OP(ASSIGN)      */ 00328000
*         RESPECIFY                                                0176 00329000
*           R1 RSTD;                                               0176 00330000
*         DECLARE                   /* ILRGOS ENTRY POINT    @Z40WPWG*/ 00331000
*           ILRGOS ENTRY BASED(ASMGOS) VALRG(0);                   0177 00332000
*         DECLARE                   /* ILRTRPAG ENTRY POINT  @Z40WPWG*/ 00333000
*           ILRTRPAG ENTRY BASED(ASMTRPAG) VALRG(0);               0178 00334000
*         DCL                                                      0179 00335000
*           ILRACAOP FIXED(8) BASED(R1);                           0179 00336000
*         R1=ADDR(ACA);             /* SET OPCODE            @Z40WPWG*/ 00337000
*         ILRACAOP=8;                                              0181 00338000
         MVI   ILRACAOP(R1),X'08'                                  0181 00339000
*         CALL ILRGOS;              /*                       @Z40WPWG*/ 00340000
         L     @10,FLCCVT                                          0182 00341000
         L     @10,CVTASMVT(,@10)                                  0182 00342000
         L     @15,ASMGOS(,@10)                                    0182 00343000
         BALR  @14,@15                                             0182 00344000
*         RESPECIFY                                                0183 00345000
*           R1 UNRSTD;                                             0183 00346000
*       END;                        /* ILRCALL                     0184 00347000
*                                      ACA(ADDR(ACA))OP(ASSIGN)      */ 00348000
*       RFY                                                        0185 00349000
*        (R1,                                                      0185 00350000
*         R15) RSTD;                                               0185 00351000
*       R1=ADDR(SECSAVE);           /* RESTORE REG 1         @Z40WSSR*/ 00352000
*                                                                  0186 00353000
         LA    R1,SECSAVE(,R11)                                    0186 00354000
*       /*************************************************************/ 00355000
*       /*                                                           */ 00356000
*       /* IF LOGICAL GROUP # CANNOT BE ASSIGNED BY ASM THEN ABEND   */ 00357000
*       /*                                                           */ 00358000
*       /*************************************************************/ 00359000
*                                                                  0187 00360000
*       IF R15>ZERO THEN                                           0187 00361000
         CL    R15,@CF00446                                        0187 00362000
         BNH   @RF00187                                            0187 00363000
*         DO;                                                      0188 00364000
*           R15=R15+ASMERROR;       /* SET REASON CODE FOR ASSIGN LG    00365000
*                                      ERROR                         */ 00366000
         AL    R15,@CF02047                                        0189 00367000
*           GO TO ABNRMLND;         /* ABEND                         */ 00368000
         B     ABNRMLND                                            0190 00369000
*         END;                                                     0191 00370000
*       RFY                                                        0192 00371000
*        (R15) UNRSTD;                                             0192 00372000
*                                                                  0192 00373000
