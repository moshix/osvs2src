*       RFY                                                        0162 00272000
*         HDRSIZE UNRSTD;           /*                       @Z40WSSR*/ 00273000
*       VBPHDSSZ=DSSIZE;            /* SET DATA SET SIZE             */ 00274000
         ST    DSSIZE,VBPHDSSZ(,R11)                               0163 00275000
*       VBPHWAD=VBPOWIA;            /* SET WINDOW ADDRESS            */ 00276000
