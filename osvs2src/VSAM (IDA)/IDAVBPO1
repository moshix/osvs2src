*       VBPHWSZ=VBPOLEN;            /* SET WINDOW SIZE       @Z40WSSR*/ 00280000
         L     @10,VBPOLEN(,@10)                                   0165 00281000
         STH   @10,VBPHWSZ(,R11)                                   0165 00282000
*       VBPHSTA=OPENFLG;            /* SET VBP OPEN IN PROGRESS FLAG    00283000
*                                                            @Z40WSSR*/ 00284000
         MVI   VBPHSTA(R11),B'10000000'                            0166 00285000
*       VBPHSPUL=JSCBACT->JSCBSWSP; /* SAVE SWA SUBPOOL FOR        0167 00286000
*                                      GETMAINING THE THE DSPCT MAP     00287000
*                                                            @VS07802*/ 00288000
         L     @10,PSATOLD                                         0167 00289000
         L     @10,TCBJSCB(,@10)                                   0167 00290000
