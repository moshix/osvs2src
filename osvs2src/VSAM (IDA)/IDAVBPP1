         LH    R8,VBPHDSG(,R11)                                    0138 00289000
         N     R8,@CF01271                                         0138 00290000
*           /*********************************************************/ 00291000
*           /*                                                       */ 00292000
*           /* IF THIS PAGE IS NOT RECLAIMABLE THEN DO NOT PASS      */ 00293000
*           /* DSPAGEID TO RSM                                       */ 00294000
*           /*                                                       */ 00295000
*           /*********************************************************/ 00296000
*                                                                  0139 00297000
*           IF R2>=RECLMLIM THEN                                   0139 00298000
         CL    R2,@CF01232                                         0139 00299000
         BL    @RF00139                                            0139 00300000
*             R8=ZERO;              /*                       @Z40WSSR*/ 00301000
*                                                                  0140 00302000
         SLR   R8,R8                                               0140 00303000
*           /*********************************************************/ 00304000
*           /*                                                       */ 00305000
*           /* BUILD VCBS FOR MOVE OUT NULL FOR ALL PAGES ASSIGNED ON*/ 00306000
*           /* PRIOR REQUEST                                         */ 00307000
*           /*                                                       */ 00308000
*           /*********************************************************/ 00309000
*                                                                  0141 00310000
*           DO R1=R1 TO R2;                                        0141 00311000
@RF00139 B     @DE00141                                            0141 00312000
@DL00141 DS    0H                                                  0142 00313000
*             VCBLINK=VCBNXT;       /* STORE ADDR OF 1ST VCB FOR MOVE   00314000
*                                      OUT NULL IN VCBSAVEAD FIELD OR   00315000
*                                      THE NEXT VCB ADDR IN CURRENT     00316000
*                                      VCB                           */ 00317000
         ST    VCBNXT,VCBLINK(,VCBPTR)                             0142 00318000
*             VCBPTR=VCBNXT;        /* MAP VCB DSECT ON VCB IN DSPCT    00319000
*                                      HDR                           */ 00320000
