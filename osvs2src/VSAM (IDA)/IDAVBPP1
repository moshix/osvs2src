         TITLE 'IDAVBPP1 VBP RECORD MANAGEMENT MODULE                  *00001000
                        '                                               00002000
IDAVBPP1 CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
        DS    0F                                                        00005000
        DC     V(IDAVBPO1)              ADDR OF IDAVBPO1                00006000
        DC     A(IDAVBPPR)              ADDR OF IDAVBPP1                00007000
        DC     V(IDAVBPC1)              ADDR OF IDAVBPC1                00008000
        DS     F                        NOT USED              @Z40WSSR  00009000
        DC     V(IDAVBPS1)              ADDR OF IDAVBPS1                00010000
        DC     AL1(8)                   LENGTH OF NAME                  00011000
        DC     C'IDAVBPP1'              MODULE NAME                     00012000
IDAVBPPR   STM  R14,R12,12(R13)          SAVE CALLERS REGS              00013000
        BALR   R12,R0                   SETUP BASE REG                  00014000
        DS     0H                                                       00015000
        USING   *,R12                                                   00016000
*   R11=BUFCDSPC;                   /* BASE DSPCT HEADER ON REG 11   */ 00017000
         L     R11,BUFCDSPC(,R1)                                   0082 00018000
*   RFY                                                            0083 00019000
*     VBPDSPCT BASED(R11);                                         0083 00020000
*   OSAVREG=ADDR(VBPHSAVE);         /* STORE MY SAVE AREA ADDR IN  0084 00021000
*                                      CALLERS SAVE AREA             */ 00022000
         LA    @10,VBPHSAVE(,R11)                                  0084 00023000
         ST    @10,OSAVREG(,R13)                                   0084 00024000
*   VBPHBUFC=R1;                    /* STORE BUFC ADDR IN DSPCT    0085 00025000
*                                      HEADER                        */ 00026000
         ST    R1,VBPHBUFC(,R11)                                   0085 00027000
*   VBPHSAVO=R13;                   /* STORE CALLERS SAVE AREA ADDR     00028000
*                                      IN MY SAVE AREA               */ 00029000
         ST    R13,VBPHSAVO(,R11)                                  0086 00030000
*   R13=ADDR(VBPHSAVE);             /* LOAD REG 13 WITH MY SAVE AREA    00031000
*                                      ADDR                          */ 00032000
         LA    R13,VBPHSAVE(,R11)                                  0087 00033000
*   VBPHSTA=RWFLG;                  /* SET TRACE FLAG IN DSPCT HDR TO   00034000
*                                      INDICATE IDAVBPP1 IN PROGRESS    00035000
*                                                            @Z40WSSR*/ 00036000
*                                                                  0088 00037000
         MVI   VBPHSTA(R11),B'01000000'                            0088 00038000
*   /*****************************************************************/ 00039000
*   /*                                                               */ 00040000
*   /* SET FUNCTIONAL RECOVERY(FRR) ENVIRONMENT                      */ 00041000
*   /*                                                               */ 00042000
*   /*****************************************************************/ 00043000
*                                                                  0089 00044000
*   RFY                                                            0089 00045000
*    (R2,                                                          0089 00046000
*     R3,                                                          0089 00047000
*     R4) RSTD;                                                    0089 00048000
*   R2=ADDR(IDAVBPR1);              /* LOAD RECOVERY ROUTINE ADDR IN    00049000
*                                      REG 2                         */ 00050000
         L     R2,@CV01151                                         0090 00051000
*   GEN CODE REFS(R2,PSACSTK,FRRSCURR,FRRS,PSA,PSALSFCC,FRRSELEN,  0091 00052000
*       FRRSENTR,FRRSLAST) SETS(R3,FRRSCURR,FRRSFRRA,FRRSPARM) NOEXIT   00053000
*       NOFLOWS;                                                   0091 00054000
  SETFRR  A,FRRAD=(2),PARMAD=(3),WRKREGS=(3,4)                          00055000
*   RFY                                                            0092 00056000
*     RDWA BASED(R3);               /* BASE RECV. RTN. COMM. AREA ON    00057000
*                                      REG.3                         */ 00058000
*   RDWAVBPH=ADDR(VBPDSPCT);        /* LOAD DSPCT HDR ADDR IN      0093 00059000
*                                      RECOVERY RTN COMM AREA        */ 00060000
         ST    R11,RDWAVBPH(,R3)                                   0093 00061000
*   VBPHFRAR=ADDR(RDWA);            /* SAVE RECV. RTN COMM. AREA ADDR   00062000
*                                      IN DSPCT HDR                  */ 00063000
         ST    R3,VBPHFRAR(,R11)                                   0094 00064000
*   RDWA2=ADDR(IDABUFC);            /* STORE INPUT PARMLIST ADDR IN     00065000
*                                      RECV. RTN COMM. AREA          */ 00066000
         ST    R1,RDWA2(,R3)                                       0095 00067000
*   RDWA32=RWIND;                   /* SET IDAVBPP1 IN PROGRESS    0096 00068000
*                                      INDICATOR IN FRR COMM. AREA   */ 00069000
         MVI   RDWA32(R3),X'02'                                    0096 00070000
*   RFY                                                            0097 00071000
*     RDWA BASED(VBPHFRAR);         /* BASE RECV. RTN. COMM. AREA ON    00072000
*                                      DSPCT HDR                     */ 00073000
*   RFY                                                            0098 00074000
*     IDABUFC BASED(VBPHBUFC);      /* BASE BUFC ON DSPCT HDR        */ 00075000
*   RFY                                                            0099 00076000
*    (R1,                                                          0099 00077000
*     R2,                                                          0099 00078000
*     R3,                                                          0099 00079000
*     R4) UNRSTD;                                                  0099 00080000
*   RFY                                                            0100 00081000
*    (VCBNXT,                                                      0100 00082000
*     VCBPTR) RSTD;                                                0100 00083000
*   VCBPTR=ADDR(VCBSAVAD);          /* MAP THE VCB DSECT ON VCBSAVE     00084000
*                                      ADDR FIELD IN DSPCT HDR TO  0101 00085000
*                                      STORE THE ADDR OF THE FIRST 0101 00086000
*                                      VCB TO BE PASSED TO RSM IN IT.*/ 00087000
         L     @10,VBPHVCB(,R11)                                   0101 00088000
         LA    VCBPTR,VCBSAVAD(,@10)                               0101 00089000
*                                                                  0102 00090000
*   /*****************************************************************/ 00091000
*   /*                                                               */ 00092000
*   /* IF WRITE REQUESTED THEN BUILD VCBS FOR MOVE OUT               */ 00093000
*   /*                                                               */ 00094000
*   /*****************************************************************/ 00095000
*                                                                  0102 00096000
*   IF BUFCMW=BITB THEN                                            0102 00097000
         L     @06,VBPHBUFC(,R11)                                  0102 00098000
         TM    BUFCMW(@06),B'10000000'                             0102 00099000
         BNO   @RF00102                                            0102 00100000
*     DO;                           /* BUILD VCB FOR MOVE OUT        */ 00101000
*       RFY                                                        0104 00102000
*        (R8,                                                      0104 00103000
*         R1,                                                      0104 00104000
*         R2) RSTD;                                                0104 00105000
*       VCBNXT=ADDR(VBPVMOVE);      /* VCB DSECT SHOULD POINT TO THE    00106000
*                                      FIRST VCB FOR MOVE OUT BUILT     00107000
*                                      IN IDAVBPR2           @Z40WSSR*/ 00108000
         LA    VCBNXT,VBPVMOVE(,@10)                               0105 00109000
*       R1=(BUFCWLEN-ONE)/PAGESIZE; /* GET # OF PAGES TO BE WRITTEN  */ 00110000
*                                                                  0106 00111000
         LH    @00,BUFCWLEN(,@06)                                  0106 00112000
         BCTR  @00,0                                               0106 00113000
         SRDA  @00,32                                              0106 00114000
         D     @00,@CF01104                                        0106 00115000
*       /*************************************************************/ 00116000
*       /*                                                           */ 00117000
*       /* IF # OF PAGES TO BE MOVED OUT AS REQUESTED BY IDDWITRM IS */ 00118000
*       /* LESS THAN # OF PAGES ASSIGNED IN THE PRIOR REQUEST THEN   */ 00119000
*       /* MOVE OUT ALL THE PAGES ASSIGNED IN THE PRIOR REQUEST      */ 00120000
*       /*                                                           */ 00121000
*       /*************************************************************/ 00122000
*                                                                  0107 00123000
*       IF R1<VBPHPNP THEN                                         0107 00124000
         LH    @10,VBPHPNP(,R11)                                   0107 00125000
         CLR   R1,@10                                              0107 00126000
         BNL   @RF00107                                            0107 00127000
*         R1=VBPHPNP;                                              0108 00128000
         LR    R1,@10                                              0108 00129000
*       R2=(BUFCORBA/PAGESIZE)+VBPHMMP;/* GET REL. PAGE OFFSET OF 1ST   00130000
*                                      PAGE IN THE WINDOW            */ 00131000
@RF00107 L     @10,VBPHBUFC(,R11)                                  0109 00132000
         L     R2,BUFCORBA(,@10)                                   0109 00133000
         SRL   R2,12                                               0109 00134000
         LH    @10,VBPHMMP(,R11)                                   0109 00135000
         ALR   R2,@10                                              0109 00136000
*       R1=R1+R2;                   /* R1 NOW HAS REL. PAGE OFFSET OF   00137000
*                                      LAST PAGE IN THE WINDOW TO BE    00138000
*                                      MOVED OUT                     */ 00139000
         ALR   R1,R2                                               0110 00140000
*       R8=VBPHDSG;                 /* LOAD REG 8 WITH DSPAGEID    0111 00141000
*                                      GENERATOR                     */ 00142000
*                                                                  0111 00143000
         LH    R8,VBPHDSG(,R11)                                    0111 00144000
         N     R8,@CF01271                                         0111 00145000
*       /*************************************************************/ 00146000
*       /*                                                           */ 00147000
*       /* PASS A ZERO DSPAGEID TO RSM AS RECLAIM SHOULD NOT BE DONE */ 00148000
*       /* IF LPID OFFSET IS > 64K                                   */ 00149000
*       /*                                                           */ 00150000
*       /*************************************************************/ 00151000
*                                                                  0112 00152000
*       IF R1>=RECLMLIM THEN                                       0112 00153000
         CL    R1,@CF01232                                         0112 00154000
         BL    @RF00112                                            0112 00155000
*         R8=ZERO;                  /*                       @Z40WSSR*/ 00156000
*                                                                  0113 00157000
         SLR   R8,R8                                               0113 00158000
*       /*************************************************************/ 00159000
*       /*                                                           */ 00160000
*       /* BUILD VCBS FOR THE # OF PAGES TO BE MOVED OUT FROM THE    */ 00161000
*       /* WINDOW                                                    */ 00162000
*       /*                                                           */ 00163000
*       /*************************************************************/ 00164000
*                                                                  0114 00165000
*       DO R2=R2 TO R1;                                            0114 00166000
@RF00112 B     @DE00114                                            0114 00167000
@DL00114 DS    0H                                                  0115 00168000
*         VCBLINK=VCBNXT;           /* STORE ADDR OF NEXT VCB OR THE    00169000
*                                      FIRST VCB FOR MOVE OUT        */ 00170000
         ST    VCBNXT,VCBLINK(,VCBPTR)                             0115 00171000
*         VCBPTR=VCBNXT;            /* MAP VCB DSECT ON VCB TO BE  0116 00172000
*                                      FILLED                        */ 00173000
         LR    VCBPTR,VCBNXT                                       0116 00174000
*         VCBNXT=VCBNXT+LENGTH(VCB);/* SAVE ADDR OF NEXT VCB TO BE 0117 00175000
*                                      FILLED                        */ 00176000
         AL    VCBNXT,@CF00127                                     0117 00177000
*         VCBLGN=VBPHLGID;          /* MOVE LGN              @Z40WSSR*/ 00178000
         L     @10,VBPHLGID(,R11)                                  0118 00179000
         ST    @10,VCBLGN(,VCBPTR)                                 0118 00180000
*         VCBLGOFF=R2;              /* SET LGID FIELD                */ 00181000
         ST    R2,VCBLGOFF(,VCBPTR)                                0119 00182000
*         VCBCPFLG=HEX00;           /* CLEAR COMPLETION FLAG @YM03910*/ 00183000
         MVI   VCBCPFLG(VCBPTR),B'00000000'                        0120 00184000
*/*  IF RECLAIM IS NOT TO BE ATTEMPTED THEN ZERO DSPAGEID            */ 00185000
*                                                                  0121 00186000
*         IF R8=ZERO THEN                                          0121 00187000
         SLR   @10,@10                                             0121 00188000
         CLR   R8,@10                                              0121 00189000
         BNE   @RF00121                                            0121 00190000
*           VCBDSID=ZERO;                                          0122 00191000
*                                                                  0122 00192000
         ST    @10,VCBDSID(,VCBPTR)                                0122 00193000
*         /***********************************************************/ 00194000
*         /*                                                         */ 00195000
*         /* ELSE PASS DSPAGEID                                      */ 00196000
*         /*                                                         */ 00197000
*         /***********************************************************/ 00198000
*                                                                  0123 00199000
*         ELSE                                                     0123 00200000
*           DO;                                                    0123 00201000
         B     @RC00121                                            0123 00202000
@RF00121 DS    0H                                                  0124 00203000
*             VCBDSN=R8;            /* PASS DSPAGEID GENERATOR       */ 00204000
         STH   R8,VCBDSN(,VCBPTR)                                  0124 00205000
*             VCBDSOFF=R2;          /* PASS LPID OFFSET              */ 00206000
         STH   R2,VCBDSOFF(,VCBPTR)                                0125 00207000
*           END;                                                   0126 00208000
*                                                                  0126 00209000
*         /***********************************************************/ 00210000
*         /*                                                         */ 00211000
*         /* IF THIS IS NOT THE LAST VCB TO BE SET THEN BUMP THE     */ 00212000
*         /* DSECT. IF NOT DEPENDING ON WHAT ELSE HAS TO BE DONE THE */ 00213000
*         /* VCBLINK FIELD IN THE CURRENT VCB HAS TO BE SET TO ZERO  */ 00214000
*         /* OR THE NEXT VCB TO BE SET FOR READ AS DETERMINED AND SET*/ 00215000
*         /* LATER ON                                                */ 00216000
*         /*                                                         */ 00217000
*         /***********************************************************/ 00218000
*                                                                  0127 00219000
*       END;                                                       0127 00220000
@RC00121 AL    R2,@CF00105                                         0127 00221000
@DE00114 CLR   R2,R1                                               0127 00222000
         BNH   @DL00114                                            0127 00223000
*       RFY                                                        0128 00224000
*        (R8,                                                      0128 00225000
*         R1,                                                      0128 00226000
*         R2) UNRSTD;                                              0128 00227000
*     END;                                                         0129 00228000
*                                                                  0130 00229000
*   /*****************************************************************/ 00230000
*   /*                                                               */ 00231000
*   /* ELSE IF READ NOT REQUESTED THEN BYPASS PROCESSING             */ 00232000
*   /*                                                               */ 00233000
*   /*****************************************************************/ 00234000
*                                                                  0130 00235000
*   ELSE                                                           0130 00236000
*     IF BUFCRRD=NULLB THEN                                        0130 00237000
         B     @RC00102                                            0130 00238000
@RF00102 L     @10,VBPHBUFC(,R11)                                  0130 00239000
         TM    BUFCRRD(@10),B'00100000'                            0130 00240000
         BZ    @RT00130                                            0130 00241000
*       GO TO FINIS;                /* IF NEITHER WRITE NOR READ   0131 00242000
*                                      REQUESTED THEN BYPASS       0131 00243000
*                                      PROCESSING                    */ 00244000
*                                                                  0132 00245000
*/*   ELSE IF READ REQUESTED NOW THEN                                */ 00246000
*                                                                  0132 00247000
*     ELSE                                                         0132 00248000
*                                                                  0132 00249000
*       /*************************************************************/ 00250000
*       /*                                                           */ 00251000
*       /* IF WRITE NOT REQUESTED NOW AND IF PRIOR REQUEST WAS A READ*/ 00252000
*       /* THEN BUILD VCBS FOR MOVE OUT NULL FOR THE PAGES ASSIGNED  */ 00253000
*       /* IN THE PRIOR REQUEST TO DISCONNECT THE WINDOW FROM VIRTUAL*/ 00254000
*       /* MEMORY AND TO SAVE THE REAL STORAGE# (RSN)S PASSED BY RSM */ 00255000
*       /* IN THE DSPCT MAP FOR RECLAIMING THESE PAGES FROM REAL     */ 00256000
*       /* STORAGE                                                   */ 00257000
*       /*                                                           */ 00258000
*       /*************************************************************/ 00259000
*                                                                  0132 00260000
*       IF VBPHPAS=BITB THEN                                       0132 00261000
         TM    VBPHPAS(R11),B'00010000'                            0132 00262000
         BNO   @RF00132                                            0132 00263000
*         DO;                                                      0133 00264000
*           RFY                                                    0134 00265000
*            (R8,                                                  0134 00266000
*             R2,                                                  0134 00267000
*             R1) RSTD;                                            0134 00268000
*           VCBNXT=ADDR(VBPVMOVE);  /* LOAD INTO VCBNXT ADDR OF 1ST     00269000
*                                      VCB FOR MOVE OUT NULL BUILT IN   00270000
*                                      IDAVBPR2              @Z40WSSR*/ 00271000
         L     VCBNXT,VBPHVCB(,R11)                                0135 00272000
