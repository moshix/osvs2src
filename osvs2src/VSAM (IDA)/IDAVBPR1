         TITLE 'IDAVBPR1 VBP FUNCTIONAL RECOVERY ROUTINE               *00001000
                        '                                               00002000
IDAVBPR1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(24)                                             0001 00008000
         DC    C'IDAVBPR1 ZA27891 4/28/78'                         0001 00009000
VBPS1FRR DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00147-VBPS1FRR)                                   00013000
         ENTRY VBPS1FRR                                                 00014000
FRRFRR   DS    0H                                                  0001 00015000
         USING *,@15                                               0001 00016000
         B     @PROLOG                                             0001 00017000
         DC    AL2(@EP00184-FRRFRR)                                     00018000
         ENTRY FRRFRR                                                   00019000
         DROP  @15                                                      00020000
@PROLOG  BALR  @12,0                                               0001 00021000
@PSTART  DS    0H                                                  0001 00022000
         USING @PSTART,@12                                         0001 00023000
         AH    @15,4(,@15)                                         0001 00024000
         BR    @15                                                 0001 00025000
@EP00001 DS    0H                                                  0002 00026000
*   R13=R0;                         /* SET R13 = 200 BYTE WORK AREA     00027000
*                                      FOR SDUMP             @VS07809*/ 00028000
         LR    R13,R0                                              0090 00029000
*   RFY                                                            0091 00030000
*     R0 UNRSTD;                    /*                       @VS07809*/ 00031000
*   R5=R14;                         /* SAVE RETURN ADDR              */ 00032000
         LR    R5,R14                                              0092 00033000
*   RFY                                                            0093 00034000
*     R14 UNRSTD;                                                  0093 00035000
*   RFY                                                            0094 00036000
*     SDWA BASED(R1);               /* BASE SDWA ON REG 1    @Z40WSSR*/ 00037000
*   RFY                                                            0095 00038000
*     VBPDSPCT BASED(RDWA1);        /* BASE DSPCT HEADER ON SDWA PARM   00039000
*                                      LIST                          */ 00040000
*   RFY                                                            0096 00041000
*     IDAVOP1 BASED(RDWA2);         /* BASE VOPEN PARM LIST ON SDWA     00042000
*                                      PARM LIST                     */ 00043000
*   RFY                                                            0097 00044000
*     IDABUFC BASED(RDWA2);         /* BASE VREAD/WRITE PARM LIST ON    00045000
*                                      SDWA PARM LIST                */ 00046000
*                                                                  0098 00047000
*   /*****************************************************************/ 00048000
*   /*                                                               */ 00049000
*   /* CONSTRUCT DUMP STORAGE LIST IN SDWA                   @Z40WSSR*/ 00050000
*   /*                                                               */ 00051000
*   /*****************************************************************/ 00052000
*                                                                  0098 00053000
*   SDWAFRM1=RDWA1;                 /* MOVE DSPCT HEADER ADDR        */ 00054000
         L     @04,SDWAPARM(,R1)                                   0098 00055000
         L     @03,RDWA1(,@04)                                     0098 00056000
         ST    @03,SDWAFRM1(,R1)                                   0098 00057000
*   SDWATO1=SDWAFRM1+LENGTH(VBPHFIX);/* MOVE ENDING ADDR OF DSPCT HDR   00058000
*                                      ZM40069                       */ 00059000
         AL    @03,@CF02315                                        0099 00060000
         ST    @03,SDWATO1(,R1)                                    0099 00061000
*   SDWAEBC=BITB;                   /* REQUEST EBCDIC PRINTOUT OF  0100 00062000
*                                      VARIABLE              @VS07089   00063000
*                                      LENGTH RECORDING AREA @VS07089*/ 00064000
*                                                                  0100 00065000
         OI    SDWAEBC(R1),B'01000000'                             0100 00066000
*   /*****************************************************************/ 00067000
*   /*                                                               */ 00068000
*   /* IF FAILURE WAS IN IDAVBPC1 THEN                               */ 00069000
*   /*                                                               */ 00070000
*   /*****************************************************************/ 00071000
*                                                                  0101 00072000
*   IF RDWA322='04'X THEN                                          0101 00073000
         CLI   RDWA322(@04),4                                      0101 00074000
         BNE   @RF00101                                            0101 00075000
*     DO;                                                          0102 00076000
*       R2=ADDR(LISTC1);            /* PASS ID TO SDUMP              */ 00077000
         LA    R2,LISTC1                                           0103 00078000
*       SDWATO1=SDWATO1|EOLMARK;    /* INDICATE END OF LIST TO SDUMP */ 00079000
         O     @03,EOLMARK                                         0104 00080000
         ST    @03,SDWATO1(,R1)                                    0104 00081000
*     END;                                                         0105 00082000
*                                                                  0105 00083000
*   /*****************************************************************/ 00084000
*   /*                                                               */ 00085000
*   /* IF FAILURE WAS IN IDAVBPO1 THEN                               */ 00086000
*   /*                                                               */ 00087000
*   /*****************************************************************/ 00088000
*                                                                  0106 00089000
*   ELSE                                                           0106 00090000
*     IF RDWA322='01'X THEN                                        0106 00091000
         B     @RC00101                                            0106 00092000
@RF00101 L     @04,SDWAPARM(,R1)                                   0106 00093000
         CLI   RDWA322(@04),1                                      0106 00094000
         BNE   @RF00106                                            0106 00095000
*       DO;                                                        0107 00096000
*         R2=ADDR(LISTO1);          /* PASS ID TO SDUMP              */ 00097000
         LA    R2,LISTO1                                           0108 00098000
*         SDWAFRM2=RDWA2;           /* PASS INPUT PARM LIST INTO   0109 00099000
*                                      IDAVBPO1 TO SDUMP             */ 00100000
         L     @04,RDWA2(,@04)                                     0109 00101000
         ST    @04,SDWAFRM2(,R1)                                   0109 00102000
*         SDWATO2=SDWAFRM2+LENGTH(IDAVOP1);/* PASS ENDING ADDR OF  0110 00103000
*                                      LIST                          */ 00104000
         AL    @04,@CF02316                                        0110 00105000
         ST    @04,SDWATO2(,R1)                                    0110 00106000
*         SDWATO2=SDWATO2|EOLMARK;  /* LAST ENTRY IN LIST            */ 00107000
         O     @04,EOLMARK                                         0111 00108000
         ST    @04,SDWATO2(,R1)                                    0111 00109000
*       END;                                                       0112 00110000
*                                                                  0112 00111000
*   /*****************************************************************/ 00112000
*   /*                                                               */ 00113000
*   /* ELSE FAILURE WAS IN IDAVBPP1                                  */ 00114000
*   /*                                                               */ 00115000
*   /*****************************************************************/ 00116000
*                                                                  0113 00117000
*     ELSE                                                         0113 00118000
*       DO;                                                        0113 00119000
         B     @RC00106                                            0113 00120000
@RF00106 DS    0H                                                  0114 00121000
*         SDWAFRM2=RDWA2;           /* PASS ADDR OF IDABUFC TO SDUMP */ 00122000
         L     @04,SDWAPARM(,R1)                                   0114 00123000
         L     @04,RDWA2(,@04)                                     0114 00124000
