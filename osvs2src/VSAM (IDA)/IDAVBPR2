*   R13=ADDR(SECSAVE);              /* LOAD REG 13 WITH MY SAVE ADDR */ 00037000
*                                                                  0063 00038000
         LR    R13,@10                                             0063 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* THIS ROUTINE GETSMAIN FOR MAP PAGES AND DOES ASSIGN OR        */ 00042000
