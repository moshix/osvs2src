IDAVBPR2 CSECT ,                                                   0001 00003000
CHEKMPPG DS    0H                                                  0001 00004000
         ENTRY CHEKMPPG                                                 00005000
@MAINENT DS    0H                                                  0001 00006000
         USING *,@15                                               0001 00007000
         B     @PROLOG                                             0001 00008000
         DC    AL2(@EP00001-@MAINENT)                                   00009000
         DC    AL1(16)                                             0001 00010000
         DC    C'IDAVBPR2  78.054'                                 0001 00011000
MONWINDO DS    0H                                                  0001 00012000
         USING *,@15                                               0001 00013000
         B     @PROLOG                                             0001 00014000
         DC    AL2(@EP00140-MONWINDO)                                   00015000
         ENTRY MONWINDO                                                 00016000
SETVCB   DS    0H                                                  0001 00017000
         USING *,@15                                               0001 00018000
         B     @PROLOG                                             0001 00019000
         DC    AL2(@EP00209-SETVCB)                                     00020000
         ENTRY SETVCB                                                   00021000
         DROP  @15                                                      00022000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00023000
         BALR  @12,0                                               0001 00024000
@PSTART  DS    0H                                                  0001 00025000
         USING @PSTART,@12                                         0001 00026000
         AH    @15,4(,@15)                                         0001 00027000
         BR    @15                                                 0001 00028000
@EP00001 DS    0H                                                  0002 00029000
*   SECSAVO=R13;                    /* STOR CALLERS SAVE AREA ADDR IN   00030000
*                                      MY SAVE AREA                  */ 00031000
         ST    R13,SECSAVO(,R11)                                   0061 00032000
*   SAVER13=ADDR(SECSAVE);          /* STORE MY SAVE AREA ADDR IN  0062 00033000
*                                      CALLERS SAVE AREA             */ 00034000
