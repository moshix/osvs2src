         LR    GPR05P,GPR11P                                       0198 00457000
*           GPR04P=GPR13P;          /* SAVE REG 13. IT WILL BE     0199 00458000
*                                      DESTROYED BY RELLOCK          */ 00459000
         LR    GPR04P,GPR13P                                       0199 00460000
*           GEN CODE REFS(PSALITA,FLC)SETS(GPR13P)SEQFLOW;         0200 00461000
  SETLOCK RELEASE,TYPE=LOCAL,RELATED=(LGCB,IDAVBPS2)                    00462000
*           GPR11P=GPR05P;          /* RESTORE RMPL ADDR             */ 00463000
         LR    GPR11P,GPR05P                                       0201 00464000
