*                                      AREA                          */ 00798000
         L     @15,SRB00(,CREGD)                                   0468 00799000
         ST    @15,SRB14(,CREGD)                                   0468 00800000
*         RFY                                                      0469 00801000
*           CREGF RSTD;             /*                       @Z40GKD1*/ 00802000
*         CREGF=RPLRTNCD;           /* SET RETURN CODE IN RETURN CODE   00803000
*                                      REG                           */ 00804000
         SLR   CREGF,CREGF                                         0470 00805000
         IC    CREGF,RPLRTNCD(,CRPLPTR)                            0470 00806000
*         IF CREGF^=0               /* GOOD RETURN CODE              */ 00807000
*           THEN                                                   0471 00808000
         LTR   CREGF,CREGF                                         0471 00809000
         BNZ   @RT00471                                            0471 00810000
*           GOTO CODECHK6;          /* NO SEE ABOUT ERROR ROUTINE    */ 00811000
*         ELSE                                                     0473 00812000
*           DO;                                                    0473 00813000
*             CREG0=RPLFDB2;        /* PUT REASON CODE IN REG 0      */ 00814000
*                                                                  0474 00815000
         SLR   CREG0,CREG0                                         0474 00816000
         IC    CREG0,RPLFDB2(,CRPLPTR)                             0474 00817000
*             /*******************************************************/ 00818000
*             /*                                                     */ 00819000
*             /* RESTORE REGISTERS                                   */ 00820000
*             /*                                                     */ 00821000
*             /*******************************************************/ 00822000
*                                                                  0475 00823000
*             LM(CREG2,CREGC,LM2TOC);                              0475 00824000
         LM    CREG2,CREGC,LM2TOC+28(CREGD)                        0475 00825000
*             CREGE=CREGD->SRB14;                                  0476 00826000
         L     CREGE,SRB14(,CREGD)                                 0476 00827000
*             GOTO CREGE->USER;     /*                       @Z40GKD1*/ 00828000
         BR    CREGE                                               0477 00829000
*           END;                                                   0478 00830000
*       END;                                                       0479 00831000
*     ELSE                          /* NO, NOT UNDER SRB AND NOT BRCH   00832000
*                                      ENTERED, SO USE THE SVC PATH  */ 00833000
*                                                                  0480 00834000
*       /*************************************************************/ 00835000
*       /*                                                           */ 00836000
*       /* PATH FOR TCB USERS WITHOUT BRANCH =YES. THEREFORE, THE    */ 00837000
*       /* TPIO SVC WILL BE ISSUED TO GET TO PSS                     */ 00838000
*       /*                                                           */ 00839000
*       /*************************************************************/ 00840000
*                                                                  0480 00841000
*       DO;                                                        0480 00842000
@RF00416 DS    0H                                                  0481 00843000
*         RFY                                                      0481 00844000
*          (CREGE) UNRSTD;          /*                       @Z40GKD1*/ 00845000
*         RFY                                                      0482 00846000
*           TPIOTYPE RSTD;                                         0482 00847000
*         IF RPLACTIV=CBYTEON       /* RPL ALREADY ACTIVE WITH     0483 00848000
*                                      REQUEST                       */ 00849000
*           THEN                                                   0483 00850000
         CLI   RPLACTIV(CRPLPTR),X'FF'                             0483 00851000
         BNE   @RF00483                                            0483 00852000
*           DO;                     /* YES, ERROR                    */ 00853000
*             CREG0=LGERRCD2;       /* LOGIC ERROR , RPL NOT TOUCHED */ 00854000
         LA    CREG0,24                                            0485 00855000
*             GOTO LOGICERR;                                       0486 00856000
         B     LOGICERR                                            0486 00857000
*           END;                                                   0487 00858000
*         ELSE                                                     0488 00859000
*           DO;                     /* NOT ACTIVE, GOOD THUS FAR.  0488 00860000
*                                      CONTINUE                      */ 00861000
@RF00483 DS    0H                                                  0489 00862000
*             IF CREG5=CBYTEON      /* SYN REQ               @ZA19513*/ 00863000
*                 &RPLNEXIT=CBITOFF /* AND USER EXIT SPECIFIED       */ 00864000
*               THEN                                               0489 00865000
         C     CREG5,@CF02810                                      0489 00866000
         BNE   @RF00489                                            0489 00867000
         TM    RPLNEXIT(CRPLPTR),B'01000000'                       0489 00868000
         BNZ   @RF00489                                            0489 00869000
*               DO;                                                0490 00870000
*                                                                  0490 00871000
*                 /***************************************************/ 00872000
*                 /*                                                 */ 00873000
*                 /* SET UP RPL PROPERLY BECAUSE THE MACRO DOES NOT  */ 00874000
*                 /*                                         @ZM31162*/ 00875000
*                 /*                                                 */ 00876000
*                 /***************************************************/ 00877000
*                                                                  0491 00878000
*                 RPLNEXIT=CBITON;  /* NO EXIT TO BE TAKEN   @ZM31162*/ 00879000
*                 RPLEXIT=CBITOFF;  /* NO EXIT AVAILABLE     @ZM31162*/ 00880000
         OI    RPLNEXIT(CRPLPTR),B'01000000'                       0492 00881000
         NI    RPLEXIT(CRPLPTR),B'11011111'                        0492 00882000
*                 RPLECB=CZERO4;    /* USE INTERNAL ECB      @ZM31162*/ 00883000
         SLR   @14,@14                                             0493 00884000
         ST    @14,RPLECB(,CRPLPTR)                                0493 00885000
*                 RPLECBIN=CBITOFF; /*                       @ZM31162*/ 00886000
         NI    RPLECBIN(CRPLPTR),B'11111110'                       0494 00887000
*               END;                                               0495 00888000
*             ELSE                                                 0496 00889000
*               DO;                                                0496 00890000
         B     @RC00489                                            0496 00891000
@RF00489 DS    0H                                                  0497 00892000
*                 IF RPLECB=CZERO4 THEN/* IF ECB OR EXIT ADDR IS ZERO*/ 00893000
         L     @14,RPLECB(,CRPLPTR)                                0497 00894000
         LTR   @14,@14                                             0497 00895000
         BNZ   @RF00497                                            0497 00896000
*                   IF RPLEXIT=CBITON THEN                         0498 00897000
         TM    RPLEXIT(CRPLPTR),B'00100000'                        0498 00898000
         BNO   @RF00498                                            0498 00899000
*                     DO;           /* AND EXIT OR                   */ 00900000
*                       RPLFDB2='02'X;                             0500 00901000
         MVI   RPLFDB2(CRPLPTR),X'02'                              0500 00902000
*                       GOTO CALLLERR;                             0501 00903000
         B     CALLLERR                                            0501 00904000
*                     END;                                         0502 00905000
*                   ELSE                                           0503 00906000
*                     IF RPLECBIN=CBITON/* EXT ECB SPECIFIED         */ 00907000
*                       THEN                                       0503 00908000
@RF00498 TM    RPLECBIN(CRPLPTR),B'00000001'                       0503 00909000
         BNO   @RF00503                                            0503 00910000
*                       DO;                                        0504 00911000
*                         RPLFDB2='03'X;/* THESE ARE ERRORS          */ 00912000
         MVI   RPLFDB2(CRPLPTR),X'03'                              0505 00913000
*CALLLERR:                                                         0506 00914000
*                         RPLRTNCD=LGERRCD1;                       0506 00915000
CALLLERR MVI   RPLRTNCD(CRPLPTR),X'14'                             0506 00916000
*                         CREG0=LGERRCD1;                          0507 00917000
         LA    CREG0,20                                            0507 00918000
*                         GOTO LOGICERR;                           0508 00919000
         B     LOGICERR                                            0508 00920000
*                       END;                                       0509 00921000
*                     ELSE                                         0510 00922000
*                       ;                                          0510 00923000
@RF00503 DS    0H                                                  0511 00924000
*               END;                /* END OF ERROR CHECKING @ZM31162*/ 00925000
@RF00497 DS    0H                                                  0512 00926000
*             DO;                                                  0512 00927000
@RC00489 DS    0H                                                  0513 00928000
*               RPLACTIV=CBYTEON;   /* RPL IS ACTIVE WITH A REQUEST  */ 00929000
         MVI   RPLACTIV(CRPLPTR),X'FF'                             0513 00930000
*               RPLREQ=CREQPTR;     /* PUT REQUEST CODE IN RPL       */ 00931000
         STC   CREQPTR,RPLREQ(,CRPLPTR)                            0514 00932000
*               RPLFDBK=0;          /* SET FEEDBACK AREAS TO ZERO    */ 00933000
         SLR   @14,@14                                             0515 00934000
         STCM  @14,7,RPLFDBK(CRPLPTR)                              0515 00935000
*               RPLFDBK2=0;                                        0516 00936000
         ST    @14,RPLFDBK2(,CRPLPTR)                              0516 00937000
*               IF RPLNEXIT=CBITON THEN/* IF NO EXIT THEN ECB      0517 00938000
*                                                            @Z40GKD1*/ 00939000
         TM    RPLNEXIT(CRPLPTR),B'01000000'                       0517 00940000
         BNO   @RF00517                                            0517 00941000
*                 DO;               /*                       @Z40GKD1*/ 00942000
*                   IF RPLECBIN=CBITON THEN/* IF EXTERNAL ECB      0519 00943000
*                                                            @Z40GKD1*/ 00944000
         TM    RPLECBIN(CRPLPTR),B'00000001'                       0519 00945000
         BNO   @RF00519                                            0519 00946000
*                     ECBPOST=CBITOFF;/* CLEAR POST BIT      @Z40GKD1*/ 00947000
         L     @14,RPLECB(,CRPLPTR)                                0520 00948000
         NI    ECBPOST(@14),B'10111111'                            0520 00949000
*                 END;              /*                       @Z40GKD1*/ 00950000
*               ELSE                                               0522 00951000
*                 ;                 /* ELSE NO ECB           @Z40GKD1*/ 00952000
@RF00517 DS    0H                                                  0523 00953000
*               GOTO BCHLAB2;       /* SET UP PROPER TPIO OPTION OR     00954000
*                                      PROCESS ERROR                 */ 00955000
@RC00517 LA    @14,BCHTAB2                                         0523 00956000
         ALR   @14,TPIOTYPE                                        0523 00957000
         L     @14,BCHPTR2(,@14)                                   0523 00958000
         BR    @14                                                 0523 00959000
*             END;                                                 0524 00960000
*           END;                                                   0525 00961000
*SVCERR:                                                           0526 00962000
*         RPLRTNCD=LGERRCD1;        /* INVALID REQUEST               */ 00963000
SVCERR   MVI   RPLRTNCD(CRPLPTR),X'14'                             0526 00964000
*         CREG0=LGERRCD1;                                          0527 00965000
         LA    CREG0,20                                            0527 00966000
*         RPLACTIV=CBYTEOFF;        /* FINISHED WITH RPL             */ 00967000
         MVI   RPLACTIV(CRPLPTR),X'00'                             0528 00968000
*         GOTO LOGICERR;                                           0529 00969000
         B     LOGICERR                                            0529 00970000
*SVCOPEN:                                                          0530 00971000
*         CREQPTR=OPENOPT;          /* SET UP OPEN OPTION FOR SVC    */ 00972000
SVCOPEN  L     CREQPTR,@CF02224                                    0530 00973000
*         GOTO SETDEBAD;            /* GO SET UP PTR TO DEB          */ 00974000
         B     SETDEBAD                                            0531 00975000
*SVCSPANY:                                                         0532 00976000
*         IF RPLNODE=CBITON THEN    /* ANY REQUEST                   */ 00977000
SVCSPANY TM    RPLNODE(CRPLPTR),B'00000010'                        0532 00978000
         BNO   @RF00532                                            0532 00979000
*           CREQPTR=ANYOPT;         /* SET UP ANY TPIO OPTION FOR SVC*/ 00980000
         L     CREQPTR,@CF02214                                    0533 00981000
*         ELSE                                                     0534 00982000
*SVCSP:                                                            0534 00983000
*           DO;                                                    0534 00984000
         B     @RC00532                                            0534 00985000
@RF00532 DS    0H                                                  0534 00986000
SVCSP    DS    0H                                                  0535 00987000
*             IF CREQPTR='25'X THEN /* SESSIONC REQUEST              */ 00988000
         C     CREQPTR,@CF02951                                    0535 00989000
         BNE   @RF00535                                            0535 00990000
*               CREQPTR=SCOPT;      /* YES, SETUP SESSIONC TPIO    0536 00991000
*                                      OPTION                        */ 00992000
         L     CREQPTR,@CF02218                                    0536 00993000
*             ELSE                  /* MUST BE SPECIFIC REQUEST      */ 00994000
*               CREQPTR=SPOPT;      /* SET UP SPECIFIC OPTION        */ 00995000
         B     @RC00535                                            0537 00996000
@RF00535 SLR   CREQPTR,CREQPTR                                     0537 00997000
*           END;                                                   0538 00998000
@RC00535 DS    0H                                                  0539 00999000
*SETDEBAD:                                                         0539 01000000
*         CREQPTR=CREQPTR|RPLDACB->ACBDEB;/* PUT DEB ADDR IN @ZA19513*/ 01001000
@RC00532 DS    0H                                                  0539 01002000
SETDEBAD L     @14,RPLDACB(,CRPLPTR)                               0539 01003000
         L     @14,ACBDEB-1(,@14)                                  0539 01004000
         LA    @14,0(,@14)                                         0539 01005000
         OR    CREQPTR,@14                                         0539 01006000
*         CRPLSAV=CREG1;            /* SAVE REG 1 ACROSS SVC @ZA19930*/ 01007000
         LR    CRPLSAV,CREG1                                       0540 01008000
*         SVC(SVCTPIO);             /* ISSUE TPIO SVC                */ 01009000
         SVC   124                                                 0541 01010000
*         CREG1=CRPLSAV;            /* RESTORE REG 1 AFTER SVC     0542 01011000
*                                                            @ZA19930*/ 01012000
         LR    CREG1,CRPLSAV                                       0542 01013000
*         RFY                                                      0543 01014000
*           TPIOTYPE UNRSTD;                                       0543 01015000
*CODECHK4:                                                         0544 01016000
*         CREG0=0;                  /* ERROR EXIT NOT CALLED YET     */ 01017000
CODECHK4 SLR   @14,@14                                             0544 01018000
         LR    CREG0,@14                                           0544 01019000
*         IF CREGF^=0               /* ERROR RETURN CODE             */ 01020000
*           THEN                                                   0545 01021000
         CR    CREGF,@14                                           0545 01022000
         BE    @RF00545                                            0545 01023000
*           DO;                     /* YES                           */ 01024000
*             RPLACTIV=CBYTEOFF;    /* FINISHED WITH RPL             */ 01025000
         MVI   RPLACTIV(CRPLPTR),X'00'                             0547 01026000
*             IF CREGF=4 THEN                                      0548 01027000
         C     CREGF,@CF00048                                      0548 01028000
         BNE   @RF00548                                            0548 01029000
*               DO;                                                0549 01030000
*                 CREG0=LGERRCD2;   /* SET LOGIC ERROR BUT NOT IN RPL*/ 01031000
         LA    CREG0,24                                            0550 01032000
*                 GOTO LOGICERR;                                   0551 01033000
         B     LOGICERR                                            0551 01034000
*               END;                                               0552 01035000
*             ELSE                                                 0553 01036000
*               IF CREGF='20'X THEN                                0553 01037000
@RF00548 C     CREGF,@CF02965                                      0553 01038000
         BNE   @RF00553                                            0553 01039000
*                 DO;                                              0554 01040000
*                   RPLRTNCD=SNERRCD4;/* VTAM INACTIVE               */ 01041000
         MVI   RPLRTNCD(CRPLPTR),X'10'                             0555 01042000
*                   RPLFDB2='0D'X;                                 0556 01043000
         MVI   RPLFDB2(CRPLPTR),X'0D'                              0556 01044000
*                 END;                                             0557 01045000
*               ELSE                                               0558 01046000
*                 IF CREGF='3C'X THEN                              0558 01047000
         B     @RC00553                                            0558 01048000
@RF00553 C     CREGF,@CF02966                                      0558 01049000
         BNE   @RF00558                                            0558 01050000
*                   DO;             /* ABENDING                      */ 01051000
*                     RPLRTNCD=SNERRCD4;                           0560 01052000
         MVI   RPLRTNCD(CRPLPTR),X'10'                             0560 01053000
*                     RPLFDB2='0E'X;                               0561 01054000
         MVI   RPLFDB2(CRPLPTR),X'0E'                              0561 01055000
*                   END;                                           0562 01056000
*                 ELSE                                             0563 01057000
*                   DO;                                            0563 01058000
         B     @RC00558                                            0563 01059000
@RF00558 DS    0H                                                  0564 01060000
*                     RPLRTNCD=SNERRCD2;                           0564 01061000
         MVI   RPLRTNCD(CRPLPTR),X'08'                             0564 01062000
*                     RPLFDB2=0;                                   0565 01063000
         MVI   RPLFDB2(CRPLPTR),X'00'                              0565 01064000
*                   END;                                           0566 01065000
*             GOTO CODECHK6;        /* SEE ABOUT ERROR EXIT          */ 01066000
         B     CODECHK6                                            0567 01067000
*           END;                                                   0568 01068000
*         ELSE                                                     0569 01069000
*           DO;                     /* GOOD RETURN CODE              */ 01070000
@RF00545 DS    0H                                                  0570 01071000
*             RFY                                                  0570 01072000
*               CREGE RSTD;         /*                       @Z40GKD1*/ 01073000
*             IF CREG5=0            /* ASYN REQUEST IND. SAVED ACROSS   01074000
*                                      SVC                   @ZM41348*/ 01075000
*               THEN                                               0571 01076000
         LTR   CREG5,CREG5                                         0571 01077000
         BNZ   @RF00571                                            0571 01078000
*               DO;                 /* YES, RETURN TO CALLER         */ 01079000
*                 LM(CREG2,CREGC,LM2TOC);                          0573 01080000
         LM    CREG2,CREGC,LM2TOC+28(CREGD)                        0573 01081000
*                 CREGE=CREGD->SRB14;                              0574 01082000
