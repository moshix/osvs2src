         TITLE 'ISTAICIR -- VS2   API INTERFACE ROUTINE                *00001000
                        '                                               00002000
ISTAICIR CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTAICIR  78.173'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                          /*@Z40GKD1*/ 00014000
     ENTRY ISTAICBE                                                     00015000
*                                                                  0210 00016000
*/*  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */ 00017000
*/*                                                                  */ 00018000
*/*    THE USER API MACRO GENERATED REQUEST CODE IS CHECKED TO       */ 00019000
*/*    DETERMINE THAT IT IS A VALID EXPECTED VTAM REQUEST            */ 00020000
*/*    THEN  EITHER THE API CHECK ROUTINE IS CALLED                  */ 00021000
*/*     OR RPL PROCESSING IS CONTINUED FOR THE OTHER VALID REQUESTS  */ 00022000
*/*  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */ 00023000
*/*                                                                  */ 00024000
*                                                                  0211 00025000
*   IF RPLID^=RPLCBCDE|             /* TEST RPL FOR VALID ID @ZM40559*/ 00026000
*       RPLLEN2^=LENGTH(ISTRPL)     /* AND LENGTH            @ZM40559*/ 00027000
*     THEN                                                         0211 00028000
         CLC   RPLID(1,CRPLPTR),RPLCBCDE                           0211 00029000
         BNE   @RT00211                                            0211 00030000
         CLI   RPLLEN2(CRPLPTR),112                                0211 00031000
         BE    @RF00211                                            0211 00032000
@RT00211 DS    0H                                                  0212 00033000
*     DO;                           /* IF INVALID            @ZM40559*/ 00034000
*       CREG0=LGERRCD2;             /* INVALID RPL           @ZM40559*/ 00035000
         LA    CREG0,24                                            0213 00036000
*       GO TO LOGICERR;             /* GO TO LERAD EXIT      @ZM40559*/ 00037000
         B     LOGICERR                                            0214 00038000
*     END;                          /*                       @ZM40559*/ 00039000
*   ELSE                                                           0216 00040000
*     ;                             /* ELSE CONTINUE PROC.   @ZM40559*/ 00041000
@RF00211 DS    0H                                                  0217 00042000
*   IF CREQPTR=XECVALUE THEN        /* EXECRPL REQ                   */ 00043000
         C     CREQPTR,@CF02796                                    0217 00044000
         BNE   @RF00217                                            0217 00045000
*     CREQPTR=RPLREQ;               /* GET REQ CODE FROM RPL         */ 00046000
         SLR   CREQPTR,CREQPTR                                     0218 00047000
         IC    CREQPTR,RPLREQ(,CRPLPTR)                            0218 00048000
*   ELSE                                                           0219 00049000
*     IF CHKVALUE=CREQPTR           /* IS IT A CHECK REQ             */ 00050000
*       THEN                                                       0219 00051000
         B     @RC00217                                            0219 00052000
@RF00217 C     CREQPTR,@CF00112                                    0219 00053000
         BNE   @RF00219                                            0219 00054000
*       DO;                         /* YES                           */ 00055000
*         RFY                                                      0221 00056000
*           CREGE UNRSTD;                                          0221 00057000
*         IF RPLACTIV^=CBYTEON      /* IS RPL ACTIVE WITH A REQUEST  */ 00058000
*           THEN                                                   0222 00059000
         CLI   RPLACTIV(CRPLPTR),X'FF'                             0222 00060000
         BE    @RF00222                                            0222 00061000
*           RPLFDB2='04'X;          /* NO, THIS IS AN ERROR          */ 00062000
         MVI   RPLFDB2(CRPLPTR),X'04'                              0223 00063000
*         ELSE                                                     0224 00064000
*           DO;                     /* YES                           */ 00065000
         B     @RC00222                                            0224 00066000
@RF00222 DS    0H                                                  0225 00067000
*             IF PSATOLD=ZEROPTR    /* ARE WE UNDER A SRB?   @ZM32718*/ 00068000
*                 &RPLNEXIT=CBITON  /* AND NO EXIT IS SPECIFIED      */ 00069000
*               THEN                                               0225 00070000
         L     @09,PSATOLD                                         0225 00071000
         LTR   @09,@09                                             0225 00072000
         BNZ   @RF00225                                            0225 00073000
         TM    RPLNEXIT(CRPLPTR),B'01000000'                       0225 00074000
         BNO   @RF00225                                            0225 00075000
*               RPLFDB2='74'X;      /* PROTOCOL ERROR, CAN'T WAIT  0226 00076000
*                                      UNDER SRB             @ZA10403*/ 00077000
         MVI   RPLFDB2(CRPLPTR),X'74'                              0226 00078000
*             ELSE                  /* OTHERWISE OKAY TO CHECK THE 0227 00079000
*                                      REQUEST                       */ 00080000
*               CALL ISTAICCK;      /* CHECK MODULE WILL RETURN TO 0227 00081000
*                                      CALLER                        */ 00082000
         B     @RC00225                                            0227 00083000
@RF00225 L     @15,@CV00034                                        0227 00084000
         BALR  @14,@15                                             0227 00085000
*           END;                                                   0228 00086000
@RC00225 DS    0H                                                  0229 00087000
*         DO;                       /* INVALID CHECK REQUEST         */ 00088000
@RC00222 DS    0H                                                  0230 00089000
*           RPLRTNCD=LGERRCD1;      /* SET ERROR CODE                */ 00090000
         MVI   RPLRTNCD(CRPLPTR),X'14'                             0230 00091000
*           CREG0=LGERRCD1;                                        0231 00092000
         LA    CREG0,20                                            0231 00093000
