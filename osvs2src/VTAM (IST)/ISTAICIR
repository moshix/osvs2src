*     /*                                                             */ 00166000
*     /***************************************************************/ 00167000
*                                                                  0249 00168000
*     DO;                           /*                       @Z40GKD1*/ 00169000
*                                                                  0249 00170000
*       /*************************************************************/ 00171000
*       /*                                                           */ 00172000
*       /* SCHEDULE AN SRB TO RUN REQUEST. IF SUCCESSFUL, RETURN IS  */ 00173000
*       /* DIRECT TO USER OR VIA CHECK. IF ERROR, RETURNS TO NSI.    */ 00174000
*       /*                                                   @Z40GKD1*/ 00175000
*       /*                                                           */ 00176000
*       /*************************************************************/ 00177000
*                                                                  0250 00178000
*       RFY                                                        0250 00179000
*        (TPIOTYPE,                                                0250 00180000
*         CREGE) RSTD;              /*                       @Z40GKD1*/ 00181000
*       IF ATCLCPTR=0               /* IF NO ATCVT THEN      @ZA30266*/ 00182000
*         THEN                      /* EXIT WITH ERROR       @ZA30266*/ 00183000
         L     @15,ATCLCPTR                                        0251 00184000
         LTR   @15,@15                                             0251 00185000
