*                                                            @Z40GKD1*/ 01093000
@RF00571 L     @15,@CV00034                                        0578 01094000
         BALR  @14,@15                                             0578 01095000
*           END;                                                   0579 01096000
*       END;                                                       0580 01097000
*                                                                  0580 01098000
*   /*****************************************************************/ 01099000
*   /*                                                               */ 01100000
*   /* THIS ROUTINE IS ENTERED HERE TO DIRECT THE PATH TO CALL THE   */ 01101000
*   /* USER ERROR EXIT BASED ON THE RPL RETURN CODE                  */ 01102000
*   /*                                                               */ 01103000
*   /*****************************************************************/ 01104000
*                                                                  0581 01105000
*CODECHK6:                                                         0581 01106000
*   CREG0=RPLRTNCD;                 /* GET RPL RETURN CODE           */ 01107000
CODECHK6 SLR   CREG0,CREG0                                         0581 01108000
         IC    CREG0,RPLRTNCD(,CRPLPTR)                            0581 01109000
*   IF CREG0>'10'X                  /* IS VALUE LOGIC ERROR TYPE     */ 01110000
*     THEN                                                         0582 01111000
         C     CREG0,@CF02801                                      0582 01112000
         BNH   @RF00582                                            0582 01113000
*     DO;                           /* YES GO TO LOGIC ERROR ROUTINE */ 01114000
*LOGICERR:                                                         0584 01115000
*                                                                  0584 01116000
*       /*************************************************************/ 01117000
*       /*                                                           */ 01118000
*       /* ON ENTRY REG 0 CONTAINS THE RPLRTNCD OR X'18'. THIS       */ 01119000
*       /* ROUTINE WILL INVOKE THE USER LOGIC ERROR EXIT IF PRESENT. */ 01120000
*       /* REG 0 CONTAINS THE FEEDBACK CODE AND REG 15 A FOUR        */ 01121000
*       /*                                                           */ 01122000
*       /*************************************************************/ 01123000
*                                                                  0584 01124000
*       RFY                                                        0584 01125000
*         CACBPTR RSTD;                                            0584 01126000
LOGICERR DS    0H                                                  0585 01127000
*       RFY                                                        0585 01128000
*         CEXLTPTR RSTD;                                           0585 01129000
*       CACBPTR=RPLDACB;                                           0586 01130000
         L     CACBPTR,RPLDACB(,CRPLPTR)                           0586 01131000
*       CEXLTPTR=ACBUEL;                                           0587 01132000
         L     @14,ACBUEL(,CACBPTR)                                0587 01133000
         LR    CEXLTPTR,@14                                        0587 01134000
*       IF ACBUEL=CZERO4|EXLLEN2<20|EXLLERA=CBITOFF|EXLLERP=CZERO4 THEN 01135000
         SLR   @06,@06                                             0588 01136000
         CR    @14,@06                                             0588 01137000
         BE    @RT00588                                            0588 01138000
         LH    @14,EXLLEN2(,CEXLTPTR)                              0588 01139000
         C     @14,@CF02590                                        0588 01140000
         BL    @RT00588                                            0588 01141000
         TM    EXLLERA(CEXLTPTR),B'01000000'                       0588 01142000
         BZ    @RT00588                                            0588 01143000
         ICM   @14,15,EXLLERP(CEXLTPTR)                            0588 01144000
         CR    @06,@14                                             0588 01145000
         BNE   @RF00588                                            0588 01146000
@RT00588 DS    0H                                                  0589 01147000
*         CREGF='04'X;              /* SET ERROR RETURN CODE         */ 01148000
         LA    CREGF,4                                             0589 01149000
*       ELSE                                                       0590 01150000
*         DO;                                                      0590 01151000
*                                                                  0590 01152000
         B     @RC00588                                            0590 01153000
@RF00588 DS    0H                                                  0591 01154000
*/*  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */ 01155000
*/*   IF A USER SUPPLIED LERAD ROUTINE IS AVAILABLE THEN             */ 01156000
*/*       THE USER LERAD ROUTINE IS ENTERED                          */ 01157000
*/*  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */ 01158000
*                                                                  0591 01159000
*           CREGF=EXLLERP;          /* SET THE EXIT ADDRESS          */ 01160000
         ICM   CREGF,15,EXLLERP(CEXLTPTR)                          0591 01161000
*           GENERATE REFS(CREGD,CREGF) SETS(CREG2,CREG3,CREG4,CREG5,    01162000
*               CREG6,CREG7,CREG8,CREG9,CREGA,CREGB,CREGC,CREGE);  0592 01163000
              LM   CREG2,CREGC,C28(CREGD)                               01164000
              BALR  CREGE,CREGF                                         01165000
*         END;                                                     0593 01166000
*                                                                  0593 01167000
*/*  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */ 01168000
*/*    IF THERE IS NO USER SUPPLIED LERAD ROUTINE OR IF THE USER     */ 01169000
*/*    SUPPLIED LERAD ROUTINE DECIDED TO RETURN HERE WITH THE SAVE   */ 01170000
*/*    REGISTER INTACT, THEN CONTROL IS                              */ 01171000
*/*    RETURNED  TO THE USER MAINLINE PROGRAM AT THE NSI FOLLOWING   */ 01172000
*/*    THE API REQUEST                                               */ 01173000
*/*  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - */ 01174000
*                                                                  0594 01175000
*       GENERATE REFS(CREGD) SETS(CREG1,CREG2,CREG3,CREG4,CREG5,CREG6,  01176000
*           CREG7,CREG8,CREG9,CREGA,CREGB,CREGC,CREGE);            0594 01177000
@RC00588 DS    0H                                                  0594 01178000
              LM   CREG1,CREGC,C24(CREGD)                               01179000
              L    CREGE,C12(CREGD)                                     01180000
*       RFY                                                        0595 01181000
*         CREGE RSTD;                                              0595 01182000
*       GOTO CREGE->USER;           /*                       @Z40GKD1*/ 01183000
         BR    CREGE                                               0596 01184000
*       RFY                                                        0597 01185000
*         CACBPTR UNRSTD;                                          0597 01186000
*       RFY                                                        0598 01187000
*         CEXLTPTR UNRSTD;                                         0598 01188000
*     END;                          /* END OF LERAD LOGIC            */ 01189000
*   ELSE                                                           0600 01190000
*     DO;                           /* MUST BE SYNAD ERROR           */ 01191000
@RF00582 DS    0H                                                  0601 01192000
*SYNADERR:                                                         0601 01193000
*                                                                  0601 01194000
*       /*************************************************************/ 01195000
*       /*                                                           */ 01196000
*       /* REG 0 CONTAINS RPLRTNCD ON ENTRY                          */ 01197000
*       /*                                                           */ 01198000
*       /*************************************************************/ 01199000
*                                                                  0601 01200000
*       RFY                                                        0601 01201000
*         CACBPTR RSTD;                                            0601 01202000
SYNADERR DS    0H                                                  0602 01203000
*       RFY                                                        0602 01204000
*         CEXLTPTR RSTD;                                           0602 01205000
*       RFY                                                        0603 01206000
