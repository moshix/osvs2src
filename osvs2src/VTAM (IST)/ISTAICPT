*                                      MPST                  @Z30PPGF*/ 00978000
         LR    NEWCRPL,COPYRPL                                     0617 00979000
*NQCRPLLO:                                                         0618 00980000
*               CRPRRPLP=OLDCRPL;   /* SET UP LINK FIELD     @Z30PPGF*/ 00981000
NQCRPLLO ST    OLDCRPL,CRPRRPLP(,COPYRPL)                          0618 00982000
*               NEWCNT=OLDRCNT+'00010001'X;/* ADD ONE TO EACH CNT  0619 00983000
*                                                            @Z30PPGF*/ 00984000
         LR    NEWCNT,OLDRCNT                                      0619 00985000
         AL    NEWCNT,@CF03513                                     0619 00986000
*               CDS(OLDCRPL,NEWCRPL,ATCCRPL);/* PUT CRPL ON ATCVT  0620 00987000
*                                      DCR4214                       */ 00988000
         CDS   OLDCRPL,NEWCRPL,ATCCRPL(ATCVTPTR)                   0620 00989000
*               BC(7,NQCRPLLO);     /* IF FAILURE TRY AGAIN  @Z30PPGF*/ 00990000
         BC    7,NQCRPLLO                                          0621 00991000
*               CRETCODE=CRETCODE-CRETCODE;/* ZERO REG 15    @Z30PPGF*/ 00992000
         SLR   CRETCODE,CRETCODE                                   0622 00993000
*               RFY                                                0623 00994000
*                (ATCVTPTR,                                        0623 00995000
*                 OLDCRPL,                                         0623 00996000
*                 NEWCRPL,                                         0623 00997000
*                 OLDRCNT,                                         0623 00998000
*                 NEWCNT) UNRSTD;   /*                       @Z30PPGF*/ 00999000
*             END;                                                 0624 01000000
*           IF CRETCODE^=0 THEN                                    0625 01001000
@RC00589 LTR   CRETCODE,CRETCODE                                   0625 01002000
         BZ    @RF00625                                            0625 01003000
*             CREG8=8;              /* INDICATE VTAM ERROR           */ 01004000
         LA    CREG8,8                                             0626 01005000
*           ELSE                                                   0627 01006000
*             ;                                                    0627 01007000
@RF00625 DS    0H                                                  0628 01008000
*           IF CREG8>0 THEN                                        0628 01009000
