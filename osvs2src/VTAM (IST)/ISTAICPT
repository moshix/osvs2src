*             FMZL0010 BASED(GPR15P) LABEL VALUERANGE(*);          0366 00226000
*           CALL FMZL0010;                                         0367 00227000
         BALR  @14,@15                                             0367 00228000
*           RFY                                                    0368 00229000
*             GPR13P RSTD;                                         0368 00230000
*           GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);     0369 00231000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              00232000
*           RFY                                                    0370 00233000
*            (GPR01P,                                              0370 00234000
*             GPR13P) UNRSTD;                                      0370 00235000
*           RFY                                                    0371 00236000
*            (GPR02P,                                              0371 00237000
*             GPR03P,                                              0371 00238000
*             GPR04P,                                              0371 00239000
*             GPR15P) UNRSTD;                                      0371 00240000
*         END;                      /* *********END OF RELSTORE    0372 00241000
*                                      EXPANSION*********            */ 00242000
*       ELSE                                                       0373 00243000
*         DO;                       /* LESS THAN LIMIT, PUT CRPL ON     00244000
*                                      MPST QUEUE            @Z30PPGF*/ 00245000
         B     @RC00320                                            0373 00246000
@RF00320 DS    0H                                                  0374 00247000
*           COPYRPL=CRPLPTR;        /* PUT RPL ADDR IN COPYRPL     0374 00248000
*                                                            @ZA17887*/ 00249000
         LR    COPYRPL,CRPLPTR                                     0374 00250000
*/*SMSID  BUFF(COPYRPL)TSKID(0)                                      */ 00251000
*                                                                  0375 00252000
*           DO;                                                    0375 00253000
*             DCL                                                  0376 00254000
*               1 CMZ0026A PTR(31) BASED((COPYRPL)-8),             0376 00255000
*                2 * BIT(8),                                       0376 00256000
*                2 CMZ0026 PTR(24);                                0376 00257000
*             CMZ0026=(0);                                         0377 00258000
         LR    @11,COPYRPL                                         0377 00259000
         AL    @11,@CF03619                                        0377 00260000
         SLR   @08,@08                                             0377 00261000
         STCM  @08,7,CMZ0026-1(@11)                                0377 00262000
*           END;                    /* SMSID DISSOCIATE CRPL FROM  0378 00263000
*                                      USER TASK             @Z40GKD1*/ 00264000
*           OLDRCNT=ATCRPLCN;       /* GET CURRENT COUNT FIELD DCR 0379 00265000
*                                      4214                          */ 00266000
         L     OLDRCNT,ATCRPLCN(,ATCVTPTR)                         0379 00267000
