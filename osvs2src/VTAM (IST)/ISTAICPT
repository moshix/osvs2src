*   CREGSAV=CRETURN;                /* SAVE RETURN ADDRESS           */ 01940000
         LR    CREGSAV,CRETURN                                     0912 01941000
*                                                                  0913 01942000
*/*VALCHEK  (CRETCODE,(CSAVE),(CRPLPTR),UA,(CLENPTR))                */ 01943000
*                                                                  0913 01944000
*   DO;                                                            0913 01945000
*     RFY                                                          0914 01946000
*      (GPR01P,                                                    0914 01947000
*       GPR02P,                                                    0914 01948000
*       GPR04P,                                                    0914 01949000
*       GPR06P,                                                    0914 01950000
*       GPR13P,                                                    0914 01951000
*       GPR14P,                                                    0914 01952000
*       GPR15P) RSTD;               /* DECLARE AND RESTRICT REGS 1,     01953000
*                                      2, 4, 6, 13,14, AND 15        */ 01954000
*     CRETCODE=0;                   /* SET RETURN CODE TO NO ERROR 0915 01955000
*                                      INVOKE XREGSET MACRO TO VERIFY   01956000
*                                      SAVEAREA OPERAND AND PUT IT IN   01957000
*                                      REG 13                        */ 01958000
         SLR   CRETCODE,CRETCODE                                   0915 01959000
*/*XREGSET  PID('SAVEAREA')PPARM((CSAVE))RREG(13,GPR13P)             */ 01960000
*                                                                  0916 01961000
*     DO;                                                          0916 01962000
*       GPR13P=CSAVE;               /* SET RREG SYMBOL TO VALUE OF 0917 01963000
*                                      PPARM OPERAND                 */ 01964000
*     END;                          /* XREGSET                       */ 01965000
*     DCL                                                          0919 01966000
*       1 SAV0190 CHAR(20) BASED(GPR13P),/* ESTABLISH SAVE AREA MAP  */ 01967000
*        2 SAV10190 CHAR(4),        /* TO SAVE REGS 1,2,4,6, AND 14  */ 01968000
*        2 SAV20190 CHAR(4),                                       0919 01969000
*        2 SAV40190 CHAR(4),                                       0919 01970000
