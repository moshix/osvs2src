         LR    @11,COPYRPL                                         0613 00965000
         AL    @11,@CF03619                                        0613 00966000
         SLR   @02,@02                                             0613 00967000
         STCM  @02,7,CMZ0117-1(@11)                                0613 00968000
*               END;                /* SMSID DISASSOCIATE CRPL FROM     00969000
*                                      USER TASK             @Z40GKD1*/ 00970000
*               OLDRCNT=ATCRPLCN;   /* GET CURRENT COUNT FIELD DCR 0615 00971000
