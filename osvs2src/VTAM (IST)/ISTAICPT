         L     GPR03F,FMZA0010                                     0360 00215000
*           DO;                     /* LISATCVT                    0361 00216000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00217000
*             GPR15P=ATCLCPTR;      /* SET UP ATCVT POINTER          */ 00218000
         L     GPR15P,ATCLCPTR                                     0362 00219000
*           END;                    /* LISATCVT                      */ 00220000
*           GPR02P=GPR15P->ATCBPDA;                                0364 00221000
         L     GPR02P,ATCBPDA(,GPR15P)                             0364 00222000
*           GPR15P=GPR15P->ATCSMRS;                                0365 00223000
         L     GPR15P,ATCSMRS(,GPR15P)                             0365 00224000
