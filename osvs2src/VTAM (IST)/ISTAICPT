*           RFY                                                    0355 00201000
*            (GPR01P,                                              0355 00202000
*             GPR13P) RSTD;                                        0355 00203000
*           GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);     0356 00204000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00205000
*           RFY                                                    0357 00206000
*             GPR13P UNRSTD;                                       0357 00207000
*           GPR04P=CRPLPTR;                                        0358 00208000
         LR    GPR04P,CRPLPTR                                      0358 00209000
*           DCL                                                    0359 00210000
*             1 FMZA0010 FIXED(31) STATIC LOCAL,                   0359 00211000
*              2 * FIXED(8) INIT(64),                              0359 00212000
*              2 * PTR(24) INIT(0);                                0359 00213000
