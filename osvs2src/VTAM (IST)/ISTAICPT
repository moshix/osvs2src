*                                      DCR4214                       */ 00269000
         L     OLDCRPL,ATCCRPLQ(,ATCVTPTR)                         0380 00270000
*           NEWCRPL=COPYRPL;        /* SET UP ADDR OF NEW CRPL FOR 0381 00271000
*                                      MPST                  @Z30PPGF*/ 00272000
         LR    NEWCRPL,COPYRPL                                     0381 00273000
*NQCRPLL1:                                                         0382 00274000
*           CRPRRPLP=OLDCRPL;       /* SET UP LINK FIELD     @Z30PPGF*/ 00275000
NQCRPLL1 ST    OLDCRPL,CRPRRPLP(,COPYRPL)                          0382 00276000
