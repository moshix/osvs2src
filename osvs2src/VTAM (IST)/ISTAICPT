*                 COMPREG UNRSTD;                                  0562 00789000
*               RFY                                                0563 00790000
*                 REASNREG UNRSTD;                                 0563 00791000
*             END;                                                 0564 00792000
*           ELSE                                                   0565 00793000
*             ;                                                    0565 00794000
@RF00555 DS    0H                                                  0566 00795000
*           GEN(EJECT);                                            0566 00796000
         EJECT                                                          00797000
*                                                                  0567 00798000
*/*                                                                  */ 00799000
*/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  */ 00800000
*/*           CPTRPL -    RPL PROCESSING                             */ 00801000
*/*                                                                  */ 00802000
*/*   THERE ARE TWO TYPES OF RPLS TO CONSIDER  IN OS/VS SYSTEMS      */ 00803000
*/*     THE COPIED RPL    POINTER CONTAINED IN THE RPHWEA            */ 00804000
*/*     THE USER RPL    POINTER CONTAINED IN THE COPIED RPL          */ 00805000
*/*                                                                  */ 00806000
*/*   A VALIDITY CHECK IS MADE OF THE USER RPL ADDRESS TO CHECK      */ 00807000
*/*     IF IT IS IN THE USER REGION                                  */ 00808000
*/*    AS INPUT TO THE CPTVALCK SUBROUTINE CLENPTR MUST POINT TO     */ 00809000
*/*     A FOUR BYTE AREA CONTAINING THE LENGTH OF THE RPL TO BE      */ 00810000
*/*     VALIDITY CHECKED                                             */ 00811000
*/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  */ 00812000
*/*                                                                  */ 00813000
*/* CPTRPL                                 BEGIN CPTPROC PROCESSING  */ 00814000
*                                                                  0567 00815000
*           RFY                                                    0567 00816000
*             COPYRPL RESTRICTED;                                  0567 00817000
*           RFY                                                    0568 00818000
*             CREG8 RESTRICTED;                                    0568 00819000
*           CREG8=0;                /* CLEAR SWITCH INDCIATOR        */ 00820000
         SLR   CREG8,CREG8                                         0569 00821000
*           COPYRPL=RPHWEA;         /* SET UP COPIED RPL PTR         */ 00822000
         L     COPYRPL,RPHWEA(,RPHPTR)                             0570 00823000
*           CRPLPTR=CRPRRPLP;       /* GET ADDRESS OF USER RPL       */ 00824000
         L     CRPLPTR,CRPRRPLP(,COPYRPL)                          0571 00825000
*           RPHSAVE1=LENGTH(IFGRPL);/* GET LENGTH OF USERS RPL     0572 00826000
*                                                            @ZM33507*/ 00827000
         MVC   RPHSAVE1(4,RPHPTR),@CF03618                         0572 00828000
*           CLENPTR=ADDR(RPHSAVE1); /* POINT TO THE LENGTH OF THE RPL   00829000
*                                                             @DM8383*/ 00830000
         LA    CLENPTR,RPHSAVE1(,RPHPTR)                           0573 00831000
*           CSAVE=ADDR(RPHSAVE2);   /* SET UP SAVE AREA PTR          */ 00832000
         LA    CSAVE,RPHSAVE2(,RPHPTR)                             0574 00833000
*           CALL CPTVALCK;          /* VALIDTY CHECK THE USER RPL    */ 00834000
         BAL   @14,CPTVALCK                                        0575 00835000
*           CSAVE=ADDR(RPHWORK);    /* REESTABLISH 16 WORD SAVE AREA    00836000
*                                      POINTER                       */ 00837000
         LA    CSAVE,RPHWORK(,RPHPTR)                              0576 00838000
*           IF CRETCODE^=0 THEN     /* CHECK RETURN CODE FROM VALIDTY   00839000
*                                      CHECK                         */ 00840000
         LTR   CRETCODE,CRETCODE                                   0577 00841000
         BZ    @RF00577                                            0577 00842000
*             CREG8=4;              /* INDICATE USER ERROR           */ 00843000
         LA    CREG8,4                                             0578 00844000
*           ELSE                                                   0579 00845000
*             DO;                   /* VALCHEK WAS GOOD              */ 00846000
         B     @RC00577                                            0579 00847000
@RF00577 DS    0H                                                  0580 00848000
*/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  */ 00849000
*/*   IF THE VALIDTY CHECK OF THE USER RPL IS GOOD THEN OVERLAY      */ 00850000
*/*   THE USER RPL WITH THE COPIED RPL STARTING WITH THE FOURTH      */ 00851000
*/*   WORD OF EACH RPL                                               */ 00852000
*/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  */ 00853000
*/*                                                                  */ 00854000
*                                                                  0580 00855000
*               CIFGRPL(13:RPLLEN)=(COPYRPL+C12)->CISTCRPL;/* BYPASS    00856000
*                                      THE FIRST 12 BYTES AND OVERLAY   00857000
*                                      THE REMAINDER OF THE USER RPL    00858000
*                                      WITH THE COPIED RPL   @Z30PPGF*/ 00859000
         SLR   @14,@14                                             0580 00860000
         IC    @14,RPLLEN(,CRPLPTR)                                0580 00861000
         SL    @14,@CF00365                                        0580 00862000
         EX    @14,@SM03620                                        0580 00863000
*             END;                                                 0581 00864000
*                                                                  0581 00865000
*/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  */ 00866000
*/*    ONCE THE USER RPL HAS BEEN OVERLAYED BY THE COPIED RPL        */ 00867000
*/*    OR THE VALCHEK OF THE USER RPL FAILED                         */ 00868000
*/*    RELEASE THE COPIED RPL STORAGE                                */ 00869000
*/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  */ 00870000
*/*                                                                  */ 00871000
*                                                                  0582 00872000
*           RFY                                                    0582 00873000
*            (ATCVTPTR,                                            0582 00874000
*             OLDCRPL,                                             0582 00875000
*             NEWCRPL,                                             0582 00876000
*             OLDRCNT,                                             0582 00877000
*             NEWCNT) RSTD;         /*                       @ZM32701*/ 00878000
@RC00577 DS    0H                                                  0583 00879000
*           DO;                     /* LISATCVT                    0583 00880000
*                                      CVTBASE(PAGEABLE,(ATCVTPTR))  */ 00881000
*             ATCVTPTR=ATCLCPTR;    /* SET UP ATCVT POINTER          */ 00882000
         L     ATCVTPTR,ATCLCPTR                                   0584 00883000
*           END;                    /* LISATCVT                      */ 00884000
*           RFY                                                    0586 00885000
*             ISTATCVT BASED(ATCVTPTR);/*                    @Z30PPGF*/ 00886000
*           MPSTPTR=RPHTSKID->PSTMPSTP;/* GET ADDRESS OF MPST      0587 00887000
*                                                            @Z30PPGF*/ 00888000
         L     @11,RPHTSKID(,RPHPTR)                               0587 00889000
         L     MPSTPTR,PSTMPSTP(,@11)                              0587 00890000
*           RFY                                                    0588 00891000
*             ISTMPST BASED(MPSTPTR);/*                      @Z30PPGF*/ 00892000
*           IF ATCRPLAC>=ATCRPLCT THEN/* IF LIMIT OF CRPL ON ATCVT 0589 00893000
*                                      REACHED RELSTORE THE CRPL DCR    00894000
*                                      4214                          */ 00895000
         CLC   ATCRPLAC(2,ATCVTPTR),ATCRPLCT(ATCVTPTR)             0589 00896000
         BL    @RF00589                                            0589 00897000
*/* RELSTORE BUFADDR(COPYRPL)SAVE(YES)CLEAR(NO)                      */ 00898000
*                                                                  0590 00899000
*             DO;                                                  0590 00900000
*                                                                  0590 00901000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00902000
*/* END OF ATBUILD                                                   */ 00903000
*                                                                  0591 00904000
*               RFY                                                0591 00905000
*                (GPR02P,                                          0591 00906000
*                 GPR03P,                                          0591 00907000
*                 GPR04P,                                          0591 00908000
*                 GPR15P) RSTD;                                    0591 00909000
*               RFY                                                0592 00910000
*                (GPR01P,                                          0592 00911000
*                 GPR13P) RSTD;                                    0592 00912000
*               GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK); 0593 00913000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00914000
*               RFY                                                0594 00915000
*                 GPR13P UNRSTD;                                   0594 00916000
*               GPR04P=COPYRPL;                                    0595 00917000
         LR    GPR04P,COPYRPL                                      0595 00918000
*               DCL                                                0596 00919000
*                 1 FMZA0101 FIXED(31) STATIC LOCAL,               0596 00920000
*                  2 * FIXED(8) INIT(64),                          0596 00921000
*                  2 * PTR(24) INIT(0);                            0596 00922000
*               GPR03F=FMZA0101;                                   0597 00923000
         L     GPR03F,FMZA0101                                     0597 00924000
*               DO;                 /* LISATCVT                    0598 00925000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00926000
*                 GPR15P=ATCLCPTR;  /* SET UP ATCVT POINTER          */ 00927000
         L     GPR15P,ATCLCPTR                                     0599 00928000
*               END;                /* LISATCVT                      */ 00929000
*               GPR02P=GPR15P->ATCBPDA;                            0601 00930000
         L     GPR02P,ATCBPDA(,GPR15P)                             0601 00931000
*               GPR15P=GPR15P->ATCSMRS;                            0602 00932000
         L     GPR15P,ATCSMRS(,GPR15P)                             0602 00933000
*               DCL                                                0603 00934000
*                 FMZL0101 BASED(GPR15P) LABEL VALUERANGE(*);      0603 00935000
*               CALL FMZL0101;                                     0604 00936000
         BALR  @14,@15                                             0604 00937000
*               RFY                                                0605 00938000
*                 GPR13P RSTD;                                     0605 00939000
*               GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P); 0606 00940000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              00941000
*               RFY                                                0607 00942000
*                (GPR01P,                                          0607 00943000
*                 GPR13P) UNRSTD;                                  0607 00944000
*               RFY                                                0608 00945000
*                (GPR02P,                                          0608 00946000
*                 GPR03P,                                          0608 00947000
*                 GPR04P,                                          0608 00948000
*                 GPR15P) UNRSTD;                                  0608 00949000
*             END;                  /* *********END OF RELSTORE    0609 00950000
*                                      EXPANSION*********            */ 00951000
*           ELSE                                                   0610 00952000
*             DO;                   /* LESS THAN LIMIT, PUT CRPL ON     00953000
*                                      MPST QUEUE            @Z30PPGF*/ 00954000
         B     @RC00589                                            0610 00955000
@RF00589 DS    0H                                                  0611 00956000
*/*SMSID  BUFF(COPYRPL)TSKID(0)                                      */ 00957000
*                                                                  0611 00958000
*               DO;                                                0611 00959000
*                 DCL                                              0612 00960000
*                   1 CMZ0117A PTR(31) BASED((COPYRPL)-8),         0612 00961000
