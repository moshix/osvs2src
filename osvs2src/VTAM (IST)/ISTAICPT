         TITLE 'ISTAICPT -- VS2 API USER POST ROUTINE                  *00001000
                        '                                               00002000
ISTAICPT CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTAICPT  78.158'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
*                                                                  0295 00011000
          USING ISTAICPT,CRETCODE RE ESTABLISH BASE REG 15    @Z30APGF  00012000
* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - * 00013000
*                          START OF CODE                              * 00014000
*                                                                     * 00015000
*  THIS MODULE CAN BE ENTERED WITH REGISTER 13 EITHER POINTING        * 00016000
*   TO A SAVE AREA OR EQUAL TO ZERO                                   * 00017000
*  CONSEQUENTLY REGISTER 15 IS USED AS THE BASE REGISTER UNTIL        * 00018000
*   THE CONTENTS OF REGISTER 13 CAN BE EVALUATED                      * 00019000
*  WHEN WE LEAVE THIS GENERATED CODE REGISTER 7  WILL BE EITHER       * 00020000
*   ZERO OR WILL CONTAIN THE RETURN ADDRESS OF THE CALLER             * 00021000
*                                                                     * 00022000
*  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - ** 00023000
*                                                                     * 00024000
CPTBEG1   EQU   *                                                       00025000
          LTR   CSAVE,CSAVE     IS THERE A SAVE AREA ADDRESS IN REG 13  00026000
          BNZ   CPTBEG2         BRANCH IF ADDRESS IS PRESENT            00027000
          LR    CREGSV,CRETURN           SAVE RETURN ADDRESS IN REG7    00028000
          B     CPTBEG3                                                 00029000
CPTBEG2   EQU   *                                                       00030000
          STM   CRETURN,CBASE,12(CSAVE)        SAVE CALLERS REGISTERS   00031000
          SR    CREGSV,CREGSV                  CLEAR REG 7              00032000
CPTBEG3   EQU   *                                                       00033000
          BALR  CBASE,0            ESTABLISH THE BASE REG FOR ISTAICPT  00034000
          DROP  CRETCODE                                                00035000
          USING *,CBASE                                                 00036000
          SR    CRETCODE,CRETCODE    INITAILIZE REG FOR RETURN CODES    00037000
*/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  */ 00038000
*/*  IF REGISTER 7  IS ZERO THEN                                     */ 00039000
*/*   THE USER PROVIDED A SAVE AREA POINTER IN REGISTER 13           */ 00040000
*/*                                                                  */ 00041000
*/*  SINCE REGISTER 13 WILL PROBABLY BE DESTROYED ACROSS SOME        */ 00042000
*/*   MACRO CALLED ROUTINES  REGISTER 7  IS USED TO SAVE THE         */ 00043000
*/*   CONTENTS OF REGISTER 13 AND WILL  HAVE ITS HIGH ORDER BIT      */ 00044000
*/*   SET ON                                                         */ 00045000
*/*                                                                  */ 00046000
*/*  IF REGISTER 7  WAS NOT ZERO THEN                                */ 00047000
*/*   THEN NO SAVE AREA POINTER WAS PASSED BY THE CALLER AND         */ 00048000
*/*   REGISTER 7  NOW CONTAINS THE RETURN ADDRESS OF THE CALLER      */ 00049000
*/*   WITH ITS HIGH ORDER BIT CLEARED TO INDICATE IT CONTAINS        */ 00050000
*/*   THE RETURN ADDRESS OF THE CALLER                               */ 00051000
*/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  */ 00052000
*/*                                                                  */ 00053000
*                                                                  0296 00054000
*   IF CREGSV=0 THEN                                               0296 00055000
         LTR   CREGSV,CREGSV                                       0296 00056000
         BNZ   @RF00296                                            0296 00057000
*     DO;                           /* WAS A SAVE AREA PROVIDED      */ 00058000
*       CREGSV=CSAVE;               /* SAVE POINTER ACROSS MODULE    */ 00059000
         LR    CREGSV,CSAVE                                        0298 00060000
*       CREGSV=CREGSV|CHIBIT;       /* INDICATE SAVE AREA POINTER    */ 00061000
         O     CREGSV,@CF01794                                     0299 00062000
*     END;                                                         0300 00063000
*   ELSE                            /* NO SAVE AREA PTR PROVIDED     */ 00064000
*     CREGSV=CREGSV&CHIOFF;         /* INDICATE RETURN ADDRESS       */ 00065000
         B     @RC00296                                            0301 00066000
@RF00296 N     CREGSV,@CF01792                                     0301 00067000
*   CSAVE=ADDR(RPHWORK);            /* SET REG SAVE AREA     @Z30PPGF*/ 00068000
@RC00296 LA    CSAVE,RPHWORK(,RPHPTR)                              0302 00069000
*                                                                  0303 00070000
*/*VMDESET  ((CRETCODE),ZERO)                                        */ 00071000
*                                                                  0303 00072000
*   DO;                             /* INVOKE MODESET MACRO          */ 00073000
*     GEN;                                                         0304 00074000
         MODESET EXTKEY=ZERO,WORKREG=2                                  00075000
*     CRETCODE=0;                   /* PUT RETURN CODE IN RCREG      */ 00076000
         SLR   CRETCODE,CRETCODE                                   0305 00077000
*   END;                            /* VMDESET GET INTO KEY ZERO   0306 00078000
*                                                            @ZM40773*/ 00079000
*   RFY                                                            0307 00080000
*     COPYRPL RSTD;                                                0307 00081000
*   CRPLPTR=RPHWEA;                 /* GET PTR TO COPIED RPL FOR RPL    00082000
*                                      BASE                  @Z30PPGF*/ 00083000
         L     CRPLPTR,RPHWEA(,RPHPTR)                             0308 00084000
*   RFY                                                            0309 00085000
*     MPSTPTR RSTD;                 /* RESTRICT MPST PTR FOR REST OF    00086000
*                                      MODULE                @ZM32701*/ 00087000
*   MPSTPTR=RPHTSKID->PSTMPSTP;     /* GET MPST ADDRESS      @ZM32701*/ 00088000
         L     @14,RPHTSKID(,RPHPTR)                               0310 00089000
         L     MPSTPTR,PSTMPSTP(,@14)                              0310 00090000
*   RFY                                                            0311 00091000
*     ISTMPST BASED(MPSTPTR);       /* COVER MPST WITH OVERLAY     0311 00092000
*                                                            @ZM32701*/ 00093000
*   RFY                                                            0312 00094000
*     ISTACDEB BASED(ACBDEB-(LENGTH(ACDDBPFX)+LENGTH(ACDVTPRX)));/*     00095000
*                                                            @Z40GKD1*/ 00096000
*   IF ACDABEND=CBITON              /* CHECK IF ABEND IN PROGRESS  0313 00097000
*                                                            @Z40GKD1*/ 00098000
*     THEN                                                         0313 00099000
         L     @14,RPLDACB(,CRPLPTR)                               0313 00100000
         L     @14,ACBDEB-1(,@14)                                  0313 00101000
         LA    @14,0(,@14)                                         0313 00102000
         TM    ACDABEND-24(@14),B'01000000'                        0313 00103000
         BNO   @RF00313                                            0313 00104000
*     DO;                           /* ABEND IS IN PROGFRESS THEN  0314 00105000
*                                                            @Z40GKD1*/ 00106000
*       RFY                                                        0315 00107000
*        (ATCVTPTR,                                                0315 00108000
*         OLDCRPL,                                                 0315 00109000
*         NEWCRPL,                                                 0315 00110000
*         OLDRCNT,                                                 0315 00111000
*         NEWCNT) RSTD;             /*                       @ZM32701*/ 00112000
*       DO;                         /* LISATCVT                    0316 00113000
*                                      CVTBASE(PAGEABLE,(ATCVTPTR))  */ 00114000
*         ATCVTPTR=ATCLCPTR;        /* SET UP ATCVT POINTER          */ 00115000
         L     ATCVTPTR,ATCLCPTR                                   0317 00116000
*       END;                        /* LISATCVT                      */ 00117000
*       RFY                                                        0319 00118000
*         ISTATCVT BASED(ATCVTPTR); /*                       @Z30PPGF*/ 00119000
*       IF ATCRPLAC>=ATCRPLCT THEN  /* IF LIMIT OF CRPL ON ATCVT   0320 00120000
*                                      REACHED RELSTORE THE CRPL DCR    00121000
*                                      4214                          */ 00122000
         CLC   ATCRPLAC(2,ATCVTPTR),ATCRPLCT(ATCVTPTR)             0320 00123000
         BL    @RF00320                                            0320 00124000
*/* RELSTORE BUFADDR(CRPLPTR)SAVE(YES)CLEAR(NO)                      */ 00125000
*                                                                  0321 00126000
*         DO;                                                      0321 00127000
*                                                                  0321 00128000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00129000
