*LOCPAB:                                                           0797 01231000
*   ;                               /* LOCATE PAB LOGIC              */ 01232000
*                                                                  0797 01233000
LOCPAB   DS    0H                                                  0798 01234000
*   /*****************************************************************/ 01235000
*   /*                                                               */ 01236000
*   /* REG2 HAS CID ON ENTRY                                         */ 01237000
*   /*                                                               */ 01238000
*   /*****************************************************************/ 01239000
*                                                                  0798 01240000
*   REG13=ADDR(CRASVC);             /* OBTAIN SAVEAREA ADDRESS       */ 01241000
         L     REG13,RPHCRR(,REG1)                                 0798 01242000
         LA    REG13,CRASVC(,REG13)                                0798 01243000
*   RFY                                                            0799 01244000
*    (REG1,                                                        0799 01245000
*     REG2,                                                        0799 01246000
*     REG13) RSTD;                  /* RESTRICT REGS                 */ 01247000
*   CIDSAV=REG2;                    /* SAVE CID THRU CIDCTL          */ 01248000
         LR    CIDSAV,REG2                                         0800 01249000
*   RWORK=REG2&SAFMASK;             /* ISOLATE SAF FROM CID          */ 01250000
