         N     RWORK,@CF03654                                      0801 01252000
*   IF RWORK=ZERO THEN              /* IF SAF PORTION OF CID IS ZERO */ 01253000
         LTR   RWORK,RWORK                                         0802 01254000
         BNZ   @RF00802                                            0802 01255000
*     CRXCOMP='05'X;                /* THEN RETURN WITH ERROR CODE   */ 01256000
         MVI   CRXCOMP(CRXPTR),X'05'                               0803 01257000
*   ELSE                                                           0804 01258000
*     DO;                                                          0804 01259000
         B     @RC00802                                            0804 01260000
@RF00802 DS    0H                                                  0805 01261000
*       DO;                         /* CIDCTL(FIND) LOCK(SHARED)     */ 01262000
*         RFY                                                      0806 01263000
*          (EMCR2,                                                 0806 01264000
*           EMCR3) RSTD;                                           0806 01265000
*         GEN SETS(RPHWORK)                                        0807 01266000
*             (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH); 0807 01267000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              01268000
*         EMCR3='00'X;              /* CLEAR EXTRA BITS              */ 01269000
