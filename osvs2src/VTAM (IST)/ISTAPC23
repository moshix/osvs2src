         TITLE 'ISTAPC23 - TPIO PROCEDURE FOR VS2                      *00001000
                        '                                               00002000
ISTAPC23 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTAPC23  77.035'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @10,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@10                                         0001 00012000
          MODESET EXTKEY=ZERO,SAVEKEY=(2)                               00013000
*   CRXPTR=ADDR(CRAPROCR);          /* ADDR COMPONENT RECOVERY RECORD*/ 00014000
         L     @03,RPHCRR(,REG1)                                   0379 00015000
         LA    @15,CRAPROCR(,@03)                                  0379 00016000
         LR    CRXPTR,@15                                          0379 00017000
*   ISTCRX=ISTCRX&&ISTCRX;          /* CLEAR CRX                     */ 00018000
         XC    ISTCRX(100,CRXPTR),ISTCRX(CRXPTR)                   0380 00019000
*   PFCCRRO=CRACRR;                 /* SAVE OLD CRR HEADER IN CRX    */ 00020000
         L     @06,CRACRR(,@03)                                    0381 00021000
         ST    @06,PFCCRRO(,CRXPTR)                                0381 00022000
*   PFCFRRO=CRAFRR;                 /* SAVE OLD FRR HEADER IN MY CRX */ 00023000
         L     @06,CRAFRR(,@03)                                    0382 00024000
         ST    @06,PFCFRRO(,CRXPTR)                                0382 00025000
*   CRACRR=ADDR(CRAPROCR);          /* SET MY CRR ADDR IN HEADER     */ 00026000
         ST    @15,CRACRR(,@03)                                    0383 00027000
*   CRXSAVE(1)=REG2;                /* SAVE REG2 IN SAVEAREA         */ 00028000
         ST    REG2,CRXSAVE(,CRXPTR)                               0384 00029000
*   RFY                                                            0385 00030000
*     REG2 UNRSTD;                  /* RELEASE REG 2                 */ 00031000
*   CRRID=PFCIDTIO;                 /* INDICATE TPIO PROCESSING      */ 00032000
         MVI   CRRID(CRXPTR),X'40'                                 0386 00033000
*   CRRLEN=LENGTH(ISTCRX);          /* SET LENGTH OF RECOVERY AREA   */ 00034000
         MVC   CRRLEN(2,CRXPTR),@CH03805                           0387 00035000
*   RFY                                                            0388 00036000
*     MQLPTR RSTD;                  /* RESTRICT MQL POINTER          */ 00037000
*   MQLPTR=RPHWEA;                  /* OBTAIN MQL POINTER            */ 00038000
         L     MQLPTR,RPHWEA(,REG1)                                0389 00039000
*   CRXFLGS=MQLDEBAD&TESTTYPE;      /* OBTAIN TPIO TYPE              */ 00040000
         L     @00,MQLDEBAD(,MQLPTR)                               0390 00041000
         N     @00,@CF03656                                        0390 00042000
         ST    @00,CRXFLGS(,CRXPTR)                                0390 00043000
*   IF MQLLCPIN=ON THEN                                            0391 00044000
         TM    MQLLCPIN(MQLPTR),B'01000000'                        0391 00045000
         BNO   @RF00391                                            0391 00046000
*     CRXLCPI=ON;                   /* GET LCPB INDICATOR            */ 00047000
         OI    CRXLCPI(CRXPTR),B'00100000'                         0392 00048000
*   IF CRXETYP=CRXCLOSE THEN                                       0393 00049000
@RF00391 CLI   CRXETYP(CRXPTR),X'0F'                               0393 00050000
         BNE   @RF00393                                            0393 00051000
*     DO;                           /* IF CLOSE THEN POST THE ECB    */ 00052000
*CLSELOCK:                                                         0395 00053000
*       ;                                                          0395 00054000
CLSELOCK DS    0H                                                  0396 00055000
*       GEN CODE REFS(REG11,REG12,REG13,REG14,PSALITA,FLC);        0396 00056000
*                                                                  0396 00057000
          SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                       *00058000
               RELATED=(MQLPOST,ISTAPC23(CLSLOCK))                      00059000
*       /*************************************************************/ 00060000
*       /*                                                           */ 00061000
*       /* POST THE ECB IN THE MQL FOR CLOSE ACB                     */ 00062000
*       /*                                                           */ 00063000
*       /*************************************************************/ 00064000
*                                                                  0397 00065000
*       RFY                                                        0397 00066000
*        (ECBCODE,                                                 0397 00067000
*         SAVEREGS,                                                0397 00068000
*         SRBECB) RSTD;                                            0397 00069000
*       SAVEREGS=ADDR(CRXSAVEA);    /* ADDRESS OF SAVE AREA          */ 00070000
         LA    SAVEREGS,CRXSAVEA(,CRXPTR)                          0398 00071000
*       STM(REG1,BASEREG,CRXSAVEA); /* SAVE REGS THRU POST           */ 00072000
         STM   REG1,BASEREG,CRXSAVEA(CRXPTR)                       0399 00073000
*       ECBCODE=ZERO;               /* SET COMPLETION CODE TO ZERO   */ 00074000
         SLR   ECBCODE,ECBCODE                                     0400 00075000
*       SRBECB=ADDR(MQLECB);        /* POINT TO THE ECB IN THE FIRST    00076000
*                                      WORD OF THE MQL               */ 00077000
         LR    SRBECB,MQLPTR                                       0401 00078000
*       CALL BRPOST;                /* POST ECB                      */ 00079000
         L     @08,CVTPTR                                          0402 00080000
         L     @15,CVT0PT01(,@08)                                  0402 00081000
         BALR  @14,@15                                             0402 00082000
*       LM(REG1,BASEREG,CRXSAVEA);  /* RESTORE REGS                  */ 00083000
         LM    REG1,BASEREG,CRXSAVEA(CRXPTR)                       0403 00084000
*       RFY                                                        0404 00085000
*        (ECBCODE,                                                 0404 00086000
*         SAVEREGS,                                                0404 00087000
*         SRBECB) UNRSTD;                                          0404 00088000
*CLSLOCK:                                                          0405 00089000
*       ;                                                          0405 00090000
CLSLOCK  DS    0H                                                  0406 00091000
*       GEN CODE REFS(REG11,REG12,REG13,REG14,PSALITA,FLC);        0406 00092000
          SETLOCK  RELEASE,TYPE=LOCAL,                                 *00093000
               RELATED=(MQLPOST,ISTAPC23(CLSELOCK))                     00094000
*     END;                          /* END TPIOTYPE CLOSE            */ 00095000
*   ELSE                                                           0408 00096000
*     DO;                                                          0408 00097000
         B     @RC00393                                            0408 00098000
@RF00393 DS    0H                                                  0409 00099000
*       CRXWEL=MQLWEPTR;            /* OBTAIN WK ELT ADDR FROM MQL   */ 00100000
         L     @00,MQLWEPTR(,MQLPTR)                               0409 00101000
         ST    @00,CRXWEL(,CRXPTR)                                 0409 00102000
*       RFY                                                        0410 00103000
*         DEBPTR RSTD;              /* RESTRICT DEB POINTER          */ 00104000
*       DEBPTR=MQLDEBP;             /* OBTAIN DEB ADDR FROM MQL      */ 00105000
         L     DEBPTR,MQLDEBP-1(,MQLPTR)                           0411 00106000
         LA    DEBPTR,0(,DEBPTR)                                   0411 00107000
*       DEBPTR=DEBPTR-(LENGTH(ACDVTPRX)+LENGTH(ACDDBPFX));/* POINT TO   00108000
*                                      PREFIX                        */ 00109000
         SL    DEBPTR,@CF00090                                     0412 00110000
*       DEBPTR=DEBPTR&ADDRESS;      /* REMOVE TYPE CODE              */ 00111000
*                                                                  0413 00112000
         N     DEBPTR,@CF01445                                     0413 00113000
*       /*************************************************************/ 00114000
*       /*                                                           */ 00115000
*       /* NOW THAT DEB IS KNOWN - SET RPHTSKID AND APCPSTA TO THE   */ 00116000
*       /* DEBS PST (ACDTSKID)                               @ZM32740*/ 00117000
*       /*                                                           */ 00118000
*       /*************************************************************/ 00119000
*                                                                  0414 00120000
*       RFY                                                        0414 00121000
*         ISTAPCRR BASED(ADDR(CRAPSS));                            0414 00122000
*       RPHTSKID=ACDTSKID;          /* RPHTSKID SET TO DEBS PST    0415 00123000
*                                                            @ZM32740*/ 00124000
         L     @09,ACDTSKID(,DEBPTR)                               0415 00125000
         ST    @09,RPHTSKID(,REG1)                                 0415 00126000
*       APCPSTA=ACDTSKID;           /* PSS CRR PTS TO DEBS PST     0416 00127000
*                                                            @ZM32740*/ 00128000
         L     @15,RPHCRR(,REG1)                                   0416 00129000
         ST    @09,APCPSTA+160(,@15)                               0416 00130000
*       CRXDEBP=DEBPTR;             /* SAVE DEB POINTER      @Z40GKD1*/ 00131000
         ST    DEBPTR,CRXDEBP(,CRXPTR)                             0417 00132000
*/*SMSID  BUFF(RPHCRR)TSKID(RPHTSKID)                                */ 00133000
*                                                                  0418 00134000
*       DO;                                                        0418 00135000
*         DCL                                                      0419 00136000
*           1 CMZ0007A PTR(31) BASED((RPHCRR)-8),                  0419 00137000
*            2 * BIT(8),                                           0419 00138000
*            2 CMZ0007 PTR(24);                                    0419 00139000
*         CMZ0007=(RPHTSKID);                                      0420 00140000
         AL    @15,@CF03812                                        0420 00141000
         STCM  @09,7,CMZ0007-1(@15)                                0420 00142000
*       END;                        /* SMSID ASSOCIATE CRA WITH USER    00143000
*                                      TASK                  @Z40GKD1*/ 00144000
*       RFY                                                        0422 00145000
*         COMPREG RSTD;             /* RESTRICT COMPARE REG          */ 00146000
*       COMPREG=MPSQEL;             /* OBTAIN MQL HEADER             */ 00147000
         L     @09,PSTMPSTP(,@09)                                  0423 00148000
         L     COMPREG,MPSQEL(,@09)                                0423 00149000
*CSLOOP:                                                           0424 00150000
*       ;                                                          0424 00151000
CSLOOP   DS    0H                                                  0425 00152000
*       MQLCHAIN=COMPREG;           /* SET CHAIN FIELD IN MQL        */ 00153000
         ST    COMPREG,MQLCHAIN(,MQLPTR)                           0425 00154000
*       CS(COMPREG,MQLPTR,MPSQEL);  /* SWAP MQL ON QUEUE             */ 00155000
         L     @09,RPHTSKID(,REG1)                                 0426 00156000
         L     @09,PSTMPSTP(,@09)                                  0426 00157000
         CS    COMPREG,@06,MPSQEL(@09)                             0426 00158000
*       BC(FAIL,CSLOOP);            /* BRANCH FOR SWAP UNSUCCESSFUL  */ 00159000
         BC    7,CSLOOP                                            0427 00160000
*       RFY                                                        0428 00161000
*        (COMPREG,                                                 0428 00162000
*         MQLPTR) UNRSTD;           /* RELEASE COMPARE REG           */ 00163000
*       CRXEMQL=ON;                 /* INDICATE SWAP DONE            */ 00164000
*                                                                  0429 00165000
         OI    CRXEMQL(CRXPTR),B'10000000'                         0429 00166000
*       /*************************************************************/ 00167000
*       /*                                                           */ 00168000
*       /* CHECK FOR ABEND OR CLOSE IN PROGRESS              @Z40GKD1*/ 00169000
*       /*                                                           */ 00170000
*       /*************************************************************/ 00171000
*                                                                  0430 00172000
*       IF(ACDABEND=OFF)&(ACDCACB=OFF) THEN                        0430 00173000
         TM    ACDABEND(DEBPTR),B'11000000'                        0430 00174000
         BNZ   @RF00430                                            0430 00175000
*         DO;                       /* PROCESS REQ.          @Z40GKD1*/ 00176000
*                                                                  0431 00177000
*           /*********************************************************/ 00178000
*           /*                                                       */ 00179000
*           /* DETERMINE TYPE OF REQUEST TO BE PROCESSED             */ 00180000
*           /*                                                       */ 00181000
*           /*********************************************************/ 00182000
*                                                                  0432 00183000
*           IF CRXETYP=CRXSPEC      /* CHECK FOR SPECIFIC REQUEST    */ 00184000
*             THEN                                                 0432 00185000
         CLI   CRXETYP(CRXPTR),X'00'                               0432 00186000
         BNE   @RF00432                                            0432 00187000
*             DO;                                                  0433 00188000
*               GEN(BAL   REG12,SPECREQ);/* BRANCH FOR SPECIFIC      */ 00189000
         BAL   REG12,SPECREQ                                            00190000
*             END;                                                 0435 00191000
*           ELSE                                                   0436 00192000
*             IF CRXETYP=CRXANY     /* CHECK FOR ANY REQUEST         */ 00193000
*               THEN                                               0436 00194000
         B     @RC00432                                            0436 00195000
@RF00432 CLI   CRXETYP(CRXPTR),X'04'                               0436 00196000
         BNE   @RF00436                                            0436 00197000
*               DO;                                                0437 00198000
*                 GEN(BAL   REG12,ANYREQ);/* BRANCH FOR ANY          */ 00199000
         BAL   REG12,ANYREQ                                             00200000
*               END;                                               0439 00201000
*             ELSE                                                 0440 00202000
*               IF CRXETYP=CRXOPEN  /* CHECK FOR OPEN REQUEST        */ 00203000
*                 THEN                                             0440 00204000
         B     @RC00436                                            0440 00205000
@RF00436 CLI   CRXETYP(CRXPTR),X'08'                               0440 00206000
         BNE   @RF00440                                            0440 00207000
*                 DO;                                              0441 00208000
*                   GEN(BAL   REG12,OPNDREQ);/* BRANCH FOR OPEN      */ 00209000
         BAL   REG12,OPNDREQ                                            00210000
*                 END;                                             0443 00211000
*               ELSE                                               0444 00212000
*                 IF CRXETYP=CRXSC THEN/* SESSION CONTROL REQUEST    */ 00213000
         B     @RC00440                                            0444 00214000
@RF00440 CLI   CRXETYP(CRXPTR),X'10'                               0444 00215000
         BNE   @RF00444                                            0444 00216000
*                   GEN CODE REFS(REG12,SCREQ) SETS(REG12) FLOWS(SCREQ) 00217000
*                       (BAL     REG12,SCREQ);                     0445 00218000
         BAL     REG12,SCREQ                                            00219000
*                 ELSE                                             0446 00220000
*                   DO;                                            0446 00221000
         B     @RC00444                                            0446 00222000
@RF00444 DS    0H                                                  0447 00223000
*                     GEN(BAL   REG12,TPPOSREQ);/* BRANCH FOR TPPOST */ 00224000
         BAL   REG12,TPPOSREQ                                           00225000
*                   END;                                           0448 00226000
*         END;                      /* END PROCESS REQUEST   @Z40GKD1*/ 00227000
*                                                                  0449 00228000
@RC00444 DS    0H                                                  0449 00229000
@RC00440 DS    0H                                                  0449 00230000
@RC00436 DS    0H                                                  0449 00231000
@RC00432 DS    0H                                                  0450 00232000
*       /*************************************************************/ 00233000
*       /*                                                           */ 00234000
*       /* EXIT TPIO PROCEDURE ISTAPC23                              */ 00235000
*       /*                                                           */ 00236000
*       /*************************************************************/ 00237000
*                                                                  0450 00238000
*     END;                                                         0450 00239000
*                                                                  0450 00240000
@RF00430 DS    0H                                                  0451 00241000
*/* TPEXIT  NORM                                                     */ 00242000
*                                                                  0451 00243000
*   DO;                                                            0451 00244000
@RC00393 DS    0H                                                  0452 00245000
*     RESPECIFY                                                    0452 00246000
*       GPR15P RSTD;                                               0452 00247000
*                                                                  0452 00248000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)                               */ 00249000
*/* END OF ATBUILD                                                   */ 00250000
*                                                                  0453 00251000
*     DO;                           /* LISATCVT                    0453 00252000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00253000
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00254000
         L     GPR15P,ATCLCPTR                                     0454 00255000
*     END;                          /* LISATCVT                      */ 00256000
*     RESPECIFY                                                    0456 00257000
*       GPR00F RSTD;                                               0456 00258000
*     GPR00F=4;                     /* SET PARAMETER REGISTER TO 4   */ 00259000
         LA    GPR00F,4                                            0457 00260000
*     GPR15P=GPR15P->ATCAP31;                                      0458 00261000
         L     GPR15P,ATCAP31(,GPR15P)                             0458 00262000
*     DCL                                                          0459 00263000
*       FMZL0008 BASED(GPR15P) LABEL VALUERANGE(*);                0459 00264000
*     CALL FMZL0008;                                               0460 00265000
         BALR  @14,@15                                             0460 00266000
*     RESPECIFY                                                    0461 00267000
*      (GPR00F,                                                    0461 00268000
*       GPR15P) UNRSTD;                                            0461 00269000
*   END;                            /* **** END OF TPEXIT EXIT TO PSS   00270000
*                                      DISPATCHER                    */ 00271000
*                                                                  0462 00272000
*   /*****************************************************************/ 00273000
*   /*                                                               */ 00274000
*   /* PROCESS TPPOST REQUEST                                        */ 00275000
*   /*                                                               */ 00276000
*   /*****************************************************************/ 00277000
*                                                                  0463 00278000
*   RFY                                                            0463 00279000
*     ISTRPH BASED(CRXWEL);         /* RPH IS WORK ELT FOR TPPOST    */ 00280000
*TPPOSREQ:                                                         0464 00281000
*   ;                               /* TPPOST LOGIC                  */ 00282000
TPPOSREQ DS    0H                                                  0465 00283000
*   CRXPABA=RPHMAJCB+RPHPABOF;      /* STORE PAB POINTER IN CRX      */ 00284000
         L     @09,CRXWEL(,CRXPTR)                                 0465 00285000
         L     @06,RPHMAJCB-1(,@09)                                0465 00286000
         LA    @06,0(,@06)                                         0465 00287000
         SLR   @00,@00                                             0465 00288000
         IC    @00,RPHPABOF(,@09)                                  0465 00289000
         ALR   @06,@00                                             0465 00290000
         ST    @06,CRXPABA(,CRXPTR)                                0465 00291000
*   RFY                                                            0466 00292000
*     ISTRPH BASED(REG1);           /* RE-BASE THE RPH               */ 00293000
*/*TPPOST  (CRXWEL)  ENA                                             */ 00294000
*                                                                  0467 00295000
*   DO;                             /* DECLARE AND RESTRICT REGS     */ 00296000
*     RESPECIFY                                                    0468 00297000
*      (GPR00F,                                                    0468 00298000
*       GPR01F,                                                    0468 00299000
*       GPR04F,                                                    0468 00300000
*       GPR05F,                                                    0468 00301000
*       GPR06F,                                                    0468 00302000
*       GPR14F,                                                    0468 00303000
*       GPR15F) RSTD;               /* 0,1,4,5,6,14, AND 15          */ 00304000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTRPH)                                 */ 00305000
*/* END OF ATBUILD                                                   */ 00306000
*                                                                  0469 00307000
*     GEN REFS(GPR01P,GPR02F,GPR06F) SETS(RPHWORK);                0469 00308000
        STM GPR02F,GPR06F,RPHWORK(GPR01P)                               00309000
*     GPR04F=CRXWEL;                /* LOAD RPH PNTR INTO REG 4      */ 00310000
         L     GPR04F,CRXWEL(,CRXPTR)                              0470 00311000
*     RFY                                                          0471 00312000
*       GPR03F RSTD;                                               0471 00313000
*     GPR03F=0;                                                    0472 00314000
         SLR   GPR03F,GPR03F                                       0472 00315000
*     DO;                           /* MODESET SAVEKEY(GPR06F)     0473 00316000
*                                      KEYADDR(GPR03F)             0473 00317000
*                                      WORKREG(GPR05F)               */ 00318000
*       RESPECIFY                                                  0474 00319000
*        (GPR02F) RESTRICTED;                                      0474 00320000
*       GPR05F=GPR02F;              /* SAVE REGISTER 2               */ 00321000
         LR    GPR05F,GPR02F                                       0475 00322000
*       IPK;                        /* SAVE CURRENT PSW KEY          */ 00323000
         IPK                                                       0476 00324000
*       GPR06F=GPR02F;                                             0477 00325000
         LR    GPR06F,GPR02F                                       0477 00326000
*       GPR02F=GPR05F;              /* RESTORE REGISTER 2            */ 00327000
         LR    GPR02F,GPR05F                                       0478 00328000
*       SPKA(GPR03F);               /* SET PSW KEY                   */ 00329000
         SPKA  0(GPR03F)                                           0479 00330000
*       RESPECIFY                                                  0480 00331000
*        (GPR02F) UNRESTRICTED;                                    0480 00332000
*     END;                          /* MODESET SAVEKEY(GPR06F)     0481 00333000
*                                      KEYADDR(GPR03F)             0481 00334000
*                                      WORKREG(GPR05F)               */ 00335000
*     RFY                                                          0482 00336000
*       GPR03F UNRSTD;                                             0482 00337000
*     GPR05P=GPR04P->RPHMAJCB+GPR04P->RPHPABOF;/* LOAD REG 5 WITH  0483 00338000
*                                      ADDR OF MAJOR CONTROL BLOCK 0483 00339000
*                                      PLUS OFFSET OF PAB IN BLOCK   */ 00340000
         L     GPR05P,RPHMAJCB-1(,GPR04P)                          0483 00341000
         LA    GPR05P,0(,GPR05P)                                   0483 00342000
         SLR   @09,@09                                             0483 00343000
         IC    @09,RPHPABOF(,GPR04P)                               0483 00344000
         ALR   GPR05P,@09                                          0483 00345000
*     GPR14P=0;                     /* LOAD 0 INTO REG 14            */ 00346000
         SLR   GPR14P,GPR14P                                       0484 00347000
*     GPR15P='FFFFFFFF'X;           /* LOAD REG 15 WITH DUMMY ADDRESS*/ 00348000
         SLR   GPR15P,GPR15P                                       0485 00349000
         BCTR  GPR15P,0                                            0485 00350000
*     CS(GPR14P,GPR15P,GPR04P->RPHRESMA);/* COMPARE RESUME ADDRESS 0486 00351000
*                                      WITH 0 AND,IF EQUAL, PUT DUMMY   00352000
*                                      ADDRESS IN CONTROL BLOCK    0486 00353000
*                                      RESUME ADDRESS                */ 00354000
         CS    GPR14P,@15,RPHRESMA(GPR04P)                         0486 00355000
*     BC(8,FMZT0016);               /* IF EQUAL, BYPASS REMAINDER OF    00356000
*                                      EXPANSION                     */ 00357000
         BC    8,FMZT0016                                          0487 00358000
*FMZL0016:                                                         0488 00359000
*     IF GPR04P->RPHWT='1'B THEN    /* WT/EXT CONTENTION FLAG ON     */ 00360000
FMZL0016 TM    RPHWT(GPR04P),B'00001000'                           0488 00361000
         BNO   @RF00488                                            0488 00362000
*       DO;                         /* IN RPH                        */ 00363000
*         GPR14P=GPR04P->RPHRESMA|RPHWT1;/* LOAD REG 14 WITH FLAG  0490 00364000
*                                      FIELD AND MAKE CERTAIN WT/EXT    00365000
*                                      CONTENTION FLAG STILL ON      */ 00366000
         L     GPR14P,RPHRESMA(,GPR04P)                            0490 00367000
         O     GPR14P,@CF01508                                     0490 00368000
*         GPR15P=GPR14P|RPHPT1;     /* LOAD REG 15 WITH FLAG FIELD 0491 00369000
*                                      AND TURN POST FLAG ON         */ 00370000
         LR    GPR15P,GPR14P                                       0491 00371000
         O     GPR15P,@CF01498                                     0491 00372000
*         CS(GPR14P,GPR15P,GPR04P->RPHRESMA);/* COMPARE REG 14 WITH     00373000
*                                      FLAG FIELD--IF WT/EXT BIT ON,    00374000
*                                      TURN ON POST FLAG             */ 00375000
         CS    GPR14P,@15,RPHRESMA(GPR04P)                         0492 00376000
*         BC(4,FMZL0016);           /* IF WT/EXT BIT WAS OFF, LOOP TO   00377000
*                                      TEST CONTENTION FLAG          */ 00378000
         BC    4,FMZL0016                                          0493 00379000
*         BC(8,FMZT0016);           /* IF WT/EXT BIT WAS OFF, BRANCH    00380000
*                                      TO BYPASS CODE GEN            */ 00381000
         BC    8,FMZT0016                                          0494 00382000
*       END;                        /* IF POST FLAG IS ON, FALL THRU    00383000
*                                      BOTTOM OF LOOP                */ 00384000
*     ELSE                                                         0496 00385000
*       ;                           /* WT/EXT CONTENTION FLAG OFF    */ 00386000
@RF00488 DS    0H                                                  0497 00387000
*     IF GPR04P->RPHTSKID=GPR01P->RPHTSKID THEN                    0497 00388000
         CLC   RPHTSKID(4,GPR04P),RPHTSKID(GPR01P)                 0497 00389000
         BNE   @RF00497                                            0497 00390000
*       DO;                                                        0498 00391000
*         GPR05P->PABCHNGP=GPR01P->RPHPABQP;/* PUT PAB QUEUE ADDRESS    00392000
*                                      IN SECOND WORD OF PAB         */ 00393000
         MVC   PABCHNGP(3,GPR05P),RPHPABQP(GPR01P)                 0499 00394000
*         GPR01P->RPHPABQP=GPR05P;  /* PUT PAB ADDRESS (IN MAJOR   0500 00395000
*                                      CONTROL BLOCK) IN PAB QUEUE 0500 00396000
