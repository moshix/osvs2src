*         RFY                                                      0811 01277000
*          (EMCR2,                                                 0811 01278000
*           EMCR3) UNRSTD;                                         0811 01279000
*       END;                        /* CIDCTL OBTAIN FMCB POINTER    */ 01280000
*       RFY                                                        0813 01281000
*         REG15 RSTD;               /* RESTRICT REGISTER 15          */ 01282000
*       IF REG15^=RC0               /* CHECK FOR SUCCESSFUL CIDCTRL  */ 01283000
*         THEN                                                     0814 01284000
         LTR   REG15,REG15                                         0814 01285000
         BZ    @RF00814                                            0814 01286000
*         CRXCOMP='05'X;            /* INDICATE ERROR                */ 01287000
         MVI   CRXCOMP(CRXPTR),X'05'                               0815 01288000
*       ELSE                                                       0816 01289000
*         DO;                                                      0816 01290000
