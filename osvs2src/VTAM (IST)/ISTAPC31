*         OLD=RPHRESMA;             /* LOOK AT WAIT/EXIT & POST FLAGS*/ 00781000
         L     OLD,RPHRESMA(,RPHPTR)                               0475 00782000
*TSTPST2:                                                          0476 00783000
*         IF RPHPT=ONBIT            /* HAS PAB/RPH BEEN POSTED?      */ 00784000
*           THEN                    /* YES,TURN OFF WAIT/EXIT AND  0476 00785000
*                                      POST FLAGS,& REDISPATCH POSTED   00786000
*                                      PAB                           */ 00787000
TSTPST2  TM    RPHPT(RPHPTR),B'00000100'                           0476 00788000
         BNO   @RF00476                                            0476 00789000
*           DO;                                                    0477 00790000
*SWAPWEP2:                                                         0478 00791000
*             NEW=OLD&RPHWT0&RPHPT0;/* TURN OFF WAIT/EXIT/POST FLAGS */ 00792000
SWAPWEP2 LR    NEW,OLD                                             0478 00793000
         N     NEW,@CF02894                                        0478 00794000
*             CS(OLD,NEW,RPHRESMA); /* TRY TO TURN OFF               */ 00795000
         CS    OLD,@06,RPHRESMA(RPHPTR)                            0479 00796000
*             BC(7,SWAPWEP2);       /* TRY TILL SUCCEED              */ 00797000
