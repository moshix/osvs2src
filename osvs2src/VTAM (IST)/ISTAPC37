         TITLE 'ISTAPC37-TPQUE PROCEDURE                               *00001000
                        '                                               00002000
ISTAPC37 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTAPC37  76.055'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @08,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@08                                         0001 00012000
*                                                                  0060 00013000
*/********************************************************************/ 00014000
*/* IF THE WORK ELEMENT QUEUE IS ALREADY GATED  AND THE PABDELAY FLAG*/ 00015000
*/* IS OFF EITHER THE PROCESS IS RUNNING OR IS EXPLICITLY BLOCKED, SO*/ 00016000
*/* QUEING IS PERFORMED BUT SCHEDULING IS NOT.  SIMILARLY IF THE     */ 00017000
*/* PAB ERROR LOCK IS SET.                                           */ 00018000
*/* FOR FIFO QUEING (OR LIFO, IF THE QUEUE IS EMPTY), THE ELEMENT    */ 00019000
*/* IS QUEUED WITH THE GATE BIT ON IN THE PAB AND THE ELEMENT.       */ 00020000
*/* FOR LIFO QUEUING, THE ELEMENT IS QUEUED AND ALL GATE BITS ARE    */ 00021000
*/* ASSUMED ON.                                                      */ 00022000
*/********************************************************************/ 00023000
*                                                                  0060 00024000
*CSLOOP:                                                           0060 00025000
*   LM(GPR02P,GPR03P,PABWQCHN);     /* GET WK ELE Q AND PAB CHN      */ 00026000
CSLOOP   LM    GPR02P,GPR03P,PABWQCHN(PABPTR)                      0060 00027000
*   IF PABERLCK=ON|                 /* PAB ERROR LOCK ON OR  @ZM42117*/ 00028000
*       (GPR02F<0&PABDELAY=OFF) THEN/* QUEUE GATED ?         @Z40GKD9*/ 00029000
         TM    PABERLCK(PABPTR),B'00000100'                        0061 00030000
         BO    @RT00061                                            0061 00031000
         LTR   GPR02F,GPR02F                                       0061 00032000
         BNM   @RF00061                                            0061 00033000
         TM    PABDELAY(PABPTR),B'00000010'                        0061 00034000
         BNZ   @RF00061                                            0061 00035000
@RT00061 DS    0H                                                  0062 00036000
*     IF(GPR02P&PABCHNG0)=0         /* YES - IS THE QUEUE EMPTY      */ 00037000
*         |LIFO=OFF THEN            /* OR FIFO?                      */ 00038000
         LR    @15,GPR02P                                          0062 00039000
         N     @15,@CF00913                                        0062 00040000
         LTR   @15,@15                                             0062 00041000
         BZ    @RT00062                                            0062 00042000
         TM    LIFO(GPR14P),B'00000001'                            0062 00043000
         BNZ   @RF00062                                            0062 00044000
@RT00062 DS    0H                                                  0063 00045000
*       DO;                                                        0063 00046000
*         WELCHN=GPR02P|PABWEQG1;   /* YES - THIS WK ELE, SET        */ 00047000
         L     @15,@CF00914                                        0064 00048000
         LR    @07,GPR02P                                          0064 00049000
         OR    @07,@15                                             0064 00050000
         ST    @07,WELCHN(,WELPTR)                                 0064 00051000
*         GPR03P=WELPTR|PABWEQG1;   /* BOTH GATE BITS,               */ 00052000
         OR    @15,WELPTR                                          0065 00053000
         LR    GPR03P,@15                                          0065 00054000
*         CS(GPR02P,GPR03P,PABWEQA);/* QUEUE THIS ONE.               */ 00055000
         CS    GPR02P,@03,PABWEQA(PABPTR)                          0066 00056000
*         BC(FAIL,CSLOOP);          /* FAILED? START OVER.           */ 00057000
         BC    4,CSLOOP                                            0067 00058000
*       END;                                                       0068 00059000
*     ELSE                          /* LIFO QUEUING REQUIRED.        */ 00060000
*       DO;                                                        0069 00061000
         B     @RC00062                                            0069 00062000
@RF00062 DS    0H                                                  0070 00063000
*         GPR03P=PABCHNG1;          /* PUT GATE MASK IN REG,         */ 00064000
         L     GPR03P,@CF00914                                     0070 00065000
*         DO WHILE GPR02P->WELCHN^=GPR03P;/* GO DOWN QUEUE UNTIL     */ 00066000
         B     @DE00071                                            0071 00067000
@DL00071 DS    0H                                                  0072 00068000
*           GPR02P=GPR02P->WELCHN;  /* ELEMENT IS FOUND GATED        */ 00069000
         L     GPR02P,WELCHN(,GPR02P)                              0072 00070000
*         END;                      /* WITH NO FORWARD PTR.          */ 00071000
@DE00071 C     GPR03P,WELCHN(,GPR02P)                              0073 00072000
         BNE   @DL00071                                            0073 00073000
*         WELCHN=GPR03P;            /* GATE ELEMENT BEING Q-ED.      */ 00074000
         ST    GPR03P,WELCHN(,WELPTR)                              0074 00075000
*         GPR03P=GPR03P|WELPTR;     /* GATE PTR TO NEW ELEMENT,      */ 00076000
         OR    GPR03P,WELPTR                                       0075 00077000
*         GPR02P->WELCHN=GPR03P;    /* AND ADD IT TO QUEUE.          */ 00078000
         ST    GPR03P,WELCHN(,GPR02P)                              0076 00079000
*       END;                                                       0077 00080000
         EJECT                                                          00081000
*                                                                  0078 00082000
*/********************************************************************/ 00083000
*/* THE QUEUE IS NOT GATED OR THE PABDELAY FLAG IS ON WHICH MEANS    */ 00084000
*/* THE PAB CANNOT BE GATED SO THE ELEMENT IS QUEUED AND BOTH THE    */ 00085000
*/* WORK ELEMENT QUEUE AND PAB CHAIN ARE GATED AND THE PABDELAY FLAG */ 00086000
*/* IS RESET.                                                        */ 00087000
*/********************************************************************/ 00088000
*                                                                  0078 00089000
*   ELSE                                                           0078 00090000
*     DO;                           /*                       @Z40GKD9*/ 00091000
         B     @RC00061                                            0078 00092000
@RF00061 DS    0H                                                  0079 00093000
*       IF(GPR02P&PABCHNG0)=0       /* YES - IS THE QUEUE EMPTY    0079 00094000
*                                                            @Z40GKD9*/ 00095000
