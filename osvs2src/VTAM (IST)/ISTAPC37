         ST    GPR03P,WELCHN(,GPR02P)                              0098 00161000
*         END;                      /*                       @Z40GKD9*/ 00162000
*       RFY                                                        0100 00163000
*        (GPR06P,                                                  0100 00164000
*         GPR07P) UNRSTD;           /*                       @Z40GKD9*/ 00165000
@RC00079 DS    0H                                                  0101 00166000
*/********************************************************************/ 00167000
*/* THE PAB HAS BEEN GATED. CHECK FOR PRIORITY SCHEDULING (IMMED).   */ 00168000
*/* IN VS2, IF DIRECT SCHEDULING (FASTPATH) HAS BEEN REQUESTED,      */ 00169000
*/* LET THE ISTAPMDR MACRO DO IT. OTHERWISE QUEUE THE PAB ON THE RPH */ 00170000
*/* OURSELVES, UNLESS THERE IS NO RPH (NSTR), THE PROCESS IS         */ 00171000
*/* RUNNING DISABLED (DIS) OR THE RPH AND PAB                        */ 00172000
*/* HAVE DIFFERENT TASK IDS.  FOR THOSE CASES,                       */ 00173000
*/* ISTAPC32 WILL QUEUE THE PAB TO THE RPH READY QUEUE.              */ 00174000
*/********************************************************************/ 00175000
*                                                                  0101 00176000
*       PABPRIOR=OFF;                                              0101 00177000
         NI    PABPRIOR(PABPTR),B'10111111'                        0101 00178000
*       IF PRIORITY=ON THEN         /* SET PRIORITY FLAG, IF         */ 00179000
         TM    PRIORITY(GPR14P),B'00000100'                        0102 00180000
         BNO   @RF00102                                            0102 00181000
*         PABPRIOR=ON;              /* REQUESTED.                    */ 00182000
         OI    PABPRIOR(PABPTR),B'01000000'                        0103 00183000
*       IF GPR00P^=0 THEN           /* IF AN SRB WAS GIVEN,          */ 00184000
@RF00102 LTR   GPR00P,GPR00P                                       0104 00185000
         BZ    @RF00104                                            0104 00186000
*         DO;                       /* DO FAST PATH SCHEDULING       */ 00187000
*           RFY                                                    0106 00188000
*            (GPR06P) RSTD;                                        0106 00189000
*           GPR06P=GPR14P;          /* PROTECT RETURN ADDRESS        */ 00190000
         LR    GPR06P,GPR14P                                       0107 00191000
*/*ISTAPMDR  SRB(GPR00P)PAB(PABPTR)                                  */ 00192000
*                                                                  0108 00193000
*           DO;                                                    0108 00194000
*             RFY                                                  0109 00195000
*              (GPR01P,                                            0109 00196000
*               GPR05P,                                            0109 00197000
*               GPR15P) RSTD;                                      0109 00198000
*             RFY                                                  0110 00199000
*               ISTPST;                                            0110 00200000
*             RFY                                                  0111 00201000
*               ISTMPST;                                           0111 00202000
*             GPR02P=GPR01P;        /* SAVE REG1 IN REG2             */ 00203000
         LR    GPR02P,GPR01P                                       0112 00204000
*             GPR01P=GPR00P;        /* REG1 = ADDR OF SRB            */ 00205000
         LR    GPR01P,GPR00P                                       0113 00206000
*             GPR05P=PABPTR;        /* REG5 = ADDR OF PAB            */ 00207000
