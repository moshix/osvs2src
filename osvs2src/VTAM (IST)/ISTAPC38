*      /*COPY SRB55 INTO SRB */                                    0097 00114000
*                        GPR01P->SRBSECT(1:LENGTH(SRBSECT))=  /*COPY TO 00115000
*SRB PASSED*/                                                      0097 00116000
*                            GPR03P->SRBSECT(1:LENGTH(SRBSECT)); /*     00117000
*FROM SRB55*/                                                      0097 00118000
         MVC   SRBSECT(44,GPR01P),SRBSECT(GPR03P)                  0097 00119000
*                        GPR01P->SRBID = 'PS59';  /*SET SRB ID = PS59*/ 00120000
         MVC   SRBID(4,GPR01P),@CC00791                            0098 00121000
*                        GPR01P->SRBPARM=GPR05P;  /*SET SRB PARM =  PAB 00122000
**/                                                                0099 00123000
         ST    GPR05P,SRBPARM(,GPR01P)                             0099 00124000
*                        GPR15P=ATCLCPTR->ATCAPC59; /*GET ADDR OF ISTAP 00125000
*59*/                                                              0100 00126000
         L     @07,ATCLCPTR                                        0100 00127000
         L     GPR15P,ATCAPC59(,@07)                               0100 00128000
*                        GPR01P->SRBEP=GPR15P; /*SRB ENTRY POINT IS IST 00129000
*PC59*/                                                            0101 00130000
         ST    GPR15P,SRBEP(,GPR01P)                               0101 00131000
*                        GEN REFS(CVTPTR,CVTLSMQ,CVTMAP,SRBFLNK,SRBSECT 00132000
*;                                                                 0102 00133000
         SCHEDULE SRB=(1),SCOPE=LOCAL                                   00134000
*                        GPR01P = GPR02P; /*RESTORE REG 1*/        0103 00135000
*                                                                  0103 00136000
         LR    GPR01P,GPR02P                                       0103 00137000
*                      RFY (GPR01P,GPR05P,GPR15P) UNRSTD;          0104 00138000
*                        END;   /*  *** END ISTAPMDR  *** */       0105 00139000
