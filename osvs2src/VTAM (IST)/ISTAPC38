         SLR   GPR15P,GPR03P                                       0093 00106000
*                        GPR03P=GPR15P->DYPTSKID; /*R3 = ADDR OF PST*/  00107000
         L     GPR03P,DYPTSKID(,GPR15P)                            0094 00108000
*                        GPR03P=GPR03P->PSTMPSTP; /*REG3=MPST ADDR*/    00109000
         L     GPR03P,PSTMPSTP(,GPR03P)                            0095 00110000
*                        GPR03P=GPR03P->MPSSRB55; /*R3 POINTS TO SRB55* 00111000
*                                                                  0096 00112000
