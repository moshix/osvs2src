         TITLE 'ISTAPC38-TPSCHED PROCEDURE                             *00001000
                        '                                               00002000
ISTAPC38 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTAPC38  76.055'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @08,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@08                                         0001 00012000
*IF PABERLOK=ON                          /* IF PAB ERROR LOCK SET,   */ 00013000
*  THEN;                                 /*   DO NOTHING.            */ 00014000
         TM    PABERLOK(PABPTR),B'00000100'                        0058 00015000
         BO    @RT00058                                            0058 00016000
*ELSE                                                              0060 00017000
*  IF UNCOND=OFF THEN                    /* UNCONDITIONAL SCHED?     */ 00018000
         TM    UNCOND(GPR14P),B'00000001'                          0060 00019000
         BNZ   @RF00060                                            0060 00020000
*    DO;                                 /*   NO, PROCESS NORMALLY.  */ 00021000
*      RFY (GPR02P,GPR03P) RSTD;                                   0062 00022000
*CSLOOPLD:                                                         0063 00023000
*      LM (GPR02P,GPR03P,PABWQCHN);      /* GET WK ELE Q AND PAB CHN */ 00024000
CSLOOPLD LM    GPR02P,GPR03P,PABWQCHN(PABPTR)                      0063 00025000
*CSLOOP:                                                           0064 00026000
*      IF GPR02F>=0                      /* IF WORK ELE Q NOT GATED, */ 00027000
*        THEN;                           /*   DO NOTHING.            */ 00028000
CSLOOP   LTR   GPR02F,GPR02F                                       0064 00029000
         BNM   @RT00064                                            0064 00030000
*      ELSE                                                        0066 00031000
*WQGATD: DO;                             /* WORK Q GATED, SO CHECK   */ 00032000
WQGATD   DS    0H                                                  0067 00033000
*          IF GPR03F>=0 THEN             /*   WHETHER PAB GATED.     */ 00034000
         LTR   GPR03F,GPR03F                                       0067 00035000
         BM    @RF00067                                            0067 00036000
*PABNGATD:   DO;                         /* PAB NOT GATED, SO ASSURE */ 00037000
PABNGATD DS    0H                                                  0069 00038000
*              IF GPR02P^=PABWEQG1 THEN  /*   WORK QUEUE NOT EMPTY.  */ 00039000
         CL    GPR02P,@CF00925                                     0069 00040000
         BE    @RF00069                                            0069 00041000
*SCHDPAB:        DO;                                               0070 00042000
SCHDPAB  DS    0H                                                  0071 00043000
*                  RFY (GPR06P,GPR07P) RSTD;                       0071 00044000
*                  GPR06P=GPR02P;        /* NOT EMPTY - GATE PAB AND */ 00045000
         LR    GPR06P,GPR02P                                       0072 00046000
*        GPR07P=(GPR03P| PABCHNG1)&PABDLAY0; /* RESET PABDELAY AND 0073 00047000
*                                                      @Z40GKD9 */ 0073 00048000
         LR    GPR07P,GPR03P                                       0073 00049000
         O     GPR07P,@CF00925                                     0073 00050000
         N     GPR07P,@CF00943                                     0073 00051000
*                  CDS (GPR02P,GPR06P,PABWQCHN); /* PRESERVE WEL Q 0074 00052000
*                                                     STATUS */    0074 00053000
         CDS   GPR02P,GPR06P,PABWQCHN(PABPTR)                      0074 00054000
*                  BC (FAIL,CSLOOP);     /* FAILED, START OVER.      */ 00055000
         BC    4,CSLOOP                                            0075 00056000
*                  RFY (GPR06P,GPR07P) UNRSTD;                     0076 00057000
         EJECT                                                          00058000
*/********************************************************************/ 00059000
*/* THE PAB HAS BEEN GATED. CHECK FOR PRIORITY SCHEDULING (IMMED).   */ 00060000
*/* IN VS2, IF DIRECT (FASTPATH) SCHEDULING (DIR) HAS BEEN REQUESTED, * 00061000
*/* LET THE ISTAPMDR MACRO DO IT. OTHERWISE QUEUE THE PAB ON THE RPH */ 00062000
*/* OURSELVES, UNLESS THERE IS NO RPH (NSTR), THE PROCESS IS RUNNING */ 00063000
*/* DISABLED (DIS), UNCONDITIONAL SCHEDULING WAS REQUESTED (UNCOND), */ 00064000
*/* OR THE RPH AND PAB HAVE DIFFERENT TASK IDS.  FOR THOSE CASES,    */ 00065000
*/* ISTAPC32 WILL QUEUE THE PAB TO THE RPH READY QUEUE. PROCESSING   */ 00066000
*/* FOR UNCONDITIONAL SCHEDULING MERGES WITH MAINLINE AT -SCHEDULE-. */ 00067000
*/********************************************************************/ 00068000
*                  PABPRIOR=OFF;                                   0077 00069000
         NI    PABPRIOR(PABPTR),B'10111111'                        0077 00070000
*                  IF PRIORITY=ON THEN   /* SET PRIORITY SCHEDULING  */ 00071000
         TM    PRIORITY(GPR14P),B'00000100'                        0078 00072000
         BNO   @RF00078                                            0078 00073000
*                    PABPRIOR=ON;        /*  IF REQUESTED.           */ 00074000
         OI    PABPRIOR(PABPTR),B'01000000'                        0079 00075000
*SCHEDULE:;                                                        0080 00076000
@RF00078 DS    0H                                                  0080 00077000
