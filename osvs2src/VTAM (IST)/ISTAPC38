*                  IF GPR00P^=0 THEN     /* IF AN SRB WAS GIVEN, DO  */ 00079000
         LTR   GPR00P,GPR00P                                       0081 00080000
         BZ    @RF00081                                            0081 00081000
*                    DO;                 /*   FASTPATH SCHEDULING.   */ 00082000
*                      RFY (GPR06P) RSTD;                          0083 00083000
*                      GPR06P=GPR14P;   /* PROTECT RETURN ADDRESS    */ 00084000
*                                                                  0084 00085000
         LR    GPR06P,GPR14P                                       0084 00086000
*/*ISTAPMDR  SRB(GPR00P)PAB(PABPTR)*/                              0085 00087000
*                      DO;                                         0085 00088000
*                                                                  0085 00089000
*                      RFY (GPR01P,GPR05P,GPR15P) RSTD;            0086 00090000
*                                                                  0086 00091000
*                      RFY ISTPST  ;                               0087 00092000
*                      RFY ISTMPST ;                               0088 00093000
*                        GPR02P = GPR01P;  /*SAVE REG1 IN REG2*/   0089 00094000
         LR    GPR02P,GPR01P                                       0089 00095000
*                        GPR01P=GPR00P; /*REG1 = ADDR OF SRB*/     0090 00096000
         LR    GPR01P,GPR00P                                       0090 00097000
*                        GPR05P=PABPTR; /*REG5 = ADDR OF PAB*/     0091 00098000
*                        GPR03P=GPR05P->PABOFFST; /*R3=OFFSET IN MAJCB* 00099000
