           LM GPR02P,GPR13P,RPHWORK(GPR01P)                             00221000
*         RFY                                                      0284 00222000
*          (GPR13P) UNRSTD;                                        0284 00223000
*         RFY                                                      0285 00224000
*          (GPR02P,                                                0285 00225000
*           GPR03P,                                                0285 00226000
*           GPR04P,                                                0285 00227000
*           GPR15P) UNRSTD;                                        0285 00228000
*       END;                                                       0286 00229000
*                                                                  0286 00230000
*       /*************************************************************/ 00231000
*       /*                                                           */ 00232000
*       /* ******** END OF REQSTORE EXPANSION REG15 CONTAINS THE RTN */ 00233000
*       /* CODE FROM SMS. IF LESS THAN 8, THE STORAGE HAS BEEN       */ 00234000
*       /* ALLOCATED. IF EQUAL TO 8,THE REQUEST HAS BEEN QUEUED BY   */ 00235000
*       /* STORAGE MANAGEMENT BECAUSE STORAGE IS NOT IMMEDIATELY     */ 00236000
*       /* AVAILABLE.                                                */ 00237000
*       /*                                                           */ 00238000
*       /*************************************************************/ 00239000
*                                                                  0287 00240000
*       IF RCREG<8                  /* CHECK RETURN CODE FROM      0287 00241000
*                                      REQSTORE                      */ 00242000
*         THEN                                                     0287 00243000
         C     RCREG,@CF00059                                      0287 00244000
         BNL   @RF00287                                            0287 00245000
*         DO;                       /* REQSTORE SUCCESSFUL           */ 00246000
*           CRAREG=RPHSRPRM;        /* PUT ADR OF SMS CRA IN CRAREG  */ 00247000
         L     CRAREG,RPHSRPRM(,RPHREG)                            0289 00248000
*           COMPREG=PSTFLGA;        /* COMP REG FOR CS               */ 00249000
         L     COMPREG,PSTFLGA(,PSTREG)                            0290 00250000
*LOOP2:                                                            0291 00251000
*           SWAPREG=COMPREG&PSTSMQR0;/* TURN OFF SMS RPH QUED FOR STG*/ 00252000
LOOP2    LR    SWAPREG,COMPREG                                     0291 00253000
         N     SWAPREG,@CF00784                                    0291 00254000
*           CS(COMPREG,SWAPREG,PSTFLGA);/* USE CS LOGIC              */ 00255000
         CS    COMPREG,@06,PSTFLGA(PSTREG)                         0292 00256000
*           BC(NE,LOOP2);           /* IF FAIL TRY AGAIN             */ 00257000
         BC    7,LOOP2                                             0293 00258000
*           RPHREG=ADDR(CRARPH);    /* SET R1 TO ADDR OF RPH         */ 00259000
         LA    RPHREG,CRARPH(,CRAREG)                              0294 00260000
*         END;                      /* END REQSTORE SUCCESSFUL       */ 00261000
*       ELSE                                                       0296 00262000
*         DO;                       /* WILL LOCK AND USE SMS CRA     */ 00263000
         B     @RC00287                                            0296 00264000
@RF00287 DS    0H                                                  0297 00265000
*           COMPREG=PSTFLGA;        /* COMP REG FOR CS               */ 00266000
         L     COMPREG,PSTFLGA(,PSTREG)                            0297 00267000
*LOOP3:                                                            0298 00268000
*           SWAPREG=COMPREG|PSTSMCU1;/* TURN ON SMS CRA IN USE BIT   */ 00269000
LOOP3    LR    SWAPREG,COMPREG                                     0298 00270000
         O     SWAPREG,@CF00776                                    0298 00271000
*           CS(COMPREG,SWAPREG,PSTFLGA);/* USE CS LOGIC              */ 00272000
         CS    COMPREG,@06,PSTFLGA(PSTREG)                         0299 00273000
*           BC(NE,LOOP3);           /* IF FAIL TRY AGAIN             */ 00274000
         BC    7,LOOP3                                             0300 00275000
*           CRAREG=PSTCRRS;         /* USING SMS CRA                 */ 00276000
         L     CRAREG,PSTCRRS(,PSTREG)                             0301 00277000
*           RPHREG=0;               /* SMS RPH IN USE                */ 00278000
         SLR   RPHREG,RPHREG                                       0302 00279000
*         END;                      /* END - LOCK AND USE SMS CRA    */ 00280000
*     END;                          /* END- REQSTORE OR USE SMS CRA  */ 00281000
*   GOTO CRAINIT;                   /* HAVE A CRA - GO INITIALIZE    */ 00282000
*                                                                  0305 00283000
         B     CRAINIT                                             0305 00284000
*   /*****************************************************************/ 00285000
*   /*                                                               */ 00286000
*   /* IF CS FAILED,BIT PSTSMCR WAS ALREADY ON OR SOMETHING ELSE     */ 00287000
*   /* CHANGED IN PSTFLGA. THE NEXT INSTRUCTION DETERMINES WHICH OF  */ 00288000
*   /* THE TWO OCCURED.                                              */ 00289000
*   /*                                                               */ 00290000
*   /*****************************************************************/ 00291000
*                                                                  0306 00292000
*TEST:                                                             0306 00293000
*   IF COMPREG=SWAPREG              /* WAS PSTSMQR BIT THE ONLY DIFF?*/ 00294000
*     THEN                          /* YES                           */ 00295000
TEST     CR    COMPREG,SWAPREG                                     0306 00296000
         BNE   @RF00306                                            0306 00297000
*     DO;                           /* GET THE SMS CRA               */ 00298000
*LOOP4:                                                            0308 00299000
*       SWAPREG=COMPREG|PSTSMCU1;   /* TURN ON SMS CRA IN USE      0308 00300000
*                                                            @ZM33591*/ 00301000
LOOP4    LR    SWAPREG,COMPREG                                     0308 00302000
         O     SWAPREG,@CF00776                                    0308 00303000
*       CS(COMPREG,SWAPREG,PSTFLGA);/* USE CS LOGIC                  */ 00304000
         CS    COMPREG,@06,PSTFLGA(PSTREG)                         0309 00305000
*       BC(NE,LOOP4);               /* IF FAIL TRY AGAIN             */ 00306000
         BC    7,LOOP4                                             0310 00307000
*       CRAREG=PSTCRRS;             /* USING SMS CRA                 */ 00308000
         L     CRAREG,PSTCRRS(,PSTREG)                             0311 00309000
*       RPHREG=0;                   /* INDICATE RPH IN USE BY SMS    */ 00310000
         SLR   RPHREG,RPHREG                                       0312 00311000
*     END;                          /* END - GET THE SMS CRA         */ 00312000
*   ELSE                                                           0314 00313000
*     GOTO GETSMRPH;                /* RPH NOT QUED YET              */ 00314000
*                                                                  0314 00315000
*   /*****************************************************************/ 00316000
*   /*                                                               */ 00317000
*   /* INIT CRA                                                      */ 00318000
*   /*                                                               */ 00319000
*   /*****************************************************************/ 00320000
*                                                                  0315 00321000
*CRAINIT:                                                          0315 00322000
*   CRACRR=ADDR(CRAPSS);            /* SET PTR TO PSS CRR            */ 00323000
CRAINIT  LA    @14,CRAPSS(,CRAREG)                                 0315 00324000
         ST    @14,CRACRR(,CRAREG)                                 0315 00325000
*   CRALKACT=0;                     /* CLEAR ACTIVE LOCKS            */ 00326000
         SLR   @07,@07                                             0316 00327000
         ST    @07,CRALKACT(,CRAREG)                               0316 00328000
*   CRAFRR=0;                       /* CLEAR FRR FIELD               */ 00329000
*                                                                  0317 00330000
         ST    @07,CRAFRR(,CRAREG)                                 0317 00331000
*   /*****************************************************************/ 00332000
*   /*                                                               */ 00333000
*   /* INITIALIZE APCRR                                              */ 00334000
*   /*                                                               */ 00335000
*   /*****************************************************************/ 00336000
*                                                                  0318 00337000
*   CRRREG=ADDR(CRAPSS);            /* SET UP BASE REG FOR APCRR     */ 00338000
         LR    CRRREG,@14                                          0318 00339000
*   APCPSTA=PSTREG;                 /* SAVE PST ADDR         @ZM42181*/ 00340000
         ST    PSTREG,APCPSTA(,CRRREG)                             0319 00341000
*   APCTCBA=SAVIQE->IQETCB;         /* GET TCB @ FROM THE IQE      0320 00342000
*                                                            @Y30APGA*/ 00343000
         L     @14,IQETCB(,SAVIQE)                                 0320 00344000
         ST    @14,APCTCBA(,CRRREG)                                0320 00345000
*   APCATFLG=0;                     /* CLEAR ACTIVE FLAGS            */ 00346000
         STH   @07,APCATFLG(,CRRREG)                               0321 00347000
*   APC51E=ONCON;                   /* ISTAPC51 ENTERED              */ 00348000
         OI    APC51E(CRRREG),B'10000000'                          0322 00349000
*   APCRFLG=0;                      /* CLEAR RUNNING FLAGS           */ 00350000
         STH   @07,APCRFLG(,CRRREG)                                0323 00351000
*   APC51R=ONCON;                   /* ISTAPC51 IS RUNNING           */ 00352000
         OI    APC51R(CRRREG),B'10000000'                          0324 00353000
*   APCHDRSV=APCHDRSV&&APCHDRSV;    /* CLEAR CRR HDR                 */ 00354000
         XC    APCHDRSV(16,CRRREG),APCHDRSV(CRRREG)                0325 00355000
*   APCCOMF=0;                      /* CLEAR COMMUNICATION FLAGS     */ 00356000
         ST    @07,APCCOMF(,CRRREG)                                0326 00357000
*   APCRTRN=SAVREG14;               /* SET RETURN ADDR               */ 00358000
         ST    SAVREG14,APCRTRN(,CRRREG)                           0327 00359000
*   IF RPHREG=0                     /* IS RPHREG ZERO                */ 00360000
*     THEN                          /* YES                           */ 00361000
         CR    RPHREG,@07                                          0328 00362000
         BNE   @RF00328                                            0328 00363000
*     APCSMCR=ONCON;                /* INDICATE SMS CRA BEING USED   */ 00364000
         OI    APCSMCR(CRRREG),B'00100000'                         0329 00365000
*   ELSE                                                           0330 00366000
*     DO;                           /* NO                            */ 00367000
*                                                                  0330 00368000
         B     @RC00328                                            0330 00369000
@RF00328 DS    0H                                                  0331 00370000
*       /*************************************************************/ 00371000
*       /*                                                           */ 00372000
*       /* IF NOT ZERO, WE HAVE EITHER A PRE-                        */ 00373000
*       /*                                                           */ 00374000
*       /*************************************************************/ 00375000
*                                                                  0331 00376000
*       RPHCRR=CRAREG;              /* ALLOCATED CRA OR ONE FROM   0331 00377000
*                                      REQSTORE.                     */ 00378000
         ST    CRAREG,RPHCRR(,RPHREG)                              0331 00379000
*     END;                          /* REG 1 POINTS TO THE RPH FOR 0332 00380000
*                                      THIS                          */ 00381000
*                                                                  0332 00382000
*   /*****************************************************************/ 00383000
*   /*                                                               */ 00384000
*   /* CRA. COPY ESTAE LIST INTO CRR                                 */ 00385000
*   /*                                                               */ 00386000
*   /*****************************************************************/ 00387000
*                                                                  0333 00388000
*   APCESTAL=ELIST(1:(ADDR(ENDESTA)-(ADDR(STRTESTA))));            0333 00389000
@RC00328 MVI   APCESTAL+1(CRRREG),C' '                             0333 00390000
         MVC   APCESTAL+2(70,CRRREG),APCESTAL+1(CRRREG)            0333 00391000
         LA    @14,ENDESTA                                         0333 00392000
         LA    @07,STRTESTA                                        0333 00393000
         SLR   @14,@07                                             0333 00394000
         BCTR  @14,0                                               0333 00395000
         EX    @14,@SM02447                                        0333 00396000
*   DO;                             /* LISATCVT                    0334 00397000
*                                      CVTBASE(PAGEABLE,(ATCVTBAS))  */ 00398000
*     ATCVTBAS=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00399000
         L     ATCVTBAS,ATCLCPTR                                   0335 00400000
*   END;                            /* LISATCVT                      */ 00401000
*   RETRYRN=ATCVTBAS->ATCAP61;      /* GET ADR OF ABEND EXIT RTN     */ 00402000
         L     RETRYRN,ATCAP61(,ATCVTBAS)                          0337 00403000
*   WKREG=RPHREG;                   /* SAVE CONTENTS OF REG 1        */ 00404000
