         TITLE 'ISTAPC51-DISPATCHER IRB ENTRY PROCEDURE                *00001000
                        '                                               00002000
ISTAPC51 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTAPC51  77.126'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*   PSTREG=GPR1;                    /* SAVE PST PTR IN PSTREG-(REG10)*/ 00013000
         LR    PSTREG,GPR1                                         0207 00014000
*   SAVREG14=REG14;                 /* SAVE RETURN PTR       @Z4M2181*/ 00015000
         LR    SAVREG14,REG14                                      0208 00016000
*   SAVIQE=REG0;                    /* SAVE IQE PTR          @ZM42181*/ 00017000
         LR    SAVIQE,REG0                                         0209 00018000
*   GEN(MODESET EXTKEY=ZERO);       /* INSURE KEY ZERO       @ZA13685*/ 00019000
         MODESET EXTKEY=ZERO                                            00020000
*   RFY                                                            0211 00021000
*    (REG14,                                                       0211 00022000
*     REG0) UNRSTD;                                                0211 00023000
*                                                                  0211 00024000
*   /*****************************************************************/ 00025000
*   /*                                                               */ 00026000
*   /* ATTEMPT TO GET AVAIL CRA FROM ATCVT. IF UNSUCCESSFUL, GET ONE */ 00027000
*   /* FROM STORAGE MANAGEMENT.                                      */ 00028000
*   /*                                                               */ 00029000
*   /*****************************************************************/ 00030000
*                                                                  0212 00031000
*   RFY                                                            0212 00032000
*    (COMPEVEN,                                                    0212 00033000
*     COMPODD,                                                     0212 00034000
*     SWAPEVEN,                                                    0212 00035000
*     SWAPODD) RSTD;                /*                       @Z40GKF6*/ 00036000
*   DO;                             /* LISATCVT                    0213 00037000
*                                      CVTBASE(PAGEABLE,(ATCVTBAS))  */ 00038000
*     ATCVTBAS=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00039000
         L     ATCVTBAS,ATCLCPTR                                   0214 00040000
*   END;                            /* LISATCVT GET ATCVTPTR @Z40GKF6*/ 00041000
*   RFY                                                            0216 00042000
*     ISTATCVT BASED(ATCVTBAS);     /*                       @Z40GKF6*/ 00043000
*   COMPEVEN=ATCCRAQ;               /* GET FIRST CRA ON ATCVT QUEUE     00044000
*                                                            @Z40GKF6*/ 00045000
         L     COMPEVEN,ATCCRAQ(,ATCVTBAS)                         0217 00046000
*   COMPODD=ATCCRACN;               /* GET ACTUAL AND RUNING CRA CNT    00047000
*                                                            @Z40GKF6*/ 00048000
         L     COMPODD,ATCCRACN(,ATCVTBAS)                         0218 00049000
*LOOP1:                             /*                       @ZA18470*/ 00050000
*   IF ATCCRAAC>0                   /* IS ACTUAL COUNT OF CRA'S > 0     00051000
*                                                            @ZA18470*/ 00052000
*     THEN                                                         0219 00053000
LOOP1    CLC   ATCCRAAC(2,ATCVTBAS),@CH00036                       0219 00054000
         BNH   @RF00219                                            0219 00055000
*     DO;                           /* YES - SWAP CRA OFF    @Z40GKF6*/ 00056000
*       SWAPEVEN=COMPEVEN->CRACHAIN;/* NEXT CRA              @Z40GKF6*/ 00057000
         L     SWAPEVEN,CRACHAIN(,COMPEVEN)                        0221 00058000
*       SWAPODD=COMPODD-1;          /* DEC. ACTUAL CT BY 1   @Z40GKF6*/ 00059000
         LR    SWAPODD,COMPODD                                     0222 00060000
         BCTR  SWAPODD,0                                           0222 00061000
*       CDS(COMPEVEN,SWAPEVEN,ATCCRA);/*                     @Z40GKF6*/ 00062000
         CDS   COMPEVEN,SWAPEVEN,ATCCRA(ATCVTBAS)                  0223 00063000
*       BC(NE,LOOP1);               /* IF CDS FAILS LOOP     @Z40GKF6*/ 00064000
         BC    7,LOOP1                                             0224 00065000
*       RFY                                                        0225 00066000
*         SWAPODD UNRSTD;                                          0225 00067000
*                                                                  0225 00068000
*/*SMSID  BUFF(COMPEVEN)TSKID(PSTREG)                                */ 00069000
*                                                                  0226 00070000
*       DO;                                                        0226 00071000
*         DCL                                                      0227 00072000
*           1 CMZ0009A PTR(31) BASED((COMPEVEN)-8),                0227 00073000
*            2 * BIT(8),                                           0227 00074000
*            2 CMZ0009 PTR(24);                                    0227 00075000
*         CMZ0009=(PSTREG);                                        0228 00076000
         LR    @14,COMPEVEN                                        0228 00077000
         AL    @14,@CF02444                                        0228 00078000
         STCM  PSTREG,7,CMZ0009-1(@14)                             0228 00079000
*       END;                        /* SMSID                 @Z40GKF6*/ 00080000
*       CRAREG=COMPEVEN;            /* PREALLOCATED CRA      @ZA18470*/ 00081000
         LR    CRAREG,COMPEVEN                                     0230 00082000
*       RPHREG=ADDR(CRARPH);        /* AVAILABLE SET ADDR    @ZA18470*/ 00083000
         LA    RPHREG,CRARPH(,CRAREG)                              0231 00084000
*     END;                          /* FOR NEW RPH           @ZA18470*/ 00085000
*   ELSE                            /*                       @Z40GKF6   00086000
*                                                            @ZA18470*/ 00087000
*GETSMRPH:                                                         0233 00088000
*     DO;                           /*                       @ZA18470*/ 00089000
         B     @RC00219                                            0233 00090000
@RF00219 DS    0H                                                  0233 00091000
GETSMRPH DS    0H                                                  0234 00092000
*       COMPREG=PSTFLGA&PSTSMQR0;   /* COMP REG WITH RPH QUED OFF    */ 00093000
         L     COMPREG,PSTFLGA(,PSTREG)                            0234 00094000
         N     COMPREG,@CF00784                                    0234 00095000
*       SWAPREG=COMPREG|PSTSMQR1;   /* SET UP TO SWAP ON RPH QUED BIT*/ 00096000
         LR    SWAPREG,COMPREG                                     0235 00097000
         O     SWAPREG,@CF00786                                    0235 00098000
*       CS(COMPREG,SWAPREG,PSTFLGA);/* LOCK SMS RPH VIA CS           */ 00099000
         CS    COMPREG,@06,PSTFLGA(PSTREG)                         0236 00100000
*       BC(NE,TEST);                /* IF FAIL GO FIND OUT WHY       */ 00101000
*                                                                  0237 00102000
         BC    7,TEST                                              0237 00103000
*       /*************************************************************/ 00104000
*       /*                                                           */ 00105000
*       /* IF HERE,EXCLUSIVE CONTROL OF THE SMS RPH HAS BEEN         */ 00106000
*       /* OBTAINED. SMS CAN NOW BE REQUESTED TO ALLOCATE STORAGE.   */ 00107000
*       /*                                                           */ 00108000
*       /*************************************************************/ 00109000
*                                                                  0238 00110000
*GETCRA:                                                           0238 00111000
*       RPHREG=ADDR(PSTCRRS->CRARPH);/* USE SMS RPH TO REQ STOR      */ 00112000
*                                                                  0238 00113000
GETCRA   L     RPHREG,PSTCRRS(,PSTREG)                             0238 00114000
         LA    RPHREG,CRARPH(,RPHREG)                              0238 00115000
*       /*************************************************************/ 00116000
*       /*                                                           */ 00117000
*       /* REQSTORE CBID(CRA)RPH(RPHREG)                             */ 00118000
*       /*                                                           */ 00119000
*       /*************************************************************/ 00120000
*                                                                  0239 00121000
*       DO;                                                        0239 00122000
*                                                                  0239 00123000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)                               */ 00124000
*/* END OF ATBUILD                                                   */ 00125000
*                                                                  0240 00126000
*         DCL                                                      0240 00127000
*           GPR01F FIXED(31) REG(01);                              0240 00128000
