         L     GPR15P,ATCLCPTR                                     0273 00199000
*         END;                      /* LISATCVT                      */ 00200000
*         GPR02P=GPR15P->ATCBPDA|'80000000'X;                      0275 00201000
         L     GPR02P,ATCBPDA(,GPR15P)                             0275 00202000
         O     GPR02P,@CF00762                                     0275 00203000
*         DCL                                                      0276 00204000
*           1 FMZA0010 FIXED(31) STATIC LOCAL,                     0276 00205000
*            2 * FIXED(8) INIT(56),                                0276 00206000
*            2 * PTR(24) INIT(0);                                  0276 00207000
*         GPR03F=FMZA0010;                                         0277 00208000
         L     GPR03F,FMZA0010                                     0277 00209000
*         GPR04F=23;                                               0278 00210000
         LA    GPR04F,23                                           0278 00211000
*         GPR15P=GPR15P->ATCSMRQ;                                  0279 00212000
         L     GPR15P,ATCSMRQ(,GPR15P)                             0279 00213000
