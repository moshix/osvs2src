*         RFY                                                      0269 00188000
*          (GPR01P,                                                0269 00189000
*           GPR13P) RSTD;                                          0269 00190000
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       0270 00191000
           STM GPR02P,GPR13P,RPHWORK(GPR01P)                            00192000
*         RFY                                                      0271 00193000
*          (GPR01P,                                                0271 00194000
*           GPR13P) UNRSTD;                                        0271 00195000
*         DO;                       /* LISATCVT                    0272 00196000
