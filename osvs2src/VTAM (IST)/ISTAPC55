*   RFY                                                            0270 00035000
*    (WRKA) UNRSTD;                                                0270 00036000
*   WORD0=MPSTREG|MPSGATE1;         /* MPST @ WITH HI BIT IN PARM  0271 00037000
*                                      AREA                          */ 00038000
*                                                                  0271 00039000
         LR    @07,MPSTREG                                         0271 00040000
         O     @07,@CF01056                                        0271 00041000
         ST    @07,WORD0(,PARREG)                                  0271 00042000
*   /*****************************************************************/ 00043000
*   /*                                                               */ 00044000
*   /* TO IND APC55 RUNNING                                  @Y30APGA*/ 00045000
*   /*                                                               */ 00046000
*   /*****************************************************************/ 00047000
*                                                                  0272 00048000
*   EXSWREG=0;                      /* FOR EXIT LOOP CONTROL         */ 00049000
         SLR   EXSWREG,EXSWREG                                     0272 00050000
*   MPSSYSRT=REG14;                 /* SAVE RETURN PTR TO SYSTEM     */ 00051000
         ST    REG14,MPSSYSRT(,MPSTREG)                            0273 00052000
*TESTDONE:                                                         0274 00053000
*   DO WHILE(EXSWREG=0);            /* OVERALL LOOP CONTROL          */ 00054000
TESTDONE B     @DE00274                                            0274 00055000
@DL00274 DS    0H                                                  0275 00056000
*     POSTPBSW=POSTPBSW&SWOFF;      /* RESET POSTED PAB SW   @ZM30835*/ 00057000
         N     POSTPBSW,@CF01054                                   0275 00058000
*     IF MPSSRBO=ON|MPSCRAO=ON      /* SMS REQUEST OUTSTANDING       */ 00059000
*       THEN                                                       0276 00060000
         TM    MPSSRBO(MPSTREG),B'11000000'                        0276 00061000
         BZ    @RF00276                                            0276 00062000
*SMSPAB:                                                           0277 00063000
*       DO;                         /*                               */ 00064000
SMSPAB   DS    0H                                                  0278 00065000
*/* FROM HERE TO THE STATEMENT-END SMSPAB- IS THE SMSPAB ROUTINE.    */ 00066000
*/* THE PABS ON THE MPST READY QUEUE ARE INSPECTED TO SEE IF A STOR. */ 00067000
*/* MGT. PAB OR POSTED PAB CAN BE FOUND.IF NEITHER FOUND, THE MPST   */ 00068000
*/* RDY Q IS UNGATED AND RETURN IS MADE TO THE SYSTEM.  IF THE SMS   */ 00069000
*/* PAB IS FOUND, IT IS DEQUEUED FROM THE MPST READY QUEUE. THE SRB  */ 00070000
*/* OR CRA STORAGE RETURNED IS PUT ON THE CRA OR SRB AVAILABLE QUE   */ 00071000
*/* OF THE PST. IF A POSTED PAB IS FOUND, SEARCHING STOPS AND THE    */ 00072000
*/* POSTED PAB WILL BE PASSED TO SUBSEQUENT ROUTINES FOR DISPATCH.   */ 00073000
*/* IF NOT IRB TYPE, SRB55 WILL BE SCHEDULED TO REDRIVE THIS MODULE. */ 00074000
*                                                                  0278 00075000
*                                   /*                               */ 00076000
*         RFY                                                      0278 00077000
*          (SMPABREG,                                              0278 00078000
*           NWODDREG,                                              0278 00079000
*           DYREG,                                                 0278 00080000
*           OLODDREG) RSTD;                                        0278 00081000
*         SVPTRREG=MPSRQUE;         /* FIRST PAB ON READY QUE        */ 00082000
         L     SVPTRREG,MPSRQUE(,MPSTREG)                          0279 00083000
*         CURPBREG=SVPTRREG&ZCLHIBYT;/* FIRST RDY Q PAB WITH HIGH  0280 00084000
*                                      ORDER BYTE CLEAR              */ 00085000
         LR    CURPBREG,SVPTRREG                                   0280 00086000
         N     CURPBREG,@CF02683                                   0280 00087000
*         BKPABREG=0;               /* IND. NO PREV. PAB             */ 00088000
         SLR   @02,@02                                             0281 00089000
         LR    BKPABREG,@02                                        0281 00090000
*         DYREG=ADDR(MPSSMPAB);     /* SMS DYN PAB WITH HDR          */ 00091000
         LA    DYREG,MPSSMPAB(,MPSTREG)                            0282 00092000
*         SMPABREG=ADDR(DYPPAB);    /* SMS PAB WITHOUT HDR           */ 00093000
         LA    SMPABREG,DYPPAB(,DYREG)                             0283 00094000
*         SRDSWREG=0;               /* SEARCH MPST RDY QUE FOR SMS 0284 00095000
*                                      PAB-NOT DONE                  */ 00096000
         LR    SRDSWREG,@02                                        0284 00097000
*SMSPAB1:                                                          0285 00098000
*         DO WHILE(SRDSWREG=0);     /* SEARCH LOOP CONTROL           */ 00099000
SMSPAB1  B     @DE00285                                            0285 00100000
@DL00285 DS    0H                                                  0286 00101000
*           IF CURPBREG>0           /* MORE PABS ON RDY QUE          */ 00102000
*             THEN                                                 0286 00103000
         LTR   CURPBREG,CURPBREG                                   0286 00104000
         BNP   @RF00286                                            0286 00105000
*SMSPAB2:                                                          0287 00106000
*             DO;                                                  0287 00107000
SMSPAB2  DS    0H                                                  0288 00108000
*               IF CURPBREG=SMPABREG|PABRPHA^=0/* PAB EQUAL SMS PAB     00109000
*                                      POSTED PAB?           @ZM30835*/ 00110000
*                 THEN                                             0288 00111000
         CR    CURPBREG,SMPABREG                                   0288 00112000
         BE    @RT00288                                            0288 00113000
         L     @02,PABRPHA-1(,CURPBREG)                            0288 00114000
         LA    @02,0(,@02)                                         0288 00115000
         LTR   @02,@02                                             0288 00116000
         BZ    @RF00288                                            0288 00117000
@RT00288 DS    0H                                                  0289 00118000
*SMSPAB3:                                                          0289 00119000
*                 DO;                                              0289 00120000
SMSPAB3  DS    0H                                                  0290 00121000
*                   IF BKPABREG=0   /* FIRST PAB WAS SMS PAB OR    0290 00122000
*                                      POSTED PAB            @ZM30835*/ 00123000
*                     THEN          /* SO NEED CS TO TAKE OFF        */ 00124000
         LTR   BKPABREG,BKPABREG                                   0290 00125000
         BNZ   @RF00290                                            0290 00126000
*SMSPAB4:                                                          0291 00127000
*                     DO;           /* MPST READY QUEUE              */ 00128000
SMSPAB4  DS    0H                                                  0292 00129000
*                       NEWREG=PABCHNGP|(SVPTRREG&ZCLWBYTS);/*     0292 00130000
*                                      FORWARD PAB PTR WITH MPST RDY    00131000
*                                      QUE FLAG BITS FOR CS          */ 00132000
         LR    NEWREG,SVPTRREG                                     0292 00133000
         N     NEWREG,@CF01103                                     0292 00134000
         L     @02,PABCHNGP-1(,CURPBREG)                           0292 00135000
         LA    @02,0(,@02)                                         0292 00136000
         OR    NEWREG,@02                                          0292 00137000
*                       CS(SVPTRREG,NEWREG,MPSRQUE);               0293 00138000
         CS    SVPTRREG,@06,MPSRQUE(MPSTREG)                       0293 00139000
*                       BC(NE,SMSPABA);/* CS FAILED-GO AGAIN         */ 00140000
         BC    7,SMSPABA                                           0294 00141000
*                       SRDSWREG=1; /* CS EQUAL-SEARCH DONE          */ 00142000
         LA    SRDSWREG,1                                          0295 00143000
*                       GOTO SMSPABB;                              0296 00144000
         B     SMSPABB                                             0296 00145000
*SMSPABA:                                                          0297 00146000
*                       CURPBREG=SVPTRREG&ZCLHIBYT;/* RELOAD WITH  0297 00147000
*                                      WHAT IS                       */ 00148000
SMSPABA  LR    CURPBREG,SVPTRREG                                   0297 00149000
         N     CURPBREG,@CF02683                                   0297 00150000
*SMSPABB:                           /* NOW ON READY QUEUE            */ 00151000
*                     END SMSPAB4;                                 0298 00152000
*                   ELSE            /* NOT FIRST PAB ON MPST         */ 00153000
*SMSPAB5:                                                          0299 00154000
*                     DO;           /* READY QUEUE-NO CS TODQ        */ 00155000
         B     @RC00290                                            0299 00156000
@RF00290 DS    0H                                                  0299 00157000
SMSPAB5  DS    0H                                                  0300 00158000
*                       BKPABREG->PABCHNGP=PABCHNGP;/* CHAIN PREVIOUS   00159000
*                                      PAB TO PAB NEXT AFTER SMS PAB */ 00160000
         MVC   PABCHNGP(3,BKPABREG),PABCHNGP(CURPBREG)             0300 00161000
*                       SRDSWREG=1; /* SEARCH MPST RDY Q FOR SMS   0301 00162000
*                                      PAB-SET IS DONE SW            */ 00163000
         LA    SRDSWREG,1                                          0301 00164000
*                     END SMSPAB5;                                 0302 00165000
