         BNZ   @RF00723                                            0723 01601000
*ENQUE2:                                                           0724 01602000
*       DO;                         /* MPST RDY Q VIA CS             */ 01603000
ENQUE2   DS    0H                                                  0725 01604000
*         CURPAREG=(CURPAREG&ZCLHIBYT)|(CURRQREG&ZCLWBYTS);        0725 01605000
*                                                                  0725 01606000
         LR    @07,CURPAREG                                        0725 01607000
         N     @07,@CF02683                                        0725 01608000
         LR    @05,CURRQREG                                        0725 01609000
         N     @05,@CF01103                                        0725 01610000
         OR    @07,@05                                             0725 01611000
         LR    CURPAREG,@07                                        0725 01612000
*/*CURPAREG NOW CONTAINS THE DEQUED PAB WHICH IS TO BE ENQUED. THE   */ 01613000
*/*HI ORDER BYTE HAS BEEN CLEARED OF WHAT WAS PASSED TO THIS SUBROUT.*/ 01614000
*/*AND REPLACED BY FLAGS PRESENTLY ON MPST READY QUEUE.              */ 01615000
*                                                                  0726 01616000
*         CS(CURRQREG,CURPAREG,MPSRQUE);/* EQ-RDY Q HAS CURPAREG     */ 01617000
         CS    CURRQREG,@04,MPSRQUE(MPSTREG)                       0726 01618000
*         BC(NE,ENQUEA);            /* AS EXPLAINED ABOVE            */ 01619000
         BC    7,ENQUEA                                            0727 01620000
*         WORD1=0;                                                 0728 01621000
         SLR   @07,@07                                             0728 01622000
         ST    @07,WORD1(,PARREG)                                  0728 01623000
*         EQDSWREG=1;               /* OUT OF ENQUE LOOP CTRL        */ 01624000
         LA    EQDSWREG,1                                          0729 01625000
*ENQUEA:                                                           0730 01626000
*         PABENREG=CURRQREG&ZCLHIBYT;/* FAIL CS-PUT PABPTR NOW       */ 01627000
ENQUEA   LR    PABENREG,CURRQREG                                   0730 01628000
         N     PABENREG,@CF02683                                   0730 01629000
*       END ENQUE2;                 /* ON RDY Q LESS FLGS            */ 01630000
*     ELSE                          /* MPST RDY QUE NOT EMPT         */ 01631000
*ENQUE3:                                                           0732 01632000
*       DO;                                                        0732 01633000
         B     @RC00723                                            0732 01634000
@RF00723 DS    0H                                                  0732 01635000
ENQUE3   DS    0H                                                  0733 01636000
*         IF PABENREG->PABCHNGP=0   /* LAST PAB ON QUEUE             */ 01637000
*           THEN                                                   0733 01638000
         L     @07,PABCHNGP-1(,PABENREG)                           0733 01639000
         LA    @07,0(,@07)                                         0733 01640000
         LTR   @07,@07                                             0733 01641000
         BNZ   @RF00733                                            0733 01642000
*ENQUE4:                                                           0734 01643000
*           DO;                     /* YES-PUT DEQUED PAB ON         */ 01644000
ENQUE4   DS    0H                                                  0735 01645000
*             PABENREG->PABCHNGP=CURPAREG;/* BACK OF MPST RDY QUE    */ 01646000
         STCM  CURPAREG,7,PABCHNGP(PABENREG)                       0735 01647000
*             WORD1=0;                                             0736 01648000
         SLR   @07,@07                                             0736 01649000
         ST    @07,WORD1(,PARREG)                                  0736 01650000
*             EQDSWREG=1;           /* OUT OF ENQUE LOOP CTRL        */ 01651000
         LA    EQDSWREG,1                                          0737 01652000
*           END ENQUE4;                                            0738 01653000
*         ELSE                      /* NOT LAST PAB ON QUEUE         */ 01654000
*ENQUE5:                                                           0739 01655000
*           PABENREG=PABENREG->PABCHNGP;/* STEP TO NEXT PAB          */ 01656000
         B     @RC00733                                            0739 01657000
@RF00733 DS    0H                                                  0739 01658000
ENQUE5   L     @07,PABCHNGP-1(,PABENREG)                           0739 01659000
         LA    @07,0(,@07)                                         0739 01660000
         LR    PABENREG,@07                                        0739 01661000
*       END ENQUE3;                                                0740 01662000
@RC00733 DS    0H                                                  0741 01663000
*   END;                            /* DO WHILE(EQDSWREG=0)          */ 01664000
@RC00723 DS    0H                                                  0741 01665000
@DE00722 LTR   EQDSWREG,EQDSWREG                                   0741 01666000
         BZ    @DL00722                                            0741 01667000
*   END ENQUE;                                                     0742 01668000
*                                                                  0742 01669000
@EL00002 DS    0H                                                  0742 01670000
@EF00002 DS    0H                                                  0742 01671000
@ER00002 BR    @14                                                 0742 01672000
@PB00002 DS    0H                                                  0743 01673000
*/* TPEPILOG                                                         */ 01674000
*                                                                  0743 01675000
*   GEN(PRINT OFF);                                                0743 01676000
         PRINT OFF                                                      01677000
*   GEN(PRINT ON);                                                 0744 01678000
*                                                                  0744 01679000
         PRINT ON                                                       01680000
*/* END OF TPEPILOG                                                  */ 01681000
