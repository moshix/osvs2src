         TITLE 'ISTAPC55-MPST DISPATCHER                               *00001000
                        '                                               00002000
ISTAPC55 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTAPC55  77.266'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*                                                                  0263 00013000
*/********************************************************************/ 00014000
*/*                                                                  */ 00015000
*/*                           CODE                                   */ 00016000
*/*                                                                  */ 00017000
*/********************************************************************/ 00018000
*                                                                  0263 00019000
*                                   /*                               */ 00020000
*   MPSTREG=REG1;                   /* MPST ADDRESS FROM INPUT       */ 00021000
         LR    MPSTREG,REG1                                        0263 00022000
*   RFY                                                            0264 00023000
*    (WRKA) RSTD;                                                  0264 00024000
*   DO;                             /* LISATCVT                    0265 00025000
*                                      CVTBASE(PAGEABLE,(VCVTREG))   */ 00026000
*     VCVTREG=ATCLCPTR;             /* SET UP ATCVT POINTER          */ 00027000
         L     VCVTREG,ATCLCPTR                                    0266 00028000
*   END;                            /* LISATCVT VTAM CVT PTR         */ 00029000
*   FRRPREG=ATCAP66;                /* FOR RECOVERY                  */ 00030000
         L     FRRPREG,ATCAP66(,VCVTREG)                           0268 00031000
*   GENERATE REFS(FRRPREG,PARREG,WRKA,WRKB,PSACSTK,PSA,FRRSCURR,FRRS,   00032000
*       FRRSLAST,FRRSELEN,FRRSFRRA,FRRSENTR,FRRSPARM,PSALSFCC);    0269 00033000
