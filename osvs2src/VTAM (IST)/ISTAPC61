*     R1,                                                          0529 00744000
*     R2) UNRSTD;                   /*                       @OZ04697*/ 00745000
*PSTCLN:                                                           0530 00746000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0530 00747000
*                                                                  0530 00748000
         B     @PB00002                                            0530 00749000
PSTCLN   DS    0H                                                  0531 00750000
*/*******************************************************************   00751000
*   THIS SUBROUTINE IS WRITTEN IN SUPPORT OF APAR OZ04697.  IT IS  0531 00752000
* * CALLED WHEN THE PROCESS WHICH ABENDED IS THE VTAM TASK ITSELF, 0531 00753000
* * PROVIDED THAT A CRA IS AVAILABLE AND THE ERROR DID NOT OCCUR IN     00754000
* * SMS (IN WHICH CASE A RETRY WILL BE ATTEMPTED).  THE PURPOSE OF 0531 00755000
* * THE SUBROUTINE IS TO CLEAN UP RESOURCES ASSOCIATED WITH THE    0531 00756000
* * PROCESS WHICH ABENDED, SO THAT PROCESSES NECESSARY FOR THE     0531 00757000
* * ORDERLY TERMINATION OF VTAM WILL BE DISPATCHABLE.              0531 00758000
* *                                                                0531 00759000
* * ON INPUT, REGISTER 7 POINTS TO THE PSS RECOVERY  RECORD        0531 00760000
* *           REGISTER 9 HOLDS THE SYSTEM RETURN ADDR              0531 00761000
* *           REGISTER 10 POINTS TO SAVE AREA IF SDWA WAS PROVIDED 0531 00762000
* *           REGISTER 11 IS THE BASE REGISTER                     0531 00763000
* *           REGISTER 12 POINTS TO THE CRA                        0531 00764000
* *           REGISTER 14 IS THE RETURN ADDR FOR THE SUBROUTINE CALL    00765000
* *                                                                0531 00766000
* *  REGISTERS 9,10,11, AND 14 WILL BE RETURNED INTACT. THE VALUE  0531 00767000
* *  OF ALL OTHER REGISTERS IS UNPREDICTABLE.  THE CRA ASSOCIATED  0531 00768000
* *  WITH THE ABENDING TASK WILL BE MARKED PURGE COMPLETE IF IT IS 0531 00769000
* *  TPWAITING.  IF ALREADY TPPOSTED, THE CRA WILL BE RELSTORED    0531 00770000
* *****************************************************************  */ 00771000
*                                                                  0531 00772000
*   /*****************************************************************/ 00773000
*   /*                                                               */ 00774000
*   /* REGISTER DECLARES FOR PSTCLN                                  */ 00775000
*   /*                                                               */ 00776000
*   /*****************************************************************/ 00777000
*                                                                  0531 00778000
*   DCL                                                            0531 00779000
*     SUBR0 PTR(31) REG(0);         /* WORK REG FOR LOCK RELEASE LOOP*/ 00780000
*   DCL                                                            0532 00781000
*     SUBRPHP PTR(31) REG(1);       /* PTR TO CRARPH                 */ 00782000
*   DCL                                                            0533 00783000
*     SUBCOMP PTR(31) REG(2);       /* CS COMPARE REG                */ 00784000
*   DCL                                                            0534 00785000
*     SUBWK3 PTR(31) REG(3);        /* WK REG FOR RECOVERY DYPAB   0534 00786000
*                                      INIT.                         */ 00787000
*   DCL                                                            0535 00788000
*     SUBCODD PTR(31) REG(3);       /* CDS ODD COMPARE REG           */ 00789000
*   DCL                                                            0536 00790000
*     SUBBACK PTR(31) REG(3);       /* BACK POINTER FOR RPHPABQ    0536 00791000
*                                      SEARCH                        */ 00792000
*   DCL                                                            0537 00793000
*     SUBWK4 PTR(31) REG(4);        /* WK REG FOR RECOVERY DYPAB   0537 00794000
*                                      INIT.                         */ 00795000
*   DCL                                                            0538 00796000
*     SUBSWAP PTR(31) REG(4);       /* CS SWAP REGISTER              */ 00797000
*   DCL                                                            0539 00798000
*     SUBSODD PTR(31) REG(5);       /* CDS ODD SWAP REG              */ 00799000
*   DCL                                                            0540 00800000
*     SUBPAB32 PTR(31) REG(5);      /* INPUT PARAM FOR ISTAPC32 CALL */ 00801000
*   DCL                                                            0541 00802000
*     SUBAPC PTR(31) REG(7) RSTD;   /* PTR TO PSS RECOVERY RECORD    */ 00803000
*   DCL                                                            0542 00804000
*     SUBATCVT PTR(31) REG(8);      /* ATCVT PTR                     */ 00805000
*   DCL                                                            0543 00806000
*     SUBPABP PTR(31) REG(8);       /* PTR TO CURRENT PAB            */ 00807000
*   DCL                                                            0544 00808000
*     SUBCRA PTR(31) REG(12) RSTD;  /* PTR TO CRA                    */ 00809000
*   DCL                                                            0545 00810000
*     SUBPSTP PTR(31) REG(12);      /* SAVE PST@ OVER CALL TO APC64  */ 00811000
*   DCL                                                            0546 00812000
*     SUBSAV14 PTR(31) REG(13) RSTD;/* SAVE RETURN ADDRESS           */ 00813000
*   DCL                                                            0547 00814000
*     SUBR14 PTR(31) REG(14);       /* SUBRTN CALL RETURN ADDR       */ 00815000
*   DCL                                                            0548 00816000
*     SUBCALLR PTR(31) REG(15);     /* ENTRY PT ADDR FOR CALLS       */ 00817000
*                                                                  0548 00818000
*   /*****************************************************************/ 00819000
*   /*                                                               */ 00820000
*   /* RESPECIFIES FOR PSTCLN                                        */ 00821000
*   /*                                                               */ 00822000
*   /*****************************************************************/ 00823000
*                                                                  0549 00824000
*   RFY                                                            0549 00825000
*     ISTAPCRR BASED(SUBAPC);                                      0549 00826000
*   RFY                                                            0550 00827000
*     ISTPST BASED(APCPSTA);                                       0550 00828000
*   RFY                                                            0551 00829000
*     ISTRPH BASED(SUBRPHP);                                       0551 00830000
*   RFY                                                            0552 00831000
*     ISTPAB BASED(SUBPABP);                                       0552 00832000
*   RFY                                                            0553 00833000
*     ISTCRA BASED(SUBCRA);                                        0553 00834000
*                                                                  0553 00835000
*   /*****************************************************************/ 00836000
*   /*                                                               */ 00837000
*   /* CONSTANTS FOR PSTCLN                                          */ 00838000
*   /*                                                               */ 00839000
*   /*****************************************************************/ 00840000
*                                                                  0554 00841000
*   DCL                                                            0554 00842000
*     SUBPGON BIT(32) CONSTANT('00000010'X);/* TURN RPHPURGE ON      */ 00843000
*   DCL                                                            0555 00844000
*     SUBON BIT(1) CONSTANT('1'B);  /* TURN A BIT ON                 */ 00845000
*   DCL                                                            0556 00846000
*     SUBOFF BIT(1) CONSTANT('0'B); /* TURN A BIT OFF                */ 00847000
*                                                                  0556 00848000
*   /*****************************************************************/ 00849000
*   /*                                                               */ 00850000
*   /* BASED VARIABLES                                               */ 00851000
*   /*                                                               */ 00852000
*   /*****************************************************************/ 00853000
*                                                                  0557 00854000
*   DCL                                                            0557 00855000
*     ISTAPC64 ENTRY BASED(SUBCALLR) VALRG(0);                     0557 00856000
*   DCL                                                            0558 00857000
*     ISTAPC41 ENTRY BASED(SUBCALLR) VALRG(0);                     0558 00858000
*   DCL                                                            0559 00859000
*     ISTAPC32 ENTRY BASED(SUBCALLR) VALRG(0);                     0559 00860000
*   DCL                                                            0560 00861000
*     RCOVPAB CHAR(LENGTH(ISTDYPAB)) DEFINED(CRAPROCR);            0560 00862000
*                                                                  0561 00863000
*   /*****************************************************************/ 00864000
*   /*                                                               */ 00865000
*   /* START OF CODE                                                 */ 00866000
*   /*                                                               */ 00867000
*   /*****************************************************************/ 00868000
*                                                                  0561 00869000
*   RFY                                                            0561 00870000
*    (SUBATCVT,                                                    0561 00871000
*     SUBRPHP) RSTD;                                               0561 00872000
*   SUBSAV14=SUBR14;                /* SAVE RETURN ADDRESS           */ 00873000
         LR    SUBSAV14,SUBR14                                     0562 00874000
*   DO;                             /* LISATCVT                    0563 00875000
*                                      CVTBASE(PAGEABLE,(SUBATCVT))  */ 00876000
*     SUBATCVT=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00877000
         L     SUBATCVT,ATCLCPTR                                   0564 00878000
*   END;                            /* LISATCVT GET ATCVT ADDRESS    */ 00879000
*   SUBATCVT->ATCVTMAB=SUBON;       /* VTAM ABENDING FLAG ON         */ 00880000
         L     @06,ATCVTMAB-3(,SUBATCVT)                           0566 00881000
@GL00004 LR    @04,@06                                             0566 00882000
         O     @04,@CF03785                                        0566 00883000
         CS    @06,@04,ATCVTMAB-3(SUBATCVT)                        0566 00884000
         BNE   @GL00004                                            0566 00885000
*   RFY                                                            0567 00886000
*     SUBATCVT UNRSTD;                                             0567 00887000
*   SUBRPHP=ADDR(CRARPH);           /* PICK UP PTR TO CRA'S RPH      */ 00888000
         LA    SUBRPHP,CRARPH(,SUBCRA)                             0568 00889000
*   RPHPURGE=SUBON;                 /* MARK PURGE IN PROGRESS IN RPH */ 00890000
         OI    RPHPURGE(SUBRPHP),B'00000010'                       0569 00891000
*   RFY                                                            0570 00892000
*    (SUBCOMP,                                                     0570 00893000
*     SUBSWAP) RSTD;                                               0570 00894000
*                                                                  0570 00895000
*   /*****************************************************************/ 00896000
*   /*                                                               */ 00897000
*   /* GATE THE PST READY QUEUE TO PREVENT FURTHER USE AT THIS TIME  */ 00898000
*   /*                                                               */ 00899000
*   /*****************************************************************/ 00900000
*                                                                  0571 00901000
*   SUBCOMP=PSTRQUE;                /* READY Q PTR                   */ 00902000
         L     @06,APCPSTA(,SUBAPC)                                0571 00903000
         L     SUBCOMP,PSTRQUE(,@06)                               0571 00904000
*SUB02:                                                            0572 00905000
*   SUBSWAP=SUBCOMP|PSTGATE1;       /* TURN GATE BIT ON FOR SWAP     */ 00906000
SUB02    LR    SUBSWAP,SUBCOMP                                     0572 00907000
         O     SUBSWAP,@CF01889                                    0572 00908000
*   CS(SUBCOMP,SUBSWAP,PSTRQUE);    /* TURN IT ON IN PST             */ 00909000
         L     @06,APCPSTA(,SUBAPC)                                0573 00910000
         CS    SUBCOMP,@04,PSTRQUE(@06)                            0573 00911000
*   BC(7,SUB02);                    /* IF FAIL, RETRY                */ 00912000
*                                                                  0574 00913000
         BC    7,SUB02                                             0574 00914000
*   /*****************************************************************/ 00915000
*   /*                                                               */ 00916000
*   /* IF A PAB EXISTS, TRY TO RESCHEDULE IT                         */ 00917000
*   /*                                                               */ 00918000
*   /*****************************************************************/ 00919000
*                                                                  0575 00920000
*   RFY                                                            0575 00921000
*     SUBPABP RSTD;                                                0575 00922000
*   SUBPABP=RPHMAJCB+RPHPABOF;      /* ADDR OF CURRENT PAB FROM RPH  */ 00923000
         L     SUBPABP,RPHMAJCB-1(,SUBRPHP)                        0576 00924000
         LA    SUBPABP,0(,SUBPABP)                                 0576 00925000
         SLR   @06,@06                                             0576 00926000
         IC    @06,RPHPABOF(,SUBRPHP)                              0576 00927000
         ALR   SUBPABP,@06                                         0576 00928000
*   RFY                                                            0577 00929000
*    (SUBCODD,                                                     0577 00930000
*     SUBSODD) RSTD;                                               0577 00931000
*   IF SUBPABP^=0                   /* IF THERE IS A PAB             */ 00932000
*     THEN                                                         0578 00933000
         LTR   SUBPABP,SUBPABP                                     0578 00934000
         BZ    @RF00578                                            0578 00935000
*     DO;                           /* YES, THERE IS A PAB           */ 00936000
*       IF(CRACRR^=ADDR(CRAPSS))|   /* IF PSS NOT IN CONTROL         */ 00937000
*           ((APC31R=SUBON)&(APCRESCH=SUBOFF))|/* OR APC31 WAS     0580 00938000
*                                      RUNNING BUT RESCHED NOT YET 0580 00939000
*                                      DONE                          */ 00940000
*           ((APC31R=SUBOFF)&(PABRPHA=SUBRPHP))/* OR APC31 WAS NOT 0580 00941000
*                                      RUNNING BUT PAB NOT YET MOVED    00942000
*                                      TO RPHPABQ                    */ 00943000
*         THEN                                                     0580 00944000
         LA    @06,CRAPSS(,SUBCRA)                                 0580 00945000
         C     @06,CRACRR(,SUBCRA)                                 0580 00946000
         BNE   @RT00580                                            0580 00947000
         TM    APC31R(SUBAPC),B'00000001'                          0580 00948000
         BNO   @GL00005                                            0580 00949000
         TM    APCRESCH(SUBAPC),B'01000000'                        0580 00950000
         BZ    @RT00580                                            0580 00951000
@GL00005 TM    APC31R(SUBAPC),B'00000001'                          0580 00952000
         BNZ   @RF00580                                            0580 00953000
         L     @06,PABRPHA-1(,SUBPABP)                             0580 00954000
         LA    @06,0(,@06)                                         0580 00955000
         CR    SUBRPHP,@06                                         0580 00956000
         BNE   @RF00580                                            0580 00957000
@RT00580 DS    0H                                                  0581 00958000
*         DO;                       /* THEN TRY TO RESCHEDULE        */ 00959000
*           SUBCOMP=PABCHAIN;       /* OBTAIN RESCHEDULE FLAG-WORD   */ 00960000
         L     SUBCOMP,PABCHAIN(,SUBPABP)                          0582 00961000
*SUB01:                                                            0583 00962000
*           SUBSWAP=SUBCOMP&PABRESC0;/* TURN OF PABRESCH IN REG      */ 00963000
SUB01    LR    SUBSWAP,SUBCOMP                                     0583 00964000
         N     SUBSWAP,@CF01893                                    0583 00965000
*           CS(SUBCOMP,SUBSWAP,PABCHAIN);/* TRY TO TURN OFF IN PAB   */ 00966000
         CS    SUBCOMP,@04,PABCHAIN(SUBPABP)                       0584 00967000
*           BC(7,SUB01);            /* TRY AGAIN IF FAIL             */ 00968000
         BC    7,SUB01                                             0585 00969000
*           PABRPHA=0;              /* DISCONNECT PAB FROM CURRENT 0586 00970000
*                                      RPH                           */ 00971000
         SLR   @06,@06                                             0586 00972000
         STCM  @06,7,PABRPHA(SUBPABP)                              0586 00973000
*           LM(SUBCOMP,SUBCODD,PABWQCHN);/* PICK UP WK ELEM Q PTR AND   00974000
*                                      PAB CHAIN PTR                 */ 00975000
         LM    SUBCOMP,SUBCODD,PABWQCHN(SUBPABP)                   0587 00976000
*SUB03:                                                            0588 00977000
*           IF((SUBCOMP&'00FFFFFF'X)^=0)|/* IF PAB WK ELEM QUEUE NOT    00978000
*                                      EMPTY (ADDR FLD ^= 0)         */ 00979000
*               ((SUBCODD&PABUNCO1)^=0)/* OR PAB IS TO BE          0588 00980000
*                                      UNCONDITIONALLY RESCHED.      */ 00981000
*             THEN                                                 0588 00982000
SUB03    SLR   @06,@06                                             0588 00983000
         LR    @00,SUBCOMP                                         0588 00984000
         N     @00,@CF03757                                        0588 00985000
         CR    @00,@06                                             0588 00986000
         BNE   @RT00588                                            0588 00987000
         LR    @00,SUBCODD                                         0588 00988000
         N     @00,@CF01907                                        0588 00989000
         CR    @00,@06                                             0588 00990000
         BE    @RF00588                                            0588 00991000
@RT00588 DS    0H                                                  0589 00992000
*             DO;                   /* THEN RESCHEDULE PAB           */ 00993000
*SUB06:                                                            0590 00994000
*               SUBSODD=SUBCODD&PABUNCO0;/* TURN OFF UNCOND. RESCHED    00995000
*                                      BIT IN REG.                   */ 00996000
SUB06    LR    SUBSODD,SUBCODD                                     0590 00997000
         N     SUBSODD,@CF01905                                    0590 00998000
*               CS(SUBCODD,SUBSODD,PABCHAIN);/* TURN OFF IN PAB      */ 00999000
         CS    SUBCODD,@05,PABCHAIN(SUBPABP)                       0591 01000000
*               BC(7,SUB06);        /* IF FAIL, TRY AGAIN            */ 01001000
         BC    7,SUB06                                             0592 01002000
*               PABCHNGP=RPHPABQP;  /* IF SUCCEED, MOVE PAB TO     0593 01003000
*                                      RPHPABQ                       */ 01004000
         MVC   PABCHNGP(3,SUBPABP),RPHPABQP(SUBRPHP)               0593 01005000
*               RPHPABQP=SUBPABP;   /* FOR RESCHEDULE                */ 01006000
         STCM  SUBPABP,7,RPHPABQP(SUBRPHP)                         0594 01007000
*             END;                  /* END WORK EL. QUEUE NOT EMPTY  */ 01008000
*           ELSE                    /* ELSE WORK EL. QUEUE IS EMPTY  */ 01009000
*             DO;                   /* RESET PAB FLAGS FOR NO      0596 01010000
*                                      RESCHEDULE                    */ 01011000
         B     @RC00588                                            0596 01012000
@RF00588 DS    0H                                                  0597 01013000
*               SUBSWAP=SUBCOMP&PABWEQG0;/* SET TO UNGATE PAB WK ELEM   01014000
*                                      Q                             */ 01015000
         L     @06,@CF01887                                        0597 01016000
         LR    SUBSWAP,SUBCOMP                                     0597 01017000
         NR    SUBSWAP,@06                                         0597 01018000
*               SUBSODD=SUBCODD&PABCHNG0;/* SET TO UNGATE PAB CHAIN  */ 01019000
         NR    @06,SUBCODD                                         0598 01020000
         LR    SUBSODD,@06                                         0598 01021000
*               CDS(SUBCOMP,SUBSWAP,PABWQCHN);/* UNGATE PAB CHAIN AND   01022000
*                                      PAB WK ELEMENT Q              */ 01023000
         CDS   SUBCOMP,SUBSWAP,PABWQCHN(SUBPABP)                   0599 01024000
*               BC(7,SUB03);        /* IF FAIL, MUST REEXAMINE WK EL    01025000
*                                      Q                             */ 01026000
         BC    7,SUB03                                             0600 01027000
*             END;                  /* END OF RESET FLAGS FOR NO   0601 01028000
*                                      RESCHED.                      */ 01029000
*         END;                      /* END OF TRY TO RESCHEDULE      */ 01030000
*     END;                          /* END OF THERE IS A PAB         */ 01031000
*   ELSE                                                           0604 01032000
*     ;                             /* NO PROCESSING TO DO IF NO PAB */ 01033000
@RF00578 DS    0H                                                  0605 01034000
*   RFY                                                            0605 01035000
*    (SUBCODD,                                                     0605 01036000
*     SUBSODD,                                                     0605 01037000
*     SUBSWAP,                                                     0605 01038000
*     SUBCOMP) UNRSTD;                                             0605 01039000
@RC00578 DS    0H                                                  0606 01040000
*                                                                  0606 01041000
*   /*****************************************************************/ 01042000
*   /*                                                               */ 01043000
*   /* RESCHEDULE ALL PABS QUEUED ON RPHPABQ                         */ 01044000
*   /*                                                               */ 01045000
*   /*****************************************************************/ 01046000
*                                                                  0606 01047000
*   RFY                                                            0606 01048000
*    (SUBBACK) RSTD;                                               0606 01049000
*   DO WHILE(RPHPABQP^=0);          /* EMPTY RPHPABQ OF ALL PABS     */ 01050000
         B     @DE00607                                            0607 01051000
@DL00607 DS    0H                                                  0608 01052000
*     SUBBACK=0;                    /* INITIALIZE BACK PTR           */ 01053000
         SLR   SUBBACK,SUBBACK                                     0608 01054000
*     SUBPABP=RPHPABQP;             /* GET 1ST PAB ON Q              */ 01055000
         L     SUBPABP,RPHPABQP-1(,SUBRPHP)                        0609 01056000
         LA    SUBPABP,0(,SUBPABP)                                 0609 01057000
*     DO WHILE(PABCHNGP^=0);        /* FIND END OF Q                 */ 01058000
         B     @DE00610                                            0610 01059000
@DL00610 DS    0H                                                  0611 01060000
*       SUBBACK=SUBPABP;            /* SAVE CURRENT PAB PTR          */ 01061000
         LR    SUBBACK,SUBPABP                                     0611 01062000
*       SUBPABP=PABCHNGP;           /* PICK UP NEXT PAB PTR          */ 01063000
         L     @06,PABCHNGP-1(,SUBPABP)                            0612 01064000
         LA    @06,0(,@06)                                         0612 01065000
         LR    SUBPABP,@06                                         0612 01066000
*     END;                          /* END DOWHILE TO END OF Q       */ 01067000
@DE00610 L     @06,PABCHNGP-1(,SUBPABP)                            0613 01068000
         LA    @06,0(,@06)                                         0613 01069000
         LTR   @06,@06                                             0613 01070000
         BNZ   @DL00610                                            0613 01071000
*     IF SUBBACK=0                  /* WAS ONLY ONE PAB ON Q?        */ 01072000
*       THEN                        /* YES                           */ 01073000
         SLR   @06,@06                                             0614 01074000
         CR    SUBBACK,@06                                         0614 01075000
         BNE   @RF00614                                            0614 01076000
*       RPHPABQP=0;                 /* DEQUEUE IT FROM RPH Q         */ 01077000
         STCM  @06,7,RPHPABQP(SUBRPHP)                             0615 01078000
*     ELSE                                                         0616 01079000
*       SUBBACK->PABCHNGP=0;        /* ELSE DEQUEUE LAST PAB ON CHAIN*/ 01080000
         B     @RC00614                                            0616 01081000
@RF00614 SLR   @06,@06                                             0616 01082000
         STCM  @06,7,PABCHNGP(SUBBACK)                             0616 01083000
*     RFY                                                          0617 01084000
*      (SUBCALLR,                                                  0617 01085000
*       SUBPAB32) RSTD;                                            0617 01086000
@RC00614 DS    0H                                                  0618 01087000
*     SUBPAB32=SUBPABP;             /* SET UP PARAM FOR CALL TO    0618 01088000
*                                      ISTAPC32                      */ 01089000
         LR    SUBPAB32,SUBPABP                                    0618 01090000
*     DO;                           /* LISATCVT                    0619 01091000
*                                      CVTBASE(PAGEABLE,(SUBATCVT))  */ 01092000
*       SUBATCVT=ATCLCPTR;          /* SET UP ATCVT POINTER          */ 01093000
         L     SUBATCVT,ATCLCPTR                                   0620 01094000
*     END;                          /* LISATCVT                      */ 01095000
*     SUBCALLR=SUBATCVT->ATCAP32;   /* GET ENTRY PT ADDRESS          */ 01096000
         L     SUBCALLR,ATCAP32(,SUBATCVT)                         0622 01097000
*     CALL ISTAPC32;                /* LET APC32 QUEUE PAB TO PST OR    01098000
*                                      MPST READY QUEUE              */ 01099000
         BALR  @14,@15                                             0623 01100000
*     RFY                                                          0624 01101000
*      (SUBCALLR,                                                  0624 01102000
*       SUBPAB32) UNRSTD;                                          0624 01103000
*   END;                            /* END DOWHILE RPHPABQ NOT EMPTY */ 01104000
@DE00607 L     @06,RPHPABQP-1(,SUBRPHP)                            0625 01105000
         LA    @06,0(,@06)                                         0625 01106000
         LTR   @06,@06                                             0625 01107000
         BNZ   @DL00607                                            0625 01108000
*   RFY                                                            0626 01109000
*    (SUBBACK) UNRSTD;                                             0626 01110000
*                                                                  0627 01111000
*   /*****************************************************************/ 01112000
*   /*                                                               */ 01113000
*   /* INITIALIZE THE RECOVERY DYPAB IN CRAPROCR SO IF THE RPH IS    */ 01114000
*   /* POSTED AFTER CLEANUP, IT WILL RUN UNDER AN SRB AND VTAM WILL  */ 01115000
*   /* GET CONTROL                                                   */ 01116000
*   /*                                                               */ 01117000
*   /*****************************************************************/ 01118000
*                                                                  0627 01119000
*   RFY                                                            0627 01120000
*    (SUBWK3,                                                      0627 01121000
*     SUBWK4) RSTD;                                                0627 01122000
*   SUBWK3=ADDR(RCOVPAB);           /* POINT TO RECOVERY DYPAB IN CRA*/ 01123000
         LA    SUBWK3,RCOVPAB(,SUBCRA)                             0628 01124000
*   SUBWK4=ADDR(SUBWK3->DYPPAB);    /* POINT TO RECOVERY PAB         */ 01125000
         LA    SUBWK4,DYPPAB(,SUBWK3)                              0629 01126000
*   SUBWK3->ISTDYPAB(1:LENGTH(ISTDYPAB))                           0630 01127000
*                                                                  0630 01128000
*   /*****************************************************************/ 01129000
*   /*                                                               */ 01130000
*   /* COPY INTO RECOVERY DYPAB                                      */ 01131000
*   /*                                                               */ 01132000
*   /*****************************************************************/ 01133000
*                                                                  0630 01134000
*       =PSTSMDYP(1:LENGTH(ISTDYPAB));                             0630 01135000
*                                                                  0630 01136000
         L     @06,APCPSTA(,SUBAPC)                                0630 01137000
         MVC   ISTDYPAB(32,SUBWK3),PSTSMDYP(@06)                   0630 01138000
*   /*****************************************************************/ 01139000
*   /*                                                               */ 01140000
*   /* FROM PST SMS PAB                                              */ 01141000
*   /*                                                               */ 01142000
*   /*****************************************************************/ 01143000
*                                                                  0631 01144000
*   SUBWK4->PABRPHA=SUBRPHP;        /* CONNECT RECOVERY PAB TO RPH   */ 01145000
         STCM  SUBRPHP,7,PABRPHA(SUBWK4)                           0631 01146000
*   SUBWK4->PABFLAGS='00'X;         /* SET ALL PAB FLAGS OFF         */ 01147000
         MVI   PABFLAGS(SUBWK4),X'00'                              0632 01148000
*   SUBWK4->PABDYNAM=SUBON;         /* NOW SET FLAG TO SHOW DYNAMIC     01149000
*                                      PAB                           */ 01150000
         OI    PABDYNAM(SUBWK4),B'00010000'                        0633 01151000
*   RPHPABOF=SUBWK4->PABOFFST;      /* CONNECT RPH TO PAB            */ 01152000
         MVC   RPHPABOF(1,SUBRPHP),PABOFFST(SUBWK4)                0634 01153000
*   RPHMAJCB=SUBWK3;                /* RPH MAJCB IS CRAPROCR=DYPAB   */ 01154000
