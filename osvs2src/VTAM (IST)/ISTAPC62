*SETSDWA:                                                          0381 00322000
*   R1=CRAPTR;                      /* LOAD UP AND USE REG 1 AS BASE    00323000
*                                      FOR CRA               @Z30APGA*/ 00324000
@RF00326 DS    0H                                                  0381 00325000
SETSDWA  L     @15,SDWAPARM(,SDWAPTR)                              0381 00326000
         L     R1,CRAPTR(,@15)                                     0381 00327000
*   GENERATE CODE;                                                 0382 00328000
          MVC SDWAVRA(4,7),CRACRR(R1)       MOVE PTR TO ACTIVE CRR      00329000
          MVC SDWAVRA+4(36,7),CRALKACT(R1)  MOVE MORE FROM CRA TO SDWA  00330000
          MVC SDWAVRA+40(20,7),CRAPSS(R1)   MOVE MORE FROM CRA TO SDWA  00331000
        MVC SDWAVRA+60(20,7),CRAPROCR(R1) MOVE MORE FROM CRA TO SDWA    00332000
          MVC SDWAVRA+80(20,7),CRASVC(R1)   MOVE MORE FROM CRA TO SDWA  00333000
*   SDWAURAL=100;                   /* SET LENGTH OF CRA INFO THAT 0383 00334000
*                                      WAS STORED IN THE SDWA        */ 00335000
         MVI   SDWAURAL(SDWAPTR),X'64'                             0383 00336000
*   SDWAHEX=ON;                     /* RECORD LOGREC IN HEX          */ 00337000
         OI    SDWAHEX(SDWAPTR),B'10000000'                        0384 00338000
*   SDWAREXN=FRRID;                 /* IDENTIFY FRR IN SDWA  @Z30APGA*/ 00339000
         MVC   SDWAREXN(8,SDWAPTR),@CC03625                        0385 00340000
*   RESPECIFY                                                      0386 00341000
*    (R1,                                                          0386 00342000
*     R5,                                                          0386 00343000
*     R13) UNRSTD;                                                 0386 00344000
*   IF SDWAFMID^=0|                 /* IF MEMORY TERM OR     @Z40GKD1*/ 00345000
*       PSTPTR=MPSTPTR THEN         /* TPIO RUNNING?         @Z40GKD1*/ 00346000
         LH    @15,SDWAFMID(,SDWAPTR)                              0387 00347000
         N     @15,@CF03778                                        0387 00348000
         LTR   @15,@15                                             0387 00349000
         BNZ   @RT00387                                            0387 00350000
         L     @15,PSTPTR(,APCRRPTR)                               0387 00351000
         C     @15,MPSTPTR(,@15)                                   0387 00352000
         BNE   @RF00387                                            0387 00353000
@RT00387 DS    0H                                                  0388 00354000
*     DO;                           /* YES-IT'S RUNNING              */ 00355000
*                                                                  0388 00356000
*       /*************************************************************/ 00357000
*       /*                                                           */ 00358000
*       /* ALL DEBS WILL BE ABENDED                                  */ 00359000
*       /*                                                           */ 00360000
*       /*************************************************************/ 00361000
*                                                                  0389 00362000
*/*            FIND EACH MPST->DCE->DEB->TCB & CALLRTM IF NOT MEMORY */ 00363000
*                                                                  0389 00364000
*       /*************************************************************/ 00365000
*       /*                                                           */ 00366000
*       /* TERMINATION                                       @Z40GKD1*/ 00367000
*       /*                                                           */ 00368000
*       /*************************************************************/ 00369000
*                                                                  0389 00370000
*       RESPECIFY                                                  0389 00371000
*        (ISTDCE) BASED(DCEPTR);                                   0389 00372000
*       RESPECIFY                                                  0390 00373000
*        (DCEPTR) RSTD;                                            0390 00374000
*       DCEPTR=MPSDBTBL;            /* ESTABLISHING PTR TO DEB TABLE    00375000
*                                      ENTRIES, FOR OPEN VTAM DEBS IN   00376000
*                                      THIS MEMORY.                  */ 00377000
         L     @15,PSTPTR(,APCRRPTR)                               0391 00378000
         L     @15,MPSTPTR(,@15)                                   0391 00379000
         L     DCEPTR,MPSDBTBL(,@15)                               0391 00380000
*       RESPECIFY                                                  0392 00381000
*        (ISTACDEB) BASED(ACDEBPTR);                               0392 00382000
*       RESPECIFY                                                  0393 00383000
*        (ACDEBPTR,                                                0393 00384000
*         TCBPTR,                                                  0393 00385000
*         R5) RSTD;                 /* NOTE-R4 IS RSTD               */ 00386000
*       RFY                                                        0394 00387000
*         ISTPST BASED(ACDTSKID);   /* BASE PST ON DEB FOR DCE CHAIN    00388000
*                                      SEARCH                @Z40GKD1*/ 00389000
*       DO WHILE(DCEPTR>0);         /* DO IF THERE ARE MORE DCE'S    */ 00390000
         B     @DE00395                                            0395 00391000
@DL00395 DS    0H                                                  0396 00392000
*         ACDEBPTR=DCEDBPTR;        /* ESTABLISH PTR TO VTAM'S DATA     00393000
*                                      EXTENT BLOCK                  */ 00394000
         L     ACDEBPTR,DCEDBPTR(,DCEPTR)                          0396 00395000
*         IF ACDTCB=ATCCDTCB        /* IF DEBTASK IS VTAM JOBSTEP  0397 00396000
*                                      TASK                  @OZ04697*/ 00397000
*             &SDWAFMID=0           /* AND NOT MEMORY TERMINATION  0397 00398000
*                                                            @OZ04697*/ 00399000
*           THEN                                                   0397 00400000
         L     @15,ACDTCB-1(,ACDEBPTR)                             0397 00401000
         LA    @15,0(,@15)                                         0397 00402000
         C     @15,ATCCDTCB(,GPR10P)                               0397 00403000
         BNE   @RF00397                                            0397 00404000
         SLR   @15,@15                                             0397 00405000
         LH    @12,SDWAFMID(,SDWAPTR)                              0397 00406000
         N     @12,@CF03778                                        0397 00407000
         CR    @15,@12                                             0397 00408000
         BNE   @RF00397                                            0397 00409000
*           DO;                     /*                       @OZ04697*/ 00410000
*             IF CRAPTR^=0          /* IF THIS IS FIRST ENCOUNTER  0399 00411000
*                                                            @OZ04697*/ 00412000
*               THEN                                               0399 00413000
         L     @12,SDWAPARM(,SDWAPTR)                              0399 00414000
         C     @15,CRAPTR(,@12)                                    0399 00415000
         BE    @RF00399                                            0399 00416000
*               DO;                 /*                       @OZ04697*/ 00417000
*                 RFY                                              0401 00418000
*                   R13 RSTD;       /*                       @OZ04697*/ 00419000
*                 R13=WRKAREAP;     /* WRKAREAP POINTS TO SDWASAVE 0402 00420000
*                                                            @OZ04697*/ 00421000
         LR    R13,WRKAREAP                                        0402 00422000
*                 CALL CRACLN;      /* CLEAN UP CRA GIVEN ON INPUT.     00423000
*                                                            @OZ04697*/ 00424000
         BAL   @14,CRACLN                                          0403 00425000
*                 RFY                                              0404 00426000
*                   R13 UNRSTD;     /*                       @OZ04697*/ 00427000
*                 CRAPTR=0;         /* INDICATE CRA PROCESSED SO   0405 00428000
*                                      WON'T REPEAT IF VTAM HAS MORE    00429000
*                                      THAN ONE DEB          @OZ04697*/ 00430000
         L     @15,SDWAPARM(,SDWAPTR)                              0405 00431000
         SLR   @12,@12                                             0405 00432000
