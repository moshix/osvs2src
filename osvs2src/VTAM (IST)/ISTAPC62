*                 TCBPTR=ACDTCB;    /* SET PTR WITH VTAM JOBSTEP TASK   00434000
*                                      ID FOR ABEND          @OZ04697*/ 00435000
         L     TCBPTR,ACDTCB-1(,ACDEBPTR)                          0406 00436000
         LA    TCBPTR,0(,TCBPTR)                                   0406 00437000
*/********************************************************************/ 00438000
*/*   CALLRTM - IS USED TO DIRECT THE SERVICES OF THE RTM TO A TASK  */ 00439000
*/*  OTHER THAN ITSELF. CALLERS MUST BE KEY 0, PRIVILEGED ROUTINES   */ 00440000
*/*  CONTAINING THE LOCAL LOCK. A SAVEAREA IS NOT REQUIRED.          */ 00441000
*/*   FOR A TYPE=ABTERM,  ASID IS NOT SPECIFIED.                     */ 00442000
*/*   THE TCB ADDRESS:  LOCATES THE TCB TO BE ABEND'ED IN THE CURRENT*/ 00443000
*/*                     CURRENT MEMORY.                              */ 00444000
*/*   CONTROL IS RETURNED TO THE CALLER. ALL REGISTERS RESTORED VIA  */ 00445000
*/*  THE SAVEAREA, SERIALIZED BY THE LOCAL LOCK.                     */ 00446000
*/********************************************************************/ 00447000
*                                                                  0407 00448000
*                 GENERATE CODE;                                   0407 00449000
          CALLRTM TYPE=ABTERM,COMPCOD=X'0AA',TCB=(TCBPTR),DUMP=NO       00450000
*               END;                /* END THIS IS THE FIRST       0408 00451000
*                                      ENCOUNTER             @OZ04697*/ 00452000
*             ELSE                                                 0409 00453000
*               ;                   /* BYPASS PROCESSING IF NO MORE     00454000
*                                      CRA                   @OZ04697*/ 00455000
@RF00399 DS    0H                                                  0410 00456000
*           END;                    /* END VTAM JOBSTEP ABEND      0410 00457000
*                                                            @OZ04697*/ 00458000
*         ELSE                                                     0411 00459000
*           DO;                     /* ELSE NOT VTAM JOBSTEP, OR MAY    00460000
*                                      BE MEMORY TERM                */ 00461000
         B     @RC00397                                            0411 00462000
@RF00397 DS    0H                                                  0412 00463000
*             R4=ACDSSFLG(1:4);     /* ACB FLAG WITH ACDABEND      0412 00464000
*                                                            @Z40GKD1*/ 00465000
         ICM   R4,15,ACDSSFLG(ACDEBPTR)                            0412 00466000
*SETAB:                                                            0413 00467000
*             R5=R4|'40000000'X;    /* TURN ON ACDABEND FOR SWAP   0413 00468000
*                                                            @Z40GKD1*/ 00469000
SETAB    LR    R5,R4                                               0413 00470000
         O     R5,@CF00121                                         0413 00471000
*             CS(R4,R5,ACDSSFLG(1:4));/*                     @Z40GKD1*/ 00472000
