*/*         SETLOCK ROUTINE - OBTAIN LOCAL MEMORY LOCK               */ 00046000
*/*  INPUT:  REG 0-REG 10: UNUSED                                    */ 00047000
*/*          REG 11-REG 13: REGS USED BY SETLOCK                     */ 00048000
*/*          REG 14: RETURN ADDRESS                                  */ 00049000
*/*          REG 15: UNUSED                                          */ 00050000
*/*  OUTPUT: REG 0-REG 10: RESTORED                                  */ 00051000
*/*          REG 11-REG 12: UNPREDICTABLE                            */ 00052000
*/*          REG 13: RETURN CODE                                     */ 00053000
*/*            IF 0= NORMAL RETURN                                   */ 00054000
*/*               4= CALLER HOLDS LOCK                               */ 00055000
*/*              12= INVALID REQUEST                                 */ 00056000
*/*              20= UNABLE TO COMPLETE                              */ 00057000
*/*          REG 14-REG 15: RESTORED                                 */ 00058000
*/********************************************************************/ 00059000
*                                                                  0290 00060000
*GETLOCK:                                                          0290 00061000
*   DO;                             /* SETLOCK (OBTAIN) MODE(UNCOND)    00062000
*                                      TYPE(LOCAL)                 0290 00063000
*                                      RELATED(LOCAL,ISTAPC62(SETRP))*/ 00064000
GETLOCK  DS    0H                                                  0291 00065000
*     RFY                                                          0291 00066000
*      (GPR11P,                                                    0291 00067000
*       GPR12P,                                                    0291 00068000
*       GPR13P,                                                    0291 00069000
*       GPR14P) RSTD;                                              0291 00070000
*     GPR13P=PSALITA;               /* ADDRESS OF LOCK INTERFACE   0292 00071000
*                                      TABLE.                        */ 00072000
         L     GPR13P,PSALITA                                      0292 00073000
*     GEN(LM GPR11P,GPR13P,576(GPR13P));/* LOAD LOCKWORD, CLHT AND 0293 00074000
*                                      ENTRY ADRS.                   */ 00075000
         LM GPR11P,GPR13P,576(GPR13P)                                   00076000
*     GEN(BALR GPR14P,GPR13P);      /* BALR TO SETLOCK RTN.          */ 00077000
         BALR GPR14P,GPR13P                                             00078000
*     RFY                                                          0295 00079000
*      (GPR11P,                                                    0295 00080000
*       GPR12P,                                                    0295 00081000
*       GPR13P,                                                    0295 00082000
*       GPR14P) UNRSTD;                                            0295 00083000
*   END;                            /* SETLOCK (OBTAIN) MODE(UNCOND)    00084000
*                                      TYPE(LOCAL)                 0296 00085000
*                                      RELATED(LOCAL,ISTAPC62(SETRP))*/ 00086000
*   IF WORD0=0                      /* DOES CRA EXIST?       @ZM32662*/ 00087000
*     THEN                                                         0297 00088000
         L     @15,SDWAPARM(,SDWAPTR)                              0297 00089000
         L     @15,WORD0(,@15)                                     0297 00090000
         LTR   @15,@15                                             0297 00091000
         BNZ   @RF00297                                            0297 00092000
*     DO;                           /* NO - RECORD THE FACT AND    0298 00093000
*                                      RETURN                @ZM32662*/ 00094000
*       SDWAVRA(1:16)=NOCRA;        /* NO CRA AVAIL          @ZM32662*/ 00095000
         MVC   SDWAVRA(16,SDWAPTR),@CC03633                        0299 00096000
*       SDWAURAL=LENGTH(NOCRA);     /* MSG LENGTH            @ZM32662*/ 00097000
         MVI   SDWAURAL(SDWAPTR),X'10'                             0300 00098000
*       SDWAREXN=FRRID;             /* IDENTITY FRR          @ZM32662*/ 00099000
         MVC   SDWAREXN(8,SDWAPTR),@CC03625                        0301 00100000
*       SDWAEBC=ON;                 /* EBIDIC MSG            @ZM32662*/ 00101000
         OI    SDWAEBC(SDWAPTR),B'01000000'                        0302 00102000
*       GOTO SETRP;                 /* WRAP IT UP            @ZM32662*/ 00103000
         B     SETRP                                               0303 00104000
*     END;                          /* END OF NO CRA         @ZM32662*/ 00105000
*/********************************************************************/ 00106000
*/*   LISATCVT MACRO - GIVES YOU THE POINTER TO VTAM'S CVT-ISTATCVT  */ 00107000
*/********************************************************************/ 00108000
*                                                                  0305 00109000
*   RFY                                                            0305 00110000
*    (GPR10P) RSTD;                 /* RESTRICT ATCVT PTR            */ 00111000
@RF00297 DS    0H                                                  0306 00112000
*   DO;                             /* LISATCVT                    0306 00113000
*                                      CVTBASE(PAGEABLE,(GPR10P))    */ 00114000
*     GPR10P=ATCLCPTR;              /* SET UP ATCVT POINTER          */ 00115000
         L     GPR10P,ATCLCPTR                                     0307 00116000
*   END;                            /* LISATCVT                      */ 00117000
*   RESPECIFY                                                      0309 00118000
*    (ISTATCVT) BASED(GPR10P);                                     0309 00119000
*   RESPECIFY                                                      0310 00120000
*    (ISTCRA) BASED(WORD0);                                        0310 00121000
*   RESPECIFY                                                      0311 00122000
*    (ISTMPST) BASED(MPSTPTR);                                     0311 00123000
*   RESPECIFY                                                      0312 00124000
*    (ISTAPCRR) BASED(APCRRPTR);                                   0312 00125000
*   RESPECIFY                                                      0313 00126000
*    (ISTPST) BASED(PSTPTR);                                       0313 00127000
*   RESPECIFY                                                      0314 00128000
*    (APCRRPTR) RSTD;                                              0314 00129000
*   APCRRPTR=ADDR(CRAPSS);          /* ESTABLISH PSS CRA POINTER     */ 00130000
         L     @15,SDWAPARM(,SDWAPTR)                              0315 00131000
         L     APCRRPTR,WORD0(,@15)                                0315 00132000
         LA    APCRRPTR,CRAPSS(,APCRRPTR)                          0315 00133000
*   IF SDWAFMID^=0                  /* IF MEMORY TERM.       @Z40GKD1*/ 00134000
*     THEN                                                         0316 00135000
         LH    @15,SDWAFMID(,SDWAPTR)                              0316 00136000
         N     @15,@CF03778                                        0316 00137000
         LTR   @15,@15                                             0316 00138000
         BZ    @RF00316                                            0316 00139000
*     DO;                                                          0317 00140000
*                                                                  0317 00141000
*       /*************************************************************/ 00142000
*       /*                                                           */ 00143000
*       /* FIND & GATE MPST READY QUEUE                      @Z40GKD1*/ 00144000
*       /*                                                           */ 00145000
*       /*************************************************************/ 00146000
*                                                                  0318 00147000
*       RFY                                                        0318 00148000
*        (R4,                                                      0318 00149000
*         R5) RSTD;                 /*                       @Z40GKD1*/ 00150000
*       R4=RPHTSKID->MPSRQUE;       /* ADDR OF READY Q FOR COMPARE 0319 00151000
*                                                            @Z40GKD1*/ 00152000
         L     @15,RPHTSKID(,RPHPTR)                               0319 00153000
         L     R4,MPSRQUE(,@15)                                    0319 00154000
*GATE:                                                             0320 00155000
*       R5=R4|MPSGATE1;             /* TURN GATE BIT ON FRO SWAP   0320 00156000
*                                                            @Z40GKD1*/ 00157000
GATE     LR    R5,R4                                               0320 00158000
         O     R5,@CF00115                                         0320 00159000
*       CS(R4,R5,RPHTSKID->MPSRQUE);/*                       @Z40GKD1*/ 00160000
         L     @15,RPHTSKID(,RPHPTR)                               0321 00161000
         CS    R4,@05,MPSRQUE(@15)                                 0321 00162000
*       BC(7,GATE);                 /* RETRY IF FAIL         @Z40GKD1*/ 00163000
         BC    7,GATE                                              0322 00164000
*       GO TO SETSDWA;              /*                       @Z40GKD1*/ 00165000
         B     SETSDWA                                             0323 00166000
*       RFY                                                        0324 00167000
*        (R4,                                                      0324 00168000
*         R5) UNRSTD;               /*                       @Z40GKD1*/ 00169000
*     END;                          /* END MEMORY TERM.      @Z40GKD1*/ 00170000
*/********************************************************************/ 00171000
*/*    SDUMP MACRO-EXECUTE FORM--                                    */ 00172000
*/*  BRANCH=YES--A BRANCH IS USED FOR THE INTERFACING                */ 00173000
*/*  ASID=R5  --CONTAINS ASID OF ADDR SPACE TO BE DUMPED             */ 00174000
*/*  MF=(E,(1))--'E' STANDS FOR THE EXECUTE FORM AND 1 IS REGISTER 1 */ 00175000
*/*              WHICH CONTAINS THE ADDRESS OF THE LIST FORM OF THE  */ 00176000
*/*              MACRO.                                              */ 00177000
*/********************************************************************/ 00178000
*                                                                  0326 00179000
*   IF SDWAPERC='0'B THEN                                          0326 00180000
@RF00316 TM    SDWAPERC(SDWAPTR),B'00010000'                       0326 00181000
         BNZ   @RF00326                                            0326 00182000
*     DO;                           /* TAKE SDUMP IF NOT PERC'D TO 0327 00183000
*                                                            @Z30APGA*/ 00184000
*       RESPECIFY                                                  0328 00185000
