         TM    PSTABINP(@15),B'00000001'                           0416 00478000
         BO    @RT00416                                            0416 00479000
*               ;                   /* IS ABEND IN PROCESS?  @Z40GKD1*/ 00480000
*             ELSE                  /* NO-ABEND NOT IN PROCESS       */ 00481000
*               DO;                 /* PREPARE TO USE CS INSTRUCTION */ 00482000
*                 R4=PSTFLGA;       /* SET WITH PST FLAGS            */ 00483000
         L     @15,ACDTSKID(,ACDEBPTR)                             0419 00484000
         L     R4,PSTFLGA(,@15)                                    0419 00485000
*RETRY1:                                                           0420 00486000
*                 R5=PSTABIP1|PSTDMPC1;/* TURN ON FLAGS      @ZM30372*/ 00487000
RETRY1   L     R5,@CF00077                                         0420 00488000
         O     R5,@CF01571                                         0420 00489000
*                 R5=R5|R4;         /* TURN ON FLAG-ABEND IN PROCESS */ 00490000
