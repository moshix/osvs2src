*                 CS(R4,R5,PSTFLGA);/* TEST TO SEE IF ON             */ 00492000
         L     @15,ACDTSKID(,ACDEBPTR)                             0422 00493000
         CS    R4,@05,PSTFLGA(@15)                                 0422 00494000
*                 BC(4,RETRY1);     /* IF NOT SET, TRY AGAIN         */ 00495000
         BC    4,RETRY1                                            0423 00496000
*                 IF SDWAFMID^=0    /* IF MEMORY TERM        @Z40GKD1*/ 00497000
*                   THEN                                           0424 00498000
         LH    @15,SDWAFMID(,SDWAPTR)                              0424 00499000
         N     @15,@CF03778                                        0424 00500000
         LTR   @15,@15                                             0424 00501000
         BNZ   @RT00424                                            0424 00502000
*                   ;               /* BYPASS CALLRTM        @Z40GKD1*/ 00503000
*                 ELSE                                             0426 00504000
*                   DO;             /*                       @Z40GKD1*/ 00505000
*SETTCB:                                                           0427 00506000
*                     TCBPTR=ACDTCB;/* SET TCBPTR WITH TCB OWNING  0427 00507000
*                                      THIS DEB                      */ 00508000
SETTCB   L     TCBPTR,ACDTCB-1(,ACDEBPTR)                          0427 00509000
         LA    TCBPTR,0(,TCBPTR)                                   0427 00510000
*                     GENERATE CODE;                               0428 00511000
