         TITLE 'ISTAPM62 - 62 FRR ROUTINE                              *00001000
                        '                                               00002000
ISTAPC62 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTAPC62  76.296'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @09,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@09                                         0001 00012000
         MODESET EXTKEY=ZERO                                            00013000
*   DO;                             /* LISATCVT                    0276 00014000
*                                      CVTBASE(PAGEABLE,(GPR02P))    */ 00015000
*     GPR02P=ATCLCPTR;              /* SET UP ATCVT POINTER          */ 00016000
         L     GPR02P,ATCLCPTR                                     0277 00017000
*   END;                            /* LISATCVT LOCATE ATCVT         */ 00018000
*   GPR04P=GPR02P->ATCAP69;         /* ADRESS OF FRR TO PROTECT THIS    00019000
*                                      FRR                           */ 00020000
         L     GPR04P,ATCAP69(,GPR02P)                             0279 00021000
*   GEN REFS(GPR02P,GPR03P,GPR04P);                                0280 00022000
     SETFRR A,FRRAD=(GPR04P),PARMAD=(GPR04P),WRKREGS=(GPR02P,GPR03P)    00023000
*   RFY                                                            0281 00024000
*     FRRPARMS BASED(GPR04P);       /* BASE FOR NEW FRR PARM LIST    */ 00025000
*   IDWORDS=FRRID;                  /* INIT THE FRR PARM     @Z30APGA*/ 00026000
         MVC   IDWORDS(8,GPR04P),@CC03625                          0282 00027000
*   RFY                                                            0283 00028000
*     FRRPARMS BASED(SDWAPARM);     /* BASE FOR APC62 PARM LIST      */ 00029000
*   RFY                                                            0284 00030000
