*       R13=R13+20;                 /* THEN SAVEAREA         @Z40GKD1*/ 00198000
         AL    R13,@CF00557                                        0332 00199000
*       IF PSTPTR^=MPSTPTR          /* IF PST GOOD           @Z40GKD1*/ 00200000
*         THEN                                                     0333 00201000
