*         R5,                                                      0328 00187000
*         R13) RSTD;                                               0328 00188000
*       R13=WRKAREAP;               /* SET UP SAVEAREA POINTER       */ 00189000
         LR    R13,WRKAREAP                                        0329 00190000
*       R5=R13;                     /* 1ST 16 BYTES = VARIABLE DATA     00191000
*                                                            @Z40GKD1*/ 00192000
         LR    R5,R13                                              0330 00193000
