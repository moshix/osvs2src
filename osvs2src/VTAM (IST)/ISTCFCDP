         BAL   @14,CDPTPMSG                                        1432 02923000
*         IF CDPR15^=0              /* WAS DISPLAY CANCELLED?      1433 02924000
*                                                            @D32CKDA*/ 02925000
*           THEN                                                   1433 02926000
         LTR   CDPR15,CDPR15                                       1433 02927000
         BNZ   @RT01433                                            1433 02928000
*           ;                       /* YES, EXIT FROM PATH DSPLY     */ 02929000
*         ELSE                                                     1435 02930000
*           DO;                                                    1435 02931000
*             CDPATHND=CDPIBPTR+RPLRLEN-LENGTH(DPAARRAY);/* END OF 1436 02932000
*                                      PATH AREA MINUS LENGTH OF 1 1436 02933000
*                                      ENTRY FOR DO WHILE LOOP     1436 02934000
*                                                            @D32CKDA*/ 02935000
*                                                                  1436 02936000
         L     @04,RPLPTR(,CDPBFPTR)                               1436 02937000
         L     @07,CDPIBPTR(,CDPBFPTR)                             1436 02938000
         AL    @07,RPLRLEN(,@04)                                   1436 02939000
         SL    @07,@CF03815                                        1436 02940000
         ST    @07,CDPATHND(,CDPBFPTR)                             1436 02941000
*             /*******************************************************/ 02942000
*             /*                                                     */ 02943000
*             /* ISSUE MSG IST168I FOR EACH PATH FOR THIS PHYSICAL   */ 02944000
*             /* UNIT. IF DISPLAY CANCELLED, DO NOT CONTINUE WITH IT */ 02945000
*             /*                                             @D32CKDA*/ 02946000
*             /*                                                     */ 02947000
*             /*******************************************************/ 02948000
*                                                                  1437 02949000
*             DO CDPIBPTR=RDTEPTR TO CDPATHND BY LENGTH(DPAARRAY) WHILE 02950000
*                   CDPR15=0;       /* FORMAT MSG 168I - NOTE:     1437 02951000
*                                      INITIALIZE CDPIBPTR SO THAT 1437 02952000
*                                      PLS WILL UPDATE-- RDTEPTR WAS    02953000
*                                      SET TO CDPIBPTR JUST PRIOR TO    02954000
*                                      CALL TO CDPPATH@D32CKDA       */ 02955000
         L     @04,RDTEPTR(,CDPBFPTR)                              1437 02956000
         ST    @04,CDPIBPTR(,CDPBFPTR)                             1437 02957000
         B     @DE01437                                            1437 02958000
@DL01437 LTR   CDPR15,CDPR15                                       1437 02959000
         BNZ   @DC01437                                            1437 02960000
*               CDPR8=PATPTHID;     /* GET PATH ID           @D32CKDA*/ 02961000
         L     @04,CDPIBPTR(,CDPBFPTR)                             1438 02962000
         SLR   CDPR8,CDPR8                                         1438 02963000
         IC    CDPR8,PATPTHID+8(,@04)                              1438 02964000
*               CALL CDPCVT;        /* CONVERT TO EBCDIC     @D32CKDA*/ 02965000
         BAL   @14,CDPCVT                                          1439 02966000
*               CDPPTHID=CDPUNPK(6:8);/* MOVE TO TO MSG BUFF @D32CKDA*/ 02967000
         L     @04,CDPMSG(,CDPBFPTR)                               1440 02968000
         MVC   CDPPTHID(3,@04),CDPUNPK+5(CDPBFPTR)                 1440 02969000
*               CDPR8=PATGRPID;     /* GET GROUP ID          @D32CKDA*/ 02970000
         L     @04,CDPIBPTR(,CDPBFPTR)                             1441 02971000
         SLR   CDPR8,CDPR8                                         1441 02972000
         IC    CDPR8,PATGRPID+8(,@04)                              1441 02973000
*               CALL CDPCVT;        /* CONVERT TO EBCDIC     @D32CKDA*/ 02974000
         BAL   @14,CDPCVT                                          1442 02975000
*               CDPGRPID=CDPUNPK(6:8);/* MOVE IT TO MSG BUFF @D32CKDA*/ 02976000
         L     @04,CDPMSG(,CDPBFPTR)                               1443 02977000
         MVC   CDPGRPID(3,@04),CDPUNPK+5(CDPBFPTR)                 1443 02978000
*               CDPR8=PATRDCNT;     /* GET DIAL RETRY CNT    @D32CKDA*/ 02979000
         L     @04,CDPIBPTR(,CDPBFPTR)                             1444 02980000
         SLR   CDPR8,CDPR8                                         1444 02981000
         IC    CDPR8,PATRDCNT+8(,@04)                              1444 02982000
*               CALL CDPCVT;        /* CONVERT TO EBCDIC     @D32CKDA*/ 02983000
         BAL   @14,CDPCVT                                          1445 02984000
*               CDPRCNT=CDPUNPK(6:8);/* MOVE IT TO MSG BUFF  @D32CKDA*/ 02985000
         L     @04,CDPMSG(,CDPBFPTR)                               1446 02986000
         MVC   CDPRCNT(3,@04),CDPUNPK+5(CDPBFPTR)                  1446 02987000
*               CDP168LN=DPAGRPNM(1);/* GROUP LINE NAME      @D32CKDA*/ 02988000
         L     @07,CDPIBPTR(,CDPBFPTR)                             1447 02989000
         MVC   CDP168LN(8,@04),DPAGRPNM(@07)                       1447 02990000
*               IF DPACTDIG(1)=0    /* BAD LENGTH VALUE DO NOT TRY TO   02991000
*                                      MOVE TELEPHONE NO     @D32CKDA*/ 02992000
*                 THEN                                             1448 02993000
         CLI   DPACTDIG(@07),0                                     1448 02994000
         BE    @RT01448                                            1448 02995000
*                 ;                 /*                       @D32CKDA*/ 02996000
*               ELSE                                               1450 02997000
*                 DO;               /* MOVE TELEPHONE NO     @D32CKDA*/ 02998000
*                   CDPWORK=MIN(32,DPACTDIG(1));/* LENGTH CANNOT BE     02999000
*                                      OVER 32               @D32CKDA*/ 03000000
         LA    @04,32                                              1451 03001000
         L     @07,CDPIBPTR(,CDPBFPTR)                             1451 03002000
         SLR   CDPWORK,CDPWORK                                     1451 03003000
         IC    CDPWORK,DPACTDIG(,@07)                              1451 03004000
         CR    CDPWORK,@04                                         1451 03005000
         BNH   *+6                                                      03006000
         LR    CDPWORK,@04                                         1451 03007000
*                   CDPR8=(((32-CDPWORK)/2)+1);/* POSITION FOR 1ST 1452 03008000
*                                      CHAR IN TELEPHONE     @D32CKDA*/ 03009000
         LR    @06,@04                                             1452 03010000
         SLR   @06,CDPWORK                                         1452 03011000
         SRDA  @06,32                                              1452 03012000
         D     @06,@CF00075                                        1452 03013000
         AL    @07,@CF00064                                        1452 03014000
         LR    CDPR8,@07                                           1452 03015000
*                   CDPR9=CDPR8-1+CDPWORK;/* POSITION FOR LAST CHAR     03016000
*                                      IN TELEPHONE          @D32CKDA*/ 03017000
         LR    CDPR9,CDPR8                                         1453 03018000
         BCTR  CDPR9,0                                             1453 03019000
         ALR   CDPR9,CDPWORK                                       1453 03020000
*                   CDPTELNO(CDPR8:CDPR9)=DPATELNO(1,1:CDPWORK);/* 1454 03021000
*                                      MOVE TELEPHONE NO     @D32CKDA*/ 03022000
         L     @04,CDPMSG(,CDPBFPTR)                               1454 03023000
         ALR   @04,CDPR8                                           1454 03024000
         LR    @07,CDPR9                                           1454 03025000
         SLR   @07,CDPR8                                           1454 03026000
         L     @06,CDPIBPTR(,CDPBFPTR)                             1454 03027000
         EX    @07,@SM03839                                        1454 03028000
*                 END;              /* END OF PHONE FORMATTING     1455 03029000
*                                                            @D32CKDA*/ 03030000
*               IF PATUSEFG=CDPON   /* IF PATH IS USEABLE BY @D32CKDA*/ 03031000
*                 THEN              /* VTAM                  @D32CKDA*/ 03032000
@RT01448 L     @04,CDPIBPTR(,CDPBFPTR)                             1456 03033000
         TM    PATUSEFG+8(@04),B'10000000'                         1456 03034000
         BNO   @RF01456                                            1456 03035000
*                 CDPAVAIL=CDPAVA;  /* FORMAT MSG WITH AVA   @D32CKDA*/ 03036000
         L     @04,CDPMSG(,CDPBFPTR)                               1457 03037000
         MVC   CDPAVAIL(3,@04),@CC03577                            1457 03038000
*               ELSE                /* PATH NOT USEABLE BY VTAM      */ 03039000
*                 CDPAVAIL=CDPNAV;  /* FORMAT MSG WITH NAV   @D32CKDA*/ 03040000
         B     @RC01456                                            1458 03041000
@RF01456 L     @04,CDPMSG(,CDPBFPTR)                               1458 03042000
         MVC   CDPAVAIL(3,@04),@CC03579                            1458 03043000
*               IF DPAMAN(1)=CDPON  /* IF TELEPHONE MUST BE  @D32CKDA*/ 03044000
*                 THEN              /* MANUALLY DIALED       @D32CKDA*/ 03045000
@RC01456 L     @04,CDPIBPTR(,CDPBFPTR)                             1459 03046000
         TM    DPAMAN(@04),B'10000000'                             1459 03047000
         BNO   @RF01459                                            1459 03048000
*                 CDPAUTO=CDPMAN;   /* FORMAT MSG WITH MAN   @D32CKDA*/ 03049000
         L     @04,CDPMSG(,CDPBFPTR)                               1460 03050000
         MVC   CDPAUTO(3,@04),@CC03581                             1460 03051000
*               ELSE                /* FOR AUTO DIAL         @D32CKDA*/ 03052000
*                 IF DPAAUTOD(1)=CDPON/* IF FOR AUTO DIAL    @D32CKDA*/ 03053000
*                   THEN                                           1461 03054000
         B     @RC01459                                            1461 03055000
@RF01459 L     @04,CDPIBPTR(,CDPBFPTR)                             1461 03056000
         TM    DPAAUTOD(@04),B'01000000'                           1461 03057000
         BNO   @RF01461                                            1461 03058000
*                   CDPAUTO=CDPAUT; /* FORMAT MSG WITH AUT   @D32CKDA*/ 03059000
         L     @04,CDPMSG(,CDPBFPTR)                               1462 03060000
         MVC   CDPAUTO(3,@04),@CC03583                             1462 03061000
*               CDPMSGID=ADDR(CDPMD168);/* PTR TO MSG 168I   @D32CKDA*/ 03062000
@RF01461 DS    0H                                                  1463 03063000
@RC01459 LA    CDPMSGID,@CC03513                                   1463 03064000
*               CALL CDPTPMSG;      /* CALL TPMSG RTN        @D32CKDA*/ 03065000
         BAL   @14,CDPTPMSG                                        1464 03066000
*             END;                  /* END OF DO WHILE LOOP  @D32CKDA*/ 03067000
         LA    @04,50                                              1465 03068000
         AL    @04,CDPIBPTR(,CDPBFPTR)                             1465 03069000
         ST    @04,CDPIBPTR(,CDPBFPTR)                             1465 03070000
@DE01437 C     @04,CDPATHND(,CDPBFPTR)                             1465 03071000
         BNH   @DL01437                                            1465 03072000
@DC01437 DS    0H                                                  1466 03073000
*           END;                    /* END OF PATH DISPLAY   @D32CKDA*/ 03074000
*       END;                        /* DISPLAY CANCELLED     @D32CKDA*/ 03075000
@RT01433 DS    0H                                                  1468 03076000
*   CDPR14=CDPSAVE2;                /* RESTORE CALLER RET ADDR     1468 03077000
*                                                            @D32CKDA*/ 03078000
@RC01423 DS    0H                                                  1468 03079000
@RT01421 L     CDPR14,CDPSAVE2(,CDPBFPTR)                          1468 03080000
*   END CDPPATH;                    /*                       @D32CKDA*/ 03081000
