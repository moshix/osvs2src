         CLI   RPLRTNCD(@04),8                                     1034 01591000
         BNE   @RF01034                                            1034 01592000
*         DO;                       /* YES, DO FOLLOWS               */ 01593000
*           CDPEXITC=8;             /* EXIT CODE = 4                 */ 01594000
         MVI   CDPEXITC(CDPBFPTR),X'08'                            1036 01595000
