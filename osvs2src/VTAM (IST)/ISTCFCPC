SVTM     TITLE ' COMMAND FAC-- ISTCFCPC -- POI CLOSE PROCESSOR         *00001000
                        '                                               00002000
ISTCFCPC CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTCFCPC  75.325'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         EJECT                                                          00014000
*                                                               01S0119 00015000
*   /*****************************************************************/ 00016000
*   /*                                                               */ 00017000
*   /* MAINLINE CODE OF ISTCFCPC -- POI CLOSE PROCESS                */ 00018000
*   /*                                                               */ 00019000
*   /*****************************************************************/ 00020000
*                                                               01S0119 00021000
*   DO;                                                         01S0119 00022000
*                                                               01S0119 00023000
*/***  CRRMAINT  PROCNM(ISTOCPOI)COMP(OC)FUNC(SUB)RPHADD((CPCRPHPT))  * 00024000
*                                                                    */ 00025000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)                        */ 00026000
*/* END OF ATBUILD                                                   */ 00027000
*/* ** SET ENTERED BIT FOR THIS PROCEDURE **                         */ 00028000
*                                                               01S0120 00029000
*     CPCRPHPT->RPHCRR->CRACRR->NOCPOI='1'B;                    01S0120 00030000
*                                                               01S0120 00031000
         L     @15,RPHCRR(,CPCRPHPT)                            01S0120 00032000
         L     @15,CRACRR(,@15)                                 01S0120 00033000
         OI    NOCPOI(@15),B'00000001'                          01S0120 00034000
*/* ** CLEAR AND SET CURRENT PROCEDURE BIT **                        */ 00035000
*                                                               01S0121 00036000
*     CPCRPHPT->RPHCRR->CRACRR->OCCCRT='0'B;                    01S0121 00037000
         XC    OCCCRT+1(2,@15),OCCCRT+1(@15)                    01S0121 00038000
         MVI   OCCCRT(@15),B'00000000'                          01S0121 00039000
*     CPCRPHPT->RPHCRR->CRACRR->COCPOI='1'B;                    01S0122 00040000
         OI    COCPOI(@15),B'00000001'                          01S0122 00041000
*   END;                                                        01S0123 00042000
*   IF ATCPOIA^=ZERO                /* POIA AND POCB EXIST           */ 00043000
*     THEN                                                      01S0124 00044000
         L     @15,RPHWEA(,CPCRPHPT)                            01S0124 00045000
         L     @15,OCWACVT(,@15)                                01S0124 00046000
         L     @00,ATCPOIA-1(,@15)                              01S0124 00047000
         ICM   @00,8,@CB01117                                   01S0124 00048000
         LTR   @00,@00                                          01S0124 00049000
         BZ    @RF00124                                         01S0124 00050000
*     DO;                                                       01S0125 00051000
*       CALL CPCBFIND;              /* FIND CORRECT POCB             */ 00052000
         BAL   @14,CPCBFIND                                     01S0126 00053000
*       RFY                                                     01S0127 00054000
*         ISTPOCB BASED(CPCBASE);                               01S0127 00055000
*       IF CPCFND=ON                /* POCB FOUND                    */ 00056000
*         THEN                                                  01S0128 00057000
         LA    @15,176                                          01S0128 00058000
         AL    @15,RPHWEA(,CPCRPHPT)                            01S0128 00059000
         TM    CPCFND(@15),B'10000000'                          01S0128 00060000
         BNO   @RF00128                                         01S0128 00061000
*         DO;                                                   01S0129 00062000
*           CPCFND=OFF;             /* CLEAR BYTE FOR NEXT USE       */ 00063000
         NI    CPCFND(@15),B'01111111'                          01S0130 00064000
*           IF POCINACT=ON          /* IF THE POCB FOUND IS INACTIVE */ 00065000
*             THEN                                              01S0131 00066000
         TM    POCINACT(CPCBASE),B'01000000'                    01S0131 00067000
         BNO   @RF00131                                         01S0131 00068000
*             DO;                                               01S0132 00069000
*                                                               01S0132 00070000
*               /*****************************************************/ 00071000
*               /*                                                   */ 00072000
*               /* FOLLOWING DO GROUP WILL DETERMINE IF THE POCB HAS */ 00073000
*               /* ALREADY BEEN TPQUED TO CLOSE THE POI.THE CHECK IS */ 00074000
*               /* NECESSARY TO PREVENT RECURSIVE ABEND FROM CAUSING */ 00075000
*               /* POCB TO BE QUEUED MORE THAN ONCE.                 */ 00076000
*               /*                                                   */ 00077000
*               /*****************************************************/ 00078000
*                                                               01S0133 00079000
*               CALL CPCCHK;        /* CHECKS DYPAB QUEUE FOR POCB   */ 00080000
         BAL   @14,CPCCHK                                       01S0133 00081000
*               IF CPCFND=ON        /* IF POCB WAS FOUND ON DYPAB       00082000
*                                      QUEUE                         */ 00083000
*                 THEN                                          01S0134 00084000
         LA    @15,176                                          01S0134 00085000
         AL    @15,RPHWEA(,CPCRPHPT)                            01S0134 00086000
         TM    CPCFND(@15),B'10000000'                          01S0134 00087000
         BNO   @RF00134                                         01S0134 00088000
*                 DO;                                           01S0135 00089000
*                   CPCPTH=CPCRPL;  /* CHECK FOR WAITING RPL         */ 00090000
         NI    CPCPTH(@15),B'10111111'                          01S0136 00091000
         OI    CPCPTH(@15),B'10000000'                          01S0136 00092000
*                   CPCFND=OFF;     /* RESET SWITCH                  */ 00093000
         NI    CPCFND(@15),B'01111111'                          01S0137 00094000
*                 END;                                          01S0138 00095000
*               ELSE                                            01S0139 00096000
*                 CPCPTH=CPCLORPL;  /* POCB NOT FOUND,WE WILL   01S0139 00097000
*                                      THEREFORE ISSUE A FULL CLOSE  */ 00098000
         B     @RC00134                                         01S0139 00099000
@RF00134 L     @15,RPHWEA(,CPCRPHPT)                            01S0139 00100000
         OI    CPCPTH+176(@15),B'11000000'                      01S0139 00101000
*             END;                                              01S0140 00102000
*           ELSE                                                01S0141 00103000
*             DO;                                               01S0141 00104000
*                                                               01S0141 00105000
         B     @RC00131                                         01S0141 00106000
@RF00131 DS    0H                                               01S0142 00107000
*               /*****************************************************/ 00108000
*               /*                                                   */ 00109000
*               /* FOLLOWING DO GROUP PROCESSES ACTIVE AND QUIESCED  */ 00110000
*               /* POCBS IT DETERMINES WHETHER OR NOT WE WILL CLOSE  */ 00111000
*               /* SUCCESSFULLY                                      */ 00112000
*               /*                                                   */ 00113000
*               /*****************************************************/ 00114000
*                                                               01S0142 00115000
*               IF CPCBASE=POIPRIMQ /* POCB PRIMARY?                 */ 00116000
*                 THEN                                          01S0142 00117000
         L     @15,RPHWEA(,CPCRPHPT)                            01S0142 00118000
         L     @15,OCWACVT(,@15)                                01S0142 00119000
         L     @14,ATCPOIA-1(,@15)                              01S0142 00120000
         LA    @14,0(,@14)                                      01S0142 00121000
         L     @00,POIPRIMQ-1(,@14)                             01S0142 00122000
         ICM   @00,8,@CB01117                                   01S0142 00123000
         CR    CPCBASE,@00                                      01S0142 00124000
         BNE   @RF00142                                         01S0142 00125000
*                 DO;                                           01S0143 00126000
*                   ATCPOUNS=OFF;   /* UNSOLICITED MSG FLAG          */ 00127000
         NI    ATCPOUNS(@15),B'01111111'                        01S0144 00128000
*                   POIPRIMQ=ZERO;  /* POI PTR TO PRIM IS ZEROED     */ 00129000
         SLR   @00,@00                                          01S0145 00130000
         STCM  @00,7,POIPRIMQ(@14)                              01S0145 00131000
*                 END;                                          01S0146 00132000
*               ELSE                                            01S0147 00133000
*                 ;                                             01S0147 00134000
@RF00142 DS    0H                                               01S0148 00135000
*               IF OCACLDEB=ON      /* ARE WE PROCESSING AN ABEND    */ 00136000
*                 THEN                                          01S0148 00137000
         TM    OCACLDEB(CPCOCAPT),B'01000000'                   01S0148 00138000
         BNO   @RF00148                                         01S0148 00139000
*                 CPCPTH=CPCLORPL;  /* WE WANT A FULL CLOSE          */ 00140000
         L     @15,RPHWEA(,CPCRPHPT)                            01S0149 00141000
         OI    CPCPTH+176(@15),B'11000000'                      01S0149 00142000
*               ELSE                                            01S0150 00143000
*                 IF POCQUI=ON      /* POCB IS QUIESCED              */ 00144000
*                   THEN                                        01S0150 00145000
         B     @RC00148                                         01S0150 00146000
@RF00148 TM    POCQUI(CPCBASE),B'00100000'                      01S0150 00147000
         BNO   @RF00150                                         01S0150 00148000
*                   CPCPTH=CPCLORPL;/* WANT A FULL CLOSE             */ 00149000
         L     @15,RPHWEA(,CPCRPHPT)                            01S0151 00150000
         OI    CPCPTH+176(@15),B'11000000'                      01S0151 00151000
*                 ELSE              /* POCB IS ACTIVE                */ 00152000
*                   IF POCMSGQ=ZERO /* NO OUTSTANDING MSG            */ 00153000
*                     THEN                                      01S0152 00154000
         B     @RC00150                                         01S0152 00155000
@RF00150 L     @00,POCMSGQ-1(,CPCBASE)                          01S0152 00156000
         ICM   @00,8,@CB01117                                   01S0152 00157000
         LTR   @00,@00                                          01S0152 00158000
         BNZ   @RF00152                                         01S0152 00159000
*                     IF POCAWTRQ=ZERO/* NO AWAITING REPLIES         */ 00160000
*                       THEN                                    01S0153 00161000
         L     @00,POCAWTRQ-1(,CPCBASE)                         01S0153 00162000
         ICM   @00,8,@CB01117                                   01S0153 00163000
         LTR   @00,@00                                          01S0153 00164000
         BNZ   @RF00153                                         01S0153 00165000
*                       CPCPTH=CPCLORPL;/* FULL CLOSE                */ 00166000
         L     @15,RPHWEA(,CPCRPHPT)                            01S0154 00167000
         OI    CPCPTH+176(@15),B'11000000'                      01S0154 00168000
*                     ELSE                                      01S0155 00169000
*                       DO;                                     01S0155 00170000
*                                                               01S0155 00171000
         B     @RC00153                                         01S0155 00172000
@RF00153 DS    0H                                               01S0156 00173000
*                         /*******************************************/ 00174000
*                         /*                                         */ 00175000
*                         /* FOLLOWING DO LOOP SEARCHES FOR PORCBS ON*/ 00176000
*                         /* DYPAB QUEUE. IF PORCB IS ON THIS QUEUE  */ 00177000
*                         /* THEN IT HAS ALREADY BEEN SATISFIED AND  */ 00178000
*                         /* CANNOT BE CONSIDERED AWAITING REPLY.    */ 00179000
*                         /*                                         */ 00180000
*                         /*******************************************/ 00181000
*                                                               01S0156 00182000
*                         CPCPOCSV=CPCBASE;                     01S0156 00183000
         LA    @15,176                                          01S0156 00184000
         AL    @15,RPHWEA(,CPCRPHPT)                            01S0156 00185000
         ST    CPCBASE,CPCPOCSV(,@15)                           01S0156 00186000
*                         RFY                                   01S0157 00187000
*                           ISTPOCB BASED(CPCPOCSV);            01S0157 00188000
*                         CPCBASE=POCAWTRQ;/* POINT TO AWAIT. REP. Q */ 00189000
         L     @14,RPHWEA(,CPCRPHPT)                            01S0158 00190000
         L     @14,CPCPOCSV+176(,@14)                           01S0158 00191000
         L     CPCBASE,POCAWTRQ-1(,@14)                         01S0158 00192000
         LA    CPCBASE,0(,CPCBASE)                              01S0158 00193000
*                         RFY                                   01S0159 00194000
*                           CPCPTR RSTD;                        01S0159 00195000
*                         CPCPTR=CPCBASE;                       01S0160 00196000
         LR    CPCPTR,CPCBASE                                   01S0160 00197000
*                         RFY                                   01S0161 00198000
*                           ISTPORCB BASED(CPCPTR);             01S0161 00199000
*                         CPCPTH=CPCRPL;/* SETS THE PATH TO A VALUE     00200000
*                                      OTHER THAN 0'S                */ 00201000
*                                                               01S0162 00202000
         NI    CPCPTH(@15),B'10111111'                          01S0162 00203000
         OI    CPCPTH(@15),B'10000000'                          01S0162 00204000
*                         /*******************************************/ 00205000
*                         /*                                         */ 00206000
*                         /* WE NOW HAVE ALL THE POINTERS TO SEARCH  */ 00207000
*                         /* THROUGH THE PORCB QUEUE                 */ 00208000
*                         /*                                         */ 00209000
*                         /*******************************************/ 00210000
*                                                               01S0163 00211000
*                         DO UNTIL(PORFWD=ZERO|CPCPTH=CPCQUI);  01S0163 00212000
@DL00163 DS    0H                                               01S0164 00213000
*                           CPCPTR=CPCBASE;                     01S0164 00214000
         LR    CPCPTR,CPCBASE                                   01S0164 00215000
*                           CALL CPCCHK;/* SEARCHES THROUGH PAB QUEUE   00216000
*                                      FOR PORCB                     */ 00217000
         BAL   @14,CPCCHK                                       01S0165 00218000
*                           IF CPCFND=ON/* PORCB FOUND               */ 00219000
*                             THEN                              01S0166 00220000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0166 00221000
         TM    CPCFND+176(@11),B'10000000'                      01S0166 00222000
         BNO   @RF00166                                         01S0166 00223000
*                             CPCBASE=PORFWD;/* UPDATE PTR           */ 00224000
         L     CPCBASE,PORFWD-1(,CPCPTR)                        01S0167 00225000
         LA    CPCBASE,0(,CPCBASE)                              01S0167 00226000
*                           ELSE                                01S0168 00227000
*                             CPCPTH=CPCQUI;/* REPLY HAS NOT BEEN       00228000
*                                      PROCESSED                     */ 00229000
         B     @RC00166                                         01S0168 00230000
@RF00166 L     @11,RPHWEA(,CPCRPHPT)                            01S0168 00231000
         NI    CPCPTH+176(@11),B'00111111'                      01S0168 00232000
*                         END;                                  01S0169 00233000
@RC00166 DS    0H                                               01S0169 00234000
@DE00163 L     @11,PORFWD-1(,CPCPTR)                            01S0169 00235000
         LA    @11,0(,@11)                                      01S0169 00236000
         LTR   @11,@11                                          01S0169 00237000
         BZ    @DC00163                                         01S0169 00238000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0169 00239000
         TM    CPCPTH+176(@11),B'11000000'                      01S0169 00240000
         BNZ   @DL00163                                         01S0169 00241000
@DC00163 DS    0H                                               01S0170 00242000
*RESTORE:                                                       01S0170 00243000
*                         CPCBASE=CPCPOCSV;/* RESET POCB PTR         */ 00244000
RESTORE  LA    @11,176                                          01S0170 00245000
         AL    @11,RPHWEA(,CPCRPHPT)                            01S0170 00246000
         L     CPCBASE,CPCPOCSV(,@11)                           01S0170 00247000
*                         RFY                                   01S0171 00248000
*                           ISTPOCB BASED(CPCBASE);             01S0171 00249000
*                         IF CPCPTH^=CPCQUI THEN                01S0172 00250000
         TM    CPCPTH(@11),B'11000000'                          01S0172 00251000
         BZ    @RF00172                                         01S0172 00252000
*                           CPCPTH=CPCLORPL;/* NO REPLIES OUT   01S0173 00253000
*                                      STANDING THEREFORE A FULL        00254000
*                                      CLOSE                         */ 00255000
         OI    CPCPTH(@11),B'11000000'                          01S0173 00256000
*                         ELSE                                  01S0174 00257000
*                           ;                                   01S0174 00258000
@RF00172 DS    0H                                               01S0175 00259000
*                       END;                                    01S0175 00260000
*                   ELSE                                        01S0176 00261000
*                     CPCPTH=CPCQUI;/* WANT TO FAIL CLOSE            */ 00262000
         B     @RC00152                                         01S0176 00263000
@RF00152 L     @11,RPHWEA(,CPCRPHPT)                            01S0176 00264000
         NI    CPCPTH+176(@11),B'00111111'                      01S0176 00265000
*             END;                                              01S0177 00266000
@RC00152 DS    0H                                               01S0177 00267000
@RC00150 DS    0H                                               01S0177 00268000
@RC00148 DS    0H                                               01S0178 00269000
*           IF CPCPTH2=ON THEN                                  01S0178 00270000
@RC00131 L     @11,RPHWEA(,CPCRPHPT)                            01S0178 00271000
         TM    CPCPTH2+176(@11),B'01000000'                     01S0178 00272000
         BNO   @RF00178                                         01S0178 00273000
*             DO;                                               01S0179 00274000
*                                                               01S0179 00275000
*               /*****************************************************/ 00276000
*               /*                                                   */ 00277000
*               /* THE DO GROUP MAKES THE POCB INACTIVE AND TPQUES IT*/ 00278000
*               /* AS A WORK ELEMENT TO THE POI DYPAB TO CLEAN UP    */ 00279000
*               /* QUEUES FOR A SUCCESSFULL CLOSE.                   */ 00280000
*               /*                                                   */ 00281000
*               /*****************************************************/ 00282000
*                                                               01S0180 00283000
*               POCINACT=ON;        /* POCB INACTIVE                 */ 00284000
*               POCQUI=OFF;         /* POCB NO LONGER QUIESCED       */ 00285000
         OI    POCINACT(CPCBASE),B'01000000'                    01S0181 00286000
         NI    POCQUI(CPCBASE),B'11011111'                      01S0181 00287000
*/*  TPQUE (CPCBASE,DYPPAB) NORM                                     */ 00288000
*                                                               01S0182 00289000
*               DO;                                             01S0182 00290000
*                 RFY                                           01S0183 00291000
*                  (GPR00P,                                     01S0183 00292000
*                   GPR01P,                                     01S0183 00293000
*                   GPR04P,                                     01S0183 00294000
*                   GPR05P,                                     01S0183 00295000
*                   GPR15P) RSTD;                               01S0183 00296000
*                 GEN REFS(GPR01P) SETS(RPHWORK)                01S0184 00297000
*                     (STM GPR02P,GPR08P,RPHWORK(GPR01P));      01S0184 00298000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00299000
*                 GPR00P=0;                                     01S0185 00300000
         SLR   GPR00P,GPR00P                                    01S0185 00301000
*                 GPR15P=ADDR(DYPPAB);/* HOLD PAB PTR                */ 00302000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0186 00303000
         L     GPR15P,OCWACVT(,@11)                             01S0186 00304000
         LA    GPR15P,DYPPAB+1552(,GPR15P)                      01S0186 00305000
*                 GPR04P=CPCBASE;   /* WORK ELE PTR                  */ 00306000
         LR    GPR04P,CPCBASE                                   01S0187 00307000
*                 GPR05P=GPR15P;    /* PASS PAB PTR                  */ 00308000
         LR    GPR05P,GPR15P                                    01S0188 00309000
*                 CALL ATCLCPTR->ATCAP37->ISTAPC37;             01S0189 00310000
         L     @11,ATCLCPTR                                     01S0189 00311000
         L     @15,ATCAP37(,@11)                                01S0189 00312000
         BALR  @14,@15                                          01S0189 00313000
*                 GEN(NOPR  0)                                  01S0190 00314000
*                 NOSETS NOREFS;                                01S0190 00315000
         NOPR  0                                                        00316000
*                 RFY                                           01S0191 00317000
*                  (GPR00P,                                     01S0191 00318000
*                   GPR01P,                                     01S0191 00319000
*                   GPR04P,                                     01S0191 00320000
*                   GPR05P,                                     01S0191 00321000
*                   GPR15P) UNRSTD;                             01S0191 00322000
*               END;                /* *** END OF TPQUE GENERATION   */ 00323000
*               OCARC=HEXZERO;                                  01S0193 00324000
         MVI   OCARC(CPCOCAPT),X'00'                            01S0193 00325000
*             END;                                              01S0194 00326000
*           ELSE                                                01S0195 00327000
*             IF CPCPTH=CPCQUI      /* FAIL THE CLOSE                */ 00328000
*               THEN                                            01S0195 00329000
         B     @RC00178                                         01S0195 00330000
@RF00178 L     @11,RPHWEA(,CPCRPHPT)                            01S0195 00331000
         TM    CPCPTH+176(@11),B'11000000'                      01S0195 00332000
         BNZ   @RF00195                                         01S0195 00333000
*               DO;                                             01S0196 00334000
*                 POCQUI=ON;        /* POCB QUIESCED                 */ 00335000
*                 POCACT=OFF;       /* POCB NO LONGER ACTIVE         */ 00336000
         OI    POCQUI(CPCBASE),B'00100000'                      01S0198 00337000
         NI    POCACT(CPCBASE),B'01111111'                      01S0198 00338000
*                 OCARC=CPCABORT;   /* FAIL APPL. CLOSE              */ 00339000
         MVI   OCARC(CPCOCAPT),X'4C'                            01S0199 00340000
*                 OCAABORT=ON;      /* FLAGS WHICH WILL CAUSE CLOSE     00341000
*                                      TO FAIL                       */ 00342000
         OI    OCAABORT(CPCOCAPT),B'00100000'                   01S0200 00343000
*               END;                                            01S0201 00344000
*                                                               01S0201 00345000
*           /*********************************************************/ 00346000
*           /*                                                       */ 00347000
*           /* WILL EXECUTE ONCE ONLY IF WE WANT RPL CHECK           */ 00348000
*           /*                                                       */ 00349000
*           /*********************************************************/ 00350000
*                                                               01S0202 00351000
*           DO CPCINDEX=1 WHILE(CPCPTH1=ON);                    01S0202 00352000
@RF00195 DS    0H                                               01S0202 00353000
@RC00178 LA    @11,1                                            01S0202 00354000
         L     @15,RPHWEA(,CPCRPHPT)                            01S0202 00355000
         ST    @11,CPCINDEX+176(,@15)                           01S0202 00356000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0202 00357000
         TM    CPCPTH1+176(@11),B'10000000'                     01S0202 00358000
         BNO   @DC00202                                         01S0202 00359000
*             IF POCRPL^=ZERO       /* RPL WAITING FOR RCVCMD        */ 00360000
*               THEN                                            01S0203 00361000
         L     @11,POCRPL(,CPCBASE)                             01S0203 00362000
         LTR   @11,@11                                          01S0203 00363000
         BZ    @RF00203                                         01S0203 00364000
*               IF RPLSTYP='FF'X    /* IF IT IS A COPIED RPL         */ 00365000
*                 THEN                                          01S0204 00366000
*                                                               01S0204 00367000
         CLI   RPLSTYP(@11),255                                 01S0204 00368000
         BNE   @RF00204                                         01S0204 00369000
*                 /***************************************************/ 00370000
*                 /*                                                 */ 00371000
*                 /* RELSTORE BUFADDR(POCRPL)                        */ 00372000
*                 /*                                                 */ 00373000
*                 /***************************************************/ 00374000
*                                                               01S0205 00375000
*                 DO;                                           01S0205 00376000
*                                                               01S0205 00377000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00378000
*/* END OF ATBUILD                                                   */ 00379000
*                                                               01S0206 00380000
*                   RFY                                         01S0206 00381000
*                    (GPR02P,                                   01S0206 00382000
*                     GPR03P,                                   01S0206 00383000
*                     GPR04P,                                   01S0206 00384000
*                     GPR15P) RSTD;                             01S0206 00385000
*                   RFY                                         01S0207 00386000
*                    (GPR01P,                                   01S0207 00387000
*                     GPR13P) RSTD;                             01S0207 00388000
*                   GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);  00389000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00390000
*                   RFY                                         01S0209 00391000
*                     GPR13P UNRSTD;                            01S0209 00392000
*                   GPR04P=POCRPL;                              01S0210 00393000
         L     GPR04P,POCRPL(,CPCBASE)                          01S0210 00394000
*                   DCL                                         01S0211 00395000
*                     1 FMZA0018 FIXED(31) STATIC LOCAL,        01S0211 00396000
*                      2 * FIXED(8) INIT(64),                   01S0211 00397000
*                      2 * PTR(24) INIT(0);                     01S0211 00398000
*                   GPR03F=FMZA0018;                            01S0212 00399000
         L     GPR03F,FMZA0018                                  01S0212 00400000
*                   DO;             /* LISATCVT                 01S0213 00401000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00402000
*                     GPR15P=ATCLCPTR;/* SET UP ATCVT POINTER        */ 00403000
         L     GPR15P,ATCLCPTR                                  01S0214 00404000
*                   END;            /* LISATCVT                      */ 00405000
*                   GPR02P=GPR15P->ATCBPDA;                     01S0216 00406000
         L     GPR02P,ATCBPDA(,GPR15P)                          01S0216 00407000
*                   GPR15P=GPR15P->ATCSMRS;                     01S0217 00408000
         L     GPR15P,ATCSMRS(,GPR15P)                          01S0217 00409000
*                   DCL                                         01S0218 00410000
*                     FMZL0018 BASED(GPR15P) LABEL VALUERANGE(*);       00411000
*                   CALL FMZL0018;                              01S0219 00412000
         BALR  @14,@15                                          01S0219 00413000
*                   RFY                                         01S0220 00414000
*                     GPR13P RSTD;                              01S0220 00415000
*                   GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);  00416000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              00417000
*                   RFY                                         01S0222 00418000
*                    (GPR01P,                                   01S0222 00419000
*                     GPR13P) UNRSTD;                           01S0222 00420000
*                   RFY                                         01S0223 00421000
*                    (GPR02P,                                   01S0223 00422000
*                     GPR03P,                                   01S0223 00423000
*                     GPR04P,                                   01S0223 00424000
*                     GPR15P) UNRSTD;                           01S0223 00425000
*                 END;              /* *********END OF RELSTORE 01S0224 00426000
*                                      EXPANSION*********            */ 00427000
*           END;                                                01S0225 00428000
@RF00204 DS    0H                                               01S0225 00429000
@RF00203 DS    0H                                               01S0225 00430000
@DC00202 DS    0H                                               01S0226 00431000
*         END;                                                  01S0226 00432000
*     END;                                                      01S0227 00433000
         EJECT                                                          00434000
*CPCBFIND:                                                      01S0228 00435000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0228 00436000
         B     @PB00002                                         01S0228 00437000
CPCBFIND DS    0H                                               01S0229 00438000
*   RFY                                                         01S0229 00439000
*     CPCRETPT RSTD;                                            01S0229 00440000
*   CPCR14SV=CPCRETPT;              /* SAVE VALUE OF RETURN ADDRESS  */ 00441000
         L     @15,RPHWEA(,CPCRPHPT)                            01S0230 00442000
         LA    @11,176                                          01S0230 00443000
         ALR   @11,@15                                          01S0230 00444000
         ST    CPCRETPT,CPCR14SV(,@11)                          01S0230 00445000
*   RFY                                                         01S0231 00446000
*     CPCRETPT UNRSTD;                                          01S0231 00447000
*   CPCBASE=POIPOCBQ;               /* SET PTR TO POCB QUEUE         */ 00448000
         L     @15,OCWACVT(,@15)                                01S0232 00449000
         L     @15,ATCPOIA-1(,@15)                              01S0232 00450000
         LA    @15,0(,@15)                                      01S0232 00451000
         L     CPCBASE,POIPOCBQ-1(,@15)                         01S0232 00452000
         LA    CPCBASE,0(,CPCBASE)                              01S0232 00453000
*   RFY                                                         01S0233 00454000
*     ISTPOCB BASED(CPCBASE);                                   01S0233 00455000
*   CPCFND=OFF;                     /* INIT FOUND SWITCH             */ 00456000
*                                                               01S0234 00457000
         NI    CPCFND(@11),B'01111111'                          01S0234 00458000
*   /*****************************************************************/ 00459000
*   /*                                                               */ 00460000
*   /* FOLLOWING DO LOOP SEARCHES POCB QUEUE FOR CORRECT POCB DESIRED*/ 00461000
*   /* POCB ADDRESS WILL BE IN CPCBASE AT EXIT FROM LOOP             */ 00462000
*   /*                                                               */ 00463000
*   /*****************************************************************/ 00464000
*                                                               01S0235 00465000
*   DO WHILE(POCFWD^=ZERO&POCDEB^=OCWACDEB);                    01S0235 00466000
         B     @DE00235                                         01S0235 00467000
@DL00235 DS    0H                                               01S0236 00468000
*     CPCBASE=POCFWD;               /* STEP TO NEXT POCB             */ 00469000
         L     @11,POCFWD-1(,CPCBASE)                           01S0236 00470000
         LA    @11,0(,@11)                                      01S0236 00471000
         LR    CPCBASE,@11                                      01S0236 00472000
*   END;                                                        01S0237 00473000
*                                                               01S0237 00474000
@DE00235 L     @11,POCFWD-1(,CPCBASE)                           01S0237 00475000
         LA    @11,0(,@11)                                      01S0237 00476000
         LTR   @11,@11                                          01S0237 00477000
         BZ    @DC00235                                         01S0237 00478000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0237 00479000
         CLC   POCDEB(4,CPCBASE),OCWACDEB(@11)                  01S0237 00480000
         BNE   @DL00235                                         01S0237 00481000
@DC00235 DS    0H                                               01S0238 00482000
*   /*****************************************************************/ 00483000
*   /*                                                               */ 00484000
*   /* WE MUST NOW SEARCH THE LAST POCB IF NOT YET FOUND             */ 00485000
*   /*                                                               */ 00486000
*   /*****************************************************************/ 00487000
*                                                               01S0238 00488000
*   IF POCDEB=OCWACDEB THEN                                     01S0238 00489000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0238 00490000
         CLC   POCDEB(4,CPCBASE),OCWACDEB(@11)                  01S0238 00491000
         BNE   @RF00238                                         01S0238 00492000
*     CPCFND=ON;                                                01S0239 00493000
         OI    CPCFND+176(@11),B'10000000'                      01S0239 00494000
*   ELSE                                                        01S0240 00495000
*     ;                                                         01S0240 00496000
@RF00238 DS    0H                                               01S0241 00497000
*   RFY                                                         01S0241 00498000
*     CPCRETPT RSTD;                                            01S0241 00499000
*   CPCRETPT=CPCR14SV;              /* RESTORE RET ADD               */ 00500000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0242 00501000
         L     CPCRETPT,CPCR14SV+176(,@11)                      01S0242 00502000
*   RFY                                                         01S0243 00503000
*     CPCRETPT UNRSTD;                                          01S0243 00504000
*   RETURN;                                                     01S0244 00505000
@EL00002 DS    0H                                               01S0244 00506000
@EF00002 DS    0H                                               01S0244 00507000
@ER00002 BR    @14                                              01S0244 00508000
*   END CPCBFIND;                                               01S0245 00509000
         EJECT                                                          00510000
*CPCCHK:                                                        01S0246 00511000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0246 00512000
*                                                               01S0246 00513000
CPCCHK   DS    0H                                               01S0247 00514000
*   /*****************************************************************/ 00515000
*   /*                                                               */ 00516000
*   /* THE SUBROUTINE CHECKS TO SEE IF THE WORK ELEMENT IN IS QUEUED */ 00517000
*   /* OFF THE POI DYPAB                                             */ 00518000
*   /*                                                               */ 00519000
*   /*****************************************************************/ 00520000
*                                                               01S0247 00521000
*   RFY                                                         01S0247 00522000
*     CPCRETPT RSTD;                                            01S0247 00523000
*   CPCR14SV=CPCRETPT;              /* SAVES RETURN ADDRESS          */ 00524000
         L     @15,RPHWEA(,CPCRPHPT)                            01S0248 00525000
         LA    @11,176                                          01S0248 00526000
         ALR   @11,@15                                          01S0248 00527000
         ST    CPCRETPT,CPCR14SV(,@11)                          01S0248 00528000
*   RFY                                                         01S0249 00529000
*     CPCRETPT UNRSTD;                                          01S0249 00530000
*   IF PABWEQP=ZERO                 /* THERE ARE NO ELEMENTS ON DYPAB   00531000
*                                      QUEUE                         */ 00532000
*     THEN                                                      01S0250 00533000
         L     @15,OCWACVT(,@15)                                01S0250 00534000
         L     @00,PABWEQP+1567(,@15)                           01S0250 00535000
         ICM   @00,8,@CB01117                                   01S0250 00536000
         LTR   @00,@00                                          01S0250 00537000
         BNZ   @RF00250                                         01S0250 00538000
*     CPCFND=OFF;                   /* OBVIOUSLY THE ELEMENT ISN'T ON   00539000
*                                      THE QUEUE                     */ 00540000
         NI    CPCFND(@11),B'01111111'                          01S0251 00541000
*   ELSE                                                        01S0252 00542000
*     DO;                                                       01S0252 00543000
*                                                               01S0252 00544000
         B     @RC00250                                         01S0252 00545000
@RF00250 DS    0H                                               01S0253 00546000
*/*   LOOP SEARCHES THE QUEUE                                        */ 00547000
*                                                               01S0253 00548000
*       RFY                                                     01S0253 00549000
*         CPCREG8 RSTD;                                         01S0253 00550000
*       CPCREG8=PABWEQP;                                        01S0254 00551000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0254 00552000
         L     @15,OCWACVT(,@11)                                01S0254 00553000
         L     CPCREG8,PABWEQP+1567(,@15)                       01S0254 00554000
         LA    CPCREG8,0(,CPCREG8)                              01S0254 00555000
*       CPCFND=OFF;                 /* INIT FOUND SWITCH             */ 00556000
         NI    CPCFND+176(@11),B'01111111'                      01S0255 00557000
*       RFY                                                     01S0256 00558000
*         ISTPOCB BASED(CPCREG8);   /* WE NOW HAVE PTRS SET UP       */ 00559000
*       DO WHILE(POCPSSQ^=ZERO&CPCREG8^=CPCBASE);               01S0257 00560000
         B     @DE00257                                         01S0257 00561000
@DL00257 DS    0H                                               01S0258 00562000
*         CPCREG8=POCPSSQ;          /* UPDATE TO NEXT ELEMENT        */ 00563000
         L     CPCREG8,POCPSSQ(,CPCREG8)                        01S0258 00564000
*       END;                                                    01S0259 00565000
@DE00257 L     @11,POCPSSQ(,CPCREG8)                            01S0259 00566000
         LTR   @11,@11                                          01S0259 00567000
         BZ    @DC00257                                         01S0259 00568000
         CR    CPCREG8,CPCBASE                                  01S0259 00569000
         BNE   @DL00257                                         01S0259 00570000
@DC00257 DS    0H                                               01S0260 00571000
*       IF CPCREG8=CPCBASE THEN                                 01S0260 00572000
         CR    CPCREG8,CPCBASE                                  01S0260 00573000
         BNE   @RF00260                                         01S0260 00574000
*         CPCFND=ON;                /* SET FOUND RESPONSE            */ 00575000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0261 00576000
         OI    CPCFND+176(@11),B'10000000'                      01S0261 00577000
*       ELSE                                                    01S0262 00578000
*         ;                                                     01S0262 00579000
@RF00260 DS    0H                                               01S0263 00580000
*     END;                                                      01S0263 00581000
*   RFY                                                         01S0264 00582000
*     ISTPOCB BASED(CPCBASE);                                   01S0264 00583000
@RC00250 DS    0H                                               01S0265 00584000
*   RFY                                                         01S0265 00585000
*     CPCREG8 UNRSTD;                                           01S0265 00586000
*   RFY                                                         01S0266 00587000
*     CPCRETPT RSTD;                                            01S0266 00588000
*   CPCRETPT=CPCR14SV;              /* RESTORES RETURN ADDRESS       */ 00589000
         L     @11,RPHWEA(,CPCRPHPT)                            01S0267 00590000
         L     CPCRETPT,CPCR14SV+176(,@11)                      01S0267 00591000
*   RFY                                                         01S0268 00592000
*     CPCRETPT UNRSTD;                                          01S0268 00593000
*   RETURN;                                                     01S0269 00594000
@EL00003 DS    0H                                               01S0269 00595000
@EF00003 DS    0H                                               01S0269 00596000
@ER00003 BR    @14                                              01S0269 00597000
*   END CPCCHK;                                                 01S0270 00598000
*                                                               01S0270 00599000
@PB00003 DS    0H                                               01S0271 00600000
*/* TPEPILOG                                                         */ 00601000
*                                                               01S0271 00602000
*   GEN(PRINT OFF);                                             01S0271 00603000
         PRINT OFF                                                      00604000
*/*      VTAM PROCESS OPTION DEFINITION BLOCK                       */  00605000
*/*  USE RESERVED BIT                                  002 06/10/74 */  00606000
*/*                                                                 */  00607000
*DECLARE                                                        01S0272 00608000
*  1 ISTPROCD BDY(WORD) BASED,                                  01S0272 00609000
*    2 PROPROC  BIT(32),          /* PROCESSING OPTIONS             */  00610000
*      3 PROPROC1 BIT(8),         /* PROCESSING OPTIONS - 1         */  00611000
*        4 PROASYIP BIT(1),       /* 1=ASYIPX 0=NASYIPX             */  00612000
*        4 PROTRUNC BIT(1),       /* 1=TRUNC 0=KEEP                 */  00613000
*        4 PROXPOPT BIT(1),       /* 1=BINARY 0=NBINARY             */  00614000
