*        4 PRORESPX BIT(1),       /* PROC-0=NRESPX/1=RESPX          */  00616000
*        4 PROCA    BIT(1),       /* 1=RECEIVE ANY CA/0=NO CA       */  00617000
*        4 PROCS    BIT(1),       /* 1=RECEIVE ANY CS/0=NO CS       */  00618000
