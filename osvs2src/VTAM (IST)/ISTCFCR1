*  R14SVPTR = R14SVPTR + 4;                                        0415 04014000
*                                                                  0415 04015000
         AL    @03,@CF00076                                        0415 04016000
         ST    @03,R14SVPTR(,@05)                                  0415 04017000
* IF POCMSGQ = ZERO                                                0416 04018000
*   THEN                         /* NO MSGS FOR THE POA */         0416 04019000
         L     @05,POCMSGQ-1(,CR1POCB)                             0416 04020000
         LA    @05,0(,@05)                                         0416 04021000
         LTR   @05,@05                                             0416 04022000
         BNZ   @RF00416                                            0416 04023000
*      IF RPLQOPT = CON                                            0417 04024000
*         THEN                   /* RCVCMD REQUEST IS TO BE Q'D*/  0417 04025000
         L     @05,OCWRPL(,OCWPTR)                                 0417 04026000
         TM    RPLQOPT(@05),B'00010000'                            0417 04027000
         BNO   @RF00417                                            0417 04028000
*           IF POCACT = CON                                        0418 04029000
*             THEN               /* Q'D REQUESTS ALLOWED ONLY */   0418 04030000
         TM    POCACT(CR1POCB),B'10000000'                         0418 04031000
         BNO   @RF00418                                            0418 04032000
*                                /* IN ACTIVE STATUS.        */    0419 04033000
*              IF POCRPL = ZERO                                    0419 04034000
*                THEN            /*NO RCVCMDS ALREADY Q'D */       0419 04035000
         L     @03,POCRPL(,CR1POCB)                                0419 04036000
         LTR   @03,@03                                             0419 04037000
         BNZ   @RF00419                                            0419 04038000
*                  DO;           /* ONLY 1 ALLOWED TO BE Q'D */    0420 04039000
*                  POCRPL = OCWRPL;    /* STORE THE ADDRESS OF THE */   04040000
         ST    @05,POCRPL(,CR1POCB)                                0421 04041000
*                                      /* RPL IN THE POCB, SO THAT */   04042000
*                                      /* WHEN A MESSAGE FORMS IT  */   04043000
*                                      /* WILL BE SENT TO SATISFY  */   04044000
*                                      /* THIS REQUEST.            */   04045000
*                  OCWNPOST = CON;     /* TURN ON 'NO POST' FLAG,  */   04046000
         OI    OCWNPOST(OCWPTR),B'01000000'                        0422 04047000
*                                      /* SINCE THIS REQUEST WAS   */   04048000
*                                      /* NOT SATISFIED, THE USER  */   04049000
*                                      /* IS NOT TO BE POSTED BACK,*/   04050000
*                                      /* THIS FLAG WILL PREVENT   */   04051000
*                                      /* THE POSTING.             */   04052000
*                  END;                                            0423 04053000
*                ELSE DO;         /* ONLY 1 Q'D REQUEST IS ALLOWED */   04054000
         B     @RC00419                                            0424 04055000
@RF00419 DS    0H                                                  0425 04056000
*                     RPLRTNCD = '08'X;                            0425 04057000
         L     @05,OCWRPL(,OCWPTR)                                 0425 04058000
         MVI   RPLRTNCD(@05),X'08'                                 0425 04059000
*                     RPLFDB2 = '6C'X;                             0426 04060000
         MVI   RPLFDB2(@05),X'6C'                                  0426 04061000
*                     END;                                         0427 04062000
*              ELSE DO;          /* THE APPL IS CLOSING */         0428 04063000
         B     @RC00418                                            0428 04064000
@RF00418 DS    0H                                                  0429 04065000
*                    RPLRTNCD = '08'X;                             0429 04066000
         L     @05,OCWRPL(,OCWPTR)                                 0429 04067000
         MVI   RPLRTNCD(@05),X'08'                                 0429 04068000
*                    RPLFDB2 = '70'X;                              0430 04069000
         MVI   RPLFDB2(@05),X'70'                                  0430 04070000
*                   END;                                           0431 04071000
*           ELSE DO;             /* NO MSGS AVAILIABLE */          0432 04072000
         B     @RC00417                                            0432 04073000
@RF00417 DS    0H                                                  0433 04074000
*                RPLRTNCD = '38'X;                                 0433 04075000
         L     @05,OCWRPL(,OCWPTR)                                 0433 04076000
         MVI   RPLRTNCD(@05),X'38'                                 0433 04077000
*                RPLFDB2 = '00'X;                                  0434 04078000
         MVI   RPLFDB2(@05),X'00'                                  0434 04079000
*                END;                                              0435 04080000
*        ELSE                    /* THERE ARE MSGS AVAILIABLE */   0436 04081000
*          IF POCRPL ^= ZERO                                       0436 04082000
*            THEN                 /* THERE IS A Q'D RCVCMD REQUEST*/    04083000
         B     @RC00416                                            0436 04084000
