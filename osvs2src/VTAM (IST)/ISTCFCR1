         BE    @RF00436                                            0436 04088000
*              IF (RPLTCRNO = CON) & (POCRPL = OCWRPL)             0437 04089000
*                THEN             /* THIS IS A REDRIVEN RPL FOR */ 0437 04090000
         L     @11,OCWRPL(,OCWPTR)                                 0437 04091000
         TM    RPLTCRNO(@11),B'00000010'                           0437 04092000
         BNO   @RF00437                                            0437 04093000
         CR    @03,@11                                             0437 04094000
         BNE   @RF00437                                            0437 04095000
*                                 /* THE Q'D REQUEST.           */ 0438 04096000
*                   DO;                                            0438 04097000
*                   RPLTCRNO = COFF;  /* NO LONGER REDRIVEN */     0439 04098000
         NI    RPLTCRNO(@11),B'11111101'                           0439 04099000
*                   POCRPL = ZERO;                                 0440 04100000
         ST    @05,POCRPL(,CR1POCB)                                0440 04101000
*                   CALL CR1TRMSG;     /* INVOKE ROUTINE TO TRANSFER*/  04102000
         BAL   @14,CR1TRMSG                                        0441 04103000
*                                      /* MESSAGE TO AREA POINTED  */   04104000
*                                      /* BY RPL.                 */    04105000
*                   END;                                           0442 04106000
*                 ELSE        /* Q'D REQUEST IS NOT TO BE SATISFIED */  04107000
*                    DO;                                           0443 04108000
         B     @RC00437                                            0443 04109000
