         MVI   RPLRTNCD(@05),X'08'                                 0444 04113000
*                    RPLFDB2 = '6C'X;                              0445 04114000
         MVI   RPLFDB2(@05),X'6C'                                  0445 04115000
*                    END;                                          0446 04116000
*             ELSE               /* NO REQUEST WAS Q'D */          0447 04117000
*                CALL CR1TRMSG;  /*INVOKE ROUTINE TO TRANSFER MSG TO*/  04118000
