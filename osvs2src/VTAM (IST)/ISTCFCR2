*           END;                    /* *** END OF TPQUE GENERATION   */ 01960000
*         END;                                                  01S0701 01961000
*       ELSE                                                    01S0702 01962000
*         ;                         /* EITHER WE HAD A GETSTORE FAIL    01963000
*                                      OR THERE IS NO WAITING RPL    */ 01964000
@RF00687 DS    0H                                               01S0703 01965000
*     END;                                                      01S0703 01966000
*                                                               01S0703 01967000
*   /*****************************************************************/ 01968000
*   /*                                                               */ 01969000
*   /* THE ORIGINAL GETSTOR FOR THE POMCB FAILED SO WE MUST ISSUE THE*/ 01970000
*   /* MESSAGE AS AN IWTO/R TO THE MASTER CONSOLE WITH AN ERROR MSG  */ 01971000
*   /*                                                               */ 01972000
*   /*****************************************************************/ 01973000
*                                                               01S0704 01974000
*   ELSE                                                        01S0704 01975000
*     DO;                                                       01S0704 01976000
         B     @RC00424                                         01S0704 01977000
@RF00424 DS    0H                                               01S0705 01978000
*       CR2R1SV=CR2RPHPT;           /* SAVE RPH @                    */ 01979000
         LR    CR2R1SV,CR2RPHPT                                 01S0705 01980000
*       RFY                                                     01S0706 01981000
*         ISTRPH BASED(CR2R1SV);                                01S0706 01982000
*                                                               01S0706 01983000
