         BNO   @RF00235                                         01S0235 00383000
         TM    POCINACT(@11),B'01000000'                        01S0235 00384000
         BNZ   @RF00235                                         01S0235 00385000
*               DO;                                             01S0236 00386000
*                 RFY                                           01S0237 00387000
*                   CR2PTR RSTD;                                01S0237 00388000
*                 CR2PTR=POIPRIMQ;  /* SAVES POCB ADDRESS            */ 00389000
