         TM    CR2ERR(CR2WAPTR),B'01000000'                     01S0687 01911000
         BNZ   @RF00687                                         01S0687 01912000
         TM    RPLTCRNO(@15),B'00000010'                        01S0687 01913000
         BNZ   @RF00687                                         01S0687 01914000
*         DO;                                                   01S0688 01915000
*                                                               01S0688 01916000
*           /*********************************************************/ 01917000
*           /*                                                       */ 01918000
*           /* IT TO THE SSPAB IN THE CORRECT ACDEB                  */ 01919000
*           /*                                                       */ 01920000
*           /*********************************************************/ 01921000
*                                                               01S0689 01922000
*           RPLTCRNO=ON;            /* TELLS THE RCVCMD PROCESS THAT    01923000
*                                      IT IS REDRIVEN RPL            */ 01924000
         OI    RPLTCRNO(@15),B'00000010'                        01S0689 01925000
*/*  TPQUE (POCRPL,ACDSSPAB) NORM                                    */ 01926000
*                                                               01S0690 01927000
*           DO;                                                 01S0690 01928000
*             RFY                                               01S0691 01929000
*              (GPR00P,                                         01S0691 01930000
*               GPR01P,                                         01S0691 01931000
*               GPR04P,                                         01S0691 01932000
*               GPR05P,                                         01S0691 01933000
