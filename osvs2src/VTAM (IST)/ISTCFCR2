*                 CR2R1SV=CR2RPHPT; /* SAVE RPH PTR ACROSS IWTO/R       00400000
*                                      CALL                          */ 00401000
         LR    CR2R1SV,CR2RPHPT                                 01S0243 00402000
*                 RFY                                           01S0244 00403000
*                   ISTRPH BASED(CR2R1SV);                      01S0244 00404000
