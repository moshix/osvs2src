@RF00246 DS    0H                                               01S0253 00450000
*                     GEN;                                      01S0253 00451000
         WTO   MF=(E,(1))                                               00452000
*                   END;            /* IWTO                          */ 00453000
*                 CR2RPHPT=CR2R1SV; /* RESTORE REG 1 RPH PTR         */ 00454000
@RC00246 LR    CR2RPHPT,CR2R1SV                                 01S0255 00455000
*                 RFY                                           01S0256 00456000
*                   ISTRPH BASED(CR2RPHPT);                     01S0256 00457000
*               END;                                            01S0257 00458000
*           END;                                                01S0258 00459000
*         ELSE                                                  01S0259 00460000
*           DO;                                                 01S0259 00461000
*                                                               01S0259 00462000
         B     @RC00232                                         01S0259 00463000
@RF00232 DS    0H                                               01S0260 00464000
*             /*******************************************************/ 00465000
*             /*                                                     */ 00466000
*             /* THE MESSAGE IS SOLICITED                            */ 00467000
*             /*                                                     */ 00468000
*             /*******************************************************/ 00469000
*                                                               01S0260 00470000
*             RFY                                               01S0260 00471000
