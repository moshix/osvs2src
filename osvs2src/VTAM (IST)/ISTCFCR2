*/* *** EQUATES FOR ZCRRCDSV *** */                             01S1443 05551000
*                                                               01S1443 05552000
*DCL ZCRMRFSB BIT(8) CONSTANT('0C'X);  /* REQUEST COMPLETION MOVE FSB   05553000
*                                         TO PROCESS                 */ 05554000
*DCL ZCRSRCHQ BIT(8) CONSTANT('10'X);  /* REQUEST COMPLETION SEARCH     05555000
*                                         REQUEST QUEUE              */ 05556000
*DCL ZCREXIT  BIT(8) CONSTANT('14'X);  /* REQUEST COMPLETION EXIT    */ 05557000
*DCL ZCRSFSB  BIT(8) CONSTANT('18'X);  /* REQUEST COMPLETION SINGLE     05558000
*                                         FSB                        */ 05559000
*DCL ZCRDLFSB BIT(8) CONSTANT('1C'X);  /* REQUEST COMPLETION DUAL FSB * 05560000
*DCL ZCRNOSCH BIT (8) CONSTANT('20'X); /* REQUEST COMPLETION, EXIT WITH 05561000
*                                         SCHED NONE                 */ 05562000
*DCL ZCRNRSCH BIT (8) CONSTANT('24'X); /* REQUEST COMPLETION, EXIT WITH 05563000
*                                         SCHED NORM                 */ 05564000
