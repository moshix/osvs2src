*               RFY                                             01S0236 00468000
*                 CRPBASE RSTD;                                 01S0236 00469000
*               CVB(CRPBASE,CRPDW); /* CONVERTS VALUE TO BINARY      */ 00470000
         CVB   CRPBASE,CRPDW(,@11)                              01S0237 00471000
*               RFY                                             01S0238 00472000
*                 CRPREG15 RSTD;                                01S0238 00473000
*                                                               01S0238 00474000
*               /*****************************************************/ 00475000
*               /*                                                   */ 00476000
*               /* FOLLOWING RFY WILL POINT CRPCHARA TO THE BYTE CON-*/ 00477000
*               /* TAINING THE BIT TO BE TURNED OFF                  */ 00478000
*               /*                                                   */ 00479000
*               /*****************************************************/ 00480000
*                                                               01S0239 00481000
*               RFY                                             01S0239 00482000
*                 CRPCHARA BASED(EVAL(ADDR(POCRPGEN)+CRPBASE/8));       00483000
*               CRPREG15=CRPWRO2((CRPBASE//8)+1);/* DETERMINES WHICH    00484000
*                                      BIT SEQUENCE WILL BE USED TO     00485000
*                                      TURN OFF CORRECT BIT WITH AN     00486000
*                                      AND                           */ 00487000
         LR    @14,CRPBASE                                      01S0240 00488000
         SRDA  @14,32                                           01S0240 00489000
