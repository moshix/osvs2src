*               RFY                                             01S0243 00509000
*                 CRPREG15 UNRSTD;                              01S0243 00510000
*                                                               01S0243 00511000
*               /*****************************************************/ 00512000
*               /*                                                   */ 00513000
*               /* WE WILL NOW TPQUE THE PORCB AS A WORK ELEMENT TO  */ 00514000
*               /* THE POI DYPPAB TO CLEAN IT UP AND POST VTAM THE   */ 00515000
*               /* MESSAGE TEXT WILL ALSO BE MOVED FROM IT TO VTAM   */ 00516000
*               /* REPLY AREA                                        */ 00517000
*               /*                                                   */ 00518000
*               /*****************************************************/ 00519000
*                                                               01S0244 00520000
*               POMFWD=CRPPOCB;     /* SAVE POCB PTR TO PASS IT TO      00521000
*                                      CFCR2,THE REP PURGE ROUTINE   */ 00522000
