*DCL ZCTYPE BIT(8) CONSTANT('00'X);   /* ZCR TYPE                   */  03178000
*DCL ZCLNG FIXED(15) CONSTANT(172);   /* ZCRR LENGTH                */  03179000
*DCL ZCRSAVNO BIT(8) CONSTANT('06'X); /* NUMBER OF SAVEAREAS IN ZCRR */ 03180000
*DCL ZCRLSVNO BIT(8) CONSTANT('04'X); /* NUMBER OF SAVE AREAS IN        03181000
*                                         LOCAL SUPPORT              */ 03182000
*/* *** EQUATES FOR ZCRRCDSV *** */                             01S0634 03183000
*                                                               01S0634 03184000
*DCL ZCRMRFSB BIT(8) CONSTANT('0C'X);  /* REQUEST COMPLETION MOVE FSB   03185000
*                                         TO PROCESS                 */ 03186000
*DCL ZCRSRCHQ BIT(8) CONSTANT('10'X);  /* REQUEST COMPLETION SEARCH     03187000
*                                         REQUEST QUEUE              */ 03188000
*DCL ZCREXIT  BIT(8) CONSTANT('14'X);  /* REQUEST COMPLETION EXIT    */ 03189000
*DCL ZCRSFSB  BIT(8) CONSTANT('18'X);  /* REQUEST COMPLETION SINGLE     03190000
*                                         FSB                        */ 03191000
*DCL ZCRDLFSB BIT(8) CONSTANT('1C'X);  /* REQUEST COMPLETION DUAL FSB * 03192000
