*                     END;                                      01S0230 00443000
*                 END;                                          01S0231 00444000
@RC00219 DS    0H                                               01S0232 00445000
*             END;                                              01S0232 00446000
@RC00199 DS    0H                                               01S0233 00447000
*           IF CRPERR^=ON           /* NO ERROR FOUND YET            */ 00448000
*             THEN                                              01S0233 00449000
