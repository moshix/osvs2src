*     CMZRA048=0;                                               01S0345 00833000
         STCM  @09,7,CMZRA048(REG1048)                          01S0345 00834000
*     CMZEA048=0;                                               01S0346 00835000
         ST    @09,CMZEA048(,REG1048)                           01S0346 00836000
*     CMZVP048=0;                                               01S0347 00837000
         ST    @09,CMZVP048(,REG1048)                           01S0347 00838000
*     CMZID048=CRPMSGID;                                        01S0348 00839000
         ST    CRPMSGID,CMZID048(,REG1048)                      01S0348 00840000
