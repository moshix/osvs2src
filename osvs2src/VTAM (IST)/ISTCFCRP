*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0326 00769000
*                                                               01S0326 00770000
         B     @PB00002                                         01S0326 00771000
CRPMSG   DS    0H                                               01S0327 00772000
*   /*****************************************************************/ 00773000
*   /*                                                               */ 00774000
*   /* THIS ROUTINE SETS UP AREAS TO ISSUE A TPMSG. AREAS ARE        */ 00775000
*   /* INITIALIZED IN POPWA                                          */ 00776000
*   /*                                                               */ 00777000
*   /*****************************************************************/ 00778000
*                                                               01S0327 00779000
*   RFY                                                         01S0327 00780000
*    (CRPRTPT,                                                  01S0327 00781000
*     CRPSVPT) RSTD;                                            01S0327 00782000
*   CRPR14SV=CRPRTPT;               /* REG 14 IS SAVED FOR RETURN @  */ 00783000
         LA    @15,176                                          01S0328 00784000
         ALR   @15,CRPOCWPT                                     01S0328 00785000
         ST    CRPRTPT,CRPR14SV(,@15)                           01S0328 00786000
*   CRPR13SV=CRPSVPT;               /* REG 13 SAVE AREA @ IS SAVED   */ 00787000
         ST    CRPSVPT,CRPR13SV(,@15)                           01S0329 00788000
*   RFY                                                         01S0330 00789000
*     CRPRTPT UNRSTD;                                           01S0330 00790000
*   CRPSVPT=ADDR(POPSAVE);          /* PROVIDES SAVE AREA FOR TPMSG  */ 00791000
         LA    @11,336                                          01S0331 00792000
         ALR   @11,CRPOCWPT                                     01S0331 00793000
         LA    CRPSVPT,POPSAVE(,@11)                            01S0331 00794000
*   RFY                                                         01S0332 00795000
*    (CRPMSGID,                                                 01S0332 00796000
*     CRPVARDT) RSTD;                                           01S0332 00797000
*   CRPMSGID=ADDR(POPMSGID);        /* SET POINTER TO MSG ID         */ 00798000
         LA    CRPMSGID,POPMSGID(,@11)                          01S0333 00799000
*   CRPVARDT=ADDR(CRPNUM);          /* SET PTR TO VAR DATA           */ 00800000
         LA    CRPVARDT,CRPNUM(,@15)                            01S0334 00801000
*/*TPMSG MSGID((CRPMSGID))MSG((CRPVARDT))OPTIONS(POPTPOPT)PLAREA(POPPLA 00802000
*                                                                    */ 00803000
*                                                               01S0335 00804000
*   DO;                                                         01S0335 00805000
*     DCL                                                       01S0336 00806000
