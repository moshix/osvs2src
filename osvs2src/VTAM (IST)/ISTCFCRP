SVTM     TITLE ' COMMAND FAC-- ISTCFCRP -- POI REPLY PROCESSOR         *00001000
                        '                                               00002000
ISTCFCRP CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTCFCRP  77.221'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         EJECT                                                          00014000
*                                                               01S0129 00015000
*   /*****************************************************************/ 00016000
*   /*                                                               */ 00017000
*   /* MAINLINE CODE OF ISTCFCRP -- POI REPLY PROCESS                */ 00018000
*   /*                                                               */ 00019000
*   /*****************************************************************/ 00020000
*                                                               01S0129 00021000
*   DO;                                                         01S0129 00022000
*                                                               01S0129 00023000
*/***  CRRMAINT  PROCNM(ISTCFCRP)COMP(CF)FUNC(SUB)RPHADD((CRPRPHPT))  * 00024000
*                                                                    */ 00025000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTCFCRR)                        */ 00026000
*/* END OF ATBUILD                                                   */ 00027000
*/* ** SET ENTERED BIT FOR THIS PROCEDURE **                         */ 00028000
*                                                               01S0130 00029000
*     CRPRPHPT->RPHCRR->CRACRR->NCFCRP='1'B;                    01S0130 00030000
*                                                               01S0130 00031000
         L     @11,RPHCRR(,CRPRPHPT)                            01S0130 00032000
         L     @11,CRACRR(,@11)                                 01S0130 00033000
         OI    NCFCRP(@11),B'00010000'                          01S0130 00034000
*/* ** CLEAR AND SET CURRENT PROCEDURE BIT **                        */ 00035000
*                                                               01S0131 00036000
*     CRPRPHPT->RPHCRR->CRACRR->CFCCRT='0'B;                    01S0131 00037000
         MVI   CFCCRT(@11),B'00000000'                          01S0131 00038000
*     CRPRPHPT->RPHCRR->CRACRR->CCFCRP='1'B;                    01S0132 00039000
         OI    CCFCRP(@11),B'00010000'                          01S0132 00040000
*   END;                                                        01S0133 00041000
*   DO UNTIL(POPTXPTR->CRPCHARA^=BLANK);                        01S0134 00042000
*                                                               01S0134 00043000
@DL00134 DS    0H                                               01S0135 00044000
*     /***************************************************************/ 00045000
*     /*                                                             */ 00046000
*     /* THE LOOP SEARCHES FOR THE FIRST NON-BLANK CHARACTER         */ 00047000
*     /* PRESUMABLY THE BEGINNING OF THE REPLY NUMBER                */ 00048000
*     /*                                                             */ 00049000
*     /***************************************************************/ 00050000
*                                                               01S0135 00051000
*     POPTXPTR=POPTXPTR+1;          /* SPACES TO THE NEXT CHAR       */ 00052000
         LA    @11,336                                          01S0135 00053000
         ALR   @11,CRPOCWPT                                     01S0135 00054000
         LA    @07,1                                            01S0135 00055000
         AL    @07,POPTXPTR(,@11)                               01S0135 00056000
         ST    @07,POPTXPTR(,@11)                               01S0135 00057000
*   END;                                                        01S0136 00058000
@DE00134 L     @11,POPTXPTR+336(,CRPOCWPT)                      01S0136 00059000
         CLI   CRPCHARA(@11),C' '                               01S0136 00060000
         BE    @DL00134                                         01S0136 00061000
*   RFY                                                         01S0137 00062000
*     CRPSTRNG RSTD;                                            01S0137 00063000
*   CRPSTRNG=POPTXPTR;              /* SAVES THE VALUE OF TEXT PTR   */ 00064000
         LR    CRPSTRNG,@11                                     01S0138 00065000
*   CRPNULL=ZERO;                   /* INITS VALUE FOR LOOP          */ 00066000
*                                                               01S0139 00067000
         SLR   @11,@11                                          01S0139 00068000
         ST    @11,CRPNULL+176(,CRPOCWPT)                       01S0139 00069000
*   /*****************************************************************/ 00070000
*   /*                                                               */ 00071000
*   /* THE FOLLOWING LOOP PICKS OUT THE REPLY ID ASSUMING THERE IS   */ 00072000
*   /* ONE                                                           */ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*                                                               01S0140 00076000
*   DO UNTIL(CRPSTRNG->CRPCHARA=COMMA|CRPNULL>TWO);             01S0140 00077000
@DL00140 DS    0H                                               01S0141 00078000
*     CRPSTRNG=CRPSTRNG+1;          /* SPACES TO NEXT CHAR           */ 00079000
         LA    @11,1                                            01S0141 00080000
         ALR   CRPSTRNG,@11                                     01S0141 00081000
*     CRPNULL=CRPNULL+1;            /* KEEPS COUNT OF LENGTH OF REPLY   00082000
*                                      ID                            */ 00083000
         LA    @09,176                                          01S0142 00084000
         ALR   @09,CRPOCWPT                                     01S0142 00085000
         AL    @11,CRPNULL(,@09)                                01S0142 00086000
         ST    @11,CRPNULL(,@09)                                01S0142 00087000
*   END;                                                        01S0143 00088000
@DE00140 CLI   CRPCHARA(CRPSTRNG),C','                          01S0143 00089000
         BE    @DC00140                                         01S0143 00090000
         L     @11,CRPNULL+176(,CRPOCWPT)                       01S0143 00091000
         CH    @11,@CH00105                                     01S0143 00092000
         BNH   @DL00140                                         01S0143 00093000
@DC00140 DS    0H                                               01S0144 00094000
*   RFY                                                         01S0144 00095000
*     CRPADBUF RSTD;                                            01S0144 00096000
*PLSBUG:                                                        01S0145 00097000
*   CRPADBUF=ADDR(POPMSG);          /* GIVES US START ADD            */ 00098000
PLSBUG   LA    @11,336                                          01S0145 00099000
         ALR   @11,CRPOCWPT                                     01S0145 00100000
         LA    CRPADBUF,POPMSG(,@11)                            01S0145 00101000
*   CRPINDEX=(POPTXPTR-CRPADBUF)+1; /* GIVES START INDEX OF REP ID   */ 00102000
         LA    @09,176                                          01S0146 00103000
         ALR   @09,CRPOCWPT                                     01S0146 00104000
         LA    @05,1                                            01S0146 00105000
         L     @00,POPTXPTR(,@11)                               01S0146 00106000
         SLR   @00,CRPADBUF                                     01S0146 00107000
         ALR   @00,@05                                          01S0146 00108000
         ST    @00,CRPINDEX(,@09)                               01S0146 00109000
*   IF CRPNULL=ONE THEN                                         01S0147 00110000
         C     @05,CRPNULL(,@09)                                01S0147 00111000
         BNE   @RF00147                                         01S0147 00112000
*     DO;                           /* THE ID IS ONE CHAR IN LENGTH  */ 00113000
*       POPMSG(CRPINDEX-ONE)=CHAR0; /* FILL IN LEADING ZERO          */ 00114000
         LR    @05,@00                                          01S0149 00115000
         ALR   @05,@11                                          01S0149 00116000
         MVI   POPMSG-2(@05),C'0'                               01S0149 00117000
*       CRPNUM=POPMSG(CRPINDEX-1:CRPINDEX);/* SETS VALUE OF REP ID   */ 00118000
         ALR   @11,@00                                          01S0150 00119000
         MVC   CRPNUM(2,@09),POPMSG-2(@11)                      01S0150 00120000
*     END;                                                      01S0151 00121000
*   ELSE                            /* WE HAVE AN ID OF LENGTH TWO(AT   00122000
*                                      LEAST)                        */ 00123000
*     CRPNUM=POPMSG(CRPINDEX:CRPINDEX+1);/* SET REP ID               */ 00124000
         B     @RC00147                                         01S0152 00125000
@RF00147 LA    @11,176                                          01S0152 00126000
         ALR   @11,CRPOCWPT                                     01S0152 00127000
         L     @09,CRPINDEX(,@11)                               01S0152 00128000
         ALR   @09,CRPOCWPT                                     01S0152 00129000
         MVC   CRPNUM(2,@11),POPMSG+335(@09)                    01S0152 00130000
*   CRPSTSW=ZERO;                                               01S0153 00131000
@RC00147 MVI   CRPSTSW+176(CRPOCWPT),X'00'                      01S0153 00132000
*   CALL CRPCBSH;                   /* WILL SEARCH FOR CORRESPONDING    00133000
*                                      PORCB                         */ 00134000
         BAL   @14,CRPCBSH                                      01S0154 00135000
*   RFY                                                         01S0155 00136000
*     ISTPORCB BASED(CRPPTR);                                   01S0155 00137000
*   IF(CRPFND=ON&PORRTPQ=OFF)       /* IF THE PORCB IS FOUND AND        00138000
*                                      REPLY                         */ 00139000
*/*                                   HAS NOT YET BEEN SATISFIED     */ 00140000
*                                                               01S0156 00141000
*     THEN                                                      01S0156 00142000
         TM    CRPFND+176(CRPOCWPT),B'10000000'                 01S0156 00143000
         BNO   @RF00156                                         01S0156 00144000
         TM    PORRTPQ(CRPPTR),B'10000000'                      01S0156 00145000
         BNZ   @RF00156                                         01S0156 00146000
*     DO;                                                       01S0157 00147000
*                                                               01S0157 00148000
*       /*************************************************************/ 00149000
*       /*                                                           */ 00150000
*       /* REST OF PROGRAM SYNTAX CHECKS THE TEXT AND MOVES IT INTO  */ 00151000
*       /* PORCB IF VALID, WE WILL THEREFORE NOW OBTAIN RDT LOCK     */ 00152000
*       /* EXCLUSIVE                                                 */ 00153000
*       /*                                                           */ 00154000
*       /*************************************************************/ 00155000
*                                                               01S0158 00156000
*/*** START -TPUNLOCK (RDTLOCK)- **                                  */ 00157000
*                                                               01S0158 00158000
*       DO;                                                     01S0158 00159000
*                                                               01S0158 00160000
*/* ATBUILD (ISTRPH,ISTLOK,ISTCRA)                                   */ 00161000
*/* END OF ATBUILD                                                   */ 00162000
*                                                               01S0159 00163000
*         RFY                                                   01S0159 00164000
*          (GPR00P,                                             01S0159 00165000
*           GPR01P,                                             01S0159 00166000
*           GPR02P,                                             01S0159 00167000
*           GPR03P,                                             01S0159 00168000
*           GPR04P,                                             01S0159 00169000
*           GPR15P) RSTD;                                       01S0159 00170000
*         GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(RPHWORK);    00171000
         STM GPR00P,GPR15P,RPHWORK(GPR01P)                              00172000
*         GPR02P=ADDR(ATCRDTLK);    /* SET PARAM - LOCKWORD PTR      */ 00173000
         L     GPR02P,OCWACVT(,CRPOCWPT)                        01S0161 00174000
         LA    GPR02P,ATCRDTLK(,GPR02P)                         01S0161 00175000
*         GPR04P=GPR01P->RPHCRR;    /* SET PARAM - ISTCRA PTR        */ 00176000
         L     GPR04P,RPHCRR(,GPR01P)                           01S0162 00177000
*         GPR00P=CRALMS03;          /* SET PARAM - LOCK LEVEL FLAG   */ 00178000
         LA    GPR00P,4                                         01S0163 00179000
*         GPR03P=ADDR(GPR04P->CRAL3PTR);/* SET PARAM - LOCK LEVEL PTR*/ 00180000
         LA    GPR03P,CRAL3PTR(,GPR04P)                         01S0164 00181000
*         DO;                       /* LISATCVT                 01S0165 00182000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00183000
*           GPR15P=ATCLCPTR;        /* SET UP ATCVT POINTER          */ 00184000
         L     GPR15P,ATCLCPTR                                  01S0166 00185000
*         END;                      /* LISATCVT                      */ 00186000
*         GPR15P=GPR15P->ATCAP33;                               01S0168 00187000
         L     GPR15P,ATCAP33(,GPR15P)                          01S0168 00188000
*         DCL                                                   01S0169 00189000
*           FMZL0012 BASED(GPR15P) LABEL VALUERANGE(*);         01S0169 00190000
*         CALL FMZL0012;                                        01S0170 00191000
         BALR  @14,@15                                          01S0170 00192000
*         GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(GPR00P,      00193000
*             GPR01P,GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,  00194000
*             GPR09P,GPR10P,GPR11P,GPR12P,GPR13P,GPR14P,GPR15P);        00195000
         LM GPR00P,GPR15P,RPHWORK(GPR01P)                               00196000
*         RFY                                                   01S0172 00197000
*          (GPR00P,                                             01S0172 00198000
*           GPR01P,                                             01S0172 00199000
*           GPR02P,                                             01S0172 00200000
*           GPR03P,                                             01S0172 00201000
*           GPR04P,                                             01S0172 00202000
*           GPR15P) UNRSTD;                                     01S0172 00203000
*       END;                                                    01S0173 00204000
*                                                               01S0173 00205000
*/*** END -TPUNLOCK (RDTLOCK)- **                                    */ 00206000
*/*** START -TPLOCK (RDTLOCK)TYPE(XCLSIVE)- **                       */ 00207000
*                                                               01S0174 00208000
*       DO;                                                     01S0174 00209000
*         RFY                                                   01S0175 00210000
*          (GPR00P,                                             01S0175 00211000
*           GPR01P,                                             01S0175 00212000
*           GPR02P,                                             01S0175 00213000
*           GPR03P,                                             01S0175 00214000
*           GPR04P,                                             01S0175 00215000
*           GPR15P) RSTD;                                       01S0175 00216000
*         GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(RPHWORK);    00217000
         STM GPR00P,GPR14P,RPHWORK(GPR01P)                              00218000
*         GPR15P=ATCAP36;           /* ADDRESS OF LOCK ROUTINE       */ 00219000
         L     @11,OCWACVT(,CRPOCWPT)                           01S0177 00220000
         L     GPR15P,ATCAP36(,@11)                             01S0177 00221000
*         GPR02P=ADDR(ATCRDTLK);    /* SET PARAM - LOCKWORD PTR      */ 00222000
         LA    GPR02P,ATCRDTLK(,@11)                            01S0178 00223000
*         GPR04P=GPR01P->RPHCRR;    /* SET PARAM - ISTCRA PTR        */ 00224000
         L     GPR04P,RPHCRR(,GPR01P)                           01S0179 00225000
*         GPR00P=CRALMS03;          /* SET PARAM - LOCK LEVEL FLAG   */ 00226000
         LA    GPR00P,4                                         01S0180 00227000
*         GPR03P=ADDR(GPR04P->CRAL3PTR);/* SET PARAM - LOCK LEVEL PTR*/ 00228000
         LA    GPR03P,CRAL3PTR(,GPR04P)                         01S0181 00229000
*         DCL                                                   01S0182 00230000
*           FMZL0022 BASED(GPR15P) LABEL VALUERANGE(*);         01S0182 00231000
*         CALL FMZL0022;                                        01S0183 00232000
         BALR  @14,@15                                          01S0183 00233000
*         GENERATE REFS(GPR00P,GPR01P,GPR14P,RPHWORK) SETS(GPR00P,      00234000
*             GPR01P,GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,  00235000
*             GPR09P,GPR10P,GPR11P,GPR12P,GPR13P,GPR14P,GPR15P);        00236000
         LM GPR00P,GPR15P,RPHWORK(GPR01P)                               00237000
*         RFY                                                   01S0185 00238000
*          (GPR00P,                                             01S0185 00239000
*           GPR01P,                                             01S0185 00240000
*           GPR02P,                                             01S0185 00241000
*           GPR03P,                                             01S0185 00242000
*           GPR04P,                                             01S0185 00243000
*           GPR15P) UNRSTD;                                     01S0185 00244000
*       END;                                                    01S0186 00245000
*                                                               01S0186 00246000
*/*** END -TPLOCK (RDTLOCK)TYPE(XCLSIVE)- **                         */ 00247000
*                                                               01S0187 00248000
*       RFY                                                     01S0187 00249000
*         CRPREG15 RSTD;                                        01S0187 00250000
*       IF CRPREG15=ZERO THEN                                   01S0188 00251000
         LTR   CRPREG15,CRPREG15                                01S0188 00252000
         BNZ   @RF00188                                         01S0188 00253000
*         DO;                                                   01S0189 00254000
*           RFY                                                 01S0190 00255000
*             CRPREG15 UNRSTD;                                  01S0190 00256000
*           IF CRPNULL>TWO THEN                                 01S0191 00257000
         L     @11,CRPNULL+176(,CRPOCWPT)                       01S0191 00258000
         CH    @11,@CH00105                                     01S0191 00259000
         BH    @RT00191                                         01S0191 00260000
*             ;                     /* SIGNIFIES TEXT IS NULL=> NO      00261000
*                                      MOVE INTO PORCB               */ 00262000
*           ELSE                                                01S0193 00263000
*             DO;                                               01S0193 00264000
*               PORMSG(1)=' ';      /* BLANK OUT REPLY AREA          */ 00265000
         MVI   PORMSG(CRPPTR),C' '                              01S0194 00266000
*               IF PORREPLN>1 THEN  /* BLANK OUT REST OF BUFF        */ 00267000
         CLI   PORREPLN(CRPPTR),1                               01S0195 00268000
         BNH   @RF00195                                         01S0195 00269000
*                 MVC(PORMSG(2:PORREPLN),PORMSG(1:PORREPLN-1)); 01S0196 00270000
         SLR   @11,@11                                          01S0196 00271000
         IC    @11,PORREPLN(,CRPPTR)                            01S0196 00272000
         BCTR  @11,0                                            01S0196 00273000
         BCTR  @11,0                                            01S0196 00274000
         EX    @11,@SM03334                                     01S0196 00275000
*               ELSE                                            01S0197 00276000
*                 ;                 /* BLANKS NOT NEEDED             */ 00277000
@RF00195 DS    0H                                               01S0198 00278000
*               CRPSTRNG=CRPSTRNG+1;/* SPACE TO FIRST CHAR OF TEXT   */ 00279000
         AH    CRPSTRNG,@CH00093                                01S0198 00280000
*               IF CRPSTRNG->CRPCHARA=QUOTE/* DO WE HAVE TEXT IN        00281000
*                                      QUOTES                        */ 00282000
*                 THEN                                          01S0199 00283000
         CLI   CRPCHARA(CRPSTRNG),X'7D'                         01S0199 00284000
         BNE   @RF00199                                         01S0199 00285000
*                 DO;                                           01S0200 00286000
*                   CRPBGN=(CRPSTRNG-CRPADBUF)+TWO;/* PROVIDES INDEX    00287000
*                                      FOR BEGIN OF TEXT             */ 00288000
*                                                               01S0201 00289000
         LR    @11,CRPSTRNG                                     01S0201 00290000
         SLR   @11,CRPADBUF                                     01S0201 00291000
         AH    @11,@CH00105                                     01S0201 00292000
         ST    @11,CRPBGN+176(,CRPOCWPT)                        01S0201 00293000
*                   /*************************************************/ 00294000
*                   /*                                               */ 00295000
*                   /* FOLLOWING SUBROUTINE EDITS THE BUFFER OF      */ 00296000
*                   /* EXTRANEOUS QUOTES AND DETERMINES LENGTH OF    */ 00297000
*                   /* TEXT                                          */ 00298000
*                   /*                                               */ 00299000
*                   /*************************************************/ 00300000
*                                                               01S0202 00301000
*                   CALL CRPEDIT;                               01S0202 00302000
         BAL   @14,CRPEDIT                                      01S0202 00303000
*                   IF CRPBUFLN>PORREPLN THEN                   01S0203 00304000
         L     @11,CRPBUFLN+176(,CRPOCWPT)                      01S0203 00305000
         SLR   @09,@09                                          01S0203 00306000
         IC    @09,PORREPLN(,CRPPTR)                            01S0203 00307000
         CR    @11,@09                                          01S0203 00308000
         BNH   @RF00203                                         01S0203 00309000
*                     DO;           /* THE TEXT IS LONGER THAN IS       00310000
*                                      ALLOWED FOR                   */ 00311000
*                                                               01S0204 00312000
*                       /*********************************************/ 00313000
*                       /*                                           */ 00314000
*                       /* ERROR LOGIC (SYNTAX ERROR ) FOLLOWS       */ 00315000
*                       /*                                           */ 00316000
*                       /*********************************************/ 00317000
*                                                               01S0205 00318000
*                       RPLRTNCD='08'X;/* SET RPL RET CODE           */ 00319000
         L     @11,OCWRPL(,CRPOCWPT)                            01S0205 00320000
         MVI   RPLRTNCD(@11),X'08'                              01S0205 00321000
*                       RPLFDB2='6E'X;/* SET RPL RET CODE            */ 00322000
         MVI   RPLFDB2(@11),X'6E'                               01S0206 00323000
*                       POPMSGID='193I';/* SET ERR MSG ID FOR TPMSG  */ 00324000
         MVC   POPMSGID+336(4,CRPOCWPT),@CC01185                01S0207 00325000
*                       CALL CRPMSG;/* TPMSG SUBROUTINE              */ 00326000
         BAL   @14,CRPMSG                                       01S0208 00327000
*                       CRPERR=ON;  /* SIGNAL TO PROG THAT WE HAVE      00328000
*                                      ERROR                         */ 00329000
         OI    CRPERR+176(CRPOCWPT),B'01000000'                 01S0209 00330000
*                     END;                                      01S0210 00331000
*                   ELSE                                        01S0211 00332000
*                     DO;                                       01S0211 00333000
*                                                               01S0211 00334000
         B     @RC00203                                         01S0211 00335000
@RF00203 DS    0H                                               01S0212 00336000
*                       /*********************************************/ 00337000
*                       /*                                           */ 00338000
*                       /* TEXT IS OF CORRECT LENGTH                 */ 00339000
*                       /*                                           */ 00340000
*                       /*********************************************/ 00341000
*                                                               01S0212 00342000
*                       CRPERR=OFF; /* NO ERROR HERE                 */ 00343000
*                                                               01S0212 00344000
         LA    @11,176                                          01S0212 00345000
         ALR   @11,CRPOCWPT                                     01S0212 00346000
         NI    CRPERR(@11),B'10111111'                          01S0212 00347000
*                       /*********************************************/ 00348000
*                       /*                                           */ 00349000
*                       /* MOVE REPLY TEXT INTO PORCB                */ 00350000
*                       /*                                           */ 00351000
*                       /*********************************************/ 00352000
*                                                               01S0213 00353000
*                       PORMSG(1:CRPBUFLN)=POPMSG(CRPBGN:CRPBGN+        00354000
*                           CRPBUFLN);                          01S0213 00355000
         L     @09,CRPBUFLN(,@11)                               01S0213 00356000
         BCTR  @09,0                                            01S0213 00357000
         L     @11,CRPBGN(,@11)                                 01S0213 00358000
         ALR   @11,CRPOCWPT                                     01S0213 00359000
         EX    @09,@SM03338                                     01S0213 00360000
*                     END;                                      01S0214 00361000
*                 END;                                          01S0215 00362000
*               ELSE                                            01S0216 00363000
*                 DO;               /* THERE IS NO QUOTE AFTER THE      00364000
*                                      REPLY ID                      */ 00365000
         B     @RC00199                                         01S0216 00366000
@RF00199 DS    0H                                               01S0217 00367000
*                   CRPBGN=(CRPSTRNG-CRPADBUF)+1;/* SET INDEX TO BGN    00368000
*                                      OF TEXT                       */ 00369000
*                                                               01S0217 00370000
         LA    @11,176                                          01S0217 00371000
         ALR   @11,CRPOCWPT                                     01S0217 00372000
         LR    @09,CRPSTRNG                                     01S0217 00373000
         SLR   @09,CRPADBUF                                     01S0217 00374000
         AH    @09,@CH00093                                     01S0217 00375000
         ST    @09,CRPBGN(,@11)                                 01S0217 00376000
*                   /*************************************************/ 00377000
*                   /*                                               */ 00378000
*                   /* LENGTH OF REPLY TEXT WILL BE COMPUTED         */ 00379000
*                   /*                                               */ 00380000
*                   /*************************************************/ 00381000
*                                                               01S0218 00382000
*                   CRPNULL=(((RPLRLEN-4)+CRPADBUF)-CRPSTRNG);  01S0218 00383000
         L     @09,OCWRPL(,CRPOCWPT)                            01S0218 00384000
         L     @09,RPLRLEN(,@09)                                01S0218 00385000
         SH    @09,@CH00085                                     01S0218 00386000
         ALR   @09,CRPADBUF                                     01S0218 00387000
         SLR   @09,CRPSTRNG                                     01S0218 00388000
         ST    @09,CRPNULL(,@11)                                01S0218 00389000
*                   IF CRPNULL>PORREPLN|CRPNULL=0/* TOO LONG? OR        00390000
*                                      INVALID NULL FORMAT           */ 00391000
*                     THEN                                      01S0219 00392000
         SLR   @11,@11                                          01S0219 00393000
         IC    @11,PORREPLN(,CRPPTR)                            01S0219 00394000
         CR    @09,@11                                          01S0219 00395000
         BH    @RT00219                                         01S0219 00396000
         LTR   @09,@09                                          01S0219 00397000
         BNZ   @RF00219                                         01S0219 00398000
@RT00219 DS    0H                                               01S0220 00399000
*                     DO;           /* ERROR FOUND                   */ 00400000
*                                                               01S0220 00401000
*                       /*********************************************/ 00402000
*                       /*                                           */ 00403000
*                       /* ERROR LOGIC (SYNTAX ERROR ) FOLLOWS       */ 00404000
*                       /*                                           */ 00405000
*                       /*********************************************/ 00406000
*                                                               01S0221 00407000
*                       RPLRTNCD='08'X;/* SET RPL RET CODE           */ 00408000
         L     @11,OCWRPL(,CRPOCWPT)                            01S0221 00409000
         MVI   RPLRTNCD(@11),X'08'                              01S0221 00410000
*                       RPLFDB2='6E'X;/* SET RPL RET CODE            */ 00411000
         MVI   RPLFDB2(@11),X'6E'                               01S0222 00412000
*                       POPMSGID='193I';/* SET ERR MSG ID FOR TPMSG  */ 00413000
         MVC   POPMSGID+336(4,CRPOCWPT),@CC01185                01S0223 00414000
*                       CALL CRPMSG;/* TPMSG SUBROUTINE              */ 00415000
         BAL   @14,CRPMSG                                       01S0224 00416000
*                       CRPERR=ON;  /* SIGNAL TO PROG THAT WE HAVE      00417000
*                                      ERROR                         */ 00418000
         OI    CRPERR+176(CRPOCWPT),B'01000000'                 01S0225 00419000
*                     END;                                      01S0226 00420000
*                   ELSE                                        01S0227 00421000
*                     DO;                                       01S0227 00422000
         B     @RC00219                                         01S0227 00423000
@RF00219 DS    0H                                               01S0228 00424000
*                       CRPERR=OFF; /* NO ERROR FOUND                */ 00425000
*                                                               01S0228 00426000
