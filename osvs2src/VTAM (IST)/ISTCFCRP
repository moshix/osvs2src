         ALR   @11,CRPOCWPT                                     01S0228 00428000
         NI    CRPERR(@11),B'10111111'                          01S0228 00429000
*                       /*********************************************/ 00430000
*                       /*                                           */ 00431000
*                       /* PUTS THE MESSAGE TEXT INTO THE PORCB      */ 00432000
*                       /*                                           */ 00433000
*                       /*********************************************/ 00434000
*                                                               01S0229 00435000
*                       PORMSG(1:CRPNULL)=POPMSG(CRPBGN:CRPBGN+CRPNULL) 00436000
*                           ;                                   01S0229 00437000
