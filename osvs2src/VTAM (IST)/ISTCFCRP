@EL00002 DS    0H                                               01S0368 00883000
@EF00002 DS    0H                                               01S0368 00884000
@ER00002 BR    @14                                              01S0368 00885000
*   RFY                                                         01S0369 00886000
*     CRPRTPT UNRSTD;                                           01S0369 00887000
*   END CRPMSG;                                                 01S0370 00888000
         EJECT                                                          00889000
*CRPEDIT:                                                       01S0371 00890000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0371 00891000
CRPEDIT  DS    0H                                               01S0372 00892000
*   RFY                                                         01S0372 00893000
*     CRPRTPT RSTD;                                             01S0372 00894000
*   CRPR14SV=CRPRTPT;                                           01S0373 00895000
         LA    @15,176                                          01S0373 00896000
         ALR   @15,CRPOCWPT                                     01S0373 00897000
         ST    CRPRTPT,CRPR14SV(,@15)                           01S0373 00898000
*   RFY                                                         01S0374 00899000
*     CRPRTPT UNRSTD;                                           01S0374 00900000
*   RFY                                                         01S0375 00901000
*    (CRPBASE,                                                  01S0375 00902000
*     CRPPTRA) RSTD;                                            01S0375 00903000
*   RFY                                                         01S0376 00904000
*     CRPCHARA BASED(CRPPTRA);                                  01S0376 00905000
*   RFY                                                         01S0377 00906000
*     CRPCHARB BASED(CRPBASE);                                  01S0377 00907000
*   RFY                                                         01S0378 00908000
*     CRPCHARC BASED(CRPSTRNG);     /* THIS WILL BE THE ACTUAL TEXT     00909000
*                                      PTR                           */ 00910000
*/*                                               AFTER IT IS EDITED */ 00911000
*                                                               01S0379 00912000
*   CRPPTRA=CRPSTRNG+TWO;           /* SET TO SECOND CHAR IN TEXT    */ 00913000
         LA    CRPPTRA,2                                        01S0379 00914000
         ALR   CRPPTRA,CRPSTRNG                                 01S0379 00915000
*   CRPBASE=CRPSTRNG+ONE;           /* SET TO FIRST CHAR IN TEXT     */ 00916000
         LA    CRPBASE,1                                        01S0380 00917000
         ALR   CRPBASE,CRPSTRNG                                 01S0380 00918000
*   CRPSTRNG=CRPBASE;               /* SET TO FIRST CHAR IN TEXT     */ 00919000
         LR    CRPSTRNG,CRPBASE                                 01S0381 00920000
*   CRPFND=OFF;                     /* INIT BIT                      */ 00921000
*   CRPSAME=OFF;                    /* SIGNALS THAT WE HAVE FOUND TWO   00922000
*                                      CONSECUTIVE QUOTES            */ 00923000
         LA    @14,176                                          01S0383 00924000
         ALR   @14,CRPOCWPT                                     01S0383 00925000
         NI    CRPFND(@14),B'01011111'                          01S0383 00926000
*   CRPBUFLN=ZERO;                                              01S0384 00927000
*                                                               01S0384 00928000
         SLR   @11,@11                                          01S0384 00929000
         ST    @11,CRPBUFLN(,@14)                               01S0384 00930000
*   /*****************************************************************/ 00931000
*   /*                                                               */ 00932000
*   /* THE FOLLOWING LOOP WILL USE THREE POINTERS: TWO TO TELL US    */ 00933000
*   /* WHEN WE HAVE HIT UPON A DOUBLE QUOTE COMBIN-                  */ 00934000
*   /* NATION(CHARA,CHARB) AND THE THIRD TO KEEP TRACK OF WHERE WE   */ 00935000
*   /* ARE IN ACTUAL TEXT. THERE WILL ALSO BE A A TOTAL TEXT CTR SO  */ 00936000
*   /* THAT WE DONT EXCEED 126 CHARS                                 */ 00937000
*   /*                                                               */ 00938000
*   /*****************************************************************/ 00939000
*                                                               01S0385 00940000
*   DO WHILE(CRPFND=OFF&CRPBUFLN<=RPLRLEN);                     01S0385 00941000
         B     @DE00385                                         01S0385 00942000
@DL00385 DS    0H                                               01S0386 00943000
*     IF(CRPCHARA=QUOTE&CRPCHARB=QUOTE) THEN                    01S0386 00944000
         CLI   CRPCHARA(CRPPTRA),X'7D'                          01S0386 00945000
         BNE   @RF00386                                         01S0386 00946000
         CLI   CRPCHARB(CRPBASE),X'7D'                          01S0386 00947000
         BNE   @RF00386                                         01S0386 00948000
*       DO;                         /* WE HAVE A DOUBLE QUOTE TO BE     00949000
*                                      REDUCED TO A SINGLE QUOTE     */ 00950000
*         CRPSAME=ON;               /* SIGNAL FOR DOUBLE QUOTE       */ 00951000
         OI    CRPSAME+176(CRPOCWPT),B'00100000'                01S0388 00952000
*         CRPPTRA=CRPPTRA+ONE;      /* ADVANCE PTR                   */ 00953000
         LA    @14,1                                            01S0389 00954000
         ALR   CRPPTRA,@14                                      01S0389 00955000
*         CRPBASE=CRPBASE+ONE;      /* ADVANCE PTR                   */ 00956000
         ALR   CRPBASE,@14                                      01S0390 00957000
*       END;                                                    01S0391 00958000
*     ELSE                                                      01S0392 00959000
*       ;                                                       01S0392 00960000
@RF00386 DS    0H                                               01S0393 00961000
*     CRPCHARC=CRPCHARB;            /* IF THEY DIFFER(IE WE HAVE HIT    00962000
*                                      DOU. QUOTE) THEN THIS WILL       00963000
*                                      EDIT BUFFER CORRECTLY         */ 00964000
         MVC   CRPCHARC(1,CRPSTRNG),CRPCHARB(CRPBASE)           01S0393 00965000
*     IF CRPSAME=OFF&CRPCHARC=QUOTE /* END OF BUFFER TEXT?           */ 00966000
*       THEN                                                    01S0394 00967000
         LA    @14,176                                          01S0394 00968000
         ALR   @14,CRPOCWPT                                     01S0394 00969000
         TM    CRPSAME(@14),B'00100000'                         01S0394 00970000
         BNZ   @RF00394                                         01S0394 00971000
         CLI   CRPCHARC(CRPSTRNG),X'7D'                         01S0394 00972000
         BNE   @RF00394                                         01S0394 00973000
*       CRPFND=ON;                  /* WE WANT TO EXIT LOOP          */ 00974000
         OI    CRPFND(@14),B'10000000'                          01S0395 00975000
*     ELSE                                                      01S0396 00976000
*       DO;                         /* ADVANCE ALL POINTERS AND 01S0396 00977000
*                                      REINIT FLAGS                  */ 00978000
         B     @RC00394                                         01S0396 00979000
@RF00394 DS    0H                                               01S0397 00980000
*         CRPSAME=OFF;                                          01S0397 00981000
         NI    CRPSAME+176(CRPOCWPT),B'11011111'                01S0397 00982000
*         CRPPTRA=CRPPTRA+ONE;                                  01S0398 00983000
         LA    @14,1                                            01S0398 00984000
         ALR   CRPPTRA,@14                                      01S0398 00985000
*         CRPBASE=CRPBASE+ONE;                                  01S0399 00986000
         ALR   CRPBASE,@14                                      01S0399 00987000
*         CRPSTRNG=CRPSTRNG+ONE;                                01S0400 00988000
         ALR   CRPSTRNG,@14                                     01S0400 00989000
*       END;                                                    01S0401 00990000
*     CRPBUFLN=((CRPBASE-CRPADBUF)+ONE);/* COMPUTE TOTAL LENGTH SO      00991000
*                                      FAR                           */ 00992000
@RC00394 LR    @14,CRPBASE                                      01S0402 00993000
         SLR   @14,CRPADBUF                                     01S0402 00994000
         AH    @14,@CH00093                                     01S0402 00995000
         ST    @14,CRPBUFLN+176(,CRPOCWPT)                      01S0402 00996000
*   END;                                                        01S0403 00997000
@DE00385 LA    @14,176                                          01S0403 00998000
         ALR   @14,CRPOCWPT                                     01S0403 00999000
         TM    CRPFND(@14),B'10000000'                          01S0403 01000000
         BNZ   @DC00385                                         01S0403 01001000
         L     @11,OCWRPL(,CRPOCWPT)                            01S0403 01002000
         L     @14,CRPBUFLN(,@14)                               01S0403 01003000
         C     @14,RPLRLEN(,@11)                                01S0403 01004000
         BNH   @DL00385                                         01S0403 01005000
@DC00385 DS    0H                                               01S0404 01006000
*   CRPBUFLN=(CRPSTRNG-CRPADBUF)-(CRPBGN-ONE);/* ACTUAL LEN OF TEXT  */ 01007000
         LA    @14,176                                          01S0404 01008000
         ALR   @14,CRPOCWPT                                     01S0404 01009000
         LR    @11,CRPSTRNG                                     01S0404 01010000
         SLR   @11,CRPADBUF                                     01S0404 01011000
         L     @09,CRPBGN(,@14)                                 01S0404 01012000
         BCTR  @09,0                                            01S0404 01013000
         SLR   @11,@09                                          01S0404 01014000
         ST    @11,CRPBUFLN(,@14)                               01S0404 01015000
*   RFY                                                         01S0405 01016000
*    (CRPBASE,                                                  01S0405 01017000
*     CRPPTRA) UNRSTD;                                          01S0405 01018000
*   RFY                                                         01S0406 01019000
*     CRPRTPT RSTD;                                             01S0406 01020000
*   CRPRTPT=CRPR14SV;               /* RESTORE RETURN @              */ 01021000
         LA    @11,176                                          01S0407 01022000
         ALR   @11,CRPOCWPT                                     01S0407 01023000
         L     CRPRTPT,CRPR14SV(,@11)                           01S0407 01024000
*   RETURN;                                                     01S0408 01025000
@EL00003 DS    0H                                               01S0408 01026000
@EF00003 DS    0H                                               01S0408 01027000
@ER00003 BR    @14                                              01S0408 01028000
*   RFY                                                         01S0409 01029000
*     CRPRTPT UNRSTD;                                           01S0409 01030000
*   END CRPEDIT;                                                01S0410 01031000
         EJECT                                                          01032000
*CRPCBSH:                                                       01S0411 01033000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0411 01034000
*                                                               01S0411 01035000
CRPCBSH  DS    0H                                               01S0412 01036000
*   /*****************************************************************/ 01037000
*   /*                                                               */ 01038000
*   /* THIS SUBROUTINE SEARCHES FOR A PROCB WITH A MATCHING REPLY    */ 01039000
*   /* NUMBER                                                        */ 01040000
*   /*                                                               */ 01041000
*   /*****************************************************************/ 01042000
*                                                               01S0412 01043000
*   RFY                                                         01S0412 01044000
*     CRPRTPT RSTD;                                             01S0412 01045000
*   CRPR14SV=CRPRTPT;                                           01S0413 01046000
         LA    @15,176                                          01S0413 01047000
         ALR   @15,CRPOCWPT                                     01S0413 01048000
         ST    CRPRTPT,CRPR14SV(,@15)                           01S0413 01049000
*   RFY                                                         01S0414 01050000
*     CRPRTPT UNRSTD;                                           01S0414 01051000
*   RFY                                                         01S0415 01052000
*     CRPBASE RSTD;                                             01S0415 01053000
*   IF POCAWTRQ=ZERO                /* NO AWAITING REPLIES           */ 01054000
*     THEN                                                      01S0416 01055000
         ICM   @11,7,POCAWTRQ(CRPPOCB)                          01S0416 01056000
         BNZ   @RF00416                                         01S0416 01057000
*     CRPFND=OFF;                   /* IT WAS NOT FOUND              */ 01058000
         NI    CRPFND(@15),B'01111111'                          01S0417 01059000
*   ELSE                                                        01S0418 01060000
*     DO;                                                       01S0418 01061000
         B     @RC00416                                         01S0418 01062000
@RF00416 DS    0H                                               01S0419 01063000
*       CRPPTR=POCAWTRQ;            /* SET PTR TO AWAIT. REP. QUEUE  */ 01064000
         SLR   CRPPTR,CRPPTR                                    01S0419 01065000
         ICM   CRPPTR,7,POCAWTRQ(CRPPOCB)                       01S0419 01066000
*       CRPBASE=CRPPTR;                                         01S0420 01067000
         LR    CRPBASE,CRPPTR                                   01S0420 01068000
*       RFY                                                     01S0421 01069000
*         ISTPORCB BASED(CRPPTR);                               01S0421 01070000
*                                                               01S0421 01071000
*       /*************************************************************/ 01072000
*       /*                                                           */ 01073000
*       /* THIS LOOP SEARCHES QUEUE FOR MATCH IN REP ID IF MATCH IS  */ 01074000
*       /* FOUND, PORCB ADDRESS WILL BE IN CRPPTR                    */ 01075000
*       /*                                                           */ 01076000
*       /*************************************************************/ 01077000
*                                                               01S0422 01078000
*       DO UNTIL(PORFWD=ZERO|CRPFND=ON);                        01S0422 01079000
@DL00422 DS    0H                                               01S0423 01080000
*         CRPPTR=CRPBASE;                                       01S0423 01081000
         LR    CRPPTR,CRPBASE                                   01S0423 01082000
*         IF CRPNUM=POREPID         /* IF THE REP IDS MATCH          */ 01083000
*           THEN                                                01S0424 01084000
         LA    @11,176                                          01S0424 01085000
         ALR   @11,CRPOCWPT                                     01S0424 01086000
         CLC   CRPNUM(2,@11),POREPID(CRPPTR)                    01S0424 01087000
         BNE   @RF00424                                         01S0424 01088000
*           CRPFND=ON;              /* WE WANT TO EXIT LOOP          */ 01089000
         OI    CRPFND(@11),B'10000000'                          01S0425 01090000
*         ELSE                                                  01S0426 01091000
*           CRPBASE=PORFWD;         /* POINT TO NEXT PORCB           */ 01092000
         B     @RC00424                                         01S0426 01093000
@RF00424 SLR   CRPBASE,CRPBASE                                  01S0426 01094000
         ICM   CRPBASE,7,PORFWD(CRPPTR)                         01S0426 01095000
*       END;                                                    01S0427 01096000
@RC00424 DS    0H                                               01S0427 01097000
@DE00422 ICM   @11,7,PORFWD(CRPPTR)                             01S0427 01098000
         BZ    @DC00422                                         01S0427 01099000
         TM    CRPFND+176(CRPOCWPT),B'10000000'                 01S0427 01100000
         BNO   @DL00422                                         01S0427 01101000
@DC00422 DS    0H                                               01S0428 01102000
*     END;                                                      01S0428 01103000
*   RFY                                                         01S0429 01104000
*     CRPBASE UNRSTD;                                           01S0429 01105000
@RC00416 DS    0H                                               01S0430 01106000
*   RFY                                                         01S0430 01107000
*     CRPRTPT RSTD;                                             01S0430 01108000
*   CRPRTPT=CRPR14SV;               /* RESTORE RET @                 */ 01109000
         L     CRPRTPT,CRPR14SV+176(,CRPOCWPT)                  01S0431 01110000
*   RETURN;                                                     01S0432 01111000
@EL00004 DS    0H                                               01S0432 01112000
@EF00004 DS    0H                                               01S0432 01113000
@ER00004 BR    @14                                              01S0432 01114000
*   RFY                                                         01S0433 01115000
*     CRPRTPT UNRSTD;                                           01S0433 01116000
*   END CRPCBSH;                                                01S0434 01117000
*                                                               01S0434 01118000
@PB00004 DS    0H                                               01S0435 01119000
*/* TPEPILOG                                                         */ 01120000
*                                                               01S0435 01121000
*   GEN(PRINT OFF);                                             01S0435 01122000
         PRINT OFF                                                      01123000
*/*      VTAM PROCESS OPTION DEFINITION BLOCK                       */  01124000
*/*  USE RESERVED BIT                                  002 06/10/74 */  01125000
*/*                                                                 */  01126000
*DECLARE                                                        01S0436 01127000
*  1 ISTPROCD BDY(WORD) BASED,                                  01S0436 01128000
*    2 PROPROC  BIT(32),          /* PROCESSING OPTIONS             */  01129000
*      3 PROPROC1 BIT(8),         /* PROCESSING OPTIONS - 1         */  01130000
*        4 PROASYIP BIT(1),       /* 1=ASYIPX 0=NASYIPX             */  01131000
*        4 PROTRUNC BIT(1),       /* 1=TRUNC 0=KEEP                 */  01132000
*        4 PROXPOPT BIT(1),       /* 1=BINARY 0=NBINARY             */  01133000
*        4 PRODFASY BIT(1),       /* PROC-0=NDFASYX/1=DFASYX        */  01134000
*        4 PRORESPX BIT(1),       /* PROC-0=NRESPX/1=RESPX          */  01135000
*        4 PROCA    BIT(1),       /* 1=RECEIVE ANY CA/0=NO CA       */  01136000
*        4 PROCS    BIT(1),       /* 1=RECEIVE ANY CS/0=NO CS       */  01137000
*        4 PRORPLC  BIT(1),       /* 1=OPTION IN RPL/0=NO RPL       */  01138000
*      3 PROPROC2 BIT(8),         /* PROCESSING OPTIONS - 2         */  01139000
*        4 PRORSV03 BIT(1),       /* RESERVED                       */  01140000
*        4 PROERPO  BIT(1),       /* 1=NERPOUT 0=ERPOUT             */  01141000
*        4 PROLGOT  BIT(1),       /* 1=NLGOUT 0=LGOUT               */  01142000
*        4 PRORSV04 BIT(1),       /* RESERVED                       */  01143000
*        4 PRORSV05 BIT(1),       /* RESERVED                       */  01144000
*        4 PRONTFL  BIT(1),       /* 1=NTMFLL 0=TMFLL               */  01145000
*        4 PROEMLC  BIT(1),       /* 1=ELC 0=NELC                   */  01146000
*        4 PROCFTX  BIT(1),       /* 1=CONFTXT 0=NCONFTXT           */  01147000
