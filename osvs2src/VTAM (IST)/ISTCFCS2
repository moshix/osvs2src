*                                  /* 0 = BRACKETS WILL NOT BE USED  */ 04710000
*          3 BINBKTR  BIT(1),      /* 1 = CONDITIONAL TERMINATION    */ 04711000
*                                  /* 0 = UNCONDITIONAL TERMINATION  */ 04712000
*          3 BINALT   BIT(1),      /* 1 = ALTERNATE CODE MAY BE USED */ 04713000
*                                  /* 0 = ALT. CODE MUST NOT BE USED */ 04714000
*          3 BINRSV04 BIT(3),      /* RESERVED                       */ 04715000
*        2 BINCMNP2,               /* COMMON NAU PROTOCOLS           */ 04716000
