SVTM     TITLE 'ISTCSCAC - CONNECTION SERVICES ACCEPT ROUTINE          *00001000
                        '                                               00002000
         ISEQ  73,80                                                    00003000
ISTCSCAC CSECT ,                                                01S0002 00004000
@MAINENT DS    0H                                               01S0002 00005000
         USING *,@15                                            01S0002 00006000
         B     @PROLOG                                          01S0002 00007000
         DC    AL1(16)                                          01S0002 00008000
         DC    C'ISTCSCAC  75.126'                              01S0002 00009000
         DROP  @15                                                      00010000
@PROLOG  BALR  @12,0                                            01S0002 00011000
@PSTART  DS    0H                                               01S0002 00012000
         USING @PSTART,@12                                      01S0002 00013000
         PRINT OFF                                                      00014000
         PRINT ON                                                       00015000
*   RETLOC=RETR;                    /* SAVE RETURN POINT REGISTER    */ 00016000
         ST    RETR,RETLOC(,RSAVE)                              02S0339 00017000
*   RFY                                                         02S0340 00018000
*     RETR UNRSTD;                                              02S0340 00019000
*   DO;                             /* SETCRR                   03S0341 00020000
*                                      FNID(CNSACIN)TYPE(ENTRY)      */ 00021000
*     CRACRR->CNSTFLDB=''B;         /* CLEAR CURRENT TRACE FIELD     */ 00022000
         L     @14,RPHCRR(,RPHPTR)                              03S0342 00023000
         L     @14,CRACRR(,@14)                                 03S0342 00024000
         XC    CNSTFLDB(3,@14),CNSTFLDB(@14)                    03S0342 00025000
*     CRACRR->CNSACINA='1'B;        /* SET HISTORY FLAG              */ 00026000
         OI    CNSACINA(@14),B'00100000'                        03S0343 00027000
*     CRACRR->CNSACINB='1'B;        /* SET CURRENT FLAG              */ 00028000
         OI    CNSACINB(@14),B'00100000'                        03S0344 00029000
*   END;                            /* SETCRR                        */ 00030000
*   CSPPTR=RPHCSPA;                 /* PICKUP CSP ADDRESS FROM THE      00031000
*                                      RPH                           */ 00032000
         L     CSPPTR,RPHCSPA(,RPHPTR)                          02S0346 00033000
*   RAPPTR=CSPSRC;                  /* PICKUP ACCEPTOR ADDR FROM THE    00034000
*                                      CSP                           */ 00035000
         L     RAPPTR,CSPSRC(,CSPPTR)                           02S0347 00036000
*   IF RAPABLGN=BITON               /* IF DEST NEVER ACCEPTS @DM10671*/ 00037000
*     THEN                          /*                       @DM10671*/ 00038000
         TM    RAPABLGN(RAPPTR),B'10000000'                     02S0348 00039000
         BNO   @RF00348                                         02S0348 00040000
*     CSPCC=NEVACC;                 /* TELL ACCEPTOR         @DM10671*/ 00041000
         MVC   CSPCC(2,CSPPTR),@CH00471                         02S0349 00042000
*   ELSE                            /* DEST ACCEPTS LOGONS,  @DM10671*/ 00043000
*     DO;                           /* SO:                   @DM10671*/ 00044000
         B     @RC00348                                         02S0350 00045000
@RF00348 DS    0H                                               02S0351 00046000
*                                                               03S0351 00047000
*/*------------------------------------------------------------------*/ 00048000
*/* SEGMENT-NAME      = LOCATICE                                     */ 00049000
*/*                                                                  */ 00050000
*/* SEGMENT-TYPE      = BLOCK                                        */ 00051000
*/*                                                                  */ 00052000
*/* SEGMENT-FUNCTION  = TO LOCATE A MATCHING LOGON ICE, IF           */ 00053000
*/*                     ONE EXISTS, FOR THE ACCEPT REQUEST           */ 00054000
*/*                                                                  */ 00055000
*/* SEGMENT-INVOKERS  = ISTCSCAC                                     */ 00056000
*/*                                                                  */ 00057000
*/* SEGMENTS-INCLUDED = NONE                                         */ 00058000
*/*                                                                  */ 00059000
*/* SEGMENTS-CALLED   = NONE                                         */ 00060000
*/*                                                                  */ 00061000
*/* ROUTINES-CALLED   = SEARCHQ  IN ISTCSCMR                         */ 00062000
*/*                                                                  */ 00063000
*/* MACROS-INVOKED    = NONE                                         */ 00064000
*/*------------------------------------------------------------------*/ 00065000
*                                                               03S0351 00066000
*LOCATICE:                                                      03S0351 00067000
*       ;                                                       03S0351 00068000
LOCATICE DS    0H                                               03S0352 00069000
*       IF CSPRSC=BITON             /* IF ACCEPT-ANY REQUEST         */ 00070000
*         THEN                                                  03S0352 00071000
         TM    CSPRSC(CSPPTR),B'10000000'                       03S0352 00072000
         BNO   @RF00352                                         03S0352 00073000
*         IF RAPICELG=ADDR(RAPICELG)/* IF NO LOGONS PENDING          */ 00074000
*           THEN                                                03S0353 00075000
         LA    @14,RAPICELG(,RAPPTR)                            03S0353 00076000
         C     @14,RAPICELG(,RAPPTR)                            03S0353 00077000
         BNE   @RF00353                                         03S0353 00078000
*           LINKPTR=FXCON0;         /* CLEAR LINK FIELD BASE         */ 00079000
         SLR   LINKPTR,LINKPTR                                  03S0354 00080000
*         ELSE                      /* LOGONS ARE PENDING, SO        */ 00081000
*           DO;                                                 03S0355 00082000
         B     @RC00353                                         03S0355 00083000
@RF00353 DS    0H                                               03S0356 00084000
*             LINKPTR=ADDR(RAPICELG);/* POINT LINK FIELD BASE        */ 00085000
         LA    LINKPTR,RAPICELG(,RAPPTR)                        03S0356 00086000
*             RCPREPTR=RAPICELG->ICEOBJ;/* POINT RESOURCE RDT ENTRY     00087000
*                                      BASE                          */ 00088000
         L     @14,RAPICELG(,RAPPTR)                            03S0357 00089000
         L     RCPREPTR,ICEOBJ(,@14)                            03S0357 00090000
*           END;                                                03S0358 00091000
*       ELSE                        /* ACCEPT-SPECIFIC, SO           */ 00092000
*         DO;                                                   03S0359 00093000
         B     @RC00352                                         03S0359 00094000
@RF00352 DS    0H                                               03S0360 00095000
*           ICEPTR=FXCON0;          /* CLEAR ICE BASE PTR            */ 00096000
         SLR   ICEPTR,ICEPTR                                    03S0360 00097000
*           RCPREPTR=CSLRDTE;       /* POINT RESOURCE RDT MAP BASE      00098000
*                                                            @D32CKMP*/ 00099000
*                                                               03S0361 00100000
         L     @14,CSPRSCE(,CSPPTR)                             03S0361 00101000
         L     RCPREPTR,CSLRDTE(,@14)                           03S0361 00102000
*           /*********************************************************/ 00103000
*           /*                                                       */ 00104000
*           /* CALL THE SEARCHQ SUBROUTINE IN ISTCSCMR TO SCAN THE   */ 00105000
*           /* ACCEPTORS LOGONS PENDING QUEUE FOR AN ICE WHOSE OBJECT*/ 00106000
*           /* IS THE RESOURCE TO BE ACCEPTED. INPUT: THE PARAMETER  */ 00107000
*           /* LIST IN THE RPH AS FOLLOWS: PARM1 - ADDRESS OF THE    */ 00108000
*           /* ACCEPTORS LOGONS PENDING CHAIN. PARM2 - OFFSET OF THE */ 00109000
*           /* LINK FIELD IN AN ICE. PARM3 - OFFSET OF THE FIELD TO  */ 00110000
*           /* BE EXAMINED IN AN ICE. PARM4 - ADDRESS OF THE RESOURCE*/ 00111000
*           /* RDTE BEING SEARCHED FOR. OUTPUT: R15 IS SET AS        */ 00112000
*           /* FOLLOWS: =0 - ELEMENT WAS NOT FOUND ^0 - ADDRESS OF   */ 00113000
*           /* THE LINK FIELD POINTING TO THE ELEMENT                */ 00114000
*           /*                                                       */ 00115000
*           /*********************************************************/ 00116000
*                                                               03S0362 00117000
*           RPHSAVE1=ADDR(RAPICELG);/* PARM1                         */ 00118000
         LA    @14,RAPICELG(,RAPPTR)                            03S0362 00119000
         ST    @14,RPHSAVE1(,RPHPTR)                            03S0362 00120000
*           RPHSAVE2=ADDR(ICESUBJQ)-ADDR(ISTICE);/* PARM2            */ 00121000
         LA    @14,ICESUBJQ(,ICEPTR)                            03S0363 00122000
         SLR   @14,ICEPTR                                       03S0363 00123000
         ST    @14,RPHSAVE2(,RPHPTR)                            03S0363 00124000
*           RPHSAVE3=ADDR(ICEOBJ)-ADDR(ISTICE);/* PARM3              */ 00125000
         LA    @14,ICEOBJ(,ICEPTR)                              03S0364 00126000
         SLR   @14,ICEPTR                                       03S0364 00127000
         ST    @14,RPHSAVE3(,RPHPTR)                            03S0364 00128000
*           RPHSAVE4=RCPREPTR;      /* PARM4                         */ 00129000
         ST    RCPREPTR,RPHSAVE4(,RPHPTR)                       03S0365 00130000
*           ENTRYR=SRCHQPTR;        /* POINT ENTRY POINT BASE        */ 00131000
         L     @07,ATCCSMA(,ATCVTPTR)                           03S0366 00132000
         L     ENTRYR,SRCHQPTR+64(,@07)                         03S0366 00133000
*           CALL MXTRN;             /* SEARCH FOR MATCH              */ 00134000
         BALR  @14,@15                                          03S0367 00135000
*         END;                      /* ACCEPT-SPEC RQST              */ 00136000
*       IF LINKPTR^=FXCON0          /* IF MATCHING ICE FOUND         */ 00137000
*         THEN                                                  02S0369 00138000
@RC00352 LTR   LINKPTR,LINKPTR                                  02S0369 00139000
         BZ    @RF00369                                         02S0369 00140000
*         DO;                                                   02S0370 00141000
*                                                               03S0371 00142000
*/*------------------------------------------------------------------*/ 00143000
*/* SEGMENT-NAME      = ICEFOUND                                     */ 00144000
*/*                                                                  */ 00145000
*/* SEGMENT-TYPE      = BLOCK                                        */ 00146000
*/*                                                                  */ 00147000
*/* SEGMENT-FUNCTION  = TO REUSE THE LOCATED ICE STORAGE             */ 00148000
*/*                     AREA AS AN ACTIVE CONNECTION                 */ 00149000
*/*                     ELEMENT FOR THE NEW SESSION                  */ 00150000
*/*                                                                  */ 00151000
*/* SEGMENT-INVOKERS  = ISTCSCAC                                     */ 00152000
*/*                                                                  */ 00153000
*/* SEGMENTS-INCLUDED = NONE                                         */ 00154000
*/*                                                                  */ 00155000
*/* SEGMENTS-CALLED   = NONE                                         */ 00156000
*/*                                                                  */ 00157000
*/* ROUTINES-CALLED   = TCONN    IN ISTCSCMR                         */ 00158000
*/*                     ACONN    IN ISTCSCMR                         */ 00159000
*/*                     DOACCT   IN ISTCSCMR                         */ 00160000
*/*                                                                  */ 00161000
*/* MACROS-INVOKED    = NONE                                         */ 00162000
*/*------------------------------------------------------------------*/ 00163000
*                                                               03S0371 00164000
*ICEFOUND:                                                      03S0371 00165000
*           ;                                                   03S0371 00166000
ICEFOUND DS    0H                                               03S0372 00167000
*           RFY                                                 03S0372 00168000
*             CHAINFLD BASED(LINKPTR);                          03S0372 00169000
*           ICEPTR=CHAINFLD;        /* POINT AT LOGON ICE            */ 00170000
         L     ICEPTR,CHAINFLD(,LINKPTR)                        03S0373 00171000
*           IF RPRSTPCT^=FXCON0     /* IF SHOULDER TAPPERS..         */ 00172000
*             THEN                  /* ...ARE WAITING                */ 00173000
         ICM   @14,3,RPRSTPCT(RCPREPTR)                         03S0374 00174000
         BZ    @RF00374                                         03S0374 00175000
*             DO;                                               03S0375 00176000
*               CSPCC=GOODSTAP;     /* TELL ACCEPTOR ABOUT           */ 00177000
         MVC   CSPCC(2,CSPPTR),@CH00094                         03S0376 00178000
*               CSLNIBF=BITON;      /* SHOULDER TAPPERS      @D32CKMP*/ 00179000
         L     @14,CSPRSCE(,CSPPTR)                             03S0377 00180000
         OI    CSLNIBF(@14),B'10000000'                         03S0377 00181000
*             END;                                              03S0378 00182000
*           RCPICELN=ADDR(RCPICELN);/* REMOVE ICE FROM RSC           */ 00183000
@RF00374 LA    @14,RCPICELN(,RCPREPTR)                          03S0379 00184000
         ST    @14,RCPICELN(,RCPREPTR)                          03S0379 00185000
*           CHAINFLD=ICESUBJQ;      /* REMOVE ICE FROM DEST          */ 00186000
         MVC   CHAINFLD(4,LINKPTR),ICESUBJQ(ICEPTR)             03S0380 00187000
*           IF ICERSC2F=BITON       /* IF RSC-2 INIT RU NAME.        */ 00188000
*             THEN                  /* ...IS PRESENT IN ICE          */ 00189000
         TM    ICERSC2F(ICEPTR),B'00010000'                     03S0381 00190000
         BNO   @RF00381                                         03S0381 00191000
*             DO;                                               03S0382 00192000
*               CSPRSC2B=ICERSC2B;  /* COPY INIT RU NAME TO..        */ 00193000
         MVC   CSPRSC2B(8,CSPPTR),ICERSC2B(ICEPTR)              03S0383 00194000
*               CSPRSC2F=BITON;     /* ...CSP & FLAG PRESENCE        */ 00195000
         OI    CSPRSC2F(CSPPTR),B'00000010'                     03S0384 00196000
*             END;                                              03S0385 00197000
*           IF ICEDATA^=FXCON0      /* IF LOGON DATA PRESENT.        */ 00198000
*             THEN                  /* ...WITH ICE                   */ 00199000
@RF00381 L     @14,ICEDATA(,ICEPTR)                             03S0386 00200000
         LTR   @14,@14                                          03S0386 00201000
         BZ    @RF00386                                         03S0386 00202000
*             CSLDATA=ICEDATA;      /* RETURN LOGON DATA PTR @D32CKMP*/ 00203000
         L     @07,CSPRSCE(,CSPPTR)                             03S0387 00204000
         ST    @14,CSLDATA(,@07)                                03S0387 00205000
*           ISTICE=ISTICE&&ISTICE;  /* CLEAR ICE FOR REUSE           */ 00206000
*                                                               03S0388 00207000
@RF00386 XC    ISTICE(44,ICEPTR),ISTICE(ICEPTR)                 03S0388 00208000
*           /*********************************************************/ 00209000
*           /*                                                       */ 00210000
*           /* CALL THE TCONN SUBROUTINE IN ISTCSCMR TO CONNECT THE  */ 00211000
*           /* ACTIVE CONNECTION ELEMENT (ACE) TO THE RESOURCE RDT   */ 00212000
*           /* ENTRY.                                                */ 00213000
*           /*                                                       */ 00214000
*           /*********************************************************/ 00215000
*                                                               03S0389 00216000
*           ENTRYR=TCONNPTR;        /* POINT AT TCONNECT RTN         */ 00217000
         L     @07,ATCCSMA(,ATCVTPTR)                           03S0389 00218000
         L     ENTRYR,TCONNPTR+68(,@07)                         03S0389 00219000
*           CALL MXTRN;             /* CONNECT ACE TO RSC            */ 00220000
*                                                               03S0390 00221000
         BALR  @14,@15                                          03S0390 00222000
*           /*********************************************************/ 00223000
*           /*                                                       */ 00224000
*           /* CALL THE ACONN SUBROUTINE IN ISTCSCMR TO CONNECT THE  */ 00225000
*           /* ACTIVE CONNECTION ELEMENT (ACE) TO THE DESTINATION RDT*/ 00226000
*           /* ENTRY.                                                */ 00227000
*           /*                                                       */ 00228000
*           /*********************************************************/ 00229000
*                                                               03S0391 00230000
*           ENTRYR=ACONNPTR;        /* POINT AT ACONNECT RTN         */ 00231000
         L     @07,ATCCSMA(,ATCVTPTR)                           03S0391 00232000
         L     ENTRYR,ACONNPTR+72(,@07)                         03S0391 00233000
*           CALL MXTRN;             /* CONNECT ACE TO DEST           */ 00234000
         BALR  @14,@15                                          03S0392 00235000
*           IF ATCACCTA^=FXCON0     /* IF ACCOUNT RTN PRESENT        */ 00236000
*             THEN                                              03S0393 00237000
         ICM   @14,15,ATCACCTA(ATCVTPTR)                        03S0393 00238000
         BZ    @RF00393                                         03S0393 00239000
*             DO;                                               03S0394 00240000
*                                                               03S0394 00241000
*               /*****************************************************/ 00242000
*               /*                                                   */ 00243000
*               /* CALL THE DOACCT SUBROUTINE IN ISTCSCMR TO         */ 00244000
*               /* INTERFACE TO THE USERS ACCOUTING EXIT TO RECORD A */ 00245000
*               /* NEW SESSION CONNECTION.                           */ 00246000
*               /*                                                   */ 00247000
*               /*****************************************************/ 00248000
*                                                               03S0395 00249000
*               ENTRYR=DACCTPTR;    /* POINT AT ACCOUNT RTN          */ 00250000
         L     @07,ATCCSMA(,ATCVTPTR)                           03S0395 00251000
         L     ENTRYR,DACCTPTR+100(,@07)                        03S0395 00252000
*               CALL MXTRN;         /* RECORD NEW CONNECTION         */ 00253000
         BALR  @14,@15                                          03S0396 00254000
*             END;                                              03S0397 00255000
*           IF CSPRSC=BITON         /* IF REQUEST WAS FOR AN.        */ 00256000
*             THEN                  /* ...ACCEPT ANY                 */ 00257000
@RF00393 TM    CSPRSC(CSPPTR),B'10000000'                       03S0398 00258000
         BNO   @RF00398                                         03S0398 00259000
*             CSPTRDTE=RCPREPTR;    /* TELL WHICH CONNECTION         */ 00260000
         ST    RCPREPTR,CSPTRDTE(,CSPPTR)                       03S0399 00261000
*           ELSE                    /* ACCEPT SPECIFIC, SO:          */ 00262000
*             CSPTRDTE=FXCON0;      /* ALREADY KNOW CONNECT          */ 00263000
         B     @RC00398                                         03S0400 00264000
@RF00398 SLR   @14,@14                                          03S0400 00265000
         ST    @14,CSPTRDTE(,CSPPTR)                            03S0400 00266000
*         END;                                                  02S0401 00267000
*       ELSE                        /* ICE WAS NOT FOUND, SO:        */ 00268000
*         DO;                                                   02S0402 00269000
         B     @RC00369                                         02S0402 00270000
@RF00369 DS    0H                                               02S0403 00271000
*                                                               03S0403 00272000
*/*------------------------------------------------------------------*/ 00273000
*/* SEGMENT-NAME      = ICENTFND                                     */ 00274000
*/*                                                                  */ 00275000
*/* SEGMENT-TYPE      = BLOCK                                        */ 00276000
*/*                                                                  */ 00277000
*/* SEGMENT-FUNCTION  = TO OBTAIN STORAGE FOR AN UNLOCATED           */ 00278000
*/*                     ICE AND LINK IT TO THE APPROPRIATE           */ 00279000
*/*                     CONTROL BLOCKS, WHEN REQUEST QUEUE           */ 00280000
*/*                     IS SPECIFIED FOR THE ACCEPT                  */ 00281000
*/*                                                                  */ 00282000
*/* SEGMENT-INVOKERS  = ISTCSCAC                                     */ 00283000
*/*                                                                  */ 00284000
*/* SEGMENTS-INCLUDED = NONE                                         */ 00285000
*/*                                                                  */ 00286000
*/* SEGMENTS-CALLED   = NONE                                         */ 00287000
*/*                                                                  */ 00288000
*/* ROUTINES-CALLED   = GETST    IN ISTCSCMR                         */ 00289000
*/*                     QUEUE    IN ISTCSCMR                         */ 00290000
*/*                     WAIT     IN ISTCSCMR                         */ 00291000
*/*                                                                  */ 00292000
*/* MACROS-INVOKED    = NONE                                         */ 00293000
*/*------------------------------------------------------------------*/ 00294000
*                                                               03S0403 00295000
*ICENTFND:                                                      03S0403 00296000
*           ;                                                   03S0403 00297000
ICENTFND DS    0H                                               03S0404 00298000
*           IF CSPQ=BITON           /* IF QUEUEING OPTIONALLY        */ 00299000
*             THEN                  /* ...REQUESTED                  */ 00300000
         TM    CSPQ(CSPPTR),B'01000000'                         03S0404 00301000
         BNO   @RF00404                                         03S0404 00302000
*             DO;                                               03S0405 00303000
*               RFY                                             03S0406 00304000
*                (CECT,                                         03S0406 00305000
*                 UECBCT) RSTD;                                 03S0406 00306000
*               CECT=FXCON1;        /* INDICATE NEED AN ICE          */ 00307000
         LA    CECT,1                                           03S0407 00308000
*               UECBCT=FXCON0;      /* INDICATE NO UECB REQD         */ 00309000
*                                                               03S0408 00310000
         SLR   UECBCT,UECBCT                                    03S0408 00311000
*               /*****************************************************/ 00312000
*               /*                                                   */ 00313000
*               /* CALL THE GETST SUBROUTINE IN ISTCSCMR TO OBTAIN A */ 00314000
*               /* CONNECTION ELEMENT STORAGE AREA. IF THE AREA WAS  */ 00315000
*               /* OBTAINED, ITS ADDRESS WILL BE IN CECT AND A RETURN*/ 00316000
*               /* CODE OF ZERO WILL BE IN RC.                       */ 00317000
*               /*                                                   */ 00318000
*               /*****************************************************/ 00319000
*                                                               03S0409 00320000
*               ENTRYR=GETSTPTR;    /* POINT AT GETSTORE RTN         */ 00321000
         L     @07,ATCCSMA(,ATCVTPTR)                           03S0409 00322000
         L     ENTRYR,GETSTPTR+84(,@07)                         03S0409 00323000
*               CALL MXTRN;         /* GET ICE STORAGE AREA          */ 00324000
         BALR  @14,@15                                          03S0410 00325000
*               IF RC=FXCON0        /* IF THE STORAGE WAS....        */ 00326000
*                 THEN              /* ...AVAILABLE                  */ 00327000
         LTR   RC,RC                                            03S0411 00328000
         BNZ   @RF00411                                         03S0411 00329000
*                 DO;                                           03S0412 00330000
*                   ICEPTR=CECT;    /* GET ICE ADDRESS               */ 00331000
         LR    ICEPTR,CECT                                      03S0413 00332000
*                   RFY                                         03S0414 00333000
*                    (CECT,                                     03S0414 00334000
*                     UECBCT) UNRSTD;                           03S0414 00335000
*                                                               03S0414 00336000
*                   /*************************************************/ 00337000
*                   /*                                               */ 00338000
*                   /* CALL THE QUEUE SUBROUTINE IN ISTCSCMR TO ADD  */ 00339000
*                   /* THE CONNECTION ELEMENT TO THE DESTINATION RDT */ 00340000
*                   /* ENTRY QUEUE OF ACCEPT ICES.                   */ 00341000
*                   /*                                               */ 00342000
*                   /*************************************************/ 00343000
*                                                               03S0415 00344000
*                   RAPPTR=CSPSRC;  /* REESTABLISH APPL PTR          */ 00345000
         L     RAPPTR,CSPSRC(,CSPPTR)                           03S0415 00346000
*                   RPHSAVE1=ICEPTR;/* PTR TO CE TO QUEUE            */ 00347000
         ST    ICEPTR,RPHSAVE1(,RPHPTR)                         03S0416 00348000
*                   RPHSAVE2=ADDR(RAPICEAC);/* QUEUE HEAD            */ 00349000
         LA    @14,RAPICEAC(,RAPPTR)                            03S0417 00350000
         ST    @14,RPHSAVE2(,RPHPTR)                            03S0417 00351000
*                   RPHSAVE3=ADDR(ICESUBJQ)-ADDR(ISTICE);/* OFFSET   */ 00352000
         LA    @14,ICESUBJQ(,ICEPTR)                            03S0418 00353000
         SLR   @14,ICEPTR                                       03S0418 00354000
         ST    @14,RPHSAVE3(,RPHPTR)                            03S0418 00355000
*                   ENTRYR=QUECBPTR;/* POINT AT QUEUE CB RTN         */ 00356000
         L     @07,ATCCSMA(,ATCVTPTR)                           03S0419 00357000
         L     ENTRYR,QUECBPTR+60(,@07)                         03S0419 00358000
*                   CALL MXTRN;     /* QUEUE ICE TO DEST             */ 00359000
         BALR  @14,@15                                          03S0420 00360000
*                   ICESUBJ=RAPPTR; /* POINT ICE AT DEST             */ 00361000
         ST    RAPPTR,ICESUBJ(,ICEPTR)                          03S0421 00362000
*                   IF CSPRSC=BITON /* IF REQUEST WAS FOR....        */ 00363000
*                     THEN          /* ...ACCEPT ANY                 */ 00364000
         TM    CSPRSC(CSPPTR),B'10000000'                       03S0422 00365000
         BNO   @RF00422                                         03S0422 00366000
*                     DO;                                       03S0423 00367000
*                       ICEOBJ=0;   /* CLEAR ICE RESOURCE PTR        */ 00368000
         SLR   @14,@14                                          03S0424 00369000
         ST    @14,ICEOBJ(,ICEPTR)                              03S0424 00370000
*                       ICESPEC=BITOF;/* INDICATE ANY REQUEST        */ 00371000
         NI    ICESPEC(ICEPTR),B'01111111'                      03S0425 00372000
*                     END;                                      03S0426 00373000
*                   ELSE            /* ACCEPT SPEC REQST, SO:        */ 00374000
*                     DO;                                       03S0427 00375000
         B     @RC00422                                         03S0427 00376000
@RF00422 DS    0H                                               03S0428 00377000
*                       ICEOBJ=CSLRDTE;/* POINT ICE AT RESOURCE 03S0428 00378000
*                                                            @D32CKMP*/ 00379000
         L     @14,CSPRSCE(,CSPPTR)                             03S0428 00380000
         MVC   ICEOBJ(4,ICEPTR),CSLRDTE(@14)                    03S0428 00381000
*                       ICESPEC=BITON;/* INDICATE SPEC REQUEST       */ 00382000
         OI    ICESPEC(ICEPTR),B'10000000'                      03S0429 00383000
*                     END;                                      03S0430 00384000
*                   ICERPHA=RPHPTR; /* POINT ICE AT ACCPT RPH        */ 00385000
*                                                               03S0431 00386000
@RC00422 ST    RPHPTR,ICERPHA(,ICEPTR)                          03S0431 00387000
*                   /*************************************************/ 00388000
*                   /*                                               */ 00389000
*                   /* CALL THE WAIT SUBROUTINE IN ISTCSCMR TO WAIT  */ 00390000
*                   /* FOR THE COMPLETION OF THE QUEUE REQUEST.      */ 00391000
*                   /*                                               */ 00392000
*                   /*************************************************/ 00393000
*                                                               03S0432 00394000
