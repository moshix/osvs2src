         L     @10,ATCCSMA(,ATCVTPTR)                           06S0437 00389000
         L     ENTRYR,SERTNPTR+84(,@10)                         06S0437 00390000
*                     CALL MXTRN;   /* GET REQUIRED STORAGE          */ 00391000
         BALR  @14,@15                                          06S0438 00392000
*                     RFY                                       06S0439 00393000
*                       ENTRYR UNRSTD;                          06S0439 00394000
*                     CES=CECT;     /* SAVE PTR TO CHAIN OF CES      */ 00395000
         ST    CECT,CES(,RSAVE)                                 06S0440 00396000
*                     UECBS=UECBCT; /* SAVE PTR TO CHAIN OF UECBS    */ 00397000
         ST    UECBCT,UECBS(,RSAVE)                             06S0441 00398000
*                   END;            /* END OF GETSTORE LOGIC SEGMENT    00399000
