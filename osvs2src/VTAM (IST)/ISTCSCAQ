@RF00382 DS    0H                                               02S0384 00153000
SETUPREQ DS    0H                                               02S0385 00154000
*         RFY                                                   02S0385 00155000
*           RSCLPTR RSTD;                                       02S0385 00156000
*         RSCLPTR=CSPRSCE;          /* POINT AT RESOURCE LIST        */ 00157000
         L     RSCLPTR,CSPRSCE(,CSPPTR)                         02S0386 00158000
*         RFY                                                   02S0387 00159000
*           NORSCRQD RSTD;                                      02S0387 00160000
*         IF CSPRSC='1'B            /* IS IT A GROUP REQUEST?        */ 00161000
*           THEN                                                02S0388 00162000
         TM    CSPRSC(CSPPTR),B'10000000'                       02S0388 00163000
         BNO   @RF00388                                         02S0388 00164000
*           DO;                                                 02S0389 00165000
*             IF CSPCOREQ='1'B      /* A COREQUESITE REQUEST?        */ 00166000
*               THEN                                            02S0390 00167000
         TM    CSPCOREQ(CSPPTR),B'01000000'                     02S0390 00168000
         BNO   @RF00390                                         02S0390 00169000
*               NORSCRQD=CSPNO;     /* NO. REQUIRED IS NO. IN LIST   */ 00170000
         LH    NORSCRQD,CSPNO(,CSPPTR)                          02S0391 00171000
*             ELSE                                              02S0392 00172000
*               NORSCRQD=1;         /* NO. REQUIRED IS 1             */ 00173000
         B     @RC00390                                         02S0392 00174000
@RF00390 LA    NORSCRQD,1                                       02S0392 00175000
*           END;                                                02S0393 00176000
*         ELSE                                                  02S0394 00177000
*           DO;                     /* SINGLE TERMINAL REDUIRED      */ 00178000
         B     @RC00388                                         02S0394 00179000
@RF00388 DS    0H                                               02S0395 00180000
*             NORSCRQD=1;           /* NO. REQUIRED IS 1             */ 00181000
         LA    @10,1                                            02S0395 00182000
         LR    NORSCRQD,@10                                     02S0395 00183000
*             CSPNO=1;              /* CREATE A LIST OF LENGTH 1        00184000
*                                                            @D32CKMP*/ 00185000
         STH   @10,CSPNO(,CSPPTR)                               02S0396 00186000
*           END;                                                02S0397 00187000
*INTRPREQ:                                                      02S0398 00188000
*                                                               03S0398 00189000
*/********************************************************************/ 00190000
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO SEE IF THE              */ 00191000
*/* REQUIRED NUMBER OF RESOURCES (TERMINALS) ARE FREE                */ 00192000
*/* TO SATISFY THE REQUEST, AND IF SO, TO TRY AND                    */ 00193000
*/* SATISFY THE REQUEST.  IF THE REQUEST CANNOT BE                   */ 00194000
*/* SATISFIED, BUT QUEUING HAS BEEN REQUESTED, TO QUEUE              */ 00195000
*/* THE REQUEST ON THE DESTINATION (APPLICATION) AND                 */ 00196000
*/* RESOURCE (TERMINAL) RDT ENTRIES.                                 */ 00197000
*/********************************************************************/ 00198000
*                                                               03S0398 00199000
*         DO;                       /* BGN OF INTRPREQ LOGIC SEGMENT    00200000
*                                                            @D32CKMP*/ 00201000
@RC00388 DS    0H                                               03S0398 00202000
INTRPREQ DS    0H                                               03S0399 00203000
*           RFY                                                 03S0399 00204000
*             FREECT RSTD;                                      03S0399 00205000
*           FREECT=0;               /* INITIALISE FREE TRML COUNT    */ 00206000
         SLR   FREECT,FREECT                                    03S0400 00207000
*           RFY                                                 03S0401 00208000
*             FFTI RSTD;                                        03S0401 00209000
*           RFY                                                 03S0402 00210000
*            (RPREPTR,                                          03S0402 00211000
*             RCPREPTR,                                         03S0402 00212000
*             RSCLINDX) RSTD;                                   03S0402 00213000
*           DO RSCLINDX=1 TO CSPNO WHILE(FREECT<NORSCRQD);      03S0403 00214000
         LA    RSCLINDX,1                                       03S0403 00215000
         B     @DE00403                                         03S0403 00216000
@DL00403 CR    FREECT,NORSCRQD                                  03S0403 00217000
         BNL   @DC00403                                         03S0403 00218000
*             RPREPTR=CSLRDTE;      /* POINT AT TRML RDTE WHOSE 03S0404 00219000
*                                      STATUS IS TO BE EXAMINED 03S0404 00220000
*                                                            @D32CKMP*/ 00221000
         LR    @10,RSCLINDX                                     03S0404 00222000
         BCTR  @10,0                                            03S0404 00223000
         MH    @10,@CH00447                                     03S0404 00224000
         ALR   @10,RSCLPTR                                      03S0404 00225000
         L     RPREPTR,CSLRDTE(,@10)                            03S0404 00226000
*             IF RPRLIVE='1'B|      /* FOR A LIVE TERMINAL           */ 00227000
*                 (RPRENTRY=RPRENTLU&/* DEVICE IS LU         @ZA19893*/ 00228000
*                 RPRNSNEA->RCCRESET='1'B) THEN/* RESET IN PROG 03S0405 00229000
*                                                            @ZA19893*/ 00230000
