*                 DO;                                           03S0407 00243000
*                   FREECT=FREECT+1;/* INCREMENT FREE COUNT          */ 00244000
         AH    FREECT,@CH00039                                  03S0408 00245000
*                   IF CSPLOGI='0'B /* IF COREQ ACQUIRE      @DL31HBS*/ 00246000
*                     THEN                                      03S0409 00247000
         TM    CSPLOGI(CSPPTR),B'00100000'                      03S0409 00248000
         BNZ   @RF00409                                         03S0409 00249000
*                     CSLRDTEF=BITON;/* INDICATE TRML RDTE IS BEING     00250000
*                                      ACQUIRED              @D32CKMP*/ 00251000
         LR    @10,RSCLINDX                                     03S0410 00252000
         BCTR  @10,0                                            03S0410 00253000
         MH    @10,@CH00447                                     03S0410 00254000
         ALR   @10,RSCLPTR                                      03S0410 00255000
         OI    CSLRDTEF(@10),B'10000000'                        03S0410 00256000
*                   ELSE                                        03S0411 00257000
*                     ;                                         03S0411 00258000
@RF00409 DS    0H                                               03S0412 00259000
*                   IF FREECT=1     /* IS IT THE FIRST FREE TRML?    */ 00260000
*                     THEN                                      03S0412 00261000
