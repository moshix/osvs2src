SVTM     TITLE 'ISTCSCLG - CONNECTION SERVICES LOGON ROUTINE           *00001000
                        '                                               00002000
         ISEQ  73,80                                                    00003000
ISTCSCLG CSECT ,                                                01S0002 00004000
@MAINENT DS    0H                                               01S0002 00005000
         USING *,@15                                            01S0002 00006000
         B     @PROLOG                                          01S0002 00007000
         DC    AL1(16)                                          01S0002 00008000
         DC    C'ISTCSCLG  75.126'                              01S0002 00009000
         DROP  @15                                                      00010000
@PROLOG  BALR  @12,0                                            01S0002 00011000
@PSTART  DS    0H                                               01S0002 00012000
         USING @PSTART,@12                                      01S0002 00013000
         PRINT OFF                                                      00014000
         PRINT ON                                                       00015000
*   SAVERET1=RETPTR;                /* SAVE RETURN ADDRESS           */ 00016000
         ST    RETPTR,SAVERET1+124(,SAVEPTR)                    01S0306 00017000
*   RFY                                                         01S0307 00018000
*     RETPTR UNRSTD;                                            01S0307 00019000
*   DO;                             /* SETCRR                   01S0308 00020000
*                                      FNID(CNSLGIN)TYPE(ENTRY)      */ 00021000
*     CRACRR->CNSTFLDB=''B;         /* CLEAR CURRENT TRACE FIELD     */ 00022000
         L     @14,RPHCRR(,RPHPTR)                              01S0309 00023000
         L     @14,CRACRR(,@14)                                 01S0309 00024000
         XC    CNSTFLDB(3,@14),CNSTFLDB(@14)                    01S0309 00025000
*     CRACRR->CNSLGINA='1'B;        /* SET HISTORY FLAG              */ 00026000
         OI    CNSLGINA(@14),B'00010000'                        01S0310 00027000
*     CRACRR->CNSLGINB='1'B;        /* SET CURRENT FLAG              */ 00028000
         OI    CNSLGINB(@14),B'00010000'                        01S0311 00029000
*   END;                            /* SETCRR                        */ 00030000
*   DO;                             /* BLOCK MAIN START OF MAIN LOGIC   00031000
*                                                            @D32CKMP*/ 00032000
*     CSPPTR=RPHCSPA;               /* FIND CSP                      */ 00033000
         L     CSPPTR,RPHCSPA(,RPHPTR)                          01S0314 00034000
*     TERMPTR=CSLRDTE;              /* ACCESS TERMINAL RDTE  @D32CKMP*/ 00035000
         L     @14,CSPRSCE(,CSPPTR)                             01S0315 00036000
         L     TERMPTR,CSLRDTE(,@14)                            01S0315 00037000
*     RAPPTR=CSPDEST;               /* ACCESS DESTINATION RDTE       */ 00038000
         L     RAPPTR,CSPDEST(,CSPPTR)                          01S0316 00039000
*/*SEE WHETHER LOGON CAN BE DONE                                     */ 00040000
*/*  LEAVE(MAIN) ON(RAPABLGN='1'B)ANDDO                              */ 00041000
*                                                               01S0317 00042000
*     IF(RAPABLGN='1'B) THEN                                    01S0317 00043000
         TM    RAPABLGN(RAPPTR),B'10000000'                     01S0317 00044000
         BNO   @RF00317                                         01S0317 00045000
*       DO;                         /* ON DEST NEVER         @D32CKMP*/ 00046000
*         CSPCC=NEVACC;             /* ACCEPTS SET CONDITION CODE AND   00047000
*                                                            @D32CKMP*/ 00048000
         MVC   CSPCC(2,CSPPTR),@CH00441                         01S0319 00049000
*         GOTO #BCM0008;            /* END(LEAVE)                    */ 00050000
         B     #BCM0008                                         01S0320 00051000
*       END;                        /* EXIT FROM LOGON PROCESSOR        00052000
*                                                            @D32CKMP*/ 00053000
*/*  LEAVE(MAIN) ON(RAPAQUI='1'B)ANDDO                               */ 00054000
*                                                               01S0322 00055000
*     IF(RAPAQUI='1'B) THEN                                     01S0322 00056000
@RF00317 TM    RAPAQUI(RAPPTR),B'01000000'                      01S0322 00057000
         BNO   @RF00322                                         01S0322 00058000
*       DO;                         /* ON DEST QUIESCING     @D32CKMP*/ 00059000
*         CSPCC=DESTQUI;            /* SET CONDITION CODE AND EXIT TO   00060000
*                                                            @D32CKMP*/ 00061000
         MVC   CSPCC(2,CSPPTR),@CH01678                         01S0324 00062000
*         GOTO #BCM0008;            /* END(LEAVE)                    */ 00063000
         B     #BCM0008                                         01S0325 00064000
*       END;                        /* FROM LOGON PROCESSOR  @D32CKMP*/ 00065000
*/*  LEAVE(MAIN) ON(RPRTIED='1'B)ANDDO                               */ 00066000
*                                                               01S0327 00067000
*     IF(RPRTIED='1'B) THEN                                     01S0327 00068000
@RF00322 TM    RPRTIED(TERMPTR),B'00100000'                     01S0327 00069000
         BNO   @RF00327                                         01S0327 00070000
*       DO;                         /* ON TRML TIED SET      @D32CKMP*/ 00071000
*         CSPCC=RSCETIED;           /* CONDITION CODE AND EXIT FROM     00072000
*                                                            @D32CKMP*/ 00073000
         MVC   CSPCC(2,CSPPTR),@CH02459                         01S0329 00074000
*         GOTO #BCM0008;            /* END(LEAVE)                    */ 00075000
         B     #BCM0008                                         01S0330 00076000
*       END;                        /* LOGON PROCESSOR       @D32CKMP*/ 00077000
*                                                               01S0331 00078000
*     /***************************************************************/ 00079000
*     /*                                                             */ 00080000
*     /* IF DESTINATION HAS A LOGON EXIT, STORAGE FOR ONE UECB IS    */ 00081000
*     /* REQUIRED                                                    */ 00082000
*     /*                                                             */ 00083000
*     /***************************************************************/ 00084000
*                                                               01S0332 00085000
*     RFY                                                       01S0332 00086000
*      (REQCE,                                                  01S0332 00087000
*       REQUECB) RSTD;                                          01S0332 00088000
@RF00327 DS    0H                                               01S0333 00089000
*     IF(RAPPTR->RPRACT='1'B)       /* IF DEST IS ACTIVE             */ 00090000
*         &(RAPASLGI='1'B)          /* & LOGON EXITS TO BE SCHEDULED */ 00091000
*       THEN                                                    01S0333 00092000
         TM    RPRACT(RAPPTR),B'10000000'                       01S0333 00093000
         BNO   @RF00333                                         01S0333 00094000
         TM    RAPASLGI(RAPPTR),B'00100000'                     01S0333 00095000
         BNO   @RF00333                                         01S0333 00096000
*       REQUECB=1;                  /* ONE UECB REQD FOR USER EXIT      00097000
*                                      SCHEDLR                       */ 00098000
         LA    REQUECB,1                                        01S0334 00099000
*     ELSE                                                      01S0335 00100000
*       REQUECB=0;                  /* NO UECBS REQD                 */ 00101000
         B     @RC00333                                         01S0335 00102000
@RF00333 SLR   REQUECB,REQUECB                                  01S0335 00103000
*     REQCE=1;                      /* ONE ICE REQD FOR LOGON0       */ 00104000
@RC00333 LA    REQCE,1                                          01S0336 00105000
*                                                               02S0337 00106000
*/********************************************************************/ 00107000
*/* THE FUNCTION OF THIS LOGIC SEGMENT IS TO OBTAIN THE              */ 00108000
*/* REQUIRED UECB AND/OR CE STORAGE FOR PROCESSING A                 */ 00109000
*/* LOGON REQUEST AND TO CALL THE CONNECTION SERVICES                */ 00110000
*/* MASTER ROUTER LOGON0 FUNCTION TO COMPLETE THE                    */ 00111000
*/* LOGGING OF THE TERMINAL TO THE APPLICATION.                      */ 00112000
*/********************************************************************/ 00113000
*                                                               02S0337 00114000
*     DO;                           /* BGN OF LOGON1 LOGIC SEGMENT      00115000
*                                                            @D32CKMP*/ 00116000
*       ENTRYREG=GETSPTR;           /* POINT TO GETSTORE ROUTN       */ 00117000
         L     @07,ATCCSMA(,ATCVTPTR)                           02S0338 00118000
         L     ENTRYREG,GETSPTR+84(,@07)                        02S0338 00119000
*       CALL MXTRN;                 /* CALL GETSTORE FOR STORAGE     */ 00120000
         BALR  @14,@15                                          02S0339 00121000
*/*  LEAVE(MAIN) ON(RETVAL^=GOOD)ANDDO                               */ 00122000
*                                                               02S0340 00123000
*       IF(RETVAL^=GOOD) THEN                                   02S0340 00124000
         CH    RETVAL,@CH00551                                  02S0340 00125000
         BE    @RF00340                                         02S0340 00126000
*         DO;                       /* ON STORAGE NOT        @D32CKMP*/ 00127000
*           CSPCC=NOCORE;           /* AVAILABLE SET CPMPLETION CODE    00128000
*                                                            @D32CKMP*/ 00129000
         MVC   CSPCC(2,CSPPTR),@CH00395                         02S0342 00130000
*           GOTO #BCM0008;          /* END(LEAVE)                    */ 00131000
         B     #BCM0008                                         02S0343 00132000
*         END;                      /* AND EXIT LOGON PROCESSOR 02S0344 00133000
*                                                            @D32CKMP*/ 00134000
*       ICEPTR=REQCE;               /* PICK UP PTR TO ICE            */ 00135000
@RF00340 LR    ICEPTR,REQCE                                     02S0345 00136000
*       ICEUECBX=REQUECB;           /* PUT ADDR OF STRGE FOR UECB IN    00137000
*                                      ICE                           */ 00138000
         ST    REQUECB,ICEUECBX(,ICEPTR)                        02S0346 00139000
*       IF CSPRSC2F='1'B THEN       /* IF REQUEST UNIT APPL NAME...  */ 00140000
         TM    CSPRSC2F(CSPPTR),B'00000010'                     02S0347 00141000
         BNO   @RF00347                                         02S0347 00142000
*         DO;                       /* ...PRESENT IN THE CSP LIST    */ 00143000
*           ICERSC2B=CSPRSC2B;      /* COPY RU APPL NAME TO ICE      */ 00144000
         MVC   ICERSC2B(8,ICEPTR),CSPRSC2B(CSPPTR)              02S0349 00145000
*           ICERSC2F='1'B;          /* INDICATE RU APPL NAME IN ICE  */ 00146000
         OI    ICERSC2F(ICEPTR),B'00010000'                     02S0350 00147000
*         END;                                                  02S0351 00148000
*       ICEDATA=CSLDATA;            /* PLACE LOGON DATA PTR IN ICE      00149000
*                                                            @D32CKMP*/ 00150000
@RF00347 L     @14,CSPRSCE(,CSPPTR)                             02S0352 00151000
         MVC   ICEDATA(4,ICEPTR),CSLDATA(@14)                   02S0352 00152000
*       TERMPTR=CSLRDTE;            /* POINT TO TRM RDTE     @D32CKMP*/ 00153000
         L     TERMPTR,CSLRDTE(,@14)                            02S0353 00154000
*       RAPPTR=CSPDEST;             /* POINT TO DEST RDTE            */ 00155000
         L     RAPPTR,CSPDEST(,CSPPTR)                          02S0354 00156000
*       ENTRYREG=LOG0PTR;           /* POINT TO LOGON0 ROUTN         */ 00157000
         L     @07,ATCCSMA(,ATCVTPTR)                           02S0355 00158000
         L     ENTRYREG,LOG0PTR+88(,@07)                        02S0355 00159000
*       CALL MXTRN;                 /* CALL LOGON0                   */ 00160000
*                                                               02S0356 00161000
         BALR  @14,@15                                          02S0356 00162000
*       /*************************************************************/ 00163000
*       /*                                                           */ 00164000
*       /* POST ANY RPHS THAT NEED POSTING                           */ 00165000
*       /*                                                           */ 00166000
*       /*************************************************************/ 00167000
*                                                               02S0357 00168000
*       ENTRYREG=POSTPTR;           /* POINT TO POST ROUTINE         */ 00169000
         L     @07,ATCCSMA(,ATCVTPTR)                           02S0357 00170000
         L     ENTRYREG,POSTPTR+56(,@07)                        02S0357 00171000
*       CALL MXTRN;                 /* POST THE RPHS                 */ 00172000
         BALR  @14,@15                                          02S0358 00173000
*     END;                          /* END OF LOGON1 LOGIC SEGMENT      00174000
*                                                            @D32CKMP*/ 00175000
*     RFY                                                       01S0360 00176000
*      (REQCE,                                                  01S0360 00177000
*       REQUECB) UNRSTD;                                        01S0360 00178000
*   END;                            /* END(MAIN)                     */ 00179000
*#BCM0008:                                                      01S0362 00180000
*   ;                               /* FINIS OF MAIN LOGOC   @D32CKMP*/ 00181000
#BCM0008 DS    0H                                               01S0363 00182000
*   DO;                             /* SETCRR FNID(CNSLGIN)TYPE(EXIT)*/ 00183000
*     CRACRR->CNSTFLDB=''B;         /* CLEAR CURRENT TRACE FIELD     */ 00184000
         L     @14,RPHCRR(,RPHPTR)                              01S0364 00185000
         L     @14,CRACRR(,@14)                                 01S0364 00186000
         XC    CNSTFLDB(3,@14),CNSTFLDB(@14)                    01S0364 00187000
*   END;                            /* SETCRR                        */ 00188000
*   RFY                                                         01S0366 00189000
*     RETPTR RSTD;                                              01S0366 00190000
*   RETPTR=SAVERET1;                /* RESTORE RETURN POINTER        */ 00191000
         L     RETPTR,SAVERET1+124(,SAVEPTR)                    01S0367 00192000
*   RFY                                                         01S0368 00193000
*     RETPTR UNRSTD;                                            01S0368 00194000
*                                                               01S0368 00195000
*/* TPEPILOG                                                         */ 00196000
*                                                               01S0369 00197000
*   GEN(PRINT OFF);                                             01S0369 00198000
         PRINT OFF                                                      00199000
