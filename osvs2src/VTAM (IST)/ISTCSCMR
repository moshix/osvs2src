*           WK2=RPHPTR;              /*SAVE RPH POINTER              */ 00291000
         ST    RPHPTR,WK2+124(,SAVEPTR)                         03S0519 00292000
*           RPHCSPA=CSPRSCE;         /* SAVE ORIGINAL CSPRSCE DURING    00293000
*                                       CALL TO AUTH RTN     @ZA10947*/ 00294000
         L     @14,CSPRSCE(,CSPPTR)                             03S0520 00295000
