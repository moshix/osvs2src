*           PARMPTR=CSPPTR;          /* REG 1 EQ PARM LIST   @ZA10947*/ 00297000
         LR    PARMPTR,CSPPTR                                   03S0521 00298000
*           IF CSPRSC='1'B           /* GROUP LIST           @ZA10947*/ 00299000
*             THEN                   /* YES                  @ZA10947*/ 00300000
         TM    CSPRSC(CSPPTR),B'10000000'                       03S0522 00301000
         BNO   @RF00522                                         03S0522 00302000
*           CSPRSCE=CSPRSCE+(CSPNO*LENGTH(ISTCSL));             03S0523 00303000
