*     CRACRR->CNSDORLB='1'B;        /* SET CURRENT FLAG              */ 01230000
         OI    CNSDORLB(@14),B'00000010'                        01S0918 01231000
*   END;                            /* SETCRR                        */ 01232000
*   IF RCPICEAQ^=ADDR(RCPICEAQ)     /* IF ICES ON RESOURCE           */ 01233000
*     THEN                          /* ACQUIRE QUEUE                 */ 01234000
