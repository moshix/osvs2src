SVTM     TITLE 'ISTCSCRL - CONNECTION SERVICES RELEASE ROUTINE         *00001000
                        '                                               00002000
         ISEQ  73,80                                                    00003000
ISTCSCRL CSECT ,                                                01S0002 00004000
@MAINENT DS    0H                                               01S0002 00005000
         USING *,@15                                            01S0002 00006000
         B     @PROLOG                                          01S0002 00007000
         DC    AL1(16)                                          01S0002 00008000
         DC    C'ISTCSCRL  78.037'                              01S0002 00009000
         DROP  @15                                                      00010000
@PROLOG  BALR  @12,0                                            01S0002 00011000
@PSTART  DS    0H                                               01S0002 00012000
         USING @PSTART,@12                                      01S0002 00013000
          ENTRY DOREL,ACQDONE                                           00014000
         PRINT ON                                                       00015000
*   SAVERET1=RETPTR;                /* SAVE RETURN ADDRESS           */ 00016000
         ST    RETPTR,SAVERET1(,SAVEPTR)                        01S0564 00017000
*   RFY                                                         01S0565 00018000
*     RETPTR UNRSTD;                                            01S0565 00019000
*   DO;                             /* SETCRR                   01S0566 00020000
*                                      FNID(CNSRLIN)TYPE(ENTRY)      */ 00021000
*     CRACRR->CNSTFLDB=''B;         /* CLEAR CURRENT TRACE FIELD     */ 00022000
         L     @14,RPHCRR(,RPHPTR)                              01S0567 00023000
         L     @14,CRACRR(,@14)                                 01S0567 00024000
         XC    CNSTFLDB(3,@14),CNSTFLDB(@14)                    01S0567 00025000
*     CRACRR->CNSRLINA='1'B;        /* SET HISTORY FLAG              */ 00026000
         OI    CNSRLINA(@14),B'00000100'                        01S0568 00027000
*     CRACRR->CNSRLINB='1'B;        /* SET CURRENT FLAG              */ 00028000
         OI    CNSRLINB(@14),B'00000100'                        01S0569 00029000
*   END;                            /* SETCRR                        */ 00030000
*   FREECES=0;                      /* INITIALISE FREE CE CHAIN      */ 00031000
         SLR   @14,@14                                          01S0571 00032000
         ST    @14,FREECES(,SAVEPTR)                            01S0571 00033000
*   CSPPTR=RPHCSPA;                 /* SET UP POINTER TO PARM LIST   */ 00034000
         L     CSPPTR,RPHCSPA(,RPHPTR)                          01S0572 00035000
*   RAPPTR=CSPSRC;                  /* SET UP APPLICATION RDTE PTR   */ 00036000
         L     RAPPTR,CSPSRC(,CSPPTR)                           01S0573 00037000
*   TERMPTR=CSLRDTE;                /* SETUP TRML RDTE PTR   @D32CKMP*/ 00038000
         L     @07,CSPRSCE(,CSPPTR)                             01S0574 00039000
         L     TERMPTR,CSLRDTE(,@07)                            01S0574 00040000
*   CEPTR=0;                        /* INITIALISE CEPTR TO OBTAIN CE    00041000
*                                      OFFSET                        */ 00042000
*                                                               01S0575 00043000
         LR    CEPTR,@14                                        01S0575 00044000
*   /*****************************************************************/ 00045000
*   /*                                                               */ 00046000
*   /* SEE IF REQUEST IS FOR REALLOCATION ONLY. IF SO, DO IT         */ 00047000
*   /*                                                               */ 00048000
*   /*****************************************************************/ 00049000
*                                                               01S0576 00050000
*   IF CSPNOREL=BITON THEN                                      01S0576 00051000
         TM    CSPNOREL(CSPPTR),B'00100000'                     01S0576 00052000
         BNO   @RF00576                                         01S0576 00053000
*     DO;                           /* IF REALLOCATION ONLY          */ 00054000
*                                                               02S0578 00055000
*       /*************************************************************/ 00056000
*       /*                                                           */ 00057000
*       /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO HANDLE A REQUEST */ 00058000
*       /* FOR REALLOCATION WITHOUT RELEASE. IF THE TERMINAL IS      */ 00059000
*       /* CONNECTED OR LOGGED ON TO AN APPLICATION, THE REQUESTOR IS*/ 00060000
*       /* INFORMED THAT THE REALLOCATION DID NOT OCCUR BECAUSE THE  */ 00061000
*       /* RESOURCE WAS NOT FREE. OTHERWISE, THIS FUNCTION IS INVOKED*/ 00062000
*       /* TO REALLOCATE THE RESOURCE SPECIFIED.                     */ 00063000
*       /*                                                           */ 00064000
*       /*************************************************************/ 00065000
*                                                               02S0578 00066000
*NOREL:                                                         02S0578 00067000
*       DO;                         /* BGN OF NOREL LOGIC SEGMENT       00068000
*                                                            @D32CKMP*/ 00069000
NOREL    DS    0H                                               02S0579 00070000
*         IF RCPACETR^=ADDR(RCPACETR)|/* IF CONNECTED        @DL29HMP*/ 00071000
*             RCPICELN^=ADDR(RCPICELN)/* OR LOGGED ON        @DL29HMP*/ 00072000
*           THEN                    /* TELL REQUESTOR        @DL29HMP*/ 00073000
         LA    @14,RCPACETR(,TERMPTR)                           02S0579 00074000
         C     @14,RCPACETR(,TERMPTR)                           02S0579 00075000
         BNE   @RT00579                                         02S0579 00076000
         LA    @14,RCPICELN(,TERMPTR)                           02S0579 00077000
         C     @14,RCPICELN(,TERMPTR)                           02S0579 00078000
         BE    @RF00579                                         02S0579 00079000
@RT00579 DS    0H                                               02S0580 00080000
*           CSPCC=NOTFREE;          /* FAILED - NOT FREE     @DL29HMP*/ 00081000
         MVC   CSPCC(2,CSPPTR),@CH03865                         02S0580 00082000
*         ELSE                      /*                       @DL29HMP*/ 00083000
*           CALL DOREL;             /* REALLOCATE RSC        @DL29HMP*/ 00084000
         B     @RC00579                                         02S0581 00085000
@RF00579 BAL   @14,DOREL                                        02S0581 00086000
*       END NOREL;                  /* END OF NOREL LOGIC SEGMENT       00087000
*                                                            @D32CKMP*/ 00088000
@RC00579 DS    0H                                               02S0583 00089000
*     END;                                                      01S0583 00090000
*   ELSE                                                        01S0584 00091000
*     DO;                           /* NORMAL RELEASE                */ 00092000
*                                                               01S0584 00093000
         B     @RC00576                                         01S0584 00094000
@RF00576 DS    0H                                               01S0585 00095000
*       /*************************************************************/ 00096000
*       /*                                                           */ 00097000
*       /* USE SEARCHQ COMMON SUBROUTINE TO LOOK FOR ACE CONNECTING  */ 00098000
*       /* PARTIES (I.E.CONNECTING THE REQUESTOR AND THE TERMINAL    */ 00099000
*       /* SPECIFIED.                                                */ 00100000
*       /*                                                           */ 00101000
*       /*************************************************************/ 00102000
*                                                               01S0585 00103000
*       RPHSAVE1=ADDR(RCPACETR);    /* CHAIN IS THE ACES QUEUED ON      00104000
*                                      THE TERMINAL                  */ 00105000
         LA    @14,RCPACETR(,TERMPTR)                           01S0585 00106000
         ST    @14,RPHSAVE1(,RPHPTR)                            01S0585 00107000
*       RPHSAVE2=ADDR(ACEOBJQ);     /* LINK FIELD IS ACEOBJQ         */ 00108000
         LA    @14,ACEOBJQ(,ACEPTR)                             01S0586 00109000
         ST    @14,RPHSAVE2(,RPHPTR)                            01S0586 00110000
*       RPHSAVE3=ADDR(ACESUBJ);     /* COMPARE FIELD IS ACESUBJ      */ 00111000
         ST    ACEPTR,RPHSAVE3(,RPHPTR)                         01S0587 00112000
*       RPHSAVE4=RAPPTR;            /* COMPARE VALUE IS RAPPTR       */ 00113000
         ST    RAPPTR,RPHSAVE4(,RPHPTR)                         01S0588 00114000
*       ENTRYREG=SRCHQPTR;          /* POINT AT SEARCHQ ROUTINE      */ 00115000
         L     @07,ATCCSMA(,ATCVTPTR)                           01S0589 00116000
         L     ENTRYREG,SRCHQPTR+64(,@07)                       01S0589 00117000
*       CALL MXTRN;                 /* CALL SEARCHQ ROUTINE          */ 00118000
         BALR  @14,@15                                          01S0590 00119000
*       IF RETVAL^=NOTINQ THEN                                  01S0591 00120000
         LTR   RETVAL,RETVAL                                    01S0591 00121000
         BZ    @RF00591                                         01S0591 00122000
*         DO;                       /* IF ACE IS PRESENT             */ 00123000
*                                                               02S0593 00124000
*           /*********************************************************/ 00125000
*           /*                                                       */ 00126000
*           /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO DISCONNECT   */ 00127000
*           /* THE ACE REALLOCATE THE TERMINAL RDT ENTRY, IF         */ 00128000
*           /* NECESSARY, BECAUSE THE RELEASER WAS IN SESSION WITH   */ 00129000
*           /* THE TERMINAL.                                         */ 00130000
*           /*                                                       */ 00131000
*           /*********************************************************/ 00132000
*                                                               02S0593 00133000
*TRUEREL:                                                       02S0593 00134000
*           DO;                     /* BGN OF TRUEREL LOGIC SEGMENT     00135000
*                                                            @D32CKMP*/ 00136000
TRUEREL  DS    0H                                               02S0594 00137000
*             DO;                   /* SETCRR                   02S0594 00138000
*                                      FNID(CNSTRRL)TYPE(SENTRY)     */ 00139000
*               CRACRR->CNSTRRLA='1'B;/* SET HISTORY FLAG            */ 00140000
         L     @14,RPHCRR(,RPHPTR)                              02S0595 00141000
         L     @14,CRACRR(,@14)                                 02S0595 00142000
         OI    CNSTRRLA(@14),B'00000001'                        02S0595 00143000
*               CRACRR->CNSTRRLB='1'B;/* SET CURRENT FLAG            */ 00144000
         OI    CNSTRRLB(@14),B'00000001'                        02S0596 00145000
*             END;                  /* SETCRR                        */ 00146000
*             ACEPTR=RETVAL->ADDRESS;/* SET UP PTR TO ACE            */ 00147000
         L     ACEPTR,ADDRESS(,RETVAL)                          02S0598 00148000
*             RETVAL->ADDRESS=ACEOBJQ;/* DEQ ACE FROM TRMNL          */ 00149000
*                                                               02S0599 00150000
         MVC   ADDRESS(4,RETVAL),ACEOBJQ(ACEPTR)                02S0599 00151000
*             /*******************************************************/ 00152000
*             /*                                                     */ 00153000
*             /* FIND LINK PRECEDING ACE IN APPLS RAPACEAP QUEUE     */ 00154000
*             /*                                                     */ 00155000
*             /*******************************************************/ 00156000
*                                                               02S0600 00157000
*             RETVAL=ADDR(RAPACEAP);/* START AT TOP OF Q             */ 00158000
         LA    RETVAL,RAPACEAP(,RAPPTR)                         02S0600 00159000
*             DO WHILE(RETVAL->ADDRESS^=ACEPTR);/* ACE IS KNOWN TO BE   00160000
*                                      THERE                         */ 00161000
         B     @DE00601                                         02S0601 00162000
@DL00601 DS    0H                                               02S0602 00163000
*               RETVAL=ADDR(RETVAL->ADDRESS->ACESUBJQ);/* MOVE TO       00164000
*                                      NEXT ACE                      */ 00165000
         L     @14,ADDRESS(,RETVAL)                             02S0602 00166000
         LA    @14,ACESUBJQ(,@14)                               02S0602 00167000
         LR    RETVAL,@14                                       02S0602 00168000
*             END;                                              02S0603 00169000
@DE00601 C     ACEPTR,ADDRESS(,RETVAL)                          02S0603 00170000
         BNE   @DL00601                                         02S0603 00171000
*             RETVAL->ADDRESS=ACESUBJQ;/* DEQ ACE FROM RAPACEAQ Q    */ 00172000
         MVC   ADDRESS(4,RETVAL),ACESUBJQ(ACEPTR)               02S0604 00173000
*             RAPACEQD=RAPACEQD-1;  /* DECREMENT CT OF ACES QD ON    */ 00174000
*                                                               02S0605 00175000
         LH    @14,RAPACEQD(,RAPPTR)                            02S0605 00176000
         BCTR  @14,0                                            02S0605 00177000
         STH   @14,RAPACEQD(,RAPPTR)                            02S0605 00178000
*             /*******************************************************/ 00179000
*             /*                                                     */ 00180000
*             /* CALL ACCOUNTING EXIT FOR BROKEN SESSION             */ 00181000
*             /*                                                     */ 00182000
*             /*******************************************************/ 00183000
*                                                               02S0606 00184000
*             IF ATCACCTA^=0 THEN   /* IF ACCT EXIT SUPPLIED         */ 00185000
         ICM   @14,15,ATCACCTA(ATCVTPTR)                        02S0606 00186000
         BZ    @RF00606                                         02S0606 00187000
*               DO;                                             02S0607 00188000
*                 TRMPTR=-TRMPTR;   /* COMPLEMENTED FOR BREAK        */ 00189000
         LCR   TRMPTR,TRMPTR                                    02S0608 00190000
*                 ENTRYREG=ACCTPTR; /* POINT TO DOACCT ROUTN         */ 00191000
         L     @07,ATCCSMA(,ATCVTPTR)                           02S0609 00192000
         L     ENTRYREG,ACCTPTR+100(,@07)                       02S0609 00193000
*                 CALL MXTRN;                                   02S0610 00194000
         BALR  @14,@15                                          02S0610 00195000
*               END;                                            02S0611 00196000
*                                                               02S0611 00197000
*             /*******************************************************/ 00198000
*             /*                                                     */ 00199000
*             /* ADD RELEASED ACE TO FREE ELEMENT CHAIN              */ 00200000
*             /*                                                     */ 00201000
*             /*******************************************************/ 00202000
*                                                               02S0612 00203000
*             ACEPTR->ICESIDE=FREECES;/* CHAIN CE ON TO FREE CHAIN   */ 00204000
@RF00606 MVC   ICESIDE(4,ACEPTR),FREECES(SAVEPTR)               02S0612 00205000
*             FREECES=ACEPTR;       /* COMPLETE FREE CE CHAIN        */ 00206000
         ST    ACEPTR,FREECES(,SAVEPTR)                         02S0613 00207000
*             CSPRLRPH=ENDON;       /* PREPARE TO POST ALL RPHS 02S0614 00208000
*                                      EXCEPT THAT ACCOCIATED WITH      00209000
*                                      HALF CLOSED TERMINAL          */ 00210000
         MVC   CSPRLRPH(4,CSPPTR),@CF00045                      02S0614 00211000
*             CALL DOREL;           /* REALLOCATE TRMNL              */ 00212000
         BAL   @14,DOREL                                        02S0615 00213000
*             DO;                   /* SETCRR                   02S0616 00214000
*                                      FNID(CNSTRRL)TYPE(SEXIT)      */ 00215000
*               CRACRR->CNSTRRLB='0'B;/* TURN OFF CURRENT FLAG       */ 00216000
         L     @14,RPHCRR(,RPHPTR)                              02S0617 00217000
         L     @14,CRACRR(,@14)                                 02S0617 00218000
         NI    CNSTRRLB(@14),B'11111110'                        02S0617 00219000
*             END;                  /* SETCRR                        */ 00220000
*           END TRUEREL;            /* END OF TRUEREL LOGIC SEGMENT     00221000
*                                                            @D32CKMP*/ 00222000
*         END;                                                  01S0620 00223000
*       ELSE                                                    01S0621 00224000
*         DO;                       /* IF ACE IS NOT PRESENT THEN       00225000
*                                      LOOK FOR ACQUIRE ICE QUEUED ON   00226000
*                                      TERMINAL                      */ 00227000
         B     @RC00591                                         01S0621 00228000
@RF00591 DS    0H                                               01S0622 00229000
*           RPHSAVE1=ADDR(RCPICEAQ);/* CHAIN IS THE ACQUIRE ICES        00230000
*                                      QUEUED ON TERMINAL            */ 00231000
         LA    @14,RCPICEAQ(,TERMPTR)                           01S0622 00232000
         ST    @14,RPHSAVE1(,RPHPTR)                            01S0622 00233000
*           RPHSAVE2=ADDR(ICEOBJQ); /* LINK FIELD IS ICEOBJQ         */ 00234000
         LA    @14,ICEOBJQ(,ICEPTR)                             01S0623 00235000
         ST    @14,RPHSAVE2(,RPHPTR)                            01S0623 00236000
*           RPHSAVE3=ADDR(ICESUBJ); /* COMPARE FIELD=CIESUBJ         */ 00237000
         ST    ICEPTR,RPHSAVE3(,RPHPTR)                         01S0624 00238000
*           ENTRYREG=SRCHQPTR;      /* POINT TO SEARCHQ ROUTINE      */ 00239000
         L     @07,ATCCSMA(,ATCVTPTR)                           01S0625 00240000
         L     ENTRYREG,SRCHQPTR+64(,@07)                       01S0625 00241000
*           CALL MXTRN;             /* CALL SEARCHQ                  */ 00242000
         BALR  @14,@15                                          01S0626 00243000
*           IF RETVAL^=NOTINQ THEN                              01S0627 00244000
         LTR   RETVAL,RETVAL                                    01S0627 00245000
         BZ    @RF00627                                         01S0627 00246000
*             DO;                   /* IF ICE IS PRESENT             */ 00247000
*                                                               02S0629 00248000
*               /*****************************************************/ 00249000
*               /*                                                   */ 00250000
*               /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO          */ 00251000
*               /* DISCONNECT THE TERMINAL ASSOCIATED WITH THE       */ 00252000
*               /* RELEASER VIA AN ACQUIRE ICE AND SEE HOW THIS      */ 00253000
*               /* AFFECTS THE TERMINAL AND THE QUEUED REQUESTS.     */ 00254000
*               /*                                                   */ 00255000
*               /*****************************************************/ 00256000
*                                                               02S0629 00257000
*REQREL:                                                        02S0629 00258000
*               DO;                 /* BGN OF REQREL LOGIC SEGMENT      00259000
*                                                            @D32CKMP*/ 00260000
REQREL   DS    0H                                               02S0630 00261000
*                 DO;               /* SETCRR                   02S0630 00262000
*                                      FNID(CNSRQRL)TYPE(SENTRY)     */ 00263000
*                   CRACRR->CNSRQRLA='1'B;/* SET HISTORY FLAG        */ 00264000
         L     @14,RPHCRR(,RPHPTR)                              02S0631 00265000
         L     @14,CRACRR(,@14)                                 02S0631 00266000
         OI    CNSRQRLA(@14),B'10000000'                        02S0631 00267000
*                   CRACRR->CNSRQRLB='1'B;/* SET CURRENT FLAG        */ 00268000
         OI    CNSRQRLB(@14),B'10000000'                        02S0632 00269000
*                 END;              /* SETCRR                        */ 00270000
*                 ICEPTR=RETVAL->ADDRESS;/* POINT TO ASSOCIATING ICE */ 00271000
         L     ICEPTR,ADDRESS(,RETVAL)                          02S0634 00272000
*                 RETVAL->ADDRESS=ICEOBJQ;/* DEQUEUE ICE FROM   02S0635 00273000
*                                      TERMINAL                      */ 00274000
         MVC   ADDRESS(4,RETVAL),ICEOBJQ(ICEPTR)                02S0635 00275000
*                 IF ICESTAPR=BITON /* IF ICE SHOULDER TAP FLAG IS      00276000
*                                      SET                           */ 00277000
*                   THEN            /* ACQUIRER IS A SHOULDER TAPPER,   00278000
*                                      SO                            */ 00279000
         TM    ICESTAPR(ICEPTR),B'00001000'                     02S0636 00280000
         BNO   @RF00636                                         02S0636 00281000
*                   RPRSTPCT=RPRSTPCT-1;/* DECREMENT COUNT OF   02S0637 00282000
*                                      SHOULDER TAPPERS              */ 00283000
*                                                               02S0637 00284000
         LH    @14,RPRSTPCT(,TERMPTR)                           02S0637 00285000
         BCTR  @14,0                                            02S0637 00286000
         STH   @14,RPRSTPCT(,TERMPTR)                           02S0637 00287000
*                 /***************************************************/ 00288000
*                 /*                                                 */ 00289000
*                 /* ...IN THE RESOURCE RDTE                         */ 00290000
*                 /*                                                 */ 00291000
*                 /***************************************************/ 00292000
*                                                               02S0638 00293000
*                 AVAILFLG=BITOF;   /* INITIALISE TO TERM NOT   02S0638 00294000
*                                      AVAILABLE                     */ 00295000
@RF00636 NI    AVAILFLG(SAVEPTR),B'01111111'                    02S0638 00296000
*                 IF(RCPICEAQ=ICEOBJQ)/* IF ICE WAS AT TOP OF QUEUE  */ 00297000
*                     &(RPRLIVE=BITON)/* ...AND TERMINAL WAS LIVE    */ 00298000
*                     &(RCPICELN=ADDR(RCPICELN)) THEN/* & TERM NOT IN   00299000
*                                      LOGON STATE                   */ 00300000
         CLC   RCPICEAQ(4,TERMPTR),ICEOBJQ(ICEPTR)              02S0639 00301000
         BNE   @RF00639                                         02S0639 00302000
         TM    RPRLIVE(TERMPTR),B'00000001'                     02S0639 00303000
         BNO   @RF00639                                         02S0639 00304000
         LA    @14,RCPICELN(,TERMPTR)                           02S0639 00305000
         C     @14,RCPICELN(,TERMPTR)                           02S0639 00306000
         BNE   @RF00639                                         02S0639 00307000
*                   IF RCPACETR=ADDR(RCPACETR) THEN/* IF NO ACES        00308000
*                                      QUEUED                        */ 00309000
         LA    @14,RCPACETR(,TERMPTR)                           02S0640 00310000
         C     @14,RCPACETR(,TERMPTR)                           02S0640 00311000
         BNE   @RF00640                                         02S0640 00312000
*                     AVAILFLG=BITON;/* TERMINAL WAS AVAILABLE       */ 00313000
         OI    AVAILFLG(SAVEPTR),B'10000000'                    02S0641 00314000
*                   ELSE            /* ACES QUEUED                   */ 00315000
*                     DO;                                       02S0642 00316000
         B     @RC00640                                         02S0642 00317000
@RF00640 DS    0H                                               02S0643 00318000
*                       RETVAL=RCPACETR;/* FIRST ACE                 */ 00319000
         L     RETVAL,RCPACETR(,TERMPTR)                        02S0643 00320000
*                       DO WHILE(RETVAL->ACEOBJQ^=ADDR(RCPACETR));/*    00321000
*                                      SEARCH FOR END OF CHAIN       */ 00322000
         B     @DE00644                                         02S0644 00323000
@DL00644 DS    0H                                               02S0645 00324000
*                         RETVAL=RETVAL->ACEOBJQ;               02S0645 00325000
         L     RETVAL,ACEOBJQ(,RETVAL)                          02S0645 00326000
*                       END;                                    02S0646 00327000
@DE00644 LA    @14,RCPACETR(,TERMPTR)                           02S0646 00328000
         C     @14,ACEOBJQ(,RETVAL)                             02S0646 00329000
         BNE   @DL00644                                         02S0646 00330000
*                     END;                                      02S0647 00331000
*                                                               02S0647 00332000
*                 /***************************************************/ 00333000
*                 /*                                                 */ 00334000
*                 /* DEQUEUE ICE FROM APPLICATION RAPICEAQ CHAIN     */ 00335000
*                 /*                                                 */ 00336000
*                 /***************************************************/ 00337000
*                                                               02S0648 00338000
*                 RETVAL=ADDR(RAPICEAQ);/* POINT TO FIRST LINK       */ 00339000
@RC00640 DS    0H                                               02S0648 00340000
@RF00639 LA    RETVAL,RAPICEAQ(,RAPPTR)                         02S0648 00341000
*                 DO WHILE(RETVAL->ADDRESS^=ICEPTR);/* SEARCH FOR END   00342000
*                                      OF CHAIN                      */ 00343000
         B     @DE00649                                         02S0649 00344000
@DL00649 DS    0H                                               02S0650 00345000
*                   RETVAL=ADDR(RETVAL->ADDRESS->ICESUBJQ);     02S0650 00346000
         L     @14,ADDRESS(,RETVAL)                             02S0650 00347000
         LA    @14,ICESUBJQ(,@14)                               02S0650 00348000
         LR    RETVAL,@14                                       02S0650 00349000
*                 END;                                          02S0651 00350000
@DE00649 C     ICEPTR,ADDRESS(,RETVAL)                          02S0651 00351000
         BNE   @DL00649                                         02S0651 00352000
*                 RETVAL->ADDRESS=ICESUBJQ;/* DEQUEUE ICE FROM APPL  */ 00353000
         MVC   ADDRESS(4,RETVAL),ICESUBJQ(ICEPTR)               02S0652 00354000
*                                                               03S0653 00355000
*                 /***************************************************/ 00356000
*                 /*                                                 */ 00357000
*                 /* THE FUNCTION OF THIS LOGIC SEGMENT IS TO DEAL   */ 00358000
*                 /* WITH THE TERMINAL RDT ENTRY AND THE REMAINDER OF*/ 00359000
*                 /* A QUEUED REQUEST AFTER THE ICE HAS BEEN DEQUEUED*/ 00360000
*                 /* FROM THE TERMINAL AND APPLICATION RDT ENTRIES.  */ 00361000
*                 /*                                                 */ 00362000
*                 /***************************************************/ 00363000
*                                                               03S0653 00364000
*REQREL1:                                                       03S0653 00365000
*                 DO;               /* BGN OF REQREL1 LOGIC SEGMENT     00366000
*                                                            @D32CKMP*/ 00367000
REQREL1  DS    0H                                               03S0654 00368000
*                   RFY                                         03S0654 00369000
*                     NEXTICE RSTD;                             03S0654 00370000
*                   ICXPTR=0;       /* INITIALISE PTR TO ICX         */ 00371000
         SLR   @14,@14                                          03S0655 00372000
         LR    ICXPTR,@14                                       03S0655 00373000
*                   IF ICESIDE=ICEPTR THEN                      03S0656 00374000
         C     ICEPTR,ICESIDE(,ICEPTR)                          03S0656 00375000
         BNE   @RF00656                                         03S0656 00376000
*                     DO;           /* IF NO OTHER ICES IN REQUEST   */ 00377000
*                       NEXTICE=0;  /* NO OTHER ICE                  */ 00378000
         LR    NEXTICE,@14                                      03S0658 00379000
*                       IF ICELGON='0'B THEN                    03S0659 00380000
         TM    ICELGON(ICEPTR),B'00100000'                      03S0659 00381000
         BNZ   @RF00659                                         03S0659 00382000
*                         DO;       /* FOR ACQUIRE                   */ 00383000
*                           ICERPHA->RPHCSPA->CSPCC=ENDRQD;     03S0661 00384000
*                                                               03S0661 00385000
         L     @14,ICERPHA(,ICEPTR)                             03S0661 00386000
         L     @07,RPHCSPA(,@14)                                03S0661 00387000
         MVC   CSPCC(2,@07),@CH03860                            03S0661 00388000
