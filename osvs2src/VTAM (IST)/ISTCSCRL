*             DO;                   /*                       @DM09471*/ 01336000
*               RPRACT=BITOF;       /* TURN OFF ACT FALG     @DM09471*/ 01337000
*               RPRTIED=BITOF;      /* TUNR OFF TIED FLAG    @DM09471*/ 01338000
