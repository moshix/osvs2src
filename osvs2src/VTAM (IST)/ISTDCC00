*  CRRFDB2='67'X;                       /*  FAILS                   */  00258000
         MVI   CRRFDB2(PTRCRR),X'67'                               0539 00259000
*  END;                                                            0540 00260000
*ELSE IF CRRSW5 = ON THEN                /* TPEXIT SWITCH           */  00261000
*                                                                  0541 00262000
