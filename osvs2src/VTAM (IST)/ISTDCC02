         TITLE 'ISTDCC02 - REQUEST PROCESSOR FINISH ROUTINE            *00001000
                        '                                               00002000
ISTDCC02 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC02  78.173'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         TITLE 'ISTDCC02 - REQUEST PROCESSOR FINISH ROUTINE - MAIN'     00013000
*   CRRREG=ADDR(CRRSPACE);          /* ESTABLISH COVER               */ 00014000
         L     CRRREG,RPHCRR(,RPHREG)                              0396 00015000
         LA    CRRREG,CRRSPACE+312(,CRRREG)                        0396 00016000
*   RESPECIFY                                                      0397 00017000
*     CRRSAREA BASED(CRRREG);       /* FOR THE CRR                   */ 00018000
*   RFY                                                            0398 00019000
*     ISTPFCRR BASED(ADDR(CRAPROCR));/*                      @ZA05260*/ 00020000
*   CRRC02=ON;                      /* SET AUDIT TRAIL               */ 00021000
         OI    CRRC02(CRRREG),B'00100000'                          0399 00022000
*   CRRMAS=CRRMAS&&CRRMAS;          /* FOR REQ. FINISH               */ 00023000
         XC    CRRMAS(8,CRRREG),CRRMAS(CRRREG)                     0400 00024000
*   CRRM02=ON;                      /* SET MOD AUDIT                 */ 00025000
         OI    CRRM02(CRRREG),B'00100000'                          0401 00026000
*   CRRSW0=OFF;                     /* CLEAR SOLICIT ALL SW  @D8356NL*/ 00027000
*   CRRSW2=OFF;                     /* RESET 'START I/O SW           */ 00028000
         NI    CRRSW0(CRRREG),B'01011111'                          0403 00029000
*   FMCBREG=CRRFMCB;                /* GET FMCB ADDRESS              */ 00030000
         L     FMCBREG,CRRFMCB(,CRRREG)                            0404 00031000
*   ACBDEBRG=RPHMAJCB;              /* GET DEB ADDRESS       @DM12444*/ 00032000
         L     ACBDEBRG,RPHMAJCB-1(,RPHREG)                        0405 00033000
         LA    ACBDEBRG,0(,ACBDEBRG)                               0405 00034000
*   RFY                                                            0406 00035000
*     ISTFMCB BASED(FMCBREG);       /* SET UP ADDRESSABILITY         */ 00036000
*   RFY                                                            0407 00037000
*     ISTACDEB BASED(ACBDEBRG);                                    0407 00038000
*   IF ACDTYPE=ACDID THEN                                          0408 00039000
         CLI   ACDTYPE(ACBDEBRG),X'0F'                             0408 00040000
         BE    @RT00408                                            0408 00041000
*     ;                             /* IF A DEB-OK           @DM12444*/ 00042000
*   ELSE                                                           0410 00043000
*     ACBDEBRG=FMCDEBA;             /* GET DEB FROM FMCB     @DM12444*/ 00044000
         L     ACBDEBRG,FMCDEBA(,FMCBREG)                          0410 00045000
*   IF CRRRTNCD^=ZERO&              /* IS ERROR CODE SET             */ 00046000
*       (CRRRTNCD^='30'X&           /* NOT EXECPTIONAL REQUEST       */ 00047000
*       CRRRTNCD^='34'X) THEN       /* NOT EXCEPTIONAL RESPONSE      */ 00048000
@RT00408 CLI   CRRRTNCD(CRRREG),0                                  0411 00049000
         BE    @RF00411                                            0411 00050000
         CLI   CRRRTNCD(CRRREG),X'30'                              0411 00051000
         BE    @RF00411                                            0411 00052000
         CLI   CRRRTNCD(CRRREG),X'34'                              0411 00053000
         BE    @RF00411                                            0411 00054000
*                                                                  0412 00055000
*/********************************************************************/ 00056000
*/*                                                                  */ 00057000
*/*  SEGMENT NAME : ERRPROC                                          */ 00058000
*/*                                                                  */ 00059000
*/*  FUNCTION: 'ERRPROC' MOVES ERROR STATUS FROM THE CRR TO THE RPL. */ 00060000
*/*        A CALL TO 'ERRINVAL' IS MADE TO RESET FMCB FLAG BITS.     */ 00061000
*/*        'FREELCPB' IS CALLED TO RELEASE LCPB STORAGE AND 'POSTIT' */ 00062000
*/*        IS CALLED TO POST THE USER'S RPL COMPLETE.                */ 00063000
*/*                                                                  */ 00064000
*/*  INPUT : FMCBREG (REG 9) - POINTS TO FMCB                        */ 00065000
*/*                                                                  */ 00066000
*/*  OUTPUT : SAME                                                   */ 00067000
*/*                                                                  */ 00068000
*/*  SUBROUTINES CALLED :                                            */ 00069000
*/*                       FREELCPB - FREE LCCW AREAS                 */ 00070000
*/*                       POSTIT   - POST RPL STATUS                 */ 00071000
*/*                       HSKPRANY - UPDATE ACDEB READ ANY QUEUE     */ 00072000
*/*                                                                  */ 00073000
*/*  MACROS USED : TPUNLOCK                                          */ 00074000
*/*                                                                  */ 00075000
*/********************************************************************/ 00076000
*                                                                  0412 00077000
*     DO;                                                          0412 00078000
*       IF RPHMAJCB->FMCTYPE=FMTYPE&/* MAJOR NODE CB AN FMCB         */ 00079000
*           FMCMODE^=FMCRCD THEN    /* AND NOT RECORD DEVICE         */ 00080000
         L     @10,RPHMAJCB-1(,RPHREG)                             0413 00081000
         LA    @10,0(,@10)                                         0413 00082000
         CLI   FMCTYPE(@10),X'03'                                  0413 00083000
         BNE   @RF00413                                            0413 00084000
         CLC   FMCMODE(8,FMCBREG),@CC03434                         0413 00085000
         BE    @RF00413                                            0413 00086000
*         DO;                                                      0414 00087000
*           IF CRRFDB2>CTLAYER|     /* TOO HIGH FOR CL ERROR         */ 00088000
*               CRRFDB2=CRRRTRUN THEN/* OR TRUNCATION INDICATOR      */ 00089000
         CLI   CRRFDB2(CRRREG),X'7F'                               0415 00090000
         BH    @RT00415                                            0415 00091000
         CLI   CRRFDB2(CRRREG),48                                  0415 00092000
         BNE   @RF00415                                            0415 00093000
@RT00415 DS    0H                                                  0416 00094000
*             CALL HSKPRANY;        /* UPDATE READ ANY QUEUE         */ 00095000
         BAL   @14,HSKPRANY                                        0416 00096000
*           ELSE                                                   0417 00097000
*             ;                     /* CONTROL LAYER ERROR           */ 00098000
@RF00415 DS    0H                                                  0418 00099000
*                                                                  0418 00100000
*/********************************************************************/ 00101000
*/*                                                                  */ 00102000
*/*  SEGMENT NAME : ERRINVAL                                         */ 00103000
*/*                                                                  */ 00104000
*/* FUNCTION: 'ERRINVAL' RESETS THE FMCB BIT SETTINGS FROM THE CRR IN*/ 00105000
*/*        CASE THERE IS AN ERROR IN AN I/O OPERATION.               */ 00106000
*/*                                                                  */ 00107000
*/* INPUT : FMCBREG (REG 9) - POINTS TO THE FMCB                     */ 00108000
*/*                                                                  */ 00109000
*/* OUTPUT : SAME                                                    */ 00110000
*/*                                                                  */ 00111000
*/* SUBROUTINES CALLED : NONE                                        */ 00112000
*/*                                                                  */ 00113000
*/* MACROS USED :  NONE                                              */ 00114000
*/*                                                                  */ 00115000
*/********************************************************************/ 00116000
*                                                                  0418 00117000
*           FMCBREG=CRRFMCB;        /* ESTABLISH COVER               */ 00118000
         L     FMCBREG,CRRFMCB(,CRRREG)                            0418 00119000
*           RESPECIFY                                              0419 00120000
*             ISTFMCB BASED(FMCBREG);/* FOR THE FMCB                 */ 00121000
*           IF CRRDEXP=ON           /* IS DATA EXPECTED              */ 00122000
*             THEN                                                 0420 00123000
         TM    CRRDEXP(CRRREG),B'10000000'                         0420 00124000
         BNO   @RF00420                                            0420 00125000
*             DO;                                                  0421 00126000
*               RFY                                                0422 00127000
*                (REG4,                                            0422 00128000
*                 REG10) RSTD;                                     0422 00129000
*C02A:                                                             0423 00130000
*               REG4=FMCCMPS2;      /* OLD VALUE                     */ 00131000
C02A     L     REG4,FMCCMPS2(,FMCBREG)                             0423 00132000
*               REG10=REG4&FLGA0OFF;/* SET DATA EXPECTED OFF         */ 00133000
         LR    REG10,REG4                                          0424 00134000
         N     REG10,@CF01211                                      0424 00135000
*               CS(REG4,REG10,FMCCMPS2);                           0425 00136000
         CS    REG4,@10,FMCCMPS2(FMCBREG)                          0425 00137000
*               BC(7,C02A);         /* RETRY IF IT FAILS             */ 00138000
         BC    7,C02A                                              0426 00139000
*             END;                                                 0427 00140000
*C02B:                                                             0428 00141000
*           REG4=FMCCMPS2;          /* GET OLD VALUE                 */ 00142000
@RF00420 DS    0H                                                  0428 00143000
C02B     L     REG4,FMCCMPS2(,FMCBREG)                             0428 00144000
*           IF CRRSTSPR=ON THEN     /* SATISFY SPECIFIC READ MODE -     00145000
*                                      CRR & FMCB SHOULD MATCH     0429 00146000
*                                      YES-SET FLAG ON               */ 00147000
         TM    CRRSTSPR(CRRREG),B'00100000'                        0429 00148000
         BNO   @RF00429                                            0429 00149000
*             REG10=REG4|FLGA1ON;   /* SET SAT SPEC READ ON          */ 00150000
         LR    REG10,REG4                                          0430 00151000
         O     REG10,@CF01219                                      0430 00152000
*           ELSE                                                   0431 00153000
*             REG10=REG4&FLGA1OFF;  /* TURN OFF SPEC READ            */ 00154000
         B     @RC00429                                            0431 00155000
@RF00429 LR    REG10,REG4                                          0431 00156000
         N     REG10,@CF01217                                      0431 00157000
*           CS(REG4,REG10,FMCCMPS2);                               0432 00158000
@RC00429 CS    REG4,@10,FMCCMPS2(FMCBREG)                          0432 00159000
*           BC(7,C02B);                                            0433 00160000
         BC    7,C02B                                              0433 00161000
*           RFY                                                    0434 00162000
*            (REG4,                                                0434 00163000
*             REG10) UNRSTD;                                       0434 00164000
*           IF CRRLCPB->DCLRQTP=DCLDO/* IS THIS A 'DO'               */ 00165000
*             THEN                                                 0435 00166000
         L     @10,CRRLCPB(,CRRREG)                                0435 00167000
         CLI   DCLRQTP(@10),X'13'                                  0435 00168000
         BNE   @RF00435                                            0435 00169000
*             RPLAAREA=CRRCLDO;     /* YES-SET LOC OF BAD LDO IN RPL */ 00170000
         L     @10,CRRARPL(,CRRREG)                                0436 00171000
         L     @04,CRRCLDO(,CRRREG)                                0436 00172000
         ST    @04,RPLAAREA(,@10)                                  0436 00173000
*           ELSE                                                   0437 00174000
*             ;                     /* NOT A DO MACRO                */ 00175000
@RF00435 DS    0H                                                  0438 00176000
*         END;                      /* END-MAJOR NODE CB IS FMCB     */ 00177000
*       ELSE                                                       0439 00178000
*         ;                         /* NOT AN FMCB                   */ 00179000
@RF00413 DS    0H                                                  0440 00180000
*       CALL FREELCPB;              /* RELEASE LCPB STG              */ 00181000
         BAL   @14,FREELCPB                                        0440 00182000
*       FMCBREG=CRRFMCB;            /* FMCB FOR POSTIT               */ 00183000
         L     FMCBREG,CRRFMCB(,CRRREG)                            0441 00184000
*       CALL POSTIT;                /* POST USER'S ECB               */ 00185000
         BAL   @14,POSTIT                                          0442 00186000
*     END;                          /* END ERRPROC                   */ 00187000
*   ELSE                                                           0444 00188000
*     DO;                           /* NO-                           */ 00189000
         B     @RC00411                                            0444 00190000
@RF00411 DS    0H                                                  0445 00191000
*       IF CRRDIRPT^=ON THEN        /* POST=SCHED SEND       @ZA32152*/ 00192000
         TM    CRRDIRPT(CRRREG),B'01000000'                        0445 00193000
         BO    @RF00445                                            0445 00194000
*         IF(RPLREQ='22'X|          /* SEND REQUEST                  */ 00195000
*             RPLREQ='23'X|         /* RECIEVE REQUEST               */ 00196000
*             RPLREQ='24'X) THEN    /* RESETSR REQUEST               */ 00197000
         L     @10,CRRARPL(,CRRREG)                                0446 00198000
         CLI   RPLREQ(@10),34                                      0446 00199000
         BE    @RT00446                                            0446 00200000
         CLI   RPLREQ(@10),35                                      0446 00201000
         BE    @RT00446                                            0446 00202000
         CLI   RPLREQ(@10),36                                      0446 00203000
         BNE   @RF00446                                            0446 00204000
@RT00446 DS    0H                                                  0447 00205000
*                                                                  0447 00206000
*/********************************************************************/ 00207000
*/*                                                                  */ 00208000
*/* SEGMENT NAME : SNAPROC - PROCESS NEW DEVICE REQUESTS             */ 00209000
*/*                                                                  */ 00210000
*/* FUNCTION :                                                       */ 00211000
*/*   SNAPROC IS CALLED TO PROCESS NEW DEVICE REQUESTS.  FOR         */ 00212000
*/*   RECIEVE OPERATIONS THE RPL IS QUEUED ON A WAIT QUEUE OR IS     */ 00213000
*/* POSTED COMPLETE DEPENDING UPON THE OPTION SPECIFIED BY THE USER. */ 00214000
*/* IF THE OPERATION IS AN ALL/ANY OPERATION THE ACBDEB WFAIT QUEUE  */ 00215000
*/* IS USED.  OTHERWISE, THE FMCB WAIT QUEUE IS USED.                */ 00216000
*/*                                                                  */ 00217000
*/* INPUT :  FMCBREG (REG 9) - POINTS TO THE FMCB                    */ 00218000
*/*                                                                  */ 00219000
*/* OUTPUT : SAME                                                    */ 00220000
*/*                                                                  */ 00221000
*/* SUBROUTINES CALLED : POSTIT   - POST RPL STATUS                  */ 00222000
*/*                                                                  */ 00223000
*/* MACROS USED : TPUNLOCK, VMDESET, TPQUE                           */ 00224000
*/*                                                                  */ 00225000
*/********************************************************************/ 00226000
*                                                                  0447 00227000
*           DO;                                                    0447 00228000
*             IF RPLREQ='22'X       /* SEND REQUEST                  */ 00229000
*               THEN                                               0448 00230000
         L     @10,CRRARPL(,CRRREG)                                0448 00231000
         CLI   RPLREQ(@10),34                                      0448 00232000
         BE    @RT00448                                            0448 00233000
*               ;                   /* NO PROCESSING NECESSARY       */ 00234000
*             ELSE                                                 0450 00235000
*               DO;                 /* A RECEIVE             @D31KKNL*/ 00236000
*                 IF CRRRPLS=ON THEN/* RPL SATISFIED         @DM9061K*/ 00237000
         TM    CRRRPLS(CRRREG),B'10000000'                         0451 00238000
         BNO   @RF00451                                            0451 00239000
*                   CALL POSTIT;    /* POST COMPLETE         @DM9061K*/ 00240000
         BAL   @14,POSTIT                                          0452 00241000
*                 ELSE              /* RPL NOT SATISFIED@D31KKNL     */ 00242000
*                   IF RPLQOPT=OFF THEN/* NQ OPNDST OPTION           */ 00243000
         B     @RC00451                                            0453 00244000
@RF00451 L     @10,CRRARPL(,CRRREG)                                0453 00245000
         TM    RPLQOPT(@10),B'00010000'                            0453 00246000
         BNZ   @RF00453                                            0453 00247000
*                     DO;                                          0454 00248000
*                       CRRRTNCD=CRRNOIN;/* NO INPUT         @D31KKNL*/ 00249000
         MVI   CRRRTNCD(CRRREG),X'38'                              0455 00250000
*                       CALL POSTIT;/* POST THIS RPL                 */ 00251000
         BAL   @14,POSTIT                                          0456 00252000
*                     END;          /*                       @D31KKNL*/ 00253000
*                   ELSE                                           0458 00254000
*                     DO;           /* QUEUE RPL IF NOT QUE'D        */ 00255000
         B     @RC00453                                            0458 00256000
@RF00453 DS    0H                                                  0459 00257000
*                       IF CRRRPLOQ=ON/* IF ALREADY ON A QUE         */ 00258000
*                         THEN                                     0459 00259000
         TM    CRRRPLOQ(CRRREG),B'01000000'                        0459 00260000
         BO    @RT00459                                            0459 00261000
*                         ;         /* BYPASS QUEUEING               */ 00262000
*                       ELSE                                       0461 00263000
*                         DO;       /* QUEUE RPL ON A QUEUE          */ 00264000
*                           WORKREG=CRRARPL;/* WORK ELEMENT  @D31KKNL*/ 00265000
         L     WORKREG,CRRARPL(,CRRREG)                            0462 00266000
*                           IF RPHMAJCB->ACDTYPE=ACDID THEN        0463 00267000
         L     @10,RPHMAJCB-1(,RPHREG)                             0463 00268000
         LA    @10,0(,@10)                                         0463 00269000
         CLI   ACDTYPE(@10),X'0F'                                  0463 00270000
         BNE   @RF00463                                            0463 00271000
*                             DO;   /* MAJOR NODE CB IS A DEB        */ 00272000
*                               ACBDEBRG=RPHMAJCB;/* ADDRESS DEB   0465 00273000
*                                                            @D31KKNL*/ 00274000
         LR    ACBDEBRG,@10                                        0465 00275000
*                               CALL UNLOCK;/* UNLOCK FMCB   @D31KKNL*/ 00276000
         BAL   @14,UNLOCK                                          0466 00277000
*                                                                  0467 00278000
*/********************************************************************/ 00279000
*/*                                                                  */ 00280000
*/* SEGMENT NAME :   LKDEB  -  LOCK  THE ACDEB                       */ 00281000
*/*                                                                  */ 00282000
*/* FUNCTION :  TO ENSURE THE ACDEB IS LOCKED                        */ 00283000
*/*                                                                  */ 00284000
*/* INPUT :  ACBDEBRG (REG 7) - POINTS TO DEB                        */ 00285000
*/*                                                                  */ 00286000
*/* OUTPUT : CRRDBLCK (='1'B) - THE ACDEB IS   LOCKED                */ 00287000
*/*                                                                  */ 00288000
*/* SUBROUTINES CALLED :  NONE                                       */ 00289000
*/*                                                                  */ 00290000
*/* MACROS USED :  VMDESET, TPLOCK                                   */ 00291000
*/*                                                                  */ 00292000
*/********************************************************************/ 00293000
*                                                                  0467 00294000
*                               IF CRRDBLCK^=ON THEN/* DEB LOCKED  0467 00295000
*                                                            @D31KKNL*/ 00296000
         TM    CRRDBLCK(CRRREG),B'00010000'                        0467 00297000
         BO    @RF00467                                            0467 00298000
*                                 DO;/* UNLOCK IT            @D31KKNL*/ 00299000
*                                                                  0469 00300000
*/*VMDESET  ((REG15),ZERO)                                           */ 00301000
*                                                                  0469 00302000
*                                   DO;/* INVOKE MODESET MACRO       */ 00303000
*                                    GEN;                          0470 00304000
         MODESET EXTKEY=ZERO,WORKREG=2                                  00305000
*                                    REG15=0;/* PUT RETURN CODE IN 0471 00306000
*                                      RCREG                         */ 00307000
         SLR   REG15,REG15                                         0471 00308000
*                                   END;/* VMDESET TO KEY ZERO     0472 00309000
*                                                            @D31KKNL*/ 00310000
*/*** START -TPLOCK (DEBLOCK)TYPE(XCLSIVE)- **                       */ 00311000
*                                                                  0473 00312000
*                                   DO;                            0473 00313000
*                                    RFY                           0474 00314000
*                                     (GPR00P,                     0474 00315000
*                                      GPR01P,                     0474 00316000
*                                      GPR02P,                     0474 00317000
*                                      GPR03P,                     0474 00318000
*                                      GPR04P,                     0474 00319000
*                                      GPR15P) RSTD;               0474 00320000
*                                    GENERATE REFS(GPR00P,GPR01P,GPR15P 00321000
*                                        ,RPHWORK) SETS(RPHWORK);  0475 00322000
         STM GPR00P,GPR14P,RPHWORK(GPR01P)                              00323000
*                                    GPR02P=ADDR(ACDLOCK);/* SET   0476 00324000
*                                      PARAM - LOCKWORD PTR          */ 00325000
         LA    GPR02P,ACDLOCK(,ACBDEBRG)                           0476 00326000
*                                    GPR04P=GPR01P->RPHCRR;/* SET  0477 00327000
*                                      PARAM - ISTCRA PTR            */ 00328000
         L     GPR04P,RPHCRR(,GPR01P)                              0477 00329000
*                                    GPR00P=CRALMS05;/* SET PARAM -     00330000
*                                      LOCK LEVEL FLAG               */ 00331000
         LA    GPR00P,16                                           0478 00332000
*                                    GPR03P=ADDR(GPR04P->CRAL5PTR);/*   00333000
*                                      SET PARAM - LOCK LEVEL PTR    */ 00334000
         LA    GPR03P,CRAL5PTR(,GPR04P)                            0479 00335000
*                                    GPR15P=ATCLCPTR->ATCAP36;/*   0480 00336000
*                                      ADDRESS OF LOCK ROUTINE       */ 00337000
         L     @10,ATCLCPTR                                        0480 00338000
         L     GPR15P,ATCAP36(,@10)                                0480 00339000
*                                    DCL                           0481 00340000
*                                      FMZL0017 BASED(GPR15P) LABEL     00341000
*                                          VALUERANGE(*);          0481 00342000
*                                    CALL FMZL0017;                0482 00343000
         BALR  @14,@15                                             0482 00344000
*                                    GENERATE REFS(GPR00P,GPR01P,GPR14P 00345000
*                                        ,RPHWORK) SETS(GPR00P,GPR01P,  00346000
*                                        GPR02P,GPR03P,GPR04P,GPR05P,   00347000
*                                        GPR06P,GPR07P,GPR08P,GPR09P,   00348000
*                                        GPR10P,GPR11P,GPR12P,GPR13P,   00349000
*                                        GPR14P,GPR15P);           0483 00350000
         LM GPR00P,GPR15P,RPHWORK(GPR01P)                               00351000
*                                    RFY                           0484 00352000
*                                     (GPR00P,                     0484 00353000
*                                      GPR01P,                     0484 00354000
*                                      GPR02P,                     0484 00355000
*                                      GPR03P,                     0484 00356000
*                                      GPR04P,                     0484 00357000
*                                      GPR15P) UNRSTD;             0484 00358000
*                                   END;                           0485 00359000
*                                                                  0485 00360000
*/*** END -TPLOCK (DEBLOCK)TYPE(XCLSIVE)- **                         */ 00361000
*                                                                  0486 00362000
*                                   /* UNLOCK THE DEB        @D31KKNL*/ 00363000
*                                   CRRDBLCK=ON;/* DEB IS UNLOCKED 0486 00364000
*                                                            @D31KKNL*/ 00365000
         OI    CRRDBLCK(CRRREG),B'00010000'                        0486 00366000
*/*VMDESET  ((REG15),VTAM)                                           */ 00367000
*                                                                  0487 00368000
*                                   DO;/* INVOKE MODESET MACRO       */ 00369000
*                                    GEN;                          0488 00370000
         MODESET EXTKEY=VTAM,WORKREG=2                                  00371000
*                                    REG15=0;/* PUT RETURN CODE IN 0489 00372000
*                                      RCREG                         */ 00373000
         SLR   REG15,REG15                                         0489 00374000
*                                   END;/* VMDESET TO VTAM KEY     0490 00375000
*                                                            @D31KKNL*/ 00376000
*                                 END;/* END DEB LOCKED      @D31KKNL*/ 00377000
*                               ELSE                               0492 00378000
*                                 ; /* DEB NOT LOCKED        @D31KKNL*/ 00379000
@RF00467 DS    0H                                                  0493 00380000
*                               WORKREG=CRRARPL;/* WORK ELEMENT    0493 00381000
*                                                            @D31KKNL*/ 00382000
         L     WORKREG,CRRARPL(,CRRREG)                            0493 00383000
*                               QUEREG=ADDR(ACDRARQ);/* QUE ANCHOR 0494 00384000
*                                                            @D31KKNL*/ 00385000
         LA    QUEREG,ACDRARQ(,ACBDEBRG)                           0494 00386000
*                               CALL QUEUP;/* QUE RPL TO DEB @D31KKNL*/ 00387000
         BAL   @14,QUEUP                                           0495 00388000
*                             END;  /* END- MAJOR CB IS A DEB        */ 00389000
*                           ELSE                                   0497 00390000
*                             DO;   /* MAJOR CB-FMCB         @D31KKNL*/ 00391000
         B     @RC00463                                            0497 00392000
@RF00463 DS    0H                                                  0498 00393000
*                               QUEREG=ADDR(FMCRPLA);/* FMCB WAIT Q     00394000
*                                                            @D31KKNL*/ 00395000
         LA    QUEREG,FMCRPLA(,FMCBREG)                            0498 00396000
*                               CALL QUEUP;/* Q RPL TO FMCB  @D31KKNL*/ 00397000
         BAL   @14,QUEUP                                           0499 00398000
*                             END;  /* END-MAJOR CB FMCB     @D31KKNL*/ 00399000
*                         END;      /* END-RPL NOT ON A QUEUE        */ 00400000
*                     END;          /* END- NQ IS NOT SPECIFIED      */ 00401000
*               END;                /* END-CRRRPLS NOT ON@D31KKNL    */ 00402000
*           END;                                                   0504 00403000
*         ELSE                                                     0505 00404000
*           DO;                     /* NOT RECORD MODE               */ 00405000
         B     @RC00446                                            0505 00406000
@RF00446 DS    0H                                                  0506 00407000
*             CALL HSKPRANY;        /* HOUSEKEEP ACB DEB READ ANY Q  */ 00408000
         BAL   @14,HSKPRANY                                        0506 00409000
