*   END POSTIT;                     /* END-USER POST RTN             */ 01515000
*                                                                  0852 01516000
*/********************************************************************/ 01517000
*/*                                                                  */ 01518000
*/* ROUTINE NAME : STARTNXT - FIND NEXT OPERATION TO BE STARTED      */ 01519000
*/*                                                                  */ 01520000
*/* FUNCTION :                                                       */ 01521000
*/*    STARTNXT TAKES AN RPL FROM THE FMCB WAIT QUEUE ANT TRIES TO   */ 01522000
*/*    FIND DATA ON A DATA QUEUE THAT MATCHES THE RPL DATA TYPE. IF  */ 01523000
*/*    A MATCH IS FOUND THE ROUTINE EXITS WITH THE REDRIVE SWITCH    */ 01524000
*/*    (CRRSW2) ON. IF NO MATCH IS FOUND STARTNXT LOOKS ON THE       */ 01525000
*/*    ACB DEB FOR AN RPL TO MATCH WITH AN FMCB OF THE SAME DATA TYPE*/ 01526000
*/*                                                                  */ 01527000
*/* INPUT :  FMCBREG (REG 9) - POINTS TO THE FMCB                    */ 01528000
*/*                                                                  */ 01529000
*/* OUTPUT : CRRSW2 - ON IF NEW OPERATION IS TO BE STARTED           */ 01530000
*/*                                                                  */ 01531000
*/* SUBROUTINES CALLED : NONE                                        */ 01532000
*/*                                                                  */ 01533000
*/*                                                                  */ 01534000
*/* MACROS USED : NONE                                               */ 01535000
*/*                                                                  */ 01536000
*/********************************************************************/ 01537000
*                                                                  0852 01538000
*STARTNXT:                                                         0852 01539000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);                     0852 01540000
STARTNXT DS    0H                                                  0853 01541000
*   CRRRTN11=REG14;                 /* SAVE RTN ADDRESS      @D31KKNL*/ 01542000
         ST    REG14,CRRRTN11(,CRRREG)                             0853 01543000
*   IF CRRFMCB^=ZERO THEN           /* FMCB EXISTS           @D31KKNL*/ 01544000
         L     @14,CRRFMCB(,CRRREG)                                0854 01545000
         LTR   @14,@14                                             0854 01546000
         BZ    @RF00854                                            0854 01547000
*     DO;                           /* SEARCH IT             @D31KKNL*/ 01548000
*       PABPTR=ADDR(FMCPAB1);       /* POINT TO CLOP         @D31KKNL*/ 01549000
         LA    PABPTR,FMCPAB1(,FMCBREG)                            0856 01550000
*       RFY                                                        0857 01551000
*         RPLPTR RSTD;                                             0857 01552000
*       RFY                                                        0858 01553000
*         IFGRPL BASED(RPLPTR);     /* REBASE RPL            @D31KKNL*/ 01554000
*       RPLPTR=FMCRPLA;             /* GET FIRST RPL         @D31KKNL*/ 01555000
         L     RPLPTR,FMCRPLA(,FMCBREG)                            0859 01556000
*       DO WHILE(RPLPTR^=ZERO&RPLPTR^=GATEBIT)&CRRSW2=OFF;/* IF RPLS    01557000
*                                      EXIST                 @DM13825*/ 01558000
*                                                                  0860 01559000
         B     @DE00860                                            0860 01560000
@DL00860 DS    0H                                                  0861 01561000
*         /***********************************************************/ 01562000
*         /*                                                         */ 01563000
*         /* CHECK THE ASYNCHRONOUS, RESPONSE, ANT SYNCHRONOUS DATA  */ 01564000
*         /* QUEUES AND IF ANY MATCHING DATA IS PRESENT SET THE      */ 01565000
*         /* REDRIVE SWITCH TO REDRIVE CONTROL LAYER.                */ 01566000
*         /*                                                         */ 01567000
*         /***********************************************************/ 01568000
*                                                                  0861 01569000
*         IF(RPLDFASY=ON&FMCFBAS^=ZERO&FMCFBAS^=GATEBIT)|(RPLRRESP=ON&  01570000
*             FMCFBRE^=ZERO&FMCFBRE^=GATEBIT)|(RPLNFSYN=OFF&FMCFBA^=    01571000
*             ZERO&FMCFBA^=GATEBIT) THEN/* THERE IS DATA     @DM13825*/ 01572000
         TM    RPLDFASY+16(RPLPTR),B'00000010'                     0861 01573000
         BNO   @GL00018                                            0861 01574000
         L     @14,FMCFBAS(,FMCBREG)                               0861 01575000
         LTR   @14,@14                                             0861 01576000
         BZ    @GL00018                                            0861 01577000
         CL    @14,@CF01213                                        0861 01578000
         BNE   @RT00861                                            0861 01579000
@GL00018 TM    RPLRRESP+16(RPLPTR),B'00001000'                     0861 01580000
         BNO   @GL00017                                            0861 01581000
         L     @14,FMCFBRE(,FMCBREG)                               0861 01582000
         LTR   @14,@14                                             0861 01583000
         BZ    @GL00017                                            0861 01584000
         CL    @14,@CF01213                                        0861 01585000
         BNE   @RT00861                                            0861 01586000
