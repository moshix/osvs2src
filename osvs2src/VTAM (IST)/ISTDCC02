         ST    REG14,CRRRTN7(,CRRREG)                              0798 01359000
*   RESPECIFY                                                      0799 01360000
*     REG14 UNRESTRICTED;           /* RELEASE REG 14                */ 01361000
*   IF(RPLREQ='22'X|                /* SEND REQUEST                  */ 01362000
*       RPLREQ='23'X|               /* RECIEVE REQUEST               */ 01363000
*       RPLREQ='24'X) THEN          /* RESETSR REQUEST               */ 01364000
         L     @14,CRRARPL(,CRRREG)                                0800 01365000
