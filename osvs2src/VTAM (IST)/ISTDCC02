         BE    @RT00737                                            0737 01165000
*     ;                             /* NO PROCESSING IF NO LCPB * *     01166000
*                                      OR SOLICIT ALL REQUEST        */ 01167000
*   ELSE                                                           0739 01168000
*     DO;                           /* EXTERNAL CALL TO HANDLE Q     */ 01169000
*       CRRDFSYN=ON;                /* INDICATE NON-SNA              */ 01170000
         OI    CRRDFSYN(CRRREG),B'00000010'                        0740 01171000
*       RFY                                                        0741 01172000
*         ISTATCVT BASED(CRRATCVT);                                0741 01173000
*       CALL ATCRCC63;              /* CALL HSPRANYQ                 */ 01174000
         L     @14,CRRATCVT(,CRRREG)                               0742 01175000
         L     @15,ATCRCC63(,@14)                                  0742 01176000
