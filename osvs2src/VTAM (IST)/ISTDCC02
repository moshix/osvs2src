*                   END;                                           0630 00818000
*               END;                /*                               */ 00819000
*             ELSE                                                 0632 00820000
*                                                                  0632 00821000
*/********************************************************************/ 00822000
*/*                                                                  */ 00823000
*/* SEGMENT NAME : NOIO  -  NO I/O PROCESSOR                         */ 00824000
*/*                                                                  */ 00825000
*/* FUNCTION: 'NOIO' CLEANS UP WHEN THE I/O REQUESTS CAN BE SATISFIED*/ 00826000
*/*        FROM PREVIOUS REQUESTS, E.G. FILLING A READ REQUEST FROM A*/ 00827000
*/*        PRIOR SOLICIT.                                            */ 00828000
*/*           IF PREVIOUS I/O WAS STARTED AND THIS CURRENT OPERATION */ 00829000
*/*        IS A READ-ANY, THE RPL IS QUEUED ON THE ACB DEB WAIT QUEUE.  00830000
*                                                                    */ 00831000
*/*        IF PREVIOUS I/O WAS SCHEDULED AND THE REQUEST WAS NOT A   */ 00832000
*/*        READ-ANY, THE RPL IS QUEUED ON THE FMCB WAIT QUEUE.  IN   */ 00833000
*/*        BOTH CASES, 'FREELCPB' IS CALLED TO RELEASE STORAGE FOR   */ 00834000
*/*        THE LCPB/LCCW BLOCKS BUILT.                               */ 00835000
*/*           IF NO PREVIOUS I/O WAS SCHEDULED, THE LCPB/LCCW BLOCKS */ 00836000
*/*        ARE FREED VIA A CALL TO 'FREELCPB' AND IF THIS IS NOT A   */ 00837000
*/*        SUBBLOCK OF AN OPERATION, THE USER IS POSTED COMPLETE VIA */ 00838000
*/*        'POSTIT'.                                                 */ 00839000
*/*                                                                  */ 00840000
*/* INPUT :  FMCBREG (REG 9) - POINTS TO THE FMCB                    */ 00841000
*/*                                                                  */ 00842000
*/* OUTPUT : SAME                                                    */ 00843000
*/*                                                                  */ 00844000
*/* SUBROUTINES CALLED : FREELCPB - FREE LCCW AREAS                  */ 00845000
*/*                      POSTIT   - POST RPL STATUS                  */ 00846000
*/*                                                                  */ 00847000
*/* MACROS USED : TPUNLOCK, VMDESET, TPQUE                           */ 00848000
*/*                                                                  */ 00849000
*/********************************************************************/ 00850000
*                                                                  0632 00851000
*               DO;                                                0632 00852000
         B     @RC00507                                            0632 00853000
@RF00507 DS    0H                                                  0633 00854000
*                 RFY                                              0633 00855000
*                  (WORKREG,                                       0633 00856000
*                   QUEREG) RSTD;   /* RESTRICT Q'ING REGS   @D31KKNL*/ 00857000
*                 IF CRRPREIO=ON    /* AWAITING-PREV-I/O?            */ 00858000
*                   THEN                                           0634 00859000
         TM    CRRPREIO(CRRREG),B'00010000'                        0634 00860000
         BNO   @RF00634                                            0634 00861000
*                   DO;             /* YES-                          */ 00862000
*                     IF CRRRPLOQ=ON/* RPL ALREADY QUEUED            */ 00863000
*                       THEN                                       0636 00864000
         TM    CRRRPLOQ(CRRREG),B'01000000'                        0636 00865000
         BO    @RT00636                                            0636 00866000
*                       ;           /* DON'T TRY TO QUEUE IT         */ 00867000
*                     ELSE                                         0638 00868000
*                       DO;         /* QUEUE THIS RPL                */ 00869000
*                         WORKREG=CRRARPL;/* ELT TO BE QUE'D @D31KKNL*/ 00870000
         L     WORKREG,CRRARPL(,CRRREG)                            0639 00871000
*                         IF CRRLCPB=ZERO/* IS REQ A READ-ANY?       */ 00872000
*                           THEN                                   0640 00873000
         L     @00,CRRLCPB(,CRRREG)                                0640 00874000
         LTR   @00,@00                                             0640 00875000
         BNZ   @RF00640                                            0640 00876000
*                           DO;     /* YES-                          */ 00877000
*                             ACBDEBRG=RPHMAJCB;/* ESTABLISH COVER   */ 00878000
         L     ACBDEBRG,RPHMAJCB-1(,RPHREG)                        0642 00879000
         LA    ACBDEBRG,0(,ACBDEBRG)                               0642 00880000
*                             RFY                                  0643 00881000
*                               ISTACDEB BASED(ACBDEBRG);/* THE    0643 00882000
*                                      ACBDEB                        */ 00883000
*/*VMDESET  ((15),ZERO)                                              */ 00884000
*                                                                  0644 00885000
*                             DO;   /* INVOKE MODESET MACRO          */ 00886000
*                               GEN;                               0645 00887000
         MODESET EXTKEY=ZERO,WORKREG=2                                  00888000
*                               RFY                                0646 00889000
*                                (GPR15P) RSTD;                    0646 00890000
*                               GPR15F=0;                          0647 00891000
         SLR   GPR15F,GPR15F                                       0647 00892000
*                               RFY                                0648 00893000
*                                (GPR15P) UNRSTD;/* PUT RETURN CODE     00894000
*                                      IN RCREG                      */ 00895000
*                             END;  /* VMDESET TO KEY 0              */ 00896000
*                             QUEREG=ADDR(ACDRARQ);/* DEB WAIT Q   0650 00897000
*                                                            @D31KKNL*/ 00898000
         LA    QUEREG,ACDRARQ(,ACBDEBRG)                           0650 00899000
*                             CALL QUEUP;/* QUE RPL TO DEB   @D31KKNL*/ 00900000
         BAL   @14,QUEUP                                           0651 00901000
*/*VMDESET  ((15),VTAM)                                              */ 00902000
*                                                                  0652 00903000
*                             DO;   /* INVOKE MODESET MACRO          */ 00904000
*                               GEN;                               0653 00905000
         MODESET EXTKEY=VTAM,WORKREG=2                                  00906000
*                               RFY                                0654 00907000
*                                (GPR15P) RSTD;                    0654 00908000
*                               GPR15F=0;                          0655 00909000
         SLR   GPR15F,GPR15F                                       0655 00910000
*                               RFY                                0656 00911000
*                                (GPR15P) UNRSTD;/* PUT RETURN CODE     00912000
*                                      IN RCREG                      */ 00913000
*                             END;  /* VMDESET TO VTAM KEY           */ 00914000
*                           END;    /*                               */ 00915000
*                         ELSE                                     0659 00916000
*                           DO;     /* NOT READ ANY-                 */ 00917000
         B     @RC00640                                            0659 00918000
@RF00640 DS    0H                                                  0660 00919000
*                             QUEREG=ADDR(FMCRPLA);/* FMCB WAIT Q  0660 00920000
*                                                            @D31KKNL*/ 00921000
         LA    QUEREG,FMCRPLA(,FMCBREG)                            0660 00922000
*                             CALL QUEUP;/* QUE RPL TO FMCB  @D31KKNL*/ 00923000
         BAL   @14,QUEUP                                           0661 00924000
*                             CALL FREELCPB;/* FREE LCPB AREAS       */ 00925000
         BAL   @14,FREELCPB                                        0662 00926000
*                           END;    /*                               */ 00927000
*                       END;                                       0664 00928000
*                   END;            /* END RPL NOT QUE'D             */ 00929000
*                 ELSE                                             0666 00930000
*                   DO;             /* NOT AWAIT-PREV-I/O            */ 00931000
         B     @RC00634                                            0666 00932000
@RF00634 DS    0H                                                  0667 00933000
*                     CALL FREELCPB;/* RELEASE LCPB STG              */ 00934000
         BAL   @14,FREELCPB                                        0667 00935000
*                     IF CRRSUBBK=OFF/* IF I/O OP COMPLETE           */ 00936000
*                       THEN                                       0668 00937000
         TM    CRRSUBBK(CRRREG),B'00000100'                        0668 00938000
         BNZ   @RF00668                                            0668 00939000
*                       CALL POSTIT;/* POST ECB OR USER EXIT ROUTINE */ 00940000
         BAL   @14,POSTIT                                          0669 00941000
*                   END;                                           0670 00942000
@RF00668 DS    0H                                                  0671 00943000
*               END;                /* END NO I/O NEEDED             */ 00944000
@RC00634 DS    0H                                                  0672 00945000
*           END;                    /* END-NON SNA PROCCESING        */ 00946000
@RC00507 DS    0H                                                  0673 00947000
*     END;                          /* NO ERROR PROC'ING             */ 00948000
@RC00446 DS    0H                                                  0673 00949000
@RF00445 DS    0H                                                  0674 00950000
*   CALL UNLOCK;                    /* UNLOCK FMCB           @D31KKNL*/ 00951000
@RC00411 BAL   @14,UNLOCK                                          0674 00952000
*                                                                  0675 00953000
*/********************************************************************/ 00954000
*/*                                                                  */ 00955000
*/* SEGMENT NAME :  UNLKDEB - UNLOCK THE ACDEB                       */ 00956000
*/*                                                                  */ 00957000
*/* FUNCTION :  TO ENSURE THE ACDEB IS UNLOCKED                      */ 00958000
*/*                                                                  */ 00959000
*/* INPUT :  ACBDEBRG (REG 7) - POINTS TO DEB                        */ 00960000
*/*                                                                  */ 00961000
*/* OUTPUT : CRRDBLCK (='0'B) - THE ACDEB IS UNLOCKED                */ 00962000
*/*                                                                  */ 00963000
*/* SUBROUTINES CALLED :  NONE                                       */ 00964000
*/*                                                                  */ 00965000
*/* MACROS USED :  VMDESET, TPUNLOCK                                 */ 00966000
*/*                                                                  */ 00967000
*/********************************************************************/ 00968000
*                                                                  0675 00969000
*   IF CRRDBLCK=ON THEN             /* DEB LOCKED            @D31KKNL*/ 00970000
         TM    CRRDBLCK(CRRREG),B'00010000'                        0675 00971000
         BNO   @RF00675                                            0675 00972000
*     DO;                           /* UNLOCK IT             @D31KKNL*/ 00973000
*       IF CRRFMCB^=ZERO            /* IF FMCB EXISTS @XM50BHNL      */ 00974000
