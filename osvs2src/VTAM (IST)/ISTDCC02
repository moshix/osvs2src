*         REG15=0;                  /* PUT RETURN CODE IN RCREG      */ 00992000
         SLR   REG15,REG15                                         0682 00993000
*       END;                        /* VMDESET TO KEY ZERO   @D31KKNL*/ 00994000
*       CRRDBLCK=OFF;                                              0684 00995000
*                                                                  0684 00996000
         NI    CRRDBLCK(CRRREG),B'11101111'                        0684 00997000
*/*** START -TPUNLOCK (DEBLOCK)- **                                  */ 00998000
*                                                                  0685 00999000
