         TITLE 'ISTDCC10-SOLICIT LDO REQUEST                           *00001000
                        '                                               00002000
ISTDCC10 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC10  75.126'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*                                                            @ZM34568*/ 00013000
*                                                                  0409 00014000
*                                                                  0409 00015000
*                                                                  0410 00016000
*   CRRPTR = ADDR(CRRSPACE);      /* REFERENCE CRR                  */  00017000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0410 00018000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0410 00019000
*   CRRC10 = ON;                  /* SET HISTORY SWITCH             */  00020000
         OI    CRRC10(CRRPTR),B'00010000'                          0411 00021000
*   CRRMAS = CRRMAS&&CRRMAS;      /* CLEAR MODULE AUDIT SWITCHES    */  00022000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0412 00023000
*   CRRM10 = ON;                  /* INITIALIZE MY AUDIT SWITCH     */  00024000
*                                                                  0413 00025000
         OI    CRRM10(CRRPTR),B'00010000'                          0413 00026000
*   FMCBPTR = CRRFMCB;            /* LOCATE FMCB                    */  00027000
*                                                                  0414 00028000
         L     FMCBPTR,CRRFMCB(,CRRPTR)                            0414 00029000
*   /*****************************************************************/ 00030000
*   /* A SOLICIT LDO REQUEST IS VALID ONLY IF THE DEVICE IS NOT AN   */ 00031000
*   /* OUTPUT ONLY DEVICE, AND THE DATA EXPECTED INDICATOR           */ 00032000
*   /* IS NOT SET FOR THIS FMCB INDICATING AN OUTSTANDING SOLICIT    */ 00033000
*   /* OR READ.                                                      */ 00034000
*   /*****************************************************************/ 00035000
*                                                                  0415 00036000
*   IF DEVINPUT=OFF THEN          /* IF NOT AN INPUT DEVICE         */  00037000
         L     @15,FMCDNCBA(,FMCBPTR)                              0415 00038000
         TM    DEVINPUT+36(@15),B'10000000'                        0415 00039000
         BNZ   @RF00415                                            0415 00040000
*         DO;                     /* ERROR - OUTPUT ONLY DEVICE     */  00041000
*         CRRFDB2=CRRSTOOD;         /* SET REASON CODE FOR FEEDBACK */  00042000
         MVI   CRRFDB2(CRRPTR),X'0E'                               0417 00043000
*         CRRRTNCD=CRRLOGIC;        /* SET LOGICAL ERROR CODE         * 00044000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0418 00045000
*         END;                                                     0419 00046000
*        ELSE CALL CPROC2;   /* CONTINUE PROCESSING                  */ 00047000
