*               GPR13P) UNRSTD;                                    0802 01524000
*             RFY                                                  0803 01525000
*              (GPR02P,                                            0803 01526000
*               GPR03P,                                            0803 01527000
*               GPR04P,                                            0803 01528000
*               GPR15P) UNRSTD;                                    0803 01529000
*           END;                    /* *********END OF RELSTORE    0804 01530000
*                                      EXPANSION*********            */ 01531000
*         ELSE                                                     0805 01532000
*                                                                  0805 01533000
*           /*********************************************************/ 01534000
*           /*                                                       */ 01535000
*           /* RELSTORE BUFADDR(WORKA)CHAIN(YES)                     */ 01536000
*           /*                                                       */ 01537000
*           /*********************************************************/ 01538000
*                                                                  0805 01539000
*           DO;                                                    0805 01540000
*                                                                  0805 01541000
         B     @RC00784                                            0805 01542000
@RF00784 DS    0H                                                  0806 01543000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 01544000
*/* END OF ATBUILD                                                   */ 01545000
*                                                                  0806 01546000
*             RFY                                                  0806 01547000
*              (GPR02P,                                            0806 01548000
*               GPR03P,                                            0806 01549000
*               GPR04P,                                            0806 01550000
