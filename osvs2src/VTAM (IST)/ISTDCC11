         BNO   @RF00591                                            0591 00694000
*         DO;                       /* NOT FIRST OPER.       @ZM02162*/ 00695000
*           CRRPREIO=ON;            /* SET AWAITING PREV     @XM10597*/ 00696000
         OI    CRRPREIO(CRRPTR),B'00010000'                        0593 00697000
*           RFY                                                    0594 00698000
*            (REG10,                                               0594 00699000
*             REG11) RSTD;                                         0594 00700000
*           REG10=FMCCMPS2;         /* GET A0 FLAG VALUE     @XM10597*/ 00701000
         L     REG10,FMCCMPS2(,@15)                                0595 00702000
*CS0:                                                              0596 00703000
*           REG11=REG10|FLGA0ON;    /* RESET THE VALUE       @XM10597*/ 00704000
CS0      LR    REG11,REG10                                         0596 00705000
         O     REG11,@CF02549                                      0596 00706000
*           CS(REG10,REG11,FMCCMPS2);/* CHANGE FMCCMPS2      @XM10597*/ 00707000
         L     @15,CRRFMCB(,CRRPTR)                                0597 00708000
         CS    REG10,@11,FMCCMPS2(@15)                             0597 00709000
*           BC(7,CS0);              /* NO CHANGE,RETRY       @XM10597*/ 00710000
