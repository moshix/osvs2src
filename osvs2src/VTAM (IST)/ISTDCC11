         CLI   DEVCHAR2(@12),X'1C'                                 0572 00582000
         BH    @RF00572                                            0572 00583000
*           /*********************************************************/ 00584000
*           /*                                                       */ 00585000
*           /* 3270 COMMAND AND DEVICE TYPE 3270                     */ 00586000
*           /*                                                       */ 00587000
*           /*********************************************************/ 00588000
*                                                                  0573 00589000
*           CALL CSUBR5;            /* CHECK DATA CHAIN AND DIALOGUE */ 00590000
