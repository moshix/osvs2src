         BL    @RF00601                                            0601 00730000
         CLI   DEVTCODE(@12),X'1C'                                 0601 00731000
         BH    @RF00601                                            0601 00732000
*           DO;                     /*                       @XM10338*/ 00733000
*             REG10=FMCCMPS2;       /* GET B7 VALUE          @XM10597*/ 00734000
         L     REG10,FMCCMPS2(,@15)                                0603 00735000
*CSB7:                                                             0604 00736000
*             REG11=REG10|FLGB7ON;  /* TURN ON THE FLAG      @XM10597*/ 00737000
CSB7     LR    REG11,REG10                                         0604 00738000
         O     REG11,@CF03732                                      0604 00739000
*             CS(REG10,REG11,FMCCMPS2);/* CHANGE FMCCMPS2    @XM10597*/ 00740000
         L     @15,CRRFMCB(,CRRPTR)                                0605 00741000
         CS    REG10,@11,FMCCMPS2(@15)                             0605 00742000
*             BC(7,CSB7);           /* FAILED,RETRY          @XM10597*/ 00743000
         BC    7,CSB7                                              0606 00744000
*           END;                    /* END FIRST OPER DO     @XM10597*/ 00745000
*         ELSE                                                     0608 00746000
*           ;                       /*                       @ZM02162*/ 00747000
