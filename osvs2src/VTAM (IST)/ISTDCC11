*               GPR15P=ATCLCPTR;    /* SET UP ATCVT POINTER          */ 01507000
         L     GPR15P,ATCLCPTR                                     0794 01508000
*             END;                  /* LISATCVT                      */ 01509000
*             GPR02P=GPR15P->ATCBPDA;                              0796 01510000
         L     GPR02P,ATCBPDA(,GPR15P)                             0796 01511000
*             GPR15P=GPR15P->ATCSMRS;                              0797 01512000
         L     GPR15P,ATCSMRS(,GPR15P)                             0797 01513000
*             DCL                                                  0798 01514000
*               FMZL0032 BASED(GPR15P) LABEL VALUERANGE(*);        0798 01515000
