         CLI   PROSETM4(@15),X'03'                                 0610 00762000
         BE    @RT00610                                            0610 00763000
         CLI   PROSETM4(@15),X'83'                                 0610 00764000
         BNE   @RF00610                                            0610 00765000
@RT00610 DS    0H                                                  0611 00766000
*           DO;                     /* VALID READ MODIFIER           */ 00767000
*                                                                  0611 00768000
*             /*******************************************************/ 00769000
*             /*                                                     */ 00770000
*             /* BUILD READ LCCW                                     */ 00771000
*             /*                                                     */ 00772000
*             /*******************************************************/ 00773000
*                                                                  0612 00774000
*             IF PROSETM4=PROSBLCK THEN/* READ BLOCK                 */ 00775000
         L     @15,CRRFMCB(,CRRPTR)                                0612 00776000
         CLI   PROSETM4+48(@15),X'01'                              0612 00777000
         BNE   @RF00612                                            0612 00778000
*               LCCWRWOP=LRB;                                      0613 00779000
         L     @15,CRRCLCCW(,CRRPTR)                               0613 00780000
         NI    LCCWRWOP(@15),B'11000110'                           0613 00781000
         OI    LCCWRWOP(@15),B'00000110'                           0613 00782000
*             ELSE                                                 0614 00783000
*               IF PROSETM4=PROSMSG THEN/* READ MESSAGE              */ 00784000
         B     @RC00612                                            0614 00785000
@RF00612 L     @15,CRRFMCB(,CRRPTR)                                0614 00786000
         CLI   PROSETM4+48(@15),X'02'                              0614 00787000
         BNE   @RF00614                                            0614 00788000
*                 LCCWRWOP=LRM;                                    0615 00789000
         L     @15,CRRCLCCW(,CRRPTR)                               0615 00790000
         NI    LCCWRWOP(@15),B'11001010'                           0615 00791000
         OI    LCCWRWOP(@15),B'00001010'                           0615 00792000
*               ELSE                                               0616 00793000
*                 IF PROSETM4=PROSTRNS THEN/* READ TRANS             */ 00794000
         B     @RC00614                                            0616 00795000
@RF00614 L     @15,CRRFMCB(,CRRPTR)                                0616 00796000
         CLI   PROSETM4+48(@15),X'03'                              0616 00797000
         BNE   @RF00616                                            0616 00798000
*                   LCCWRWOP=LRT;                                  0617 00799000
         L     @15,CRRCLCCW(,CRRPTR)                               0617 00800000
