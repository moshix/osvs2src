*               GPR13P) UNRSTD;                                    0822 01585000
*             RFY                                                  0823 01586000
*              (GPR02P,                                            0823 01587000
*               GPR03P,                                            0823 01588000
*               GPR04P,                                            0823 01589000
*               GPR15P) UNRSTD;                                    0823 01590000
*           END;                    /* *********END OF RELSTORE    0824 01591000
*                                      EXPANSION********* NOT      0824 01592000
*                                      CONFIDENTIAL                  */ 01593000
*       END;                        /* DONE WITH CURRENT FSB         */ 01594000
@RC00784 DS    0H                                                  0826 01595000
*     RFY                                                          0826 01596000
*      (FDBPTR,                                                    0826 01597000
*       WORKA,                                                     0826 01598000
*       COUNTER) UNRSTD;                                           0826 01599000
@RC00748 DS    0H                                                  0827 01600000
*     IF CRRSW0=ON THEN             /* MUST MAKE ANOTHER PASS        */ 01601000
         TM    CRRSW0(CRRPTR),B'10000000'                          0827 01602000
         BNO   @RF00827                                            0827 01603000
*       CRRSW7=ON;                  /* SHOW LOOPED MORE THAN ONCE    */ 01604000
         OI    CRRSW7(CRRPTR),B'00000001'                          0828 01605000
*     ELSE                                                         0829 01606000
*       CRRSW1=OFF;                 /* DONE WITH LOOP                */ 01607000
         B     @RC00827                                            0829 01608000
@RF00827 NI    CRRSW1(CRRPTR),B'10111111'                          0829 01609000
*   END;                            /* END OF DO WHILE PROCESSING    */ 01610000
@RC00827 DS    0H                                                  0830 01611000
@DE00725 TM    CRRSW1(CRRPTR),B'01000000'                          0830 01612000
         BO    @DL00725                                            0830 01613000
*   IF CRRELSRI=ON THEN             /* WAS ERROR LOCK SET IN FSB?    */ 01614000
         TM    CRRELSRI(CRRPTR),B'00001000'                        0831 01615000
         BNO   @RF00831                                            0831 01616000
*     IF CRRSW7=ON THEN             /* WERE WE TRUNCATING?           */ 01617000
*                                                                  0832 01618000
         TM    CRRSW7(CRRPTR),B'00000001'                          0832 01619000
         BNO   @RF00832                                            0832 01620000
*       /*************************************************************/ 01621000
*       /*                                                           */ 01622000
*       /* IF THE ERROR LOCK IS SET IN A FSB BEING FLUSHED, CALL     */ 01623000
*       /* LCPBINIT TO INITIALIZE THE LCPB TO A RESET ERROR LOCK     */ 01624000
*       /* SINCE THE USER WILL NOT KNOW THAT THE ERROR LOCK IS SET.  */ 01625000
*       /*                                                           */ 01626000
*       /*************************************************************/ 01627000
*                                                                  0833 01628000
*       CALL LCPBINIT;              /* ISSUE RESET ERROR LOCK        */ 01629000
         BAL   @14,LCPBINIT                                        0833 01630000
*     ELSE                                                         0834 01631000
*       CRRELSRI=OFF;               /* NOT TRUNCATING-USER WILL FIND */ 01632000
         B     @RC00832                                            0834 01633000
@RF00832 NI    CRRELSRI(CRRPTR),B'11110111'                        0834 01634000
*   ELSE                                                           0835 01635000
*     ;                             /* ERROR LOCK NOT SET            */ 01636000
@RF00831 DS    0H                                                  0836 01637000
*   RETPTR=CRRRTN8;                 /* GET RETURN ADDRESS            */ 01638000
@RC00831 L     RETPTR,CRRRTN8(,CRRPTR)                             0836 01639000
*   END FREEFSB;                    /* RETURN TO CALLER              */ 01640000
@EL00009 DS    0H                                                  0837 01641000
@EF00009 DS    0H                                                  0837 01642000
@ER00009 BR    @14                                                 0837 01643000
*UPLDOPTR:                                                         0838 01644000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0838 01645000
UPLDOPTR DS    0H                                                  0839 01646000
*   CRRRTN7=RETPTR;                 /* SAVE RETURN ADDRESS           */ 01647000
         ST    RETPTR,CRRRTN7(,CRRPTR)                             0839 01648000
*/********************************************************************/ 01649000
*/*                                                                  */ 01650000
*/* ROUTINE NAME :   UPLDOP7R - UPDATE LDO POINTER                   */ 01651000
*/*                                                                  */ 01652000
*/* FUNCTION :                                                       */ 01653000
*/*    FOR A NORMAL ENTRY (CRRSW2 NOT SET) UPLDOPTR WILL SEARCH      */ 01654000
*/*    THROUGH THE CHAIN OF LDO'S ASSOCIATED WITH THE READ TO FIND   */ 01655000
*/*    THE LAST DATA CHAINED LDO.  THIS ONE WILL BE TESTED TO SEE    */ 01656000
*/*    IF IT IS COMMAND CHAINED.  IF SO, AN ERROR INDICATION WILL    */ 01657000
*/*    BE SET IN THE CRR, AND AN I/O OPERATION WILL NOT OCCUR.  IF   */ 01658000
*/*    ANY LDO'S ARE FOUND TO BE INVALID, NO I/O WILL OCCUR. IF      */ 01659000
*/*    CRRSW2 IS SET, UPLDOPTR WILL ONLY UPDATE TO THE NEXT NON-JUMP */ 01660000
*/*    LDO.                                                          */ 01661000
*/* INPUT :  LDOPTR (REG 10) - ADDRESS OF LDO                        */ 01662000
*/*                                                                  */ 01663000
*/* OUTPUT :     LDOPTR REG(REG 9) - POINTS TO PROPER LDO            */ 01664000
*/*                                                                  */ 01665000
*/* SUBROUTINES CALLED :  NONE                                       */ 01666000
*/*                                                                  */ 01667000
*/*  MACROS USED :   NONE                                            */ 01668000
*/*                                                                  */ 01669000
*/********************************************************************/ 01670000
*                                                                  0840 01671000
*   LDOPTR=CRRCLDO;                 /* GET CURRENT LDO               */ 01672000
         L     LDOPTR,CRRCLDO(,CRRPTR)                             0840 01673000
*   DO WHILE(CRRFINI=OFF&CRRSW1=OFF);/* LOOP UNTIL ERROR OR END      */ 01674000
         B     @DE00841                                            0841 01675000
@DL00841 DS    0H                                                  0842 01676000
*     IF LDOCMD=LDOJUMP THEN                                       0842 01677000
         CLI   LDOCMD(LDOPTR),X'08'                                0842 01678000
         BNE   @RF00842                                            0842 01679000
*       DO;                         /* CURRENT LDO IS A JUMP         */ 01680000
*         IF LDOADDR->LDOCMD=LDOJUMP THEN                          0844 01681000
         L     @15,LDOADDR(,LDOPTR)                                0844 01682000
         CLI   LDOCMD(@15),X'08'                                   0844 01683000
         BNE   @RF00844                                            0844 01684000
*           DO;                     /* ERROR - JUMP TO A JUMP        */ 01685000
*                                                                  0845 01686000
*             /*******************************************************/ 01687000
*             /*                                                     */ 01688000
*             /* ERROR-JUMP TO A JUMP LDO                            */ 01689000
*             /*                                                     */ 01690000
*             /*******************************************************/ 01691000
*                                                                  0846 01692000
*             CRRFDB2=CRRJTOJ;      /* SET REASON CODE FOR FEEDBACK  */ 01693000
         MVI   CRRFDB2(CRRPTR),X'19'                               0846 01694000
*             CRRRTNCD=CRRLOGIC;    /* SET LOGICAL I/O ERROR CODE    */ 01695000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0847 01696000
*             CRRFINI=ON;           /* DO PROCESSING FINISHED        */ 01697000
         OI    CRRFINI(CRRPTR),B'00001000'                         0848 01698000
*           END;                                                   0849 01699000
*         ELSE                      /* JUMP TO GOOD LDO              */ 01700000
*                                                                  0850 01701000
*           /*********************************************************/ 01702000
*           /*                                                       */ 01703000
*           /* JUMP TO A VALID LDO                                   */ 01704000
*           /*                                                       */ 01705000
*           /*********************************************************/ 01706000
*                                                                  0850 01707000
*           LDOPTR=LDOADDR;         /* REFERENCE NEXT LDO            */ 01708000
         B     @RC00844                                            0850 01709000
@RF00844 L     LDOPTR,LDOADDR(,LDOPTR)                             0850 01710000
*       END;                        /* END OF JUMP PROCESSING        */ 01711000
*     ELSE                                                         0852 01712000
*       IF LDOACHN=ON THEN                                         0852 01713000
         B     @RC00842                                            0852 01714000
@RF00842 TM    LDOACHN(LDOPTR),B'10000000'                         0852 01715000
         BNO   @RF00852                                            0852 01716000
*         DO;                       /* LDO IS DATA CHAINED           */ 01717000
*           IF CRRSW2=ON THEN                                      0854 01718000
         TM    CRRSW2(CRRPTR),B'00100000'                          0854 01719000
         BNO   @RF00854                                            0854 01720000
*             CRRSW1=ON;            /* LDO CHAIN UPDATED             */ 01721000
         OI    CRRSW1(CRRPTR),B'01000000'                          0855 01722000
*           ELSE                                                   0856 01723000
*             ;                     /* MORE PASSES THROUGH           */ 01724000
@RF00854 DS    0H                                                  0857 01725000
*           LDOPTR=LDOPTR+LDOSIZE;  /* REF NEXT LDO                  */ 01726000
         AL    LDOPTR,@CF00103                                     0857 01727000
*         END;                      /* END OF DATA CHAIN PROCESSING  */ 01728000
*       ELSE                                                       0859 01729000
*         IF LDOCMHN=ON THEN                                       0859 01730000
         B     @RC00852                                            0859 01731000
@RF00852 TM    LDOCMHN(LDOPTR),B'01000000'                         0859 01732000
         BNO   @RF00859                                            0859 01733000
*           DO;                     /* READ LDO CAN'T BE COMMEND   0860 01734000
*                                      CHAIN                         */ 01735000
*                                                                  0860 01736000
*             /*******************************************************/ 01737000
*             /*                                                     */ 01738000
*             /* ERROR-LDO IS COMMAND CHAINED                        */ 01739000
*             /*                                                     */ 01740000
*             /*******************************************************/ 01741000
*                                                                  0861 01742000
*             CRRFDB2=CRRRIOCC;     /* SET REASON CODE FOR FEEDBACK  */ 01743000
         MVI   CRRFDB2(CRRPTR),X'1D'                               0861 01744000
*             CRRRTNCD=CRRLOGIC;    /* SET LOGICAL I/O ERROR CODE    */ 01745000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0862 01746000
*             CRRFINI=ON;           /* DO PROCESSING FINISHED        */ 01747000
         OI    CRRFINI(CRRPTR),B'00001000'                         0863 01748000
*           END;                                                   0864 01749000
*         ELSE                                                     0865 01750000
*           CRRSW1=ON;              /* FINISHED - EXIT LOOP          */ 01751000
         B     @RC00859                                            0865 01752000
@RF00859 OI    CRRSW1(CRRPTR),B'01000000'                          0865 01753000
*     CRRCLDO=LDOPTR;               /* SAVE CURRENT LDOPTR IN CRR    */ 01754000
@RC00859 DS    0H                                                  0866 01755000
@RC00852 DS    0H                                                  0866 01756000
@RC00842 ST    LDOPTR,CRRCLDO(,CRRPTR)                             0866 01757000
*   END;                            /* END OF DO WHILE LOOP          */ 01758000
@DE00841 TM    CRRFINI(CRRPTR),B'00001000'                         0867 01759000
         BNZ   @DC00841                                            0867 01760000
         TM    CRRSW1(CRRPTR),B'01000000'                          0867 01761000
         BZ    @DL00841                                            0867 01762000
@DC00841 DS    0H                                                  0868 01763000
*   CRRSW2=OFF;                     /* CLEAR CONTROL SWITHCES        */ 01764000
*   CRRSW1=OFF;                                                    0869 01765000
         NI    CRRSW2(CRRPTR),B'10011111'                          0869 01766000
*   RETPTR=CRRRTN7;                 /* GET RETURN ADDRESS            */ 01767000
         L     RETPTR,CRRRTN7(,CRRPTR)                             0870 01768000
*   END UPLDOPTR;                   /* RETURN TO CALLER              */ 01769000
@EL00010 DS    0H                                                  0871 01770000
@EF00010 DS    0H                                                  0871 01771000
@ER00010 BR    @14                                                 0871 01772000
*CSUBR8:                                                           0872 01773000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0872 01774000
*                                                                  0872 01775000
CSUBR8   DS    0H                                                  0873 01776000
*/********************************************************************/ 01777000
*/*                                                                  */ 01778000
*/* ROUTINE NAME :  SUBR8 - VALIDATE LDO                             */ 01779000
*/*                                                                  */ 01780000
*/* FUNCTION :                                                       */ 01781000
*/*    THE LDO IS CHECKED TO INSURE THAT THE LDO IS A READ           */ 01782000
*/*    AND IS NOT COMMAND CHAINED. SUBR3 IS CALLED TO MAKE           */ 01783000
*/*    FURTHER CHECKS.                                               */ 01784000
*/*                                                                  */ 01785000
*/* INPUT :  LDOPTR (REG 10) - ADDRESS OF LDO                        */ 01786000
*/*                                                                  */ 01787000
*/* OUTPUT :   THE CURRENT LDO IS VALIDATED                          */ 01788000
*/*                                                                  */ 01789000
*/* SUBROUTINES CALLED :                                             */ 01790000
*/*         SUBR3 - CHECK LDO                                        */ 01791000
*/*                                                                  */ 01792000
*/*  MACROS USED :  NONE                                             */ 01793000
*/*                                                                  */ 01794000
*/********************************************************************/ 01795000
*                                                                  0873 01796000
*   CRRRTN2=RETPTR;                 /* SAVE RETURN ADDRESS           */ 01797000
         ST    RETPTR,CRRRTN2(,CRRPTR)                             0873 01798000
*   IF LDOCMD=LDORDBUF|LDOCMD=LDORDMOD|LDOCMD=LDOREAD THEN/* LDO IS A   01799000
*                                      READ                          */ 01800000
         CLI   LDOCMD(LDOPTR),X'16'                                0874 01801000
         BE    @RT00874                                            0874 01802000
         CLI   LDOCMD(LDOPTR),X'12'                                0874 01803000
         BE    @RT00874                                            0874 01804000
         CLI   LDOCMD(LDOPTR),X'02'                                0874 01805000
         BNE   @RF00874                                            0874 01806000
@RT00874 DS    0H                                                  0875 01807000
*     IF LDOCMHN=ON THEN                                           0875 01808000
         TM    LDOCMHN(LDOPTR),B'01000000'                         0875 01809000
         BNO   @RF00875                                            0875 01810000
*       DO;                         /* ERROR-RD LDO IS COMMAND     0876 01811000
*                                      CHAINED                       */ 01812000
*         RFY                                                      0877 01813000
*           RETCODE UNRSTD;                                        0877 01814000
*                                                                  0877 01815000
*         /***********************************************************/ 01816000
*         /*                                                         */ 01817000
*         /* ERROR-READ LDO COMMAND CHAINED                          */ 01818000
*         /*                                                         */ 01819000
*         /***********************************************************/ 01820000
*                                                                  0878 01821000
*         CRRFDB2=CRRRIOCC;         /* SET REASON CODE               */ 01822000
         MVI   CRRFDB2(CRRPTR),X'1D'                               0878 01823000
*         CRRRTNCD=CRRLOGIC;        /* LOGICAL I/O ERROR CODE        */ 01824000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0879 01825000
*         CRRFINI=ON;               /* DO PROCESSING FINISHED        */ 01826000
         OI    CRRFINI(CRRPTR),B'00001000'                         0880 01827000
*         RFY                                                      0881 01828000
*           RETCODE RSTD;           /* RESTRICT RETURN CODE REG      */ 01829000
*       END;                                                       0882 01830000
*                                                                  0882 01831000
*   /*****************************************************************/ 01832000
*   /*                                                               */ 01833000
*   /* VALID READ LDO                                                */ 01834000
*   /*                                                               */ 01835000
*   /*****************************************************************/ 01836000
*                                                                  0883 01837000
*     ELSE                                                         0883 01838000
*       CALL CSUBR3;                /* CHECK LDO                     */ 01839000
         B     @RC00875                                            0883 01840000
@RF00875 BAL   @14,CSUBR3                                          0883 01841000
*   ELSE                                                           0884 01842000
*     DO;                           /* OP CODE NOT A READ            */ 01843000
*                                                                  0884 01844000
         B     @RC00874                                            0884 01845000
@RF00874 DS    0H                                                  0885 01846000
*       /*************************************************************/ 01847000
*       /*                                                           */ 01848000
*       /* ERROR-NOT VALID READ OP CODE                              */ 01849000
*       /*                                                           */ 01850000
*       /*************************************************************/ 01851000
*                                                                  0885 01852000
*       CRRFDB2=CRRILDOP;           /* SET REASON CODE               */ 01853000
         MVI   CRRFDB2(CRRPTR),X'06'                               0885 01854000
*       CRRRTNCD=CRRLOGIC;          /* SET LOGICAL I/O ERROR CODE    */ 01855000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0886 01856000
*       CRRFINI=ON;                 /* DO PROCESSING FINISHED        */ 01857000
         OI    CRRFINI(CRRPTR),B'00001000'                         0887 01858000
*     END;                                                         0888 01859000
*   RETPTR=CRRRTN2;                 /* GET RETURN ADDRESS            */ 01860000
@RC00874 L     RETPTR,CRRRTN2(,CRRPTR)                             0889 01861000
*   END CSUBR8;                     /* RETURN TO CALLER              */ 01862000
@EL00011 DS    0H                                                  0890 01863000
@EF00011 DS    0H                                                  0890 01864000
@ER00011 BR    @14                                                 0890 01865000
*CSUBR9:                                                           0891 01866000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0891 01867000
CSUBR9   DS    0H                                                  0892 01868000
*   CRRRTN4=RETPTR;                 /* SAVE RETURN ADDRESS           */ 01869000
*                                                                  0892 01870000
         ST    RETPTR,CRRRTN4(,CRRPTR)                             0892 01871000
*   /*****************************************************************/ 01872000
*   /*                                                               */ 01873000
*   /* DETERMINE WHETHER THE MOVE OF DATA IS COMPLETE. THE MOVE      */ 01874000
*   /* ROUTINE SUCCESSFULLY MOVED ALL THE DATA FROM ONE FSB INTO THE */ 01875000
*   /* USER'S BUFFER. IF THE FSB IS NOT A SUBBLOCK (NO EOB), THE     */ 01876000
*   /* RETURN CODE IS NOT 0, OR LEADING GRAPHICS WERE RECEIVED THEN  */ 01877000
*   /* PROCESSING IS FINISHED. THE FSB WILL BE FREED AND THE LDO'S   */ 01878000
*   /* WILL BE EXAMINED TO MAKE SURE THE LAST ONE ASSOCIATED WITH    */ 01879000
*   /* THIS READ IS NOT COMMAND CHAINED. IF THE CURRENT FSB IS A     */ 01880000
*   /* SUBBLOCK AND THERE IS MORE ROOM IN THE USER'S BUFFERS, CSUBR11*/ 01881000
*   /* WILL BE CALLED. IF THERE IS NO ROOM IN THE USER'S BUFFER,     */ 01882000
*   /* CSUBR10 WILL BE CALLED TO TEST WHETHER THE USER HAS SPECIFIED */ 01883000
*   /* TRUNC OR KEEP IN HIS FMCB.                                    */ 01884000
*   /*                                                               */ 01885000
*   /*****************************************************************/ 01886000
*                                                                  0893 01887000
*   IF(FSBEBI=OFF&FSBLGI=OFF&       /* SUBBLOCK AND NO LEADING GRAPH */ 01888000
*       FSBSOHI=OFF&                /* START OF HEADER               */ 01889000
*       FSBMJRCD=FRNORM)&           /* NO ERROR CODE IN FSB          */ 01890000
*       FSBLCPB^=ZERO THEN          /* NOT UNSOLICITED FSB           */ 01891000
*                                                                  0893 01892000
         TM    FSBEBI(FSBPTR),B'00100011'                          0893 01893000
         BNZ   @RF00893                                            0893 01894000
         CLI   FSBMJRCD(FSBPTR),X'00'                              0893 01895000
         BNE   @RF00893                                            0893 01896000
         SLR   @12,@12                                             0893 01897000
         C     @12,FSBLCPB(,FSBPTR)                                0893 01898000
         BE    @RF00893                                            0893 01899000
*     /***************************************************************/ 01900000
*     /*                                                             */ 01901000
*     /* CURRENT FSB IS A SUBBLOCK                                   */ 01902000
*     /*                                                             */ 01903000
*     /***************************************************************/ 01904000
*                                                                  0894 01905000
*     IF(CRRBLEN=0&LDOACHN=OFF) THEN                               0894 01906000
         CH    @12,CRRBLEN(,CRRPTR)                                0894 01907000
         BNE   @RF00894                                            0894 01908000
         TM    LDOACHN(LDOPTR),B'10000000'                         0894 01909000
         BNZ   @RF00894                                            0894 01910000
*       DO;                         /* NO ROOM IN BUFFER             */ 01911000
*                                                                  0895 01912000
*         /***********************************************************/ 01913000
*         /*                                                         */ 01914000
*         /* MORE DATA TO MOVE, BUT NO MORE ROOM                     */ 01915000
*         /*                                                         */ 01916000
*         /***********************************************************/ 01917000
*                                                                  0896 01918000
*         CALL FREEFSB;             /* RELEASE FSB JUST COMPLETED    */ 01919000
         BAL   @14,FREEFSB                                         0896 01920000
*         CRRSW7=ON;                /* INDICATE TRUNCATION           */ 01921000
         OI    CRRSW7(CRRPTR),B'00000001'                          0897 01922000
*         CALL CSUBR10;             /* CHECK TRUNC/KEEP OPTION       */ 01923000
         BAL   @14,CSUBR10                                         0898 01924000
*       END;                                                       0899 01925000
*                                                                  0899 01926000
*   /*****************************************************************/ 01927000
*   /*                                                               */ 01928000
*   /* MORE DATA EXPECTED AND ROOM IN BUFFER                         */ 01929000
*   /*                                                               */ 01930000
*   /*****************************************************************/ 01931000
*                                                                  0900 01932000
*     ELSE                                                         0900 01933000
*       CALL CSUBR11;               /* SEE IF MORE DATA TO MOVE      */ 01934000
         B     @RC00894                                            0900 01935000
@RF00894 BAL   @14,CSUBR11                                         0900 01936000
*   ELSE                                                           0901 01937000
*     DO;                           /* ALL DATA WAS MOVED            */ 01938000
*                                                                  0901 01939000
         B     @RC00893                                            0901 01940000
@RF00893 DS    0H                                                  0902 01941000
*       /*************************************************************/ 01942000
*       /*                                                           */ 01943000
*       /* PROCESSING FINISHED-ALL DATA MOVED                        */ 01944000
*       /*                                                           */ 01945000
*       /*************************************************************/ 01946000
*                                                                  0902 01947000
*       CALL FREEFSB;               /* DEQUEUE AND FEE FSB           */ 01948000
         BAL   @14,FREEFSB                                         0902 01949000
*       CRRCLDO=CRRBLDAD;           /* SAVE PTR TO LAST LDO USED     */ 01950000
         L     @12,CRRBLDAD(,CRRPTR)                               0903 01951000
         ST    @12,CRRCLDO(,CRRPTR)                                0903 01952000
*       RPLAAREA=CRRBLDAD;          /* SAVE LDO POINTER FOR USER     */ 01953000
         LR    RPLAAREA,@12                                        0904 01954000
*       CALL UPLDOPTR;              /* UPDATE TO END-CHECK CMD CHAIN */ 01955000
         BAL   @14,UPLDOPTR                                        0905 01956000
*       CRRSW3=ON;                  /* FINISHED WITH MOVE            */ 01957000
         OI    CRRSW3(CRRPTR),B'00010000'                          0906 01958000
*     END;                                                         0907 01959000
*   RETPTR=CRRRTN4;                 /* GET RETURN ADDRESS            */ 01960000
@RC00893 L     RETPTR,CRRRTN4(,CRRPTR)                             0908 01961000
*   END CSUBR9;                     /* GET RETURN ADDRESS            */ 01962000
@EL00012 DS    0H                                                  0909 01963000
@EF00012 DS    0H                                                  0909 01964000
@ER00012 BR    @14                                                 0909 01965000
*CSUBR10:                                                          0910 01966000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0910 01967000
CSUBR10  DS    0H                                                  0911 01968000
*   CRRRTN7=RETPTR;                 /* SAVE RETURN ADDRESS           */ 01969000
*                                                                  0911 01970000
         ST    RETPTR,CRRRTN7(,CRRPTR)                             0911 01971000
*   /*****************************************************************/ 01972000
*   /*                                                               */ 01973000
*   /* THERE IS NO ROOM IN THE USER'S BUFFER, BUT THERE IS MORE DATA */ 01974000
*   /* TO BE MOVED. IF THE USER SPECIFIED KEEP IN HIS FMCB, AN       */ 01975000
*   /* INDICATION THAT PARTIAL DATA WAS RECEIVED IS SET AS FEEDBACK  */ 01976000
*   /* STATUS AND THE FSB IS NOT FREED. IF TRUNC WAS SPECIFIED, A    */ 01977000
*   /* LOGICAL I/O ERROR IS SET AS STATUS AND ALL THE DATA IS THROWN */ 01978000
*   /* AWAY UNTIL AND ENDING CONDITION IS FOUND(EOB).                */ 01979000
*   /*                                                               */ 01980000
*   /*****************************************************************/ 01981000
*                                                                  0912 01982000
*   IF PROTRUNC=ON THEN                                            0912 01983000
         L     @12,CRRFMCB(,CRRPTR)                                0912 01984000
         TM    PROTRUNC+48(@12),B'01000000'                        0912 01985000
         BNO   @RF00912                                            0912 01986000
*     DO;                           /* TRUNC OPTION                  */ 01987000
*                                                                  0913 01988000
*       /*************************************************************/ 01989000
*       /*                                                           */ 01990000
*       /* TRUNC PROCESSING                                          */ 01991000
*       /*                                                           */ 01992000
*       /*************************************************************/ 01993000
*                                                                  0914 01994000
*       IF FSBLGI=OFF&              /* TEST FOR LEADING GRAPHICS     */ 01995000
*           FSBSOHI=OFF&            /* START OF HEADER               */ 01996000
*           FSBMJRCD=FRNORM&        /* NORMAL RETURN CODE            */ 01997000
*           FSBLCPB^=ZERO THEN      /* NOT UNSOLICITED FSB           */ 01998000
         TM    FSBLGI(FSBPTR),B'00000011'                          0914 01999000
         BNZ   @RF00914                                            0914 02000000
         CLI   FSBMJRCD(FSBPTR),X'00'                              0914 02001000
         BNE   @RF00914                                            0914 02002000
         L     @12,FSBLCPB(,FSBPTR)                                0914 02003000
         LTR   @12,@12                                             0914 02004000
         BZ    @RF00914                                            0914 02005000
*         CRRSW0=ON;                /* INDICATE FLUSH TO EOB         */ 02006000
         OI    CRRSW0(CRRPTR),B'10000000'                          0915 02007000
*       ELSE                                                       0916 02008000
*         ;                         /* LEADING GRAPH TREATED AS EOB  */ 02009000
@RF00914 DS    0H                                                  0917 02010000
*       CALL FREEFSB;               /* DEQUEUE AND FREE STG FOR FSB     02011000
*                                      D31JKMQ                       */ 02012000
         BAL   @14,FREEFSB                                         0917 02013000
*     END;                                                         0918 02014000
*   ELSE                                                           0919 02015000
*     DO;                           /* KEEP OPTION                   */ 02016000
*                                                                  0919 02017000
         B     @RC00912                                            0919 02018000
@RF00912 DS    0H                                                  0920 02019000
*       /*************************************************************/ 02020000
*       /*                                                           */ 02021000
*       /* KEEP PROCESSING                                           */ 02022000
*       /*                                                           */ 02023000
*       /*************************************************************/ 02024000
*                                                                  0920 02025000
*       CRRRDEOB=OFF;               /* INDICATE PARTIAL DATA         */ 02026000
*       CRRRDEOM=OFF;                                              0921 02027000
*       CRRRDEOT=OFF;                                              0922 02028000
         NI    CRRRDEOB(CRRPTR),B'11000111'                        0922 02029000
*       IF CRRBLDAD->LDOCMHN=ON THEN                               0923 02030000
         L     @12,CRRBLDAD(,CRRPTR)                               0923 02031000
         TM    LDOCMHN(@12),B'01000000'                            0923 02032000
         BNO   @RF00923                                            0923 02033000
*         DO;                       /* CURRENT LDO IS COMMAND CHAINED*/ 02034000
*                                                                  0924 02035000
*           /*********************************************************/ 02036000
*           /*                                                       */ 02037000
*           /* ERROR-LDO COMMAND CHAINED                             */ 02038000
*           /*                                                       */ 02039000
*           /*********************************************************/ 02040000
*                                                                  0925 02041000
*           CRRFDB2=CRRRIOCC;       /* SET REASON CODE FOR FEEDBACK  */ 02042000
         MVI   CRRFDB2(CRRPTR),X'1D'                               0925 02043000
*           CRRRTNCD=CRRLOGIC;      /* SET LOGICAL I/O ERROR CODE    */ 02044000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0926 02045000
*         END;                                                     0927 02046000
*       ELSE                                                       0928 02047000
*         ;                         /* NOT COMMAND CHAINED           */ 02048000
@RF00923 DS    0H                                                  0929 02049000
*     END;                                                         0929 02050000
*   CRRSW3=ON;                      /* MOVE FINISHED                 */ 02051000
@RC00912 OI    CRRSW3(CRRPTR),B'00010000'                          0930 02052000
*   CRRCLDO=CRRBLDAD;               /* SAVE CURRENT LDO FROM MOVE    */ 02053000
         L     @12,CRRBLDAD(,CRRPTR)                               0931 02054000
         ST    @12,CRRCLDO(,CRRPTR)                                0931 02055000
*   RPLAAREA=CRRCLDO;               /* SET LDO FOR FEEDBACK          */ 02056000
         LR    RPLAAREA,@12                                        0932 02057000
*   RETPTR=CRRRTN7;                 /* GET RETURN ADDRESS            */ 02058000
         L     RETPTR,CRRRTN7(,CRRPTR)                             0933 02059000
*   END CSUBR10;                    /* RETURN TO CALLER              */ 02060000
@EL00013 DS    0H                                                  0934 02061000
@EF00013 DS    0H                                                  0934 02062000
@ER00013 BR    @14                                                 0934 02063000
*CSUBR11:                                                          0935 02064000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0935 02065000
CSUBR11  DS    0H                                                  0936 02066000
*   CRRRTN5=RETPTR;                 /* SAVE RETURN ADDRESS           */ 02067000
*                                                                  0936 02068000
         ST    RETPTR,CRRRTN5(,CRRPTR)                             0936 02069000
*   /*****************************************************************/ 02070000
*   /*                                                               */ 02071000
*   /* THE FSB WHICH WAS EXHAUSTED WAS A SUBBLOCK (NO EOB) AND THERE */ 02072000
*   /* IS ROOM IN THE USER'S BUFFER FOR MORE DATA. FREE AND RELEASE  */ 02073000
*   /* THE FSB JUST USED. IF THERE IS ANOTHER FSB ON THE FMCB DATA   */ 02074000
*   /* QUEUE, SAVE IT'S ADDRESS AND SET UP TO MOVE AGAIN. IF THERE IS*/ 02075000
*   /* NO FSB, AND THERE WAS A LCPB ASSOCIATED WITH THE LAST FSB,    */ 02076000
*   /* CHECK FOR ERROR LOCK SET. IF SO, NOTIFY THE USER NOW.         */ 02077000
*   /* OTHERWISE, INDICATE TO REQUEST FINISH THAT THE CURRENT RPL IS */ 02078000
*   /* WAITING FOR A FSB FROM A PREVIOUS I/O OPERATION. SAVE THE     */ 02079000
*   /* STATUS FROM THE MOVE IN THE LCPB SO THAT WHEN DATA ARRIVES THE*/ 02080000
*   /* MOVE CAN CONTINUE AT THE PROPER PLACE.                        */ 02081000
*   /*                                                               */ 02082000
*   /*****************************************************************/ 02083000
*                                                                  0937 02084000
*   LCPBPTR=FSBLCPB;                /* SAVE LCPB PTR FROM USED FSB   */ 02085000
         L     LCPBPTR,FSBLCPB(,FSBPTR)                            0937 02086000
*   CALL FREEFSB;                   /* DEQUE AND RELEASE STG FOR FSB */ 02087000
         BAL   @14,FREEFSB                                         0938 02088000
*/* TPFEL (,FMCFBA) RETPTR(FSBPTR)                                   */ 02089000
*                                                                  0939 02090000
*   DO;                             /* BEGIN TPFEL CODE GENERATION   */ 02091000
*     RFY                                                          0940 02092000
*      (GPR00P,                                                    0940 02093000
*       GPR01P,                                                    0940 02094000
*       GPR14P,                                                    0940 02095000
*       GPR15P) RSTD;                                              0940 02096000
*     GPR15P=ADDR(FMCFBA);          /* ADDRESS QUEUE POINTER         */ 02097000
         L     GPR15P,CRRFMCB(,CRRPTR)                             0941 02098000
         LA    GPR15P,FMCFBA(,GPR15P)                              0941 02099000
*     GPR14P=GPR15P->PABWEQA|FMZCA020;                             0942 02100000
         L     @12,@CF02549                                        0942 02101000
         L     GPR14P,PABWEQA(,GPR15P)                             0942 02102000
         OR    GPR14P,@12                                          0942 02103000
*     IF GPR14P=FMZCA020 THEN                                      0943 02104000
         CLR   GPR14P,@12                                          0943 02105000
         BE    @RT00943                                            0943 02106000
*       ;                                                          0944 02107000
*     ELSE                                                         0945 02108000
*       DO;                                                        0945 02109000
*         GPR00P=FMZCA020;          /* INITIALIZE QUIT FLAG          */ 02110000
         L     GPR00P,@CF02549                                     0946 02111000
*         DO WHILE(GPR00P^=GPR14P->WKECHAIN);                      0947 02112000
         B     @DE00947                                            0947 02113000
@DL00947 DS    0H                                                  0948 02114000
*           GPR14P=GPR14P->WKECHAIN;/* SEARCH FOR END OF CHAIN       */ 02115000
         L     GPR14P,WKECHAIN(,GPR14P)                            0948 02116000
*         END;                                                     0949 02117000
@DE00947 C     GPR00P,WKECHAIN(,GPR14P)                            0949 02118000
         BNE   @DL00947                                            0949 02119000
*       END;                                                       0950 02120000
*     GPR14P=GPR14P&FMZCB020;                                      0951 02121000
@RT00943 N     GPR14P,@CF02547                                     0951 02122000
*     FSBPTR=GPR14P;                                               0952 02123000
         LR    FSBPTR,GPR14P                                       0952 02124000
*     RFY                                                          0953 02125000
*      (GPR00P,                                                    0953 02126000
*       GPR01P,                                                    0953 02127000
*       GPR14P,                                                    0953 02128000
*       GPR15P) UNRSTD;                                            0953 02129000
*   END;                            /* END TPFEL GENERATION          */ 02130000
*   IF FSBPTR^=0&FSBPTR^=GATE0 THEN                                0955 02131000
         SLR   @12,@12                                             0955 02132000
         CR    FSBPTR,@12                                          0955 02133000
         BE    @RF00955                                            0955 02134000
         CL    FSBPTR,@CF02549                                     0955 02135000
         BE    @RF00955                                            0955 02136000
*     DO;                           /* ANOTHER FSB IS PRESENT        */ 02137000
*                                                                  0956 02138000
*       /*************************************************************/ 02139000
*       /*                                                           */ 02140000
*       /* MORE DATA PRESENT ON Q                                    */ 02141000
*       /*                                                           */ 02142000
*       /*************************************************************/ 02143000
*                                                                  0957 02144000
*       CRRFSB=FSBPTR;              /* NEW FSB ADDR IN CRR           */ 02145000
         ST    FSBPTR,CRRFSB(,CRRPTR)                              0957 02146000
*       IF CRRBLEN=0 THEN                                          0958 02147000
         CH    @12,CRRBLEN(,CRRPTR)                                0958 02148000
         BNE   @RF00958                                            0958 02149000
*         DO;                       /* LDO SATISFIED - UPDATE TO NEXT*/ 02150000
*                                                                  0959 02151000
*           /*********************************************************/ 02152000
*           /*                                                       */ 02153000
*           /* UPDATE TO NEXT LDO                                    */ 02154000
*           /*                                                       */ 02155000
*           /*********************************************************/ 02156000
*                                                                  0960 02157000
*           CRRSW2=ON;              /* INDICATE NEXT LDO WANTED      */ 02158000
         OI    CRRSW2(CRRPTR),B'00100000'                          0960 02159000
*           CALL UPLDOPTR;          /* GET NEXT NON-JUMP LDO         */ 02160000
         BAL   @14,UPLDOPTR                                        0961 02161000
*           CRRBLDAD=CRRCLDO;       /* SAVE LDO FOR MOVE             */ 02162000
         L     @12,CRRCLDO(,CRRPTR)                                0962 02163000
         ST    @12,CRRBLDAD(,CRRPTR)                               0962 02164000
*         END;                                                     0963 02165000
*       ELSE                                                       0964 02166000
*         ;                         /* ALL SET FOR NEXT MOVE         */ 02167000
@RF00958 DS    0H                                                  0965 02168000
*     END;                                                         0965 02169000
*   ELSE                                                           0966 02170000
*     DO;                           /* NO MORE FSB'S ON DATA QUEUE   */ 02171000
*                                                                  0966 02172000
         B     @RC00955                                            0966 02173000
@RF00955 DS    0H                                                  0967 02174000
*       /*************************************************************/ 02175000
*       /*                                                           */ 02176000
*       /* NO MORE DATA ON DATA QUEUE                                */ 02177000
*       /*                                                           */ 02178000
*       /*************************************************************/ 02179000
*                                                                  0967 02180000
*       IF LCPBPTR^=0 THEN          /* IF FSB POINTED TO LCPB        */ 02181000
         LTR   LCPBPTR,LCPBPTR                                     0967 02182000
         BZ    @RF00967                                            0967 02183000
*         IF CRRERLK=ON THEN        /* IF ERROR LOCK WAS SET         */ 02184000
*                                                                  0968 02185000
         TM    CRRERLK(CRRPTR),B'10000000'                         0968 02186000
         BNO   @RF00968                                            0968 02187000
*           /*********************************************************/ 02188000
*           /*                                                       */ 02189000
*           /* ERROR CONDITION-NO MORE DATA                          */ 02190000
*           /*                                                       */ 02191000
*           /*********************************************************/ 02192000
*                                                                  0969 02193000
*           DCLRPLA=DCLRPLA&&DCLRPLA;/* DONE WITH THIS REQUEST       */ 02194000
         L     @12,DCLRPLA(,LCPBPTR)                               0969 02195000
         LR    @06,@12                                             0969 02196000
         XR    @06,@12                                             0969 02197000
         ST    @06,DCLRPLA(,LCPBPTR)                               0969 02198000
*         ELSE                                                     0970 02199000
*           DO;                     /* NO ERROR INDICATED            */ 02200000
*                                                                  0970 02201000
         B     @RC00968                                            0970 02202000
@RF00968 DS    0H                                                  0971 02203000
*             /*******************************************************/ 02204000
*             /*                                                     */ 02205000
*             /* WAIT FOR MORE DATA TO ARRIVE                        */ 02206000
*             /*                                                     */ 02207000
*             /*******************************************************/ 02208000
*                                                                  0971 02209000
*             CRRSUBBK=ON;          /* WAITING FOR SUBBLOCK          */ 02210000
         OI    CRRSUBBK(CRRPTR),B'00000100'                        0971 02211000
*             CRRSW3=ON;            /* MOVE FINISHED                 */ 02212000
         OI    CRRSW3(CRRPTR),B'00010000'                          0972 02213000
*             DCLRPLA=CRRARPL;      /* RPL ASSOCIATED WITH THIS LCPB */ 02214000
         L     @12,CRRARPL(,CRRPTR)                                0973 02215000
         ST    @12,DCLRPLA(,LCPBPTR)                               0973 02216000
*             DCLCLDOA=CRRBLDAD;    /* LDO TO PROCESS NEXT           */ 02217000
         L     @12,CRRBLDAD(,CRRPTR)                               0974 02218000
         ST    @12,DCLCLDOA(,LCPBPTR)                              0974 02219000
*             IF ADDR(LCPLCCW)->LCCWOP^=LTIC THEN                  0975 02220000
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0975 02221000
         BE    @RF00975                                            0975 02222000
*               DCLLDO1=CRRBLDAD;   /* SAVE NEXT LDO                 */ 02223000
         ST    @12,DCLLDO1(,LCPBPTR)                               0976 02224000
*             ELSE                  /* SAVE NEXT LDO IN EXTENSION    */ 02225000
*               EVAL(CRRCLCCW+LDOFFSET)->LDOSAVE=CRRBLDAD;         0977 02226000
         B     @RC00975                                            0977 02227000
@RF00975 LA    @12,32                                              0977 02228000
         AL    @12,CRRCLCCW(,CRRPTR)                               0977 02229000
         L     @06,CRRBLDAD(,CRRPTR)                               0977 02230000
         ST    @06,LDOSAVE(,@12)                                   0977 02231000
*             DCLBTLEN=CRRBTLEN;    /* SAVE RESIDUAL COUNT           */ 02232000
@RC00975 L     @12,CRRBTLEN(,CRRPTR)                               0978 02233000
         ST    @12,DCLBTLEN(,LCPBPTR)                              0978 02234000
*             DCLIOLEN=CRRBLEN;     /* AMOUNT LEFT TO MOVE           */ 02235000
         LH    @12,CRRBLEN(,CRRPTR)                                0979 02236000
         STH   @12,DCLIOLEN(,LCPBPTR)                              0979 02237000
*             DCLIOBUF=CRRBARAD;    /* RESUME BUFFER ADDRESS         */ 02238000
         L     @12,CRRBARAD(,CRRPTR)                               0980 02239000
         ST    @12,DCLIOBUF(,LCPBPTR)                              0980 02240000
*           END;                                                   0981 02241000
*       ELSE                                                       0982 02242000
*         ;                         /* FSB DID NOT POINT TO LCPB     */ 02243000
@RF00967 DS    0H                                                  0983 02244000
*     END;                                                         0983 02245000
@RC00967 DS    0H                                                  0984 02246000
*   RETPTR=CRRRTN5;                 /* GET RETURN ADDRESS            */ 02247000
@RC00955 L     RETPTR,CRRRTN5(,CRRPTR)                             0984 02248000
*   END CSUBR11;                    /* RETURN TO CALLER              */ 02249000
@EL00014 DS    0H                                                  0985 02250000
@EF00014 DS    0H                                                  0985 02251000
@ER00014 BR    @14                                                 0985 02252000
*LCPBINIT:                                                         0986 02253000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0986 02254000
LCPBINIT DS    0H                                                  0987 02255000
*   CRRRTN9=RETPTR;                 /* SAVE CALLER'S RETURN ADDR     */ 02256000
*                                                                  0987 02257000
         ST    RETPTR,CRRRTN9(,CRRPTR)                             0987 02258000
*   /*****************************************************************/ 02259000
*   /*                                                               */ 02260000
*   /* THIS ROUTINE IS INVOKED BY FREEFSB WHEN AN ERROR LOCK IS FOUND*/ 02261000
*   /* ON IN A FSB BEING FLUSHED. THE RESET ERROR LOCK MUST BE ISSUED*/ 02262000
*   /* INTERNALLY SINCE THE USER DOES NOT KNOW IT IS SET. INITIALIZE */ 02263000
*   /* LCPB FOR RESET ERROR LOCK AND QUEUE TO TPIOS OUTBOUND PAB WITH*/ 02264000
*   /* RESET OPTION.                                                 */ 02265000
*   /*                                                               */ 02266000
*   /*****************************************************************/ 02267000
*                                                                  0988 02268000
*   LCPBPTR=CRRLCPB;                /* REFERENCE LCPB                */ 02269000
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0988 02270000
*   LCPNXLCW=ADDR(DCLLCCW2);        /* POINT TO LCCW FOR RESET       */ 02271000
         LA    @12,DCLLCCW2(,LCPBPTR)                              0989 02272000
         ST    @12,LCPNXLCW(,LCPBPTR)                              0989 02273000
*   DCLRQTP=DCLRESET;               /* INDICATE RESET OPERATION      */ 02274000
         MVI   DCLRQTP(LCPBPTR),X'12'                              0990 02275000
*   RFY                                                            0991 02276000
*     ISTLCCW BASED(LCPNXLCW);                                     0991 02277000
*   LCCWOP=LXEL;                    /* RESET ERROR LOCK              */ 02278000
         MVI   LCCWOP(@12),X'0B'                                   0992 02279000
*   DCLSRESW=ON;                    /* INDICATE SPECIAL RESET        */ 02280000
         OI    DCLSRESW(LCPBPTR),B'10000000'                       0993 02281000
*/*  TPQUE (LCPBPTR,FMCPAB3) RESET                                   */ 02282000
*                                                                  0994 02283000
*   DO;                                                            0994 02284000
*     RFY                                                          0995 02285000
*      (GPR00P,                                                    0995 02286000
*       GPR01P,                                                    0995 02287000
*       GPR02P,                                                    0995 02288000
*       GPR15P) RSTD;                                              0995 02289000
*     GEN REFS(GPR01P) SETS(RPHWORK)                               0996 02290000
*         (STM   GPR14P,GPR09P,RPHWORK(GPR01P));                   0996 02291000
         STM   GPR14P,GPR09P,RPHWORK(GPR01P)                            02292000
*     GPR00P=ADDR(FMCPAB3);                                        0997 02293000
         L     @12,CRRFMCB(,CRRPTR)                                0997 02294000
         LA    GPR00P,FMCPAB3(,@12)                                0997 02295000
*     GPR02P=LCPBPTR;                                              0998 02296000
         LR    GPR02P,LCPBPTR                                      0998 02297000
*     CALL ATCLCPTR->ATCAP34->ISTAPC34;                            0999 02298000
         L     @12,ATCLCPTR                                        0999 02299000
         L     @15,ATCAP34(,@12)                                   0999 02300000
         BALR  @14,@15                                             0999 02301000
*     GEN REFS(RPHWORK,GPR01P) NOSETS                              1000 02302000
*         (LM    GPR14P,GPR09P,RPHWORK(GPR01P));                   1000 02303000
         LM    GPR14P,GPR09P,RPHWORK(GPR01P)                            02304000
