         N     @15,@CF04303                                        0661 00992000
         STH   @15,CRRBLEN(,CRRPTR)                                0661 00993000
*   DO WHILE(CRRSW3=OFF);           /* CONTROL LOOP FOR MOVE         */ 00994000
*                                                                  0662 00995000
         B     @DE00662                                            0662 00996000
@DL00662 DS    0H                                                  0663 00997000
*     /***************************************************************/ 00998000
*     /*                                                             */ 00999000
*     /* CYCLE UNTIL MOVE COMPLETE                                   */ 01000000
*     /*                                                             */ 01001000
*     /***************************************************************/ 01002000
*                                                                  0663 01003000
*     CALL CSUBR7;                  /* MOVE DATA                     */ 01004000
         BAL   @14,CSUBR7                                          0663 01005000
*   END;                            /* END OF DO WHILE LOOP          */ 01006000
@DE00662 TM    CRRSW3(CRRPTR),B'00010000'                          0664 01007000
         BZ    @DL00662                                            0664 01008000
*   CRRFINI=ON;                     /* INDICATE DO PROCESSING      0665 01009000
*                                      FINISHED                      */ 01010000
         OI    CRRFINI(CRRPTR),B'00001000'                         0665 01011000
*   RETPTR=CRRRTN3;                 /* GET RETURN ADDRESS            */ 01012000
         L     RETPTR,CRRRTN3(,CRRPTR)                             0666 01013000
*   END CSUBR6;                     /* RETURN TO CALLER              */ 01014000
@EL00007 DS    0H                                                  0667 01015000
@EF00007 DS    0H                                                  0667 01016000
@ER00007 BR    @14                                                 0667 01017000
*                                                                  0668 01018000
*/********************************************************************/ 01019000
*/*                                                                  */ 01020000
*/* ROUTINE NAME :  SUBR7 - MOVE DATA TO USER AREA                   */ 01021000
*/*                                                                  */ 01022000
*/* FUNCTION :                                                       */ 01023000
*/*    SUBR7 CALLS INPUT BUFFER MOVE(ISTDCC60) TO MOVE THE DATA      */ 01024000
*/*    AND HANDLES THE FEEDBACK FROM THE MOVE ROUTINE                */ 01025000
*/*                                                                  */ 01026000
*/* INPUT :  LDOPTR (REG 10) - ADDRESS OF LDO                        */ 01027000
*/*                                                                  */ 01028000
*/* OUTPUT :  FEEDBACK STATUS SET IN THE CRR- SOME OR ALL DATA MOVED */ 01029000
*/*                                                                  */ 01030000
*/* SUBROUTINES CALLED :                                             */ 01031000
*/*         ISTDCC60 - INPUT BUFFER MOVE                             */ 01032000
*/*         SUBR9    - HANDLE RETURN CODE ZERO                       */ 01033000
*/*         SUBR10   - CHECK FOR TRUNC/KEEP                          */ 01034000
*/*                                                                  */ 01035000
*/*  MACROS USED :  VMDESET, VSETLCK                                 */ 01036000
*/*                                                                  */ 01037000
*/********************************************************************/ 01038000
*                                                                  0668 01039000
*CSUBR7:                                                           0668 01040000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0668 01041000
CSUBR7   DS    0H                                                  0669 01042000
*   CRRRTN6=RETPTR;                 /* SAVE RETURN ADDRESS           */ 01043000
*                                                                  0669 01044000
         ST    RETPTR,CRRRTN6(,CRRPTR)                             0669 01045000
*   /*****************************************************************/ 01046000
*   /*                                                               */ 01047000
*   /* INVOKE ISTDCC60 TO MOVE THE DATA TO THE USER'S BUFFERS        */ 01048000
*   /*                                                               */ 01049000
*   /*****************************************************************/ 01050000
*                                                                  0670 01051000
*   RFY                                                            0670 01052000
*    (GPR10F,                                                      0670 01053000
*     GPR14F) RSTD;                 /*                       @Z30APGE*/ 01054000
*   STM(GPR10F,GPR14F,CRRSAVE);     /* SAVE REGS ACROSS SETLOCK    0671 01055000
*                                                            @Z30APGE*/ 01056000
         STM   GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0671 01057000
*   GPR10F=CRRPTR;                  /* SAVE REG 13           @Z30APGE*/ 01058000
         LR    GPR10F,CRRPTR                                       0672 01059000
*C11LOCK:                                                          0673 01060000
*   DO;                             /* SETLOCK (OBTAIN) MODE(UNCOND)    01061000
*                                      TYPE(LOCAL) RELATED(LOCAL,ISTD   01062000
*                                      CC11(C11UNLCK))               */ 01063000
C11LOCK  DS    0H                                                  0674 01064000
*     RFY                                                          0674 01065000
*      (GPR11P,                                                    0674 01066000
*       GPR12P,                                                    0674 01067000
*       GPR13P,                                                    0674 01068000
*       GPR14P) RSTD;                                              0674 01069000
*     GPR13P=PSALITA;               /* ADDRESS OF LOCK INTERFACE   0675 01070000
*                                      TABLE.                        */ 01071000
         L     GPR13P,PSALITA                                      0675 01072000
*     GEN(LM GPR11P,GPR13P,576(GPR13P));/* LOAD LOCKWORD, CLHT AND 0676 01073000
*                                      ENTRY ADRS.                   */ 01074000
         LM GPR11P,GPR13P,576(GPR13P)                                   01075000
*     GEN(BALR GPR14P,GPR13P);      /* BALR TO SETLOCK RTN.          */ 01076000
         BALR GPR14P,GPR13P                                             01077000
*     RFY                                                          0678 01078000
*      (GPR11P,                                                    0678 01079000
*       GPR12P,                                                    0678 01080000
*       GPR13P,                                                    0678 01081000
*       GPR14P) UNRSTD;                                            0678 01082000
*   END;                            /* SETLOCK (OBTAIN) MODE(UNCOND)    01083000
*                                      TYPE(LOCAL) RELATED(LOCAL,ISTD   01084000
*                                      CC11(C11UNLCK))       @Z30APGE*/ 01085000
*   CRRPTR=GPR10F;                  /* RESTORE REG 13        @Z30APGE*/ 01086000
         LR    CRRPTR,GPR10F                                       0680 01087000
*   LM(GPR10F,GPR14F,CRRSAVE);      /* RESTORE REGS          @Z30APGE*/ 01088000
         LM    GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0681 01089000
*   CRRPTR=ADDR(CRRSAVE1);          /* SAVE AREA FOR MOVE ROUTINE    */ 01090000
         LA    @15,CRRSAVE1(,CRRPTR)                               0682 01091000
         LR    CRRPTR,@15                                          0682 01092000
*   RFY                                                            0683 01093000
*    (GPR10F,                                                      0683 01094000
*     GPR14F) UNRSTD;               /*                       @Z30APGE*/ 01095000
*   CALL ATCDCC60;                  /* INVOKE MOVE ROUTINE           */ 01096000
         L     @15,ATCDCC60(,ATCVTPTR)                             0684 01097000
         BALR  @14,@15                                             0684 01098000
*   CRRPTR=ADDR(CRRSPACE);          /* REBASE CRR            @D8388NL*/ 01099000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0685 01100000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0685 01101000
*   RPLRLEN=CRRBTLEN;               /* SET AMT OF DATA MOVED IN RPL  */ 01102000
         L     @15,CRRARPL(,CRRPTR)                                0686 01103000
         L     @12,CRRBTLEN(,CRRPTR)                               0686 01104000
         ST    @12,RPLRLEN(,@15)                                   0686 01105000
*   RFY                                                            0687 01106000
*    (GPR10F,                                                      0687 01107000
*     GPR14F) RSTD;                 /*                       @Z30APGE*/ 01108000
*   STM(GPR10F,GPR14F,CRRSAVE);     /* SAVE REGS ACROSS SETLOCK    0688 01109000
*                                                            @Z30APGE*/ 01110000
         STM   GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0688 01111000
*   GPR10F=CRRPTR;                  /* SAVE REG 13           @Z30APGE*/ 01112000
         LR    GPR10F,CRRPTR                                       0689 01113000
*C11UNLCK:                                                         0690 01114000
*   DO;                             /* SETLOCK (RELEASE) TYPE(LOCAL)    01115000
*                                      RELATED(LOCAL,ISTDCC11(C11LOCK   01116000
*                                      ))                            */ 01117000
C11UNLCK DS    0H                                                  0691 01118000
*     RFY                                                          0691 01119000
*      (GPR11P,                                                    0691 01120000
*       GPR12P,                                                    0691 01121000
*       GPR13P,                                                    0691 01122000
*       GPR14P) RSTD;                                              0691 01123000
*     GPR13P=PSALITA;               /* ADDRESS OF LOCK INTERFACE   0692 01124000
*                                      TABLE.                        */ 01125000
         L     GPR13P,PSALITA                                      0692 01126000
*     GEN(LM GPR11P,GPR13P,588(GPR13P));/* LOAD LOCKWORD, CLHT AND 0693 01127000
*                                      ENTRY ADRS.                   */ 01128000
         LM GPR11P,GPR13P,588(GPR13P)                                   01129000
*     GEN(BALR GPR14P,GPR13P);      /* BALR TO SETLOCK RTN.          */ 01130000
         BALR GPR14P,GPR13P                                             01131000
*     RFY                                                          0695 01132000
*      (GPR11P,                                                    0695 01133000
*       GPR12P,                                                    0695 01134000
*       GPR13P,                                                    0695 01135000
*       GPR14P) UNRSTD;                                            0695 01136000
*   END;                            /* SETLOCK (RELEASE) TYPE(LOCAL)    01137000
*                                      RELATED(LOCAL,ISTDCC11(C11LOCK   01138000
*                                      ))                    @Z30APGE*/ 01139000
*   CRRPTR=GPR10F;                  /* RESTORE REG 13        @Z30APGE*/ 01140000
         LR    CRRPTR,GPR10F                                       0697 01141000
*   LM(GPR10F,GPR14F,CRRSAVE);      /* RESTORE REGS          @Z30APGE*/ 01142000
         LM    GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0698 01143000
*   RFY                                                            0699 01144000
*    (GPR10F,                                                      0699 01145000
*     GPR14F) UNRSTD;               /*                       @Z30APGE*/ 01146000
*                                                                  0699 01147000
*   /*****************************************************************/ 01148000
*   /*                                                               */ 01149000
*   /* THE MOVE ROUTINE WILL RETURN A CODE OF 0, 4 , OR 8 TO INDICATE*/ 01150000
*   /* THE STATUS AFTER THE MOVE. A CODE OF 8 INDICATES THAT NO DATA */ 01151000
*   /* WAS MOVED DUE TO AN ERROR CONDITION. THE MOVE ROUTINE SET THE */ 01152000
*   /* ERROR STATUS IN THE CRR. THE FSB SHOULD REMAIN ON THE DATA    */ 01153000
*   /* QUEUE. A CODE OF 4 INDICATES THAT THE USER'S BUFFER WAS       */ 01154000
*   /* FILLED, BUT THERE IS MORE DATA IN THE FSB. CSUBR10 WILL BE    */ 01155000
*   /* INVOKED TO HANDLE THIS CONDITION. A CODE OF 0 INDICATES THAT  */ 01156000
*   /* ALL THE DATA ASSOCIATED WITH ONE FSB WAS MOVED. CSUBR9 WILL BE*/ 01157000
*   /* INVOKED TO DETERMINE IF ANOTHER FSB SHOULD BE REFERENCED AND  */ 01158000
*   /* THE MOVE SHOULD BE CONTINUED.                                 */ 01159000
*   /*                                                               */ 01160000
*   /*****************************************************************/ 01161000
*                                                                  0700 01162000
*   IF CRRBRTCD=CRRBEROR THEN                                      0700 01163000
         CLI   CRRBRTCD(CRRPTR),8                                  0700 01164000
         BNE   @RF00700                                            0700 01165000
*     DO;                           /* ERROR - NO DATA WAS MOVED     */ 01166000
*                                                                  0701 01167000
*       /*************************************************************/ 01168000
*       /*                                                           */ 01169000
*       /* ERROR-NO DATA MOVED                                       */ 01170000
*       /*                                                           */ 01171000
*       /*************************************************************/ 01172000
*                                                                  0702 01173000
*       CRRCLDO=CRRBLDAD;           /* SAVE INVALID LDO              */ 01174000
         L     @15,CRRBLDAD(,CRRPTR)                               0702 01175000
         ST    @15,CRRCLDO(,CRRPTR)                                0702 01176000
*       CRRSW3=ON;                  /* INDICATE MOVE COMPLETED       */ 01177000
         OI    CRRSW3(CRRPTR),B'00010000'                          0703 01178000
*     END;                                                         0704 01179000
*   ELSE                                                           0705 01180000
*     DO;                           /* DATA WAS MOVED                */ 01181000
*                                                                  0705 01182000
         B     @RC00700                                            0705 01183000
@RF00700 DS    0H                                                  0706 01184000
*       /*************************************************************/ 01185000
*       /*                                                           */ 01186000
*       /* SET FEEDBACK STATUS IN CRR FROM FSB                       */ 01187000
*       /*                                                           */ 01188000
*       /*************************************************************/ 01189000
*                                                                  0706 01190000
*       CRRRTNCD=FSBSRCAT;          /* MAJOR RETURN CODE             */ 01191000
         MVC   CRRRTNCD(1,CRRPTR),FSBSRCAT(FSBPTR)                 0706 01192000
*       CRRFDB2=FSBMNRCD;           /* MINOR RETURN CODE             */ 01193000
         MVC   CRRFDB2(1,CRRPTR),FSBMNRCD(FSBPTR)                  0707 01194000
*       CRRFDB3=FSBERC;             /* DATA FLAGS                    */ 01195000
         MVC   CRRFDB3(1,CRRPTR),FSBERC(FSBPTR)                    0708 01196000
*       CRRESR1=FSBDMGRS;           /* SYSTEM RESPONSE               */ 01197000
         MVC   CRRESR1(1,CRRPTR),FSBDMGRS(FSBPTR)                  0709 01198000
*       CRRESR2=FSBEXTRS;           /* EXTENDED SYSTEM RESPONSE      */ 01199000
         MVC   CRRESR2(1,CRRPTR),FSBEXTRS(FSBPTR)                  0710 01200000
*       IF CRRRTNCD=CRRPHYSC THEN                                  0711 01201000
         CLI   CRRRTNCD(CRRPTR),X'0C'                              0711 01202000
         BNE   @RF00711                                            0711 01203000
*         IF CRRSTSAV=ON THEN                                      0712 01204000
         TM    CRRSTSAV(CRRPTR),B'00000001'                        0712 01205000
         BNO   @RF00712                                            0712 01206000
*           CRRDSB=FSBDMGRS;        /* COPY SENSE DATA               */ 01207000
         MVC   CRRDSB(2,CRRPTR),FSBDMGRS(FSBPTR)                   0713 01208000
*         ELSE                                                     0714 01209000
*           ;                       /* NO SENSE DATA TO MOVE         */ 01210000
@RF00712 DS    0H                                                  0715 01211000
*       ELSE                                                       0715 01212000
*         ;                                                        0715 01213000
@RF00711 DS    0H                                                  0716 01214000
*       IF CRRBRTCD=CRRBMDAT THEN   /* BUFFER WAS FILLED             */ 01215000
*                                                                  0716 01216000
         CLI   CRRBRTCD(CRRPTR),4                                  0716 01217000
         BNE   @RF00716                                            0716 01218000
*         /***********************************************************/ 01219000
*         /*                                                         */ 01220000
*         /* BUFFER FILLED-NOT ALL DATA MOVED                        */ 01221000
*         /*                                                         */ 01222000
*         /***********************************************************/ 01223000
*                                                                  0717 01224000
*         CALL CSUBR10;             /* CHECK TRUNC/KEEP OPTION       */ 01225000
*                                                                  0717 01226000
         BAL   @14,CSUBR10                                         0717 01227000
*       /*************************************************************/ 01228000
*       /*                                                           */ 01229000
*       /* ALL DATA FROM CURRENT FSB MOVED                           */ 01230000
*       /*                                                           */ 01231000
*       /*************************************************************/ 01232000
*                                                                  0718 01233000
*       ELSE                                                       0718 01234000
*         CALL CSUBR9;              /* ANALYZE FOR RETURN CODE 0     */ 01235000
         B     @RC00716                                            0718 01236000
@RF00716 BAL   @14,CSUBR9                                          0718 01237000
*     END;                                                         0719 01238000
@RC00716 DS    0H                                                  0720 01239000
*   RETPTR=CRRRTN6;                 /* GET RETURN ADDRESS            */ 01240000
@RC00700 L     RETPTR,CRRRTN6(,CRRPTR)                             0720 01241000
*   END CSUBR7;                     /* RETURN TO CALLER              */ 01242000
@EL00008 DS    0H                                                  0721 01243000
@EF00008 DS    0H                                                  0721 01244000
@ER00008 BR    @14                                                 0721 01245000
*FREEFSB:                                                          0722 01246000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0722 01247000
FREEFSB  DS    0H                                                  0723 01248000
*   CRRRTN8=RETPTR;                 /* SAVE RETURN ADDRESS           */ 01249000
         ST    RETPTR,CRRRTN8(,CRRPTR)                             0723 01250000
*/********************************************************************/ 01251000
*/*                                                                  */ 01252000
*/* ROUTINE NAME : FREEFSB - FREE THE FSB/FDB BUFFERS                */ 01253000
*/*                                                                  */ 01254000
*/* FUNCTION :                                                       */ 01255000
*/*    THIS ROUTINE WILL DEQUEUE AND RELEASE THE STORAGE FOR AT LEAST*/ 01256000
*/*    ONE FSB AND THE FDBS ASSOCIATED WITH IT.                      */ 01257000
*/*    IF CRRSW0 IS ON INDICATING A FLUSH CONDITION (ON FOR TRUNC)   */ 01258000
*/*    THEN ALL FSB'S WILL BE DEQUEUED AND FREED UNTIL AND ENDING    */ 01259000
*/*    CONDITION IS ENCOUNTERED OR UNTIL THERE ARE NO MORE FSB'S ON  */ 01260000
*/*    THE FMCB DATA QUEUE.  IF THE QUEUE IS EXHAUSTED, THE FLUSH    */ 01261000
*/*    BIT IS TURNED ON IN THE FMCB TO TELL FEEDBACK TO THROW AWAY   */ 01262000
*/*    FSB'S UNTIL THE ENDING CONDITION IS MET.                      */ 01263000
*/*                                                                  */ 01264000
*/* INPUT :  LDOPTR (REG 10) - ADDRESS OF LDO                        */ 01265000
*/*                                                                  */ 01266000
*/* OUTPUT :   FSB/FDB CHAIN IS RELEASED AND ERROR LOCK RESET        */ 01267000
*/*                                                                  */ 01268000
*/* SUBROUTINES CALLED :                                             */ 01269000
*/*    LCPBINIT - BUILD AND SCHEDULE RESET ERROR LOCK                */ 01270000
*/*                                                                  */ 01271000
*/*  MACROS USED :  TPDEQ, LCPBINIT                                  */ 01272000
*/*                                                                  */ 01273000
*/********************************************************************/ 01274000
*                                                                  0724 01275000
*   CRRSW1=ON;                      /* SET UP FOR DO WHILE LOOP      */ 01276000
         OI    CRRSW1(CRRPTR),B'01000000'                          0724 01277000
*   DO WHILE(CRRSW1=ON);            /* FREE FSB'S                    */ 01278000
         B     @DE00725                                            0725 01279000
@DL00725 DS    0H                                                  0726 01280000
*/* TPDEQ (,FMCFBA) RETPTR(FSBPTR)                                   */ 01281000
*                                                                  0726 01282000
*     DO;                           /* BEGIN TPDEQ CODE GENERATION   */ 01283000
*       RFY                                                        0727 01284000
*        (GPR00P,                                                  0727 01285000
*         GPR01P,                                                  0727 01286000
*         GPR14P,                                                  0727 01287000
*         GPR15P) RSTD;                                            0727 01288000
*       GPR15P=ADDR(FMCFBA);        /* ADDRESS QUEUE POINTER         */ 01289000
         L     GPR15P,CRRFMCB(,CRRPTR)                             0728 01290000
         LA    GPR15P,FMCFBA(,GPR15P)                              0728 01291000
*       GPR14P=GPR15P->PABWEQA|FMZCA020;                           0729 01292000
         L     @12,@CF02549                                        0729 01293000
         L     GPR14P,PABWEQA(,GPR15P)                             0729 01294000
         OR    GPR14P,@12                                          0729 01295000
*       IF GPR14P=FMZCA020 THEN                                    0730 01296000
         CLR   GPR14P,@12                                          0730 01297000
         BE    @RT00730                                            0730 01298000
*         ;                                                        0731 01299000
*       ELSE                                                       0732 01300000
*         DO;                                                      0732 01301000
*FMZL1028:                                                         0733 01302000
*           ;                                                      0733 01303000
FMZL1028 DS    0H                                                  0734 01304000
*           DO WHILE(GPR14P->WKECHAIN^=FMZCA020);                  0734 01305000
         B     @DE00734                                            0734 01306000
@DL00734 DS    0H                                                  0735 01307000
*             GPR15P=ADDR(GPR14P->WKECHAIN);                       0735 01308000
         LA    GPR15P,WKECHAIN(,GPR14P)                            0735 01309000
*             GPR14P=GPR15P->PABWEQA;                              0736 01310000
         L     GPR14P,PABWEQA(,GPR15P)                             0736 01311000
*           END;                                                   0737 01312000
@DE00734 L     @12,@CF02549                                        0737 01313000
         CL    @12,WKECHAIN(,GPR14P)                               0737 01314000
         BNE   @DL00734                                            0737 01315000
*           GPR00P=FMZCA020;                                       0738 01316000
         LR    GPR00P,@12                                          0738 01317000
*           CS(GPR14P,GPR00P,GPR15P->PABWEQA);                     0739 01318000
         CS    GPR14P,@00,PABWEQA(GPR15P)                          0739 01319000
*           BC(7,FMZL1028);                                        0740 01320000
         BC    7,FMZL1028                                          0740 01321000
*           GPR14P->WKECHAIN=0;                                    0741 01322000
         SLR   @12,@12                                             0741 01323000
         ST    @12,WKECHAIN(,GPR14P)                               0741 01324000
*         END;                                                     0742 01325000
*ISTA0028:                                                         0743 01326000
*       ;                                                          0743 01327000
@RT00730 DS    0H                                                  0743 01328000
ISTA0028 DS    0H                                                  0744 01329000
*       GPR14P=GPR14P&FMZCB020;                                    0744 01330000
         N     GPR14P,@CF02547                                     0744 01331000
*       FSBPTR=GPR14P;                                             0745 01332000
         LR    FSBPTR,GPR14P                                       0745 01333000
*       RFY                                                        0746 01334000
*        (GPR00P,                                                  0746 01335000
*         GPR01P,                                                  0746 01336000
*         GPR14P,                                                  0746 01337000
*         GPR15P) UNRSTD;                                          0746 01338000
*     END;                          /* END TPDEQ GENERATION          */ 01339000
*     IF FSBPTR=0|FSBPTR=GATE0 THEN                                0748 01340000
         LTR   FSBPTR,FSBPTR                                       0748 01341000
         BZ    @RT00748                                            0748 01342000
         CL    FSBPTR,@CF02549                                     0748 01343000
         BNE   @RF00748                                            0748 01344000
@RT00748 DS    0H                                                  0749 01345000
*       DO;                         /* NO MORE FSB'S ON DATA QUEUE   */ 01346000
*                                                                  0749 01347000
*         /***********************************************************/ 01348000
*         /*                                                         */ 01349000
*         /* NO MORE FSB'S ON DATA QUEUE                             */ 01350000
*         /*                                                         */ 01351000
*         /***********************************************************/ 01352000
*                                                                  0750 01353000
*         RFY                                                      0750 01354000
*          (REG10,                                                 0750 01355000
*           REG11) RSTD;                                           0750 01356000
*CS3:                                                              0751 01357000
*         REG10=FMCCMPS2;           /* REFERENCE OLD FLAG VALUE      */ 01358000
CS3      L     @15,CRRFMCB(,CRRPTR)                                0751 01359000
         L     REG10,FMCCMPS2(,@15)                                0751 01360000
*         REG11=REG10|FLGA3ON;      /* TURN ON FLUSH FLAG            */ 01361000
         LR    REG11,REG10                                         0752 01362000
         O     REG11,@CF02565                                      0752 01363000
*         CS(REG10,REG11,FMCCMPS2); /* SET NEW FLAG IN FMCB          */ 01364000
         CS    REG10,@11,FMCCMPS2(@15)                             0753 01365000
*         BC(7,CS3);                /* TRY AGIN IF FAILURE           */ 01366000
         BC    7,CS3                                               0754 01367000
*         RFY                                                      0755 01368000
*          (REG10,                                                 0755 01369000
*           REG11) UNRSTD;                                         0755 01370000
*         CRRSW0=OFF;               /* SET END OF LOOP               */ 01371000
         NI    CRRSW0(CRRPTR),B'01111111'                          0756 01372000
*       END;                                                       0757 01373000
*     ELSE                                                         0758 01374000
*       DO;                         /* FSB FOUND ON QUEUE            */ 01375000
*                                                                  0758 01376000
         B     @RC00748                                            0758 01377000
@RF00748 DS    0H                                                  0759 01378000
*         /***********************************************************/ 01379000
*         /*                                                         */ 01380000
*         /* FSB PRESENT ON QUEUE                                    */ 01381000
*         /*                                                         */ 01382000
*         /***********************************************************/ 01383000
*                                                                  0759 01384000
*         IF FSBEBI=ON|FSBLCPB=ZERO THEN                           0759 01385000
         TM    FSBEBI(FSBPTR),B'00100000'                          0759 01386000
         BO    @RT00759                                            0759 01387000
         L     @15,FSBLCPB(,FSBPTR)                                0759 01388000
         LTR   @15,@15                                             0759 01389000
         BNZ   @RF00759                                            0759 01390000
@RT00759 DS    0H                                                  0760 01391000
*           CRRSW0=OFF;             /* RESET FLUSH CONDITION         */ 01392000
         NI    CRRSW0(CRRPTR),B'01111111'                          0760 01393000
*         ELSE                                                     0761 01394000
*           ;                       /* ENDING CONDITION NOT FOUND    */ 01395000
@RF00759 DS    0H                                                  0762 01396000
*         IF FSBMNELI=ON THEN       /* ERROR LOCK SET IN FSB?        */ 01397000
         TM    FSBMNELI(FSBPTR),B'10000000'                        0762 01398000
         BNO   @RF00762                                            0762 01399000
*           DO;                                                    0763 01400000
*             CRRELSRI=ON;          /* INDICATE ERROR LOCK SET       */ 01401000
         OI    CRRELSRI(CRRPTR),B'00001000'                        0764 01402000
*             CRRSW0=OFF;           /* EXIT LOOP                     */ 01403000
         NI    CRRSW0(CRRPTR),B'01111111'                          0765 01404000
*           END;                                                   0766 01405000
*         ELSE                                                     0767 01406000
*           ;                       /* ERROR LOCK NOT SET            */ 01407000
*                                                                  0767 01408000
@RF00762 DS    0H                                                  0768 01409000
*         /***********************************************************/ 01410000
*         /*                                                         */ 01411000
*         /* DECREMENT COUNT OF NUMBER OF BUFFERS IN USE BY NUMBER   */ 01412000
*         /* NOW BEING FREED                                         */ 01413000
*         /*                                                         */ 01414000
*         /***********************************************************/ 01415000
*                                                                  0768 01416000
*         RFY                                                      0768 01417000
*          (FDBPTR,                                                0768 01418000
*           WORKA,                                                 0768 01419000
*           COUNTER) RSTD;                                         0768 01420000
*         FDBPTR=ADDR(FSBFDB);      /* REFERENCE FIRST FDB           */ 01421000
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0769 01422000
*         COUNTER=0;                /* CLEAR BUFFER COUNT ACCUMULATOR*/ 01423000
         SLR   COUNTER,COUNTER                                     0770 01424000
*         DO WHILE(FDBPTR^=0);      /* LOOP THROUGH ALL FDB'S        */ 01425000
         B     @DE00771                                            0771 01426000
@DL00771 DS    0H                                                  0772 01427000
*           COUNTER=COUNTER+1;      /* ACCUMULATE BUFFER COUNT       */ 01428000
         AL    COUNTER,@CF00105                                    0772 01429000
*           FDBPTR=FDBFDBA;         /* REFERENCE NEXT FDB            */ 01430000
         L     FDBPTR,FDBFDBA(,FDBPTR)                             0773 01431000
*         END;                                                     0774 01432000
@DE00771 LTR   FDBPTR,FDBPTR                                       0774 01433000
         BNZ   @DL00771                                            0774 01434000
*         RFY                                                      0775 01435000
*          (REG10,                                                 0775 01436000
*           REG11) RSTD;                                           0775 01437000
*CS5:                                                              0776 01438000
*         REG10=FMCCMPS1;           /* REFERENCE CURRENT COUNT       */ 01439000
CS5      L     @15,CRRFMCB(,CRRPTR)                                0776 01440000
         L     REG10,FMCCMPS1(,@15)                                0776 01441000
*         REG11=REG10-COUNTER;      /* DECREMENT BUFFER COUNT        */ 01442000
         LR    REG11,REG10                                         0777 01443000
         SLR   REG11,COUNTER                                       0777 01444000
*         CS(REG10,REG11,FMCCMPS1); /* SET NEW COUNT IN FMCB         */ 01445000
         CS    REG10,@11,FMCCMPS1(@15)                             0778 01446000
*         BC(7,CS5);                /* TRY AGAIN IF FAILURE          */ 01447000
         BC    7,CS5                                               0779 01448000
*         RFY                                                      0780 01449000
*          (REG10,                                                 0780 01450000
*           REG11) UNRSTD;                                         0780 01451000
*                                                                  0780 01452000
*         /***********************************************************/ 01453000
*         /*                                                         */ 01454000
*         /* FREE STORAGE FOR FSB AND ALL FDBS                       */ 01455000
*         /*                                                         */ 01456000
*         /***********************************************************/ 01457000
*                                                                  0781 01458000
*         FDBPTR=ADDR(FSBFDB);                                     0781 01459000
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0781 01460000
*         WORKA=FDBPTR-FDBOFSET;    /* START OF AREA TO RELEASE      */ 01461000
         LR    WORKA,FDBPTR                                        0782 01462000
         SH    WORKA,FDBOFSET(,FDBPTR)                             0782 01463000
*         WORKA->FDBFDBA=FDBFDBA;   /* UPDATE CHAIN PTR TO NEXT FDB  */ 01464000
         L     @15,FDBFDBA(,FDBPTR)                                0783 01465000
         ST    @15,FDBFDBA(,WORKA)                                 0783 01466000
*         IF PROCFTX=ON THEN                                       0784 01467000
*                                                                  0784 01468000
         L     @15,CRRFMCB(,CRRPTR)                                0784 01469000
         TM    PROCFTX+48(@15),B'00000001'                         0784 01470000
         BNO   @RF00784                                            0784 01471000
*           /*********************************************************/ 01472000
*           /*                                                       */ 01473000
*           /* RELSTORE BUFADDR(WORKA)CHAIN(YES)CLEAR(YES)           */ 01474000
*           /*                                                       */ 01475000
*           /*********************************************************/ 01476000
*                                                                  0785 01477000
*           DO;                                                    0785 01478000
*                                                                  0785 01479000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 01480000
*/* END OF ATBUILD                                                   */ 01481000
*                                                                  0786 01482000
*             RFY                                                  0786 01483000
*              (GPR02P,                                            0786 01484000
*               GPR03P,                                            0786 01485000
*               GPR04P,                                            0786 01486000
*               GPR15P) RSTD;                                      0786 01487000
*             RFY                                                  0787 01488000
*              (GPR01P,                                            0787 01489000
*               GPR13P) RSTD;                                      0787 01490000
*             GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);   0788 01491000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             01492000
*             RFY                                                  0789 01493000
*               GPR13P UNRSTD;                                     0789 01494000
*             GPR04P=WORKA|'80000000'X;                            0790 01495000
         LR    @12,WORKA                                           0790 01496000
         O     @12,@CF02549                                        0790 01497000
         LR    GPR04P,@12                                          0790 01498000
*             DCL                                                  0791 01499000
*               1 FMZA0032 FIXED(31) STATIC LOCAL,                 0791 01500000
*                2 * FIXED(8) INIT(80),                            0791 01501000
*                2 * PTR(24) INIT(0);                              0791 01502000
*             GPR03F=FMZA0032;                                     0792 01503000
         L     GPR03F,FMZA0032                                     0792 01504000
*             DO;                   /* LISATCVT                    0793 01505000
