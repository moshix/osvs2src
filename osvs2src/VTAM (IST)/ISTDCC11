         TITLE 'ISTDCC11 - READ LDO REQUEST                            *00001000
                        '                                               00002000
ISTDCC11 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC11  75.346'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @08,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@08                                         0001 00012000
*                                                                  0467 00013000
*/********************************************************************/ 00014000
*/*                                                                  */ 00015000
*/*           INFORMATION TABLE FOR MAIN ROUTINE                     */ 00016000
*/*                                                                  */ 00017000
*/********************************************************************/ 00018000
*/*                                                                  */ 00019000
*/*                       CONTROL BLOCKS                             */ 00020000
*/*------------------------------------------------------------------*/ 00021000
*/* CB    FIELD        S/R/T (SET,REF,TEST)      COMMENTS            */ 00022000
*/*------------------------------------------------------------------*/ 00023000
*/* RPH                                                              */ 00024000
*/*       RPHCRR         R    POINTER TO CONTROL LAYER'S CRR         */ 00025000
*/*------------------------------------------------------------------*/ 00026000
*/* CRR                                                              */ 00027000
*/*       CRRC11         S    HISTORY AUDIT TRAIL SWITCH             */ 00028000
*/*       CRRMAS         S    MODULE AUDIT TRAIL SWITCHES            */ 00029000
*/*       CRRM11         S    ISTDCC11'S MODULE AUDIT TRAIL SWITCH   */ 00030000
*/*       CRRCLDO        R    POINTER TO CURRENT LDO TO PROCESS      */ 00031000
*/*       CRRFMCB        R    POINTER TO FMCB                        */ 00032000
*/*       CRRFINI        S    INDICATES REQUEST PROCESSING FINISHED  */ 00033000
*/*       CRRRTNCD       S    MAJOR RETURN CODE FOR FEEDBACK         */ 00034000
*/*       CRRFDB2        S    MINOR RETURN CODE FOR FEEDBACK         */ 00035000
*/*       CRRSPACE       R    BASE FOR CRRSAREA                      */ 00036000
*/*------------------------------------------------------------------*/ 00037000
*/* DEVCH                                                            */ 00038000
*/*       DEVINPUT       T    SEE IF INPUT DEVICE FOR READ           */ 00039000
*/********************************************************************/ 00040000
*/*                                                                  */ 00041000
*/*                             REGISTERS                            */ 00042000
*/*------------------------------------------------------------------*/ 00043000
*/* RPHPTR (REGISTER 1) INPUT PARAMETER WHICH POINTS TO THE RPH      */ 00044000
*/* LDOPTR (REGISTER 9) POINTER TO CURRENT LDO TO BE PROCESSED       */ 00045000
*/********************************************************************/ 00046000
*/*                                                                  */ 00047000
*/*                              MACROS                              */ 00048000
*/*------------------------------------------------------------------*/ 00049000
*/* TPESC - USED TO TRANSFER CONTROL TO THE NEXT DVT ENTRY           */ 00050000
*/********************************************************************/ 00051000
*/*                                                                  */ 00052000
*/*                              CALLS                               */ 00053000
*/*------------------------------------------------------------------*/ 00054000
*/* CSUBR1 - PROCESS READ LDO IF DEVICE IS AN INPUT DEVICE           */ 00055000
*/********************************************************************/ 00056000
*                                                                  0467 00057000
*   /*****************************************************************/ 00058000
*   /*                                                               */ 00059000
*   /* INITIALIZE POINTERS AND SET AUDIT TRAIL SWITCHES              */ 00060000
*   /*                                                               */ 00061000
*   /*****************************************************************/ 00062000
*                                                                  0467 00063000
*   CRRPTR=ADDR(CRRSPACE);          /* REFERENCE CRR                 */ 00064000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0467 00065000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0467 00066000
*   CRRC11=ON;                      /* INITIALIZE HISTORY SWITCHES   */ 00067000
         OI    CRRC11(CRRPTR),B'00001000'                          0468 00068000
*   CRRMAS=CRRMAS&&CRRMAS;          /* CLEAR MODULE AUDIT SWITCHES   */ 00069000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0469 00070000
*   CRRM11=ON;                      /* INDICATE ISTDCC11 PROCESSING  */ 00071000
         OI    CRRM11(CRRPTR),B'00001000'                          0470 00072000
*   LDOPTR=CRRCLDO;                 /* REFERENCE CURRENT LDO         */ 00073000
*                                                                  0471 00074000
         L     LDOPTR,CRRCLDO(,CRRPTR)                             0471 00075000
*   /*****************************************************************/ 00076000
*   /*                                                               */ 00077000
*   /* IF FMCB INDICATES THAT THE CURRENT DEVICE TO BE PROCESSED IS  */ 00078000
*   /* AN OUTPUT ONLY DEVICE, PROCESSING OF READ LDO'S MUST BE       */ 00079000
*   /* TERMINATED WITH AN ERROR INDICATION.                          */ 00080000
*   /*                                                               */ 00081000
*   /*****************************************************************/ 00082000
*                                                                  0472 00083000
*   IF DEVINPUT=OFF&                /* SEE IF DEVICE FOR INPUT       */ 00084000
*       DEVCHAR2^=DEV3284&DEVCHAR2^=DEV3286 THEN                   0472 00085000
         L     @15,CRRFMCB(,CRRPTR)                                0472 00086000
         LA    @12,36                                              0472 00087000
         AL    @12,FMCDNCBA(,@15)                                  0472 00088000
         TM    DEVINPUT(@12),B'10000000'                           0472 00089000
         BNZ   @RF00472                                            0472 00090000
         CLI   DEVCHAR2(@12),X'1A'                                 0472 00091000
         BE    @RF00472                                            0472 00092000
         CLI   DEVCHAR2(@12),X'1B'                                 0472 00093000
         BE    @RF00472                                            0472 00094000
*     DO;                           /* OUTPUT ONLY DEVICE            */ 00095000
*                                                                  0473 00096000
*       /*************************************************************/ 00097000
*       /*                                                           */ 00098000
*       /* ERROR-NOT INPUT DEVICE                                    */ 00099000
*       /*                                                           */ 00100000
*       /*************************************************************/ 00101000
*                                                                  0474 00102000
*       CRRFDB2=CRRRTOOD;           /* SET REASON CODE FOR FEEDBACK  */ 00103000
         MVI   CRRFDB2(CRRPTR),X'10'                               0474 00104000
*       CRRRTNCD=CRRLOGIC;          /* SET LOGICAL I/O ERROR CODE    */ 00105000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0475 00106000
*       CRRFINI=ON;                 /* INDICATE DO PROCESSING      0476 00107000
*                                      FINISHED                      */ 00108000
         OI    CRRFINI(CRRPTR),B'00001000'                         0476 00109000
*     END;                                                         0477 00110000
*   ELSE                                                           0478 00111000
*     CALL CSUBR1;                  /* HANDLE LDO'S                  */ 00112000
         B     @RC00472                                            0478 00113000
@RF00472 BAL   @14,CSUBR1                                          0478 00114000
*/*TPESC                                                             */ 00115000
*                                                                  0479 00116000
*   DO;                                                            0479 00117000
*                                                                  0479 00118000
@RC00472 DS    0H                                                  0480 00119000
*/* ATBUILD (ISTDYPAB,ISTAVT,ISTATCVT)                               */ 00120000
*                                                                  0480 00121000
*/* VTAM ADDRESS VECTOR TABLE */                                   0480 00122000
*/*  REVISIONS                                         002 03/06/75  */ 00123000
*DCL  1  ISTAVT BDY(WORD) BASED,                                   0480 00124000
*       2  ISTACVT PTR(31),            /* ADDR OF VTAM CVT           */ 00125000
*       2  ISTAS49 PTR(31),            /* ADDR OF SVC 49 CODE        */ 00126000
*          3  ISTAHIP BIT(1),          /* HALT IN PROGRESS           */ 00127000
*       2  ISTAS53 PTR(31),            /* ADDR OF SVC 53 CODE        */ 00128000
*       2  ISTAPSTA PTR(31),           /* ADDR OF APS TABLE          */ 00129000
*       2  AVTASID FIXED(15),          /* VTAM ASID                  */ 00130000
*       2  ISTVTTIK FIXED(15),         /* VTAMRP TASK ID             */ 00131000
*       2  ISTAPSEX PTR(31),           /* ADDR OF APS EXIT           */ 00132000
*       2  ISTPHNM CHAR(5),            /* PHASE NAME OF TRANSIENT    */ 00133000
*       2  ISTX1 CHAR(1),              /* TEST FIELD                 */ 00134000
*       2  ISTARID PTR(15),            /* ADDR OF RID                */ 00135000
*       2  ISTATRT PTR(15),            /* ADDR OF VTAM GATES IN RETAB*/ 00136000
*       2  ISTAGTWT PTR(15),           /* ADDR OF GATEWAIT ROUTINE   */ 00137000
*       2  ISTVTTP  PTR(15),           /* ADDR OF CODE TO CHECK FOR  */ 00138000
*                                      /* PENDING TIMER INTERRUPT    */ 00139000
*       2  ISTAVR01 CHAR(2),           /* RESERVED                   */ 00140000
*       2  ISTPDAVP PTR(31);           /* VECTOR FOR PDAIDS SMS TRACE*/ 00141000
*                                                                  0481 00142000
*/* END OF ATBUILD                                                   */ 00143000
*                                                                  0481 00144000
*     RFY                                                          0481 00145000
*       GPR15P RSTD;                                               0481 00146000
*     DO;                           /* LISATCVT                    0482 00147000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00148000
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00149000
         L     GPR15P,ATCLCPTR                                     0483 00150000
*     END;                          /* LISATCVT                      */ 00151000
*     GPR15P=GPR15P->ATCESC01;                                     0485 00152000
         L     GPR15P,ATCESC01(,GPR15P)                            0485 00153000
*     DCL                                                          0486 00154000
*       FMZL0008 BASED(GPR15P) LABEL VALUERANGE(*);                0486 00155000
*     CALL FMZL0008;                                               0487 00156000
         BALR  @14,@15                                             0487 00157000
*     RESPECIFY                                                    0488 00158000
*       GPR15P UNRSTD;                                             0488 00159000
*   END;                            /* **** END OF TPESC EXIT TO NEXT   00160000
*                                      DVT ENTRY                     */ 00161000
*CSUBR1:                                                           0490 00162000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0490 00163000
*                                                                  0490 00164000
         B     @PB00002                                            0490 00165000
CSUBR1   DS    0H                                                  0491 00166000
*/********************************************************************/ 00167000
*/*                                                                  */ 00168000
*/*              INFORMATION TABLE FOR CSUBR1                        */ 00169000
*/*                                                                  */ 00170000
*/********************************************************************/ 00171000
*/*                                                                  */ 00172000
*/*                           CONTROL BLOCKS                         */ 00173000
*/*------------------------------------------------------------------*/ 00174000
*/* CB    FIELD        S/R/T       COMMENTS                          */ 00175000
*/*------------------------------------------------------------------*/ 00176000
*/* CRR                                                              */ 00177000
*/*       CRRRTN1        S/R  SAVE AREA FOR CALLER'S RETURN ADDRESS  */ 00178000
*/*       CRRDOCUR       S    INDICATES READ OCCURRED FOR DO GROUP   */ 00179000
*/*       CRRFINI        T    INDICATES REQUEST PROCESSING FINISHED  */ 00180000
*/*       CRRSW0         S    INTERNAL CONTROL SWITCH                */ 00181000
*/*       CRRSW1         S    INTERNAL CONTROL SWITCH                */ 00182000
*/*       CRRSW2         S    INTERNAL CONTROL SWITCH                */ 00183000
*/********************************************************************  00184000
*                                                                    */ 00185000
*/*                                                                  */ 00186000
*/*                           REGISTERS                              */ 00187000
*/*------------------------------------------------------------------*/ 00188000
*/* RETPTR (REGISTER 14) CALLER'S RETURN ADDRESS                     */ 00189000
*/********************************************************************/ 00190000
*/*                                                                  */ 00191000
*/*                           NO MACROS                              */ 00192000
*/********************************************************************/ 00193000
*/*                                                                  */ 00194000
*/*                              CALLS                               */ 00195000
*/*------------------------------------------------------------------*/ 00196000
*/* CSUBR8 - IF A PREVIOUS READ HAS NOT OCCURRED, CSUBR8 IS CALLED   */ 00197000
*/*          TO VALIDATE THE LDO AND TO CALL CSUBR3 TO HANDLE READ   */ 00198000
*/*          MODIFIED OR READ BUFFER.                                */ 00199000
*/* CSUBR2 - IF NO ERRORS WERE FOUND IN CSUBR8 OR CSUBR3, CSUBR2 IS  */ 00200000
*/*         INVOKED TO CONTINUE PROCESSING FOR A NORMAL READ.        */ 00201000
*/********************************************************************  00202000
*                                                                    */ 00203000
*                                                                  0491 00204000
*   CRRRTN1=RETPTR;                 /* SAVE RETURN ADDRESS           */ 00205000
*                                                                  0491 00206000
         ST    RETPTR,CRRRTN1(,CRRPTR)                             0491 00207000
*   /*****************************************************************/ 00208000
*   /*                                                               */ 00209000
*   /* CSUBR8 IS CALLED TO VALIDATE THE LDO, AND CSUBR2 IS CALLED IF */ 00210000
*   /* THE LDO IS VALID AND IT WAS NOT A READ MODIFIED OR READ       */ 00211000
*   /* BUFFER.                                                       */ 00212000
*   /*                                                               */ 00213000
*   /*****************************************************************/ 00214000
*                                                                  0492 00215000
*   DO;                             /* MODESET EXTKEY(ZERO)          */ 00216000
*     SPKA(0);                      /* SET PSW KEY                   */ 00217000
         SLR   @15,@15                                             0493 00218000
         SPKA  0(@15)                                              0493 00219000
*   END;                            /* MODESET EXTKEY(ZERO) GET USER    00220000
*                                      KEY                   @VM40576*/ 00221000
*   CRRSW3=OFF;                     /* CLEAR STRUCTURE SWITCHES      */ 00222000
*   CRRSW2=OFF;                                                    0496 00223000
*   CRRSW1=OFF;                                                    0497 00224000
*   CRRSW0=OFF;                                                    0498 00225000
         NI    CRRSW3(CRRPTR),B'00001111'                          0498 00226000
*   CRRDOCUR=ON;                    /* INDICATE READ HAS OCCURRED    */ 00227000
         OI    CRRDOCUR(CRRPTR),B'10000000'                        0499 00228000
*   LCPBPTR=CRRLCPB;                /* REFERENCE LCPB                */ 00229000
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0500 00230000
*   CALL CSUBR8;                    /* LOCK AND VALIDATE LDO         */ 00231000
         BAL   @14,CSUBR8                                          0501 00232000
*   IF CRRFINI=ON THEN                                             0502 00233000
         TM    CRRFINI(CRRPTR),B'00001000'                         0502 00234000
         BO    @RT00502                                            0502 00235000
*     ;                             /* PROCESSING FINISHED           */ 00236000
*   ELSE                                                           0504 00237000
*     CALL CSUBR2;                  /* CONTINUE LDO PROCESSING       */ 00238000
         BAL   @14,CSUBR2                                          0504 00239000
*   DO;                             /* MODESET EXTKEY(VTAM)          */ 00240000
@RT00502 DS    0H                                                  0506 00241000
*     SPKA(96);                     /* SET PSW KEY                   */ 00242000
         LA    @15,96                                              0506 00243000
         SPKA  0(@15)                                              0506 00244000
*   END;                            /* MODESET EXTKEY(VTAM) GET VTAM    00245000
*                                      KEY VS30 122          @Z30APGE*/ 00246000
*   RETPTR=CRRRTN1;                 /* GET RETURN ADDRESS            */ 00247000
         L     RETPTR,CRRRTN1(,CRRPTR)                             0508 00248000
*   END CSUBR1;                     /* RETURN TO CALLER              */ 00249000
@EL00002 DS    0H                                                  0509 00250000
@EF00002 DS    0H                                                  0509 00251000
@ER00002 BR    @14                                                 0509 00252000
*CSUBR2:                                                           0510 00253000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0510 00254000
*                                                                  0510 00255000
CSUBR2   DS    0H                                                  0511 00256000
*/********************************************************************/ 00257000
*/*                                                                  */ 00258000
*/*              INFORMATION TABLE FOR CSUBR2                        */ 00259000
*/*                                                                  */ 00260000
*/********************************************************************/ 00261000
*/*                                                                  */ 00262000
*/*                           CONTROL BLOCKS                         */ 00263000
*/*------------------------------------------------------------------*/ 00264000
*/* CB    FIELD        S/R/T (SET,REF,TEST)      COMMENTS            */ 00265000
*/*------------------------------------------------------------------*/ 00266000
*/* CRR                                                              */ 00267000
*/*       CRRRTN2        S/R  SAVE AREA FOR CALLER'S RETURN ADDRESS  */ 00268000
*/*       CRRINIO        T    SEE IF THE LDO WAS PROCEEDED BY  WRITE */ 00269000
*/*                           WHICH WILL CAUSE I/O TO BE INITIATED   */ 00270000
*/*       CRRFINI        S    INDICATES REQUEST PROCESSING FINISHED  */ 00271000
*/*       CRRFSB         S    FSB POINTER OBTAINED FROM TPFEL        */ 00272000
*/*------------------------------------------------------------------*/ 00273000
*/* FMCB                                                             */ 00274000
*/*       FMCFBA         R    START OF FMCB DATA QUEUE               */ 00275000
*/*------------------------------------------------------------------*/ 00276000
*/* PROCD                                                            */ 00277000
*/*       PROMODE        T    DETERMINE WHETHER READ BLOCK, MESSAGE, */ 00278000
*/*                           TRANSMISSION OR CONTINUOUS LCCW IS TO  */ 00279000
*/*                           BE BUILT                               */ 00280000
*/*------------------------------------------------------------------*/ 00281000
*/* LCCW                                                             */ 00282000
*/*       LCCWRWOP       S    LCCW OP CODE                           */ 00283000
*/********************************************************************/ 00284000
*/*                                                                  */ 00285000
*/*                           REGISTERS                              */ 00286000
*/*------------------------------------------------------------------*/ 00287000
*/* FSBPTR - CONTAINS FSB POINTER AFTER TPFEL                        */ 00288000
*/* RETPTR (REGISTER 14) CALLER'S RETURN ADDRESS                     */ 00289000
*/********************************************************************/ 00290000
*/*                                                                  */ 00291000
*/*                              MACROS                              */ 00292000
*/*------------------------------------------------------------------*/ 00293000
*/* TPFEL - GET ADDRESS OF OLDEST FSB ON THE FMCB DATA QUEUE         */ 00294000
*/********************************************************************/ 00295000
*/*                                                                  */ 00296000
*/*                              CALLS                               */ 00297000
*/*------------------------------------------------------------------*/ 00298000
*/* CSUBR5 - IF LCCW WAS BUILT, CSUBR5 IS CALLED TO SET UP FOR I/O   */ 00299000
*/* CSUBR4 - IF NO FSB ON FMCB DATA QUEUE, SEE IF DATA EXPECTED      */ 00300000
*/* CSUBR6 - MOVE DATA FROM FSB TO USER'S BUFFER                     */ 00301000
*/********************************************************************/ 00302000
*                                                                  0511 00303000
*   CRRRTN2=RETPTR;                 /* SAVE RETURN ADDRESS           */ 00304000
*                                                                  0511 00305000
         ST    RETPTR,CRRRTN2(,CRRPTR)                             0511 00306000
*   /*****************************************************************/ 00307000
*   /*                                                               */ 00308000
*   /* IF A PREVIOUS WRITE LDO IN THIS DO GROUP CAUSED AN I/O        */ 00309000
*   /* OPERATION TO BE INITIATED, THEN A LCCW MUST BE BUILT FOR THIS */ 00310000
*   /* READ CAUSING AN I/O OPERATION TO OCCUR. IF I/O HAS NOT BEEN   */ 00311000
*   /* INITIATED AND THERE IS DATA ON THE FMCB DATA QUEUE, THIS MAY  */ 00312000
*   /* BE USED TO SATISFY THE READ. IF NO DATA IS PRESENT, BUT THE   */ 00313000
*   /* FMCB INDICATES DATA IS EXPECTED, THEN AN INDICATION IS SET FOR*/ 00314000
*   /* REQUEST FINISH INDICATING THAT THIS LDO IS AWAITING DATA FROM */ 00315000
*   /* A PREVIOUS I/O OPERATION. OTHERWISE A READ LCCW IS BUILT TO BE*/ 00316000
*   /* SHIPPED TO TPIOS.                                             */ 00317000
*   /*                                                               */ 00318000
*   /*****************************************************************/ 00319000
*                                                                  0512 00320000
*   IF CRRINIO=ON THEN              /* PREVIOUS WRITE LDO?           */ 00321000
         TM    CRRINIO(CRRPTR),B'00100000'                         0512 00322000
         BNO   @RF00512                                            0512 00323000
*     DO;                                                          0513 00324000
*       IF(PROSETM4=PROSBLCK|       /* VALID READ MODIFIER?          */ 00325000
*           PROSETM4=PROSMSG|PROSETM4=PROSTRNS|PROSETM4=PROSCONT) THEN  00326000
         LA    @15,48                                              0514 00327000
         AL    @15,CRRFMCB(,CRRPTR)                                0514 00328000
         CLI   PROSETM4(@15),X'01'                                 0514 00329000
         BE    @RT00514                                            0514 00330000
         CLI   PROSETM4(@15),X'02'                                 0514 00331000
         BE    @RT00514                                            0514 00332000
         CLI   PROSETM4(@15),X'03'                                 0514 00333000
         BE    @RT00514                                            0514 00334000
         CLI   PROSETM4(@15),X'83'                                 0514 00335000
         BNE   @RF00514                                            0514 00336000
@RT00514 DS    0H                                                  0515 00337000
*         DO;                       /* I/O HAS BEEN INITIATED        */ 00338000
*                                                                  0515 00339000
*           /*********************************************************/ 00340000
*           /*                                                       */ 00341000
*           /* BUILD READ LCCW                                       */ 00342000
*           /*                                                       */ 00343000
*           /*********************************************************/ 00344000
*                                                                  0516 00345000
*           IF PROSETM4=PROSBLCK THEN/* READ BLOCK                   */ 00346000
         L     @15,CRRFMCB(,CRRPTR)                                0516 00347000
         CLI   PROSETM4+48(@15),X'01'                              0516 00348000
         BNE   @RF00516                                            0516 00349000
*             LCCWRWOP=LRB;                                        0517 00350000
         L     @15,CRRCLCCW(,CRRPTR)                               0517 00351000
         NI    LCCWRWOP(@15),B'11000110'                           0517 00352000
         OI    LCCWRWOP(@15),B'00000110'                           0517 00353000
*           ELSE                                                   0518 00354000
*             IF PROSETM4=PROSMSG THEN/* READ MESSAGE                */ 00355000
         B     @RC00516                                            0518 00356000
@RF00516 L     @15,CRRFMCB(,CRRPTR)                                0518 00357000
         CLI   PROSETM4+48(@15),X'02'                              0518 00358000
         BNE   @RF00518                                            0518 00359000
*               LCCWRWOP=LRM;                                      0519 00360000
         L     @15,CRRCLCCW(,CRRPTR)                               0519 00361000
         NI    LCCWRWOP(@15),B'11001010'                           0519 00362000
         OI    LCCWRWOP(@15),B'00001010'                           0519 00363000
*             ELSE                                                 0520 00364000
*               IF PROSETM4=PROSTRNS THEN/* READ TRANS               */ 00365000
         B     @RC00518                                            0520 00366000
@RF00518 L     @15,CRRFMCB(,CRRPTR)                                0520 00367000
         CLI   PROSETM4+48(@15),X'03'                              0520 00368000
         BNE   @RF00520                                            0520 00369000
*                 LCCWRWOP=LRT;                                    0521 00370000
         L     @15,CRRCLCCW(,CRRPTR)                               0521 00371000
         NI    LCCWRWOP(@15),B'11001110'                           0521 00372000
         OI    LCCWRWOP(@15),B'00001110'                           0521 00373000
*               ELSE                                               0522 00374000
*                 LCCWRWOP=LRCONT;  /* READ CONT                     */ 00375000
         B     @RC00520                                            0522 00376000
@RF00520 L     @15,CRRCLCCW(,CRRPTR)                               0522 00377000
         NI    LCCWRWOP(@15),B'11011010'                           0522 00378000
         OI    LCCWRWOP(@15),B'00011010'                           0522 00379000
*           CALL CSUBR5;            /* CHECK DATA CHAIN AND DIALOGUE */ 00380000
@RC00520 DS    0H                                                  0523 00381000
@RC00518 DS    0H                                                  0523 00382000
@RC00516 BAL   @14,CSUBR5                                          0523 00383000
*         END;                                                     0524 00384000
*       ELSE                                                       0525 00385000
*         DO;                       /* VALID READ MODIFIER NOT GIVEN */ 00386000
*                                                                  0525 00387000
         B     @RC00514                                            0525 00388000
@RF00514 DS    0H                                                  0526 00389000
*           /*********************************************************/ 00390000
*           /*                                                       */ 00391000
*           /* ERROR-NO VALID READ MODIFIER GIVEN                    */ 00392000
*           /*                                                       */ 00393000
*           /*********************************************************/ 00394000
*                                                                  0526 00395000
*           CRRRTNCD=CRRLOGIC;      /* SHOW LOGIC ERROR FOR FEEDBACK */ 00396000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0526 00397000
*           CRRFDB2=CRRINVRM;       /* SET ERROR CODE                */ 00398000
         MVI   CRRFDB2(CRRPTR),X'2A'                               0527 00399000
*         END;                                                     0528 00400000
*       CRRFINI=ON;                 /* INDICATE DO PROCESSING      0529 00401000
*                                      FINISHED                      */ 00402000
@RC00514 OI    CRRFINI(CRRPTR),B'00001000'                         0529 00403000
*     END;                          /* END-I/O HAS BEEN INITIATED    */ 00404000
*   ELSE                                                           0531 00405000
*     DO;                           /* I/O HAS NOT BEEN INITIATED    */ 00406000
*                                                                  0531 00407000
         B     @RC00512                                            0531 00408000
@RF00512 DS    0H                                                  0532 00409000
*       /*************************************************************/ 00410000
*       /*                                                           */ 00411000
*       /* GET ADDRESS OF OLDEST FSB ON FMCB DATA QUEUE              */ 00412000
*       /*                                                           */ 00413000
*       /*************************************************************/ 00414000
*                                                                  0532 00415000
*/* TPFEL (,FMCFBA) RETPTR(FSBPTR)                                   */ 00416000
*                                                                  0532 00417000
*       DO;                         /* BEGIN TPFEL CODE GENERATION   */ 00418000
*         DCL                                                      0533 00419000
*           FMZCA020 BIT(32) CONSTANT('80000000'X);                0533 00420000
*         DCL                                                      0534 00421000
*           FMZCB020 BIT(32) CONSTANT('7FFFFFFF'X);                0534 00422000
*         RFY                                                      0535 00423000
*          (GPR00P,                                                0535 00424000
*           GPR01P,                                                0535 00425000
*           GPR14P,                                                0535 00426000
*           GPR15P) RSTD;                                          0535 00427000
*         GPR15P=ADDR(FMCFBA);      /* ADDRESS QUEUE POINTER         */ 00428000
         L     GPR15P,CRRFMCB(,CRRPTR)                             0536 00429000
