*      (GPR00P,                                                    1001 02306000
*       GPR01P,                                                    1001 02307000
*       GPR02P,                                                    1001 02308000
*       GPR15P) UNRSTD;                                            1001 02309000
*   END;                            /* *** END OF TPQUE GENERATION 1002 02310000
*                                      SHIP RESET TO TPIOS           */ 02311000
*   RETPTR=CRRRTN9;                 /* GET CALLER'S RETURN ADDRESS   */ 02312000
         L     RETPTR,CRRRTN9(,CRRPTR)                             1003 02313000
