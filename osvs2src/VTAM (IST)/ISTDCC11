*       IF CRRPREIO=OFF THEN        /* NOT AWAITING PREVIOS I/O    0609 00749000
*                                                            @ZM02162*/ 00750000
@RC00591 TM    CRRPREIO(CRRPTR),B'00010000'                        0609 00751000
         BNZ   @RF00609                                            0609 00752000
*         IF(PROSETM4=PROSBLCK|     /* VALID READ MODIFIER GIVEN?    */ 00753000
*             PROSETM4=PROSMSG|PROSETM4=PROSTRNS|PROSETM4=PROSCONT)     00754000
*           THEN                                                   0610 00755000
         LA    @15,48                                              0610 00756000
         AL    @15,CRRFMCB(,CRRPTR)                                0610 00757000
