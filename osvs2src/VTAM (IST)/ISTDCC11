*           CRRDEXP=ON;             /* SET DATA EXPECTED     @XM10597*/ 00712000
         OI    CRRDEXP(CRRPTR),B'10000000'                         0599 00713000
*         END;                      /* END NOT FIRST         @XM10597*/ 00714000
*                                                                  0600 00715000
*       /*************************************************************/ 00716000
*       /*                                                           */ 00717000
*       /* THIS IS THE FIRST I/O OPERATION TO THIS DEVICE,RESET THE  */ 00718000
*       /* I/O FLAG IN THE FMCB, AND PERMIT THIS REQUEST TO GO OUT   */ 00719000
*       /*                                                           */ 00720000
*       /*************************************************************/ 00721000
*                                                                  0601 00722000
*       ELSE                                                       0601 00723000
*         IF(DEVTCODE>=DEV3277&DEVTCODE<=DEV3275) THEN             0601 00724000
         B     @RC00591                                            0601 00725000
@RF00591 L     @15,CRRFMCB(,CRRPTR)                                0601 00726000
         LA    @12,36                                              0601 00727000
         AL    @12,FMCDNCBA(,@15)                                  0601 00728000
