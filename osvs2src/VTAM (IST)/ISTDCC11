         OI    LCCWRWOP(@15),B'00001110'                           0617 00802000
*                 ELSE                                             0618 00803000
*                   LCCWRWOP=LRCONT;/* READ CONT                     */ 00804000
         B     @RC00616                                            0618 00805000
@RF00616 L     @15,CRRCLCCW(,CRRPTR)                               0618 00806000
         NI    LCCWRWOP(@15),B'11011010'                           0618 00807000
         OI    LCCWRWOP(@15),B'00011010'                           0618 00808000
*             CALL CSUBR5;          /* CHECK DATA CHAIN AND DIALOGUE */ 00809000
@RC00616 DS    0H                                                  0619 00810000
@RC00614 DS    0H                                                  0619 00811000
@RC00612 BAL   @14,CSUBR5                                          0619 00812000
*           END;                                                   0620 00813000
*         ELSE                                                     0621 00814000
*           DO;                     /* INVALID READ MODIFIER         */ 00815000
*                                                                  0621 00816000
         B     @RC00610                                            0621 00817000
@RF00610 DS    0H                                                  0622 00818000
*             /*******************************************************/ 00819000
*             /*                                                     */ 00820000
*             /* ERROR-NO VALID READ MODIFIER GIVEN                  */ 00821000
*             /*                                                     */ 00822000
*             /*******************************************************/ 00823000
*                                                                  0622 00824000
*             CRRRTNCD=CRRLOGIC;    /* SHOW LOGIC ERROR FOR FEEDBACK */ 00825000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0622 00826000
*             CRRFDB2=CRRINVRM;     /* SET ERROR CODE                */ 00827000
         MVI   CRRFDB2(CRRPTR),X'2A'                               0623 00828000
*           END;                                                   0624 00829000
*       ELSE                                                       0625 00830000
*         ;                         /*                       @ZM02162*/ 00831000
@RF00609 DS    0H                                                  0626 00832000
*     END;                          /* DATA NOT EXPECTED     @DM14120*/ 00833000
@RC00609 DS    0H                                                  0627 00834000
*   CRRFINI=ON;                     /* INDICATE DO PROCESSING      0627 00835000
*                                      FINISHED                      */ 00836000
@RC00585 OI    CRRFINI(CRRPTR),B'00001000'                         0627 00837000
*   RETPTR=CRRRTN3;                 /* GET RETURN ADDRESS            */ 00838000
         L     RETPTR,CRRRTN3(,CRRPTR)                             0628 00839000
*   END CSUBR4;                                                    0629 00840000
@EL00005 DS    0H                                                  0629 00841000
@EF00005 DS    0H                                                  0629 00842000
@ER00005 BR    @14                                                 0629 00843000
*CSUBR5:                                                           0630 00844000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0630 00845000
*                                                                  0630 00846000
CSUBR5   DS    0H                                                  0631 00847000
*/********************************************************************/ 00848000
*/*                                                                  */ 00849000
*/* ROUTINE NAME : CSUBR5 - CHECK DATA CHAIN AND DIALOGUE            */ 00850000
*/*                                                                  */ 00851000
*/* FUNCTION :                                                       */ 00852000
*/*    IF LDO IS DATA CHAINED, UPDATE CURRENT LDO POINTER TO FIRST   */ 00853000
*/*    NON-DATA CHAINED LDO.  SET SWITCHES TO START I/O.  INDICATE   */ 00854000
*/*    DATA IS NOW EXPECTED FROM THE DEVICE AND THAT A READ HAS      */ 00855000
*/*    BEEN ISSUED TO THE DEVICE.  TEST WHETHER USER WISHES TO BE    */ 00856000
*/*    IN DIALOGUE WITH A SPECIFIC TERMINAL OR WITH ANY TERMINAL.    */ 00857000
*/*                                                                  */ 00858000
*/* INPUT :  LDOPTR  (REG 9) - POINTS TO CURRENT LDO                 */ 00859000
*/*                                                                  */ 00860000
*/* OUTPUT : SAME                                                    */ 00861000
*/*                                                                  */ 00862000
*/* SUBROUTINES CALLED : UPLDOPTR - FIND NON-DATA CHAINED LDO        */ 00863000
*/*                                                                  */ 00864000
*/* MACROS USED : NONE                                               */ 00865000
*/*                                                                  */ 00866000
*/********************************************************************/ 00867000
*                                                                  0631 00868000
*   CRRRTN4=RETPTR;                 /* SAVE RETURN ADDRESS           */ 00869000
         ST    RETPTR,CRRRTN4(,CRRPTR)                             0631 00870000
*   IF LDOACHN=ON THEN                                             0632 00871000
         TM    LDOACHN(LDOPTR),B'10000000'                         0632 00872000
         BNO   @RF00632                                            0632 00873000
*     CALL UPLDOPTR;                /* DATA CHAINED                  */ 00874000
         BAL   @14,UPLDOPTR                                        0633 00875000
*   ELSE                                                           0634 00876000
*     ;                             /* NOT DATA CHAINED              */ 00877000
@RF00632 DS    0H                                                  0635 00878000
*   IF CRRFINI=ON THEN                                             0635 00879000
         TM    CRRFINI(CRRPTR),B'00001000'                         0635 00880000
         BO    @RT00635                                            0635 00881000
*     ;                             /* ERROR - DO NOT START I/O      */ 00882000
*                                                                  0636 00883000
*   /*****************************************************************/ 00884000
*   /*                                                               */ 00885000
*   /* INDICATE I/O TO BE STARTED                                    */ 00886000
*   /*                                                               */ 00887000
*   /*****************************************************************/ 00888000
*                                                                  0637 00889000
*   ELSE                                                           0637 00890000
*     DO;                           /* NO ERRORS FOUND BY UPLDOPTR   */ 00891000
*       RFY                                                        0638 00892000
*        (REG10,                                                   0638 00893000
*         REG11) RSTD;                                             0638 00894000
*CS1:                                                              0639 00895000
*       REG10=FMCCMPS2;             /* REFERENCE OLD FLAG VALUE      */ 00896000
CS1      L     @15,CRRFMCB(,CRRPTR)                                0639 00897000
         L     REG10,FMCCMPS2(,@15)                                0639 00898000
*       REG11=REG10|FLGA06ON;       /* SET DATA EXPECTED AND READ  0640 00899000
*                                      DONE TO DEVICE                */ 00900000
         LR    REG11,REG10                                         0640 00901000
         O     REG11,@CF04162                                      0640 00902000
*       CS(REG10,REG11,FMCCMPS2);   /* SET NEW VALUE IN FMCB         */ 00903000
         CS    REG10,@11,FMCCMPS2(@15)                             0641 00904000
*       BC(7,CS1);                  /* TRY AGAIN IF FAILURE          */ 00905000
         BC    7,CS1                                               0642 00906000
*       CRRDEXP=ON;                 /* SET DATA EXPECTED IN CRR      */ 00907000
         OI    CRRDEXP(CRRPTR),B'10000000'                         0643 00908000
*       CRRINIO=ON;                 /* I/O REQUEST TO BE INITIATED   */ 00909000
         OI    CRRINIO(CRRPTR),B'00100000'                         0644 00910000
*CS2:                                                              0645 00911000
*       REG10=FMCCMPS2;             /* REFERENCE OLD FLAG VALUE      */ 00912000
CS2      L     @15,CRRFMCB(,CRRPTR)                                0645 00913000
         L     REG10,FMCCMPS2(,@15)                                0645 00914000
*       IF RPLDLGIN=OFF THEN        /* TEST FOR CONTINUE ANY         */ 00915000
         L     @15,CRRARPL(,CRRPTR)                                0646 00916000
         TM    RPLDLGIN(@15),B'10000000'                           0646 00917000
         BNZ   @RF00646                                            0646 00918000
*         REG11=REG10&FLGA1OFF;     /* RESET SATISFY SPECIFIC RD     */ 00919000
         LR    REG11,REG10                                         0647 00920000
         N     REG11,@CF02553                                      0647 00921000
*       ELSE                                                       0648 00922000
*         REG11=REG10|FLGA1ON;      /* SET SATISFY SPECIFIC READ     */ 00923000
         B     @RC00646                                            0648 00924000
@RF00646 LR    REG11,REG10                                         0648 00925000
         O     REG11,@CF02555                                      0648 00926000
*       CS(REG10,REG11,FMCCMPS2);   /* SET NEW VALUE IN FMCB         */ 00927000
@RC00646 L     @15,CRRFMCB(,CRRPTR)                                0649 00928000
         CS    REG10,@11,FMCCMPS2(@15)                             0649 00929000
*       BC(7,CS2);                  /* TRY AGAIN IF FAILURE          */ 00930000
         BC    7,CS2                                               0650 00931000
*       RFY                                                        0651 00932000
*        (REG10,                                                   0651 00933000
*         REG11) UNRSTD;                                           0651 00934000
*     END;                                                         0652 00935000
*   RETPTR=CRRRTN4;                 /* GET RETURN ADDRESS            */ 00936000
@RT00635 L     RETPTR,CRRRTN4(,CRRPTR)                             0653 00937000
*   END CSUBR5;                     /* RETURN TO CALLER              */ 00938000
@EL00006 DS    0H                                                  0654 00939000
@EF00006 DS    0H                                                  0654 00940000
@ER00006 BR    @14                                                 0654 00941000
*CSUBR6:                                                           0655 00942000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0655 00943000
*                                                                  0655 00944000
CSUBR6   DS    0H                                                  0656 00945000
*/********************************************************************/ 00946000
*/*                                                                  */ 00947000
*/* ROUTINE NAME :  SUBR6 - MOVE DATA                                */ 00948000
*/*                                                                  */ 00949000
*/* FUNCTION :                                                       */ 00950000
*/*    SUBR6 SET UP THE PARAMETER LIST FOR THE MOVE ROUTINE          */ 00951000
*/*    AND CALLS CSUBR7 TO ACTUALLY MOVE THE DATA. SUBR7 IS          */ 00952000
*/*    CALLED AS MANY TIMES AS NECESSARY UNTIL ALL THE DATA          */ 00953000
*/*    IS MOVED.                                                     */ 00954000
*/*                                                                  */ 00955000
*/* INPUT :  LDOPTR (REG 10) - ADDRESS OF LDO                        */ 00956000
*/*                                                                  */ 00957000
*/* OUTPUT :    ALL DATA IN FSB MOVED TO USER'S AREA                 */ 00958000
*/*                                                                  */ 00959000
*/* SUBROUTINES CALLED :                                             */ 00960000
*/*         SUBR7 - MOVE DATA                                        */ 00961000
*/*                                                                  */ 00962000
*/*  MACROS USED :  NONE                                             */ 00963000
*/*                                                                  */ 00964000
*/********************************************************************/ 00965000
*                                                                  0656 00966000
*   CRRRTN3=RETPTR;                 /* SAVE RETURN ADDRESS           */ 00967000
*                                                                  0656 00968000
         ST    RETPTR,CRRRTN3(,CRRPTR)                             0656 00969000
*   /*****************************************************************/ 00970000
*   /*                                                               */ 00971000
*   /* SET UP INPUT PARAMETERS FOR THE MOVE ROUTINE TO MOVE DATA FROM*/ 00972000
*   /* THE FSB TO THE USER'S BUFFER                                  */ 00973000
*   /*                                                               */ 00974000
*   /*****************************************************************/ 00975000
*                                                                  0657 00976000
*   ATCVTPTR=CRRATCVT;              /* BASE FOR VTAM CVT             */ 00977000
         L     ATCVTPTR,CRRATCVT(,CRRPTR)                          0657 00978000
*   CRRBARAD=LDOADDR;               /* ADDR OF USER'S INPUT AREA     */ 00979000
         L     @15,LDOADDR(,LDOPTR)                                0658 00980000
         ST    @15,CRRBARAD(,CRRPTR)                               0658 00981000
*   CRRBLDAD=CRRCLDO;               /* CURRENT LDO FOR MOVE          */ 00982000
         L     @15,CRRCLDO(,CRRPTR)                                0659 00983000
         ST    @15,CRRBLDAD(,CRRPTR)                               0659 00984000
*   CRRBTLEN=CRRBTLEN&&CRRBTLEN;    /* CLEAR RESIDUAL COUNT          */ 00985000
         L     @15,CRRBTLEN(,CRRPTR)                               0660 00986000
         LR    @12,@15                                             0660 00987000
         XR    @12,@15                                             0660 00988000
         ST    @12,CRRBTLEN(,CRRPTR)                               0660 00989000
*   CRRBLEN=LDOLEN;                 /* SIZE OF CURRENT INPUT AREA    */ 00990000
