*       LCCWPTR=CRRCLCCW;           /* UPDATE LCCW BASE REG          */ 01560000
         LR    LCCWPTR,@11                                         0822 01561000
*       CRRLCNT=CRRLCNT+1;          /* UPDATE COUNT OF LCCWS         */ 01562000
         LA    @06,1                                               0823 01563000
