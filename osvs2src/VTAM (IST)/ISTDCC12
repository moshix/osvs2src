         L     GPR15P,ATCLCPTR                                     0873 01742000
*             END;                  /* LISATCVT                      */ 01743000
*             RESPECIFY                                            0875 01744000
*               GPR00F RSTD;                                       0875 01745000
*             GPR00F=0;             /* SET PARAMETER REGISTER TO 0   */ 01746000
         SLR   GPR00F,GPR00F                                       0876 01747000
*             GPR15P=GPR15P->ATCAP31;                              0877 01748000
         L     GPR15P,ATCAP31(,GPR15P)                             0877 01749000
*             DCL                                                  0878 01750000
*               FMZL0110 BASED(GPR15P) LABEL VALUERANGE(*);        0878 01751000
*             CALL FMZL0110;                                       0879 01752000
         BALR  @14,@15                                             0879 01753000
*             RESPECIFY                                            0880 01754000
*              (GPR00F,                                            0880 01755000
*               GPR15P) UNRSTD;                                    0880 01756000
*           END;                    /* **** END OF TPEXIT            */ 01757000
*LHAB0105:                                                         0882 01758000
*           ;                                                      0882 01759000
LHAB0105 DS    0H                                                  0883 01760000
*           GPR01P->RPHRESMA=0;                                    0883 01761000
         SLR   @11,@11                                             0883 01762000
         ST    @11,RPHRESMA(,GPR01P)                               0883 01763000
*           RFY                                                    0884 01764000
*            (GPR00P) RSTD;                                        0884 01765000
*LHAA0105:                                                         0885 01766000
*           ;                                                      0885 01767000
LHAA0105 DS    0H                                                  0886 01768000
*           GEN(LM    GPR00P,GPR15P,RPHWORK(GPR01P))               0886 01769000
*           REFS(RPHWORK,GPR01P) SETS(GPR00P,GPR15P);              0886 01770000
         LM    GPR00P,GPR15P,RPHWORK(GPR01P)                            01771000
*           RFY                                                    0887 01772000
*            (GPR00P,                                              0887 01773000
*             GPR01P,                                              0887 01774000
*             GPR14P,                                              0887 01775000
*             GPR15P) UNRSTD;                                      0887 01776000
*         END;                      /* ****END OF TPWAIT WAIT FOR  0888 01777000
*                                      STORAGE                       */ 01778000
*       RFY                                                        0889 01779000
