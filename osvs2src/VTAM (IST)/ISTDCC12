*                 GPR15P=ATCLCPTR;  /* SET UP ATCVT POINTER          */ 01231000
         L     GPR15P,ATCLCPTR                                     0715 01232000
*               END;                /* LISATCVT                      */ 01233000
*               RESPECIFY                                          0717 01234000
*                 GPR00F RSTD;                                     0717 01235000
*               GPR00F=0;           /* SET PARAMETER REGISTER TO 0   */ 01236000
         SLR   GPR00F,GPR00F                                       0718 01237000
*               GPR15P=GPR15P->ATCAP31;                            0719 01238000
         L     GPR15P,ATCAP31(,GPR15P)                             0719 01239000
*               DCL                                                0720 01240000
*                 FMZL0042 BASED(GPR15P) LABEL VALUERANGE(*);      0720 01241000
*               CALL FMZL0042;                                     0721 01242000
         BALR  @14,@15                                             0721 01243000
*               RESPECIFY                                          0722 01244000
*                (GPR00F,                                          0722 01245000
*                 GPR15P) UNRSTD;                                  0722 01246000
*             END;                  /* **** END OF TPEXIT            */ 01247000
*LHAB0037:                                                         0724 01248000
*             ;                                                    0724 01249000
LHAB0037 DS    0H                                                  0725 01250000
*             GPR01P->RPHRESMA=0;                                  0725 01251000
         SLR   @00,@00                                             0725 01252000
         ST    @00,RPHRESMA(,GPR01P)                               0725 01253000
*             RFY                                                  0726 01254000
*              (GPR00P) RSTD;                                      0726 01255000
*LHAA0037:                                                         0727 01256000
*             ;                                                    0727 01257000
LHAA0037 DS    0H                                                  0728 01258000
*             GEN(LM    GPR00P,GPR15P,RPHWORK(GPR01P))             0728 01259000
*             REFS(RPHWORK,GPR01P) SETS(GPR00P,GPR15P);            0728 01260000
         LM    GPR00P,GPR15P,RPHWORK(GPR01P)                            01261000
*             RFY                                                  0729 01262000
*              (GPR00P,                                            0729 01263000
*               GPR01P,                                            0729 01264000
*               GPR14P,                                            0729 01265000
*               GPR15P) UNRSTD;                                    0729 01266000
*           END;                    /* ****END OF TPWAIT WAIT FOR  0730 01267000
*                                      STORAGE                       */ 01268000
*         RFY                                                      0731 01269000
*           RETCODE UNRSTD;                                        0731 01270000
@RF00700 DS    0H                                                  0732 01271000
*         CRRLCNT=CRRLCNT&&CRRLCNT; /* RESET LCCW COUNT TO ZERO      */ 01272000
         XC    CRRLCNT(2,CRRPTR),CRRLCNT(CRRPTR)                   0732 01273000
*         RPHSRPRM->LCCWOP=LCCWOP;  /* PUT CURRENT LCCW OPCODE IN  0733 01274000
*                                      FIRST SLOT OF NEW LCCW      0733 01275000
