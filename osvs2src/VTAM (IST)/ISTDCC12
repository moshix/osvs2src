         MVI   LCCWOP(LCCWPTR),X'08'                               0805 01513000
*       LCCWDATA=RPHSRPRM;          /* POINT LTIC LCCW TO NEW AREA   */ 01514000
         ST    @15,LCCWDATA(,LCCWPTR)                              0806 01515000
*       CRRCLCCW=RPHSRPRM;          /* POINT LCCW POINTER TO THE   0807 01516000
*                                      NEWLY REQSTOR'D AREA          */ 01517000
         ST    @15,CRRCLCCW(,CRRPTR)                               0807 01518000
*       LCCWPTR=CRRCLCCW;           /* UPDATE LCCW BASE REG          */ 01519000
         LR    LCCWPTR,@15                                         0808 01520000
*     END;                          /* END GET LCCW EXT DO           */ 01521000
*   ELSE                                                           0810 01522000
*     ;                                                            0810 01523000
@RF00748 DS    0H                                                  0811 01524000
*   IF CRRSW0=ON THEN                                              0811 01525000
