         O     GPR15P,@CF01093                                     0709 01214000
*             CS(GPR14P,GPR15P,GPR01P->RPHRESMA);                  0710 01215000
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         0710 01216000
*             BC(7,LHAB0037);                                      0711 01217000
*                                                                  0711 01218000
         BC    7,LHAB0037                                          0711 01219000
*/* TPEXIT  NONE                                                     */ 01220000
*                                                                  0712 01221000
*             DO;                                                  0712 01222000
*               RESPECIFY                                          0713 01223000
*                 GPR15P RSTD;                                     0713 01224000
*                                                                  0713 01225000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)                               */ 01226000
*/* END OF ATBUILD                                                   */ 01227000
*                                                                  0714 01228000
*               DO;                 /* LISATCVT                    0714 01229000
