         XC    CRRLCNT(2,CRRPTR),CRRLCNT(CRRPTR)                   0803 01505000
*       RPHSRPRM->LCCWOP=LCCWOP;    /* PUT CURRENT LCCW OPCODE IN  0804 01506000
*                                      FIRST SLOT OF NEW LCCW      0804 01507000
*                                      EXTENSION                     */ 01508000
         L     @15,RPHSRPRM(,RPHPTR)                               0804 01509000
         MVC   LCCWOP(1,@15),LCCWOP(LCCWPTR)                       0804 01510000
*       LCCWOP=LTIC;                /* BUILD LTIC LCCW IN AREA     0805 01511000
