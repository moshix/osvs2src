         STM   GPR00P,GPR15P,RPHWORK(GPR01P)                            01714000
*           RFY                                                    0863 01715000
*            (GPR00P) UNRSTD;                                      0863 01716000
*           GPR15P=ADDR(LHAA0105);                                 0864 01717000
         LA    GPR15P,LHAA0105                                     0864 01718000
*           GPR14P=0;                                              0865 01719000
         SLR   GPR14P,GPR14P                                       0865 01720000
*           GPR15P=GPR15P&'00FFFFFF'X;                             0866 01721000
         N     GPR15P,@CF03563                                     0866 01722000
*           GPR15P=GPR15P|RPHWT1;                                  0867 01723000
         O     GPR15P,@CF01093                                     0867 01724000
*           CS(GPR14P,GPR15P,GPR01P->RPHRESMA);                    0868 01725000
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         0868 01726000
*           BC(7,LHAB0105);                                        0869 01727000
*                                                                  0869 01728000
         BC    7,LHAB0105                                          0869 01729000
*/* TPEXIT  NONE                                                     */ 01730000
*                                                                  0870 01731000
*           DO;                                                    0870 01732000
*             RESPECIFY                                            0871 01733000
*               GPR15P RSTD;                                       0871 01734000
*                                                                  0871 01735000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)                               */ 01736000
*/* END OF ATBUILD                                                   */ 01737000
*                                                                  0872 01738000
*             DO;                   /* LISATCVT                    0872 01739000
