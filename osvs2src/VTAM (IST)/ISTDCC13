*/********************************************************************/ 00144000
*/*                                                                  */ 00145000
*/*  ROUTINE NAME : PROCESSD - BUILD RESET LCCW                      */ 00146000
*/*                                                                  */ 00147000
*/*  FUNCTION : BUILDS THE PROPER LCCW FOR THE RESET COMMAND.A RESET */ 00148000
*/*       IS EITHER CONDITIONAL OR UNCONDITIONAL. IF UNCONDITIONAL   */ 00149000
*/*       A CHECK IS MADE TO SEE IF IT IS IMMEDIATE ,ORDERLY,OR RESET*/ 00150000
*/*       AT END OF COMMAND. IF RESET CONDITIONAL THE CORESPONDING   */ 00151000
*/*       FLAG IS SET IN THE FMCB.                                   */ 00152000
*/*                                                                  */ 00153000
*/*  INPUT : SAME AS MAINLINE                                        */ 00154000
*/*                                                                  */ 00155000
*/*  OUTPUT : NONE                                                   */ 00156000
*/*                                                                  */ 00157000
*/*  SUBROUTINES CALLED : NONE                                       */ 00158000
*/*                                                                  */ 00159000
*/*  MACROS USED : NONE                                              */ 00160000
*/*                                                                  */ 00161000
*/********************************************************************/ 00162000
*                                                                  0450 00163000
*PROCESSD:                                                         0450 00164000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0450 00165000
         B     @PB00002                                            0450 00166000
PROCESSD DS    0H                                                  0451 00167000
*   RFY                                                            0451 00168000
*     ISTLCCW BASED(ADDR(DCLLCCW2));/* USE 2ND LCCW                  */ 00169000
*   RFY                                                            0452 00170000
*     REG14 RSTD;                                                  0452 00171000
*   CRRRTN1=REG14;                  /* SAVE THE RETURN ADDRESS       */ 00172000
         ST    REG14,CRRRTN1(,ADDRCRR)                             0453 00173000
*   LCCWOP=LCPNXLCW->LCCWOP;        /* MOVE LDO CMD TO 2ND LCCW      */ 00174000
         L     @11,CRRLCPB(,ADDRCRR)                               0454 00175000
         LA    @08,56                                              0454 00176000
         ALR   @08,@11                                             0454 00177000
         L     @05,LCPNXLCW(,@11)                                  0454 00178000
         MVC   LCCWOP(1,@08),LCCWOP(@05)                           0454 00179000
*   LCPNXLCW=ADDR(DCLLCCW2);        /* ADJUST PTR TO POINT TO 2ND  0455 00180000
*                                      LCCW                          */ 00181000
         LA    @05,DCLLCCW2(,@11)                                  0455 00182000
         ST    @05,LCPNXLCW(,@11)                                  0455 00183000
*   LCPLCCW=LCPLCCW(1:4)&&LCPLCCW(1:4);/* ZERO 1ST WORD OF 1ST LCCW  */ 00184000
         XC    LCPLCCW(8,@11),LCPLCCW(@11)                         0456 00185000
*   IF LDOCMD=LDORSETU THEN         /* CHECK FOR RESET ORDINARY      */ 00186000
         L     @11,CRRCLDO(,ADDRCRR)                               0457 00187000
         CLI   LDOCMD(@11),X'2B'                                   0457 00188000
         BNE   @RF00457                                            0457 00189000
*     DO;                                                          0458 00190000
*       IF DEVCHAR2>=DEV2980&DEVCHAR2<=DEV3275 THEN/* 2980? - 3270?  */ 00191000
         L     @11,CRRFMCB(,ADDRCRR)                               0459 00192000
         LA    @05,36                                              0459 00193000
         AL    @05,FMCDNCBA(,@11)                                  0459 00194000
         CLI   DEVCHAR2(@05),X'18'                                 0459 00195000
         BL    @RF00459                                            0459 00196000
         CLI   DEVCHAR2(@05),X'1C'                                 0459 00197000
         BH    @RF00459                                            0459 00198000
*         LCCWOP=LXEC;                                             0460 00199000
         MVI   LCCWOP(@08),X'4B'                                   0460 00200000
*       ELSE                                                       0461 00201000
*         IF DEVCRVB=ON THEN        /* CHECK FOR RESET IMMEDIATE     */ 00202000
         B     @RC00459                                            0461 00203000
@RF00459 L     @11,CRRFMCB(,ADDRCRR)                               0461 00204000
         L     @11,FMCDNCBA(,@11)                                  0461 00205000
         TM    DEVCRVB+36(@11),B'00100000'                         0461 00206000
         BNO   @RF00461                                            0461 00207000
*           LCCWOP=LXI;             /* SET RESET IMMEDIATE COMMAND   */ 00208000
         L     @11,CRRLCPB(,ADDRCRR)                               0462 00209000
         MVI   LCCWOP+56(@11),X'2B'                                0462 00210000
*         ELSE                                                     0463 00211000
*           IF DEVCBSC=ON THEN      /* CHECK FOR RESET ORDERLY       */ 00212000
         B     @RC00461                                            0463 00213000
@RF00461 L     @11,CRRFMCB(,ADDRCRR)                               0463 00214000
         L     @11,FMCDNCBA(,@11)                                  0463 00215000
         TM    DEVCBSC+36(@11),B'10000000'                         0463 00216000
         BNO   @RF00463                                            0463 00217000
*             LCCWOP=LXO;           /* SET RESET ORDERLY COMMAND     */ 00218000
         L     @11,CRRLCPB(,ADDRCRR)                               0464 00219000
         MVI   LCCWOP+56(@11),X'5B'                                0464 00220000
*           ELSE                                                   0465 00221000
*             LCCWOP=LXEC;          /* OTHERWISE, SET RESET AT END OF   00222000
*                                      COMMAND COMMAND               */ 00223000
         B     @RC00463                                            0465 00224000
@RF00463 L     @11,CRRLCPB(,ADDRCRR)                               0465 00225000
         MVI   LCCWOP+56(@11),X'4B'                                0465 00226000
*     END;                                                         0466 00227000
*   ELSE                                                           0467 00228000
*     ;                                                            0467 00229000
@RF00457 DS    0H                                                  0468 00230000
*   DCLLDO1=ADDRLDO;                /* PUT ADDRESS OF LDO IN LCPB    */ 00231000
@RC00457 L     @11,CRRLCPB(,ADDRCRR)                               0468 00232000
         ST    ADDRLDO,DCLLDO1(,@11)                               0468 00233000
*   IF LDOCMD=LDORSETC THEN         /* RESET CONDITIONAL?            */ 00234000
         L     @11,CRRCLDO(,ADDRCRR)                               0469 00235000
         CLI   LDOCMD(@11),X'1B'                                   0469 00236000
         BNE   @RF00469                                            0469 00237000
*     IF PABERRLK=ON THEN           /* TEST TOP PAB ERROR LOCK       */ 00238000
         L     @11,CRRFMCB(,ADDRCRR)                               0470 00239000
         TM    PABERRLK+96(@11),B'00000100'                        0470 00240000
         BNO   @RF00470                                            0470 00241000
*       DO;                         /* SET ERROR CODES               */ 00242000
*         CRRRTNCD=CRRLOGIC;        /* LOGICAL ERROR                 */ 00243000
         MVI   CRRRTNCD(ADDRCRR),X'08'                             0472 00244000
*         CRRFDB2=CRRCRJEL;         /* RESET CONDITIONAL REJECTED    */ 00245000
         MVI   CRRFDB2(ADDRCRR),X'29'                              0473 00246000
*       END;                        /* BECAUSE ERROR LOCK SET        */ 00247000
*     ELSE                                                         0475 00248000
*       DO;                         /* SET RESET CONDITIONAL FLAG    */ 00249000
         B     @RC00470                                            0475 00250000
@RF00470 DS    0H                                                  0476 00251000
*         OLDVALUE=FMCCMPS2;        /* SAVE OLD VALUE                */ 00252000
         L     @11,CRRFMCB(,ADDRCRR)                               0476 00253000
         L     OLDVALUE,FMCCMPS2(,@11)                             0476 00254000
*CSWAP1:                                                           0477 00255000
*         NEWVALUE=OLDVALUE|FLAG8ON;/* TURN ON FMCFLGA8 IN REG       */ 00256000
