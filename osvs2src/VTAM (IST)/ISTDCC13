*         CS(OLDVALUE,NEWVALUE,FMCCMPS2);/* TURN ON FMCFLGA8 IN FMCB */ 00259000
         L     @11,CRRFMCB(,ADDRCRR)                               0478 00260000
         CS    OLDVALUE,@10,FMCCMPS2(@11)                          0478 00261000
*         BC(7,CSWAP1);             /* TRY AGAIN IF NO GOOD          */ 00262000
         BC    7,CSWAP1                                            0479 00263000
*       END;                                                       0480 00264000
*   ELSE                                                           0481 00265000
*     ;                             /* NOT RESET CONDITIONAL         */ 00266000
@RF00469 DS    0H                                                  0482 00267000
*   IF CRRRTNCD=CRRNOERR THEN       /* AND THERE WERE NO ERRORS      */ 00268000
@RC00469 CLI   CRRRTNCD(ADDRCRR),X'00'                             0482 00269000
         BNE   @RF00482                                            0482 00270000
*     DO;                                                          0483 00271000
*       CRRRESET=ON;                /* SET RESET OPERATION FLAG ON   */ 00272000
         OI    CRRRESET(ADDRCRR),B'00000010'                       0484 00273000
*       CRRINIO=ON;                 /* TURN INITIATE I/O FLAG ON     */ 00274000
         OI    CRRINIO(ADDRCRR),B'00100000'                        0485 00275000
*/*   LINES 174310 THRU 174337 REMOVED FOR DCR 1992 ( 9/14/73 )      */ 00276000
*                                                                  0486 00277000
*       IF RPLDLGIN=OFF THEN        /* CHECK THE 'CONTINUE ANY'      */ 00278000
         TM    RPLDLGIN(ADDRRPL),B'10000000'                       0486 00279000
         BNZ   @RF00486                                            0486 00280000
*         DO;                       /* SWITCH IN THE RPL AND SET THE */ 00281000
*           OLDVALUE=FMCCMPS2;      /* 'SATISFY SPECIFIC READ FLAG IN   00282000
*                                      THE FMCB                      */ 00283000
         L     @11,CRRFMCB(,ADDRCRR)                               0488 00284000
         L     OLDVALUE,FMCCMPS2(,@11)                             0488 00285000
*CSWAP2:                                                           0489 00286000
*           NEWVALUE=OLDVALUE&FLAG1OFF;                            0489 00287000
CSWAP2   LR    NEWVALUE,OLDVALUE                                   0489 00288000
         N     NEWVALUE,@CF01833                                   0489 00289000
*           CS(OLDVALUE,NEWVALUE,FMCCMPS2);                        0490 00290000
         L     @11,CRRFMCB(,ADDRCRR)                               0490 00291000
         CS    OLDVALUE,@10,FMCCMPS2(@11)                          0490 00292000
*           BC(7,CSWAP2);                                          0491 00293000
         BC    7,CSWAP2                                            0491 00294000
*         END;                                                     0492 00295000
*       ELSE                                                       0493 00296000
*         DO;                                                      0493 00297000
         B     @RC00486                                            0493 00298000
@RF00486 DS    0H                                                  0494 00299000
*           OLDVALUE=FMCCMPS2;                                     0494 00300000
         L     @11,CRRFMCB(,ADDRCRR)                               0494 00301000
         L     OLDVALUE,FMCCMPS2(,@11)                             0494 00302000
*CSWAP3:                                                           0495 00303000
*           NEWVALUE=OLDVALUE|FLAG1ON;                             0495 00304000
CSWAP3   LR    NEWVALUE,OLDVALUE                                   0495 00305000
         O     NEWVALUE,@CF01737                                   0495 00306000
