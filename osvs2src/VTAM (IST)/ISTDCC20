         TITLE 'ISTDCC20 -- SOLICIT SPECIFIC MACRO PROCESSOR           *00001000
                        '                                               00002000
ISTDCC20 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC20  77.126'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*   CRRPTR=ADDR(CRRSPACE);                                         0431 00013000
*                                                                  0431 00014000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0431 00015000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0431 00016000
*/*                                                                  */ 00017000
*/*******************FMCB (FUNCTION MGMT CONTROL BLOCK)***************/ 00018000
*                                                                  0432 00019000
*   DCL                                                            0432 00020000
*     FMCBPTR PTR(31) RESTRICTED REG(3);                           0432 00021000
*   FMCBPTR=CRRFMCB;                /* GET FMCB ADDR FROM CRR        */ 00022000
         L     FMCBPTR,CRRFMCB(,CRRPTR)                            0433 00023000
*   RFY                                                            0434 00024000
*     ISTFMCB BASED(FMCBPTR);                                      0434 00025000
*   RFY                                                            0435 00026000
*     ISTPROCD BASED(ADDR(FMCPROCD));/* BASE FOR ISTPROCD            */ 00027000
*   RFY                                                            0436 00028000
*     ISTNCB BASED(FMCDNCBA);       /* ADDRESS NCB DEVCH     @D32CKNL*/ 00029000
*   RFY                                                            0437 00030000
*     ISTDEVCH BASED(ADDR(NCBDEVCH));/* BASE FOR ISTDEVCH    @D32CKNL*/ 00031000
*   RFY                                                            0438 00032000
*     ISTRLN BASED(NCBRDTE);        /* RDT BASE              @ZA19868*/ 00033000
*   RFY                                                            0439 00034000
*     ISTRPRE BASED(NCBRDTE);       /* RDT BASE              @ZA19868*/ 00035000
*   RFY                                                            0440 00036000
*     ISTRPT BASED(NCBRDTE);        /* RDT BASE              @ZA19868*/ 00037000
*/*                                                                  */ 00038000
*/***********************RPL (REQUEST PARAMETER LIST)*****************/ 00039000
*                                                                  0441 00040000
*   DCL                                                            0441 00041000
*     RPLPTR PTR(31) RESTRICTED REG(4);                            0441 00042000
*   RPLPTR=CRRARPL;                 /* GET RPL ADDR FROM CRR         */ 00043000
         L     RPLPTR,CRRARPL(,CRRPTR)                             0442 00044000
*   RFY                                                            0443 00045000
*     IFGRPL BASED(RPLPTR);                                        0443 00046000
*                                                                  0443 00047000
*/*                                                                  */ 00048000
*/************LCPB (LOGICAL CHANNEL PROGRAM BLOCK)********************/ 00049000
*                                                                  0444 00050000
*   DCL                                                            0444 00051000
*     LCPBPTR PTR(31) RESTRICTED REG(5);                           0444 00052000
*   LCPBPTR=CRRLCPB;                /* GET LCPB ADDR FROM CRR        */ 00053000
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0445 00054000
*   RFY                                                            0446 00055000
*     ISTDCLCP BASED(LCPBPTR);                                     0446 00056000
*   RFY                                                            0447 00057000
*     ISTLCPB BASED(ADDR(DCLLCPB));                                0447 00058000
*                                                                  0447 00059000
*/*                                                                  */ 00060000
*/************ LCCW (LOGICAL CHANNEL COMMAND WORD)********************/ 00061000
*                                                                  0448 00062000
*   RFY                                                            0448 00063000
*     ISTLCCW BASED(LCPNXLCW);      /* BASE LCCW ON LCPNXLCW         */ 00064000
*/*                                                                  */ 00065000
*/********************************************************************/ 00066000
*                                                                  0449 00067000
*   DCL                                                            0449 00068000
*     REG8 REG(8) FIXED(32) RSTD;   /* COMPARE AND SWAP OLD VALUE    */ 00069000
*   DCL                                                            0450 00070000
*     REG9 REG(9) FIXED(32) RSTD;   /* COMPARE AND SWAP NEW VALUE    */ 00071000
*/*                                                                  */ 00072000
*/********************************************************************/ 00073000
*                                                                  0451 00074000
*                                                                  0451 00075000
*/********************************************************************/ 00076000
*/*                                                                  */ 00077000
*/*                     CONSTANTS                                    */ 00078000
*/*                                                                  */ 00079000
*/********************************************************************/ 00080000
*                                                                  0451 00081000
*   DCL                                                            0451 00082000
*     OFF BIT(1) CONSTANT('0'B);    /* ON & OFF ARE GENERAL PURPOSE  */ 00083000
*   DCL                                                            0452 00084000
*     ON BIT(1) CONSTANT('1'B);     /* MASKS TO SET & REF FLAG BITS  */ 00085000
*   DCL                                                            0453 00086000
*     CLEAR FIXED(32) CONSTANT(0000);/* MASK FOR CLEARING AND SET- 0453 00087000
*                                      TING FLAGS                    */ 00088000
*/********************************************************************/ 00089000
*/*                                                                  */ 00090000
*/*        THE FOLLOWING DECLARES ARE USED IN COMPARE AND SWAP       */ 00091000
*/*        AND ARE DEPENDENT UPON THE FMCB FLAG FIELD                */ 00092000
*/*                                                                  */ 00093000
*/********************************************************************/ 00094000
*                                                                  0454 00095000
*   DCL                                                            0454 00096000
*     FLGA06ON BIT(32) CONSTANT('82000000'X);                      0454 00097000
*   DCL                                                            0455 00098000
*     FLGA1ON BIT(32) CONSTANT('40000000'X);                       0455 00099000
*   DCL                                                            0456 00100000
*     FLGA1OFF BIT(32) CONSTANT('BFFFFFFF'X);                      0456 00101000
*   DCL                                                            0457 00102000
*     FLGB7ON BIT(32) CONSTANT('00010000'X);/* USED TO TURN ON THE 0457 00103000
*                                      FIRST OPERATION FLAG IN THE 0457 00104000
*                                      FMCB                  @XM10957*/ 00105000
*                                                                  0458 00106000
*/*                                                                  */ 00107000
*/*************************MAINLINE CODE BEGINS HERE******************/ 00108000
*/*                                                                  */ 00109000
*                                                                  0458 00110000
*   CRRC20=ON;                      /* SET SOLICIT AUDIT TRAIL BIT   */ 00111000
         OI    CRRC20(CRRPTR),B'00000001'                          0458 00112000
*   CRRMAS=CRRMAS&&CRRMAS;          /* ZERO PREVIOUS MODULE AUDIT  0459 00113000
*                                      SETTINGS                      */ 00114000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0459 00115000
*   CRRM20=ON;                      /* SET SOLICIT MODULE AUDIT SW   */ 00116000
         OI    CRRM20(CRRPTR),B'00000001'                          0460 00117000
*   IF DEVINPUT=ON|                 /* IF 'INPUT DEVICE' THEN      0461 00118000
*                                      CONTINUE PROCESSING. ELSE SET    00119000
*                                      ERROR CODE & RETURN           */ 00120000
*       (RPRENTRY=RPRENTLN&         /* ENTRY FOR LINE        @ZA19853*/ 00121000
*       RLNPORT=ON&                 /* THERE IS A PORT       @ZA19853*/ 00122000
*       RPTATERM^=0&                /* THERE IS A TERM       @ZA19853*/ 00123000
*       ADDR(RPTATERM->RPRDEVCH)->DEVINPUT=ON) THEN/* IT'S INPT    0461 00124000
*                                                            @ZA19853*/ 00125000
         L     @02,FMCDNCBA(,FMCBPTR)                              0461 00126000
         TM    DEVINPUT+36(@02),B'10000000'                        0461 00127000
         BO    @RT00461                                            0461 00128000
         L     @02,NCBRDTE(,@02)                                   0461 00129000
         CLI   RPRENTRY(@02),X'50'                                 0461 00130000
         BNE   @RF00461                                            0461 00131000
         TM    RLNPORT(@02),B'00010000'                            0461 00132000
         BNO   @RF00461                                            0461 00133000
         L     @02,RPTATERM(,@02)                                  0461 00134000
         LTR   @02,@02                                             0461 00135000
         BZ    @RF00461                                            0461 00136000
         TM    DEVINPUT+44(@02),B'10000000'                        0461 00137000
         BNO   @RF00461                                            0461 00138000
@RT00461 DS    0H                                                  0462 00139000
*     DO;                                                          0462 00140000
*                                                                  0462 00141000
*/********************************************************************/ 00142000
*                                                                  0463 00143000
*       IF(PROSETM4=PROSBLCK|       /* INSURE VALID READ MODIFIER    */ 00144000
*           PROSETM4=PROSMSG|PROSETM4=PROSTRNS|PROSETM4=PROSCONT) THEN  00145000
         LA    @02,48                                              0463 00146000
         ALR   @02,FMCBPTR                                         0463 00147000
         CLI   PROSETM4(@02),X'01'                                 0463 00148000
         BE    @RT00463                                            0463 00149000
         CLI   PROSETM4(@02),X'02'                                 0463 00150000
         BE    @RT00463                                            0463 00151000
         CLI   PROSETM4(@02),X'03'                                 0463 00152000
         BE    @RT00463                                            0463 00153000
         CLI   PROSETM4(@02),X'83'                                 0463 00154000
         BNE   @RF00463                                            0463 00155000
@RT00463 DS    0H                                                  0464 00156000
*         DO;                       /* READ TYPE SPECIFIED           */ 00157000
*           IF FMCFLGA0=ON THEN                                    0465 00158000
         TM    FMCFLGA0(FMCBPTR),B'10000000'                       0465 00159000
         BO    @RT00465                                            0465 00160000
*             ;                     /* IF DATA IS EXPECTED-LEAVE     */ 00161000
*                                                                  0466 00162000
*           /*********************************************************/ 00163000
*           /*                                                       */ 00164000
*           /* DATA IS NOT EXPECTED, BUT IF THIS IS A GENERAL POLL   */ 00165000
*           /* DEVICE ALL DATA COMING IN IS UNSOLICITED, THEREFORE IF*/ 00166000
*           /* THIS IS NOT THE FIRST I/O OPERATION TO THIS WE DO NOT */ 00167000
*           /* WANT TO DO ANYTHING                                   */ 00168000
*           /*                                                       */ 00169000
*           /*********************************************************/ 00170000
*                                                                  0467 00171000
*           ELSE                    /* CHECK FOR GENERAL POLL        */ 00172000
*             IF(DEVTCODE>=DEV3277&DEVTCODE<=DEV3275)&FMCFLGB7=ON THEN  00173000
         LA    @02,36                                              0467 00174000
         AL    @02,FMCDNCBA(,FMCBPTR)                              0467 00175000
         CLI   DEVTCODE(@02),X'19'                                 0467 00176000
         BL    @RF00467                                            0467 00177000
         CLI   DEVTCODE(@02),X'1C'                                 0467 00178000
         BH    @RF00467                                            0467 00179000
         TM    FMCFLGB7(FMCBPTR),B'00000001'                       0467 00180000
         BO    @RT00467                                            0467 00181000
*               ;                   /* GENERAL POLL AND NOT THE FIRST   00182000
*                                      OPERATION TO THE DEVICE     0468 00183000
*                                                            @XM10957*/ 00184000
*             ELSE                                                 0469 00185000
*               DO;                 /* DATA NOT EXPECTED/3270 DEV    */ 00186000
*                                                                  0469 00187000
@RF00467 DS    0H                                                  0470 00188000
*                 /***************************************************/ 00189000
*                 /*                                                 */ 00190000
*                 /* THIS IS NOT THE FIRST OPERATION TO THIS DEVICE, */ 00191000
*                 /* IF THIS IS A GENERAL POLL DEVICE THEN WE WANT TO*/ 00192000
*                 /* TURN ON THE FLAG IN THE FMCB, INDICATING THE    */ 00193000
*                 /* FIRST I/O OPERATION HAS GONE            @XM10957*/ 00194000
*                 /*                                                 */ 00195000
*                 /***************************************************/ 00196000
*                                                                  0470 00197000
*                 IF(DEVTCODE>=DEV3277&DEVTCODE<=DEV3275) THEN     0470 00198000
         LA    @02,36                                              0470 00199000
         AL    @02,FMCDNCBA(,FMCBPTR)                              0470 00200000
         CLI   DEVTCODE(@02),X'19'                                 0470 00201000
         BL    @RF00470                                            0470 00202000
         CLI   DEVTCODE(@02),X'1C'                                 0470 00203000
         BH    @RF00470                                            0470 00204000
*                   DO;                                            0471 00205000
*                     REG8=FMCCMPS2;/* GET SET UP FO CS      @XM10957*/ 00206000
         L     REG8,FMCCMPS2(,FMCBPTR)                             0472 00207000
*CS0:                                                              0473 00208000
*                     REG9=REG8|FLGB7ON;/*                   @XM10957*/ 00209000
CS0      LR    REG9,REG8                                           0473 00210000
         O     REG9,@CF01999                                       0473 00211000
*                     CS(REG8,REG9,FMCCMPS2);/* CHANGE FMCB@XM10957  */ 00212000
         CS    REG8,@09,FMCCMPS2(FMCBPTR)                          0474 00213000
*                     BC(7,CS0);    /* FAILED,RETRY          @XM10957*/ 00214000
         BC    7,CS0                                               0475 00215000
*                   END;            /* END 3270 DO GROUP     @XM10957*/ 00216000
*                 ELSE                                             0477 00217000
*                   ;               /*                       @XM10957*/ 00218000
@RF00470 DS    0H                                                  0478 00219000
*CS1:                                                              0478 00220000
*                 REG8=FMCCMPS2;    /* REFERENCE OLD VALUE           */ 00221000
CS1      L     REG8,FMCCMPS2(,FMCBPTR)                             0478 00222000
*                 REG9=REG8|FLGA06ON;/* SET NEW FLAG VALUES- DATA  0479 00223000
*                                      EXPECTED,READ DONE            */ 00224000
         LR    REG9,REG8                                           0479 00225000
         O     REG9,@CF03808                                       0479 00226000
*                 CS(REG8,REG9,FMCCMPS2);/* SET NEW VALUE IN FMCB    */ 00227000
         CS    REG8,@09,FMCCMPS2(FMCBPTR)                          0480 00228000
*                 BC(7,CS1);        /* BRANCH IF FAILURE             */ 00229000
         BC    7,CS1                                               0481 00230000
*                 CRRDEXP=ON;       /* SET 'DATA EXPECTED' IN CRR    */ 00231000
         OI    CRRDEXP(CRRPTR),B'10000000'                         0482 00232000
*                 CRRSOLRQ=ON;      /* SET 'SOLICIT REQUEST' IN CRR  */ 00233000
*                 CRRINIO=ON;       /* SET 'INITIATE I/O' IN CRR     */ 00234000
         OI    CRRSOLRQ(CRRPTR),B'00100001'                        0484 00235000
*                 IF PROSETM4=PROSBLCK/* IF FMCB SPECIFIES BLOCK MODE*/ 00236000
*                   THEN                                           0485 00237000
         CLI   PROSETM4+48(FMCBPTR),X'01'                          0485 00238000
         BNE   @RF00485                                            0485 00239000
*                   LCCWRWOP=LRB;   /* INDICATE READ BLOCK IN LCPB   */ 00240000
         L     @02,LCPNXLCW(,LCPBPTR)                              0486 00241000
         NI    LCCWRWOP(@02),B'11000110'                           0486 00242000
         OI    LCCWRWOP(@02),B'00000110'                           0486 00243000
*                 ELSE                                             0487 00244000
*                   IF PROSETM4=PROSMSG/* IF FMCB SPECIFIES MSG MODE */ 00245000
*                     THEN                                         0487 00246000
         B     @RC00485                                            0487 00247000
@RF00485 CLI   PROSETM4+48(FMCBPTR),X'02'                          0487 00248000
         BNE   @RF00487                                            0487 00249000
*                     LCCWRWOP=LRM; /* INDICATE READ MSG IN LCPB     */ 00250000
         L     @02,LCPNXLCW(,LCPBPTR)                              0488 00251000
         NI    LCCWRWOP(@02),B'11001010'                           0488 00252000
         OI    LCCWRWOP(@02),B'00001010'                           0488 00253000
*                   ELSE                                           0489 00254000
*                     IF PROSETM4=PROSTRNS/* IF FMCB SPECIFIES TRN 0489 00255000
*                                      MODE                          */ 00256000
*                       THEN                                       0489 00257000
         B     @RC00487                                            0489 00258000
@RF00487 CLI   PROSETM4+48(FMCBPTR),X'03'                          0489 00259000
         BNE   @RF00489                                            0489 00260000
*                       LCCWRWOP=LRT;/* INDICATE READ TRN IN LCPB    */ 00261000
         L     @02,LCPNXLCW(,LCPBPTR)                              0490 00262000
         NI    LCCWRWOP(@02),B'11001110'                           0490 00263000
         OI    LCCWRWOP(@02),B'00001110'                           0490 00264000
*                     ELSE                                         0491 00265000
*                       LCCWRWOP=LRCONT;/* INDICATE READ CONTN IN  0491 00266000
*                                      LCPB                          */ 00267000
         B     @RC00489                                            0491 00268000
@RF00489 L     @02,LCPNXLCW(,LCPBPTR)                              0491 00269000
         NI    LCCWRWOP(@02),B'11011010'                           0491 00270000
         OI    LCCWRWOP(@02),B'00011010'                           0491 00271000
*                 DCLRPLA=CLEAR;    /* CLEAR RPL ADDR IN DCLCP       */ 00272000
@RC00489 DS    0H                                                  0492 00273000
@RC00487 DS    0H                                                  0492 00274000
@RC00485 SLR   @02,@02                                             0492 00275000
         ST    @02,DCLRPLA(,LCPBPTR)                               0492 00276000
*               END;                /* END-DATA NOT EXPECTED         */ 00277000
*CS2:                                                              0494 00278000
*           REG8=FMCCMPS2;          /* REFERENCE OLD VALUE           */ 00279000
@RT00467 DS    0H                                                  0494 00280000
@RT00465 DS    0H                                                  0494 00281000
CS2      L     REG8,FMCCMPS2(,FMCBPTR)                             0494 00282000
*           IF RPLDLGIN=OFF THEN    /* IF 'CONTINUE ANY' IS INDICATED   00283000
*                                      IN RPL                        */ 00284000
         TM    RPLDLGIN(RPLPTR),B'10000000'                        0495 00285000
         BNZ   @RF00495                                            0495 00286000
*             REG9=REG8&FLGA1OFF;   /* RESET SATISFY SPECIFIC READ   */ 00287000
         LR    REG9,REG8                                           0496 00288000
         N     REG9,@CF02486                                       0496 00289000
*           ELSE                                                   0497 00290000
*             REG9=REG8|FLGA1ON;    /* SET SATISFY SPEC READ         */ 00291000
         B     @RC00495                                            0497 00292000
@RF00495 LR    REG9,REG8                                           0497 00293000
         O     REG9,@CF01973                                       0497 00294000
*           CS(REG8,REG9,FMCCMPS2); /* SET NEW VALUE                 */ 00295000
@RC00495 CS    REG8,@09,FMCCMPS2(FMCBPTR)                          0498 00296000
*           BC(7,CS2);              /* RETRY IF FAILURE              */ 00297000
         BC    7,CS2                                               0499 00298000
*         END;                      /* END OF NORMAL SOLICIT       0500 00299000
*                                      PROCESSING                    */ 00300000
*       ELSE                                                       0501 00301000
*         DO;                       /* READ TYPE NOT SPECIFIED       */ 00302000
         B     @RC00463                                            0501 00303000
@RF00463 DS    0H                                                  0502 00304000
*           CRRFDB2=CRRINVRM;       /* INDICATE READ TYPE NOT GIVEN  */ 00305000
         MVI   CRRFDB2(CRRPTR),X'2A'                               0502 00306000
*           CRRRTNCD=CRRLOGIC;      /* INDICATE LOGICAL ERROR        */ 00307000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0503 00308000
*         END;                                                     0504 00309000
*     END;                          /* END DEVICE-NOT-OUTPUT-ONLY    */ 00310000
*   ELSE                            /* IF DEVICE IS OUTPUT ONLY      */ 00311000
*     DO;                                                          0506 00312000
         B     @RC00461                                            0506 00313000
@RF00461 DS    0H                                                  0507 00314000
*       CRRFDB2=CRRSTOOD;           /* SET 'OUTPUT-ONLY DEVICE' ERROR   00315000
*                                      CODE                          */ 00316000
         MVI   CRRFDB2(CRRPTR),X'0E'                               0507 00317000
*       CRRRTNCD=CRRLOGIC;          /* & 'LOGIC ERROR' IN CRR        */ 00318000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0508 00319000
*     END;                                                         0509 00320000
*                                                                  0509 00321000
*/*TPESC                                                             */ 00322000
*                                                                  0510 00323000
*   DO;                                                            0510 00324000
*                                                                  0510 00325000
@RC00461 DS    0H                                                  0511 00326000
*/* ATBUILD (ISTDYPAB,ISTAVT,ISTATCVT)                               */ 00327000
*                                                                  0511 00328000
*              /* DYNAMIC PAB 1K633 */                             0511 00329000
* /* 1K0997 JAN 20, 1973  MB   */                                  0511 00330000
* /* 1K1142 MAR 03, 1973  MB   */                                  0511 00331000
* DCL 1 ISTDYPAB BDY(DWORD) BASED, /* LEVEL 1 DECLARE 1K633 */     0511 00332000
*       2  DYPTYPE CHAR(1), /* CONTROL BLOCK TYPE CODE 1K633 */    0511 00333000
*       2  DYPLNGTH PTR(8), /* CONTROL BLOCK LENGTH (BYTES)  1K633 */   00334000
*       2 DYPFLGS CHAR(2),     /* FLAGS 1K633 */                   0511 00335000
*       2 DYPCHAIN PTR(31),    /* CHAIN POINTER 1K633 */           0511 00336000
*       2 DYPTSKID PTR(31),    /* TASKID OR PTR TO PSS TABLES 1K633 */  00337000
