*/* VTAM ADDRESS VECTOR TABLE */                                   0513 00343000
*/*  REVISIONS                                         002 03/06/75  */ 00344000
*DCL  1  ISTAVT BDY(WORD) BASED,                                   0513 00345000
*       2  ISTACVT PTR(31),            /* ADDR OF VTAM CVT           */ 00346000
*       2  ISTAS49 PTR(31),            /* ADDR OF SVC 49 CODE        */ 00347000
*          3  ISTAHIP BIT(1),          /* HALT IN PROGRESS           */ 00348000
*       2  ISTAS53 PTR(31),            /* ADDR OF SVC 53 CODE        */ 00349000
*       2  ISTAPSTA PTR(31),           /* ADDR OF APS TABLE          */ 00350000
*       2  AVTASID FIXED(15),          /* VTAM ASID                  */ 00351000
*       2  ISTVTTIK FIXED(15),         /* VTAMRP TASK ID             */ 00352000
*       2  ISTAPSEX PTR(31),           /* ADDR OF APS EXIT           */ 00353000
*       2  ISTPHNM CHAR(5),            /* PHASE NAME OF TRANSIENT    */ 00354000
*       2  ISTX1 CHAR(1),              /* TEST FIELD                 */ 00355000
*       2  ISTARID PTR(15),            /* ADDR OF RID                */ 00356000
*       2  ISTATRT PTR(15),            /* ADDR OF VTAM GATES IN RETAB*/ 00357000
*       2  ISTAGTWT PTR(15),           /* ADDR OF GATEWAIT ROUTINE   */ 00358000
*       2  ISTVTTP  PTR(15),           /* ADDR OF CODE TO CHECK FOR  */ 00359000
*                                      /* PENDING TIMER INTERRUPT    */ 00360000
*       2  ISTAVR01 CHAR(2),           /* RESERVED                   */ 00361000
*       2  ISTPDAVP PTR(31);           /* VECTOR FOR PDAIDS SMS TRACE*/ 00362000
*/*  REVISIONS                                         015 11/16/75   * 00363000
*DCL ATCLCPTR PTR LOCATION(1032);      /* POINTER TO ATCVT            * 00364000
*DCL  1  ISTATCVT BDY(DWORD) BASED,                                0515 00365000
*        2  ATCCOM BDY(WORD),          /* SYSTEM INDEPENDENT FIELDS   * 00366000
*           3  ATCSTAT CHAR(4),        /* VTAM  STATUS INDICATORS     * 00367000
*              4  ATCSTAT1 CHAR(1),    /* VTAM  STATUS BYTE 1         * 00368000
*                 5  ATCSTART BIT(1),  /* VTAM  IS STARTING           * 00369000
*                 5  ATCACTIV BIT(1),  /* VTAM  IS ACTIVE             * 00370000
*                 5  ATCSNHLT BIT(1),  /* VTAM  SESSION HALT          * 00371000
*                 5  ATCQKHLT BIT(1),  /* VTAM  QUICK HALT            * 00372000
*                 5  ATCNETSL BIT(1),  /* NETSOL=YES SPECIFIED ON CMD * 00373000
*                 5  ATCINHLT BIT(1),  /* INTERNAL VTAM HALT QUICK    * 00374000
*                 5  ATCMDHLT BIT(1),  /* VTAM CMD PROC HALTED        * 00375000
*                 5 * BIT(1),          /* RESERVED                    * 00376000
*              4  ATCSTFLG BIT(8),     /* STORAGE MANAGEMENT FLAGS    * 00377000
*                 5  ATCHSDMF BIT(1),  /* SYSTEM SLOW DOWN FLAG       * 00378000
*              4  ATCPOFLG BIT(8),     /* PROGRAM OPERATOR            * 00379000
*                                      /* INTERFACE FLAGS             * 00380000
*                 5 ATCPOUNS BIT(1),   /* APPLICATION AUTHORIZED TO   * 00381000
*                                      /* RECEIVE UNSOLICITED MESSAGES* 00382000
*                                      /* 1 = YES                     * 00383000
*                 5 *        BIT(7),   /* RESERVED                    * 00384000
*              4  ATCSTAT4 CHAR(1),    /* VTAM  STATUS BYTE 4         * 00385000
*                 5  ATCPRTAT BIT(1),  /* 1 = TRACE PRINT ACTIVE,  0515 00386000
