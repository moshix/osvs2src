*   END PROCESSC;                   /* RETURN TO MAINLINE            */ 00850000
@EL00003 DS    0H                                                  0613 00851000
@EF00003 DS    0H                                                  0613 00852000
@ER00003 BR    @14                                                 0613 00853000
*                                                                  0614 00854000
*/********************************************************************/ 00855000
*/*                                                                  */ 00856000
*/* ROUTINE NAME : PROCESSCA - PROCESS MORE DATA                     */ 00857000
*/*                                                                  */ 00858000
*/* FUNCTION : PROCESSCA DOES THE FOLLOWING :                        */ 00859000
*/* IF MOVE HAS INDICATED 'MORE DATA', PROCESCA EITHER FREES FSB(S)  */ 00860000
*/* AND FDB(S) AND SETS 'TRUNCATION REQUESTED',OR SETS'PARTIAL DATA',*/ 00861000
*/* AND RETURNS TO PROCESSC.  IF MOVE HAS NOT INDICATED 'MORE DATA'  */ 00862000
*/* PROCESCA EITHER CALLS PROCESSE TO FREE FSB AND FDB STORAGE, OR   */ 00863000
*/* CALLS PROCESSD TO HANDLE SUBBLOCK PROCESSING, DEPENDING UPON     */ 00864000
*/* OTHER CONDITIONS.                                                */ 00865000
*/*                                                                  */ 00866000
*/* INPUT : SEE INPUT TABLE BELOW                                    */ 00867000
*/*                                                                  */ 00868000
*/* OUTPUT : SEE OUTPUT TABLE BELOW                                  */ 00869000
*/*                                                                  */ 00870000
*/* SUBROUTINES CALLED : PROCESSD , PROCESSE                         */ 00871000
*/*                                                                  */ 00872000
*/* MACROS USED : NONE                                               */ 00873000
*/*                                                                  */ 00874000
*/********************************************************************/ 00875000
*/* * * * * * * * * * INPUT TABLE FOR PROCESCA * * * * * * * * * * * */ 00876000
*/*                                                                  */ 00877000
*/*   CRRBRTCB SHOULD HAVE BEEN SET=4 (MEANING MORE DATA) BY MOVE IF */ 00878000
*/*        THERE IS MORE DATA TO MOVE                                */ 00879000
*/*   CRRBLEN SHOULD CONTAIN THE RESIDUAL LENGTH (IN WHICH CASE      */ 00880000
*/*        CALL PROCESSD TO HANDLE SUBBLOCKS), OR ZERO (IN WHICH     */ 00881000
*/*        CASE CALL PROCESSE TO FREE FSB'S AND FDB'S                */ 00882000
*/*   FSBEBI  , FSBEMI, OR FSBETI INDICATE 'END OF RECORD'           */ 00883000
*/*        AND IF SET, INDICATE THAT FSB'S & FDB'S SHOULD BE FREED   */ 00884000
*/*   PROTRUNC   SHOULD BE SET IF USER REQUEST TRUNCATION            */ 00885000
*/*                                                                  */ 00886000
*/* * * * * * * * * * OUTPUT FROM PROCESCA* * * * * * * * * * * * * **/ 00887000
*/*                                                                  */ 00888000
*/* FIELDS LISTED BELOW COMPRISE POSSIBLE OUTPUT FROM THIS PROCEDURE.*/ 00889000
*/* IF A FIELD IS ASSIGNED A VALUE FROM AN INPUT FIELD, THE INPUT    */ 00890000
*/* FIELD IS NOTED BETWEEN ASTERISKS.  IF A FIELD'S BEING SET IS     */ 00891000
*/* LOGICALLY DEPENDENT UPON A TEST OF AN INPUT FIELD, THE INPUT     */ 00892000
*/* FIELD IS SHOWN BETWEEN PARENTHESES:                              */ 00893000
*/*                                                                  */ 00894000
*/* CRRRTN3*RESUMEAD*   CRRSW2(CRRBRTCD)    CRRF4(RPLRTRUN)          */ 00895000
*/* CRRRTNCD(RPLRTRUN)     "  (LIBLK)       CRRPDATA(RPLRTRUN)       */ 00896000
*/* CRRRDEOB(RPLRTRUN)     "  (LIMSG)                                */ 00897000
*/* CRRRDEOM(RPLRTRUN)     "  (LITRN)                                */ 00898000
*/* CRRDEOT(RPLRTRUN)   RESUMEAD*CRRRTN3*                            */ 00899000
*/*                                                                  */ 00900000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/ 00901000
*                                                                  0614 00902000
*PROCESCA:                                                         0614 00903000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0614 00904000
PROCESCA DS    0H                                                  0615 00905000
*   CRRRTN3=RESUMEAD;               /* SAVE RETURN ADDR (REG14)      */ 00906000
         ST    RESUMEAD,CRRRTN3(,CRRPTR)                           0615 00907000
*   RFY                                                            0616 00908000
*     RESUMEAD UNRSTD;                                             0616 00909000
*   CRRSW2=OFF;                     /* INIT SWITCH THAT'LL INDICATE     00910000
*                                      WHETHER OR NOT THIS ROUTINE 0617 00911000
*                                      SHOULD HANDLE TRUNCATION    0617 00912000
*                                      PROBLEMS SET FEEDBACK STATUS     00913000
*                                      IN CRR:                       */ 00914000
         NI    CRRSW2(CRRPTR),B'11011111'                          0617 00915000
*   RFY                                                            0618 00916000
*    (REG9,                                                        0618 00917000
*     REG10) RSTD;                                                 0618 00918000
*CS3:                                                              0619 00919000
*   REG9=FMCCMPS2;                  /* REFERENCE FMCB FLAGS          */ 00920000
CS3      L     REG9,FMCCMPS2(,FMCBPTR)                             0619 00921000
*   IF RPLDLGIN=OFF THEN            /* IF CONTINUE ANY               */ 00922000
         TM    RPLDLGIN(RPLPTR),B'10000000'                        0620 00923000
         BNZ   @RF00620                                            0620 00924000
*     REG10=REG9&FLGA1OFF;          /* RESET SATISFY SPECIFIC READ   */ 00925000
         LR    REG10,REG9                                          0621 00926000
         N     REG10,@CF01327                                      0621 00927000
*   ELSE                                                           0622 00928000
*     REG10=REG9|FLGA1ON;           /* SET SATISFY SPECIFIC READ     */ 00929000
         B     @RC00620                                            0622 00930000
@RF00620 LR    REG10,REG9                                          0622 00931000
         O     REG10,@CF01329                                      0622 00932000
*   CS(REG9,REG10,FMCCMPS2);        /* SET NEW FLAG VALUE            */ 00933000
@RC00620 CS    REG9,@10,FMCCMPS2(FMCBPTR)                          0623 00934000
*   BC(7,CS3);                      /* TRY AGAIN IF FAILED           */ 00935000
         BC    7,CS3                                               0624 00936000
*   RFY                                                            0625 00937000
*    (REG9,                                                        0625 00938000
*     REG10) UNRSTD;                                               0625 00939000
*   CRRRTNCD=FSBSRCAT;              /* SYSTEM RESPONSE CATEGORY      */ 00940000
         MVC   CRRRTNCD(1,CRRPTR),FSBSRCAT(FSBPTR)                 0626 00941000
*   CRRFDB2=FSBMNRCD;               /* MINOR RETURN CODE OR PHYSICAL    00942000
*                                      CONDITION FLAGS               */ 00943000
         MVC   CRRFDB2(1,CRRPTR),FSBMNRCD(FSBPTR)                  0627 00944000
*   CRRFDB3=FSBERC;                 /* EXTENDED RESPONSE CONDITIONS  */ 00945000
         MVC   CRRFDB3(1,CRRPTR),FSBERC(FSBPTR)                    0628 00946000
*   CRRESR1=FSBSYSRS;               /* SYSTEM RE'POSE                */ 00947000
         MVC   CRRESR1(1,CRRPTR),FSBSYSRS(FSBPTR)                  0629 00948000
*   CRRESR2=FSBEXTRS;               /* EXTENDED RESPONSE             */ 00949000
         MVC   CRRESR2(1,CRRPTR),FSBEXTRS(FSBPTR)                  0630 00950000
*   IF(CRRRTNCD=CRRPHYSC)&(CRRSTSAV=ON) THEN/* IF SENSE DATA IS    0631 00951000
*                                      AVAILABLE RECORD ADDITIONAL 0631 00952000
*                                      FEEDBACK DATA:                */ 00953000
