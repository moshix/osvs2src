*         IF CRRRDEOB=OFF&          /* IF 'SUBBLOCK' INDIC   @D8355CF*/ 00986000
*             CRRRLG=OFF&           /* NO LEADING GRAPHICS (D1869)   */ 00987000
*             CRRRDSOH=OFF&         /* NO BEGINNING SOH (D1869)      */ 00988000
*             CRRRTNCD=CRRNOERR&    /* NON-ERROR RETURN CODE         */ 00989000
