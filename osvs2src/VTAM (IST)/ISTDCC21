*     IF PROTRUNC=ON THEN           /* IF RPL INDICATES TRUNCATION   */ 01026000
         TM    PROTRUNC+48(FMCBPTR),B'01000000'                    0648 01027000
         BNO   @RF00648                                            0648 01028000
*       CALL PROCESSE;              /* FREE FSB AND FDB STORAGE      */ 01029000
         BAL   @14,PROCESSE                                        0649 01030000
*     ELSE                                                         0650 01031000
*       DO;                         /* IF NO TRUNCATION REQESTED     */ 01032000
