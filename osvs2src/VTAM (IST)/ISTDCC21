*   DCL                                                            0424 00098000
*     REG10 REG(10) FIXED(32);                                     0424 00099000
*   DCL                                                            0425 00100000
*     COUNTER REG(2) FIXED(32);                                    0425 00101000
*                                                                  0426 00102000
*/********************************************************************/ 00103000
*/*                                                                  */ 00104000
*/*                     CONSTANTS AND REGISTERS                      */ 00105000
*/*           NOT USED AS BASE REGISTERS FOR CONTROL BLOCKS          */ 00106000
*/*                                                                  */ 00107000
*/********************************************************************/ 00108000
*                                                                  0426 00109000
*   DCL                                                            0426 00110000
*     OFF BIT(1) CONSTANT('0'B);    /* MASK TO TURN BITS OFF         */ 00111000
*   DCL                                                            0427 00112000
*     ON BIT(1) CONSTANT('1'B);     /* MASK TO TURN BITS ON          */ 00113000
*   DCL                                                            0428 00114000
*     GATE0 BIT(32) CONSTANT('80000000'X);                         0428 00115000
*   DCL                                                            0429 00116000
*     CLEAR FIXED(32) CONSTANT(0000);/* MASK FOR ADDRS AND FLAGS     */ 00117000
*   DCL                                                            0430 00118000
