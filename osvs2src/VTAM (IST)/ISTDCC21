*           REG10=REG9|FLGA06ON;    /* TURN ON DATA EXPECTED AND READ   00523000
*                                      DONE TO DEVICE                */ 00524000
         LR    REG10,REG9                                          0523 00525000
         O     REG10,@CF04257                                      0523 00526000
*           CS(REG9,REG10,FMCCMPS2);/* CHANGE FLAG VALUE             */ 00527000
