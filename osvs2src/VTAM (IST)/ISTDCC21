*           IF RPLDLGIN=OFF THEN    /* IF CONTINUE ANY               */ 00537000
         TM    RPLDLGIN(RPLPTR),B'10000000'                        0528 00538000
         BNZ   @RF00528                                            0528 00539000
*             REG10=REG9&FLGA1OFF;  /* RESET SATISFY SPEC READ       */ 00540000
         LR    REG10,REG9                                          0529 00541000
         N     REG10,@CF01327                                      0529 00542000
*           ELSE                                                   0530 00543000
*             REG10=REG9|FLGA1ON;   /* SET SATISFY SPECIFIC READ     */ 00544000
         B     @RC00528                                            0530 00545000
@RF00528 LR    REG10,REG9                                          0530 00546000
         O     REG10,@CF01329                                      0530 00547000
*           CS(REG9,REG10,FMCCMPS2);/* SET NEW FLAG VALUES           */ 00548000
@RC00528 CS    REG9,@10,FMCCMPS2(FMCBPTR)                          0531 00549000
*           BC(7,CS2);              /* TRY AGAIN IF FAILURE          */ 00550000
         BC    7,CS2                                               0532 00551000
*           RFY                                                    0533 00552000
*            (REG9,                                                0533 00553000
*             REG10) UNRSTD;                                       0533 00554000
*         END;                      /* END OF DATA NOT EXPECTED DO   */ 00555000
*       ELSE                                                       0535 00556000
*         DO;                       /* DATA IS EXPECTED              */ 00557000
         B     @RC00497                                            0535 00558000
@RF00497 DS    0H                                                  0536 00559000
*           CRRRTNCD=CRRLOGIC;      /* LGIC ERROR                    */ 00560000
