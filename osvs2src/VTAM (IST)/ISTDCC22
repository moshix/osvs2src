*/* TABLE FOR PROCESSA                                               */ 00170000
*/*------------------------------------------------------------------*/ 00171000
*/* CB       FIELD     R/S MEANING                                   */ 00172000
*/*------------------------------------------------------------------*/ 00173000
*/* CRR      CRRFDB2   R/S CHECKED FOR ERROR CODE,                   */ 00174000
*/*                        MAY BE SET WITH ERROR CODE                */ 00175000
*/*          CRRRTN1   R/S SAVE R14 FOR RETURN TO MAIN PROC          */ 00176000
*/*          CRRRTNCD  S   SET TO 8 IF ERROR OCCURS                  */ 00177000
*/*                                                                  */ 00178000
*/* FMCB     FMCDE270  R   1 IF 3270                                 */ 00179000
*/*                                                                  */ 00180000
*/* LCCW     LCCWCNT   S   COUNT                                     */ 00181000
*/*          LCCWDATA  S   ADDR OF DATA                              */ 00182000
*/*          LCCWRWOP  S   OP CODE                                   */ 00183000
*/*          LCCWOP    S   OP CODE                                   */ 00184000
*/*                                                                  */ 00185000
*/* RPL      RPLAREA   R   CONTAINS ADDRESS OF DATA                  */ 00186000
*/*          RPLEAU    R   1 IF ERASE ALL UNPROTECTED                */ 00187000
*/*          RPLEOB    R   1 IF BLOCK OPERATION                      */ 00188000
*/*          RPLEOM    R   1 IF MESSAGE OPERATION                    */ 00189000
*/*          RPLEOT    R   1 IF TRANSMISSION OPERATION               */ 00190000
*/*          RPLERAS   R   1 IF ERASE WRITE                          */ 00191000
*/*          RPLRLEN   R   CONTAINS LENGTH OF DATA                   */ 00192000
*/*                                                                  */ 00193000
*/* R14                S   RESTORED TO RETURN ADDR                   */ 00194000
*/*------------------------------------------------------------------*/ 00195000
*/********************************************************************/ 00196000
*/*                                                                  */ 00197000
*/* SEGMENT NAME : PROCESSA                                          */ 00198000
*/*                                                                  */ 00199000
*/* FUNCTION : TO FILTER OUT ERROR SITUATIONS AND BUILD AN LCCW      */ 00200000
*/*            FOR DEVICES OTHER THAN 3270S.  FOR 3270S A WRITE-     */ 00201000
*/*            ERASE-ALL-UNPROTECTED LCCW WILL BE BUILT IF IT IS     */ 00202000
*/*            THE LAST IN TRANSMISSION.                             */ 00203000
*/* INPUT : RPL,DCLCP,FMCB, & RPH ADDRESSES                          */ 00204000
*/*                                                                  */ 00205000
*/* OUTPUT : LCCWS THAT WERE DESCRIBED UNDER 'FUNCTION.'             */ 00206000
*/*                                                                  */ 00207000
*/* SUBROUTINES CALLED :                                             */ 00208000
*/*                                                                  */ 00209000
*/* MACROS USED : NONE                                               */ 00210000
*/*                                                                  */ 00211000
*/********************************************************************/ 00212000
* IF CRRFDB2 = ZERO THEN DO;        /* NO ERROR CODE IN CRR          */ 00213000
@RC00487 CLI   CRRFDB2(CRRPTR),0                                   0521 00214000
         BNE   @RF00521                                            0521 00215000
*     IF (DEVCHAR2 >= DEV3277 & DEVCHAR2 <= DEV3275) THEN          0523 00216000
         LA    @15,36                                              0523 00217000
         AL    @15,FMCDNCBA(,FMCBPTR)                              0523 00218000
         CLI   DEVCHAR2(@15),X'19'                                 0523 00219000
         BL    @RF00523                                            0523 00220000
         CLI   DEVCHAR2(@15),X'1C'                                 0523 00221000
         BH    @RF00523                                            0523 00222000
*       IF RPLEOM=OFF & RPLEOT=OFF THEN   /* WRT BLK TO 3270 ILLEGIT */ 00223000
         TM    RPLEOM(RPLPTR),B'01100000'                          0524 00224000
         BNZ   @RF00524                                            0524 00225000
*          DO;                                                     0525 00226000
*           CRRRTNCD=CRRLOGIC;      /* SET ERROR IN MAJOR RTN CODE   */ 00227000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0526 00228000
*           CRRFDB2=CRRWRBLK;       /* SET ERROR IN MINOR RTN CODE   */ 00229000
         MVI   CRRFDB2(CRRPTR),X'24'                               0527 00230000
*          END;                                                    0528 00231000
*       ELSE;                                                      0529 00232000
@RF00524 DS    0H                                                  0530 00233000
*     ELSE;                                                        0530 00234000
@RF00523 DS    0H                                                  0531 00235000
*    IF RPLEAU = ON THEN            /* ERASE ALL UNPROTECTED         */ 00236000
         TM    RPLEAU(RPLPTR),B'00001000'                          0531 00237000
         BNO   @RF00531                                            0531 00238000
*    IF (DEVCHAR2 >= DEV3277 & DEVCHAR2 <= DEV3275) THEN           0532 00239000
         LA    @15,36                                              0532 00240000
         AL    @15,FMCDNCBA(,FMCBPTR)                              0532 00241000
         CLI   DEVCHAR2(@15),X'19'                                 0532 00242000
         BL    @RF00532                                            0532 00243000
         CLI   DEVCHAR2(@15),X'1C'                                 0532 00244000
         BH    @RF00532                                            0532 00245000
*          IF RPLWROPT=OFF THEN DO;                                0533 00246000
         TM    RPLWROPT(RPLPTR),B'00000001'                        0533 00247000
         BNZ   @RF00533                                            0533 00248000
*          LCCWRWOP = LEAU;         /* WRITE/ERASE ALL UNPROTECTED   */ 00249000
         NI    LCCWRWOP(LCCWPTR),B'11010001'                       0535 00250000
         OI    LCCWRWOP(LCCWPTR),B'00010001'                       0535 00251000
*          LCCWCNT = RPLRLEN;       /* FILL IN COUNT                 */ 00252000
         L     @00,RPLRLEN(,RPLPTR)                                0536 00253000
         STH   @00,LCCWCNT(,LCCWPTR)                               0536 00254000
*          LCCWDATA = RPLAREA;      /* FILL IN AREA                  */ 00255000
         L     @00,RPLAREA(,RPLPTR)                                0537 00256000
         ST    @00,LCCWDATA(,LCCWPTR)                              0537 00257000
*          END;                     /* END-3270&ERASE/WRITE ALL UNPROT* 00258000
*          ELSE DO;                 /* ERASE WRITE CONVERSATIONAL   */  00259000
         B     @RC00533                                            0539 00260000
@RF00533 DS    0H                                                  0540 00261000
*          CRRRTNCD=CRRLOGIC;                                      0540 00262000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0540 00263000
*          CRRFDB2=CRREWCNV;       /* ERASE WRITE CONV MINOR RTN CODE*/ 00264000
         MVI   CRRFDB2(CRRPTR),X'15'                               0541 00265000
*          END;                    /* END ERASE WRITE CONVERSATIONAL*/  00266000
*      ELSE DO;                     /* NOT 3270                      */ 00267000
         B     @RC00532                                            0543 00268000
@RF00532 DS    0H                                                  0544 00269000
*       CRRRTNCD = CRRLOGIC;        /* CRR ERROR CODE                */ 00270000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0544 00271000
*       CRRFDB2 = CRREWAU3;         /* USER ERROR CODE: ERASE WRITE  */ 00272000
         MVI   CRRFDB2(CRRPTR),X'13'                               0545 00273000
*                                   /* ALL UNPROT TO NON-3270 DEV    */ 00274000
*       END;                        /* END- NOT 3270                 */ 00275000
*    ELSE DO;                       /* NOT ERASE/WRITE ALL UNPROT    */ 00276000
         B     @RC00531                                            0547 00277000
@RF00531 DS    0H                                                  0548 00278000
*       IF RPLEOB = ON THEN         /* IF WRITE BLOCK                */ 00279000
         TM    RPLEOB(RPLPTR),B'10000000'                          0548 00280000
         BNO   @RF00548                                            0548 00281000
*       LCCWRWOP = LWB;             /* FILL WRITE BLOCK OP           */ 00282000
         NI    LCCWRWOP(LCCWPTR),B'11000101'                       0549 00283000
         OI    LCCWRWOP(LCCWPTR),B'00000101'                       0549 00284000
*       ELSE                                                       0550 00285000
*       IF RPLEOM = ON THEN         /* IF WRITE MESSAGE              */ 00286000
         B     @RC00548                                            0550 00287000
@RF00548 TM    RPLEOM(RPLPTR),B'01000000'                          0550 00288000
         BNO   @RF00550                                            0550 00289000
*       LCCWRWOP = LWM;             /* FILL IN WRITE MESSAGE OP      */ 00290000
         NI    LCCWRWOP(LCCWPTR),B'11001001'                       0551 00291000
         OI    LCCWRWOP(LCCWPTR),B'00001001'                       0551 00292000
*       ELSE                                                       0552 00293000
* IF RPLEOT=ON THEN LCCWRWOP = LWT; /* FILL IN WRITE TRANSMISSION    */ 00294000
         B     @RC00550                                            0552 00295000
@RF00550 TM    RPLEOT(RPLPTR),B'00100000'                          0552 00296000
         BNO   @RF00552                                            0552 00297000
         NI    LCCWRWOP(LCCWPTR),B'11001101'                       0553 00298000
         OI    LCCWRWOP(LCCWPTR),B'00001101'                       0553 00299000
*       LCCWDATA = RPLAREA;         /* FILL IN AREA                  */ 00300000
