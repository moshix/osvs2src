         TITLE 'ISTDCC22, CONTROL LAYER:  WRITE SPECIFIC MACRO PROCESSO*00001000
               OR       '                                               00002000
ISTDCC22 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC22  77.126'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
* CRRPTR = ADDR(CRACRR -> CRRSPACE);/* PLACE ADDR OF CRR IN PTR    */   00013000
         L     @15,RPHCRR(,ADDRRPH)                                0456 00014000
         L     CRRPTR,CRACRR(,@15)                                 0456 00015000
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0456 00016000
* RFY CRRSAREA BASED(CRRPTR);       /* POINT TO CRR                  */ 00017000
* RPLPTR = CRRARPL;                 /* PLACE ADDR OF RPL IN PTR      */ 00018000
         L     RPLPTR,CRRARPL(,CRRPTR)                             0458 00019000
* RFY IFGRPL BASED(RPLPTR);         /* POINT TO THE RPL              */ 00020000
* FMCBPTR = CRRFMCB;                /* PLACE ADDR OF FMCB IN PTR     */ 00021000
         L     FMCBPTR,CRRFMCB(,CRRPTR)                            0460 00022000
* RFY ISTFMCB BASED(FMCBPTR);       /* POINT TO FMCB                 */ 00023000
* RFY ISTNCB BASED(FMCDNCBA); /* ADDRESS NCB FOR DEVCH       @D32CKNL*/ 00024000
* RFY ISTDEVCH BASED(ADDR(NCBDEVCH)); /* BASE FOR DEVICE CHARSTCS    */ 00025000
* RFY ISTRPRE BASED(NCBRDTE);       /* RDTE BASE             @ZA19868*/ 00026000
* RFY ISTRLN BASED(NCBRDTE);        /* RDTE BASE             @ZA19868*/ 00027000
* RFY ISTRPT BASED(NCBRDTE);        /* RDTE BASE             @ZA19868*/ 00028000
* LCPBPTR = CRRLCPB;                /* PLACE ADDR OF DCLCP IN PTR    */ 00029000
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0467 00030000
* RFY ISTDCLCP BASED(LCPBPTR);      /* POINT TO THE DCLCP            */ 00031000
* RFY ISTLCPB BASED(ADDR(DCLLCPB));                                0469 00032000
* LCCWPTR = LCPNXLCW;               /* PLACE 1ST LCCW ADDR IN PTR    */ 00033000
         L     LCCWPTR,LCPNXLCW(,LCPBPTR)                          0470 00034000
* RFY ISTLCCW BASED(LCCWPTR);       /* POINT TO WRITE LCCW           */ 00035000
*                                                                  0471 00036000
* CRRC22 = ON;                      /* SET THE AUDIT TRAIL SW       */  00037000
         OI    CRRC22(CRRPTR),B'01000000'                          0472 00038000
* CRRMAS = CRRMAS&&CRRMAS;          /* ZERO THE MODULE AUDIT FIELD   */ 00039000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0473 00040000
* CRRM22 = ON;                      /* SET MODULE AUDIT SW           */ 00041000
         OI    CRRM22(CRRPTR),B'01000000'                          0474 00042000
* IF DEVOTPUT = OFF &               /* IF INPUT ONLY DEVICE          */ 00043000
*    (RPRENTRY^=RPRENTLN |          /* ENTRY NOT A LINE      @ZA19868*/ 00044000
*     RLNPORT=OFF |                 /* THERE IS NO PORT      @ZA19868*/ 00045000
*     RPTATERM=0 |                  /* NO TERM               @ZA19868*/ 00046000
*     ADDR(RPTATERM->RPRDEVCH)->DEVOTPUT=OFF) THEN DO; /*INPUT ONLY  */ 00047000
         L     @15,FMCDNCBA(,FMCBPTR)                              0475 00048000
         TM    DEVOTPUT+36(@15),B'01000000'                        0475 00049000
         BNZ   @RF00475                                            0475 00050000
         L     @15,NCBRDTE(,@15)                                   0475 00051000
         CLI   RPRENTRY(@15),X'50'                                 0475 00052000
         BNE   @RT00475                                            0475 00053000
         TM    RLNPORT(@15),B'00010000'                            0475 00054000
         BZ    @RT00475                                            0475 00055000
         L     @15,RPTATERM(,@15)                                  0475 00056000
         LTR   @15,@15                                             0475 00057000
         BZ    @RT00475                                            0475 00058000
         TM    DEVOTPUT+44(@15),B'01000000'                        0475 00059000
         BNZ   @RF00475                                            0475 00060000
@RT00475 DS    0H                                                  0476 00061000
*    CRRRTNCD = CRRLOGIC;           /* CRR RETURN CODE               */ 00062000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0477 00063000
*    CRRFDB2 = CRRWTOI;             /* USER ERROR CODE: WRITE TO     */ 00064000
         MVI   CRRFDB2(CRRPTR),X'11'                               0478 00065000
*                                   /*      INPUT ONLY DEVICE TYPE   */ 00066000
* END;                              /* END - INPUT-ONLY DEVICE       */ 00067000
* ELSE DO;                          /* NOT INPUT-ONLY DEVICE         */ 00068000
         B     @RC00475                                            0480 00069000
@RF00475 DS    0H                                                  0481 00070000
*      IF DEVCHAR2=DEV3735 & FMCFLGA6=OFF THEN                     0481 00071000
         L     @15,FMCDNCBA(,FMCBPTR)                              0481 00072000
         CLI   DEVCHAR2+36(@15),X'0B'                              0481 00073000
         BNE   @RF00481                                            0481 00074000
         TM    FMCFLGA6(FMCBPTR),B'00000010'                       0481 00075000
         BNZ   @RF00481                                            0481 00076000
*         DO;                                                      0482 00077000
*           CRRRTNCD=CRRLOGIC;                                     0483 00078000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0483 00079000
*           CRRFDB2=CRRRDNFO;                                      0484 00080000
         MVI   CRRFDB2(CRRPTR),X'28'                               0484 00081000
*         END;                                                     0485 00082000
*      ELSE DO;                                                    0486 00083000
         B     @RC00481                                            0486 00084000
@RF00481 DS    0H                                                  0487 00085000
*    IF RPLERACE = ON THEN          /* ERASE WRITE IN RPL            */ 00086000
         TM    RPLERACE(RPLPTR),B'00000100'                        0487 00087000
         BNO   @RF00487                                            0487 00088000
*       IF RPLEOB = ON THEN DO;     /* IF BLOCK MODE IN RPL          */ 00089000
         TM    RPLEOB(RPLPTR),B'10000000'                          0488 00090000
         BNO   @RF00488                                            0488 00091000
*       CRRRTNCD = CRRLOGIC;        /* CRR RETURN CODE               */ 00092000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0490 00093000
*       CRRFDB2 = CRREWBLK;         /* USER ERROR CODE: BLOCK MODE   */ 00094000
         MVI   CRRFDB2(CRRPTR),X'1E'                               0491 00095000
*                                   /*      WITH ERASE WRITE         */ 00096000
*       END;                        /* END - BLOCK MODE ERASE WRITE  */ 00097000
*       ELSE DO;                    /* NOT BLOCK MODE ERASE WRITE    */ 00098000
         B     @RC00488                                            0493 00099000
@RF00488 DS    0H                                                  0494 00100000
*        IF RPLWROPT = ON THEN DO;  /* IS CONVERSATIONAL REQUESTED?  */ 00101000
         TM    RPLWROPT(RPLPTR),B'00000001'                        0494 00102000
         BNO   @RF00494                                            0494 00103000
*       CRRRTNCD=CRRLOGIC;          /* SET LOGIC ERROR CODE          */ 00104000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0496 00105000
*       CRRFDB2=CRREWCNV;           /* SET ERASE-WRITE-CONV ERROR    */ 00106000
         MVI   CRRFDB2(CRRPTR),X'15'                               0497 00107000
*       END;                        /* END-CONVERSATIONAL ERASE WRITE*/ 00108000
*     ELSE DO;                      /* NOT ERASE-WRITE-CONVERSATIONAL*/ 00109000
         B     @RC00494                                            0499 00110000
@RF00494 DS    0H                                                  0500 00111000
*      IF (DEVCHAR2 >= DEV3277 & DEVCHAR2 <= DEV3275) |            0500 00112000
*          DEVCHAR2 = DEV2265 THEN  /*      OR 2265?                 */ 00113000
         LA    @15,36                                              0500 00114000
         AL    @15,FMCDNCBA(,FMCBPTR)                              0500 00115000
         CLI   DEVCHAR2(@15),X'19'                                 0500 00116000
         BL    @GL00006                                            0500 00117000
         CLI   DEVCHAR2(@15),X'1C'                                 0500 00118000
         BNH   @RT00500                                            0500 00119000
@GL00006 L     @15,FMCDNCBA(,FMCBPTR)                              0500 00120000
         CLI   DEVCHAR2+36(@15),X'7D'                              0500 00121000
         BNE   @RF00500                                            0500 00122000
@RT00500 DS    0H                                                  0501 00123000
*          IF DEVCHAR2 = DEV2265 THEN DO; /* IF 2265                 */ 00124000
         L     @15,FMCDNCBA(,FMCBPTR)                              0501 00125000
         CLI   DEVCHAR2+36(@15),X'7D'                              0501 00126000
         BNE   @RF00501                                            0501 00127000
*          DCLECHAR = ERASCHAR;     /* MOVE ERASE CHAR TO DCLCP      */ 00128000
         MVC   DCLECHAR(2,LCPBPTR),@CB03810                        0503 00129000
*          LCCWCD = ON;             /* SET CHAINED DATA BIT          */ 00130000
* /* LINE # 88411 ADDED FOR DCR 2007    */                         0505 00131000
*          LCCWNTVC=ON;             /* TURN ON THE DO NOT VALHEK BIT */ 00132000
         OI    LCCWCD(LCCWPTR),B'10010000'                         0505 00133000
*          IF RPLEOM = ON THEN      /* WRITE MESSAGE?                */ 00134000
         TM    RPLEOM(RPLPTR),B'01000000'                          0506 00135000
         BNO   @RF00506                                            0506 00136000
*          LCCWRWOP = LEWM;         /* FILL WRITE MESSAGE OP         */ 00137000
         NI    LCCWRWOP(LCCWPTR),B'11011001'                       0507 00138000
         OI    LCCWRWOP(LCCWPTR),B'00011001'                       0507 00139000
*          ELSE LCCWRWOP = LEWT;    /* ELSE WRITE TRANMISSION        */ 00140000
         B     @RC00506                                            0508 00141000
@RF00506 NI    LCCWRWOP(LCCWPTR),B'11011101'                       0508 00142000
         OI    LCCWRWOP(LCCWPTR),B'00011101'                       0508 00143000
*          LCCWCNT = ERASSIZE;      /* FILL IN COUNT                 */ 00144000
@RC00506 LH    @00,ERASSIZE                                        0509 00145000
