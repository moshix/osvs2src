*           3  ATCTPCP0 PTR(31),       /* PTR TO ISTZFSAV            */ 01031000
*           3  ATCTPCP1 PTR(31),       /* PTR TO ISTZ19SV            */ 01032000
*           3  ATCZFLAG BIT(8),        /* FLAG BYTE                  */ 01033000
*              4 ATCZF001 BIT(1),      /* MDR RECORDING SUSPENDED    */ 01034000
*              4 ATCRSV66 BIT(7),      /* RESERVED                   */ 01035000
*           3  ATCRSV75 BIT(8),        /* RESERVED                   */ 01036000
*           3  ATCSRBCT FIXED(16),     /* COUNT OF PREALLOCATED SRB'S*/ 01037000
*           3  ATCCRACT FIXED(16),     /* COUNT OF PREALLOCATED CRA'S*/ 01038000
*           3  ATCRPLCT FIXED(16),     /* COUNT OF PREALLOCATED CRPL */ 01039000
