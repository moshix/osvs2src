*               RFY                                                0519 00402000
*                (GPR15P) UNRSTD;   /* PUT RETURN CODE IN RCREG      */ 00403000
*             END;                  /* VMDESET                       */ 00404000
*             IF CRRBRTCD<CRRBEROR  /* MOVE COMPLETE                 */ 00405000
*               THEN                                               0521 00406000
         CLI   CRRBRTCD(CRRPTR),8                                  0521 00407000
         BNL   @RF00521                                            0521 00408000
*                                                                  0522 00409000
*/********************************************************************/ 00410000
*/*                                                                  */ 00411000
*/* SEGMENT NAME : PROCESSA - PROCESS MORE DATA                      */ 00412000
*/*                                                                  */ 00413000
*/* FUNCTION :                                                       */ 00414000
*/*  PROCESSA IS CALLED BY PROCESSC TO HANDLE THE FEEDBACK FROM      */ 00415000
*/* THE MOVE ROUTINE, ISTDCC60.  THE FEEDBACK STATUS IS FIRST MOVED  */ 00416000
*/* FROM THE FSB TO THE CRR.                                         */ 00417000
*/*  IF THERE WAS  A RETURN  CODE OF 4 FROM THE  MOVE AND THE        */ 00418000
*/* TRUNCATE OPTION  IS SPECIFIED IN THE FMCB, PROCESSE IS CALLED    */ 00419000
*/* TO FREE THE FSB AND FDBS.  THE TRUNCATE  INDICATORS ARE SET IN   */ 00420000
*/* THE CRR.  IF TRUNCATE WAS NOT SPECIFIED, THE EOM, EOB, AND EOT   */ 00421000
*/* FLAGS ARE TURNED OFF IN THE CRR.  THE  SUBROUTINE RETURNS TO     */ 00422000
*/* PROCESSC.                                                        */ 00423000
*/* IF THERE WAS A 0 RETURN CODE FROM THE MOVE AND A RESIDUAL        */ 00424000
*/* LENGTH IN THE PARM, PROCESSD IS CALLED TO HANDLE SUBBLOCK        */ 00425000
*/* PROCESSING.  IF PROCESSD FINDS MORE DATA, CRRSW0 IS SET ON       */ 00426000
*/* RETURN, CAUSING PROCESSC TO RETURN TO PROCESSA UNTIL ALL THE     */ 00427000
*/* DATA HAS BEEN HANDLED.                                           */ 00428000
*/*  IF THERE WAS NO RESIDUAL LENGTH IN THE PARM, PROCESSE IS        */ 00429000
*/* CALLED TO FREE THE FSB AND FDBS. IF SUBBLOCK MODE, PROCESSING    */ 00430000
*/* IDENTICAL TO RETURN CODE 4 IS PERFORMED. IF NOT IN SUBBLOCK      */ 00431000
*/* MODE, THE ROUTINE RETURNS TO PROCESSC.                           */ 00432000
*/*                                                                  */ 00433000
*/* INPUT :  FSBPTR (REG 10) - ADDRESS OF FSB                        */ 00434000
*/*          FMCBPTR (REG 8) - ADDRESS OF FMCB                       */ 00435000
*/*                                                                  */ 00436000
*/* OUTPUT :  ADDITIONAL DATA PROCESSED                              */ 00437000
*/*                                                                  */ 00438000
*/* SUBROUTINES CALLED :                                             */ 00439000
*/*                                                                  */ 00440000
*/*              PROCESSE - FREE FSB/FDB BUFFERS                     */ 00441000
*/*                                                                  */ 00442000
*/*  MACROS USED :  NONE                                             */ 00443000
*/*                                                                  */ 00444000
*/********************************************************************/ 00445000
*                                                                  0522 00446000
*               DO;                                                0522 00447000
*                 CRRSW2=OFF;       /* TURN OFF CONTROL SWITCH       */ 00448000
         NI    CRRSW2(CRRPTR),B'11011111'                          0523 00449000
*/*      MOVE FEEDBACK STATUS FROM FSB TO   CRR                      */ 00450000
*                                                                  0524 00451000
*                 CRRRTNCD=FSBSRCAT;/* MAJOR RETURN CODE             */ 00452000
         MVC   CRRRTNCD(1,CRRPTR),FSBSRCAT(FSBPTR)                 0524 00453000
*                 CRRFDB2=FSBMNRCD; /* MINOR RETURN CODE             */ 00454000
         MVC   CRRFDB2(1,CRRPTR),FSBMNRCD(FSBPTR)                  0525 00455000
*                 CRRFDB3=FSBERC;                                  0526 00456000
         MVC   CRRFDB3(1,CRRPTR),FSBERC(FSBPTR)                    0526 00457000
*                 CRRESR1=FSBSYSRS;                                0527 00458000
         MVC   CRRESR1(1,CRRPTR),FSBSYSRS(FSBPTR)                  0527 00459000
*                 CRRESR2=FSBEXTRS;                                0528 00460000
         MVC   CRRESR2(1,CRRPTR),FSBEXTRS(FSBPTR)                  0528 00461000
*                 IF CRRRTNCD=CRRPHYSC THEN/* IF PHYSICAL CONDITION  */ 00462000
         CLI   CRRRTNCD(CRRPTR),X'0C'                              0529 00463000
         BNE   @RF00529                                            0529 00464000
*                   IF CRRSTSAV=ON  /* SENSE DATA AVAILABLE          */ 00465000
*                     THEN                                         0530 00466000
         TM    CRRSTSAV(CRRPTR),B'00000001'                        0530 00467000
         BNO   @RF00530                                            0530 00468000
*                     CRRDSB=FSBDMGRS;                             0531 00469000
         MVC   CRRDSB(2,CRRPTR),FSBDMGRS(FSBPTR)                   0531 00470000
*                   ELSE                                           0532 00471000
*                     ;                                            0532 00472000
@RF00530 DS    0H                                                  0533 00473000
*                 ELSE                                             0533 00474000
*                   ;                                              0533 00475000
@RF00529 DS    0H                                                  0534 00476000
*                 IF CRRBRTCD^=CRRBMDAT/* RETURN CODE FROM MOVE      */ 00477000
*                   THEN                                           0534 00478000
         CLI   CRRBRTCD(CRRPTR),4                                  0534 00479000
         BE    @RF00534                                            0534 00480000
*                   IF CRRBLEN=ZERO /* RESIDUAL LENGTH =0            */ 00481000
*                     THEN                                         0535 00482000
         LH    @15,CRRBLEN(,CRRPTR)                                0535 00483000
         LTR   @15,@15                                             0535 00484000
         BNZ   @RF00535                                            0535 00485000
*                     DO;                                          0536 00486000
*                       CALL PROCESSE;                             0537 00487000
         BAL   @14,PROCESSE                                        0537 00488000
*                       IF(CRRRDEOB=OFF&/* SUB BLOCK MODE            */ 00489000
*                           CRRRLG=OFF&/* LEADING GRAPHICS           */ 00490000
*                           CRRRDSOH=OFF&/* START OF HEADER          */ 00491000
*                           CRRRTNCD=FRNORM)&/* NO ERROR STATUS      */ 00492000
*                           LCPBPTR^=ZERO THEN                     0538 00493000
         TM    CRRRDEOB(CRRPTR),B'00100011'                        0538 00494000
         BNZ   @RF00538                                            0538 00495000
         CLI   CRRRTNCD(CRRPTR),X'00'                              0538 00496000
         BNE   @RF00538                                            0538 00497000
         LTR   LCPBPTR,LCPBPTR                                     0538 00498000
         BZ    @RF00538                                            0538 00499000
*                         DO;                                      0539 00500000
*                           CRRSW7=ON;                             0540 00501000
*                           CRRSW2=ON;                             0541 00502000
         OI    CRRSW7(CRRPTR),B'00100001'                          0541 00503000
*                         END;                                     0542 00504000
*                       ELSE                                       0543 00505000
*                         ;                                        0543 00506000
@RF00538 DS    0H                                                  0544 00507000
*                     END;                                         0544 00508000
*                   ELSE                                           0545 00509000
*                                                                  0545 00510000
*                     /***********************************************/ 00511000
*                     /*                                             */ 00512000
*                     /* PROCESSD                                    */ 00513000
*                     /*                                             */ 00514000
*                     /***********************************************/ 00515000
*                                                                  0545 00516000
*                                                                  0545 00517000
*/********************************************************************/ 00518000
*/*                                                                  */ 00519000
*/* SEGMENT NAME : PROCESSD - PROCESS SUBBLOCKS                      */ 00520000
*/*                                                                0545 00521000
*/* FUNCTION :                                                       */ 00522000
*/*  PROCESSD IS CALLED BY PROCESSA TO HANDLE SUBBLOCK               */ 00523000
*/* PROCESSING. IF SUBBLOCK IS INDICATED IN THE FSB, PROCESSE IS     */ 00524000
*/* CALLED TO FREE THE FSB AND FDBS, TPFEL IS ISSUED TO GET THE NEXT */ 00525000
*/* FSB ADDRESS. IF THERE IS ANOTHER FSB, ITS ADDRESS IS PTU INTO    */ 00526000
*/* THE CRR AND THE ROUTINE RETURNS TO PROCESSA WITH CRRSW0 SET      */ 00527000
*/* INDICATING MORE DATA. IF THERE IS NOT ANOTHER FSB AND THE FSB    */ 00528000
*/* HAD POINTED TO AN LCPB, THE LCPB PARM LIST IS FILLED IN WITH     */ 00529000
*/* INFO FROM THE CRR MOVE PARM LIST, THE RPL ADDRESS IS PUT IN THE  */ 00530000
*/* LCPB AND THE AWAITING SUBBLOCK SWITCH IN THE CRR IS SET. THE     */ 00531000
*/* SUBROUTINE RETURNS TO PROCESSA. IF THE FSB DID NOT POINT TO AN   */ 00532000
*/* LCPB, THE SUBROUTINE RETURNS TO PROCESSA.                        */ 00533000
*/*  IF THE FSB DOES NOT INDICATE SUBBLOCK MODE, PROCESSING          */ 00534000
*/* IS IDENTICAL TO THAT PERFORMED IF THERE IS NOT ANOTHER FSB ON    */ 00535000
*/* THE QUEUE.                                                       */ 00536000
*/*                                                                  */ 00537000
*/* INPUT :  FSBPTR (REG 10) - ADDRESS OF FSB                        */ 00538000
*/*          FMCBPTR (REG 8) - ADDRESS OF FMCB                       */ 00539000
*/*                                                                  */ 00540000
*/* OUTPUT :     SUBBLOCK PROCESSED                                  */ 00541000
*/*                                                                  */ 00542000
*/* SUBROUTINES CALLED :                                             */ 00543000
*/*              PROCESSE - FREE FSB/FDB STORAGE                     */ 00544000
*/*                                                                  */ 00545000
*/*  MACROS USED :  TPFEL                                            */ 00546000
*/*                                                                  */ 00547000
*/********************************************************************/ 00548000
*                                                                  0545 00549000
*                     DO;                                          0545 00550000
         B     @RC00535                                            0545 00551000
@RF00535 DS    0H                                                  0546 00552000
*                       CRRSW1=OFF;                                0546 00553000
         NI    CRRSW1(CRRPTR),B'10111111'                          0546 00554000
*                       IF(FSBEBI=OFF&/* SUB BLOCK OR EOB            */ 00555000
*                           FSBLGI=OFF&/* NO LEADING GRAPHICS        */ 00556000
*                           FSBSOHI=OFF&/* NO START OF HEADER        */ 00557000
*                           FSBMJRCD=FRNORM&/* NORMAL RETURN CODE    */ 00558000
*                           FSBLCPB^=ZERO)/* NOT UNSOLICITED DATA    */ 00559000
*                         THEN                                     0547 00560000
         TM    FSBEBI(FSBPTR),B'00100011'                          0547 00561000
