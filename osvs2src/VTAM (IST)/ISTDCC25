         L     @11,CRRBTLEN(,CRRPTR)                               0499 00335000
         ST    @11,RPLRLEN(,@15)                                   0499 00336000
*/*VSETLCK  (15,RELEASE,LOCAL,(LOCAL,ISTDCC25(LOCK)),)               */ 00337000
*                                                                  0500 00338000
*UNLOCK:                                                           0500 00339000
