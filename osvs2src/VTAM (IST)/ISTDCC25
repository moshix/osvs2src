*                             FSBPTR=GPR14P;                       0569 00622000
         LR    FSBPTR,GPR14P                                       0569 00623000
*                             RFY                                  0570 00624000
*                              (GPR00P,                            0570 00625000
*                               GPR01P,                            0570 00626000
*                               GPR14P,                            0570 00627000
*                               GPR15P) UNRSTD;                    0570 00628000
*                           END;    /* END TPFEL GENERATION          */ 00629000
*                           IF FSBPTR=ZERO/* ADDR OF FSB FROM FMCB   */ 00630000
*                             THEN                                 0572 00631000
         SLR   @15,@15                                             0572 00632000
         CR    FSBPTR,@15                                          0572 00633000
         BNE   @RF00572                                            0572 00634000
*                             IF LCPBPTR^=ZERO/* DID FSB POINT TO  0573 00635000
