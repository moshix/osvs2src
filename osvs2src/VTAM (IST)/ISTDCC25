         BNO   @RF00551                                            0551 00577000
*                         DO;                                      0552 00578000
*                                                                  0552 00579000
*/* TPFEL (,FMCFBA) RETPTR(FSBPTR)                                   */ 00580000
*                                                                  0553 00581000
*                           DO;     /* BEGIN TPFEL CODE GENERATION   */ 00582000
*                             DCL                                  0554 00583000
*                               FMZCA017 BIT(32) CONSTANT('80000000'X); 00584000
*                             DCL                                  0555 00585000
*                               FMZCB017 BIT(32) CONSTANT('7FFFFFFF'X); 00586000
*                             RFY                                  0556 00587000
*                              (GPR00P,                            0556 00588000
*                               GPR01P,                            0556 00589000
*                               GPR14P,                            0556 00590000
*                               GPR15P) RSTD;                      0556 00591000
*                             RFY                                  0557 00592000
*                               ISTWKE;                            0557 00593000
*                             GPR15P=ADDR(FMCFBA);/* ADDRESS QUEUE 0558 00594000
*                                      POINTER                       */ 00595000
         LA    GPR15P,FMCFBA(,FMCBPTR)                             0558 00596000
*                             GPR14P=GPR15P->PABWEQA|FMZCA017;     0559 00597000
         L     @11,@CF02782                                        0559 00598000
         L     GPR14P,PABWEQA(,GPR15P)                             0559 00599000
         OR    GPR14P,@11                                          0559 00600000
*                             IF GPR14P=FMZCA017 THEN              0560 00601000
         CLR   GPR14P,@11                                          0560 00602000
         BE    @RT00560                                            0560 00603000
*                               ;                                  0561 00604000
*                             ELSE                                 0562 00605000
*                               DO;                                0562 00606000
*                                 GPR00P=FMZCA017;/* INITIALIZE QUIT    00607000
*                                      FLAG                          */ 00608000
         L     GPR00P,@CF02782                                     0563 00609000
*                                 DO WHILE(GPR00P^=GPR14P->WKECHAIN);   00610000
         B     @DE00564                                            0564 00611000
@DL00564 DS    0H                                                  0565 00612000
*                                   GPR14P=GPR14P->WKECHAIN;/* SEARCH   00613000
*                                      FOR END OF CHAIN              */ 00614000
         L     GPR14P,WKECHAIN(,GPR14P)                            0565 00615000
*                                 END;                             0566 00616000
