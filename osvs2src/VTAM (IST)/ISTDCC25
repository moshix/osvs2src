         LA    @15,CRRSAVE1(,CRRPTR)                               0496 00325000
         LR    CRRPTR,@15                                          0496 00326000
*             CALL ATCDCC60;        /* MOVE DATA TO USERS AREA       */ 00327000
         L     @15,ATCDCC60(,ATCVTPTR)                             0497 00328000
         BALR  @14,@15                                             0497 00329000
*             CRRPTR=ADDR(CRRSPACE);/* REESTABLISH BASE              */ 00330000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0498 00331000
