*             GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(GPR00P,  00861000
*                 GPR01P,GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,     00862000
*                 GPR08P,GPR09P,GPR10P,GPR11P,GPR12P,GPR13P,GPR14P,     00863000
*                 GPR15P);                                         0653 00864000
         LM GPR00P,GPR15P,RPHWORK(GPR01P)                               00865000
*             RFY                                                  0654 00866000
*              (GPR00P,                                            0654 00867000
*               GPR01P,                                            0654 00868000
*               GPR02P,                                            0654 00869000
*               GPR03P,                                            0654 00870000
*               GPR04P,                                            0654 00871000
*               GPR15P) UNRSTD;                                    0654 00872000
*           END;                                                   0655 00873000
*                                                                  0655 00874000
*/*** END -TPUNLOCK (DEBLOCK)- **                                    */ 00875000
*                                                                  0656 00876000
*                                   /* LOCK THE DEB                  */ 00877000
*/*VMDESET  ((15),ATCAM)                                             */ 00878000
*                                                                  0656 00879000
*           DO;                     /* INVOKE MODESET MACRO          */ 00880000
*             GEN;                                                 0657 00881000
         MODESET EXTKEY=VTAM,WORKREG=2                                  00882000
*             RFY                                                  0658 00883000
*              (GPR15P) RSTD;                                      0658 00884000
*             GPR15F=0;                                            0659 00885000
         SLR   GPR15F,GPR15F                                       0659 00886000
*             RFY                                                  0660 00887000
*              (GPR15P) UNRSTD;     /* PUT RETURN CODE IN RCREG      */ 00888000
*           END;                    /* VMDESET BACK TO VTAM KEY      */ 00889000
*         END;                                                     0662 00890000
*       ELSE                                                       0663 00891000
*         CRRSW3=ON;                /* RPL HAS BEEN SATISFIED        */ 00892000
         B     @RC00449                                            0663 00893000
@RF00449 OI    CRRSW3(CRRPTR),B'00010000'                          0663 00894000
*       IF CRRSW3=ON THEN                                          0664 00895000
@RC00449 TM    CRRSW3(CRRPTR),B'00010000'                          0664 00896000
         BNO   @RF00664                                            0664 00897000
*         CRRPREIO=ON;              /* SET AWAITING PREVIOUS I/O   0665 00898000
*                                      SWITCH                        */ 00899000
         OI    CRRPREIO(CRRPTR),B'00010000'                        0665 00900000
*       ELSE                                                       0666 00901000
*         ;                                                        0666 00902000
@RF00664 DS    0H                                                  0667 00903000
*     END;                                                         0667 00904000
*   ELSE                                                           0668 00905000
*     ;                                                            0668 00906000
*                                                                  0668 00907000
@RF00447 DS    0H                                                  0669 00908000
*/*TPESC                                                             */ 00909000
*                                                                  0669 00910000
*   DO;                                                            0669 00911000
*                                                                  0669 00912000
*/* ATBUILD (ISTDYPAB,ISTAVT,ISTATCVT)                               */ 00913000
*                                                                  0670 00914000
*/* VTAM ADDRESS VECTOR TABLE */                                   0670 00915000
*/*  REVISIONS                                         002 03/06/75  */ 00916000
*DCL  1  ISTAVT BDY(WORD) BASED,                                   0670 00917000
*       2  ISTACVT PTR(31),            /* ADDR OF VTAM CVT           */ 00918000
*       2  ISTAS49 PTR(31),            /* ADDR OF SVC 49 CODE        */ 00919000
*          3  ISTAHIP BIT(1),          /* HALT IN PROGRESS           */ 00920000
*       2  ISTAS53 PTR(31),            /* ADDR OF SVC 53 CODE        */ 00921000
