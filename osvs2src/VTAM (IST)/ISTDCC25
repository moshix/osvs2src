         TITLE 'READ ANY MACRO PROCESSOR                               *00001000
                        '                                               00002000
ISTDCC25 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC25  77.320'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*   ;                                                              0417 00013000
*   DCL                                                            0418 00014000
*     ON BIT(1) CONSTANT('1'B);                                    0418 00015000
*   DCL                                                            0419 00016000
*     OFF BIT(1) CONSTANT('0'B);                                   0419 00017000
*   DCL                                                            0420 00018000
*     ZERO FIXED(31) CONSTANT(0);                                  0420 00019000
*   DCL                                                            0421 00020000
*     WAITCODE FIXED(31) CONSTANT(8);                              0421 00021000
*   DCL                                                            0422 00022000
*     GATEWRD BIT(32) CONSTANT('80000000'X);                       0422 00023000
*   DCL                                                            0423 00024000
*     FLGA2OFF BIT(32) CONSTANT('DFFFFFFF'X);                      0423 00025000
*   DCL                                                            0424 00026000
*     FLGA1ON BIT(32) CONSTANT('40000000'X);                       0424 00027000
*   DCL                                                            0425 00028000
*     FLGA3ON BIT(32) CONSTANT('10000000'X);                       0425 00029000
*   DCL                                                            0426 00030000
*     FLGA3OFF BIT(32) CONSTANT('EFFFFFFF'X);                      0426 00031000
*   DCL                                                            0427 00032000
*     FLGA5OFF BIT(32) CONSTANT('FBFFFFFF'X);/*              @ZA27850*/ 00033000
*   RFY                                                            0428 00034000
*     ISTCRA BASED(RPHCRR);                                        0428 00035000
*   RFY                                                            0429 00036000
*     ISTDCCRR BASED(ADDR(CRAPROCR));                              0429 00037000
*   RESPECIFY                                                      0430 00038000
*     CRRSAREA BASED(CRRPTR);                                      0430 00039000
*   RESPECIFY                                                      0431 00040000
*     ISTACDEB BASED(RPHMAJCB);                                    0431 00041000
*   RESPECIFY                                                      0432 00042000
*     ISTDCLCP BASED(LCPBPTR);                                     0432 00043000
*   RESPECIFY                                                      0433 00044000
*     ISTLCPB BASED(ADDR(DCLLCPB));                                0433 00045000
*   RESPECIFY                                                      0434 00046000
*     ISTFSB BASED(FSBPTR);                                        0434 00047000
*   RESPECIFY                                                      0435 00048000
*     IFGRPL BASED(CRRARPL);                                       0435 00049000
*   RESPECIFY                                                      0436 00050000
*     ISTDNCB BASED(FMCDNCBA);                                     0436 00051000
*   RFY                                                            0437 00052000
*     ISTNCB BASED(ADDR(DNCNCB));                                  0437 00053000
*   RESPECIFY                                                      0438 00054000
*     ISTATCVT BASED(ATCVTPTR);                                    0438 00055000
*   RESPECIFY                                                      0439 00056000
*     ISTFDB BASED(FDBPTR);                                        0439 00057000
*   RESPECIFY                                                      0440 00058000
*     ISTPROCD BASED(ADDR(FMCPROCD));                              0440 00059000
*                                                                  0441 00060000
*/********************************************************************/ 00061000
*/*                                                                  */ 00062000
*/*   THE MAINLINE ROUTINE TRANSFERS CONTROL TO THE RDANY SUBROUTINE */ 00063000
*/* IF A READ ANY HAS BEEN ISSUED AND THERE ARE NO ERROR INDICATORS  */ 00064000
*/* SET IN THE CRR.                                                  */ 00065000
*/*    IF THERE IS NO FMCB ON THE ACBDEB READ ANY  QUEUE, THE        */ 00066000
*/* 'AWAITING PREVIOUS IO' SWITCH  IS SET IN THE CRR AND THE         */ 00067000
*/* ROUTINE EXITS.                                                   */ 00068000
*/*                                                                  */ 00069000
*/*   INPUT TO THE ROUTINE IS REGISTER 1  POINTING TO THE RPH AND    */ 00070000
*/* RPHMAJCB INITIALIZED TO THE ADDRESS OF THE  ACBDEB.              */ 00071000
*/*                                                                  */ 00072000
*/*    EXIT VIA  TPESC.                                              */ 00073000
*/*                                                                  */ 00074000
*/********************************************************************/ 00075000
*                                                                  0441 00076000
*   CRRPTR=ADDR(CRRSPACE);          /* ADDR OF CRR                   */ 00077000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0441 00078000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0441 00079000
*   CRRMAS=CRRMAS&&CRRMAS;          /* ZERO MODULE AUDIT AND AUDIT 0442 00080000
*                                      TRAIL SWITCHES                */ 00081000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0442 00082000
*   CRRC25=ON;                      /* SET AUDIT TRAIL               */ 00083000
         OI    CRRC25(CRRPTR),B'00001000'                          0443 00084000
*   CRRM25=ON;                      /* SET MODULE AUDIT              */ 00085000
         OI    CRRM25(CRRPTR),B'00001000'                          0444 00086000
*   FMCBPTR=CRRFMCB;                /* GET FMCB ADDRESS @DM8480NL    */ 00087000
         L     FMCBPTR,CRRFMCB(,CRRPTR)                            0445 00088000
*   FSBPTR=CRRFSB;                  /* GET FSB ADDRESS @DM9062NL     */ 00089000
         L     FSBPTR,CRRFSB(,CRRPTR)                              0446 00090000
*   IF(RPLREQ='1D'X|CRRNSNA=ON)&CRRRTNCD=ZERO THEN/* READ ANY, BASIC,   00091000
*                                      NO ERROR                      */ 00092000
         L     @15,CRRARPL(,CRRPTR)                                0447 00093000
         CLI   RPLREQ(@15),29                                      0447 00094000
         BE    @GL00001                                            0447 00095000
         TM    CRRNSNA(CRRPTR),B'00000001'                         0447 00096000
         BNO   @RF00447                                            0447 00097000
@GL00001 CLI   CRRRTNCD(CRRPTR),0                                  0447 00098000
         BNE   @RF00447                                            0447 00099000
*     DO;                           /* READ ANY REQUEST AND NO ERR   */ 00100000
*       IF CRRRPLS=ON THEN                                         0449 00101000
         TM    CRRRPLS(CRRPTR),B'10000000'                         0449 00102000
         BNO   @RF00449                                            0449 00103000
*         DO;                                                      0450 00104000
*           CRRSW3=OFF;             /* TURN OFF CONTROL SWITCH     0451 00105000
*                                      PROCESS READ ANY REQUEST      */ 00106000
         NI    CRRSW3(CRRPTR),B'11101111'                          0451 00107000
*                                                                  0452 00108000
*/********************************************************************/ 00109000
*/*                                                                  */ 00110000
*/* SEGMENT NAME : RDANY - PROCESS THE READ ANY REQUEST              */ 00111000
*/*                                                                  */ 00112000
*/* FUNCTION :                                                       */ 00113000
*/*  THE RDANY  SUBROUTINE RECEIVES CONTROL FROM  THE MAINLINE       */ 00114000
*/*  ROUTINE.   IF  THE 'CONTINUE ANY' SWITCH                        */ 00115000
*/* IS  OFF IN THE  RPL, THE FMCB IS DEQUEUED FROM THE READ ANY      */ 00116000
*/* QUEUE AND THE SATISTY SPECIFIC READ  SWITCH IS SET  IN THE FMCB. */ 00117000
*/*  THE NIB AND CID FIELDS OF THE RPL ARE INITIALIZED AND           */ 00118000
*/*  SUBROUTINE MOVEPROC IS CALLED TO  MOVE THE DATA AND PROCESS     */ 00119000
*/* THE  FEEDBACK  FROM THE MOVE.                                    */ 00120000
*/*                                                                  */ 00121000
*/* INPUT :  FSBPTR (REG 10) - ADDRESS OF FSB                        */ 00122000
*/*          FMCBPTR (REG 8) - ADDRESS OF FMCB                       */ 00123000
*/*                                                                  */ 00124000
*/* OUTPUT :  READ ANY RPL SATISFIED                                 */ 00125000
*/*                                                                  */ 00126000
*/* SUBROUTINES CALLED :                                             */ 00127000
*/*                                                                  */ 00128000
*/*                                                                  */ 00129000
*/*  MACROS USED :  VMDESET, TPDEQ                                   */ 00130000
*/*                                                                  */ 00131000
*/********************************************************************/ 00132000
*                                                                  0452 00133000
*           IF RPLDLGIN^=OFF THEN   /* REQUEST FOR SPECIFIC MODE?    */ 00134000
         L     @15,CRRARPL(,CRRPTR)                                0452 00135000
         TM    RPLDLGIN(@15),B'10000000'                           0452 00136000
         BZ    @RF00452                                            0452 00137000
*             DO;                   /* CHANGE KEY AND DEQUEUE FMCB 0453 00138000
*                                      FROM READ ANY Q               */ 00139000
*               CALL DEQUE;                                        0454 00140000
         BAL   @14,DEQUE                                           0454 00141000
*               RFY                                                0455 00142000
*                (REG2,                                            0455 00143000
*                 REG3) RSTD;                                      0455 00144000
*CS1:                                                              0456 00145000
*               REG2=FMCCMPS2;      /* GET OLD FLAG VALUE            */ 00146000
CS1      L     REG2,FMCCMPS2(,FMCBPTR)                             0456 00147000
*               REG3=REG2&FLGA2OFF; /* RESET FMCB ON READ ANY Q      */ 00148000
         LR    REG3,REG2                                           0457 00149000
         N     REG3,@CF02885                                       0457 00150000
*               REG3=REG3|FLGA1ON;  /* SATISFY SPEC READ             */ 00151000
         O     REG3,@CF02784                                       0458 00152000
*               CS(REG2,REG3,FMCCMPS2);/* SET NEW VALUE IN FMCB      */ 00153000
         CS    REG2,@03,FMCCMPS2(FMCBPTR)                          0459 00154000
*               BC(7,CS1);          /* TRY AGAIN IF FAILURE          */ 00155000
         BC    7,CS1                                               0460 00156000
*               RFY                                                0461 00157000
*                (REG2,                                            0461 00158000
*                 REG3) UNRSTD;                                    0461 00159000
*             END;                                                 0462 00160000
*           ELSE                                                   0463 00161000
*             ;                                                    0463 00162000
@RF00452 DS    0H                                                  0464 00163000
*           RPLSAF=FMCSAF;          /* SET SOURCE ID IN RPL          */ 00164000
         L     @15,CRRARPL(,CRRPTR)                                0464 00165000
         LH    @11,FMCSAF(,FMCBPTR)                                0464 00166000
         N     @11,@CF04579                                        0464 00167000
         STH   @11,RPLSAF(,@15)                                    0464 00168000
*           RPLDAF=NCBDAF;          /* SET DESTINATION ID IN RPL     */ 00169000
         L     @11,FMCDNCBA(,FMCBPTR)                              0465 00170000
         LH    @11,NCBDAF(,@11)                                    0465 00171000
         N     @11,@CF04579                                        0465 00172000
         STH   @11,RPLDAF(,@15)                                    0465 00173000
*           RPLNIB=OFF;             /* RPL NOW HAS CID               */ 00174000
*                                                                  0466 00175000
         NI    RPLNIB(@15),B'11111011'                             0466 00176000
*           /*********************************************************/ 00177000
*           /*                                                       */ 00178000
*           /* MOVEPROC SEGMENT                                      */ 00179000
*           /*                                                       */ 00180000
*           /*********************************************************/ 00181000
*                                                                  0467 00182000
*                                                                  0467 00183000
*/********************************************************************/ 00184000
*/*                                                                  */ 00185000
*/* SEGMENT NAME : MOVEPROC - MOVE PROCESSING                        */ 00186000
*/*                                                                  */ 00187000
*/* FUNCTION :                                                       */ 00188000
*/*  MOVEPROC FIRST CALLS SUBROUTINE PROCESSC TO MOVE THE  DATA.     */ 00189000
*/* IF THE FMCB DATA QUEUE IS EMPTY ON RETURN FROM PROCESSC AND THE  */ 00190000
*/* FMCB IS  ON THE READ ANY QUEUE, THE FMCB IS DEQUEUED.  OTHERWISE */ 00191000
*/* THE SUBROUTINE RETURNS TO RDANY.                                 */ 00192000
*/*                                                                  */ 00193000
*/* INPUT :  FSBPTR (REG 10) - ADDRESS OF FSB                        */ 00194000
*/*          FMCBPTR (REG 8) - ADDRESS OF FMCB                       */ 00195000
*/*                                                                  */ 00196000
*/* OUTPUT :    DATA MOVED TO USER'S AREA, FMCB HOUSEKEPT            */ 00197000
*/*                                                                  */ 00198000
*/* SUBROUTINES CALLED :                                             */ 00199000
*/*                                                                  */ 00200000
*/*                                                                  */ 00201000
*/*  MACROS USED :  TPUNLOCK, VMDESET, TPDEQ                         */ 00202000
*/*                                                                  */ 00203000
*/********************************************************************/ 00204000
*                                                                  0467 00205000
*                                                                  0467 00206000
*/****************************************************************** */ 00207000
*/*                                                                  */ 00208000
*/* SEGMENT NAME : PROCESSC - MOVE DATA                              */ 00209000
*/*                                                                  */ 00210000
*/*  PROCESSC IS CALLED BY MOVEPROC TO MOVE THE  DATA TO THE USERS'  */ 00211000
*/* AREA.  THE LCPB ADDRESS IS MOVED FROM THE FSB TO THE CRR  AS     */ 00212000
*/* INPUT FOR PROCESSD.  THE MOVE PARM LIST IS BUILT IN THE CRR      */ 00213000
*/* FROM INFORMATION IN THE RPL AND ISTDCC60 IS CALLED TO MOVE THE   */ 00214000
*/* DATA TO THE USERS' AREA.  IF THERE WAS NO ERROR  DURING THE MOVE */ 00215000
*/* PROCESSA IS CALLED TO PROCESS MOVE DATA IF NECESSARY.            */ 00216000
*/* IF THERE IS MORE DATA TO PROCESS, CRRSW0 WILL BE SET IN PROCESSD */ 00217000
*/* AND THE MOVE LOGIC WILL BE REPEATED.  IF THERE IS NO MORE DATA   */ 00218000
*/* THE  SUBROUTINE RETURNS TO MOVEPROC.                             */ 00219000
*/*                                                                  */ 00220000
*/********************************************************************/ 00221000
*/* INPUT :  FSBPTR (REG 10) - ADDRESS OF FSB                        */ 00222000
*/*          FMCBPTR (REG 8) - ADDRESS OF FMCB                       */ 00223000
*/*                                                                  */ 00224000
*/* OUTPUT : DATA MOVED TO USER'S AREA                               */ 00225000
*/*                                                                  */ 00226000
*/* SUBROUTINES CALLED :                                             */ 00227000
*/*           ISTDCC60 - INPUT BUFFER MOVE                           */ 00228000
*/*                                                                  */ 00229000
*/*                                                                  */ 00230000
*/*  MACROS USED :  VMDESET, VSETLCK                                 */ 00231000
*/*                                                                  */ 00232000
*/********************************************************************/ 00233000
*                                                                  0467 00234000
*           LCPBPTR=FSBLCPB;        /* GET LCPB ADR FROM FSB         */ 00235000
         L     LCPBPTR,FSBLCPB(,FSBPTR)                            0467 00236000
*           CRRBARAD=RPLAREA;       /* BUILD MOVE PARM LIST          */ 00237000
         L     @11,RPLAREA(,@15)                                   0468 00238000
         ST    @11,CRRBARAD(,CRRPTR)                               0468 00239000
*           CRRBLEN=RPLBUFL;        /* SET AREA ADDR AND LENGTH      */ 00240000
         L     @15,RPLBUFL(,@15)                                   0469 00241000
         STH   @15,CRRBLEN(,CRRPTR)                                0469 00242000
*           CRRBTLEN=ZERO;          /* SET LENGTH MOVED EQ 0         */ 00243000
         SLR   @15,@15                                             0470 00244000
         ST    @15,CRRBTLEN(,CRRPTR)                               0470 00245000
*           ATCVTPTR=CRRATCVT;      /* REFERENCE VTAM CVT            */ 00246000
         L     ATCVTPTR,CRRATCVT(,CRRPTR)                          0471 00247000
*           CRRSW0=ON;              /* INITIALIZE LOOP CONTROL VALUE */ 00248000
         OI    CRRSW0(CRRPTR),B'10000000'                          0472 00249000
*           DO WHILE CRRSW0=ON;     /* LOOP TO MOVE DATA             */ 00250000
         B     @DE00473                                            0473 00251000
@DL00473 DS    0H                                                  0474 00252000
*                                                                  0474 00253000
*/*VMDESET  ((15),ZERO)                                              */ 00254000
*                                                                  0474 00255000
*             DO;                   /* INVOKE MODESET MACRO          */ 00256000
*               GEN;                                               0475 00257000
         MODESET EXTKEY=ZERO,WORKREG=2                                  00258000
*               RFY                                                0476 00259000
*                (GPR15P) RSTD;                                    0476 00260000
*               GPR15F=0;                                          0477 00261000
         SLR   GPR15F,GPR15F                                       0477 00262000
*               RFY                                                0478 00263000
*                (GPR15P) UNRSTD;   /* PUT RETURN CODE IN RCREG      */ 00264000
*             END;                  /* VMDESET                       */ 00265000
*/*VSETLCK  (15,OBTAIN,LOCAL,(LOCAL,ISTDCC25(UNLOCK)),)              */ 00266000
*                                                                  0480 00267000
*LOCK:                                                             0480 00268000
*             DO;                   /* DECLARE AND RESTRICT REGS   0480 00269000
*                                      2,10,11,12,13,14, AND 15      */ 00270000
LOCK     DS    0H                                                  0481 00271000
*               RFY                                                0481 00272000
*                (GPR02P,                                          0481 00273000
*                 GPR10P,                                          0481 00274000
*                 GPR11P,                                          0481 00275000
*                 GPR12P,                                          0481 00276000
*                 GPR13P,                                          0481 00277000
*                 GPR14P,                                          0481 00278000
*                 GPR15P) RSTD;     /* SAVE REGS 2 AND 10 THRU 14    */ 00279000
*               GEN REFS(GPR02F,GPR10F,GPR11F,GPR12F,GPR14F,GPR13P);    00280000
         STM   GPR10F,GPR12F,0(GPR13P)                                  00281000
         ST    GPR14F,12(GPR13P)                                        00282000
         ST    GPR02F,16(GPR13P)                                        00283000
*                                   /* INVOKE MODESET TO CHANGE PSW     00284000
*                                      PROTECTION KEY TO 0           */ 00285000
*               GEN SETS(2);                                       0483 00286000
         MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2                      00287000
*               GPR10P=GPR13P;      /* PUT SAVE AREA ADDR IN R 10  0484 00288000
*                                      INVOKE OS/VS SETLOCK MACRO    */ 00289000
         LR    GPR10P,GPR13P                                       0484 00290000
*               GEN REFS(GPR13P,PSALITA,FLC) SETS(GPR12F);         0485 00291000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        *00292000
               RELATED=(LOCAL,ISTDCC25(UNLOCK))                         00293000
*               GPR15P=GPR13P;      /* PUT RETURN CODE IN R 15 INVOKE   00294000
