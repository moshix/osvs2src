*                 GPR11P,                                          0493 00315000
*                 GPR12P,                                          0493 00316000
*                 GPR13P,                                          0493 00317000
*                 GPR14P,                                          0493 00318000
*                 GPR15P) UNRSTD;                                  0493 00319000
@RF00491 DS    0H                                                  0494 00320000
*             END;                  /* LOCK:VSETLCK                  */ 00321000
*             CRRSW0=OFF;                                          0495 00322000
         NI    CRRSW0(CRRPTR),B'01111111'                          0495 00323000
