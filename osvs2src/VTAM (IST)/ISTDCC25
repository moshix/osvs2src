*       LCCWOP=LXEL;                /* RESET ERROR LOCK              */ 01511000
         MVI   LCCWOP(@15),X'0B'                                   0863 01512000
*       DCLSRESW=ON;                /* INDICATE SPECIAL RESET        */ 01513000
         OI    DCLSRESW(LCPBPTR),B'10000000'                       0864 01514000
*/*  TPQUE (LCPBPTR,FMCPAB3) RESET                                   */ 01515000
*                                                                  0865 01516000
*       DO;                                                        0865 01517000
*         RFY                                                      0866 01518000
*          (GPR00P,                                                0866 01519000
*           GPR01P,                                                0866 01520000
*           GPR02P,                                                0866 01521000
*           GPR15P) RSTD;                                          0866 01522000
*         GEN REFS(GPR01P) SETS(RPHWORK)                           0867 01523000
*             (STM   GPR14P,GPR09P,RPHWORK(GPR01P));               0867 01524000
         STM   GPR14P,GPR09P,RPHWORK(GPR01P)                            01525000
*         GPR00P=ADDR(FMCPAB3);                                    0868 01526000
         LA    GPR00P,FMCPAB3(,FMCBPTR)                            0868 01527000
*         GPR02P=LCPBPTR;                                          0869 01528000
         LR    GPR02P,LCPBPTR                                      0869 01529000
*         CALL ATCLCPTR->ATCAP34->ISTAPC34;                        0870 01530000
         L     @11,ATCLCPTR                                        0870 01531000
         L     @15,ATCAP34(,@11)                                   0870 01532000
         BALR  @14,@15                                             0870 01533000
*         GEN REFS(RPHWORK,GPR01P) NOSETS                          0871 01534000
*             (LM    GPR14P,GPR09P,RPHWORK(GPR01P));               0871 01535000
         LM    GPR14P,GPR09P,RPHWORK(GPR01P)                            01536000
*         RFY                                                      0872 01537000
*          (GPR00P,                                                0872 01538000
*           GPR01P,                                                0872 01539000
*           GPR02P,                                                0872 01540000
*           GPR15P) UNRSTD;                                        0872 01541000
*       END;                        /* *** END OF TPQUE GENERATION 0873 01542000
*                                      SHIP RESET TO TPIOS           */ 01543000
*     END;                          /* END LCPBINIT                  */ 01544000
*   ELSE                                                           0875 01545000
*     CRRELSRI=OFF;                                                0875 01546000
         B     @RC00799                                            0875 01547000
@RF00799 NI    CRRELSRI(CRRPTR),B'11110111'                        0875 01548000
*   RTNREG=CRRRTN6;                 /* RETTORE RETURN REG            */ 01549000
@RC00799 L     RTNREG,CRRRTN6(,CRRPTR)                             0876 01550000
*   END PROCESSE;                                                  0877 01551000
@EL00002 DS    0H                                                  0877 01552000
@EF00002 DS    0H                                                  0877 01553000
@ER00002 BR    @14                                                 0877 01554000
*DEQUE:                                                            0878 01555000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0878 01556000
DEQUE    DS    0H                                                  0879 01557000
*   RFY                                                            0879 01558000
*     RTNREG RSTD;                                                 0879 01559000
*   CRRRTN7=RTNREG;                                                0880 01560000
         ST    RTNREG,CRRRTN7(,CRRPTR)                             0880 01561000
*   RFY                                                            0881 01562000
*     RTNREG UNRSTD;                                               0881 01563000
*                                                                  0881 01564000
*/*VMDESET  ((15),ZERO)                                              */ 01565000
*                                                                  0882 01566000
*   DO;                             /* INVOKE MODESET MACRO          */ 01567000
*     GEN;                                                         0883 01568000
         MODESET EXTKEY=ZERO,WORKREG=2                                  01569000
*     RFY                                                          0884 01570000
*      (GPR15P) RSTD;                                              0884 01571000
*     GPR15F=0;                                                    0885 01572000
         SLR   GPR15F,GPR15F                                       0885 01573000
*     RFY                                                          0886 01574000
*      (GPR15P) UNRSTD;             /* PUT RETURN CODE IN RCREG      */ 01575000
*   END;                            /* VMDESET                       */ 01576000
*/* TPDEQ (FMCBPTR,ACDRAFQ) CHAIN(FMCRAFM)                           */ 01577000
*                                                                  0888 01578000
*   DO;                             /* BEGIN TPDEQ CODE GENERATION   */ 01579000
*     RFY                                                          0889 01580000
*      (GPR00P,                                                    0889 01581000
*       GPR01P,                                                    0889 01582000
*       GPR14P,                                                    0889 01583000
*       GPR15P) RSTD;                                              0889 01584000
*     GPR15P=ADDR(ACDRAFQ);         /* ADDRESS QUEUE POINTER         */ 01585000
