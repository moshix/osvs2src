*                                                                  0680 00954000
*/********************************************************************/ 00955000
*/*                                                                  */ 00956000
*/* ROUTINE NAME : PROCESSE -FREE FSB/FDB BUFFERS                    */ 00957000
*/*                                                                  */ 00958000
*/* FUNCTION :                                                       */ 00959000
*/*                                                                  */ 00960000
*/*  PROCESSE IS CALLED TO FREE THE FSB AND FDBS.  THE               */ 00961000
*/* SUBROUTINE IS ENTERED BY PROCESSA WITH CRRSW2 ON FOR SUBBLOCE    */ 00962000
*/* PROCESSING. CRRSW2 IS TURNED OFF BY PROCESSE WHENEVER AN EOB     */ 00963000
*/* IS FOUND, CAUSING THE SUBROUTINE TO RETURN TO THE CALLER.        */ 00964000
*/*  IF THERE IS CONFIDENTIAL TEXT, THE RELSTORE IS ISSUED           */ 00965000
*/* WITH THE CLEAR=YES OPTION, OTHERWISE THE BUFFER IS NOT CLEARED.  */ 00966000
*/* WHEN THERE ARE NO MORE FSBS, THE FLUSH DATA INDICATOR IS SET IN  */ 00967000
*/* THE FMCB AND IF THE DATA QUEUE IS ZERO, THE SUBROUTINE RETURNS TO*/ 00968000
*/* THE CALLER. IF THE DATA QUEUE IS NOT ZERO, THE FLUSH BIT IS      */ 00969000
*/* TURNED OFF AND PROCESSING CONTINUES.                             */ 00970000
*/* INPUT :  FSBPTR (REG 10) - ADDRESS OF FSB                        */ 00971000
*/*          FMCBPTR (REG 8) - ADDRESS OF FMCB                       */ 00972000
*/*                                                                  */ 00973000
*/* OUTPUT :   FSB/FDB STORAGE RELEASED                              */ 00974000
*/*                                                                  */ 00975000
*/* SUBROUTINES CALLED :                                             */ 00976000
*/*                                                                  */ 00977000
*/*                                                                  */ 00978000
*/*  MACROS USED :  TPDEQ, RELSTORE                                  */ 00979000
*/*                                                                  */ 00980000
*/********************************************************************/ 00981000
*                                                                  0680 00982000
*PROCESSE:                                                         0680 00983000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0680 00984000
         B     @PB00002                                            0680 00985000
PROCESSE DS    0H                                                  0681 00986000
*   CRRRTN6=RTNREG;                 /* SAVE RETURN REG               */ 00987000
         ST    RTNREG,CRRRTN6(,CRRPTR)                             0681 00988000
*   CRRSW4=ON;                                                     0682 00989000
         OI    CRRSW4(CRRPTR),B'00001000'                          0682 00990000
*   DO WHILE CRRSW4=ON;                                            0683 00991000
*                                                                  0683 00992000
         B     @DE00683                                            0683 00993000
@DL00683 DS    0H                                                  0684 00994000
*/* TPDEQ (,FMCFBA) RETPTR(FSBPTR)                                   */ 00995000
*                                                                  0684 00996000
*     DO;                           /* BEGIN TPDEQ CODE GENERATION   */ 00997000
*       RFY                                                        0685 00998000
*        (GPR00P,                                                  0685 00999000
*         GPR01P,                                                  0685 01000000
*         GPR14P,                                                  0685 01001000
*         GPR15P) RSTD;                                            0685 01002000
*       GPR15P=ADDR(FMCFBA);        /* ADDRESS QUEUE POINTER         */ 01003000
         LA    GPR15P,FMCFBA(,FMCBPTR)                             0686 01004000
*       GPR14P=GPR15P->PABWEQA|FMZCA017;                           0687 01005000
         L     @11,@CF02782                                        0687 01006000
         L     GPR14P,PABWEQA(,GPR15P)                             0687 01007000
         OR    GPR14P,@11                                          0687 01008000
*       IF GPR14P=FMZCA017 THEN                                    0688 01009000
         CLR   GPR14P,@11                                          0688 01010000
         BE    @RT00688                                            0688 01011000
*         ;                                                        0689 01012000
*       ELSE                                                       0690 01013000
*         DO;                                                      0690 01014000
*FMZL1055:                                                         0691 01015000
*           ;                                                      0691 01016000
FMZL1055 DS    0H                                                  0692 01017000
*           DO WHILE(GPR14P->WKECHAIN^=FMZCA017);                  0692 01018000
         B     @DE00692                                            0692 01019000
@DL00692 DS    0H                                                  0693 01020000
*             GPR15P=ADDR(GPR14P->WKECHAIN);                       0693 01021000
         LA    GPR15P,WKECHAIN(,GPR14P)                            0693 01022000
*             GPR14P=GPR15P->PABWEQA;                              0694 01023000
         L     GPR14P,PABWEQA(,GPR15P)                             0694 01024000
*           END;                                                   0695 01025000
@DE00692 L     @11,@CF02782                                        0695 01026000
         CL    @11,WKECHAIN(,GPR14P)                               0695 01027000
         BNE   @DL00692                                            0695 01028000
*           GPR00P=FMZCA017;                                       0696 01029000
         LR    GPR00P,@11                                          0696 01030000
*           CS(GPR14P,GPR00P,GPR15P->PABWEQA);                     0697 01031000
         CS    GPR14P,@00,PABWEQA(GPR15P)                          0697 01032000
*           BC(7,FMZL1055);                                        0698 01033000
         BC    7,FMZL1055                                          0698 01034000
*           GPR14P->WKECHAIN=0;                                    0699 01035000
         SLR   @11,@11                                             0699 01036000
         ST    @11,WKECHAIN(,GPR14P)                               0699 01037000
*         END;                                                     0700 01038000
*ISTA0055:                                                         0701 01039000
*       ;                                                          0701 01040000
@RT00688 DS    0H                                                  0701 01041000
ISTA0055 DS    0H                                                  0702 01042000
*       GPR14P=GPR14P&FMZCB017;                                    0702 01043000
         N     GPR14P,@CF02877                                     0702 01044000
*       FSBPTR=GPR14P;                                             0703 01045000
         LR    FSBPTR,GPR14P                                       0703 01046000
*       RFY                                                        0704 01047000
*        (GPR00P,                                                  0704 01048000
*         GPR01P,                                                  0704 01049000
*         GPR14P,                                                  0704 01050000
*         GPR15P) UNRSTD;                                          0704 01051000
*     END;                          /* END TPDEQ GENERATION          */ 01052000
*     IF FSBPTR=ZERO|FSBPTR=GATEWRD THEN                           0706 01053000
         LTR   FSBPTR,FSBPTR                                       0706 01054000
         BZ    @RT00706                                            0706 01055000
         CL    FSBPTR,@CF02782                                     0706 01056000
         BNE   @RF00706                                            0706 01057000
@RT00706 DS    0H                                                  0707 01058000
*       DO;                                                        0707 01059000
*         RFY                                                      0708 01060000
*          (REG2,                                                  0708 01061000
*           REG3) RSTD;                                            0708 01062000
*CS3:                                                              0709 01063000
*         REG2=FMCCMPS2;            /* GET OLD FLAG VALUE            */ 01064000
CS3      L     REG2,FMCCMPS2(,FMCBPTR)                             0709 01065000
*         REG3=REG2|FLGA3ON;        /* SET FLUSH INDICATOR           */ 01066000
         LR    REG3,REG2                                           0710 01067000
         O     REG3,@CF02788                                       0710 01068000
*         CS(REG2,REG3,FMCCMPS2);   /* SET VALUE IN FMCB             */ 01069000
         CS    REG2,@03,FMCCMPS2(FMCBPTR)                          0711 01070000
*         BC(7,CS3);                /* TRY AGAIN IF FAILURE          */ 01071000
         BC    7,CS3                                               0712 01072000
*         RFY                                                      0713 01073000
*          (REG2,                                                  0713 01074000
*           REG3) UNRSTD;                                          0713 01075000
*         CRRSW2=OFF;               /* TURN OFF CONTROL SWITCH       */ 01076000
         NI    CRRSW2(CRRPTR),B'11011111'                          0714 01077000
*       END;                                                       0715 01078000
*     ELSE                                                         0716 01079000
*       DO;                                                        0716 01080000
         B     @RC00706                                            0716 01081000
@RF00706 DS    0H                                                  0717 01082000
*         IF FSBEBI=ON THEN         /* EOB IN FSB                    */ 01083000
         TM    FSBEBI(FSBPTR),B'00100000'                          0717 01084000
         BNO   @RF00717                                            0717 01085000
*           CRRSW2=OFF;                                            0718 01086000
         NI    CRRSW2(CRRPTR),B'11011111'                          0718 01087000
*         ELSE                                                     0719 01088000
*           IF(FSBLGI=ON|           /* LEADING GRAPHICS?             */ 01089000
*               FSBSOHI=ON|         /* START OF HEADER               */ 01090000
*               FSBMJRCD^=FRNORM|   /* ERROR CONDITION?              */ 01091000
*               FSBLCPB=ZERO) THEN  /* IS UNSOLICITED DATA           */ 01092000
         B     @RC00717                                            0719 01093000
@RF00717 TM    FSBLGI(FSBPTR),B'00000011'                          0719 01094000
         BNZ   @RT00719                                            0719 01095000
         CLI   FSBMJRCD(FSBPTR),X'00'                              0719 01096000
         BNE   @RT00719                                            0719 01097000
         L     @15,FSBLCPB(,FSBPTR)                                0719 01098000
         LTR   @15,@15                                             0719 01099000
         BNZ   @RF00719                                            0719 01100000
@RT00719 DS    0H                                                  0720 01101000
*             CRRSW2=OFF;           /* TREAT AS EOB                  */ 01102000
         NI    CRRSW2(CRRPTR),B'11011111'                          0720 01103000
*           ELSE                                                   0721 01104000
*             ;                     /* SUBBLOCK                      */ 01105000
@RF00719 DS    0H                                                  0722 01106000
*         IF FSBMNELI=ON THEN       /* ERROR LOCK SET IN FSB?        */ 01107000
@RC00717 TM    FSBMNELI(FSBPTR),B'10000000'                        0722 01108000
         BNO   @RF00722                                            0722 01109000
*           DO;                                                    0723 01110000
*             CRRELSRI=ON;          /* INDICATE ERROR LOCK SET       */ 01111000
         OI    CRRELSRI(CRRPTR),B'00001000'                        0724 01112000
*             CRRSW2=OFF;           /* ENDING CONDITION              */ 01113000
         NI    CRRSW2(CRRPTR),B'11011111'                          0725 01114000
*           END;                                                   0726 01115000
*         ELSE                                                     0727 01116000
*           ;                       /* ERROR LOCK NOT SET            */ 01117000
@RF00722 DS    0H                                                  0728 01118000
*         FDBPTR=ADDR(FSBFDB);      /* REFERENCE FIRST FDB           */ 01119000
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0728 01120000
*         RFY                                                      0729 01121000
*           COUNTER RSTD;                                          0729 01122000
*         COUNTER=0;                /* CLEAR BUFFER ACCUMULATOR      */ 01123000
         SLR   COUNTER,COUNTER                                     0730 01124000
*         DO WHILE FDBPTR^=0;       /* LOOP THROUGH ALL FDB'S        */ 01125000
         B     @DE00731                                            0731 01126000
@DL00731 DS    0H                                                  0732 01127000
*           COUNTER=COUNTER+1;      /* COUNT ALL BUFFERS             */ 01128000
         AL    COUNTER,@CF00067                                    0732 01129000
*           FDBPTR=FDBFDBA;         /* REFERENCE NEXT FDB            */ 01130000
         L     FDBPTR,FDBFDBA(,FDBPTR)                             0733 01131000
*         END;                                                     0734 01132000
@DE00731 LTR   FDBPTR,FDBPTR                                       0734 01133000
         BNZ   @DL00731                                            0734 01134000
*         RFY                                                      0735 01135000
*          (REG2,                                                  0735 01136000
*           REG3) RSTD;                                            0735 01137000
*CS5:                                                              0736 01138000
*         REG2=FMCCMPS1;            /* GET OLD COUNT                 */ 01139000
CS5      L     REG2,FMCCMPS1(,FMCBPTR)                             0736 01140000
*         REG3=REG2-COUNTER;        /* DECREMENT BUFFER COUNT        */ 01141000
         LR    REG3,REG2                                           0737 01142000
         SLR   REG3,COUNTER                                        0737 01143000
*         CS(REG2,REG3,FMCCMPS1);   /* SET NEW COUNT IN FMCB         */ 01144000
         CS    REG2,@03,FMCCMPS1(FMCBPTR)                          0738 01145000
*         BC(7,CS5);                /* TRY AGAIN IF FAILURE          */ 01146000
         BC    7,CS5                                               0739 01147000
*         RFY                                                      0740 01148000
*          (REG2,                                                  0740 01149000
*           REG3,                                                  0740 01150000
*           COUNTER) UNRSTD;                                       0740 01151000
*         FDBPTR=ADDR(FSBFDB);                                     0741 01152000
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0741 01153000
*         FSBPTR=FDBPTR-FDBOFSET;   /* FDB ADR MINUS OFFSET          */ 01154000
         LR    FSBPTR,FDBPTR                                       0742 01155000
         SH    FSBPTR,FDBOFSET(,FDBPTR)                            0742 01156000
*         FSBPTR->FDBFDBA=FDBFDBA;                                 0743 01157000
         L     @15,FDBFDBA(,FDBPTR)                                0743 01158000
         ST    @15,FDBFDBA(,FSBPTR)                                0743 01159000
*         IF PROCFTX=ON THEN        /* CONFIDENTIAL TEXT             */ 01160000
*                                                                  0744 01161000
         TM    PROCFTX+48(FMCBPTR),B'00000001'                     0744 01162000
         BNO   @RF00744                                            0744 01163000
*           /*********************************************************/ 01164000
*           /*                                                       */ 01165000
*           /* RELSTORE BUFADDR(FSBPTR)CHAIN(YES)CLEAR(YES)          */ 01166000
*           /*                                                       */ 01167000
*           /*********************************************************/ 01168000
*                                                                  0745 01169000
*           DO;                                                    0745 01170000
*                                                                  0745 01171000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 01172000
*/* END OF ATBUILD                                                   */ 01173000
*                                                                  0746 01174000
*             RFY                                                  0746 01175000
*              (GPR02P,                                            0746 01176000
*               GPR03P,                                            0746 01177000
*               GPR04P,                                            0746 01178000
*               GPR15P) RSTD;                                      0746 01179000
*             RFY                                                  0747 01180000
*              (GPR01P,                                            0747 01181000
*               GPR13P) RSTD;                                      0747 01182000
*             GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);   0748 01183000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             01184000
*             RFY                                                  0749 01185000
*               GPR13P UNRSTD;                                     0749 01186000
*             GPR04P=FSBPTR|'80000000'X;                           0750 01187000
         LR    GPR04P,FSBPTR                                       0750 01188000
         O     GPR04P,@CF02782                                     0750 01189000
*             DCL                                                  0751 01190000
*               1 FMZA0059 FIXED(31) STATIC LOCAL,                 0751 01191000
*                2 * FIXED(8) INIT(80),                            0751 01192000
*                2 * PTR(24) INIT(0);                              0751 01193000
*             GPR03F=FMZA0059;                                     0752 01194000
         L     GPR03F,FMZA0059                                     0752 01195000
*             DO;                   /* LISATCVT                    0753 01196000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 01197000
*               GPR15P=ATCLCPTR;    /* SET UP ATCVT POINTER          */ 01198000
         L     GPR15P,ATCLCPTR                                     0754 01199000
