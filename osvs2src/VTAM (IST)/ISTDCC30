*/*                                                                  */ 00242000
*/* ROUTINE NAME : INITCRR - INITIALIZE THE CRR                      */ 00243000
*/*                                                                  */ 00244000
*/* FUNCTION : INITIALIZES THE FIELDS IN THE CRR. THE MODULE AND COM-*/ 00245000
*/*      PONENT TRAIL SWITCHES ARE SET BY THIS SUBROUTINE.           */ 00246000
*/*                                                                  */ 00247000
*/* INPUT : CRRPTR (REG 13) - PTS TO THE CRR                         */ 00248000
*/*         RPHPTR (REG 1) - PTS TO THE RPH                          */ 00249000
*/*                                                                  */ 00250000
*/* OUTPUT : SAME AS INPUT                                           */ 00251000
*/*                                                                  */ 00252000
*/* SUBROUTINES CALLED : NONE                                        */ 00253000
*/*                                                                  */ 00254000
*/* MACROS USED : LISTATCVT                                          */ 00255000
*/*                                                                  */ 00256000
*/********************************************************************/ 00257000
*                                                                  0538 00258000
*INITCRR:                                                          0538 00259000
*   PROC OPTIONS(NOSAVE,            /* GET CRA AND CRR               */ 00260000
*       NOSAVEAREA);                /* AND INIT CRR                  */ 00261000
         B     @PB00002                                            0538 00262000
INITCRR  DS    0H                                                  0539 00263000
*   CRRRTN2=RTNREG;                 /* SAVE RET REG TO MAIN LINE     */ 00264000
         ST    RTNREG,CRRRTN2(,CRRPTR)                             0539 00265000
*   CRACRR=ADDR(CRAPROCR);          /* ADDR OF CL CRR CRR IN       0540 00266000
*                                      INITIALZATN RESTORE SAVED REGS*/ 00267000
         L     @15,RPHCRR(,RPHPTR)                                 0540 00268000
         LA    @00,CRAPROCR(,@15)                                  0540 00269000
         ST    @00,CRACRR(,@15)                                    0540 00270000
*   CRRCHAIN=CRACRR;                /* SAVE CURR CRR                 */ 00271000
         LA    @09,312                                             0541 00272000
         ALR   @09,@15                                             0541 00273000
         ST    @00,CRRCHAIN(,@09)                                  0541 00274000
*   CRRFRR=CRAFRR;                  /* SAVE FRR ADDR                 */ 00275000
*                                                                  0542 00276000
         L     @15,CRAFRR(,@15)                                    0542 00277000
         ST    @15,CRRFRR(,@09)                                    0542 00278000
*   /*****************************************************************/ 00279000
*   /*                                                               */ 00280000
*   /* INITIALIZE CRR FLAGS                                          */ 00281000
*   /*                                                               */ 00282000
*   /*****************************************************************/ 00283000
*                                                                  0543 00284000
*   CRRFLGS1='00'X;                 /* CLEAR THE FLAGS               */ 00285000
         MVI   CRRFLGS1(CRRPTR),X'00'                              0543 00286000
*   CRRFLGS2='00'X;                                                0544 00287000
         MVI   CRRFLGS2(CRRPTR),X'00'                              0544 00288000
*   CRRFLGS3='00'X;                                                0545 00289000
         MVI   CRRFLGS3(CRRPTR),X'00'                              0545 00290000
*   CRRFLGS='00'X;                  /* CLEAR SWITCHES                */ 00291000
*                                                                  0546 00292000
         MVI   CRRFLGS(CRRPTR),X'00'                               0546 00293000
*   /*****************************************************************/ 00294000
*   /*                                                               */ 00295000
*   /* INITIALIZE THE RETURN CODES TO ZERO.                          */ 00296000
*   /*                                                               */ 00297000
*   /*****************************************************************/ 00298000
*                                                                  0547 00299000
*   CRRFDBK1=CRRFDBK1&&CRRFDBK1;                                   0547 00300000
         XC    CRRFDBK1(3,CRRPTR),CRRFDBK1(CRRPTR)                 0547 00301000
*   CRRFDBK2=CRRFDBK2&&CRRFDBK2;                                   0548 00302000
         XC    CRRFDBK2(4,CRRPTR),CRRFDBK2(CRRPTR)                 0548 00303000
*   CRRLDOCT=0;                     /* ZERO OUT THE LDO CT@XM08747   */ 00304000
         SLR   @15,@15                                             0549 00305000
         STH   @15,CRRLDOCT(,CRRPTR)                               0549 00306000
*   FSBPTR=RPHWEA;                  /* COVER FSB                     */ 00307000
         L     FSBPTR,RPHWEA(,RPHPTR)                              0550 00308000
*   CRRFMCB=RPHMAJCB;               /* COVER FMCB                    */ 00309000
         L     @15,RPHMAJCB-1(,RPHPTR)                             0551 00310000
         LA    @15,0(,@15)                                         0551 00311000
         ST    @15,CRRFMCB(,CRRPTR)                                0551 00312000
*   RFY                                                            0552 00313000
*     ISTATCVT BASED(CRRATCVT);     /* COVER VTAM CVT                */ 00314000
*                                                                  0552 00315000
*   /*****************************************************************/ 00316000
*   /*                                                               */ 00317000
*   /* INITIALIZE TRAIL SWITCHES                                     */ 00318000
*   /*                                                               */ 00319000
*   /*****************************************************************/ 00320000
*                                                                  0553 00321000
*   CRRMAS=CRRMAS&&CRRMAS;          /* SET AUDIT                     */ 00322000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0553 00323000
*   CRRM30=ON;                      /* TRAIL SWS                     */ 00324000
         OI    CRRM30(CRRPTR),B'00000100'                          0554 00325000
*   CRRC30=ON;                      /* AUDIT TRAIL                   */ 00326000
         OI    CRRC30(CRRPTR),B'00000100'                          0555 00327000
*   DO;                             /* LISATCVT                    0556 00328000
*                                      CVTBASE(PAGEABLE,(CRRATCVT))  */ 00329000
*     CRRATCVT=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00330000
         L     @15,ATCLCPTR                                        0557 00331000
         ST    @15,CRRATCVT(,CRRPTR)                               0557 00332000
*   END;                            /* LISATCVT GET ATCVT PTR PUT PTR   00333000
*                                      IN CRR                        */ 00334000
*                                                                  0558 00335000
*   /*****************************************************************/ 00336000
*   /*                                                               */ 00337000
*   /* GET ID AND LENGTH IN THE CRR                                  */ 00338000
*   /*                                                               */ 00339000
*   /*****************************************************************/ 00340000
*                                                                  0559 00341000
*   CRRID=CRRTYPE;                  /* SET CRR ID FIELD              */ 00342000
         MVI   CRRID(@09),X'22'                                    0559 00343000
*   CRRLEN=LENGTH(ISTDCCRR)         /* SET CRR LENGTH                */ 00344000
*       +LENGTH(CRRSAREA);                                         0560 00345000
         MVC   CRRLEN(2,@09),@CH04426                              0560 00346000
*   CRRFSB=FSBPTR;                  /* FSB TO CRR                    */ 00347000
         ST    FSBPTR,CRRFSB(,CRRPTR)                              0561 00348000
*   CRRCLCCW=FSBLCCWA;              /* LCCW TO FSB                   */ 00349000
         L     @15,FSBLCCWA(,FSBPTR)                               0562 00350000
         ST    @15,CRRCLCCW(,CRRPTR)                               0562 00351000
*   RTNREG=CRRRTN2;                 /* RESTORE RET REG               */ 00352000
         L     @15,CRRRTN2(,CRRPTR)                                0563 00353000
         LR    RTNREG,@15                                          0563 00354000
*   RETURN;                                                        0564 00355000
@EL00002 DS    0H                                                  0564 00356000
@EF00002 DS    0H                                                  0564 00357000
@ER00002 BR    @14                                                 0564 00358000
*   END INITCRR;                                                   0565 00359000
@PB00002 DS    0H                                                  0565 00360000
*   GEN NOSETS NOREFS(EJECT);                                      0566 00361000
*                                                                  0566 00362000
         EJECT                                                          00363000
*/********************************************************************/ 00364000
*/*                                                                  */ 00365000
*/* ROUTINE NAME : CKNOIO - CHECK FOR NO I/O                         */ 00366000
*/*                                                                  */ 00367000
*/* FUNCTION : CKNOIO WILL CHECK WILL CHECK FOR THE FOLLOWING AND SET*/ 00368000
*/*      UP FOR A TPDVTS TO THE PURGE DVT:                           */ 00369000
*/*              . RESET SPECIFIED AND PURGE IS IN PROGRESS          */ 00370000
*/*              . NO LCPB ,PURGE IN PROGRESS , AND NOT A VARY       */ 00371000
*/*              . LCPB OPERATION TYPE IS PURGE                      */ 00372000
*/*      SETUP IS DONE FOR A TPDVTS TO PREEMPT/RESTORE IF THE FOLLOW-*/ 00373000
*/*      OPERATION TYPE IS PREEMPT OR RESTORE.                       */ 00374000
*/*                                                                  */ 00375000
*/* INPUT : FSBPTR (REG 5) - PTR TO THE FSB                          */ 00376000
*/*                                                                  */ 00377000
*/* OUTPUT : DVTPTR EQUAL TO ADDR OF A DVT IF ONE OF THE ABOVE COND- */ 00378000
*/*      ITIONS ARE MET.                                             */ 00379000
*/*                                                                  */ 00380000
*/* SUBROUTINES CALLED : NONE                                        */ 00381000
*/*                                                                  */ 00382000
*/* MACROS USED : NONE                                               */ 00383000
*/*                                                                  */ 00384000
*/********************************************************************/ 00385000
*                                                                  0567 00386000
*CKNOIO:                                                           0567 00387000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0567 00388000
*                                                                  0567 00389000
         B     @PB00003                                            0567 00390000
CKNOIO   DS    0H                                                  0568 00391000
*   /*****************************************************************/ 00392000
*   /*                                                               */ 00393000
*   /* DOES THE FSB MAJOR RTN CDE INDICATE RESET? IF SO AND PURGE IS */ 00394000
*   /* IN PROGRESS, SETUP FOR TPDVTS TO PURGE.                       */ 00395000
*   /*                                                               */ 00396000
*   /*****************************************************************/ 00397000
*                                                                  0568 00398000
*   IF FSBMJRCD=FRCMDRST&FMCFLGA4=ON THEN                          0568 00399000
         CLI   FSBMJRCD(FSBPTR),X'18'                              0568 00400000
         BNE   @RF00568                                            0568 00401000
         L     @15,CRRFMCB(,CRRPTR)                                0568 00402000
         TM    FMCFLGA4(@15),B'00001000'                           0568 00403000
         BNO   @RF00568                                            0568 00404000
*     DVTPTR=ADDR(DVTPURGE);                                       0569 00405000
*                                                                  0569 00406000
         LA    DVTPTR,DVTPURGE                                     0569 00407000
*   /*****************************************************************/ 00408000
*   /*                                                               */ 00409000
*   /* NOT A RESET, IF NO LCPB, PURGE IN PROGRESS AND THIS NOT FROM  */ 00410000
*   /* VARY ,SETUP FOR PURGE.                                        */ 00411000
*   /*                                                               */ 00412000
*   /*****************************************************************/ 00413000
*                                                                  0570 00414000
*   ELSE                            /* NOT A RESET                   */ 00415000
*     IF FSBLCPB=0 THEN             /* NO LCPB                       */ 00416000
         B     @RC00568                                            0570 00417000
@RF00568 L     @15,FSBLCPB(,FSBPTR)                                0570 00418000
         LTR   @15,@15                                             0570 00419000
         BNZ   @RF00570                                            0570 00420000
*       IF FMCFLGA4=ON&             /* PURGE IN PROGRESS             */ 00421000
*           FMCPRMRC^='05'X&FMCPRMRC^='06'X THEN/* NOT A VARY        */ 00422000
         L     @15,CRRFMCB(,CRRPTR)                                0571 00423000
         TM    FMCFLGA4(@15),B'00001000'                           0571 00424000
         BNO   @RF00571                                            0571 00425000
         CLI   FMCPRMRC(@15),X'05'                                 0571 00426000
         BE    @RF00571                                            0571 00427000
         CLI   FMCPRMRC(@15),X'06'                                 0571 00428000
         BE    @RF00571                                            0571 00429000
*         DVTPTR=ADDR(DVTPURGE);    /* SETUP FOR PURGE               */ 00430000
         LA    DVTPTR,DVTPURGE                                     0572 00431000
*       ELSE                                                       0573 00432000
*         ;                         /* NO LCPB WITH NO PURGE/VARY    */ 00433000
*                                                                  0573 00434000
@RF00571 DS    0H                                                  0574 00435000
*   /*****************************************************************/ 00436000
*   /*                                                               */ 00437000
*   /* LCPB PRESENT,IF LCPB TYPE IS PURGE SETUP FOR TPDVTS TO PURGE  */ 00438000
*   /*                                                               */ 00439000
*   /*****************************************************************/ 00440000
*                                                                  0574 00441000
*     ELSE                                                         0574 00442000
*       DO;                         /* LCPB PRESENT                  */ 00443000
         B     @RC00570                                            0574 00444000
@RF00570 DS    0H                                                  0575 00445000
*         LCPBPTR=FSBLCPB;          /* GET ADDR OF THE LCPB          */ 00446000
         L     LCPBPTR,FSBLCPB(,FSBPTR)                            0575 00447000
*         IF DCLRQTP=DCLPURGE THEN                                 0576 00448000
         CLI   DCLRQTP(LCPBPTR),X'15'                              0576 00449000
         BNE   @RF00576                                            0576 00450000
*           DVTPTR=ADDR(DVTPURGE);                                 0577 00451000
         LA    DVTPTR,DVTPURGE                                     0577 00452000
*       END;                                                       0578 00453000
*   RETURN;                                                        0579 00454000
@EL00003 DS    0H                                                  0579 00455000
@EF00003 DS    0H                                                  0579 00456000
@ER00003 BR    @14                                                 0579 00457000
*   END CKNOIO;                                                    0580 00458000
         B     @EL00003                                            0580 00459000
@PB00003 DS    0H                                                  0580 00460000
*   GEN NOSETS NOREFS(EJECT);                                      0581 00461000
*                                                                  0581 00462000
         EJECT                                                          00463000
*/********************************************************************/ 00464000
*/*                                                                  */ 00465000
*/* ROUTINE NAME : CKATT - CHECK FOR ATTENTION                       */ 00466000
*/*                                                                  */ 00467000
*/* FUNCTION : CHECK FOR AN ATTENTION INTERRUPT ( PHYSICAL I/O RETURN*/ 00468000
*/*      CODE ). IF THERE WAS AN ATTN AND NO DATA WAS RECEIVED,      */ 00469000
*/*      FREE THE FSB AND SETUP TO HANDLE THE ATTN. IF ATTN WITH DATA*/ 00470000
*/*      VTAM MUST FIND THE APPLICATION THAT ISSUED THE IT. DCC30 IS */ 00471000
*/*      FINISHED.                                                   */ 00472000
*/*                                                                  */ 00473000
*/* INPUT : FSBPTR (REG 5) - PTR TO FSB                              */ 00474000
*/*         WRKREG (REG 10) - PTR TO FDB                             */ 00475000
*/*                                                                  */ 00476000
*/* OUTPUT : DVTPTR (REG 11) - IF ATTN AND NO DATA, PTR TO DVT       */ 00477000
*/*                                                                  */ 00478000
*/* SUBROUTINES CALLED : CKUNSD                                      */ 00479000
*/*                                                                  */ 00480000
*/* MACROS USED : NONE                                               */ 00481000
*/*                                                                  */ 00482000
*/********************************************************************/ 00483000
*                                                                  0582 00484000
*CKATT:                                                            0582 00485000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0582 00486000
         B     @PB00004                                            0582 00487000
CKATT    DS    0H                                                  0583 00488000
*   CRRRTN2=RTNREG;                 /* SAVE RET REG                  */ 00489000
*                                                                  0583 00490000
         ST    RTNREG,CRRRTN2(,CRRPTR)                             0583 00491000
*   /*****************************************************************/ 00492000
*   /*                                                               */ 00493000
*   /* CHECK FOR A PHYSICAL I/O ERROR AND NO LCPB( ATTENTION INT.)   */ 00494000
*   /*                                                               */ 00495000
*   /*****************************************************************/ 00496000
*                                                                  0584 00497000
*   IF FSBMJRCD=FRPHYSC             /* IF PHYSICAL I/O               */ 00498000
*     THEN                          /* CHECK FOR ATTN,DAT            */ 00499000
         CLI   FSBMJRCD(FSBPTR),X'0C'                              0584 00500000
         BNE   @RF00584                                            0584 00501000
*     IF FSBLCPB=0 THEN                                            0585 00502000
         SLR   @15,@15                                             0585 00503000
         C     @15,FSBLCPB(,FSBPTR)                                0585 00504000
         BNE   @RF00585                                            0585 00505000
*       DO;                         /* FSB RESPONSE TO I/O           */ 00506000
*         RESPECIFY                                                0587 00507000
*           ISTFDB BASED(WRKREG);   /* COVER FDB                     */ 00508000
*         WRKREG=ADDR(FSBFDB);      /* GET FDB ADDR                  */ 00509000
*                                                                  0588 00510000
         LA    WRKREG,FSBFDB(,FSBPTR)                              0588 00511000
*         /***********************************************************/ 00512000
*         /*                                                         */ 00513000
*         /* THERE WAS AN ATTENTION INTERRUPT, CHECK FOR DATA PRESENT*/ 00514000
*         /*                                                         */ 00515000
*         /***********************************************************/ 00516000
*                                                                  0589 00517000
*         IF FSBMNATN=ON&FDBDATCT=0 /* ATTN AND NO DATA CK           */ 00518000
*           THEN                                                   0589 00519000
         TM    FSBMNATN(FSBPTR),B'00100000'                        0589 00520000
         BNO   @RF00589                                            0589 00521000
         CH    @15,FDBDATCT(,WRKREG)                               0589 00522000
         BNE   @RF00589                                            0589 00523000
*           DO;                                                    0590 00524000
*                                                                  0590 00525000
*             /*******************************************************/ 00526000
*             /*                                                     */ 00527000
*             /* NO DATA PRESENT, RELEASE FSB,ZERO FSB ADDR IN CRR   */ 00528000
*             /* AND SET UP TPDVTS TO FEEDBACK START EXTENTION       */ 00529000
*             /*                                                     */ 00530000
*             /*******************************************************/ 00531000
*                                                                  0591 00532000
*             CALL FREEFSB;         /* GET RID OF DATA               */ 00533000
         BAL   @14,FREEFSB                                         0591 00534000
*             CRRFSB=0;             /* CLEAR FSB PTR                 */ 00535000
         SLR   @15,@15                                             0592 00536000
         ST    @15,CRRFSB(,CRRPTR)                                 0592 00537000
*             DVTPTR=ADDR(DVTEXT);  /* SETUP FOR TPDVTS TO DCC35     */ 00538000
         LA    DVTPTR,DVTEXT                                       0593 00539000
*           END;                    /* END ATTN-NO DATA              */ 00540000
*         ELSE                                                     0595 00541000
*           ;                       /* NOT ATTN-NO DATA              */ 00542000
@RF00589 DS    0H                                                  0596 00543000
*       END;                        /* END CRRPHYSC|SPECC            */ 00544000
*     ELSE                                                         0597 00545000
*       ;                           /* FSB POINTS TO LCPB            */ 00546000
@RF00585 DS    0H                                                  0598 00547000
*   ELSE                                                           0598 00548000
*     ;                             /* ATTN AND DATA REC             */ 00549000
*                                                                  0598 00550000
@RF00584 DS    0H                                                  0599 00551000
*   /*****************************************************************/ 00552000
*   /*                                                               */ 00553000
*   /* THE FSB WAS NOT FREED,CHECK TO SEE IF DATA WAS SOLICITED      */ 00554000
*   /*                                                               */ 00555000
*   /*****************************************************************/ 00556000
*                                                                  0599 00557000
*   IF CRRFSB^=0 THEN               /* NO FSB IN CRR                 */ 00558000
         L     @15,CRRFSB(,CRRPTR)                                 0599 00559000
         LTR   @15,@15                                             0599 00560000
         BZ    @RF00599                                            0599 00561000
*     CALL CKUNSD;                                                 0600 00562000
         BAL   @14,CKUNSD                                          0600 00563000
*   ELSE                                                           0601 00564000
*     ;                                                            0601 00565000
@RF00599 DS    0H                                                  0602 00566000
*   RTNREG=CRRRTN2;                 /* REST RET REG                  */ 00567000
         L     RTNREG,CRRRTN2(,CRRPTR)                             0602 00568000
*   RETURN;                                                        0603 00569000
@EL00004 DS    0H                                                  0603 00570000
@EF00004 DS    0H                                                  0603 00571000
@ER00004 BR    @14                                                 0603 00572000
*   END CKATT;                                                     0604 00573000
@PB00004 DS    0H                                                  0604 00574000
*   GEN NOSETS NOREFS(EJECT);                                      0605 00575000
*                                                                  0605 00576000
         EJECT                                                          00577000
*/********************************************************************/ 00578000
*/*                                                                  */ 00579000
*/* ROUTINE NAME : FREEFSB - FREE THE FSB(S) AND FDB(S)              */ 00580000
*/*                                                                  */ 00581000
*/* FUNCTION : FREES THE FSB AND ASSOCIATED FDB(S) BY PLACING THE FDB*/ 00582000
*/*      CHAIN POINTER IN THE FIRST WORD OF THE FSB . FREEFSB THEN   */ 00583000
*/*      CHECKS TO SEE IF THE DATA THAT IS BEING FREED IS MARKED     */ 00584000
*/*      CONFIDENTIAL, IF SO A IF NOT THE DATA IS NOT CLEARED, IF SO */ 00585000
*/*      IT IS.                                                      */ 00586000
*/*                                                                  */ 00587000
*/* INPUT : FSBPTR (REG(5) - PTS TO THE FSB                          */ 00588000
*/*                                                                  */ 00589000
*/* OUTPUT : NONE                                                    */ 00590000
*/*                                                                  */ 00591000
*/* SUBROUTINES CALLED : NONE                                        */ 00592000
*/*                                                                  */ 00593000
*/* MACROS USED : RELSTORE(2)                                        */ 00594000
*/*                                                                  */ 00595000
*/********************************************************************/ 00596000
*                                                                  0606 00597000
*FREEFSB:                                                          0606 00598000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0606 00599000
         B     @PB00005                                            0606 00600000
FREEFSB  DS    0H                                                  0607 00601000
*   CRRRTN6=RTNREG;                 /* FDBS SAVE RETURN              */ 00602000
         ST    RTNREG,CRRRTN6(,CRRPTR)                             0607 00603000
*   RESPECIFY                                                      0608 00604000
*     ISTFDB BASED(WRKREG);         /* COVER FDB                     */ 00605000
*   WRKREG=ADDR(FSBFDB);            /* GET ADDR OF FDB               */ 00606000
*                                                                  0609 00607000
         LA    WRKREG,FSBFDB(,FSBPTR)                              0609 00608000
*   /*****************************************************************/ 00609000
*   /*                                                               */ 00610000
*   /* FIND THE BEGINNING OF THE FSB AND PLACE THE CHAIN PTR IN THE  */ 00611000
*   /* FDBFDBA FIELD                                                 */ 00612000
*   /*                                                               */ 00613000
*   /*****************************************************************/ 00614000
*                                                                  0610 00615000
*   FSBPTR=WRKREG-FDBOFSET;         /* ADD IN OFFSET                 */ 00616000
         LR    FSBPTR,WRKREG                                       0610 00617000
         SH    FSBPTR,FDBOFSET(,WRKREG)                            0610 00618000
*   FSBPTR->FDBFDBA=FDBFDBA;        /* POINT TO NEXT FDB             */ 00619000
         L     @15,FDBFDBA(,WRKREG)                                0611 00620000
         ST    @15,FDBFDBA(,FSBPTR)                                0611 00621000
*   RFY                                                            0612 00622000
*     ISTPROCD BASED(ADDR(FMCPROCD));                              0612 00623000
*   IF PROCFTX=ON                   /* IF CONFIDENTIAL               */ 00624000
*     THEN                                                         0613 00625000
*                                                                  0613 00626000
         L     @15,CRRFMCB(,CRRPTR)                                0613 00627000
         TM    PROCFTX+48(@15),B'00000001'                         0613 00628000
         BNO   @RF00613                                            0613 00629000
*     /***************************************************************/ 00630000
*     /*                                                             */ 00631000
*     /* TEXT IS CONFIDENTIAL, CLEAR IT. RELSTORE                    */ 00632000
*     /* BUFADDR(FSBPTR)CLEAR(YES)CHAIN(YES)                         */ 00633000
*     /*                                                             */ 00634000
*     /***************************************************************/ 00635000
*                                                                  0614 00636000
*     DO;                                                          0614 00637000
*                                                                  0614 00638000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00639000
*/* END OF ATBUILD                                                   */ 00640000
*                                                                  0615 00641000
*       RFY                                                        0615 00642000
*        (GPR02P,                                                  0615 00643000
*         GPR03P,                                                  0615 00644000
*         GPR04P,                                                  0615 00645000
*         GPR15P) RSTD;                                            0615 00646000
*       RFY                                                        0616 00647000
*        (GPR01P,                                                  0616 00648000
*         GPR13P) RSTD;                                            0616 00649000
*       GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);         0617 00650000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00651000
*       RFY                                                        0618 00652000
*         GPR13P UNRSTD;                                           0618 00653000
*       GPR04P=FSBPTR|'80000000'X;                                 0619 00654000
         LR    GPR04P,FSBPTR                                       0619 00655000
         O     GPR04P,@CF01349                                     0619 00656000
*       DCL                                                        0620 00657000
*         1 FMZA0048 FIXED(31) STATIC LOCAL,                       0620 00658000
*          2 * FIXED(8) INIT(80),                                  0620 00659000
*          2 * PTR(24) INIT(0);                                    0620 00660000
*       GPR03F=FMZA0048;                                           0621 00661000
         L     GPR03F,FMZA0048                                     0621 00662000
*       DO;                         /* LISATCVT                    0622 00663000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00664000
*         GPR15P=ATCLCPTR;          /* SET UP ATCVT POINTER          */ 00665000
         L     GPR15P,ATCLCPTR                                     0623 00666000
*       END;                        /* LISATCVT                      */ 00667000
*       GPR02P=GPR15P->ATCBPDA;                                    0625 00668000
         L     GPR02P,ATCBPDA(,GPR15P)                             0625 00669000
*       GPR15P=GPR15P->ATCSMRS;                                    0626 00670000
         L     GPR15P,ATCSMRS(,GPR15P)                             0626 00671000
*       DCL                                                        0627 00672000
*         FMZL0048 BASED(GPR15P) LABEL VALUERANGE(*);              0627 00673000
*       CALL FMZL0048;                                             0628 00674000
         BALR  @14,@15                                             0628 00675000
*       RFY                                                        0629 00676000
*         GPR13P RSTD;                                             0629 00677000
*       GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);         0630 00678000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              00679000
*       RFY                                                        0631 00680000
*        (GPR01P,                                                  0631 00681000
*         GPR13P) UNRSTD;                                          0631 00682000
*       RFY                                                        0632 00683000
*        (GPR02P,                                                  0632 00684000
*         GPR03P,                                                  0632 00685000
*         GPR04P,                                                  0632 00686000
*         GPR15P) UNRSTD;                                          0632 00687000
*     END;                          /* *********END OF RELSTORE    0633 00688000
*                                      EXPANSION*********            */ 00689000
*   ELSE                                                           0634 00690000
*                                                                  0634 00691000
*     /***************************************************************/ 00692000
*     /*                                                             */ 00693000
*     /* TEXT NOT CONFIDENTIAL, NO NEED TO CLEAR IT . RELSTORE       */ 00694000
*     /* BUFADDR(FSBPTR)CHAIN(YES)                                   */ 00695000
*     /*                                                             */ 00696000
*     /***************************************************************/ 00697000
*                                                                  0634 00698000
*     DO;                                                          0634 00699000
*                                                                  0634 00700000
         B     @RC00613                                            0634 00701000
@RF00613 DS    0H                                                  0635 00702000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00703000
*/* END OF ATBUILD                                                   */ 00704000
*                                                                  0635 00705000
*       RFY                                                        0635 00706000
*        (GPR02P,                                                  0635 00707000
*         GPR03P,                                                  0635 00708000
*         GPR04P,                                                  0635 00709000
*         GPR15P) RSTD;                                            0635 00710000
*       RFY                                                        0636 00711000
*        (GPR01P,                                                  0636 00712000
*         GPR13P) RSTD;                                            0636 00713000
*       GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);         0637 00714000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00715000
*       RFY                                                        0638 00716000
