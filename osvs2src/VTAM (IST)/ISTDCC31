         TITLE 'ISTDCC31 - ANALYZER - (CONTROL LAYER)                  *00001000
                        '                                               00002000
ISTDCC31 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC31  75.128'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*CRRPTR=ADDR(CRRSPACE);                                            0295 00013000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0295 00014000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0295 00015000
*/*                                                                  */ 00016000
*/*                        RPL RPL RPL RPL RPL                       */ 00017000
*/*                        RPL RPL RPL RPL RPL                       */ 00018000
*/*                        RPL RPL RPL RPL RPL                       */ 00019000
*/*                    RPL RPL RPL RPL RPL RPL RPL                   */ 00020000
*/*                        RPL RPL RPL RPL RPL                       */ 00021000
*/*                            RPL RPL RPL                           */ 00022000
*/*                                RPL                               */ 00023000
*RPLPTR=CRRARPL;                       /*GET ADDR OF RPL FROM CRR    */ 00024000
         L     RPLPTR,CRRARPL(,CRRPTR)                             0296 00025000
*RFY IFGRPL BASED(RPLPTR);             /*ESTAB ADDR FOR RPL          */ 00026000
*/*                                                                  */ 00027000
*/*                        LCCWLCCWLCCWLCCWLCCW                      */ 00028000
*/*                        LCCWLCCWLCCWLCCWLCCW                      */ 00029000
*/*                        LCCWLCCWLCCWLCCWLCCW                      */ 00030000
*/*                    LCCWLCCWLCCWLCCWLCCWLCCWLCCW                  */ 00031000
*/*                        LCCWLCCWLCCWLCCWLCCW                      */ 00032000
*/*                            LCCWLCCWLCCW                          */ 00033000
*/*                                LCCW                              */ 00034000
*RFY ISTLCCW BASED(CRRCLCCW);          /*GET ADDR OF LCCW FROM CRR -    00035000
*                                        FEEDBACK START SHOULD HAVE SET 00036000
*                                        CRRCLCCW TO THE CURRENT LCCW*/ 00037000
*/*                                                                  */ 00038000
*/*                        LDO LDO LDO LDO LDO                       */ 00039000
*/*                        LDO LDO LDO LDO LDO                       */ 00040000
*/*                        LDO LDO LDO LDO LDO                       */ 00041000
*/*                    LDO LDO LDO LDO LDO LDO LDO                   */ 00042000
*/*                        LDO LDO LDO LDO LDO                       */ 00043000
*/*                            LDO LDO LDO                           */ 00044000
*/*                                LDO                               */ 00045000
*RFY ISTLDO BASED(LDOPTR);             /*ESTAB ADDR FOR LDO        0299 00046000
*                                        LDO ADDRESSABILITY WILL   0299 00047000
*                                        BE ESTABLISHED IN THE CODE     00048000
*                                        BASED ON CRRCLDO (IN CRR) 0299 00049000
*                                        AND ON LDOADDR (LDO FORWARD    00050000
*                                        POINTER) WHEN LDO IS A    0299 00051000
*                                        JUMP LDO                    */ 00052000
*/********************************************************************/ 00053000
*/*                                                                  */ 00054000
*/*                  MAINLINE CODE BEGINS                            */ 00055000
*/*                                                                  */ 00056000
*/********************************************************************/ 00057000
*/*                                                                  */ 00058000
*/* THERE ARE THREE MAIN ROUTES THROUGH MAINLINE:                    */ 00059000
*/*                                                                  */ 00060000
*/* 1. IF CRRFINI IS SET (FINISH REQUESTED), GET THE SID             */ 00061000
*/*    OF REQUEST FINISH AND EXIT VIA TPDVTS.                        */ 00062000
*/*                                                                  */ 00063000
*/* 2. IF CRRREQP IS SET (REQUEST PROCESSING), CALL PROCESSA         */ 00064000
*/*    TO LOCATE AND VALIDATE THE APPROPRIATE LDO, AND CALL          */ 00065000
*/*    PROCESSC TO GET THE APPROPRIATE SID FOR THAT LDO,             */ 00066000
*/*    AND EXIT VIA TPDVTS.                                          */ 00067000
*/*                                                                  */ 00068000
*/* 3. IF CRRREQP IS NOT SET (MEANING THAT THIS IS FEEDBACK          */ 00069000
*/*    PROCESSING), GET THE APPROPRIATE FEEDBACK SID,                */ 00070000
*/*    AND EXIT VIA TPDVTS.                                          */ 00071000
*/*                                                                  */ 00072000
*/********************************************************************/ 00073000
*/*                                                                  */ 00074000
*/* INPUT (FIELDS TESTED):                                           */ 00075000
*/*                                                                  */ 00076000
*/* CRRFINI, WHEN SET, INDICATES 'FINISH REQUESTED'                  */ 00077000
*/* CRRREQP, WHEN SET, INDICATES 'REQUEST PROCESSOR'                 */ 00078000
*/* LCCWTYPE AND LCCWCTL ARE TESTED TO DETERMINE LCCW TYPE           */ 00079000
*/*                                                                  */ 00080000
*/********************************************************************/ 00081000
*/*                                                                  */ 00082000
*/* OUTPUT (FIELDS SET):                                             */ 00083000
*/*                                                                  */ 00084000
*/* FIELD    WHEN        SET      DEPENDENT                          */ 00085000
*/* NAME:    SET:        TO:      UPON:                              */ 00086000
*/*                                                                  */ 00087000
*/* CRRMAS   ALWAYS      ZERO     N/A                                */ 00088000
*/* CRRM31   ALWAYS      ON       N/A                                */ 00089000
*/* CRRC31   ALWAYS      ON       N/A                                */ 00090000
*/*                                                                  */ 00091000
*/********************************************************************/ 00092000
*CRRMAS=CRRMAS&&CRRMAS;                /*CLEAR PREVIOUS MODULE TRAIL    00093000
*                                        SETTINGS                    */ 00094000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0300 00095000
*CRRM31=ON;                            /*SET MODULE TRAIL SWITCH FOR    00096000
*                                        ANALYZER                    */ 00097000
         OI    CRRM31(CRRPTR),B'00000010'                          0301 00098000
*CRRC31=ON;                            /*SET AUDIT TRAIL SWITCH FOR     00099000
*                                        ANALYZER                    */ 00100000
         OI    CRRC31(CRRPTR),B'00000010'                          0302 00101000
*IF CRRFINI=ON THEN                    /*IF FINISH IS REQUESTED    0303 00102000
*                                        BY THE LAST PROCESSOR       */ 00103000
         TM    CRRFINI(CRRPTR),B'00001000'                         0303 00104000
         BNO   @RF00303                                            0303 00105000
*   DVTNAME=ADDR(REQFIN);              /*GET SID OF REQUEST FINISH   */ 00106000
         LA    DVTNAME,REQFIN                                      0304 00107000
*ELSE                                  /*IF FINISH NOT REQUESTED     */ 00108000
*   DO;                                                            0305 00109000
         B     @RC00303                                            0305 00110000
@RF00303 DS    0H                                                  0306 00111000
*   IF CRRREQP=ON THEN                 /*IF THIS IS REQUEST        0306 00112000
*                                        PROCESSING (NOT FEEDBACK)   */ 00113000
         TM    CRRREQP(CRRPTR),B'00000010'                         0306 00114000
         BNO   @RF00306                                            0306 00115000
*      DO;                                                         0307 00116000
*      CALL PROCESSA;                  /*PROCESSA PROCESSES LDO (MAY    00117000
*                                        CALL PROCESSB, WHICH WILL SET  00118000
*                                        CRRSW1 IF 'VALID LDO'):     */ 00119000
         BAL   @14,PROCESSA                                        0308 00120000
*      CALL PROCESSC;                  /*PROCESSC GETS LDO SID IF  0309 00121000
*                                        CRRSW1 IS SET, OR FINISH SID   00122000
*                                        IF IT ISN'T SET(INVALID LDO)*/ 00123000
         BAL   @14,PROCESSC                                        0309 00124000
*      END;                            /*END OF REQ PROC DO GROUP    */ 00125000
*   ELSE                               /*IF NOT REQ PROCESSING(THIS IS  00126000
*                                        FEEDBACK PROCESSING)        */ 00127000
*      DO;                                                         0311 00128000
         B     @RC00306                                            0311 00129000
@RF00306 DS    0H                                                  0312 00130000
*      IF LCCWTYPE=LWRITE |            /*IF LCCW IS WRITE OR RESET   */ 00131000
*         LCCWTYPE=LCNTRL THEN         /*WRT=01 OR 11 -- RESET=11    */ 00132000
         L     @15,CRRCLCCW(,CRRPTR)                               0312 00133000
         TM    LCCWTYPE(@15),B'00000001'                           0312 00134000
         BNO   @GL00001                                            0312 00135000
         TM    LCCWTYPE(@15),B'00000010'                           0312 00136000
         BZ    @RT00312                                            0312 00137000
@GL00001 L     @15,CRRCLCCW(,CRRPTR)                               0312 00138000
         TM    LCCWTYPE(@15),B'00000011'                           0312 00139000
         BNO   @RF00312                                            0312 00140000
@RT00312 DS    0H                                                  0313 00141000
*         DO;                                                      0313 00142000
*         IF LCCWCTL=LRESET THEN       /*IF LCCW IS RESET            */ 00143000
         L     @15,CRRCLCCW(,CRRPTR)                               0314 00144000
         TM    LCCWCTL(@15),B'00001011'                            0314 00145000
         BNO   @RF00314                                            0314 00146000
         TM    LCCWCTL(@15),B'00000100'                            0314 00147000
         BNZ   @RF00314                                            0314 00148000
*            DVTNAME=ADDR(RESETFBK);   /*GET SID OF RESET FDBK       */ 00149000
         LA    DVTNAME,RESETFBK                                    0315 00150000
*         ELSE                                                     0316 00151000
*            DVTNAME=ADDR(WRITEFBK);   /*GET SID OF WRITE FDBK       */ 00152000
         B     @RC00314                                            0316 00153000
@RF00314 LA    DVTNAME,WRITEFBK                                    0316 00154000
*         END;                         /*END WRITE OR RESET DO       */ 00155000
*      ELSE                                                        0318 00156000
*         DVTNAME=ADDR(SOLRDFBK);      /*GET SID OF SOL/RD FDBK      */ 00157000
         B     @RC00312                                            0318 00158000
@RF00312 LA    DVTNAME,SOLRDFBK                                    0318 00159000
*      END;                            /*END OF NOT-REQUEST DO GROUP */ 00160000
@RC00312 DS    0H                                                  0320 00161000
*   END;                               /*END OF FINISH NOT REQ     0320 00162000
*                                        ELSE-DO GROUP               */ 00163000
@RC00306 DS    0H                                                  0321 00164000
*/*  TPDVTS NAMEPTR(DVTNAME) */                                    0321 00165000
* DO;                                                              0321 00166000
*                                                                  0321 00167000
@RC00303 DS    0H                                                  0322 00168000
*/* ATBUILD (ISTRPH,ISTPAB,ISTDYPAB,ISTATCVT)  */                  0322 00169000
