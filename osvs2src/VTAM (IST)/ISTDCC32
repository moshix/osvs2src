*                      THEN DO;          /*THEN                      */ 00309000
         TM    LCCWCD(LCCWPTR),B'11000000'                         0491 00310000
         BZ    @RF00491                                            0491 00311000
*                            CRRSW1 = OFF;  /* TURN OFF NO MORE LCCW    00312000
*                                        BLOCKS INTERNAL SWITCH      */ 00313000
         NI    CRRSW1(DCCRRPTR),B'10111111'                        0493 00314000
*                            LCCWPTR = LCCWPTR + 8;                0494 00315000
         AL    LCCWPTR,@CF00039                                    0494 00316000
