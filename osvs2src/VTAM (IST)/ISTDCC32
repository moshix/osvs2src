*                 STRTLCCW = LCCWPTR;    /* GET START OF LCCW BLOCK  */ 00287000
         LR    STRTLCCW,LCCWPTR                                    0485 00288000
*CHECKTC:         DO WHILE LCCWPTR->LCCWOP = LTIC;                 0486 00289000
CHECKTC  B     @DE00486                                            0486 00290000
@DL00486 DS    0H                                                  0487 00291000
*                                        /* DO WHILE LCCW IS A TIC   */ 00292000
*                      LCCWPTR = LCCWPTR->LCCWDATA;                0487 00293000
         L     LCCWPTR,LCCWDATA(,LCCWPTR)                          0487 00294000
*                                        /* POINT LCCW PTR TO NEXT 0488 00295000
*                                        LCCW BLOCK                  */ 00296000
*                      CALL FREEUP;                         /*@D31JKMQ* 00297000
         BAL   @14,FREEUP                                          0488 00298000
*                                        /* FREE LCCW BLOCK          */ 00299000
*                      STRTLCCW = LCCWPTR;                         0489 00300000
