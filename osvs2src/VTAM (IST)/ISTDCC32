*/*                                                                  */ 00270000
*/********************************************************************/ 00271000
*RESPECIFY (RETPTR) RESTRICTED;          /* RESTRICT RETURN @ REG    */ 00272000
*CRRRTN3 = RETPTR;                       /* SAVE RETURN ADDRESS      */ 00273000
         ST    RETPTR,CRRRTN3(,DCCRRPTR)                           0479 00274000
*RESPECIFY (RETPTR) UNRESTRICTED;        /* UNRESTRICT RETURN @ REG  */ 00275000
*LCPBLCCW = ADDR(LCPLCCW);               /* ADDRESS LCPB LCCW        */ 00276000
         L     LCPBLCCW,CRRLCPB(,DCCRRPTR)                         0481 00277000
         LA    LCPBLCCW,LCPLCCW(,LCPBLCCW)                         0481 00278000
*IF LCPBLCCW->LCCWOP = LTIC              /* IF LCPB LCCW IS A TIC    */ 00279000
*          THEN DO;                      /* THEN                     */ 00280000
         CLI   LCCWOP(LCPBLCCW),X'08'                              0482 00281000
         BNE   @RF00482                                            0482 00282000
*                 LCCWPTR = LCPBLCCW->LCCWDATA;                    0484 00283000
         L     LCCWPTR,LCCWDATA(,LCPBLCCW)                         0484 00284000
