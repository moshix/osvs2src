*   CRRSW1=ON;                      /* SET CB CHECK SW ON            */ 00462000
*                                                                  0616 00463000
         OI    CRRSW1(CRRPTR),B'01000000'                          0616 00464000
*   /*****************************************************************/ 00465000
*   /*                                                               */ 00466000
*   /* CHECK THE ACB TO MAKE SURE IT HAS NOT BEEN OVERLAID. IF IT HAS*/ 00467000
*   /* NOT, PICK UP THE ADDRESS OF THE EXIT LIST.                    */ 00468000
*   /*                                                               */ 00469000
*   /*****************************************************************/ 00470000
*                                                                  0617 00471000
*   IF ACBID='A0'X THEN                                            0617 00472000
         CLI   ACBID(WRKREG1),160                                  0617 00473000
         BNE   @RF00617                                            0617 00474000
