*                                                                  0657 00625000
         L     WRKREG2,ACBUEL(,WRKREG1)                            0657 00626000
*           /*********************************************************/ 00627000
*           /*                                                       */ 00628000
*           /* IF THE EXLST EXIST AND HAS NOT BEEN OVERLAID, CHECK TO*/ 00629000
*           /* SEE IF THE LOSTERM EXIT IS AVAILABLE                  */ 00630000
*           /*                                                       */ 00631000
*           /*********************************************************/ 00632000
*                                                                  0658 00633000
*           IF WRKREG2^=0           /* IF EXLIST ADDR                */ 00634000
*             THEN                                                 0658 00635000
         LTR   WRKREG2,WRKREG2                                     0658 00636000
         BZ    @RF00658                                            0658 00637000
*             DO;                   /* NOT ZERO                      */ 00638000
*               IF EXLID=129 THEN   /* EXLST ID OK                   */ 00639000
*                                                                  0660 00640000
         CLI   EXLID(WRKREG2),129                                  0660 00641000
         BNE   @RF00660                                            0660 00642000
*                 /***************************************************/ 00643000
*                 /*                                                 */ 00644000
*                 /* CHECK THE EXLST LENGTH ,TO SEE IF IT IS LONG    */ 00645000
*                 /* ENOUGH TO HAVE THE SPECIFIED EXIT & AN ADDR IS  */ 00646000
*                 /* THERE, SCHEDULE THE EXIT                        */ 00647000
*                 /*                                                 */ 00648000
*                 /***************************************************/ 00649000
*                                                                  0661 00650000
