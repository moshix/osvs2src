*/********************************************************************/ 00767000
*/*                                                                  */ 00768000
*/* ROUTINE NAME : CKTYPE - GET REQUEST TYPE                         */ 00769000
*/*                                                                  */ 00770000
*/* FUNCTION : CHECK REQUEST TYPE AND INITIALIZE THE DVTPTR TO PT TO */ 00771000
*/*      THE PROPER DVT NAME.                                        */ 00772000
*/*                                                                  */ 00773000
*/* INPUT : CRRPTR (REG 13) - PTR TO THE CRR                         */ 00774000
*/*                                                                  */ 00775000
*/* OUTPUT : DVTPTR (REG 11) - PTR TO A DVT NAME                     */ 00776000
*/*                                                                  */ 00777000
*/* SUBROUTINES CALLED : NONE                                        */ 00778000
*/*                                                                  */ 00779000
*/* MACROS USED : NONE                                               */ 00780000
*/*                                                                  */ 00781000
*/********************************************************************/ 00782000
*                                                                  0694 00783000
*CKTYPE:                                                           0694 00784000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0694 00785000
         B     @PB00005                                            0694 00786000
CKTYPE   DS    0H                                                  0695 00787000
*   RFY                                                            0695 00788000
*     LCPBPTR RSTD;                                                0695 00789000
*   LCPBPTR=CRRLCPB;                /* PROC LOAD REG                 */ 00790000
*                                                                  0696 00791000
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0696 00792000
*   /*****************************************************************/ 00793000
*   /*                                                               */ 00794000
*   /* CHECK THE REQUEST TYPE FOR THE DO MACRO. IF IT IS , SET THE   */ 00795000
*   /* LDO/LCCW PROC SWITCH AND INIT PTR TO ADDR OF DO DVT NAME      */ 00796000
*   /*                                                               */ 00797000
*   /*****************************************************************/ 00798000
*                                                                  0697 00799000
*   IF DCLRQTP=DCLDO                /* IO REQ A DO                   */ 00800000
*     THEN                                                         0697 00801000
         CLI   DCLRQTP(LCPBPTR),X'13'                              0697 00802000
         BNE   @RF00697                                            0697 00803000
*     DO;                                                          0698 00804000
*       DVTPTR=ADDR(DVTDO);         /* SET UP SID                    */ 00805000
         LA    DVTPTR,DVTDO                                        0699 00806000
*     END;                                                         0700 00807000
*                                                                  0700 00808000
*   /*****************************************************************/ 00809000
*   /*                                                               */ 00810000
*   /* NOT A DO. CHECK FOR SOL/READ/WRITE REQUEST. IF SO, PT TO THE  */ 00811000
*   /* SOLICIT/READ/WRITE DVT NAME.                                  */ 00812000
*   /*                                                               */ 00813000
*   /*****************************************************************/ 00814000
*                                                                  0701 00815000
*   ELSE                                                           0701 00816000
*     IF DCLRQTP=DCLSLCIT|          /* SOL.RD.WR REQ                 */ 00817000
*         DCLRQTP=DCLREAD|DCLRQTP=DCLWRITE THEN                    0701 00818000
         B     @RC00697                                            0701 00819000
@RF00697 CLI   DCLRQTP(LCPBPTR),X'1E'                              0701 00820000
         BE    @RT00701                                            0701 00821000
         CLI   DCLRQTP(LCPBPTR),X'1D'                              0701 00822000
         BE    @RT00701                                            0701 00823000
         CLI   DCLRQTP(LCPBPTR),X'11'                              0701 00824000
         BNE   @RF00701                                            0701 00825000
@RT00701 DS    0H                                                  0702 00826000
*       DVTPTR=ADDR(DVTSRW);        /* PTR TO SID                    */ 00827000
         LA    DVTPTR,DVTSRW                                       0702 00828000
*     ELSE                                                         0703 00829000
*       DVTPTR=ADDR(DVTRSET);       /* MUST BE A RESET               */ 00830000
         B     @RC00701                                            0703 00831000
@RF00701 LA    DVTPTR,DVTRSET                                      0703 00832000
*   RETURN;                                                        0704 00833000
@EL00005 DS    0H                                                  0704 00834000
@EF00005 DS    0H                                                  0704 00835000
@ER00005 BR    @14                                                 0704 00836000
*   END CKTYPE;                                                    0705 00837000
         B     @EL00005                                            0705 00838000
@PB00005 DS    0H                                                  0705 00839000
*   GEN NOSETS NOREFS(EJECT);                                      0706 00840000
*                                                                  0706 00841000
         EJECT                                                          00842000
*/********************************************************************/ 00843000
*/*                                                                  */ 00844000
*/* ROUTINE NAME : CKCONT - CONTINUE MACRO CHECK                     */ 00845000
*/*                                                                  */ 00846000
*/* FUNCTION : CHECKS FOR AN INVALID MACRO REQUEST, IF FOUND ERROR   */ 00847000
*/*      CODES ARE SET, FSB FLAGS ARE SET , AND THE FSB IS MOVED TO  */ 00848000
*/*      PAGEABLE STORAGE. OTHERWISE, IF THE TYPE IS NOT A DO THE    */ 00849000
*/*      SID IS FOUND FOR THE REQUEST. IF A DO REQUEST THE VALIDITY  */ 00850000
*/*      OF THE LDO IS CHECKED.                                      */ 00851000
*/*                                                                  */ 00852000
*/* INPUT : LCPBPTR (REG 4) - PTR TO THE LCPB                        */ 00853000
*/*         FSBPTR (REG 5) - PTR TO THE FSB                          */ 00854000
*/*         DVTPTR (REG 11) - 0                                      */ 00855000
*/*                                                                  */ 00856000
*/* OUTPUT : DVTPTR (REG 11) - MAY PT TO A DVT NAME                  */ 00857000
*/*           CRRLDO MAY BE MODIFIED                                 */ 00858000
*/*           CRR DO POST FLAG MAY BE SET                            */ 00859000
*/*           CRRRTNCD MAY BE CHANGED                                */ 00860000
*/*                                                                  */ 00861000
*/* SUBROUTINES CALLED : CKCONT , CKTYPE , XFRFSB                    */ 00862000
*/*                                                                  */ 00863000
*/* MACROS USED : NONE                                               */ 00864000
*/*                                                                  */ 00865000
*/********************************************************************/ 00866000
*                                                                  0707 00867000
*CKCONT:                                                           0707 00868000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0707 00869000
         B     @PB00006                                            0707 00870000
CKCONT   DS    0H                                                  0708 00871000
*   CRRRTN1=RTNREG;                                                0708 00872000
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0708 00873000
*   RFY                                                            0709 00874000
*    (WRKREG,                                                      0709 00875000
*     WRKREG1) RSTD;                                               0709 00876000
*   LCPBPTR=CRRLCPB;                /* GET THE ADDR OF THE LCPB    0710 00877000
*                                                            @DM09260*/ 00878000
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0710 00879000
*   WRKREG1=DCLRPLA;                /* GET NEW RPL ADDR              */ 00880000
         L     WRKREG1,DCLRPLA(,LCPBPTR)                           0711 00881000
*   CRRARPL=WRKREG1;                /* RPL PTR TO CRR                */ 00882000
         ST    WRKREG1,CRRARPL(,CRRPTR)                            0712 00883000
*   RESPECIFY                                                      0713 00884000
*     IFGRPL BASED(WRKREG1);        /* COVER RPL LCPB COV IN NOTUNS  */ 00885000
*                                                                  0713 00886000
*   /*****************************************************************/ 00887000
*   /*                                                               */ 00888000
*   /* DOES THE REQUEST TYPE FALL IN THE MACRO REQUEST RANGE         */ 00889000
*   /*                                                               */ 00890000
*   /*****************************************************************/ 00891000
*                                                                  0714 00892000
*   IF RPLREQ<='13'X&RPLREQ>='11'X|RPLREQ<='1E'X&RPLREQ>='1D'X/*   0714 00893000
*                                      MACRO TYPE VALID              */ 00894000
*     THEN                                                         0714 00895000
         CLI   RPLREQ(WRKREG1),19                                  0714 00896000
         BH    @GL00003                                            0714 00897000
         CLI   RPLREQ(WRKREG1),17                                  0714 00898000
         BNL   @RT00714                                            0714 00899000
@GL00003 CLI   RPLREQ(WRKREG1),30                                  0714 00900000
         BH    @RF00714                                            0714 00901000
         CLI   RPLREQ(WRKREG1),29                                  0714 00902000
         BL    @RF00714                                            0714 00903000
@RT00714 DS    0H                                                  0715 00904000
*     DO;                                                          0715 00905000
*       DCLRQTP=RPLREQ;             /* MACRO TYPE TO LCPB            */ 00906000
*                                                                  0716 00907000
         MVC   DCLRQTP(1,LCPBPTR),RPLREQ(WRKREG1)                  0716 00908000
*       /*************************************************************/ 00909000
*       /*                                                           */ 00910000
*       /* THIS IS A VALID MACRO, IF THIS IS A READ OR A SOLICIT THE */ 00911000
*       /* NODE FLAG IS CHECKED ,IF ON, THIS IS AN ANY(ALL) REQUEST  */ 00912000
*       /* AND MUST BE SO NOTED IN THE LCPB TO BE PASSED TO TPIOS    */ 00913000
*       /*                                                           */ 00914000
*       /*************************************************************/ 00915000
*                                                                  0717 00916000
*       IF(RPLREQ='1E'X|RPLREQ='1D'X)&/* READ OR SOLICT              */ 00917000
*           RPLNODE=ON              /* NODE IND                      */ 00918000
*         THEN                                                     0717 00919000
         CLI   RPLREQ(WRKREG1),30                                  0717 00920000
         BE    @GL00006                                            0717 00921000
         CLI   RPLREQ(WRKREG1),29                                  0717 00922000
         BNE   @RF00717                                            0717 00923000
@GL00006 TM    RPLNODE(WRKREG1),B'00000010'                        0717 00924000
         BNO   @RF00717                                            0717 00925000
*         DCLBNSP=ON;               /* SET READ ANY SOL              */ 00926000
*                                                                  0718 00927000
         OI    DCLBNSP(LCPBPTR),B'00000001'                        0718 00928000
*       /*************************************************************/ 00929000
*       /*                                                           */ 00930000
*       /* IF THIS A DO MACRO REQUEST THE LDO(S) HAVE TO BE CHECKED  */ 00931000
*       /* FOR VALIDITY. THE CKCONT1 SUBRTN IS CALLED TO DO THIS.    */ 00932000
*       /*                                                           */ 00933000
*       /*************************************************************/ 00934000
*                                                                  0719 00935000
*       IF RPLREQ='13'X             /* MACRO A DO                    */ 00936000
*         THEN                                                     0719 00937000
@RF00717 CLI   RPLREQ(WRKREG1),19                                  0719 00938000
         BNE   @RF00719                                            0719 00939000
*         DO;                                                      0720 00940000
*           RFY                                                    0721 00941000
*             ISTLDO BASED(WRKREG); /* COVER LDO                     */ 00942000
*           WRKREG=RPLAREA;         /* GET LDO ADDR                  */ 00943000
         L     WRKREG,RPLAREA(,WRKREG1)                            0722 00944000
*           CALL CKCONT1;           /* CHECK OUT LDOS                */ 00945000
*                                                                  0723 00946000
         BAL   @14,CKCONT1                                         0723 00947000
*           /*********************************************************/ 00948000
*           /*                                                       */ 00949000
*           /* CRRSW1 IS CHECKED TO SEE IF THE LDO CHAIN WAS VALID.  */ 00950000
*           /* IF IN- VALID, THE FSB FLAGS ARE SET AND THE XFRFSB IS */ 00951000
*           /* CALLED TO MOVE THE FSB TO PAGEABLE STORAGE.           */ 00952000
*           /*                                                       */ 00953000
*           /*********************************************************/ 00954000
*                                                                  0724 00955000
*           IF CRRSW1=ON            /* IF BAD LDO CHN                */ 00956000
*             THEN                                                 0724 00957000
         TM    CRRSW1(CRRPTR),B'01000000'                          0724 00958000
         BNO   @RF00724                                            0724 00959000
*             DO;                   /*                               */ 00960000
*               FSBELI=OFF;         /* TURN OFF END DATA             */ 00961000
*               FSBECI=OFF;         /* TURN OFF END OF COMMAND     0727 00962000
*                                      INDICATOR                     */ 00963000
         NI    FSBELI(FSBPTR),B'00111111'                          0727 00964000
*               CALL XFRFSB;        /* Q PAGEABLE FSB                */ 00965000
         BAL   @14,XFRFSB                                          0728 00966000
*               CRRCLDO=ADDR(ISTLDO);/* LDO ADDR TO CRR              */ 00967000
         ST    WRKREG,CRRCLDO(,CRRPTR)                             0729 00968000
*               CRRPOST=ON;         /* TURN ON DO POST               */ 00969000
         OI    CRRPOST(CRRPTR),B'00010000'                         0730 00970000
*             END;                  /* END BAD LDO CHAIN             */ 00971000
*           ELSE                                                   0732 00972000
*             DO;                   /* IF GOOD LDO CHAIN             */ 00973000
*                                                                  0732 00974000
         B     @RC00724                                            0732 00975000
@RF00724 DS    0H                                                  0733 00976000
*               /*****************************************************/ 00977000
*               /*                                                   */ 00978000
*               /* GET THE ADDRESS OF THE FIRST LDO AND IF WE HAVE A */ 00979000
*               /* JUMP ,WE WANT TO PUT THE ADDRESS OF THE LDO IN THE*/ 00980000
*               /* LCCW EXTENSION.                                   */ 00981000
*               /*                                                   */ 00982000
*               /*****************************************************/ 00983000
*                                                                  0733 00984000
*               WRKREG=RPLAREA;     /* POINT TO FIRST LDO            */ 00985000
         L     WRKREG,RPLAREA(,WRKREG1)                            0733 00986000
*               IF LDOCMD=LDOJUMP THEN/* IF FIRST IS A JUMP          */ 00987000
         CLI   LDOCMD(WRKREG),X'08'                                0734 00988000
         BNE   @RF00734                                            0734 00989000
*                 WRKREG=LDOADDR;   /* GET ADDR OF READ              */ 00990000
         L     WRKREG,LDOADDR(,WRKREG)                             0735 00991000
*               ELSE                                               0736 00992000
*                 ;                 /* NOT A JUMP                    */ 00993000
@RF00734 DS    0H                                                  0737 00994000
*               RFY                                                0737 00995000
*                 ISTLCCW BASED(ADDR(LCPLCCW));                    0737 00996000
*               IF LCCWOP=LTIC      /* IF LCCW IS A TIC              */ 00997000
*                 THEN                                             0738 00998000
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0738 00999000
         BNE   @RF00738                                            0738 01000000
*                 EVAL(CRRCLCCW+32)->ADDRLDO=WRKREG;/* LDO ADDR TO 0739 01001000
*                                      EXTEN.                        */ 01002000
*                                                                  0739 01003000
         LA    @15,32                                              0739 01004000
         AL    @15,CRRCLCCW(,CRRPTR)                               0739 01005000
         ST    WRKREG,ADDRLDO(,@15)                                0739 01006000
*               /*****************************************************/ 01007000
*               /*                                                   */ 01008000
*               /* GET THE ADDR OF THE LDO IN THE LCPB OR THE LCCW   */ 01009000
*               /* EXTENSION AND CALL CKTYPE TO SETUP DVTPTR TO PT TO*/ 01010000
*               /* THE DVTNAME FOR DCC31                             */ 01011000
*               /*                                                   */ 01012000
*               /*****************************************************/ 01013000
*                                                                  0740 01014000
*               ELSE                                               0740 01015000
*                 DCLLDO1=WRKREG;   /* LDO ADDR TO LCPB              */ 01016000
         B     @RC00738                                            0740 01017000
@RF00738 ST    WRKREG,DCLLDO1(,LCPBPTR)                            0740 01018000
*               CALL CKTYPE;        /* CHECK MACRO TYPE              */ 01019000
@RC00738 BAL   @14,CKTYPE                                          0741 01020000
*             END;                                                 0742 01021000
*         END;                                                     0743 01022000
*       ELSE                                                       0744 01023000
*         CALL CKTYPE;              /* CHECK FOR MACRO T             */ 01024000
         B     @RC00719                                            0744 01025000
@RF00719 BAL   @14,CKTYPE                                          0744 01026000
*     END;                          /* END VALID MACRO REQUEST       */ 01027000
*   ELSE                                                           0746 01028000
*     DO;                                                          0746 01029000
*                                                                  0746 01030000
         B     @RC00714                                            0746 01031000
@RF00714 DS    0H                                                  0747 01032000
*       /*************************************************************/ 01033000
*       /*                                                           */ 01034000
*       /* THE MACRO REQUESTED WAS NOT VALID, SET THE RETURN CODE AND*/ 01035000
*       /* CALL XFRFSB TO MOVE THE FSB TO PAGEABLE STORAGE AND SET DO*/ 01036000
*       /* POST                                                      */ 01037000
*       /*                                                           */ 01038000
*       /*************************************************************/ 01039000
*                                                                  0747 01040000
*       CRRRTNCD=CRRLOGIC;          /* SET RET CODE                  */ 01041000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0747 01042000
*       CRRFDB2=CRRCRIRT;                                          0748 01043000
         MVI   CRRFDB2(CRRPTR),X'1C'                               0748 01044000
*       CRRSW1=ON;                  /* SET SW                        */ 01045000
         OI    CRRSW1(CRRPTR),B'01000000'                          0749 01046000
*       FSBELI=OFF;                 /* TURN OFF END DATA             */ 01047000
*       FSBECI=OFF;                 /* END COMMUNICATIONS            */ 01048000
         NI    FSBELI(FSBPTR),B'00111111'                          0751 01049000
*       CALL XFRFSB;                /* Q PAGEABLE FSB                */ 01050000
         BAL   @14,XFRFSB                                          0752 01051000
*       CRRPOST=ON;                 /* TURN ON DO POST               */ 01052000
         OI    CRRPOST(CRRPTR),B'00010000'                         0753 01053000
*     END;                                                         0754 01054000
*                                                                  0754 01055000
*   /*****************************************************************/ 01056000
*   /*                                                               */ 01057000
*   /* A CHECK IS MADE TO SEE IF THE COMMAND WAS RESET OR A CONTROL  */ 01058000
*   /* BLOCK ERROR WAS ENCOUNTER. IF EITHER IS FOUND THE PROPER      */ 01059000
*   /* RETURN CODE IS SET                                            */ 01060000
*   /*                                                               */ 01061000
*   /*****************************************************************/ 01062000
*                                                                  0755 01063000
*   IF FSBMJRCD=FRCMDRST&FMCFLGA5=ON THEN                          0755 01064000
@RC00714 CLI   FSBMJRCD(FSBPTR),X'18'                              0755 01065000
         BNE   @RF00755                                            0755 01066000
         L     @15,CRRFMCB(,CRRPTR)                                0755 01067000
         TM    FMCFLGA5(@15),B'00000100'                           0755 01068000
         BNO   @RF00755                                            0755 01069000
*     DO;                                                          0756 01070000
*       FSBMJRCD=FRPURGE;           /* COMMAND PURGED                */ 01071000
         MVI   FSBMJRCD(FSBPTR),X'1C'                              0757 01072000
*       FSBMNRCD=FRPDBTEX;          /* REASON: BUFFER EXCEEDED       */ 01073000
         MVI   FSBMNRCD(FSBPTR),X'09'                              0758 01074000
*     END;                                                         0759 01075000
*   ELSE                                                           0760 01076000
*     IF FSBMJRCD=FRCBLKE THEN                                     0760 01077000
         B     @RC00755                                            0760 01078000
@RF00755 CLI   FSBMJRCD(FSBPTR),X'04'                              0760 01079000
         BNE   @RF00760                                            0760 01080000
*       FSBMJRCD=FRLOGICE;                                         0761 01081000
         MVI   FSBMJRCD(FSBPTR),X'08'                              0761 01082000
*   RTNREG=CRRRTN1;                 /* RESTORE RETURN REG            */ 01083000
@RF00760 DS    0H                                                  0762 01084000
@RC00755 L     RTNREG,CRRRTN1(,CRRPTR)                             0762 01085000
*   RETURN;                                                        0763 01086000
@EL00006 DS    0H                                                  0763 01087000
@EF00006 DS    0H                                                  0763 01088000
@ER00006 BR    @14                                                 0763 01089000
*   END CKCONT;                                                    0764 01090000
@PB00006 DS    0H                                                  0764 01091000
*   GEN NOSETS NOREFS(EJECT);                                      0765 01092000
         EJECT                                                          01093000
*                                                                  0766 01094000
*/********************************************************************/ 01095000
*/*                                                                  */ 01096000
*/* ROUTINE NAME : CKCONT1 - CONTINUE CHECK PART(1)                  */ 01097000
*/*                                                                  */ 01098000
*/* FUNCTION : CHECKS FOR INVALID LDO(S). CHECKS MADE ARE AS FOLLOWS:*/ 01099000
*/*               . LDO IS A JUMP TO ANOTHER JUMP LDO (INVALID)      */ 01100000
*/*               . FIRST LDO NOT A JUMP AND NOT A READ (INVALID)    */ 01101000
*/*            IF LDO IS VALID, THE CKCONT SUBRTN IS CALLED.         */ 01102000
*/*                                                                  */ 01103000
*/* INPUT : WRKREG (REG 10) - PTR TO THE FIRST LDO                   */ 01104000
*/*                                                                  */ 01105000
*/* OUTPUT : CRRSW1 (ERROR SWITCH) - TURNED IN CASE LDO IS INVALID   */ 01106000
*/*                                                                  */ 01107000
*/* SUBROUTINES CALLED : CKCONT2                                     */ 01108000
*/*                                                                  */ 01109000
*/* MACROS USED : NONE                                               */ 01110000
*/*                                                                  */ 01111000
*/********************************************************************/ 01112000
*                                                                  0766 01113000
*CKCONT1:                                                          0766 01114000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0766 01115000
         B     @PB00007                                            0766 01116000
CKCONT1  DS    0H                                                  0767 01117000
*   CRRRTN2=RTNREG;                                                0767 01118000
         ST    RTNREG,CRRRTN2(,CRRPTR)                             0767 01119000
*   RESPECIFY                                                      0768 01120000
*     ISTLDO BASED(WRKREG);                                        0768 01121000
*                                                                  0768 01122000
*   /*****************************************************************/ 01123000
*   /*                                                               */ 01124000
*   /* IF LDO OP CODE IS FOR A JUMP GET THE ADDR OF THE NEXT LDO     */ 01125000
*   /*                                                               */ 01126000
*   /*****************************************************************/ 01127000
*                                                                  0769 01128000
*   IF LDOCMD=LDOJUMP THEN                                         0769 01129000
         CLI   LDOCMD(WRKREG),X'08'                                0769 01130000
         BNE   @RF00769                                            0769 01131000
*     WRKREG=LDOADDR;                                              0770 01132000
*                                                                  0770 01133000
         L     WRKREG,LDOADDR(,WRKREG)                             0770 01134000
*   /*****************************************************************/ 01135000
*   /*                                                               */ 01136000
*   /* CHECK THE LDO POINTED TO. IF NOT A READ IT IS INVALID.        */ 01137000
*   /*                                                               */ 01138000
*   /*****************************************************************/ 01139000
*                                                                  0771 01140000
*   IF LDOCMD=LDOREAD THEN                                         0771 01141000
@RF00769 CLI   LDOCMD(WRKREG),X'02'                                0771 01142000
         BNE   @RF00771                                            0771 01143000
*     CALL CKCONT2;                 /* LDO MUST BE A READ            */ 01144000
         BAL   @14,CKCONT2                                         0772 01145000
*   ELSE                                                           0773 01146000
*     DO;                           /* THE LDO IS INVALID            */ 01147000
         B     @RC00771                                            0773 01148000
@RF00771 DS    0H                                                  0774 01149000
*       CRRRTNCD=CRRLOGIC;          /* SET LOGIC ERROR CODE          */ 01150000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0774 01151000
*       CRRFDB2=CRRILDOP;           /* SET INVALID LDO OPCODE        */ 01152000
         MVI   CRRFDB2(CRRPTR),X'06'                               0775 01153000
*       CRRSW1=ON;                  /* SET ERROR SWITCH              */ 01154000
         OI    CRRSW1(CRRPTR),B'01000000'                          0776 01155000
*     END;                                                         0777 01156000
*   RTNREG=CRRRTN2;                                                0778 01157000
@RC00771 L     RTNREG,CRRRTN2(,CRRPTR)                             0778 01158000
*   RETURN;                                                        0779 01159000
@EL00007 DS    0H                                                  0779 01160000
@EF00007 DS    0H                                                  0779 01161000
@ER00007 BR    @14                                                 0779 01162000
*   END CKCONT1;                                                   0780 01163000
@PB00007 DS    0H                                                  0780 01164000
*   GEN NOSETS NOREFS(EJECT);                                      0781 01165000
         EJECT                                                          01166000
*                                                                  0782 01167000
*/********************************************************************/ 01168000
*/*                                                                  */ 01169000
*/* ROUTINE NAME : CKCONT2 - CONTINUE LDO CHECK PART(2)              */ 01170000
*/*                                                                  */ 01171000
*/* FUNCTION : THIS ROUTINE WILL DO THE FOLLOWING:                   */ 01172000
*/*                  . COUNT THE NUMBER OF LDOS                      */ 01173000
*/*                  . INSURE THAT THEY ARE ONLY DATA CHAINED        */ 01174000
*/*                  . IF NEXT LDO IS A JUMP, CALL CKCONT3           */ 01175000
*/*                                                                  */ 01176000
*/* INPUT : WRKREG (REG 10) - PTR TO THE CURRENT LDO                 */ 01177000
*/*                                                                  */ 01178000
*/*                                                                  */ 01179000
*/* OUTPUT : CRRSW1 (ERROR SWITCH) - MAY BE ON IF ERROR FOUND        */ 01180000
*/*                                                                  */ 01181000
*/* SUBROUTINES CALLED : CKCONT3                                     */ 01182000
*/*                                                                  */ 01183000
*/* MACROS USED : NONE                                               */ 01184000
*/*                                                                  */ 01185000
*/********************************************************************/ 01186000
*                                                                  0782 01187000
*CKCONT2:                                                          0782 01188000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0782 01189000
         B     @PB00008                                            0782 01190000
CKCONT2  DS    0H                                                  0783 01191000
*   CRRRTN3=RTNREG;                                                0783 01192000
         ST    RTNREG,CRRRTN3(,CRRPTR)                             0783 01193000
*   RFY                                                            0784 01194000
*     WRKREG2 RSTD;                                                0784 01195000
*                                                                  0784 01196000
*   /*****************************************************************/ 01197000
*   /*                                                               */ 01198000
*   /* WRKREG2 IS USED AS A SWITCH. TO GET OUT OF THE DO LOOP IT WILL*/ 01199000
*   /* BE CHANGED TO A NON ZERO VALUE (4).                           */ 01200000
*   /*                                                               */ 01201000
*   /*****************************************************************/ 01202000
*                                                                  0785 01203000
*   WRKREG2=0;                                                     0785 01204000
         SLR   @15,@15                                             0785 01205000
         LR    WRKREG2,@15                                         0785 01206000
*   CRRLDOCT=0;                     /* ZERO CNT OF LDOS      @XM06417*/ 01207000
         STH   @15,CRRLDOCT(,CRRPTR)                               0786 01208000
*   DO WHILE WRKREG2=0;                                            0787 01209000
         B     @DE00787                                            0787 01210000
@DL00787 DS    0H                                                  0788 01211000
*     CRRLDOCT=CRRLDOCT+1;          /* UP COUNT IN CRR               */ 01212000
*                                                                  0788 01213000
         LA    @15,1                                               0788 01214000
         AH    @15,CRRLDOCT(,CRRPTR)                               0788 01215000
         STH   @15,CRRLDOCT(,CRRPTR)                               0788 01216000
*     /***************************************************************/ 01217000
*     /*                                                             */ 01218000
*     /* ALSO IF THE COUNT OF LDOS SURPASSES 100 THEN THE CAN IS IN- */ 01219000
*     /* VALID.                                                      */ 01220000
*     /*                                                             */ 01221000
*     /***************************************************************/ 01222000
*                                                                  0789 01223000
*     DO WHILE CRRLDOCT^>LDOLIMIT&  /* DO WHILE LIMIT NOT            */ 01224000
*           WRKREG2=0;                                             0789 01225000
*                                                                  0789 01226000
         B     @DE00789                                            0789 01227000
@DL00789 DS    0H                                                  0790 01228000
*       /*************************************************************/ 01229000
*       /*                                                           */ 01230000
*       /* IF THE LDO IS DATA CHAINED THE COUNT CONTINUES. THE PTR TO*/ 01231000
*       /* THE NEXT LDO IS OBTAINED AND THE CHECK CONTINUES.         */ 01232000
*       /*                                                           */ 01233000
*       /*************************************************************/ 01234000
*                                                                  0790 01235000
*       IF LDOACHN=ON               /* CURRENT LDO IS A              */ 01236000
*         THEN                                                     0790 01237000
         TM    LDOACHN(WRKREG),B'10000000'                         0790 01238000
         BNO   @RF00790                                            0790 01239000
*         DO;                       /* READ IF DATA CHAIN            */ 01240000
*           WRKREG=WRKREG+LENGTH(ISTLDO);/* EXAMINE NEXT LDO         */ 01241000
*                                                                  0792 01242000
         AL    WRKREG,@CF00038                                     0792 01243000
*           /*********************************************************/ 01244000
*           /*                                                       */ 01245000
*           /* IF THE CMD IS A JUMP, CKCONT3 MUST BE CHECKED TO MAKE */ 01246000
*           /* SURE IT IS NOT A JUMP TO A JUMP.                      */ 01247000
*           /*                                                       */ 01248000
*           /*********************************************************/ 01249000
*                                                                  0793 01250000
*           IF LDOCMD=LDOJUMP       /* CHECK IT OUT                  */ 01251000
*             THEN                                                 0793 01252000
         CLI   LDOCMD(WRKREG),X'08'                                0793 01253000
         BNE   @RF00793                                            0793 01254000
*             DO;                   /* IN CKCONT3                    */ 01255000
*               CALL CKCONT3;                                      0795 01256000
         BAL   @14,CKCONT3                                         0795 01257000
*               IF CRRSW1=ON        /* IF SEQ AFTER IT               */ 01258000
*                 THEN                                             0796 01259000
         TM    CRRSW1(CRRPTR),B'01000000'                          0796 01260000
         BNO   @RF00796                                            0796 01261000
*                 WRKREG2=4;        /* BAD, RETURN                   */ 01262000
         LA    WRKREG2,4                                           0797 01263000
*               ELSE                                               0798 01264000
*                 WRKREG2=0;                                       0798 01265000
         B     @RC00796                                            0798 01266000
@RF00796 SLR   WRKREG2,WRKREG2                                     0798 01267000
*             END;                                                 0799 01268000
*         END;                                                     0800 01269000
*       ELSE                                                       0801 01270000
*         DO;                       /* FOUND END OF CHAIN            */ 01271000
*                                                                  0801 01272000
         B     @RC00790                                            0801 01273000
@RF00790 DS    0H                                                  0802 01274000
*           /*********************************************************/ 01275000
*           /*                                                       */ 01276000
*           /* IF NOT DATA CHAINED AND NOT CMD CHAINED THE SEARCH AND*/ 01277000
*           /* COUNT IS COMPLETE. THE REGISTER SWITCH IS CHANGED TO  */ 01278000
*           /* NON ZERO.                                             */ 01279000
*           /*                                                       */ 01280000
*           /*********************************************************/ 01281000
*                                                                  0802 01282000
*           IF LDOCMHN=OFF          /* IF NOT CMD CHAIN              */ 01283000
*             THEN                                                 0802 01284000
         TM    LDOCMHN(WRKREG),B'01000000'                         0802 01285000
         BNZ   @RF00802                                            0802 01286000
*             DO;                   /* SO LEAVE BY SETTI             */ 01287000
*               CRRDOCUR=ON;        /* NG SW AND INDICAT             */ 01288000
         OI    CRRDOCUR(CRRPTR),B'10000000'                        0804 01289000
*               WRKREG2=4;          /* E END IN WRKREG2              */ 01290000
         LA    WRKREG2,4                                           0805 01291000
*             END;                  /* WITH NONZERO VALUE            */ 01292000
*                                                                  0806 01293000
*           /*********************************************************/ 01294000
*           /*                                                       */ 01295000
*           /* THE LDO IS CMD CHAINED AND READS CANNOT BE CMD CHAINED*/ 01296000
*           /* .                                                     */ 01297000
*           /*                                                       */ 01298000
*           /*********************************************************/ 01299000
*                                                                  0807 01300000
*           ELSE                                                   0807 01301000
*             DO;                   /* NOT DATA CHAINED              */ 01302000
         B     @RC00802                                            0807 01303000
@RF00802 DS    0H                                                  0808 01304000
*               CRRRTNCD=CRRLOGIC;  /* BUT COMMAND CHANED            */ 01305000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0808 01306000
*               CRRFDB2=CRRRIOCC;                                  0809 01307000
         MVI   CRRFDB2(CRRPTR),X'1D'                               0809 01308000
*               CRRSW1=ON;          /* SO SET ERROR SW AN            */ 01309000
         OI    CRRSW1(CRRPTR),B'01000000'                          0810 01310000
*               WRKREG2=4;                                         0811 01311000
         LA    WRKREG2,4                                           0811 01312000
*             END;                                                 0812 01313000
*         END;                                                     0813 01314000
@RC00802 DS    0H                                                  0814 01315000
*     END;                          /* END OF DO WHILE               */ 01316000
@RC00790 DS    0H                                                  0814 01317000
@DE00789 LH    @15,CRRLDOCT(,CRRPTR)                               0814 01318000
         C     @15,@CF04336                                        0814 01319000
         BH    @DC00789                                            0814 01320000
         LTR   WRKREG2,WRKREG2                                     0814 01321000
         BZ    @DL00789                                            0814 01322000
@DC00789 DS    0H                                                  0815 01323000
*     IF WRKREG2=0                  /* LDO LIMIT EXCEEDED            */ 01324000
*       THEN                                                       0815 01325000
         LTR   WRKREG2,WRKREG2                                     0815 01326000
         BNZ   @RF00815                                            0815 01327000
*       DO;                         /* AND END STILL NOT             */ 01328000
*                                                                  0816 01329000
*         /***********************************************************/ 01330000
*         /*                                                         */ 01331000
*         /* THE COUNT OF LDOS EXCEEDED 100. INDICATE ERROR.         */ 01332000
*         /*                                                         */ 01333000
*         /***********************************************************/ 01334000
*                                                                  0817 01335000
*         CRRRTNCD=CRRLOGIC;        /* REACHED SET                   */ 01336000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0817 01337000
*         CRRFDB2=CRRM100;          /* ERROR FLAGS                   */ 01338000
         MVI   CRRFDB2(CRRPTR),X'1A'                               0818 01339000
*         CRRSW1=ON;                                               0819 01340000
         OI    CRRSW1(CRRPTR),B'01000000'                          0819 01341000
*         WRKREG2=4;                                               0820 01342000
         LA    WRKREG2,4                                           0820 01343000
*       END;                                                       0821 01344000
*     ELSE                                                         0822 01345000
*       ;                                                          0822 01346000
@RF00815 DS    0H                                                  0823 01347000
*   END;                                                           0823 01348000
@DE00787 LTR   WRKREG2,WRKREG2                                     0823 01349000
         BZ    @DL00787                                            0823 01350000
*   RTNREG=CRRRTN3;                                                0824 01351000
         L     RTNREG,CRRRTN3(,CRRPTR)                             0824 01352000
*   RETURN;                                                        0825 01353000
@EL00008 DS    0H                                                  0825 01354000
@EF00008 DS    0H                                                  0825 01355000
@ER00008 BR    @14                                                 0825 01356000
*   END CKCONT2;                                                   0826 01357000
@PB00008 DS    0H                                                  0826 01358000
*   RFY                                                            0827 01359000
*     WRKREG2 UNRSTD;                                              0827 01360000
*   GEN NOSETS NOREFS(EJECT);                                      0828 01361000
         EJECT                                                          01362000
*                                                                  0829 01363000
*/********************************************************************/ 01364000
*/*                                                                  */ 01365000
*/* ROUTINE NAME : CKCONT3 - CONTINE CHECK PART 3                    */ 01366000
*/*                                                                  */ 01367000
*/* FUNCTION : MAKE SURE THE LDO JUMPED TO IS NOT ANOTHER JUMP, AND  */ 01368000
*/*      THIS NEW LDO IS NOT CMD CHAINED.                            */ 01369000
*/*                                                                  */ 01370000
*/* INPUT : WRKREG (REG 10) - PTR TO CURRENT LDO                     */ 01371000
*/*                                                                  */ 01372000
*/* OUTPUT : CRRSW1 (ERROR SWITCH) - MAY BE SET IF ERROR FOUND       */ 01373000
*/*                                                                  */ 01374000
*/* SUBROUTINES CALLED : NONE                                        */ 01375000
*/*                                                                  */ 01376000
*/* MACROS USED : NONE                                               */ 01377000
*/*                                                                  */ 01378000
*/********************************************************************/ 01379000
*                                                                  0829 01380000
*CKCONT3:                                                          0829 01381000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0829 01382000
         B     @PB00009                                            0829 01383000
CKCONT3  DS    0H                                                  0830 01384000
*   WRKREG=LDOADDR;                 /* AT LDO JUMPED TO              */ 01385000
         L     WRKREG,LDOADDR(,WRKREG)                             0830 01386000
*   IF LDOCMD=LDOJUMP               /* IF JUMP TO JUMP               */ 01387000
*     THEN                                                         0831 01388000
         CLI   LDOCMD(WRKREG),X'08'                                0831 01389000
         BNE   @RF00831                                            0831 01390000
*     DO;                           /* BAD LDO CHAIN SO              */ 01391000
*       CRRRTNCD=CRRLOGIC;          /* SET ERROR FLAGS AN            */ 01392000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0833 01393000
*       CRRFDB2=CRRRIOCC;           /* D RET CODE AND                */ 01394000
         MVI   CRRFDB2(CRRPTR),X'1D'                               0834 01395000
*       CRRSW1=ON;                  /* LEAVE                         */ 01396000
         OI    CRRSW1(CRRPTR),B'01000000'                          0835 01397000
*     END;                                                         0836 01398000
*   ELSE                                                           0837 01399000
*     DO;                           /* IF NOT DATA CHAINE            */ 01400000
         B     @RC00831                                            0837 01401000
@RF00831 DS    0H                                                  0838 01402000
*       IF LDOACHN=OFF&             /* D BUT COMMAND CHAI            */ 01403000
*           LDOCMHN=ON              /* NED THEN SET ERROR            */ 01404000
*         THEN                                                     0838 01405000
         TM    LDOCMHN(WRKREG),B'01000000'                         0838 01406000
         BNO   @RF00838                                            0838 01407000
         TM    LDOACHN(WRKREG),B'10000000'                         0838 01408000
         BNZ   @RF00838                                            0838 01409000
*         DO;                       /* FLAGS AND RETURN              */ 01410000
*           CRRRTNCD=CRRLOGIC;      /* CODE AND LEAVE                */ 01411000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0840 01412000
*           CRRSW1=ON;                                             0841 01413000
         OI    CRRSW1(CRRPTR),B'01000000'                          0841 01414000
*           CRRFDB2=CRRRIOCC;                                      0842 01415000
         MVI   CRRFDB2(CRRPTR),X'1D'                               0842 01416000
*         END;                                                     0843 01417000
*     END;                                                         0844 01418000
*   RETURN;                                                        0845 01419000
@EL00009 DS    0H                                                  0845 01420000
@EF00009 DS    0H                                                  0845 01421000
@ER00009 BR    @14                                                 0845 01422000
*   END CKCONT3;                                                   0846 01423000
         B     @EL00009                                            0846 01424000
@PB00009 DS    0H                                                  0846 01425000
*   GEN NOSETS NOREFS(EJECT);                                      0847 01426000
         EJECT                                                          01427000
*                                                                  0848 01428000
*/********************************************************************/ 01429000
*/*                                                                  */ 01430000
*/* ROUTINE NAME : UECBINIT - UECB INITIALIZATION                    */ 01431000
*/*                                                                  */ 01432000
*/* FUNCTION : INITIALIZE THE UECB  COMMON FIELDS WITH THE COMMON IN-*/ 01433000
*/*      FORMATION AND THEN DETERMINE WHAT SPECIFIC EXIT IS BEING RE-*/ 01434000
*/*      QUESTED SO THE UECB ID AND ,IF NECESSARY, DATA COUNT CAN AL-*/ 01435000
*/*      SO BE FILLED IN. THE UECB IS THEN QUEUED TO ITSELF AND SCHED*/ 01436000
*/*                                                                  */ 01437000
*/* INPUT : CRRSW1 - SET FOR LOSTERM, NOT SET FOR ATTN               */ 01438000
*/*         CRRSW2 - SET FOR LOSTERM OR ATTN, NOT SET FOR UNSIP      */ 01439000
*/*         WRKREG1 (REG 9) - PTR TO THE ACB                         */ 01440000
*/*                                                                  */ 01441000
*/* OUTPUT : USER EXIT SCHEDULED                                     */ 01442000
*/*                                                                  */ 01443000
*/* SUBROUTINES CALLED : NONE                                        */ 01444000
*/*                                                                  */ 01445000
*/* MACROS USED : REQSTORE , TPQUE                                   */ 01446000
*/*                                                                  */ 01447000
*/********************************************************************/ 01448000
*                                                                  0848 01449000
*UECBINIT:                                                         0848 01450000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0848 01451000
         B     @PB00010                                            0848 01452000
UECBINIT DS    0H                                                  0849 01453000
*   CRRRTN3=RTNREG;                                                0849 01454000
         ST    RTNREG,CRRRTN3(,CRRPTR)                             0849 01455000
*   RFY                                                            0850 01456000
*    (UECBPTR,                                                     0850 01457000
*     WRKREG1,                                                     0850 01458000
*     WRKREG2,                                                     0850 01459000
*     RCREG) RSTD;                                                 0850 01460000
*   RFY                                                            0851 01461000
*     ISTFDB BASED(WRKREG2);                                       0851 01462000
*                                                                  0851 01463000
*   /*****************************************************************/ 01464000
*   /*                                                               */ 01465000
*   /* GET THE STORAGE FOR THE USER EXIT CB. IF STORAGE IS NOT IMMED */ 01466000
*   /* IATELY AVAILABLE, ISSUE A TPWAIT AND WAIT UNTIL IT IS ACQUIRED*/ 01467000
*   /* REQSTORE CBID(UECB)RPH(RPHPTR)CLEAR(YES)PRIORITY(YES)         */ 01468000
*   /*                                                               */ 01469000
*   /*****************************************************************/ 01470000
*                                                                  0852 01471000
*   DO;                                                            0852 01472000
*                                                                  0852 01473000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)                               */ 01474000
*/* END OF ATBUILD                                                   */ 01475000
*                                                                  0853 01476000
*     RFY                                                          0853 01477000
*      (GPR02P,                                                    0853 01478000
*       GPR03P,                                                    0853 01479000
*       GPR04P,                                                    0853 01480000
*       GPR15P) RSTD;                                              0853 01481000
*     RFY                                                          0854 01482000
*      (GPR01P,                                                    0854 01483000
*       GPR13P) RSTD;                                              0854 01484000
*     GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);           0855 01485000
           STM GPR02P,GPR13P,RPHWORK(GPR01P)                            01486000
*     RFY                                                          0856 01487000
*      (GPR01P,                                                    0856 01488000
*       GPR13P) UNRSTD;                                            0856 01489000
*     DO;                           /* LISATCVT                    0857 01490000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 01491000
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 01492000
         L     GPR15P,ATCLCPTR                                     0858 01493000
*     END;                          /* LISATCVT                      */ 01494000
*     GPR02P=GPR15P->ATCBPDA|'80000000'X;                          0860 01495000
         L     GPR02P,ATCBPDA(,GPR15P)                             0860 01496000
         O     GPR02P,@CF01364                                     0860 01497000
*     DCL                                                          0861 01498000
*       1 FMZA0062 FIXED(31) STATIC LOCAL,                         0861 01499000
*        2 * FIXED(8) INIT(248),                                   0861 01500000
*        2 * PTR(24) INIT(0);                                      0861 01501000
*     GPR03F=FMZA0062;                                             0862 01502000
         L     GPR03F,FMZA0062                                     0862 01503000
*     GPR04F=17;                                                   0863 01504000
         LA    GPR04F,17                                           0863 01505000
*     GPR15P=GPR15P->ATCSMRQ;                                      0864 01506000
         L     GPR15P,ATCSMRQ(,GPR15P)                             0864 01507000
*     DCL                                                          0865 01508000
*       FMZL0062 BASED(GPR15P) LABEL VALUERANGE(*);                0865 01509000
*     CALL FMZL0062;                                               0866 01510000
         BALR  @14,@15                                             0866 01511000
*     RFY                                                          0867 01512000
*      (GPR13P) RSTD;                                              0867 01513000
*     GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);           0868 01514000
           LM GPR02P,GPR13P,RPHWORK(GPR01P)                             01515000
*     RFY                                                          0869 01516000
*      (GPR13P) UNRSTD;                                            0869 01517000
*     RFY                                                          0870 01518000
*      (GPR02P,                                                    0870 01519000
*       GPR03P,                                                    0870 01520000
*       GPR04P,                                                    0870 01521000
*       GPR15P) UNRSTD;                                            0870 01522000
*   END;                            /* ******** END OF REQSTORE    0871 01523000
*                                      EXPANSION GET STORAGE FOR UECB*/ 01524000
*   IF RCREG=8 THEN                                                0872 01525000
*                                                                  0872 01526000
         C     RCREG,@CF00038                                      0872 01527000
         BNE   @RF00872                                            0872 01528000
*/*  TPWAIT                                                          */ 01529000
*                                                                  0873 01530000
*     DO;                           /* BEGIN TPWAIT GENERATION       */ 01531000
*       RFY                                                        0874 01532000
*        (GPR01P,                                                  0874 01533000
*         GPR14P,                                                  0874 01534000
*         GPR15P) RSTD;                                            0874 01535000
*       RFY                                                        0875 01536000
*        (GPR00P) RSTD;                                            0875 01537000
*       GEN(STM   GPR00P,GPR15P,RPHWORK(GPR01P))                   0876 01538000
*       REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);                  0876 01539000
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)                            01540000
*       RFY                                                        0877 01541000
*        (GPR00P) UNRSTD;                                          0877 01542000
*       GPR15P=ADDR(LHAA0081);                                     0878 01543000
         LA    GPR15P,LHAA0081                                     0878 01544000
*       GPR14P=0;                                                  0879 01545000
         SLR   GPR14P,GPR14P                                       0879 01546000
*       GPR15P=GPR15P&'00FFFFFF'X;                                 0880 01547000
         N     GPR15P,@CF04438                                     0880 01548000
*       GPR15P=GPR15P|RPHWT1;                                      0881 01549000
         O     GPR15P,@CF01305                                     0881 01550000
*       CS(GPR14P,GPR15P,GPR01P->RPHRESMA);                        0882 01551000
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         0882 01552000
*       BC(7,LHAB0081);                                            0883 01553000
*                                                                  0883 01554000
         BC    7,LHAB0081                                          0883 01555000
*/* TPEXIT  NONE                                                     */ 01556000
*                                                                  0884 01557000
*       DO;                                                        0884 01558000
*         RESPECIFY                                                0885 01559000
*           GPR15P RSTD;                                           0885 01560000
*                                                                  0885 01561000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)                               */ 01562000
*/* END OF ATBUILD                                                   */ 01563000
*                                                                  0886 01564000
*         DO;                       /* LISATCVT                    0886 01565000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 01566000
*           GPR15P=ATCLCPTR;        /* SET UP ATCVT POINTER          */ 01567000
         L     GPR15P,ATCLCPTR                                     0887 01568000
*         END;                      /* LISATCVT                      */ 01569000
*         RESPECIFY                                                0889 01570000
*           GPR00F RSTD;                                           0889 01571000
*         GPR00F=0;                 /* SET PARAMETER REGISTER TO 0   */ 01572000
         SLR   GPR00F,GPR00F                                       0890 01573000
*         GPR15P=GPR15P->ATCAP31;                                  0891 01574000
         L     GPR15P,ATCAP31(,GPR15P)                             0891 01575000
*         DCL                                                      0892 01576000
*           FMZL0086 BASED(GPR15P) LABEL VALUERANGE(*);            0892 01577000
*         CALL FMZL0086;                                           0893 01578000
         BALR  @14,@15                                             0893 01579000
*         RESPECIFY                                                0894 01580000
*          (GPR00F,                                                0894 01581000
*           GPR15P) UNRSTD;                                        0894 01582000
*       END;                        /* **** END OF TPEXIT            */ 01583000
*LHAB0081:                                                         0896 01584000
*       ;                                                          0896 01585000
LHAB0081 DS    0H                                                  0897 01586000
*       GPR01P->RPHRESMA=0;                                        0897 01587000
         SLR   @00,@00                                             0897 01588000
         ST    @00,RPHRESMA(,GPR01P)                               0897 01589000
*       RFY                                                        0898 01590000
*        (GPR00P) RSTD;                                            0898 01591000
*LHAA0081:                                                         0899 01592000
*       ;                                                          0899 01593000
LHAA0081 DS    0H                                                  0900 01594000
*       GEN(LM    GPR00P,GPR15P,RPHWORK(GPR01P))                   0900 01595000
*       REFS(RPHWORK,GPR01P) SETS(GPR00P,GPR15P);                  0900 01596000
         LM    GPR00P,GPR15P,RPHWORK(GPR01P)                            01597000
*       RFY                                                        0901 01598000
*        (GPR00P,                                                  0901 01599000
*         GPR01P,                                                  0901 01600000
*         GPR14P,                                                  0901 01601000
*         GPR15P) UNRSTD;                                          0901 01602000
*     END;                          /* ****END OF TPWAIT WAIT FOR  0902 01603000
*                                      STORAGE                       */ 01604000
*   RFY                                                            0903 01605000
*     RCREG UNRSTD;                                                0903 01606000
@RF00872 DS    0H                                                  0904 01607000
*   UECBPTR=RPHSRPRM;               /*                       @DM08634*/ 01608000
         L     UECBPTR,RPHSRPRM(,RPHPTR)                           0904 01609000
*   RESPECIFY                                                      0905 01610000
*     ISTUECB BASED(UECBPTR);       /* UECB                  @DM08634*/ 01611000
*   RESPECIFY                                                      0906 01612000
*     ISTPAB BASED(ADDR(UECPAB));   /* COVER PAB IN UECB             */ 01613000
*                                                                  0906 01614000
*   /*****************************************************************/ 01615000
*   /*                                                               */ 01616000
*   /* INITIALIZE THE UECB SO IT CAN BE SCHEDULED                    */ 01617000
*   /*                                                               */ 01618000
*   /*****************************************************************/ 01619000
*                                                                  0907 01620000
*   UECID=UEID;                     /* SET BLOCK ID                  */ 01621000
         MVI   UECID(UECBPTR),X'10'                                0907 01622000
*   UECLEN=LENGTH(ISTUECB);                                        0908 01623000
         MVI   UECLEN(UECBPTR),X'54'                               0908 01624000
*   UECACB=WRKREG1;                 /* GET ADDR OF ACB               */ 01625000
         ST    WRKREG1,UECACB(,UECBPTR)                            0909 01626000
*   UECSAF=FMCSAF;                  /* GET SOURCE ID                 */ 01627000
         L     @15,CRRFMCB(,CRRPTR)                                0910 01628000
         MVC   UECSAF(2,UECBPTR),FMCSAF(@15)                       0910 01629000
*   UECDAF=NCBDAF;                  /* GET DEST. ID                  */ 01630000
         L     @07,FMCDNCBA(,@15)                                  0911 01631000
         MVC   UECDAF(2,UECBPTR),NCBDAF(@07)                       0911 01632000
*   UECUSFLD=FMCUSFLD;              /* USER FLD FROM FMCB            */ 01633000
         MVC   UECUSFLD(4,UECBPTR),FMCUSFLD(@15)                   0912 01634000
*   UECTSKID=FMCTSKID;              /* TASK ID FROM FMCB             */ 01635000
         L     @15,FMCTSKID(,@15)                                  0913 01636000
         ST    @15,UECTSKID(,UECBPTR)                              0913 01637000
*   UECDVTPT=ATCUEP;                /* USER EXIT INTERFACE           */ 01638000
         L     @15,CRRATCVT(,CRRPTR)                               0914 01639000
         L     @15,ATCUEP(,@15)                                    0914 01640000
         ST    @15,UECDVTPT(,UECBPTR)                              0914 01641000
*   PABOFFST=ADDR(UECPAB)-ADDR(ISTUECB);/* PAB OFFSET                */ 01642000
         LA    @15,16                                              0915 01643000
         ALR   @15,UECBPTR                                         0915 01644000
         LA    @00,UECPAB(,UECBPTR)                                0915 01645000
         SLR   @00,UECBPTR                                         0915 01646000
         STC   @00,PABOFFST(,@15)                                  0915 01647000
*   PABDVTA=ADDR(UECDVTPT);         /* POINT PABDVT TO XIT           */ 01648000
         LA    @00,UECDVTPT(,UECBPTR)                              0916 01649000
         STCM  @00,7,PABDVTA(@15)                                  0916 01650000
*   PABAPYP=ON;                                                    0917 01651000
*   PABNORPR=ON;                    /* SET NORMAL PRIOR              */ 01652000
*                                                                  0918 01653000
         OI    PABAPYP(@15),B'10100000'                            0918 01654000
*   /*****************************************************************/ 01655000
*   /*                                                               */ 01656000
*   /* IF CRRSW1 IS ON THIS IS A LOSTERM EXIT                        */ 01657000
*   /*                                                               */ 01658000
*   /*****************************************************************/ 01659000
*                                                                  0919 01660000
*   IF CRRSW1=ON THEN                                              0919 01661000
         TM    CRRSW1(CRRPTR),B'01000000'                          0919 01662000
         BNO   @RF00919                                            0919 01663000
*     DO;                           /* IF FOR LOST TERM.             */ 01664000
*                                                                  0920 01665000
*       /*************************************************************/ 01666000
*       /*                                                           */ 01667000
*       /* SW 1 IS ON, THIS IS A LOST TERMINAL EXIT, TEST FOR DIAL IN*/ 01668000
*       /* OR DIAL OUT DEVICE AND PUT THE CORRECT CODE IN THE UECB   */ 01669000
*       /* ALSO.                                                     */ 01670000
*       /*                                                           */ 01671000
*       /*************************************************************/ 01672000
*                                                                  0921 01673000
*       UECTYPE=UECLOGOF;           /* SET LOST TERM. ID             */ 01674000
         MVI   UECTYPE(UECBPTR),X'09'                              0921 01675000
*       IF FSBMNDNU=ON THEN                                        0922 01676000
         TM    FSBMNDNU(FSBPTR),B'00010000'                        0922 01677000
         BNO   @RF00922                                            0922 01678000
*         UECRSNCD='00'X;           /* DIAL DEV.                     */ 01679000
         XC    UECRSNCD+1(3,UECBPTR),UECRSNCD+1(UECBPTR)           0923 01680000
         MVI   UECRSNCD(UECBPTR),X'00'                             0923 01681000
*       ELSE                                                       0924 01682000
*         UECRSNCD='04'X;           /* DIAL OUT DEVICE               */ 01683000
         B     @RC00922                                            0924 01684000
@RF00922 XC    UECRSNCD+1(3,UECBPTR),UECRSNCD+1(UECBPTR)           0924 01685000
         MVI   UECRSNCD(UECBPTR),X'04'                             0924 01686000
*     END;                          /* END SETUP FOR LOSTERM EXIT    */ 01687000
*                                                                  0925 01688000
*   /*****************************************************************/ 01689000
*   /*                                                               */ 01690000
*   /* THIS IS AN ATTENTION EXIT , PLACE THE ATTN ID IN THE UECB     */ 01691000
*   /*                                                               */ 01692000
*   /*****************************************************************/ 01693000
