         TITLE 'ISTDCC35-CONTROL LAYER FEEDBACK START EXTENSION        *00001000
                        '                                               00002000
ISTDCC35 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC35  75.206'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*                                                                  0496 00013000
*   /*****************************************************************/ 00014000
*   /*                                                               */ 00015000
*   /* START OF ISTDCC35 MAINLINE                                    */ 00016000
*   /*                                                               */ 00017000
*   /*****************************************************************/ 00018000
*                                                                  0497 00019000
*   CRRPTR=ADDR(CRRSPACE);          /* GET ADDRESSABILITY OF CRR     */ 00020000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0497 00021000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0497 00022000
*   CRRMAS=CRRMAS&&CRRMAS;          /* ZERO OUT THE MODULE AUDIT   0498 00023000
*                                      TRAIL SWITCHES                */ 00024000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0498 00025000
*   CRRM3X=ON;                      /* TURN ON THE 35 MODULE SW      */ 00026000
         OI    CRRM3X(CRRPTR),B'00000100'                          0499 00027000
*   CRRC3X=ON;                      /* TURN ON COMPONENT SWITCH      */ 00028000
*                                                                  0500 00029000
         OI    CRRC3X(CRRPTR),B'00000100'                          0500 00030000
*   /*****************************************************************/ 00031000
*   /*                                                               */ 00032000
*   /* THE DVTPTR HAS TO BE ZEROED SO THAT IT CAN BE USED TO DE-     */ 00033000
*   /* TERMINE IF A TPESC OR TPDVTS IS TO BE DONE. IF A TPDVTS IS TO */ 00034000
*   /* BE DONE, THE DVTPTR WILL POINT TO THE DVT THAT IS TO GET CTL  */ 00035000
*   /* NAME.                                                         */ 00036000
*   /*                                                               */ 00037000
*   /*****************************************************************/ 00038000
*                                                                  0501 00039000
*   DVTPTR=DVTPTR&&DVTPTR;          /* CLEAR REG 11                  */ 00040000
*                                                                  0501 00041000
         XR    DVTPTR,DVTPTR                                       0501 00042000
*   /*****************************************************************/ 00043000
*   /*                                                               */ 00044000
*   /* IF DCC30 FOUND THAT THIS WAS AN ATTN CASE ,IT ZEROED THE FSB  */ 00045000
*   /* PTR IN THE CRR, AND SET UP FOR THE TPDVTS TO DCC35. CALL      */ 00046000
*   /* HDATTN                                                        */ 00047000
*   /*                                                               */ 00048000
*   /*****************************************************************/ 00049000
*                                                                  0502 00050000
*   DO;                             /* MODESET EXTKEY(ZERO)          */ 00051000
*     SPKA(0);                      /* SET PSW KEY                   */ 00052000
         SLR   @15,@15                                             0503 00053000
         SPKA  0(@15)                                              0503 00054000
*   END;                            /* MODESET EXTKEY(ZERO) VS30123     00055000
*                                                            @Y30APCF*/ 00056000
*   IF CRRFSB=0 THEN                                               0505 00057000
         C     @15,CRRFSB(,CRRPTR)                                 0505 00058000
         BNE   @RF00505                                            0505 00059000
*     CALL HDATTN;                  /* ATTN CASE                     */ 00060000
*                                                                  0506 00061000
         BAL   @14,HDATTN                                          0506 00062000
*   /*****************************************************************/ 00063000
*   /*                                                               */ 00064000
*   /* IF DCC30 FOUND AN LCPB WAS NOT ASSOCIATED WITH THE LCPB THEN  */ 00065000
*   /* THE FSB WAS NOT CHANGED AND CTL PASSED TO DCC35. CALL CKUNSD. */ 00066000
*   /*                                                               */ 00067000
*   /*****************************************************************/ 00068000
*                                                                  0507 00069000
*   ELSE                                                           0507 00070000
*     DO;                           /* NOT AN ATTN CASE              */ 00071000
         B     @RC00505                                            0507 00072000
@RF00505 DS    0H                                                  0508 00073000
*       RFY                                                        0508 00074000
*         FSBPTR RSTD;                                             0508 00075000
*       FSBPTR=CRRFSB;              /* INIT FSB POINTER              */ 00076000
         L     FSBPTR,CRRFSB(,CRRPTR)                              0509 00077000
*       IF FSBLCPB=0 THEN                                          0510 00078000
         L     @15,FSBLCPB(,FSBPTR)                                0510 00079000
         LTR   @15,@15                                             0510 00080000
         BNZ   @RF00510                                            0510 00081000
*         CALL CKUNSD;              /* DATA UNSOLICITED.             */ 00082000
*                                                                  0511 00083000
         BAL   @14,CKUNSD                                          0511 00084000
*       /*************************************************************/ 00085000
*       /*                                                           */ 00086000
*       /* AN RPL WAS FOUND THAT ASSOCIATED WITH THIS REQUEST IN     */ 00087000
*       /* DCC30 SO DCC35 WAS GIVEN CONTROL TO CONTINE THE CHECK     */ 00088000
*       /*                                                           */ 00089000
*       /*************************************************************/ 00090000
*                                                                  0512 00091000
*       ELSE                                                       0512 00092000
*         CALL CKCONT;              /* CONTINUE TO CHECK OUT REQ.    */ 00093000
         B     @RC00510                                            0512 00094000
@RF00510 BAL   @14,CKCONT                                          0512 00095000
*     END;                          /* END LCPB PTR IN FSB           */ 00096000
*                                                                  0513 00097000
@RC00510 DS    0H                                                  0514 00098000
*   /*****************************************************************/ 00099000
*   /*                                                               */ 00100000
*   /* PROCESSING HAS COMPLETED, FIND OUT IF THE NEXT PROCESSOR WAS  */ 00101000
*   /* FOUND AND THE REQUEST IS TO BE CONTINUED, OR CTL IS TO BE     */ 00102000
*   /* PASSED TO DCC32 TO FINISH PROCESSING.                         */ 00103000
*   /*                                                               */ 00104000
*   /*****************************************************************/ 00105000
*                                                                  0514 00106000
*   DO;                             /* MODESET EXTKEY(VTAM)          */ 00107000
@RC00505 DS    0H                                                  0515 00108000
*     SPKA(96);                     /* SET PSW KEY                   */ 00109000
         LA    @15,96                                              0515 00110000
         SPKA  0(@15)                                              0515 00111000
*   END;                            /* MODESET EXTKEY(VTAM) VS30123     00112000
*                                                            @Y30APCF*/ 00113000
*   IF DVTPTR=0 THEN                                               0517 00114000
*                                                                  0517 00115000
         LTR   DVTPTR,DVTPTR                                       0517 00116000
         BNZ   @RF00517                                            0517 00117000
*/*TPESC                                                             */ 00118000
*                                                                  0518 00119000
*     DO;                                                          0518 00120000
*                                                                  0518 00121000
*/* ATBUILD (ISTDYPAB,ISTAVT,ISTATCVT)                               */ 00122000
*/* END OF ATBUILD                                                   */ 00123000
*                                                                  0519 00124000
*       RFY                                                        0519 00125000
*         GPR15P RSTD;                                             0519 00126000
*       DO;                         /* LISATCVT                    0520 00127000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00128000
*         GPR15P=ATCLCPTR;          /* SET UP ATCVT POINTER          */ 00129000
         L     GPR15P,ATCLCPTR                                     0521 00130000
*       END;                        /* LISATCVT                      */ 00131000
*       GPR15P=GPR15P->ATCESC01;                                   0523 00132000
         L     GPR15P,ATCESC01(,GPR15P)                            0523 00133000
*       DCL                                                        0524 00134000
*         FMZL0011 BASED(GPR15P) LABEL VALUERANGE(*);              0524 00135000
*       CALL FMZL0011;                                             0525 00136000
