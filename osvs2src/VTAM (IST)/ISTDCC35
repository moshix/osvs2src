*       RESPECIFY                                                  0526 00138000
*         GPR15P UNRSTD;                                           0526 00139000
*     END;                          /* **** END OF TPESC PROCESSING     00140000
*                                      COMPLETE                      */ 00141000
*/*  TPDVTS NAMEPTR(DVTPTR)                                          */ 00142000
*                                                                  0528 00143000
*   DO;                                                            0528 00144000
*                                                                  0528 00145000
@RF00517 DS    0H                                                  0529 00146000
*/* ATBUILD (ISTRPH,ISTPAB,ISTDYPAB,ISTATCVT)                        */ 00147000
*/* END OF ATBUILD                                                   */ 00148000
*                                                                  0529 00149000
*     RFY                                                          0529 00150000
*      (GPR01P,                                                    0529 00151000
*       GPR02P,                                                    0529 00152000
*       GPR06P,                                                    0529 00153000
*       GPR08P) RSTD;                                              0529 00154000
*     GEN REFS(GPR01P,GPR02P,GPR08P) SETS(RPHWORK)                 0530 00155000
*         (STM   GPR02P,GPR08P,RPHWORK(GPR01P));                   0530 00156000
         STM   GPR02P,GPR08P,RPHWORK(GPR01P)                            00157000
*     RFY                                                          0531 00158000
*      (GPR02P,                                                    0531 00159000
*       GPR08P) UNRSTD;                                            0531 00160000
*     RFY                                                          0532 00161000
*       GPR00P RSTD;                                               0532 00162000
*     GPR00P=DVTPTR;                                               0533 00163000
         LR    GPR00P,DVTPTR                                       0533 00164000
*     GPR01P->RPHSRPRM=0;                                          0534 00165000
         SLR   @15,@15                                             0534 00166000
         ST    @15,RPHSRPRM(,GPR01P)                               0534 00167000
*     RFY                                                          0535 00168000
*       GPR15P RSTD;                                               0535 00169000
*     DO;                           /* LISATCVT                    0536 00170000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00171000
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00172000
         L     GPR15P,ATCLCPTR                                     0537 00173000
*     END;                          /* LISATCVT                      */ 00174000
*     GPR15P=GPR15P->ATCESC02;                                     0539 00175000
         L     GPR15P,ATCESC02(,GPR15P)                            0539 00176000
*     DCL                                                          0540 00177000
*       FMZL0019 BASED(GPR15P) LABEL VALUERANGE(*);                0540 00178000
*     CALL FMZL0019;                                               0541 00179000
         BALR  @14,@15                                             0541 00180000
*     RFY                                                          0542 00181000
*      (GPR00P,                                                    0542 00182000
*       GPR15P) UNRSTD;                                            0542 00183000
*     RFY                                                          0543 00184000
*      (GPR02P,                                                    0543 00185000
*       GPR08P) RSTD;                                              0543 00186000
*     GEN REFS(RPHWORK) SETS(GPR02P,GPR08P)                        0544 00187000
*         (LM    GPR02P,GPR08P,RPHWORK(GPR01P));                   0544 00188000
         LM    GPR02P,GPR08P,RPHWORK(GPR01P)                            00189000
*     RFY                                                          0545 00190000
*      (GPR01P,                                                    0545 00191000
*       GPR02P,                                                    0545 00192000
*       GPR06P,                                                    0545 00193000
*       GPR08P) UNRSTD;                                            0545 00194000
*   END;                                                           0546 00195000
*                                                                  0546 00196000
*/***** END OF TPDVTS GENERATION ****                                */ 00197000
*                                                                  0547 00198000
*                                   /* CONTINUE PROCESSING           */ 00199000
*/*VABEND COMPCODE(VTAM)REASCODE(ABNDCONT)DUMP(NO)                   */ 00200000
*                                                                  0547 00201000
*   DO;                             /* ****BEGIN VABEND            0547 00202000
*                                      GENERATION****                */ 00203000
*     RFY                                                          0548 00204000
*      (GPR01P,                                                    0548 00205000
*       GPR15P) RSTD;               /* DECLARE AND RESTRICT REGS     */ 00206000
*     GPR15F=ABNDCONT;              /* PUT REASON CODE IN REG15      */ 00207000
         L     GPR15F,@CF04344                                     0549 00208000
*     GPR01F='A9000'X;              /* COMP CODE IN R1 BITS 8-19   0550 00209000
*                                      INVOKE OS/VS ABEND MACRO      */ 00210000
         L     GPR01F,@CF04371                                     0550 00211000
*     GEN REFS(GPR01F);                                            0551 00212000
         ABEND (1),,STEP                                                00213000
*     RFY                                                          0552 00214000
*      (GPR01P,                                                    0552 00215000
*       GPR15P) UNRSTD;             /* UNRESTRICT REGS 1 AND 15      */ 00216000
*   END;                            /* ****END OF VABEND           0553 00217000
*                                      GENERATION****                */ 00218000
*   GEN NOSETS NOREFS(EJECT);                                      0554 00219000
*                                                                  0554 00220000
         EJECT                                                          00221000
*/********************************************************************/ 00222000
*/*                                                                  */ 00223000
*/* ROUTINE NAME : FREEFSB - FREE THE FSB                            */ 00224000
*/*                                                                  */ 00225000
*/* FUNCTION : FREE THE FSB AND ALL ASSOCIATED FDB(S). IF DATA IS    */ 00226000
*/*      CONFIDENTIAL ,CLEAR THE DATA AREA, OTHERWISE JUST FREE IT.  */ 00227000
*/*                                                                  */ 00228000
*/* INPUT : FSBPTR (REG 5) - PTR TO FSB                              */ 00229000
*/*                                                                  */ 00230000
*/* OUTPUT : NONE                                                    */ 00231000
*/*                                                                  */ 00232000
*/* SUBROUTINES CALLED : NONE                                        */ 00233000
*/*                                                                  */ 00234000
*/* MACROS USED : RELSTORE(2)                                        */ 00235000
*/*                                                                  */ 00236000
*/********************************************************************/ 00237000
*                                                                  0555 00238000
*FREEFSB:                                                          0555 00239000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0555 00240000
         B     @PB00002                                            0555 00241000
FREEFSB  DS    0H                                                  0556 00242000
*   RFY                                                            0556 00243000
*     WRKREG RSTD;                                                 0556 00244000
*   CRRRTN6=RTNREG;                 /* FDBS SAVE RETURN              */ 00245000
         ST    RTNREG,CRRRTN6(,CRRPTR)                             0557 00246000
*   RESPECIFY                                                      0558 00247000
*     ISTFDB BASED(WRKREG);         /* COVER FDB                     */ 00248000
*                                                                  0558 00249000
*   /*****************************************************************/ 00250000
*   /*                                                               */ 00251000
*   /* GET HE OFFSET OF THE FDB FROM THE BEGINNING OF THE FSB AND    */ 00252000
*   /* SUBTRACT IT . PLACE THE CHAIN PTR TO THE NEXT FDB IN THAT WORD*/ 00253000
*   /* SO THAT SMS WILL KNOW WHAT OTHER BLOCKS TO FREE.              */ 00254000
*   /*                                                               */ 00255000
*   /*****************************************************************/ 00256000
*                                                                  0559 00257000
*   WRKREG=ADDR(FSBFDB);            /* GET ADDR OF FDB               */ 00258000
         LA    WRKREG,FSBFDB(,FSBPTR)                              0559 00259000
*   FSBPTR=WRKREG-FDBOFSET;         /* ADD IN OFFSET                 */ 00260000
         LR    FSBPTR,WRKREG                                       0560 00261000
         SH    FSBPTR,FDBOFSET(,WRKREG)                            0560 00262000
*   FSBPTR->FDBFDBA=FDBFDBA;        /* POINT TO NEXT FDB             */ 00263000
         L     @15,FDBFDBA(,WRKREG)                                0561 00264000
         ST    @15,FDBFDBA(,FSBPTR)                                0561 00265000
*   RFY                                                            0562 00266000
*     ISTPROCD BASED(ADDR(FMCPROCD));                              0562 00267000
*                                                                  0562 00268000
*   /*****************************************************************/ 00269000
*   /*                                                               */ 00270000
*   /* IF DATA IS CONFIDENTIAL, CLEAR IT OUT BEFORE RELEASING IT     */ 00271000
*   /*                                                               */ 00272000
*   /*****************************************************************/ 00273000
*                                                                  0563 00274000
*   IF PROCFTX=ON                   /* IF CONFIDENTIAL               */ 00275000
*     THEN                                                         0563 00276000
*                                                                  0563 00277000
         L     @15,CRRFMCB(,CRRPTR)                                0563 00278000
         TM    PROCFTX+48(@15),B'00000001'                         0563 00279000
         BNO   @RF00563                                            0563 00280000
*     /***************************************************************/ 00281000
*     /*                                                             */ 00282000
*     /* RELSTORE BUFADDR(FSBPTR)CLEAR(YES)CHAIN(YES)                */ 00283000
*     /*                                                             */ 00284000
*     /***************************************************************/ 00285000
*                                                                  0564 00286000
*     DO;                                                          0564 00287000
*                                                                  0564 00288000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00289000
*/* END OF ATBUILD                                                   */ 00290000
*                                                                  0565 00291000
*       RFY                                                        0565 00292000
*        (GPR02P,                                                  0565 00293000
*         GPR03P,                                                  0565 00294000
*         GPR04P,                                                  0565 00295000
*         GPR15P) RSTD;                                            0565 00296000
*       RFY                                                        0566 00297000
*        (GPR01P,                                                  0566 00298000
*         GPR13P) RSTD;                                            0566 00299000
*       GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);         0567 00300000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00301000
*       RFY                                                        0568 00302000
*         GPR13P UNRSTD;                                           0568 00303000
*       GPR04P=FSBPTR|'80000000'X;                                 0569 00304000
         LR    GPR04P,FSBPTR                                       0569 00305000
         O     GPR04P,@CF01364                                     0569 00306000
*       DCL                                                        0570 00307000
*         1 FMZA0030 FIXED(31) STATIC LOCAL,                       0570 00308000
*          2 * FIXED(8) INIT(80),                                  0570 00309000
*          2 * PTR(24) INIT(0);                                    0570 00310000
*       GPR03F=FMZA0030;                                           0571 00311000
         L     GPR03F,FMZA0030                                     0571 00312000
*       DO;                         /* LISATCVT                    0572 00313000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00314000
*         GPR15P=ATCLCPTR;          /* SET UP ATCVT POINTER          */ 00315000
         L     GPR15P,ATCLCPTR                                     0573 00316000
*       END;                        /* LISATCVT                      */ 00317000
*       GPR02P=GPR15P->ATCBPDA;                                    0575 00318000
         L     GPR02P,ATCBPDA(,GPR15P)                             0575 00319000
*       GPR15P=GPR15P->ATCSMRS;                                    0576 00320000
         L     GPR15P,ATCSMRS(,GPR15P)                             0576 00321000
*       DCL                                                        0577 00322000
*         FMZL0030 BASED(GPR15P) LABEL VALUERANGE(*);              0577 00323000
*       CALL FMZL0030;                                             0578 00324000
         BALR  @14,@15                                             0578 00325000
*       RFY                                                        0579 00326000
*         GPR13P RSTD;                                             0579 00327000
*       GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);         0580 00328000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              00329000
*       RFY                                                        0581 00330000
*        (GPR01P,                                                  0581 00331000
*         GPR13P) UNRSTD;                                          0581 00332000
*       RFY                                                        0582 00333000
*        (GPR02P,                                                  0582 00334000
*         GPR03P,                                                  0582 00335000
*         GPR04P,                                                  0582 00336000
*         GPR15P) UNRSTD;                                          0582 00337000
*     END;                          /* *********END OF RELSTORE    0583 00338000
*                                      EXPANSION*********            */ 00339000
*                                                                  0583 00340000
*   /*****************************************************************/ 00341000
*   /*                                                               */ 00342000
*   /* DATA NOT CONFIDENTIAL ,NO NEED TO DO THE CLEAR.               */ 00343000
*   /*                                                               */ 00344000
*   /*****************************************************************/ 00345000
*                                                                  0584 00346000
*   ELSE                                                           0584 00347000
*                                                                  0584 00348000
*     /***************************************************************/ 00349000
*     /*                                                             */ 00350000
*     /* RELSTORE BUFADDR(FSBPTR)CHAIN(YES)                          */ 00351000
*     /*                                                             */ 00352000
*     /***************************************************************/ 00353000
*                                                                  0584 00354000
*     DO;                                                          0584 00355000
*                                                                  0584 00356000
         B     @RC00563                                            0584 00357000
@RF00563 DS    0H                                                  0585 00358000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00359000
*/* END OF ATBUILD                                                   */ 00360000
*                                                                  0585 00361000
*       RFY                                                        0585 00362000
*        (GPR02P,                                                  0585 00363000
*         GPR03P,                                                  0585 00364000
*         GPR04P,                                                  0585 00365000
*         GPR15P) RSTD;                                            0585 00366000
*       RFY                                                        0586 00367000
*        (GPR01P,                                                  0586 00368000
*         GPR13P) RSTD;                                            0586 00369000
*       GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);         0587 00370000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00371000
*       RFY                                                        0588 00372000
