         C     @15,@CF02698                                        0624 00501000
         BL    @RF00624                                            0624 00502000
*               DO;                                                0625 00503000
*                 CRRSW1=OFF;       /* TURN OFF CB ERR SW            */ 00504000
         NI    CRRSW1(CRRPTR),B'10111111'                          0626 00505000
*                 CALL UECBINIT;    /* CREATE & INIT UECB            */ 00506000
         BAL   @14,UECBINIT                                        0627 00507000
*               END;                /* END ATTN ACTIVE               */ 00508000
*             ELSE                                                 0629 00509000
*               ;                   /* ATTN NOT ACTIVE               */ 00510000
@RF00624 DS    0H                                                  0630 00511000
*           END;                    /* END XLIST ID OK               */ 00512000
