*       GPR04P=FSBPTR;                                             0589 00374000
         LR    GPR04P,FSBPTR                                       0589 00375000
*       DCL                                                        0590 00376000
*         1 FMZA0046 FIXED(31) STATIC LOCAL,                       0590 00377000
*          2 * FIXED(8) INIT(80),                                  0590 00378000
*          2 * PTR(24) INIT(0);                                    0590 00379000
*       GPR03F=FMZA0046;                                           0591 00380000
         L     GPR03F,FMZA0046                                     0591 00381000
*       DO;                         /* LISATCVT                    0592 00382000
