*       RFY                                                        0601 00399000
*        (GPR01P,                                                  0601 00400000
*         GPR13P) UNRSTD;                                          0601 00401000
*       RFY                                                        0602 00402000
*        (GPR02P,                                                  0602 00403000
*         GPR03P,                                                  0602 00404000
*         GPR04P,                                                  0602 00405000
*         GPR15P) UNRSTD;                                          0602 00406000
*     END;                          /* *********END OF RELSTORE    0603 00407000
*                                      EXPANSION*********            */ 00408000
*   RTNREG=CRRRTN6;                 /* RESTORE RETURN REG            */ 00409000
@RC00563 L     RTNREG,CRRRTN6(,CRRPTR)                             0604 00410000
*   RETURN;                                                        0605 00411000
@EL00002 DS    0H                                                  0605 00412000
@EF00002 DS    0H                                                  0605 00413000
@ER00002 BR    @14                                                 0605 00414000
*   RFY                                                            0606 00415000
*     WRKREG UNRSTD;                                               0606 00416000
*   END FREEFSB;                                                   0607 00417000
@PB00002 DS    0H                                                  0607 00418000
*   GEN NOSETS NOREFS(EJECT);                                      0608 00419000
*                                                                  0608 00420000
         EJECT                                                          00421000
*/********************************************************************/ 00422000
*/*                                                                  */ 00423000
*/* ROUTINE NAME : HDATTN - HANDLE ATTENTION EXITS                   */ 00424000
*/*                                                                  */ 00425000
*/* FUNCTION : IF THE USER SPECIFIED A RTN AT OPNDST TO HANDLE ATTNS,*/ 00426000
*/*      THEN SCHEDULE THAT RTN AND SET SWITCHES. OTHERWISE BUILD AN */ 00427000
*/*      LCCW TO SCHEDULE TPIOS TO UNLOCK THE TERMINAL SINCE IT CAN- */ 00428000
*/*      NOT HANDLE ATTENTIONS.                                      */ 00429000
*/*                                                                  */ 00430000
*/* INPUT : RPHPTR (REG 1) - PT TO THE RPH                           */ 00431000
*/*         DVTPTR (REG 11) - ZERO                                   */ 00432000
*/*                                                                  */ 00433000
*/* OUTPUT : DVTPTR (REG 11) - ZERO                                  */ 00434000
*/*                                                                  */ 00435000
*/* SUBROUTINES CALLED : LCPBINIT                                    */ 00436000
*/*                                                                  */ 00437000
