*   WRKREG1=WRKREG+COUNTER;         /* ADD CTR VALUE                 */ 02083000
C30A     LR    WRKREG1,WRKREG                                      1041 02084000
         ALR   WRKREG1,COUNTER                                     1041 02085000
*   CS(WRKREG,WRKREG1,FMCCMPS1);    /* SET NEW VALUE                 */ 02086000
