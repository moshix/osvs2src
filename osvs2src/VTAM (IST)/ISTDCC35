*/*                                                                  */ 00550000
*/* ROUTINE NAME : CKUNSD - CHECK FOR UNSOLICITED DATA               */ 00551000
*/*                                                                  */ 00552000
*/* FUNCTION : CHECKS TO SEE IF THE DEVICE IS DICONNECTED. IF NOT THE*/ 00553000
*/*      UNSEXT SUB ROUTINE IS CALLED TO HANDLE THE UNSOLICITED DATA */ 00554000
*/*      OTHERWISE THE TERMINAL HAS BEEN LOST, AND THIS ROUTINE CHECK*/ 00555000
*/*      S TO SEE IF THE USER SPECIFIED A LOST TERMINAL EXIT.        */ 00556000
*/*                                                                  */ 00557000
*/* INPUT : FSBPTR (REG 5) - PTR TO THE FSB                          */ 00558000
*/*                                                                  */ 00559000
*/* OUTPUT : CRRRTNCD MAY BE MODIFIED                                */ 00560000
*/*                                                                  */ 00561000
*/* SUBROUTINES CALLED : UNSEXT , UECBINIT , FREEFSB                 */ 00562000
*/*                                                                  */ 00563000
*/* MACROS USED : NONE                                               */ 00564000
*/*                                                                  */ 00565000
*/********************************************************************/ 00566000
*                                                                  0646 00567000
*CKUNSD:                                                           0646 00568000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0646 00569000
         B     @PB00004                                            0646 00570000
CKUNSD   DS    0H                                                  0647 00571000
*   CRRRTN1=RTNREG;                 /* SAVE THE RETURN ADDRESS       */ 00572000
*                                                                  0647 00573000
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0647 00574000
*   /*****************************************************************/ 00575000
*   /*                                                               */ 00576000
*   /* IF THE DEVICE IS DISCONNECTED, CHECK FOR A LOSTERM EXIT       */ 00577000
*   /*                                                               */ 00578000
*   /*****************************************************************/ 00579000
*                                                                  0648 00580000
*   IF FSBMJRCD=FRDISCNT THEN                                      0648 00581000
         CLI   FSBMJRCD(FSBPTR),X'28'                              0648 00582000
         BNE   @RF00648                                            0648 00583000
*     DO;                           /* FSB INDICATES DISCONNECT      */ 00584000
*       RESPECIFY                                                  0650 00585000
*         ISTACDEB BASED(WRKREG);   /* COVER ACDEB FROM              */ 00586000
*                                                                  0650 00587000
*       /*************************************************************/ 00588000
*       /*                                                           */ 00589000
*       /* GET THE ADDR OF THE ACB FROM THE ACDEB                    */ 00590000
*       /*                                                           */ 00591000
*       /*************************************************************/ 00592000
