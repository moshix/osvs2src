         BL    @RF00661                                            0661 00655000
*                   CALL UECBINIT;  /* GET & INIT UECB               */ 00656000
         BAL   @14,UECBINIT                                        0662 00657000
*                 ELSE                                             0663 00658000
*                   ;                                              0663 00659000
*                                                                  0663 00660000
@RF00661 DS    0H                                                  0664 00661000
*               /*****************************************************/ 00662000
*               /*                                                   */ 00663000
*               /* THE EXIT LIST HAS BEEN OVERLAID, SET ERROR RETURN */ 00664000
*               /* CODES                                             */ 00665000
*               /*                                                   */ 00666000
*               /*****************************************************/ 00667000
*                                                                  0664 00668000
*               ELSE                                               0664 00669000
*                 DO;               /* XLIST ID NOT OK               */ 00670000
         B     @RC00660                                            0664 00671000
@RF00660 DS    0H                                                  0665 00672000
*                   CRRRTNCD=CRRLOGIC;/* SET ERROR CODE              */ 00673000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0665 00674000
*                   CRRFDB2=CRRIVEXL;                              0666 00675000
         MVI   CRRFDB2(CRRPTR),X'23'                               0666 00676000
*                 END;              /* END XLIST NOT OK              */ 00677000
*             END;                  /* END XLIST ID OK               */ 00678000
*         END;                      /* END ACB ID OK                 */ 00679000
*                                                                  0669 00680000
*       /*************************************************************/ 00681000
*       /*                                                           */ 00682000
*       /* THE ACB HAS BEEN OVERLAID, SET ERROR RETURN CODES         */ 00683000
*       /*                                                           */ 00684000
*       /*************************************************************/ 00685000
*                                                                  0670 00686000
*       ELSE                                                       0670 00687000
*         DO;                       /* ACB ID NOT OK                 */ 00688000
         B     @RC00655                                            0670 00689000
@RF00655 DS    0H                                                  0671 00690000
*           CRRRTNCD=CRRLOGIC;      /* SET ERROR CODE                */ 00691000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0671 00692000
*           CRRFDB2=CRRIVACB;                                      0672 00693000
         MVI   CRRFDB2(CRRPTR),X'22'                               0672 00694000
*         END;                      /* END ACB ID NOT OK             */ 00695000
*                                                                  0673 00696000
*       /*************************************************************/ 00697000
*       /*                                                           */ 00698000
*       /* THE FSB HAS TO BE FREED, NO ONE NEEDS IT                  */ 00699000
*       /*                                                           */ 00700000
*       /*************************************************************/ 00701000
*                                                                  0674 00702000
*       CALL FREEFSB;               /* FREE THE FSB                  */ 00703000
@RC00655 BAL   @14,FREEFSB                                         0674 00704000
*     END;                          /* END IS DISCONN ON             */ 00705000
*                                                                  0675 00706000
*   /*****************************************************************/ 00707000
*   /*                                                               */ 00708000
*   /* THE DEVICE IS AVAILABLE. GO QUEUE THE DATA TO THE DATA QUEUE  */ 00709000
*   /*                                                               */ 00710000
*   /*****************************************************************/ 00711000
*                                                                  0676 00712000
*   ELSE                                                           0676 00713000
*     DO;                           /* NO DISCONN.           @D31KKNL*/ 00714000
         B     @RC00648                                            0676 00715000
@RF00648 DS    0H                                                  0677 00716000
*       FSBELI=ON;                  /* END OF LCP            @D31KKNL*/ 00717000
*       FSBECI=OFF;                 /* END OF COMM'CATION    @D31KKNL*/ 00718000
         OI    FSBELI(FSBPTR),B'10000000'                          0678 00719000
         NI    FSBECI(FSBPTR),B'10111111'                          0678 00720000
*       CALL XFRFSB;                /* QUE UP DATA           @D31KKNL*/ 00721000
         BAL   @14,XFRFSB                                          0679 00722000
*       IF FMCFLGA1=OFF THEN                                       0680 00723000
         L     @15,CRRFMCB(,CRRPTR)                                0680 00724000
         TM    FMCFLGA1(@15),B'01000000'                           0680 00725000
         BNZ   @RF00680                                            0680 00726000
*         DO;                       /* FMCB IN CA MODE?      @D33SKCF*/ 00727000
*                                                                  0681 00728000
*           /*********************************************************/ 00729000
*           /*                                                       */ 00730000
*           /* SET THE FLAGS IN THE FMCB SO ONLY THE SYNCHRONOUS DATA*/ 00731000
*           /* QUEUE IS CHECKED FOR DATA AND CALL RCC63 TO PUT THE   */ 00732000
*           /* FMCB ON THE ACDEB READ ANY QUEUE IF NECESSARY.        */ 00733000
*           /*                                               @D33SKCF*/ 00734000
*           /*                                                       */ 00735000
*           /*********************************************************/ 00736000
*                                                                  0682 00737000
*           CRRDFSYN=ON;            /* SYNCHRONOUS DATA      @D33SKCF*/ 00738000
*           CRRDFASY=OFF;           /* NOT ASYNCHRONOUS      @D33SKCF*/ 00739000
*           CRRRESP=OFF;            /* NOT RESPONSE          @D33SKCF*/ 00740000
         OI    CRRDFSYN(CRRPTR),B'00000010'                        0684 00741000
         NI    CRRDFASY(CRRPTR),B'11110011'                        0684 00742000
*           CALL HSKPANYQ;          /* CALL ISTRCC63         @D33SKCF*/ 00743000
         L     @04,CRRATCVT(,CRRPTR)                               0685 00744000
         L     @15,ATCRCC63(,@04)                                  0685 00745000
         BALR  @14,@15                                             0685 00746000
*         END;                      /* END CONT ANY MODE     @D33SKCF*/ 00747000
*       IF FSBMNELI=ON THEN         /* ERROR LOCK            @D31KKNL*/ 00748000
@RF00680 TM    FSBMNELI(FSBPTR),B'10000000'                        0687 00749000
         BNO   @RF00687                                            0687 00750000
*         CALL LCPBINIT;            /* RESET IT              @D31KKNL*/ 00751000
         BAL   @14,LCPBINIT                                        0688 00752000
*       ELSE                                                       0689 00753000
*         ;                         /*                       @D31KKNL*/ 00754000
@RF00687 DS    0H                                                  0690 00755000
*     END;                          /* END-ON DISC           @D31KKNL*/ 00756000
*   RTNREG=CRRRTN1;                 /* RESTORE THE RETURN REG        */ 00757000
