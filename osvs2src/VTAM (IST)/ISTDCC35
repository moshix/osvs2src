*                                                                  0654 00611000
         OI    CRRSW1(CRRPTR),B'01000000'                          0654 00612000
*       /*************************************************************/ 00613000
*       /*                                                           */ 00614000
*       /* IF THE ACB HAS NOT BEEN OVERLAID, GET THE EXLST ADDR FROM */ 00615000
*       /* IT.                                                       */ 00616000
*       /*                                                           */ 00617000
*       /*************************************************************/ 00618000
*                                                                  0655 00619000
*       IF ACBID='A0'X THEN                                        0655 00620000
         CLI   ACBID(WRKREG1),160                                  0655 00621000
         BNE   @RF00655                                            0655 00622000
*         DO;                       /* CHECK THE ACB ID              */ 00623000
