@EL00005 DS    0H                                                  0685 00773000
@EF00005 DS    0H                                                  0685 00774000
@ER00005 BR    @14                                                 0685 00775000
*/********************************************************************/ 00776000
*/*                                                                  */ 00777000
*/* ROUTINE NAME : MOVEFDBK - MOVE ROUTINE FEEDBACK                  */ 00778000
*/*                                                                  */ 00779000
*/* FUNCTION : ANALYZE THE RESULTS OF THE MOVE ROUTINE. THE MOVE     */ 00780000
*/* ROUTINE WILL RETURN A CODE OF 8, 4, OR 0 TO INDICATE WHAT        */ 00781000
*/* HAPPENED.  A CODE OF 8 MEANS A LOGIC ERROR SO THAT NO DATA       */ 00782000
*/* WAS MOVED.  THE ROUTINE SET THE PROPER ERROR CODES IN THE        */ 00783000
*/* CRR.  LDO PROCESSING IS FINISHED AND THE FSB WILL BE SAVED       */ 00784000
*/* FOR A LATER READ.  A RETURN CODE OF 4 MEANS THAT PART OF THE     */ 00785000
*/* DATA WAS MOVED, AND NOW THE USER'S BUFFER IS FULL.  THE          */ 00786000
*/* TRUNC/KEEP OPTION WILL DETERMINE HOW THE REMAINING DATA WILL     */ 00787000
*/* BE TREATED.  A CODE OF 0 INDICATES THAT ALL THE DATA             */ 00788000
*/* CORRESPONDING TO THE CURRENT FSB WAS MOVED.  WHETHER AN EOB,     */ 00789000
*/* EOM, OR EOT WAS RECEIVED DETERMINES WHETHER THE REQUEST IS       */ 00790000
*/* COMPLETE.                                                        */ 00791000
*/*                                                                  */ 00792000
*/* INPUT : CRRPTR (REG 13) - PTR TO THE CRR                         */ 00793000
*/*                                                                  */ 00794000
*/* OUTPUT : CRRPOST AND CRRSW1 POSSIBLY SET                         */ 00795000
*/*                                                                  */ 00796000
*/* SUBROUTINES CALLED : TRUNPROC , KEEPPROC                         */ 00797000
*/*                                                                  */ 00798000
*/* MACROS USED : NONE                                               */ 00799000
*/*                                                                  */ 00800000
*/********************************************************************/ 00801000
*MOVEFDBK: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0686 00802000
*                                                                  0686 00803000
MOVEFDBK DS    0H                                                  0687 00804000
*   CRRRTN4 = RETPTR;             /* SAVE RETURN ADDRESS             */ 00805000
         ST    RETPTR,CRRRTN4(,CRRPTR)                             0687 00806000
*   CRRCLDO = CRRBLDAD;           /* SAVE LAST LDO PROCESSED         */ 00807000
         L     @15,CRRBLDAD(,CRRPTR)                               0688 00808000
         ST    @15,CRRCLDO(,CRRPTR)                                0688 00809000
*   IF CRRBRTCD = CRRBEROR THEN                                    0689 00810000
         CLI   CRRBRTCD(CRRPTR),8                                  0689 00811000
         BNE   @RF00689                                            0689 00812000
*         DO;                     /* LOG ERROR-NO DATA WAS MOVED     */ 00813000
*                                                                  0690 00814000
*         /********************************************/           0691 00815000
*         /*        ERROR-NO DATA MOVED               */           0691 00816000
*         /********************************************/           0691 00817000
*                                                                  0691 00818000
*         CRRPOST=ON;             /* RPL SHOULD BE POSTED COMPLETE   */ 00819000
         OI    CRRPOST(CRRPTR),B'00010000'                         0691 00820000
*         CRRSW1=ON;              /* FSB IS TO BE SAVED              */ 00821000
         OI    CRRSW1(CRRPTR),B'01000000'                          0692 00822000
*         END;                                                     0693 00823000
*   ELSE IF PROTRUNC=ON THEN                                       0694 00824000
*                                                                  0694 00825000
         B     @RC00689                                            0694 00826000
@RF00689 L     @15,CRRFMCB(,CRRPTR)                                0694 00827000
         TM    PROTRUNC+48(@15),B'01000000'                        0694 00828000
         BNO   @RF00694                                            0694 00829000
*              /*****************************************/         0695 00830000
*              /*  MOVE SUCCESSFUL-CHECK TRUNCATION     */         0695 00831000
*              /*****************************************/         0695 00832000
*                                                                  0695 00833000
*              CALL TRUNPROC;     /* CODE 0 OR 4 - CHECK TRUNC OPTION*/ 00834000
*                                                                  0695 00835000
         BAL   @14,TRUNPROC                                        0695 00836000
*             /******************************************/         0696 00837000
*             /*      MOVE SUCCESSFUL-CHECK KEEP        */         0696 00838000
*             /******************************************/         0696 00839000
*                                                                  0696 00840000
*        ELSE CALL KEEPPROC;      /* CODE 0 OR 4 - CHECK KEEP OPTION */ 00841000
         B     @RC00694                                            0696 00842000
@RF00694 BAL   @14,KEEPPROC                                        0696 00843000
*   RPLAAREA = CRRCLDO;           /* SET LAST LDO IN RPL FOR FEEDBACK*/ 00844000
*                                                                  0697 00845000
@RC00694 DS    0H                                                  0697 00846000
@RC00689 L     @15,CRRARPL(,CRRPTR)                                0697 00847000
         L     @08,CRRCLDO(,CRRPTR)                                0697 00848000
         ST    @08,RPLAAREA(,@15)                                  0697 00849000
*   RETPTR = CRRRTN4;             /* GET RETURN ADDRESS              */ 00850000
         L     RETPTR,CRRRTN4(,CRRPTR)                             0698 00851000
*END MOVEFDBK;                    /* RETURN TO CALLER                */ 00852000
@EL00006 DS    0H                                                  0699 00853000
@EF00006 DS    0H                                                  0699 00854000
@ER00006 BR    @14                                                 0699 00855000
*/********************************************************************/ 00856000
*/*                                                                  */ 00857000
*/* ROUTINE NAME : FINDLDO - FIND THE CURRENT LDO                    */ 00858000
*/*                                                                  */ 00859000
*/* FUNCTION : LOCATE THE LDO ADDRESS SAVED WHEN THE LCCW WAS BUILT. */ 00860000
*/* IF A SINGLE READ LDO WAS ISSUED, THE LDO ADDRESS WAS SAVED       */ 00861000
*/* IN THE LCPB.  OTHERWISE, IT IS IN THE LCCW EXTENSION AREA.       */ 00862000
*/* AFTER THE LDO ADDRESS IS FOUND.                                  */ 00863000
*/*                                                                  */ 00864000
*/* INPUT : LCPBPTR (REG 7) - PTR TO THE LCPB                        */ 00865000
*/*                                                                  */ 00866000
*/* OUTPUT : CRRCLDO - PT RTO THE NEXT LDO                           */ 00867000
*/*          RPLAAREA - PTR TO THE LAST LDO EXECUTED                 */ 00868000
*/*                                                                  */ 00869000
*/* SUBROUTINES CALLED : NONE                                        */ 00870000
*/*                                                                  */ 00871000
*/* MACROS USED : NONE                                               */ 00872000
*/*                                                                  */ 00873000
*/********************************************************************/ 00874000
*FINDLDO: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0700 00875000
*                                                                  0700 00876000
FINDLDO  DS    0H                                                  0701 00877000
*   CRRRTN3 = RETPTR;             /* SAVE RETURN ADDRESS             */ 00878000
         ST    RETPTR,CRRRTN3(,CRRPTR)                             0701 00879000
* /* DCR 1991 IMPLEMENTED ( 10/2/73 ) */                           0702 00880000
*                                                                  0702 00881000
*   IF EVAL(ADDR(LCPLCCW))->LCCWOP=LTIC THEN                       0702 00882000
*                                                                  0702 00883000
         LA    @15,LCPLCCW(,LCPBPTR)                               0702 00884000
         CLI   LCCWOP(@15),X'08'                                   0702 00885000
         BNE   @RF00702                                            0702 00886000
*         /************************************/                   0703 00887000
*         /*  LDO ADDRESS IS LOCATED IN THE   */                   0703 00888000
*         /*  EXTENSION AREA BECAUSE MORE     */                   0703 00889000
*         /*  THAN ONE REQUEST WAS ISSUED.    */                   0703 00890000
*         /*  FSBLCCWA POINTS TO THE LAST     */                   0703 00891000
*         /*  PROCESSED LCCW WHICH IS FOR     */                   0703 00892000
*         /*  THE READ.  PICK UP LDO ADDRESS  */                   0703 00893000
*         /*  AT SPECIFIC OFFSET FROM LCCW.   */                   0703 00894000
*         /************************************/                   0703 00895000
*                                                                  0703 00896000
*         LDOPTR=(FSBLCCWA+LDOFFSET)->ADDRLDO; /* GET LDO ADDR       */ 00897000
*                                                                  0703 00898000
         L     @15,FSBLCCWA(,FSBPTR)                               0703 00899000
         L     LDOPTR,ADDRLDO+32(,@15)                             0703 00900000
*        /**********************************/                      0704 00901000
*        /*  ONLY ONE REQUEST.  LDO ADDRESS*/                      0704 00902000
*        /*  WAS SAVED IN THE DCLCPB.      */                      0704 00903000
*        /**********************************/                      0704 00904000
*                                                                  0704 00905000
*   ELSE LDOPTR=DCLLDO1;          /* GET LDO FROM LCPB               */ 00906000
         B     @RC00702                                            0704 00907000
@RF00702 L     LDOPTR,DCLLDO1(,LCPBPTR)                            0704 00908000
*   CRRCLDO=LDOPTR;               /* SAVE LDO IN CRR                 */ 00909000
@RC00702 ST    LDOPTR,CRRCLDO(,CRRPTR)                             0705 00910000
*   RPLAAREA=LDOPTR;              /* SAVE IN RPL AS FEEDBACK         */ 00911000
         L     @15,CRRARPL(,CRRPTR)                                0706 00912000
         ST    LDOPTR,RPLAAREA(,@15)                               0706 00913000
* /* LINE 150270 CHANGED FOR ZTR 5332 ( 10/17/73 ) */              0707 00914000
*                                                                  0707 00915000
*   RETPTR = CRRRTN3;             /* GET RETURN ADDRESS              */ 00916000
         L     RETPTR,CRRRTN3(,CRRPTR)                             0707 00917000
*END FINDLDO;                     /* RETURN TO LDOPROC               */ 00918000
@EL00007 DS    0H                                                  0708 00919000
@EF00007 DS    0H                                                  0708 00920000
@ER00007 BR    @14                                                 0708 00921000
*/********************************************************************/ 00922000
*/*                                                                  */ 00923000
*/* ROUTINE NAME : FREEFSB - FREE UP THE FSB                         */ 00924000
*/*                                                                  */ 00925000
*/* FUNCTION : RELEASE THE FSB WHICH HAS BEEN PROCESSED              */ 00926000
*/* BY THE MOVE ROUTINE.  THE FSB IS FOLLOWED BY A FDB WHICH         */ 00927000
*/* CONTAINS A POINTER TO THE DATA AND POSSIBLY A POINTER TO         */ 00928000
*/* ANOTHER FDB.  ALL THE FDB'S AND THE FSB MUST BE RELEASED.        */ 00929000
*/* A NEGATIVE OFFSET MUST BE SUBTRACTED FROM THE FDB TO GET THE     */ 00930000
*/* AMOUNT TO RELEASE.  FOR THE FIRST FDB, THE NEGATIVE OFFSET       */ 00931000
*/* WILL INCLUDE THE FSB.                                            */ 00932000
*/*                                                                  */ 00933000
*/* INPUT : FSBPTR (REG 6) - PTR TO THE FSB                          */ 00934000
*/*         FDBPTR (REG 5) - PTR TO THE FDB                          */ 00935000
*/*                                                                  */ 00936000
*/* OUTPUT : FSB FREED                                               */ 00937000
*/*                                                                  */ 00938000
*/* SUBROUTINES CALLED : NONE                                        */ 00939000
*/*                                                                  */ 00940000
*/* MACROS USED : RELSTORE(2)                                        */ 00941000
*/*                                                                  */ 00942000
*/********************************************************************/ 00943000
*FREEFSB: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0709 00944000
FREEFSB  DS    0H                                                  0710 00945000
*RFY WORKA RSTD;                                                   0710 00946000
*                                                                  0710 00947000
*   CRRRTN3 = RETPTR;             /* SAVE RETURN ADDRESS             */ 00948000
