*(C40LOCK)) *//*@Z30APGE*/                                         0678 00759000
* CRRPTR=GPR10F;/*RESTORE REG 13 @Z30APGE*/                        0679 00760000
         LR    CRRPTR,GPR10F                                       0679 00761000
* LM (GPR10F,GPR14F,CRRSAVE);/*RESTORE REGS @Z30APGE*/             0680 00762000
         LM    GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0680 00763000
* RFY (GPR10F,GPR14F) UNRSTD;/*@Z30APGE*/                          0681 00764000
*                                                                  0681 00765000
*   CALL MOVEFDBK;                /* ANALYZE RESULTS OF MOVE         */ 00766000
*                                                                  0682 00767000
         BAL   @14,MOVEFDBK                                        0682 00768000
*   RETPTR = CRRRTN3;             /* GET RETURN ADDRESS              */ 00769000
         L     RETPTR,CRRRTN3(,CRRPTR)                             0683 00770000
*RFY ATCVTPTR UNRSTD;                                              0684 00771000
