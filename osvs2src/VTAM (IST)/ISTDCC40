         LR    WORKA,FDBPTR                                        0714 00960000
         SH    WORKA,FDBOFSET(,FDBPTR)                             0714 00961000
*   WORKA->FDBFDBA=FDBFDBA;        /* SET UP CHAIN FIELD             */ 00962000
         L     @15,FDBFDBA(,FDBPTR)                                0715 00963000
         ST    @15,FDBFDBA(,WORKA)                                 0715 00964000
*   IF PROCFTX=ON THEN   /*RELSTORE BUFADDR(WORKA)CLEAR(YES)CHAIN(YES)* 00965000
*                                                                  0716 00966000
         L     @15,CRRFMCB(,CRRPTR)                                0716 00967000
         TM    PROCFTX+48(@15),B'00000001'                         0716 00968000
         BNO   @RF00716                                            0716 00969000
*                         DO;                                      0717 00970000
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  0718 00971000
*/* END OF ATBUILD */    RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;   0718 00972000
*         RFY (GPR01P,GPR13P) RSTD;                                0719 00973000
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       0720 00974000
*                                                                  0720 00975000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00976000
*         RFY GPR13P UNRSTD;                                       0721 00977000
*         GPR04P=WORKA | '80000000'X;                              0722 00978000
         LR    @08,WORKA                                           0722 00979000
         O     @08,@CF02699                                        0722 00980000
         LR    GPR04P,@08                                          0722 00981000
*         DCL 1 FMZA0033 FIXED(31) STATIC LOCAL,                   0723 00982000
*             2 * FIXED(8) INIT(80),                               0723 00983000
*             2 * PTR(24) INIT(0);                                 0723 00984000
*         GPR03F = FMZA0033;                                       0724 00985000
*                                                                  0724 00986000
         L     GPR03F,FMZA0033                                     0724 00987000
*                        DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */  00988000
*                          GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/  00989000
         L     GPR15P,ATCLCPTR                                     0726 00990000
*                        END; /*LISATCVT*/                         0727 00991000
*         GPR02P = GPR15P->ATCBPDA;                                0728 00992000
         L     GPR02P,ATCBPDA(,GPR15P)                             0728 00993000
*         GPR15P=GPR15P->ATCSMRS;                                  0729 00994000
         L     GPR15P,ATCSMRS(,GPR15P)                             0729 00995000
*         DCL FMZL0033 BASED(GPR15P) LABEL VALUERANGE(*);          0730 00996000
*         CALL FMZL0033;                                           0731 00997000
         BALR  @14,@15                                             0731 00998000
*         RFY GPR13P RSTD;                                         0732 00999000
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       0733 01000000
*                                                                  0733 01001000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              01002000
*         RFY (GPR01P,GPR13P) UNRSTD;                              0734 01003000
*                        RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD; 0735 01004000
*                        END ;/**********END OF RELSTORE EXPANSION***** 01005000
*****/                                                             0736 01006000
*   ELSE   /*RELSTORE BUFADDR(WORKA)CHAIN(YES)*/                   0737 01007000
*           DO;                                                    0737 01008000
         B     @RC00716                                            0737 01009000
@RF00716 DS    0H                                                  0738 01010000
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  0738 01011000
*/* END OF ATBUILD */                                              0738 01012000
*          RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;                 0738 01013000
*         RFY (GPR01P,GPR13P) RSTD;                                0739 01014000
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       0740 01015000
*                                                                  0740 01016000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             01017000
*         RFY GPR13P UNRSTD;                                       0741 01018000
*         GPR04P=WORKA;                                            0742 01019000
*         DCL 1 FMZA0049 FIXED(31) STATIC LOCAL,                   0743 01020000
*             2 * FIXED(8) INIT(80),                               0743 01021000
*             2 * PTR(24) INIT(0);                                 0743 01022000
