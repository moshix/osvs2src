@EL00008 DS    0H                                                  0759 01050000
@EF00008 DS    0H                                                  0759 01051000
@ER00008 BR    @14                                                 0759 01052000
*/********************************************************************/ 01053000
*/*                                                                  */ 01054000
*/* ROUTINE NAME : TRUNPROC - TRUNCATE PROCESSING                    */ 01055000
*/*                                                                  */ 01056000
*/* FUNCTION : ENTERED WITH A RETURN CODE OF 0 OR 4 FROM THE         */ 01057000
*/* MOVE ROUTINE AND WITH THE TRUNC OPTION SPECIFIED IN THE FMCB.    */ 01058000
*/* IF THE FSB JUST PROCESSED CONTAINED AN ENDING CONDITION (EOB)    */ 01059000
*/* THEN MOVE PROCESSING IS COMPLETED. IF THE FSB IS A SUBBLOCK,     */ 01060000
*/* MORE DATA IS EXPECTED.IF THE BUFFER IS FILLED AND THERE ARE NO   */ 01061000
*/* MORE LDO'S CHAINED TO THE CURRENT ONE,THE FLUSH BIT IS SET TO    */ 01062000
*/* THROW AWAY INCOMING FSB'S.IF THERE IS MORE ROOM, THE INFO        */ 01063000
*/* NEEDED BY THE MOVE ROUTINE IS SAVED IN THE LCPB SO PROCESSING    */ 01064000
*/* CAN RESUME WHEN THE NEXT FSB ARRIVES.  IN ALL CASES, THE         */ 01065000
*/* FSB IS RELEASED.                                                 */ 01066000
*/*                                                                  */ 01067000
*/* INPUT : FSBPTR (REG 6) - PTR TO THE FSB                          */ 01068000
*/*         LDOPTR (REG 9) - PTR TO THE LDO                          */ 01069000
*/*                                                                  */ 01070000
*/* OUTPUT : CRRPOST POSSIBLY SET                                    */ 01071000
*/*                                                                  */ 01072000
*/* SUBROUTINES CALLED : NEXTLDO                                     */ 01073000
*/*                                                                  */ 01074000
*/* MACROS USED : NONE                                               */ 01075000
*/*                                                                  */ 01076000
*/********************************************************************/ 01077000
*TRUNPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0760 01078000
*                                                                  0760 01079000
TRUNPROC DS    0H                                                  0761 01080000
*   CRRRTN5 = RETPTR;             /* SAVE RETURN ADDRESS             */ 01081000
         ST    RETPTR,CRRRTN5(,CRRPTR)                             0761 01082000
*   IF FSBEBI=OFF & FSBLGI=OFF & FSBSOHI=OFF & FSBMJRCD=FRNORM THEN     01083000
         TM    FSBEBI(FSBPTR),B'00100011'                          0762 01084000
         BNZ   @RF00762                                            0762 01085000
         CLI   FSBMJRCD(FSBPTR),X'00'                              0762 01086000
         BNE   @RF00762                                            0762 01087000
*           DO;                   /* CURRENT FSB IS A SUBBLOCK       */ 01088000
*                                                                  0763 01089000
*           /********************************************/         0764 01090000
*           /*  FSB JUST PROCESSES IS A SUBBLOCK        */         0764 01091000
*           /********************************************/         0764 01092000
*                                                                  0764 01093000
*           DCLBTLEN=CRRBTLEN;    /* SAVE TOTAL LENGTH MOVED         */ 01094000
         L     @15,CRRBTLEN(,CRRPTR)                               0764 01095000
         ST    @15,DCLBTLEN(,LCPBPTR)                              0764 01096000
*           IF CRRBLEN=ZERO THEN   /* NO MORE ROOM IN BUFFER */    0765 01097000
         LH    @15,CRRBLEN(,CRRPTR)                                0765 01098000
         LTR   @15,@15                                             0765 01099000
         BNZ   @RF00765                                            0765 01100000
*              IF LDOACHN=ON THEN  /* LDO IS DATA CHAINED */       0766 01101000
         TM    LDOACHN(LDOPTR),B'10000000'                         0766 01102000
         BNO   @RF00766                                            0766 01103000
*                  IF CRRERLK=ON THEN        /* ERROR LOCK IS SET    */ 01104000
*                                                                  0767 01105000
         TM    CRRERLK(CRRPTR),B'10000000'                         0767 01106000
         BNO   @RF00767                                            0767 01107000
*                      /**************************************/    0768 01108000
*                      /*   PROCESSING COMPLETED.  ERROR     */    0768 01109000
*                      /*   LOCK SET IN FSB JUST PROCESSED   */    0768 01110000
*                      /**************************************/    0768 01111000
*                                                                  0768 01112000
*                      CRRPOST=ON;           /* FINISH PROCESSING    */ 01113000
         OI    CRRPOST(CRRPTR),B'00010000'                         0768 01114000
*                  ELSE                                            0769 01115000
*                                                                  0769 01116000
*                   /*****************************************/    0769 01117000
*                   /*   FSB JUST PROCESSED IS A SUBBLOCK,   */    0769 01118000
*                   /*   THERE IS NO MORE ROOM IN THE CURRENT*/    0769 01119000
*                   /*   BUFFER BUT THE LDO IS DATA CHAINED  */    0769 01120000
*                   /*****************************************/    0769 01121000
*                                                                  0769 01122000
*                   CALL NEXTLDO; /* MORE ROOM-GET NEXT DATA CHND LDO*/ 01123000
         B     @RC00767                                            0769 01124000
@RF00767 BAL   @14,NEXTLDO                                         0769 01125000
*              ELSE DO;           /* NO MORE ROOM IN BUFFER          */ 01126000
*                                                                  0770 01127000
         B     @RC00766                                            0770 01128000
@RF00766 DS    0H                                                  0771 01129000
*                   /******************************************/   0771 01130000
*                   /*  FSB JUST PROCESSED IS A SUBBLOCK,     */   0771 01131000
*                   /*  BUT THERE IS NO ROOM IN THE USER'S    */   0771 01132000
*                   /*  BUFFER.  INFORM USER HIS DATA HAS     */   0771 01133000
*                   /*  BEEN TRUNCATED AND SET SWITCH ON TO   */   0771 01134000
*                   /*  INDICATE THE REST OF THE SUBBLOCKS    */   0771 01135000
*                   /*  SHOULD BE THROWN AWAY.                */   0771 01136000
*                   /******************************************/   0771 01137000
*                                                                  0771 01138000
*                   CRRPOST=ON;   /* INDICATE RPL SHOULD BE POSTED   */ 01139000
         OI    CRRPOST(CRRPTR),B'00010000'                         0771 01140000
*                   RFY (REG10,REG11) RSTD;                        0772 01141000
*             CS4:  REG10=FMCCMPS2;          /* GET OLD FLAG VALUE   */ 01142000
CS4      L     @15,CRRFMCB(,CRRPTR)                                0773 01143000
         L     REG10,FMCCMPS2(,@15)                                0773 01144000
*                   REG11=REG10|FLGA3ON;     /* SET NEW FLAG VALUE   */ 01145000
         LR    REG11,REG10                                         0774 01146000
         O     REG11,@CF02705                                      0774 01147000
*                   CS (REG10,REG11,FMCCMPS2); /* SET IN FMCB        */ 01148000
         CS    REG10,@11,FMCCMPS2(@15)                             0775 01149000
*                   BC (7,CS4);              /* TRY AGAIN IF FAILURE */ 01150000
         BC    7,CS4                                               0776 01151000
*                   RFY (REG10,REG11) UNRSTD;                      0777 01152000
*                   END;                                           0778 01153000
*           ELSE                  /* MORE ROOM IN CURRENT BUFFER     */ 01154000
*              IF CRRERLK=ON THEN            /* SEE IF ERROR LOCK SET*/ 01155000
*                                                                  0779 01156000
         B     @RC00765                                            0779 01157000
@RF00765 TM    CRRERLK(CRRPTR),B'10000000'                         0779 01158000
         BNO   @RF00779                                            0779 01159000
*                     /*****************************************/  0780 01160000
*                     /*  FSB JUST PROCESSED IS A SUBBLOCK AND */  0780 01161000
*                     /*  THERE IS ROOM IN THE BUFFER BUT THE  */  0780 01162000
*                     /*  ERROR LOCK IS SET IN THE FSB JUST    */  0780 01163000
*                     /*  PROCESSED.  POST USER COMPLETE SO    */  0780 01164000
*                     /*  HE KNOWS THAT AN ERROR OCCURRED.     */  0780 01165000
*                     /*****************************************/  0780 01166000
*                                                                  0780 01167000
*                     CRRPOST=ON;            /* FINISHED PROCESSING  */ 01168000
         OI    CRRPOST(CRRPTR),B'00010000'                         0780 01169000
*              ELSE DO;                      /* ERROR LOCK NOT SET   */ 01170000
*                                                                  0781 01171000
         B     @RC00779                                            0781 01172000
@RF00779 DS    0H                                                  0782 01173000
*                /********************************************/    0782 01174000
*                /*  FSB JUST PROCESSED IS A SUBBLOCK AND    */    0782 01175000
*                /*  AND THERE IS ROOM IN THE BUFFER FOR     */    0782 01176000
*                /*  MORE DATA.  SAVE STATUS IN DCLCP SO     */    0782 01177000
*                /*  THAT THE MOVE CAN BE RESUMED WHEN THE   */    0782 01178000
*                /*  NEXT SUBBLOCK ARRIVES.                  */    0782 01179000
*                /********************************************/    0782 01180000
*                                                                  0782 01181000
*                DCLCLDOA=CRRBLDAD; /* SAVE ADDR OF LAST LDO PROCESS */ 01182000
         L     @15,CRRBLDAD(,CRRPTR)                               0782 01183000
         ST    @15,DCLCLDOA(,LCPBPTR)                              0782 01184000
*                DCLIOLEN=CRRBLEN;  /* SAVE RESIDUAL COUNT IN LCPB   */ 01185000
         LH    @08,CRRBLEN(,CRRPTR)                                0783 01186000
         STH   @08,DCLIOLEN(,LCPBPTR)                              0783 01187000
*                DCLIOBUF=CRRBARAD; /* SAVE RESUME BUFFER ADDRESS    */ 01188000
         L     @08,CRRBARAD(,CRRPTR)                               0784 01189000
         ST    @08,DCLIOBUF(,LCPBPTR)                              0784 01190000
*                DCLRPLA=CRRARPL;   /* SAVE POINTER TO THE RPL       */ 01191000
         L     @08,CRRARPL(,CRRPTR)                                0785 01192000
         ST    @08,DCLRPLA(,LCPBPTR)                               0785 01193000
*                IF ADDR(LCPLCCW)->LCCWOP^=LTIC THEN               0786 01194000
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0786 01195000
         BE    @RF00786                                            0786 01196000
*                   DCLLDO1=CRRBLDAD;        /* SAVE LDO IN LCPB     */ 01197000
         ST    @15,DCLLDO1(,LCPBPTR)                               0787 01198000
*                ELSE                        /* SAVE LDO IN EXTENSION*/ 01199000
*                   EVAL(CRRCLCCW+LDOFFSET)->ADDRLDO=CRRBLDAD;     0788 01200000
         B     @RC00786                                            0788 01201000
@RF00786 LA    @15,32                                              0788 01202000
         AL    @15,CRRCLCCW(,CRRPTR)                               0788 01203000
         L     @08,CRRBLDAD(,CRRPTR)                               0788 01204000
         ST    @08,ADDRLDO(,@15)                                   0788 01205000
*                END;                                              0789 01206000
*           END;                                                   0790 01207000
*   ELSE DO;                      /* ENDING CONDITION MET            */ 01208000
*                                                                  0791 01209000
         B     @RC00762                                            0791 01210000
@RF00762 DS    0H                                                  0792 01211000
*        /*********************************************/           0792 01212000
*        /*     CURRENT FSB IS NOT A SUBBLOCK         */           0792 01213000
*        /*********************************************/           0792 01214000
*                                                                  0792 01215000
*        DCLBTLEN=DCLBTLEN&&DCLBTLEN;     /* CLEAR LENGTH ACCUMULATOR * 01216000
         L     @15,DCLBTLEN(,LCPBPTR)                              0792 01217000
         LR    @08,@15                                             0792 01218000
         XR    @08,@15                                             0792 01219000
         ST    @08,DCLBTLEN(,LCPBPTR)                              0792 01220000
*        CRRPOST=ON;              /* RPL SHOULD BE POSTED COMPLETE   */ 01221000
         OI    CRRPOST(CRRPTR),B'00010000'                         0793 01222000
*        END;                                                      0794 01223000
*                                                                  0794 01224000
*   RETPTR = CRRRTN5;             /* GET RETURN ADDRESS              */ 01225000
@RC00762 L     RETPTR,CRRRTN5(,CRRPTR)                             0795 01226000
*END TRUNPROC;                     /* RETURN TO CALLER                * 01227000
@EL00009 DS    0H                                                  0796 01228000
@EF00009 DS    0H                                                  0796 01229000
@ER00009 BR    @14                                                 0796 01230000
*/********************************************************************/ 01231000
*/*                                                                  */ 01232000
*/* ROUTINE NAME : KEEPPROC - KEEP DATA PROCESSING                   */ 01233000
*/*                                                                  */ 01234000
*/* FUNCTION : ENTERED WITH A RETURN CODE OF 0 OR 4 FROM THE         */ 01235000
*/* MOVE ROUTINE AND WITH THE KEEP OPTION SPECIFIED IN THE FMCB      */ 01236000
*/* IF THE RETURN CODE IS 0 INDICATING THAT ALL THE DATA FOR THE     */ 01237000
*/* CURRENT FSB WAS MOVED AND THE CURRENT FSB IS NOT A SUBBLOCK,     */ 01238000
*/* THEN FEEDBACK PROCESSING IS FINISHED.  IF IT IS A SUBBLOCK AND   */ 01239000
*/* THERE IS MORE ROOM, THE THE INFORMATION NEEDED TO RESUME THE     */ 01240000
*/* MOVE IS SAVED IN THE LCPB UNTIL THE NEXT FSB ARRIVES. IF THERE   */ 01241000
*/* IS NO MORE ROOM OR THE RETURN CODE WAS 4 INDICATING THE BUFFER   */ 01242000
*/* WAS FILLED BEFORE ALL THE DATA WAS MOVED, THE PARTIAL DATA       */ 01243000
*/* INDICATOR IS SET FOR FEEDBACK STATUS AND PROCESSING IS           */ 01244000
*/* FINISHED.  WHTH A RETURN CODE OF 0, THE CURRENT FSB IS TO BE     */ 01245000
*/* RELEASED WHILE WITH A CODE OF 4, IT MUST BE SAVED BECAUSE IT     */ 01246000
*/* CONTAINS GOOD DATA.                                              */ 01247000
*/*                                                                  */ 01248000
*/* INPUT : FSBPTR (REG6) - PTR TO THE FSB                           */ 01249000
*/*         LCPBPTR (REG 7) - PTR TO THE LCPB                        */ 01250000
*/*         LDOPTR (REG 9) - PTR TO THE LDO                          */ 01251000
*/*                                                                  */ 01252000
*/* OUTPUT : CRRPOST POSSIBLY SET                                    */ 01253000
*/*                                                                  */ 01254000
*/* SUBROUTINES CALLED : NEXTLDO                                     */ 01255000
*/*                                                                  */ 01256000
*/* MACROS USED : NONE                                               */ 01257000
*/*                                                                  */ 01258000
*/********************************************************************/ 01259000
*KEEPPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0797 01260000
KEEPPROC DS    0H                                                  0798 01261000
*   CRRRTN5 = RETPTR;             /* SAVE RETURN ADDRESS             */ 01262000
         ST    RETPTR,CRRRTN5(,CRRPTR)                             0798 01263000
*   IF CRRBRTCD=CRRBRSUC THEN   /* RETURN CODE 0 FORM MOVE ROUTINE  */  01264000
         CLI   CRRBRTCD(CRRPTR),0                                  0799 01265000
         BNE   @RF00799                                            0799 01266000
*       IF FSBEBI=OFF & FSBLGI=OFF & FSBSOHI=OFF & FSBMJRCD=FRNORM THEN 01267000
         TM    FSBEBI(FSBPTR),B'00100011'                          0800 01268000
         BNZ   @RF00800                                            0800 01269000
         CLI   FSBMJRCD(FSBPTR),X'00'                              0800 01270000
         BNE   @RF00800                                            0800 01271000
*             IF CRRBLEN=ZERO THEN  /* NO ROOM IN BUFFER */        0801 01272000
         LH    @15,CRRBLEN(,CRRPTR)                                0801 01273000
         LTR   @15,@15                                             0801 01274000
         BNZ   @RF00801                                            0801 01275000
*                IF LDOACHN=ON THEN /* LDO IS DATA CHAINED       */     01276000
         TM    LDOACHN(LDOPTR),B'10000000'                         0802 01277000
         BNO   @RF00802                                            0802 01278000
*                      IF CRRERLK=ON THEN    /* SEE IF ERROR LOCK SET*/ 01279000
*                                                                  0803 01280000
         TM    CRRERLK(CRRPTR),B'10000000'                         0803 01281000
         BNO   @RF00803                                            0803 01282000
*                           /************************************/ 0804 01283000
*                           /*  CURRENT FSB IS A SUBBLOCK, ALL  */ 0804 01284000
*                           /*  THE DATA WAS MOVED, AND THERE IS*/ 0804 01285000
*                           /*  ANOTHER LDO, BUT THE ERROR LOCK */ 0804 01286000
*                           /*  IS SET.  LET USER KNOW THIS. DO */ 0804 01287000
*                           /*  NOT WAIT FOR ANOTHER SUBBLOCK.  */ 0804 01288000
*                           /************************************/ 0804 01289000
*                                                                  0804 01290000
*                           CRRPOST=ON;      /* FINISHED PROCESSING  */ 01291000
         OI    CRRPOST(CRRPTR),B'00010000'                         0804 01292000
*                      ELSE DO;              /* ERROR LOCK NOT SET   */ 01293000
*                                                                  0805 01294000
         B     @RC00803                                            0805 01295000
@RF00803 DS    0H                                                  0806 01296000
*                           /*************************************/     01297000
*                           /*  CURRENT FSB IS A SUBBLOCK, ALL   */     01298000
*                           /*  THE DATA WAS MOVED FROM CURRENT  */     01299000
*                           /*  FSB.  GET DATA CHAINED LDO AND   */     01300000
*                           /*  SAVE STATUS FOR NEXT SUBBLOCK    */     01301000
*                           /*************************************/     01302000
*                                                                  0806 01303000
*                           CALL NEXTLDO; /* GET NEXT LDO FOR MOVE  */  01304000
         BAL   @14,NEXTLDO                                         0806 01305000
*                           DCLBTLEN=CRRBTLEN; /* SAVE TOTAL LENGTH */  01306000
         L     @15,CRRBTLEN(,CRRPTR)                               0807 01307000
         ST    @15,DCLBTLEN(,LCPBPTR)                              0807 01308000
*                           END;                                   0808 01309000
*                ELSE         /* NO MORE ROOM BUT MORE DATA      */     01310000
*                                                                  0809 01311000
*                     /**************************************/     0809 01312000
*                     /*  CURRENT FSB IS A SUBBLOCK.  ALL   */     0809 01313000
*                     /*  DATA MOVED.  THERE IS NO ROOM IN  */     0809 01314000
*                     /*  BUFFER FOR NEXT SUBBLOCK.  MOVE   */     0809 01315000
*                     /*  PROCESSING COMPLETE.              */     0809 01316000
*                     /**************************************/     0809 01317000
*                                                                  0809 01318000
*                     CRRPOST=ON;  /* RPL SHOULD BE POSTED COMPLETE  */ 01319000
         B     @RC00802                                            0809 01320000
@RF00802 OI    CRRPOST(CRRPTR),B'00010000'                         0809 01321000
*             ELSE                /* ROOM IN BUF-SAVE INFO FOR MOVE  */ 01322000
*              IF CRRERLK=ON THEN            /* SEE IF ERROR LOCK SET*/ 01323000
*                                                                  0810 01324000
         B     @RC00801                                            0810 01325000
@RF00801 TM    CRRERLK(CRRPTR),B'10000000'                         0810 01326000
         BNO   @RF00810                                            0810 01327000
*                   /**********************************/           0811 01328000
*                   /*  CURRENT FSB IS A SUBBLOCK. ALL*/           0811 01329000
*                   /*  THE DATA WAS MOVED.  THERE IS */           0811 01330000
*                   /*  MORE ROOM IN THE BUFFER BUT   */           0811 01331000
*                   /*  THE ERROR LOCK IS SET.  DO    */           0811 01332000
*                   /*  NOT WAIT FOR NEXT SUBBLOCK.   */           0811 01333000
*                   /*  INFORM USER WHAT HAS OCCURRED */           0811 01334000
*                   /**********************************/           0811 01335000
*                                                                  0811 01336000
*                   CRRPOST=ON;              /* FINISHED PROCESSING  */ 01337000
         OI    CRRPOST(CRRPTR),B'00010000'                         0811 01338000
*              ELSE DO;                      /* ERROR LOCK NOT SET   */ 01339000
*                                                                  0812 01340000
         B     @RC00810                                            0812 01341000
@RF00810 DS    0H                                                  0813 01342000
*                  /**********************************/            0813 01343000
*                  /*  FSB JUST PROCESSED WAS A      */            0813 01344000
*                  /*  SUBBLOCK.  THERE IS MORE ROOM */            0813 01345000
*                  /*  IN THE BUFFER.  SAVE STATUS   */            0813 01346000
*                  /*  IN DCLCPB SO MOVE CAN BE      */            0813 01347000
*                  /*  CONTINUED WHEN NEXT SUBBLOCK  */            0813 01348000
*                  /*  ARRIVES.                      */            0813 01349000
*                  /**********************************/            0813 01350000
*                                                                  0813 01351000
*                  DCLCLDOA=CRRBLDAD; /* SAVE ADDR OF LAST LDO  */ 0813 01352000
         L     @15,CRRBLDAD(,CRRPTR)                               0813 01353000
         ST    @15,DCLCLDOA(,LCPBPTR)                              0813 01354000
*                  DCLIOLEN=CRRBLEN;  /* SAVE REMAINING LENGTH       */ 01355000
         LH    @08,CRRBLEN(,CRRPTR)                                0814 01356000
         STH   @08,DCLIOLEN(,LCPBPTR)                              0814 01357000
*                  DCLIOBUF=CRRBARAD; /* RESUME BUFFER ADDRESS       */ 01358000
         L     @08,CRRBARAD(,CRRPTR)                               0815 01359000
         ST    @08,DCLIOBUF(,LCPBPTR)                              0815 01360000
*                  DCLBTLEN=CRRBTLEN; /* SAVE TOTAL LENGTH ACCUMULATOR* 01361000
         L     @08,CRRBTLEN(,CRRPTR)                               0816 01362000
         ST    @08,DCLBTLEN(,LCPBPTR)                              0816 01363000
*                  DCLRPLA=CRRARPL;   /* SAVE POINTER TO THE RPL     */ 01364000
         L     @08,CRRARPL(,CRRPTR)                                0817 01365000
         ST    @08,DCLRPLA(,LCPBPTR)                               0817 01366000
*                  IF ADDR(LCPLCCW)->LCCWOP^=LTIC THEN             0818 01367000
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0818 01368000
         BE    @RF00818                                            0818 01369000
*                   DCLLDO1=CRRBLDAD;        /* SAVE LDO IN LCPB     */ 01370000
         ST    @15,DCLLDO1(,LCPBPTR)                               0819 01371000
*                  ELSE                     /* SAVE LDO IN EXTENSION */ 01372000
*                   EVAL(CRRCLCCW+LDOFFSET)->ADDRLDO=CRRBLDAD;     0820 01373000
         B     @RC00818                                            0820 01374000
@RF00818 LA    @15,32                                              0820 01375000
         AL    @15,CRRCLCCW(,CRRPTR)                               0820 01376000
         L     @08,CRRBLDAD(,CRRPTR)                               0820 01377000
         ST    @08,ADDRLDO(,@15)                                   0820 01378000
*                  END;                                            0821 01379000
*        ELSE                     /* FSB NOT SUBBLOCK-MOVE FINISHED  */ 01380000
*                                                                  0822 01381000
*             /***********************************/                0822 01382000
*             /*  FSB JUST PROCESSED IS NOT A    */                0822 01383000
*             /*  SUBBLOCK.  ALL THE DATA WAS    */                0822 01384000
*             /*  MOVED.  FEEDBACK PROCESSING IS */                0822 01385000
*             /*  FINISHED.                      */                0822 01386000
*             /***********************************/                0822 01387000
*                                                                  0822 01388000
*             CRRPOST=ON;         /* RPL SHOULD BE POSTED COMPLETE   */ 01389000
         B     @RC00800                                            0822 01390000
@RF00800 OI    CRRPOST(CRRPTR),B'00010000'                         0822 01391000
*   ELSE DO;                      /* CODE 4 - BUFFER FILLED          */ 01392000
*                                                                  0823 01393000
         B     @RC00799                                            0823 01394000
@RF00799 DS    0H                                                  0824 01395000
*        /**************************************/                  0824 01396000
*        /*  FSB JUST PROCESSED IS NOT A       */                  0824 01397000
*        /*  SUBBLOCK.  THE USER'S BUFFER WAS  */                  0824 01398000
*        /*  FILLED BEFORE ALL THE DATA WAS    */                  0824 01399000
*        /*  MOVED.  RESET ANY ENDING FEEDBACK */                  0824 01400000
*        /*  STATUS TO INDICATE NOT ALL DATA   */                  0824 01401000
*        /*  WAS RECEIVED.  SAVE FSB SO        */                  0824 01402000
*        /*  REMAINING DATA WILL BE PROCESSED  */                  0824 01403000
*        /*  UPON ISSUANCE OF NEXT READ.       */                  0824 01404000
*        /**************************************/                  0824 01405000
*                                                                  0824 01406000
*        CRRRDEOB=OFF;            /* INDICATE PARTIAL DATA IN BUFFER  * 01407000
*        CRRRDEOM=OFF;                                             0825 01408000
*        CRRRDEOT=OFF;                                             0826 01409000
         NI    CRRRDEOB(CRRPTR),B'11000111'                        0826 01410000
*        CRRSW1=ON;               /* SAVE FSB                        */ 01411000
         OI    CRRSW1(CRRPTR),B'01000000'                          0827 01412000
*        CRRPOST=ON;              /* RPL SHOULD BE POSTED COMPLETE   */ 01413000
         OI    CRRPOST(CRRPTR),B'00010000'                         0828 01414000
*        END;                                                      0829 01415000
*                                                                  0829 01416000
*   RETPTR = CRRRTN5;             /* GET RETURN ADDRESS              */ 01417000
@RC00799 L     RETPTR,CRRRTN5(,CRRPTR)                             0830 01418000
*END KEEPPROC;                     /* RETURN TO CALLER                * 01419000
@EL00010 DS    0H                                                  0831 01420000
@EF00010 DS    0H                                                  0831 01421000
@ER00010 BR    @14                                                 0831 01422000
*/********************************************************************/ 01423000
*/*                                                                  */ 01424000
*/* ROUTINE NAME : NEXTLDO - FIND THE NEXT LDO                       */ 01425000
*/*                                                                  */ 01426000
*/* FUNCTION : THIS ROUTINE IS CALLED BY TRUNPROC OR KEEPPROC WHEN TH*/ 01427000
*/* CURRENT USER BUFFER IS FILLED, BUT THE CURRENT LDO IS DATA       */ 01428000
*/* CHAINED.  THE NEXT LDO IS TO BE LOCATED, AND                     */ 01429000
*/* THE PARAMETERS FOR THE MOVE ROUTINE SAVED IN THE LCPB.  IF AN    */ 01430000
*/* ERROR IS FOUND, PROCESSING IS TERMINATED.  IF A JUMP LDO IS      */ 01431000
*/* ENCOUNTERED, JUMPPROC IS INVOKED TO VERIFY IT.                   */ 01432000
*/*                                                                  */ 01433000
*/* INPUT : LDOPTR (REG 9) - PTR TO THE LDO                          */ 01434000
*/*         LCPBPTR (REG 7) - PTR TO THE LCPB                        */ 01435000
*/*                                                                  */ 01436000
*/* OUTPUT : CRRCLDO UPDATED                                         */ 01437000
*/*                                                                  */ 01438000
*/* SUBROUTINES CALLED : JUMPPROC                                    */ 01439000
*/*                                                                  */ 01440000
*/* MACROS USED : NONE                                               */ 01441000
*/*                                                                  */ 01442000
*/********************************************************************/ 01443000
*NEXTLDO: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0832 01444000
NEXTLDO  DS    0H                                                  0833 01445000
*   CRRRTN6 = RETPTR;             /* SAVE RETURN ADDRESS             */ 01446000
         ST    RETPTR,CRRRTN6(,CRRPTR)                             0833 01447000
*   LDOPTR = CRRCLDO + LDOSIZE;   /* UPDATE TO NEXT LDO              */ 01448000
         LA    LDOPTR,8                                            0834 01449000
         AL    LDOPTR,CRRCLDO(,CRRPTR)                             0834 01450000
*   CRRCLDO = LDOPTR;             /* SAVE CURRENT LDO IN CRR         */ 01451000
*                                                                  0835 01452000
         ST    LDOPTR,CRRCLDO(,CRRPTR)                             0835 01453000
*        IF LDOCMD=LDOJUMP THEN                                    0836 01454000
*                                                                  0836 01455000
         CLI   LDOCMD(LDOPTR),X'08'                                0836 01456000
         BNE   @RF00836                                            0836 01457000
*              /******************************************/        0837 01458000
*              /*   PROCESS DATA CHAINED JUMP LDO        */        0837 01459000
*              /******************************************/        0837 01460000
*              CALL JUMPPROC;     /* VALIDATE JUMP LDO               */ 01461000
*                                                                  0837 01462000
         BAL   @14,JUMPPROC                                        0837 01463000
*        ELSE DO;                 /* NOT JUMP-SAVE MOVE INFORMATION  */ 01464000
*                                                                  0838 01465000
         B     @RC00836                                            0838 01466000
@RF00836 DS    0H                                                  0839 01467000
*             /*****************************************/          0839 01468000
*             /*  VALID DATA CHAINED LDO.  SAVE STATUS */          0839 01469000
*             /*  FOR NEW LDO IN DCLCP SO MOVE         */          0839 01470000
*             /*  PROCESSING CAN BE CONTINUED WHEN     */          0839 01471000
*             /*  NEXT SUBBLOCK ARRIVES.               */          0839 01472000
*             /*****************************************/          0839 01473000
*                                                                  0839 01474000
*             DCLCLDOA=LDOPTR;    /* LDO TO BE PROCESSED NEXT        */ 01475000
         ST    LDOPTR,DCLCLDOA(,LCPBPTR)                           0839 01476000
*             DCLIOBUF=LDOADDR;   /* RESUME DATA ADDRESS             */ 01477000
         L     @15,LDOADDR(,LDOPTR)                                0840 01478000
         ST    @15,DCLIOBUF(,LCPBPTR)                              0840 01479000
*             DCLIOLEN=LDOLEN;    /* REMAINING LENGTH TO MOVE        */ 01480000
         LH    @15,LDOLEN(,LDOPTR)                                 0841 01481000
         N     @15,@CF04418                                        0841 01482000
         STH   @15,DCLIOLEN(,LCPBPTR)                              0841 01483000
*             DCLRPLA=CRRARPL;    /* ADDRESS OF RPL FOR MOVE         */ 01484000
         L     @15,CRRARPL(,CRRPTR)                                0842 01485000
         ST    @15,DCLRPLA(,LCPBPTR)                               0842 01486000
*             IF ADDR(LCPLCCW)->LCCWOP^=LTIC THEN                  0843 01487000
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0843 01488000
         BE    @RF00843                                            0843 01489000
*                   DCLLDO1=LDOPTR;        /* SAVE LDO IN LCPB     */   01490000
         ST    LDOPTR,DCLLDO1(,LCPBPTR)                            0844 01491000
*             ELSE                     /* SAVE LDO IN EXTENSION */ 0845 01492000
*                   EVAL(CRRCLCCW+LDOFFSET)->ADDRLDO=LDOPTR;       0845 01493000
         B     @RC00843                                            0845 01494000
@RF00843 LA    @15,32                                              0845 01495000
         AL    @15,CRRCLCCW(,CRRPTR)                               0845 01496000
         ST    LDOPTR,ADDRLDO(,@15)                                0845 01497000
*             END;                                                 0846 01498000
*                                                                  0846 01499000
@RC00843 DS    0H                                                  0847 01500000
*   RETPTR=CRRRTN6;               /* GET RETURN ADDRESS              */ 01501000
@RC00836 L     RETPTR,CRRRTN6(,CRRPTR)                             0847 01502000
*END NEXTLDO;                      /* RETURN TO CALLER                * 01503000
@EL00011 DS    0H                                                  0848 01504000
@EF00011 DS    0H                                                  0848 01505000
@ER00011 BR    @14                                                 0848 01506000
*/********************************************************************/ 01507000
*/*                                                                  */ 01508000
*/* ROUTINE NAME : JUMPPROC - JUMP LDO PROCESSING                    */ 01509000
*/*                                                                  */ 01510000
*/* FUNCTION : CALLED BY NEXTLDO TO VALIDATE A JUMP LDO.             */ 01511000
*/* IF THE LDO JUMPED TO IS VALID ( NOT ANOTHER JUMP )  THEN         */ 01512000
*/* IT IS USED TO INITIALIZE PARAMETERS FOR THE MOVE ROUTINE         */ 01513000
*/* IN THE LCPB.  IF THE LDO JUMPED TO IS INVALID ( ANOTHER          */ 01514000
*/* JUMP )  THEN AN ERROR CODE IS SET IN THE CRR AND FEEDBACK        */ 01515000
*/* PROCESSING IS TERMINATED.                                        */ 01516000
*/*                                                                  */ 01517000
*/* INPUT : LDOPTR (REG 9) - PTR TO THE LDO                          */ 01518000
*/*         LCPBPTR (REG 7) - PTR TO THE LCPB                        */ 01519000
*/*                                                                  */ 01520000
*/* OUTPUT : CRRPOST POSSIBLY SET                                    */ 01521000
*/*                                                                  */ 01522000
*/* SUBROUTINES CALLED : NONE                                        */ 01523000
*/*                                                                  */ 01524000
*/* MACROS USED : NONE                                               */ 01525000
*/*                                                                  */ 01526000
*/********************************************************************/ 01527000
*JUMPPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0849 01528000
JUMPPROC DS    0H                                                  0850 01529000
*   CRRRTN7 = RETPTR;             /* SAVE RETURN ADDRESS             */ 01530000
         ST    RETPTR,CRRRTN7(,CRRPTR)                             0850 01531000
*   LDOPTR = LDOADDR;             /* ADDRESS OF JUMPED TO LDO        */ 01532000
*                                                                  0851 01533000
         L     LDOPTR,LDOADDR(,LDOPTR)                             0851 01534000
*         IF LDOCMD=LDOJUMP THEN                                   0852 01535000
         CLI   LDOCMD(LDOPTR),X'08'                                0852 01536000
         BNE   @RF00852                                            0852 01537000
*              DO;                /* ERROR - JUMP TO A JUMP          */ 01538000
*              RFY RETCODE UNRSTD;                                 0854 01539000
*                                                                  0854 01540000
*              /************************************/              0855 01541000
*              /*     ERROR-JUMP TO A JUMP         */              0855 01542000
*              /************************************/              0855 01543000
*                                                                  0855 01544000
*              CRRFDB2=CRRJTOJ;   /* SET REASON CODE FOR FEEDBACK    */ 01545000
         MVI   CRRFDB2(CRRPTR),X'19'                               0855 01546000
*              CRRRTNCD=CRRLOGIC; /* LOGICAL I/O ERROR               */ 01547000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0856 01548000
*              CRRPOST=ON;        /* RPL SHOULD BE POSTED COMPLETE   */ 01549000
         OI    CRRPOST(CRRPTR),B'00010000'                         0857 01550000
*              END;                                                0858 01551000
*         ELSE DO;                /* JUMP TO A VALID LDO             */ 01552000
*                                                                  0859 01553000
         B     @RC00852                                            0859 01554000
@RF00852 DS    0H                                                  0860 01555000
*              /*************************************/             0860 01556000
*              /*  VALID JUMP TO LDO.  SAVE STATUS  */             0860 01557000
*              /*  FROM NEW LDO IN DCLCPB SO MOVE   */             0860 01558000
*              /*  CAN BE CONTINUED WHEN NEXT       */             0860 01559000
*              /*  SUBBLOCK ARRIVES.                */             0860 01560000
*              /*************************************/             0860 01561000
*                                                                  0860 01562000
*              DCLCLDOA=LDOPTR;   /* LDO TO BE PROCESSED NEXT        */ 01563000
         ST    LDOPTR,DCLCLDOA(,LCPBPTR)                           0860 01564000
*              DCLIOBUF=LDOADDR;  /* RESUME DATA ADDRESS             */ 01565000
         L     @15,LDOADDR(,LDOPTR)                                0861 01566000
         ST    @15,DCLIOBUF(,LCPBPTR)                              0861 01567000
*              DCLIOLEN=LDOLEN;   /* RESUME DATA COUNT               */ 01568000
         LH    @15,LDOLEN(,LDOPTR)                                 0862 01569000
         N     @15,@CF04418                                        0862 01570000
         STH   @15,DCLIOLEN(,LCPBPTR)                              0862 01571000
*              CRRCLDO=LDOPTR;    /* UPDATE CURRENT LDO POINTER      */ 01572000
         ST    LDOPTR,CRRCLDO(,CRRPTR)                             0863 01573000
*              DCLRPLA=CRRARPL;   /* SAVE RPL ADDRESS FOR NEXT MOVE  */ 01574000
         L     @15,CRRARPL(,CRRPTR)                                0864 01575000
         ST    @15,DCLRPLA(,LCPBPTR)                               0864 01576000
*              IF ADDR(LCPLCCW)->LCCWOP^=LTIC THEN                 0865 01577000
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0865 01578000
         BE    @RF00865                                            0865 01579000
*                   DCLLDO1=LDOPTR;        /* SAVE LDO IN LCPB     */   01580000
         ST    LDOPTR,DCLLDO1(,LCPBPTR)                            0866 01581000
*              ELSE                     /* SAVE LDO IN EXTENSION */     01582000
*                   EVAL(CRRCLCCW+LDOFFSET)->ADDRLDO=LDOPTR;       0867 01583000
         B     @RC00865                                            0867 01584000
@RF00865 LA    @15,32                                              0867 01585000
         AL    @15,CRRCLCCW(,CRRPTR)                               0867 01586000
         ST    LDOPTR,ADDRLDO(,@15)                                0867 01587000
*              END;                                                0868 01588000
*                                                                  0868 01589000
@RC00865 DS    0H                                                  0869 01590000
*   RETPTR = CRRRTN7;             /* GET RETURN ADDRESS              */ 01591000
@RC00852 L     RETPTR,CRRRTN7(,CRRPTR)                             0869 01592000
*END JUMPPROC;                     /* RETURN TO NEXTLDO               * 01593000
@EL00012 DS    0H                                                  0870 01594000
@EF00012 DS    0H                                                  0870 01595000
@ER00012 BR    @14                                                 0870 01596000
*/********************************************************************/ 01597000
*/*                                                                  */ 01598000
*/* ROUTINE NAME : FLUSHRTN - FLUSH PROCESSING                       */ 01599000
*/*                                                                  */ 01600000
*/* FUNCTION : INVOKED WHEN THE FLUSH BIT IS ON IN THE FMCB.         */ 01601000
*/* ALL THE FSB'S ARE TO BE IGNORED UNTIL AN END COMDITION IS        */ 01602000
*/* MET. (EOB, EOM, OR EOT INDICATED IN FSB.)  ONCE AN END           */ 01603000
*/* CONDITION IS MET, THE RPL CAN BE POSTED COMPLETE.  IN EITHER     */ 01604000
*/* CASE, FREEFSB IS CALLED TO RELEASE THE STORAGE FOR THE FSB       */ 01605000
*/* AND FDB'S.                                                       */ 01606000
*/*                                                                  */ 01607000
*/* INPUT : CRRFMCB - PTR TO THE FMCB                                */ 01608000
*/*         FSBPTR (REG 6) - PTR TO THE FSB                          */ 01609000
*/*                                                                  */ 01610000
*/* OUTPUT : CRRSW5 POSSIBLY SET                                     */ 01611000
*/*          FMCFLGA3 POSSIBLY TURNED OFF                            */ 01612000
*/*                                                                  */ 01613000
*/* SUBROUTINES CALLED : LCPBINIT                                    */ 01614000
*/*                                                                  */ 01615000
*/* MACROS USED : TPDEQ , TPQUE                                      */ 01616000
*/*                                                                  */ 01617000
*/********************************************************************/ 01618000
*FLUSHRTN: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0871 01619000
FLUSHRTN DS    0H                                                  0872 01620000
*   CRRRTN2 = RETPTR;             /* SAVE RETURN ADDRESS             */ 01621000
         ST    RETPTR,CRRRTN2(,CRRPTR)                             0872 01622000
*   IF FMCFLGA3=ON THEN            /* IS FLUSH INDICATOR STILL SET?  */ 01623000
         L     @15,CRRFMCB(,CRRPTR)                                0873 01624000
         TM    FMCFLGA3(@15),B'00010000'                           0873 01625000
         BNO   @RF00873                                            0873 01626000
*     DO;                                                          0874 01627000
*       IF FSBMNELI=ON THEN        /* IS FSB ERROR LOCK SET?         */ 01628000
*                                                                  0875 01629000
         TM    FSBMNELI(FSBPTR),B'10000000'                        0875 01630000
         BNO   @RF00875                                            0875 01631000
*          /***************************************/               0876 01632000
*          /* ERROR LOCK SET IN FSB BEING FLUSHED */               0876 01633000
*          /***************************************/               0876 01634000
*                                                                  0876 01635000
*          CRRSW5=ON;              /* YES, INDICATE ERRROR LOCK SET  */ 01636000
         OI    CRRSW5(CRRPTR),B'00000100'                          0876 01637000
*       ELSE                                                       0877 01638000
*          CRRSW5=OFF;             /* NO, ERROR LOCK NOT SET         */ 01639000
         B     @RC00875                                            0877 01640000
@RF00875 NI    CRRSW5(CRRPTR),B'11111011'                          0877 01641000
*       IF FSBEBI=ON |             /* ENDING CONDITION FOUND IN FSB? */ 01642000
*          FSBELI=ON THEN               /* END OF CHANNED PROGRAM?    * 01643000
@RC00875 TM    FSBEBI(FSBPTR),B'10100000'                          0878 01644000
         BZ    @RF00878                                            0878 01645000
*          DO;                                                     0879 01646000
*                                                                  0879 01647000
*            /*************************************/               0880 01648000
*            /* RESET INDICATOR TO INDICATE FLUSH */               0880 01649000
*            /* FINISHED.  EITHER ENDING CONDITION*/               0880 01650000
*            /* (EOB,EOM,EOT) FOUND IN FSB OR END */               0880 01651000
*            /* OF LOGICAL CHANNEL PROGRAM REACHED*/               0880 01652000
*            /*************************************/               0880 01653000
*                                                                  0880 01654000
*            RFY (REG10,REG11) RSTD;                               0880 01655000
*       CS5: REG10=FMCCMPS2;            /* GET OLD FLAG VALUE        */ 01656000
CS5      L     @15,CRRFMCB(,CRRPTR)                                0881 01657000
         L     REG10,FMCCMPS2(,@15)                                0881 01658000
*            REG11=REG10&FLGA3OFF;      /* SET NEW FLAG VALUE        */ 01659000
         LR    REG11,REG10                                         0882 01660000
         N     REG11,@CF03452                                      0882 01661000
*            CS (REG10,REG11,FMCCMPS2); /* SET NEW VALUE IN FMCB     */ 01662000
         CS    REG10,@11,FMCCMPS2(@15)                             0883 01663000
*            BC (7,CS5);                /* TRY AGAIN IF FAILURE      */ 01664000
         BC    7,CS5                                               0884 01665000
*            RFY (REG10,REG11) UNRSTD;                             0885 01666000
*          END;                                                    0886 01667000
*       ELSE;                                                      0887 01668000
@RF00878 DS    0H                                                  0888 01669000
*       IF CRRSW6=ON THEN                                          0888 01670000
         TM    CRRSW6(CRRPTR),B'00000010'                          0888 01671000
         BNO   @RF00888                                            0888 01672000
*          DO;                     /* FSB HAS BEEN TPQUE'D           */ 01673000
*                                                                  0889 01674000
*          /*************************************/                 0890 01675000
*          /*  IF FSB NOW BEING FLUSHED HAD     */                 0890 01676000
*          /*  BEEN PREVIOUSLY BEEN QUEUED BY   */                 0890 01677000
*          /*  THIS MODULE, IT MUST BE DEQUEUED */                 0890 01678000
*          /*  SO IT CAN BE FLUSHED.            */                 0890 01679000
*          /*************************************/                 0890 01680000
*                                                                  0890 01681000
*                                                                  0890 01682000
*/* TPDEQ (,FMCFBA) RETPTR(FSBPTR) */                              0890 01683000
*           DO; /* BEGIN TPDEQ CODE GENERATION */                  0890 01684000
*           DCL FMZCA065 BIT(32) CONSTANT('80000000'X);            0891 01685000
*           DCL FMZCB065 BIT(32) CONSTANT('7FFFFFFF'X);            0892 01686000
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                0893 01687000
*                                                                  0893 01688000
*           RFY ISTWKE  ;                                          0894 01689000
*           GPR15P=ADDR(FMCFBA); /*ADDRESS QUEUE POINTER */        0895 01690000
         L     GPR15P,CRRFMCB(,CRRPTR)                             0895 01691000
         LA    GPR15P,FMCFBA(,GPR15P)                              0895 01692000
*           GPR14P=GPR15P->PABWEQA|FMZCA065;                       0896 01693000
         L     @08,@CF02699                                        0896 01694000
         L     GPR14P,PABWEQA(,GPR15P)                             0896 01695000
         OR    GPR14P,@08                                          0896 01696000
*           IF GPR14P=FMZCA065 THEN;                               0897 01697000
         CLR   GPR14P,@08                                          0897 01698000
         BE    @RT00897                                            0897 01699000
*           ELSE DO;                                               0899 01700000
*FMZL1065:;                                                        0900 01701000
FMZL1065 DS    0H                                                  0901 01702000
*             DO WHILE(GPR14P->WKECHAIN^=FMZCA065);                0901 01703000
         B     @DE00901                                            0901 01704000
@DL00901 DS    0H                                                  0902 01705000
*               GPR15P=ADDR(GPR14P->WKECHAIN);                     0902 01706000
         LA    GPR15P,WKECHAIN(,GPR14P)                            0902 01707000
*               GPR14P=GPR15P->PABWEQA;                            0903 01708000
         L     GPR14P,PABWEQA(,GPR15P)                             0903 01709000
*             END;                                                 0904 01710000
@DE00901 L     @08,@CF02699                                        0904 01711000
         CL    @08,WKECHAIN(,GPR14P)                               0904 01712000
         BNE   @DL00901                                            0904 01713000
*             GPR00P=FMZCA065;                                     0905 01714000
         LR    GPR00P,@08                                          0905 01715000
*             CS(GPR14P,GPR00P,GPR15P->PABWEQA);                   0906 01716000
         CS    GPR14P,@00,PABWEQA(GPR15P)                          0906 01717000
*             BC(7,FMZL1065);                                      0907 01718000
         BC    7,FMZL1065                                          0907 01719000
*             GPR14P->WKECHAIN=0;                                  0908 01720000
         SLR   @08,@08                                             0908 01721000
         ST    @08,WKECHAIN(,GPR14P)                               0908 01722000
*           END;                                                   0909 01723000
*ISTA0065:;                                                        0910 01724000
@RT00897 DS    0H                                                  0910 01725000
ISTA0065 DS    0H                                                  0911 01726000
*           GPR14P=GPR14P&FMZCB065;                                0911 01727000
         N     GPR14P,@CF03439                                     0911 01728000
*           FSBPTR=GPR14P;                                         0912 01729000
         LR    FSBPTR,GPR14P                                       0912 01730000
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;              0913 01731000
*           END; /* END TPDEQ GENERATION */ /* DEQUE FSB             */ 01732000
*          RFY (REG10,REG11) RSTD; /* BUFFER THRESH COMMENTED OUT*/     01733000
*  C40B:   REG10=FMCCMPS1;         /* CURRENT BUFFER COUNT          */  01734000
C40B     L     @15,CRRFMCB(,CRRPTR)                                0916 01735000
         L     REG10,FMCCMPS1(,@15)                                0916 01736000
*          REG11=REG10&BUFNB0;     /* NEW BUFFER COUNT               */ 01737000
         LR    REG11,REG10                                         0917 01738000
         N     REG11,@CF04271                                      0917 01739000
*          CS (REG10,REG11,FMCCMPS1);  /* PUT IN FMCB                */ 01740000
         CS    REG10,@11,FMCCMPS1(@15)                             0918 01741000
*          BC (7,C40B);            /* TRY AGAIN IF FAILED            */ 01742000
         BC    7,C40B                                              0919 01743000
*          RFY (REG10,REG11) UNRSTD;                               0920 01744000
*          IF FSBPTR^=0 THEN                                       0921 01745000
         LTR   FSBPTR,FSBPTR                                       0921 01746000
         BZ    @RF00921                                            0921 01747000
*             CALL FREEFSB;        /* FSB WAS DEQUEUED               */ 01748000
         BAL   @14,FREEFSB                                         0922 01749000
*          ELSE;                                                   0923 01750000
@RF00921 DS    0H                                                  0924 01751000
*          END;                                                    0924 01752000
*       ELSE CALL FREEFSB;         /* THROW FSB AWAY                 */ 01753000
         B     @RC00888                                            0925 01754000
@RF00888 BAL   @14,FREEFSB                                         0925 01755000
*       CRRSW4=OFF;                /* INDICATE FLUSH OCCURRED        */ 01756000
@RC00888 NI    CRRSW4(CRRPTR),B'11110111'                          0926 01757000
*     END;                                                         0927 01758000
*   ELSE CRRSW4=ON;                /* INDICATE FLUSH DID NOT OCCUR   */ 01759000
         B     @RC00873                                            0928 01760000
@RF00873 OI    CRRSW4(CRRPTR),B'00001000'                          0928 01761000
* /* TPUNLOCK REMOVED FOR DCR 1988 ( 10/2/73 ) */                  0929 01762000
*   IF CRRSW5=ON THEN                                              0929 01763000
@RC00873 TM    CRRSW5(CRRPTR),B'00000100'                          0929 01764000
         BNO   @RF00929                                            0929 01765000
*      DO;                         /* FSB HAD ERROR LOCK SET         */ 01766000
*                                                                  0930 01767000
*      /*************************************/                     0931 01768000
*      /*  IF FSB BEING FLUSHED HAS THE     */                     0931 01769000
*      /*  ERROR LOCK SET, IT MUST BE       */                     0931 01770000
*      /*  INTERNALLY RESET SINCE THE USER  */                     0931 01771000
*      /*  WILL NOT KNOW IT IS SET.  INVOKE */                     0931 01772000
*      /*  LCPBINIT TO GET AND INITIALIZE A */                     0931 01773000
*      /*  DCLCPB.  BUILD RESET ERROR LOCK  */                     0931 01774000
*      /*  AND QUEUE TO TPIOS OUTBOUND PAB. */                     0931 01775000
*      /*************************************/                     0931 01776000
*                                                                  0931 01777000
*      CALL LCPBINIT;              /* GET AND INIT LCCW FOR RESET    */ 01778000
         BAL   @14,LCPBINIT                                        0931 01779000
*      LCCWOP=LXEL;                 /* RESET ERROR LOCK              */ 01780000
         L     @15,LCPNXLCW(,LCPBPTR)                              0932 01781000
         MVI   LCCWOP(@15),X'0B'                                   0932 01782000
*      DCLSRESW=ON;                /* INDICATE SPECIAL RESET         */ 01783000
*                                                                  0933 01784000
         OI    DCLSRESW(LCPBPTR),B'10000000'                       0933 01785000
*/*  TPQUE (LCPBPTR,FMCPAB3) RESET */                              0934 01786000
*                                                                  0934 01787000
*       DO;                                                        0934 01788000
*       RFY (GPR00P,GPR01P,GPR02P,GPR15P) RSTD;                    0935 01789000
*       GEN REFS(GPR01P) SETS(RPHWORK)                             0936 01790000
*        (STM   GPR14P,GPR09P,RPHWORK(GPR01P));                    0936 01791000
         STM   GPR14P,GPR09P,RPHWORK(GPR01P)                            01792000
*       GPR00P=ADDR(FMCPAB3);                                      0937 01793000
         L     @08,CRRFMCB(,CRRPTR)                                0937 01794000
         LA    GPR00P,FMCPAB3(,@08)                                0937 01795000
*       GPR02P=LCPBPTR;                                            0938 01796000
         LR    GPR02P,LCPBPTR                                      0938 01797000
*       CALL ATCLCPTR->ATCAP34->ISTAPC34;                          0939 01798000
         L     @08,ATCLCPTR                                        0939 01799000
         L     @15,ATCAP34(,@08)                                   0939 01800000
         BALR  @14,@15                                             0939 01801000
*       GEN REFS(RPHWORK,GPR01P) NOSETS                            0940 01802000
*        (LM    GPR14P,GPR09P,RPHWORK(GPR01P));                    0940 01803000
         LM    GPR14P,GPR09P,RPHWORK(GPR01P)                            01804000
*       RFY (GPR00P,GPR01P,GPR02P,GPR15P) UNRSTD;                  0941 01805000
*       END; /**** END OF TPQUE GENERATION ****/ /* SHIP RESET TO TPIOS 01806000
*       */                                                         0942 01807000
*      END;                                                        0943 01808000
*   ELSE;                                                          0944 01809000
*                                                                  0944 01810000
@RF00929 DS    0H                                                  0945 01811000
*   RETPTR = CRRRTN2;             /* GET RETURN ADDRESS              */ 01812000
         L     RETPTR,CRRRTN2(,CRRPTR)                             0945 01813000
*END FLUSHRTN;                     /* RETURN TO CALLER                * 01814000
@EL00013 DS    0H                                                  0946 01815000
@EF00013 DS    0H                                                  0946 01816000
@ER00013 BR    @14                                                 0946 01817000
*/********************************************************************/ 01818000
*/*                                                                  */ 01819000
*/* ROUTINE NAME : LCPBINIT - INITIALIZE THE LCPB                    */ 01820000
*/*                                                                  */ 01821000
*/* FUNCTION : REQUEST STORAGE FOR A DCLCPB AND WAIT UNTIL STORAGE   */ 01822000
*/* IS AVAILABLE.  INITIALIZE ISTDCLCPB FOR A RESET OPERATION.       */ 01823000
*/* THIS SUBROUTINE IS INVOKED WHENEVER AN INTERNAL RESET NEEDS      */ 01824000
*/* TO BE ISSUED.  THIS IS DONE IF I/O NEEDS TO BE RESET IF          */ 01825000
*/* BUFFER THRESHOLD IS EXCEEDED OR IF THE ERROR LOCK IS SET ON      */ 01826000
*/* IN A FSB BEING FLUSHED.                                          */ 01827000
*/*                                                                  */ 01828000
*/* INPUT : RPHPTR (REG 1) - PTR TO THE RPH                          */ 01829000
*/*                                                                  */ 01830000
*/* OUTPUT : LCPBPTR (REG 7) - PTR TO THE RESET LCPB                 */ 01831000
*/*                                                                  */ 01832000
*/* SUBROUTINES CALLED : NONE                                        */ 01833000
*/*                                                                  */ 01834000
*/* MACROS USED : REQSTORE , TPWAIT                                  */ 01835000
*/*                                                                  */ 01836000
*/********************************************************************/ 01837000
*LCPBINIT: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0947 01838000
LCPBINIT DS    0H                                                  0948 01839000
*   CRRRTN8=RETPTR;                /* SAVE RETURN ADDRESS           */  01840000
         ST    RETPTR,CRRRTN8(,CRRPTR)                             0948 01841000
*    /*REQSTORE CBID(DCLCP)RPH(RPHPTR)CLEAR(YES)PRIORITY(YES)*/    0949 01842000
