         TITLE 'ISTDCC40-SOL/RD LDO FEEDBACK                           *00001000
                        '                                               00002000
ISTDCC40 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC40  75.136'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*                                                                  0463 00013000
*   /************************************************************/ 0463 00014000
*   /*    INITIALIZE POINTERS AND SET AUDIT TRAIL SWITCHES      */ 0463 00015000
*   /************************************************************/ 0463 00016000
*                                                                  0463 00017000
*   CRRPTR = ADDR(CRRSPACE);      /* REFERENCE CRR                   */ 00018000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0463 00019000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0463 00020000
*   FSBPTR = CRRFSB;              /* REFERENCE FSB                   */ 00021000
         L     FSBPTR,CRRFSB(,CRRPTR)                              0464 00022000
*   CRRC40 = ON;                  /* SET HISTORY SWITCH              */ 00023000
         OI    CRRC40(CRRPTR),B'10000000'                          0465 00024000
*   CRRMAS = CRRMAS&&CRRMAS;      /* CLEAR MODULE AUDIT SWITCHES     */ 00025000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0466 00026000
*   CRRM40 = ON;                  /* INDICATE ISTDCC40 IS PROCESSING */ 00027000
         OI    CRRM40(CRRPTR),B'10000000'                          0467 00028000
*   CRRSW6=OFF;                    /* INITIALIZE CONTROL SWITCH       * 00029000
*   CRRSW4=ON;                     /* INITIALIZE CONTROL SWITCH       * 00030000
         OI    CRRSW4(CRRPTR),B'00001000'                          0469 00031000
         NI    CRRSW6(CRRPTR),B'11111101'                          0469 00032000
*   CALL ENDANAL;                 /* TEST FSB FOR LAST FSB RECEIVED  */ 00033000
*                                                                  0470 00034000
         BAL   @14,ENDANAL                                         0470 00035000
*   /*****************************************************************/ 00036000
*   /* IF THE FLUSH INDICATOR IS ON IN THE FMCB, THIS FSB SHOULD BE  */ 00037000
*   /* THROWN AWAY AND NO FEEDBACK PROCESSING SHOULD BE DONE.  IF    */ 00038000
*   /* THE LCPB DOES NOT CONTAIN AN RPL ADDRESS, THEN THE REQUEST    */ 00039000
*   /* WAS FOR A SOLICIT SO ALL THAT SHOULD BE DONE IS TO QUEUE THE  */ 00040000
*   /* FSB ON THE FMCB DATA QUEUE SO IT CAN BE REFERENCED BY A LATER */ 00041000
*   /* READ.  OTHERWISE FEEDBACK PROCESSING SHOULD CONTINUE.         */ 00042000
*   /*****************************************************************/ 00043000
*                                                                  0471 00044000
*   LCPBPTR = FSBLCPB;            /* REFERENCE LCPB                  */ 00045000
         L     LCPBPTR,FSBLCPB(,FSBPTR)                            0471 00046000
*   IF FMCFLGA3=ON THEN           /* TEST FLUSH BIT                  */ 00047000
*                                                                  0472 00048000
         L     @15,CRRFMCB(,CRRPTR)                                0472 00049000
         TM    FMCFLGA3(@15),B'00010000'                           0472 00050000
         BNO   @RF00472                                            0472 00051000
*              /***********************************************/   0473 00052000
*              /*   FLUSH INDICATOR IS SET.  THE FSB SHOULD   */   0473 00053000
*              /*   NOT BE PROCESSED, BUT INSTEAD BE THROWN   */   0473 00054000
*              /*   AWAY.                                     */   0473 00055000
*              /***********************************************/   0473 00056000
*                                                                  0473 00057000
*              CALL FLUSHRTN;     /* IGNORE DATA UNTIL EOB,EOM,OR EOT*/ 00058000
         BAL   @14,FLUSHRTN                                        0473 00059000
*   ELSE;                          /* FSB SHOULD BE PROCESSED         * 00060000
@RF00472 DS    0H                                                  0475 00061000
*   IF CRRSW4=ON THEN                                              0475 00062000
         TM    CRRSW4(CRRPTR),B'00001000'                          0475 00063000
         BNO   @RF00475                                            0475 00064000
*        DO;                      /* FSB SHOULD BE PROCESSED         */ 00065000
*        IF DCLRPLA=0 THEN                                         0477 00066000
         L     @15,DCLRPLA(,LCPBPTR)                               0477 00067000
         LTR   @15,@15                                             0477 00068000
         BNZ   @RF00477                                            0477 00069000
*           DO;                                                    0478 00070000
*                                                                  0478 00071000
*              /***********************************************/   0479 00072000
*              /*    SOLICIT REQUEST.  NO FEEDBACK PROCESSING */   0479 00073000
*              /*    TO DO.  SAVE FSB TO BE PROCESSED BY      */   0479 00074000
*              /*    FUTURE READ REQUEST.                     */   0479 00075000
*              /***********************************************/   0479 00076000
*                                                                  0479 00077000
*              CRRSW1=ON;         /* SOLICIT-FSB SHOULD BE QUEUED    */ 00078000
*              CRRSW6=ON;         /* INDICATE SOLICIT BEING PROCESSED * 00079000
         OI    CRRSW1(CRRPTR),B'01000010'                          0480 00080000
*           END;                                                   0481 00081000
*        ELSE DO;                 /* READ OR WRITE WITH RD OR SOL    */ 00082000
*                                                                  0482 00083000
         B     @RC00477                                            0482 00084000
@RF00477 DS    0H                                                  0483 00085000
*              /***********************************************/   0483 00086000
*              /*   READ REQUEST.  FIND LDO AND MOVE DATA     */   0483 00087000
*              /*   TO USER'S BUFFER.                         */   0483 00088000
*              /***********************************************/   0483 00089000
*                                                                  0483 00090000
*             CRRSW1=OFF;         /* CLEAR FSB QUEUE INDICATOR       */ 00091000
         NI    CRRSW1(CRRPTR),B'10111111'                          0483 00092000
*             CALL LDOPROC;       /* CONTINUE PROCESSING             */ 00093000
         BAL   @14,LDOPROC                                         0484 00094000
*             END;                                                 0485 00095000
*        IF CRRSW1=ON THEN                                         0486 00096000
*                                                                  0486 00097000
@RC00477 TM    CRRSW1(CRRPTR),B'01000000'                          0486 00098000
         BNO   @RF00486                                            0486 00099000
*              /***********************************************/   0487 00100000
*              /*   FSB IS TO BE QUEUED ON THE FMCB DATA Q    */   0487 00101000
*              /***********************************************/   0487 00102000
*                                                                  0487 00103000
*              CALL HOLDDATA;     /* SAVE FSB ON FMCB DATA QUEUE     */ 00104000
         BAL   @14,HOLDDATA                                        0487 00105000
*        ELSE;                    /* FSB WAS RELEASED                */ 00106000
@RF00486 DS    0H                                                  0489 00107000
*        END;                                                      0489 00108000
*   ELSE;                                                          0490 00109000
*                                                                  0490 00110000
@RF00475 DS    0H                                                  0491 00111000
*/*TPESC*/                                                         0491 00112000
*     DO;                                                          0491 00113000
*                                                                  0491 00114000
*/* ATBUILD (ISTDYPAB,ISTAVT,ISTATCVT)  */                         0492 00115000
*/* VTAM ADDRESS VECTOR TABLE */                                   0492 00116000
*/*  REVISIONS                                         002 03/06/75  */ 00117000
*DCL  1  ISTAVT BDY(WORD) BASED,                                   0492 00118000
*       2  ISTACVT PTR(31),            /* ADDR OF VTAM CVT           */ 00119000
*       2  ISTAS49 PTR(31),            /* ADDR OF SVC 49 CODE        */ 00120000
*          3  ISTAHIP BIT(1),          /* HALT IN PROGRESS           */ 00121000
*       2  ISTAS53 PTR(31),            /* ADDR OF SVC 53 CODE        */ 00122000
*       2  ISTAPSTA PTR(31),           /* ADDR OF APS TABLE          */ 00123000
*       2  AVTASID FIXED(15),          /* VTAM ASID                  */ 00124000
*       2  ISTVTTIK FIXED(15),         /* VTAMRP TASK ID             */ 00125000
*       2  ISTAPSEX PTR(31),           /* ADDR OF APS EXIT           */ 00126000
*       2  ISTPHNM CHAR(5),            /* PHASE NAME OF TRANSIENT    */ 00127000
*       2  ISTX1 CHAR(1),              /* TEST FIELD                 */ 00128000
*       2  ISTARID PTR(15),            /* ADDR OF RID                */ 00129000
*       2  ISTATRT PTR(15),            /* ADDR OF VTAM GATES IN RETAB*/ 00130000
*       2  ISTAGTWT PTR(15),           /* ADDR OF GATEWAIT ROUTINE   */ 00131000
*       2  ISTVTTP  PTR(15),           /* ADDR OF CODE TO CHECK FOR  */ 00132000
*                                      /* PENDING TIMER INTERRUPT    */ 00133000
*       2  ISTAVR01 CHAR(2),           /* RESERVED                   */ 00134000
*       2  ISTPDAVP PTR(31);           /* VECTOR FOR PDAIDS SMS TRACE*/ 00135000
*/* END OF ATBUILD */                                              0493 00136000
*       RFY GPR15P RSTD;                                           0493 00137000
*                                                                  0493 00138000
*     DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */                0494 00139000
*       GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/                0495 00140000
         L     GPR15P,ATCLCPTR                                     0495 00141000
*     END; /*LISATCVT*/                                            0496 00142000
*       GPR15P=GPR15P->ATCESC01;                                   0497 00143000
         L     GPR15P,ATCESC01(,GPR15P)                            0497 00144000
*       DCL FMZL0005 BASED(GPR15P) LABEL VALUERANGE(*);            0498 00145000
*       CALL FMZL0005;                                             0499 00146000
         BALR  @14,@15                                             0499 00147000
*       RESPECIFY GPR15P UNRSTD;                                   0500 00148000
*     END;  /***** END OF  TPESC *****/                      /* EXIT TO 00149000
*FEEDBACK FINISH         */                                        0501 00150000
*/********************************************************************/ 00151000
*/*                                                                  */ 00152000
*/* ROUTINE NAME : ENDANAL - END OF LCPB ANALYSIS                    */ 00153000
*/*                                                                  */ 00154000
*/* FUNCTION : SETS THE END OF LCCW FLAG AND TURNS OFF THE DATA EX-  */ 00155000
*/* PECTED FLAG IF THIS IS THE END OF THE LCPB.                      */ 00156000
*/*                                                                  */ 00157000
*/* INPUT : FSBPTR (REG 6) - PTR TO THE FSB                          */ 00158000
*/*         CRRPTR (REG 13) - PTR TO THE CRR                         */ 00159000
*/*                                                                  */ 00160000
*/* OUTPUT : CRRENDLW POSSIBLY SET                                   */ 00161000
*/*          FMCFLGA0 TURNED OFF                                     */ 00162000
*/*                                                                  */ 00163000
*/* SUBROUTINES CALLED : NONE                                        */ 00164000
*/*                                                                  */ 00165000
*/* MACROS USED : NONE                                               */ 00166000
*/*                                                                  */ 00167000
*/********************************************************************/ 00168000
*ENDANAL: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0502 00169000
*                                                                  0502 00170000
         B     @PB00002                                            0502 00171000
ENDANAL  DS    0H                                                  0503 00172000
*   CRRRTN1 = RETPTR;             /* SAVE RETURN ADDRESS             */ 00173000
         ST    RETPTR,CRRRTN1(,CRRPTR)                             0503 00174000
*   IF FSBELI=ON THEN             /* END OF LOGICAL CHANNEL PGM */ 0504 00175000
         TM    FSBELI(FSBPTR),B'10000000'                          0504 00176000
         BNO   @RF00504                                            0504 00177000
*         DO;                     /* ENDING CONDITION MET            */ 00178000
*                                                                  0505 00179000
*         /**************************************/                 0506 00180000
*         /*   END OF LOGICAL CHANNEL PROGRAM   */                 0506 00181000
*         /*   SET SWITCH IN CRR SO ISTDCC32    */                 0506 00182000
*         /*   WILL FREE STORAGE OF ISTDCLCPB.  */                 0506 00183000
*         /*   INDICATE NO MORE DATA EXPECTED   */                 0506 00184000
*         /*   FROM THIS DEVICE.                */                 0506 00185000
*         /**************************************/                 0506 00186000
*                                                                  0506 00187000
*         RFY (REG10,REG11) RSTD;                                  0506 00188000
*    CS1: REG10=FMCCMPS2;          /* REFERENCE OLD FLAG VALUE       */ 00189000
CS1      L     @15,CRRFMCB(,CRRPTR)                                0507 00190000
         L     REG10,FMCCMPS2(,@15)                                0507 00191000
*         REG11=REG10&FLGA0OFF;    /* SET NEW FLAG VALUE             */ 00192000
         LR    REG11,REG10                                         0508 00193000
         N     REG11,@CF03439                                      0508 00194000
*         CS (REG10,REG11,FMCCMPS2);  /* SET NEW VALUE IN FMCB       */ 00195000
         CS    REG10,@11,FMCCMPS2(@15)                             0509 00196000
*         BC (7,CS1);              /* TRY AGAIN IF FAILURE           */ 00197000
         BC    7,CS1                                               0510 00198000
*         RFY (REG10,REG11) UNRSTD;                                0511 00199000
*         CRRENDLW=ON;            /* INDICATE END OF LCCW'S          */ 00200000
         OI    CRRENDLW(CRRPTR),B'00100000'                        0512 00201000
*         END;                                                     0513 00202000
*   ELSE;                         /* MORE DATA TO COME               */ 00203000
*                                                                  0514 00204000
@RF00504 DS    0H                                                  0515 00205000
*   RETPTR = CRRRTN1;             /* GET RETURN ADDRESS              */ 00206000
         L     RETPTR,CRRRTN1(,CRRPTR)                             0515 00207000
*END ENDANAL;                     /* RETURN TO MAIN PROCEDURE        */ 00208000
@EL00002 DS    0H                                                  0516 00209000
@EF00002 DS    0H                                                  0516 00210000
@ER00002 BR    @14                                                 0516 00211000
*/********************************************************************/ 00212000
*/*                                                                  */ 00213000
*/* ROUTINE NAME : HOLDDATA - PRESERVE FSB                           */ 00214000
*/*                                                                  */ 00215000
*/* FUNCTION : HOLDDATA IS ENTERED TO QUEUE A FSB, THE ADDRESS OF    */ 00216000
*/* WHICH IS IN FSBPTR, TO THE FMCB DATA QUEUE SO IT CAN BE SAVED  */   00217000
*/* FOR A LATER READ.  THE FSB WAS RECEIVED FROM TPIOS AND SO        */ 00218000
*/* RESIDES IN FIXED STORAGE.  THE FSB WILL FIRST BE MOVED           */ 00219000
*/* TO PAGEABLE STORAGE SO THAT THE FIXED STORAGE WILL NOT BE        */ 00220000
*/* TIED UP.  THE COUNT OF BUFFERS IN USE WILL BE UPDATED BY THE     */ 00221000
*/* NUMBER OF BUFFERS OCCUPIED BY THE FSB AND FDBS.  IF THE BUFFER   */ 00222000
*/* LIMIT HAS BEEN EXCEEDED, A RESET WILL BE ISSUED TO RESET ANY     */ 00223000
*/* OUTSTANDING I/O SINCE NO BUFFERS ARE AVAILABLE FOR THE           */ 00224000
*/* FEEDBACK.  IF THE FSB TO BE QUEUED IS FROM A SOLICIT AND THE     */ 00225000
*/* FLUSH INDICATOR IS NOW SET, INVOKE THE FLUSH ROUTINE TO FREE     */ 00226000
*/* UP THAT FSB.                                                     */ 00227000
*/*                                                                  */ 00228000
*/* INPUT : CRRFMCB - PTR TO THE FMCB                                */ 00229000
*/*         FSBPTR (REG 6) - PTR TO THE FSB                          */ 00230000
*/*         LCPBPTR (REG 7) - PTR TO THE LCPB                        */ 00231000
*/* OUTPUT : FSB QUEUED THE FMCB DATA QUEUE                          */ 00232000
*/*          POSSIBLY AN LCPB (RESET) QUEUED TO THE TOP              */ 00233000
*/*                                                                  */ 00234000
*/* SUBROUTINES CALLED : ISTDCC61 (EXTERNEL RTN) , LCPBINIT          */ 00235000
*/*                                                                  */ 00236000
*/* MACROS USED : TPQUE(2)                                           */ 00237000
*/*                                                                  */ 00238000
*/********************************************************************/ 00239000
*HOLDDATA: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0517 00240000
HOLDDATA DS    0H                                                  0518 00241000
*RFY ( ATCVTPTR,COUNTER ) RSTD;                                    0518 00242000
*                                                                  0518 00243000
*   CRRRTN1 = RETPTR;             /* SAVE RETURN ADDRESS             */ 00244000
         ST    RETPTR,CRRRTN1(,CRRPTR)                             0519 00245000
*   ATCVTPTR=CRRATCVT;      /* REFERENCE VTAM CVT                    */ 00246000
         L     ATCVTPTR,CRRATCVT(,CRRPTR)                          0520 00247000
*   CRRPTR=ADDR(CRRSAVE1);  /* POINT TO SAVEAREA FOR MOVE            */ 00248000
*                                                                  0521 00249000
         LA    @15,CRRSAVE1(,CRRPTR)                               0521 00250000
         LR    CRRPTR,@15                                          0521 00251000
*   /*****************************************************************/ 00252000
*   /*             MOVE FSB TO PAGEABLE STORAGE                      */ 00253000
*   /*****************************************************************/ 00254000
*                                                                  0522 00255000
*   CALL ATCDCC61;          /* MOVE FSB TO PAGEABLE STORAGE          */ 00256000
         L     @15,ATCDCC61(,ATCVTPTR)                             0522 00257000
         BALR  @14,@15                                             0522 00258000
*   CRRPTR=ADDR(CRRSPACE);  /* REESTABLISH BASE FOR CRR              */ 00259000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0523 00260000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0523 00261000
*   FSBPTR=CRRFSB;          /* REFERENCE NEW FSB                     */ 00262000
         L     FSBPTR,CRRFSB(,CRRPTR)                              0524 00263000
*   FSBELI=OFF;             /* CLEAR SWITCH                          */ 00264000
*   FSBECI=OFF;             /* CLEAR UNUSED SWITCH                   */ 00265000
*                                                                  0526 00266000
         NI    FSBELI(FSBPTR),B'00111111'                          0526 00267000
*   /*****************************************************/        0527 00268000
*   /*    ACCUMULATE COUNT OF BUFFERS IN FSB AND FDB'S   */        0527 00269000
*   /*    AND UPDATE BUFFER COUNT IN FMCB.               */        0527 00270000
*   /*****************************************************/        0527 00271000
*                                                                  0527 00272000
*   COUNTER=0;              /* INIT BUFFER COUNT ACCUMULATOR         */ 00273000
         SLR   COUNTER,COUNTER                                     0527 00274000
*   FDBPTR=ADDR(FSBFDB);    /* REFERENCE FIRST FDB                   */ 00275000
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0528 00276000
*   DO WHILE (FDBPTR^=0);   /* LOOP THROUGH ALL FDB'S                */ 00277000
         B     @DE00529                                            0529 00278000
@DL00529 DS    0H                                                  0530 00279000
*     COUNTER=COUNTER+1;    /* TOTAL NUMBER OF BUFFERS               */ 00280000
         AL    COUNTER,@CF00107                                    0530 00281000
*     FDBPTR=FDBFDBA;       /* REFERENCE NEXT FDB                    */ 00282000
         L     FDBPTR,FDBFDBA(,FDBPTR)                             0531 00283000
*   END;                                                           0532 00284000
@DE00529 LTR   FDBPTR,FDBPTR                                       0532 00285000
         BNZ   @DL00529                                            0532 00286000
*    RFY (REG10,REG11) RSTD;                                       0533 00287000
*CS2:REG10=FMCCMPS1;               /* GET OLD BUFFER COUNT           */ 00288000
CS2      L     @15,CRRFMCB(,CRRPTR)                                0534 00289000
         L     REG10,FMCCMPS1(,@15)                                0534 00290000
*    REG11=REG10+COUNTER;          /* COMPUTE NEW COUNT              */ 00291000
         LR    REG11,REG10                                         0535 00292000
         ALR   REG11,COUNTER                                       0535 00293000
*    CS (REG10,REG11,FMCCMPS1);    /* SET NEW COUNT IN FMCB          */ 00294000
         CS    REG10,@11,FMCCMPS1(@15)                             0536 00295000
*    BC (7,CS2);                   /* TRY AGAIN IF FAILURE           */ 00296000
         BC    7,CS2                                               0537 00297000
*    RFY (REG10,REG11) UNRSTD;                                     0538 00298000
*                                                                  0538 00299000
*/*  TPQUE (FSBPTR,FMCFBA)  */                                     0539 00300000
*                                                                  0539 00301000
*    DO;                                                           0539 00302000
*    RFY (GPR00P,GPR01P,GPR02P,GPR14P,GPR15P) RSTD;                0540 00303000
*    GEN REFS(GPR01P) SETS(RPHWORK)                                0541 00304000
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0541 00305000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00306000
*    GPR15P=FSBPTR; /* GET WORK ELEM POINTER */                    0542 00307000
         LR    GPR15P,FSBPTR                                       0542 00308000
*    GPR14P=ADDR(FMCFBA); /* ADDRESS PAB/QUEUE POINTER */          0543 00309000
         L     GPR14P,CRRFMCB(,CRRPTR)                             0543 00310000
         LA    GPR14P,FMCFBA(,GPR14P)                              0543 00311000
*    GPR15P=GPR15P|PABWEQG1;                                       0544 00312000
         O     GPR15P,@CF02699                                     0544 00313000
*    GPR02P=GPR14P->PABWEQA;/* INITIALIZE WITH QUEUE HEADER */     0545 00314000
         L     GPR02P,PABWEQA(,GPR14P)                             0545 00315000
*CMZA0013:; /* CS FAIL TARGET */                                   0546 00316000
CMZA0013 DS    0H                                                  0547 00317000
*    GPR00P=GPR02P; /* SAVE QUEUE CONTENTS */                      0547 00318000
         LR    GPR00P,GPR02P                                       0547 00319000
*    GPR15P->PABCHAIN=GPR02P|PABWEQG1; /*FORWARD CHAIN WORK ELEMENT*/   00320000
         LR    @08,GPR02P                                          0548 00321000
         O     @08,@CF02699                                        0548 00322000
         ST    @08,PABCHAIN(,GPR15P)                               0548 00323000
*    CS   (GPR02P,GPR15P,GPR14P->PABWEQA); /* CHECK FOR END OF QUEUE */ 00324000
         CS    GPR02P,@15,PABWEQA(GPR14P)                          0549 00325000
*    BC (7,CMZA0013);                                              0550 00326000
         BC    7,CMZA0013                                          0550 00327000
*    GEN REFS(RPHWORK,GPR01P) NOSETS                               0551 00328000
*        (LM    GPR02P,GPR08P,RPHWORK(GPR01P));                    0551 00329000
         LM    GPR02P,GPR08P,RPHWORK(GPR01P)                            00330000
*    RFY (GPR00P,GPR01P,GPR02P,GPR14P,GPR15P) UNRSTD;              0552 00331000
*    END; /**** END OF TPQUE GENERATION ****/        /* QUEUE FSB TO FM 00332000
*B DATA QUEUE   */                                                 0553 00333000
*   IF FMCBUFNB>FMCBUFLM THEN      /* IS BUFFER LIMIT EXCEEDED?      */ 00334000
         L     @15,CRRFMCB(,CRRPTR)                                0554 00335000
         CLC   FMCBUFNB(2,@15),FMCBUFLM(@15)                       0554 00336000
         BNH   @RF00554                                            0554 00337000
*      IF FMCFLGA5=OFF THEN        /* IS BUFFER THRESHOLD EXCEEDED?  */ 00338000
         TM    FMCFLGA5(@15),B'00000100'                           0555 00339000
         BNZ   @RF00555                                            0555 00340000
*        DO;                                                       0556 00341000
*                                                                  0556 00342000
*          /*****************************************/             0557 00343000
*          /*     BUFFER THRESHOLD EXCEEDED         */             0557 00344000
*          /*****************************************/             0557 00345000
*                                                                  0557 00346000
*          RFY (REG10,REG11) RSTD;                                 0557 00347000
*     CS3: REG10=FMCCMPS2;         /* GET OLD FLAG VALUE              * 00348000
CS3      L     @15,CRRFMCB(,CRRPTR)                                0558 00349000
         L     REG10,FMCCMPS2(,@15)                                0558 00350000
*          REG11=REG10|FLGA5ON;    /* SET NEW FLAG VALUE              * 00351000
         LR    REG11,REG10                                         0559 00352000
         O     REG11,@CF01427                                      0559 00353000
*          CS (REG10,REG11,FMCCMPS2); /* SET NEW VALUE IN FMCB        * 00354000
         CS    REG10,@11,FMCCMPS2(@15)                             0560 00355000
*          BC (7,CS3);             /* TRY AGAIN IF FAILURE            * 00356000
         BC    7,CS3                                               0561 00357000
*          RFY (REG10,REG11) UNRSTD;                               0562 00358000
*          CALL LCPBINIT;          /* GET AND INIT LCCW FOR RESET    */ 00359000
*                                                                  0563 00360000
         BAL   @14,LCPBINIT                                        0563 00361000
*          /********************************************/          0564 00362000
*          /*  RESET ANY OUTSTANDING I/O. BUILD RESET  */          0564 00363000
*          /*  ACCORDING TO DEVICE TYPE AND QUEUE TO   */          0564 00364000
*          /*  TPIOS OUTBOUND PAB.                     */          0564 00365000
*          /********************************************/          0564 00366000
*                                                                  0564 00367000
*         IF DEVCHAR2>=DEV2980&DEVCHAR2<=DEV3275 THEN /*2980 OR 3270? * 00368000
         L     @15,CRRFMCB(,CRRPTR)                                0564 00369000
         LA    @08,36                                              0564 00370000
         AL    @08,FMCFMCBA(,@15)                                  0564 00371000
         CLI   DEVCHAR2(@08),X'18'                                 0564 00372000
         BL    @RF00564                                            0564 00373000
         CLI   DEVCHAR2(@08),X'1C'                                 0564 00374000
         BH    @RF00564                                            0564 00375000
*            LCCWOP=LXEC;         /* RESET AT END OF CMD             */ 00376000
         L     @15,LCPNXLCW(,LCPBPTR)                              0565 00377000
         MVI   LCCWOP(@15),X'4B'                                   0565 00378000
*         ELSE IF DEVCRVB=ON THEN                                  0566 00379000
         B     @RC00564                                            0566 00380000
@RF00564 L     @15,CRRFMCB(,CRRPTR)                                0566 00381000
         L     @15,FMCFMCBA(,@15)                                  0566 00382000
         TM    DEVCRVB+36(@15),B'00100000'                         0566 00383000
         BNO   @RF00566                                            0566 00384000
*                  LCCWOP=LXI;     /* RESET IMMEDIATE                */ 00385000
         L     @15,LCPNXLCW(,LCPBPTR)                              0567 00386000
         MVI   LCCWOP(@15),X'2B'                                   0567 00387000
*          ELSE IF DEVCBSC=ON THEN                                 0568 00388000
         B     @RC00566                                            0568 00389000
@RF00566 L     @15,CRRFMCB(,CRRPTR)                                0568 00390000
         L     @15,FMCFMCBA(,@15)                                  0568 00391000
         TM    DEVCBSC+36(@15),B'10000000'                         0568 00392000
         BNO   @RF00568                                            0568 00393000
*                  LCCWOP=LXO;     /* RESET ORDERLY                  */ 00394000
         L     @15,LCPNXLCW(,LCPBPTR)                              0569 00395000
         MVI   LCCWOP(@15),X'5B'                                   0569 00396000
*               ELSE                                               0570 00397000
*                  LCCWOP=LXEC;    /* RESET AT END OF COMMAND        */ 00398000
         B     @RC00568                                            0570 00399000
@RF00568 L     @15,LCPNXLCW(,LCPBPTR)                              0570 00400000
         MVI   LCCWOP(@15),X'4B'                                   0570 00401000
*          DCLSRESW=ON;            /* INDICATE SPECIAL RESET COMMAND */ 00402000
@RC00568 DS    0H                                                  0571 00403000
@RC00566 DS    0H                                                  0571 00404000
@RC00564 DS    0H                                                  0572 00405000
*          DCLBTEIS=ON;            /* INDICATE BUFFER THRESHOLD    0572 00406000
*                                     EXCEEDED, INPUT STOPPED        */ 00407000
*                                                                  0572 00408000
         OI    DCLSRESW(LCPBPTR),B'10010000'                       0572 00409000
*/*  TPQUE (LCPBPTR,FMCPAB3) RESET */                              0573 00410000
*                                                                  0573 00411000
*           DO;                                                    0573 00412000
*           RFY (GPR00P,GPR01P,GPR02P,GPR15P) RSTD;                0574 00413000
*           GEN REFS(GPR01P) SETS(RPHWORK)                         0575 00414000
*        (STM   GPR14P,GPR09P,RPHWORK(GPR01P));                    0575 00415000
         STM   GPR14P,GPR09P,RPHWORK(GPR01P)                            00416000
*           GPR00P=ADDR(FMCPAB3);                                  0576 00417000
         L     @08,CRRFMCB(,CRRPTR)                                0576 00418000
         LA    GPR00P,FMCPAB3(,@08)                                0576 00419000
*           GPR02P=LCPBPTR;                                        0577 00420000
         LR    GPR02P,LCPBPTR                                      0577 00421000
*           CALL ATCLCPTR->ATCAP34->ISTAPC34;                      0578 00422000
         L     @08,ATCLCPTR                                        0578 00423000
         L     @15,ATCAP34(,@08)                                   0578 00424000
         BALR  @14,@15                                             0578 00425000
*           GEN REFS(RPHWORK,GPR01P) NOSETS                        0579 00426000
*        (LM    GPR14P,GPR09P,RPHWORK(GPR01P));                    0579 00427000
         LM    GPR14P,GPR09P,RPHWORK(GPR01P)                            00428000
*           RFY (GPR00P,GPR01P,GPR02P,GPR15P) UNRSTD;              0580 00429000
*           END; /**** END OF TPQUE GENERATION ****/ /* SHIP RESET TO T 00430000
*IOS    */                                                         0581 00431000
*        END;                                                      0582 00432000
*      ELSE;                       /* BUFFER THRESHOLD PREVIOUSLY  0583 00433000
*                                     EXCEEDED                       */ 00434000
@RF00555 DS    0H                                                  0584 00435000
*   ELSE;                          /* BUFFER LIMIT NOT EXCEEDED      */ 00436000
@RF00554 DS    0H                                                  0585 00437000
*   IF CRRSW6=ON THEN              /* IS DATA FROM SOLICIT           */ 00438000
         TM    CRRSW6(CRRPTR),B'00000010'                          0585 00439000
         BNO   @RF00585                                            0585 00440000
*      IF FMCFLGA3=ON THEN         /* IS FLUSH INDICATOR NOW SET?    */ 00441000
*                                                                  0586 00442000
         L     @15,CRRFMCB(,CRRPTR)                                0586 00443000
         TM    FMCFLGA3(@15),B'00010000'                           0586 00444000
         BNO   @RF00586                                            0586 00445000
*          /*****************************************/             0587 00446000
*          /*   FSB FROM SOLICIT REQUEST WAS        */             0587 00447000
*          /*   QUEUED BUT NOW THE FLUSH INDICATOR  */             0587 00448000
*          /*   IS SET.  INVOKE FLUSH ROUTINE TO    */             0587 00449000
*          /*   DEQUEUE FSB AND THROW IT AWAY.      */             0587 00450000
*          /*****************************************/             0587 00451000
*                                                                  0587 00452000
*          CALL FLUSHRTN;          /* THROW AWAY DATA                */ 00453000
         BAL   @14,FLUSHRTN                                        0587 00454000
*      ELSE;                       /* FLUSH INDICATOR NOT SET        */ 00455000
@RF00586 DS    0H                                                  0589 00456000
*   ELSE;                          /* DATA NOT FROM SOLICIT          */ 00457000
@RF00585 DS    0H                                                  0590 00458000
*   RETPTR = CRRRTN1;             /* GET RETURN ADDRESS              */ 00459000
         L     RETPTR,CRRRTN1(,CRRPTR)                             0590 00460000
*RFY ( ATCVTPTR,COUNTER ) UNRSTD;                                  0591 00461000
*END HOLDDATA;                     /* RETURN TO MAIN PROCEDURE        * 00462000
@EL00003 DS    0H                                                  0592 00463000
@EF00003 DS    0H                                                  0592 00464000
@ER00003 BR    @14                                                 0592 00465000
*/********************************************************************/ 00466000
*/*                                                                  */ 00467000
*/* ROUTINE NAME : LDOPROC - LDO PROCESSING                          */ 00468000
*/*                                                                  */ 00469000
*/* FUNCTION : FIND THE LDO ASSOCIATED WITH THE FSB IF THE           */ 00470000
*/* DATA RECEIVED IS FOR A SOLICIT, FEEDBACK PROCESSING IS           */ 00471000
*/* COMPLETE.  THE DATA WILL BE SAVED FOR A SUBSEQUENT READ          */ 00472000
*/* REQUEST.  OTHERWISE, INVOKE MOVEPROC TO MOVE THE DATA FROM       */ 00473000
*/* THE FSB TO THE USER'S BUFFER.                                    */ 00474000
*/*                                                                  */ 00475000
*/* INPUT : LCPBPTR (REG 7) - PTR TO THE LCPB                        */ 00476000
*/*         LDOPTR (REG 9) - PTR THE LDO                             */ 00477000
*/*         CRRPTR (REG 13) - PTR TO THE CRR                         */ 00478000
*/*                                                                  */ 00479000
*/* OUTPUT : CRR FEEDBACK FIELDS INITIALIZED                         */ 00480000
*/*          DATA MOVED TO THE USER'S BUFFER                         */ 00481000
*/*                                                                  */ 00482000
*/* SUBROUTINES CALLED : MOVEPROC , FINDLDO , FREEFSB                */ 00483000
*/*                                                                  */ 00484000
*/* MACROS USED : NONE                                               */ 00485000
*/*                                                                  */ 00486000
*/********************************************************************/ 00487000
*LDOPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0593 00488000
LDOPROC  DS    0H                                                  0594 00489000
*  DO; /*MODESET  EXTKEY(ZERO)*/                                   0594 00490000
*    SPKA (0); /*SET PSW KEY*/                                     0595 00491000
         SLR   @15,@15                                             0595 00492000
         SPKA  0(@15)                                              0595 00493000
*  END; /*MODESET  EXTKEY(ZERO)*//*GET SUPER KEY TO REFER LDO @Z30APGE* 00494000
*                                                                  0596 00495000
*   CRRRTN2 = RETPTR;           /* SAVE RETURN ADDRESS             */   00496000
         ST    RETPTR,CRRRTN2(,CRRPTR)                             0597 00497000
*   DCLRPLA=DCLRPLA&&DCLRPLA;      /* CLEAR RPL PTR IN LCPB         */  00498000
         L     @15,DCLRPLA(,LCPBPTR)                               0598 00499000
         LR    @08,@15                                             0598 00500000
         XR    @08,@15                                             0598 00501000
         ST    @08,DCLRPLA(,LCPBPTR)                               0598 00502000
*   CALL FINDLDO;                 /* FIND THE LDO AND VALIDATE IT    */ 00503000
         BAL   @14,FINDLDO                                         0599 00504000
*        IF LDOCMD=LDOSLCT THEN                                    0600 00505000
         CLI   LDOCMD(LDOPTR),X'F2'                                0600 00506000
         BNE   @RF00600                                            0600 00507000
*              DO;                /* SOLICIT REQUEST-PROCESSING DONE */ 00508000
*                                                                  0601 00509000
*              /**********************************************/    0602 00510000
*              /*  SOLICIT LDO.  PROCESSING FINISHED.  SET   */    0602 00511000
*              /*  NORMAL FEEDBACK STATUS IN CRR.            */    0602 00512000
*              /**********************************************/    0602 00513000
*                                                                  0602 00514000
*              CRRRTNCD=CRRNOERR;  /* SET RETURN CODE OF 0           */ 00515000
         MVI   CRRRTNCD(CRRPTR),X'00'                              0602 00516000
*              CRRFDB2=CRRNOERR;   /* SET MINOR RETURN CODE OF 0     */ 00517000
         MVI   CRRFDB2(CRRPTR),X'00'                               0603 00518000
*              CRRFDB3=CRRNOERR;   /* SET DATA FLAGS TO 0            */ 00519000
         MVI   CRRFDB3(CRRPTR),X'00'                               0604 00520000
*              CRRPOST=ON;        /* RPL SHOULD BE POSTED COMPLETE   */ 00521000
         OI    CRRPOST(CRRPTR),B'00010000'                         0605 00522000
*              CRRSW1=ON;         /* SAVE FSB FOR A LATER READ       */ 00523000
         OI    CRRSW1(CRRPTR),B'01000000'                          0606 00524000
*              END;                                                0607 00525000
*        ELSE DO;      /* MOVE DATA TO USER'S BUFFER      */       0608 00526000
*                                                                  0608 00527000
         B     @RC00600                                            0608 00528000
@RF00600 DS    0H                                                  0609 00529000
*             /*******************************************/        0609 00530000
*             /* MOVE FEEDBACK STATUS FROM FSB TO CRR    */        0609 00531000
*             /*******************************************/        0609 00532000
*                                                                  0609 00533000
*             CRRRTNCD=FSBSRCAT;  /* MOVE MAJOR RETURN CODE          */ 00534000
         MVC   CRRRTNCD(1,CRRPTR),FSBSRCAT(FSBPTR)                 0609 00535000
*             CRRFDB2=FSBMNRCD;   /* MOVE MINOR RETURN CODE          */ 00536000
         MVC   CRRFDB2(1,CRRPTR),FSBMNRCD(FSBPTR)                  0610 00537000
*             CRRFDB3=FSBERC;     /* COPY DATA FLAGS                 */ 00538000
         MVC   CRRFDB3(1,CRRPTR),FSBERC(FSBPTR)                    0611 00539000
*             CRRUINPT=OFF;       /* RESET UNSOLICITED INPUT FLAG    */ 00540000
*             CRRUNUSD=OFF;       /* RESET UNUSED BIT                */ 00541000
         NI    CRRUINPT(CRRPTR),B'00111111'                        0613 00542000
*             CRRESR2=FSBEXTRS;    /* EXTENDED SYSTEM RESPONSE       */ 00543000
         MVC   CRRESR2(1,CRRPTR),FSBEXTRS(FSBPTR)                  0614 00544000
*             CRRESR1=FSBSYSRS;  /* SYSTEM RESPONSE                 */  00545000
         MVC   CRRESR1(1,CRRPTR),FSBSYSRS(FSBPTR)                  0615 00546000
*             IF CRRRTNCD=CRRPHYSC THEN                            0616 00547000
         CLI   CRRRTNCD(CRRPTR),X'0C'                              0616 00548000
         BNE   @RF00616                                            0616 00549000
*             IF CRRSTSAV=ON THEN                                  0617 00550000
         TM    CRRSTSAV(CRRPTR),B'00000001'                        0617 00551000
         BNO   @RF00617                                            0617 00552000
*                CRRDSB=FSBDMGRS;  /* COPY SENSE BYTE                */ 00553000
         MVC   CRRDSB(2,CRRPTR),FSBDMGRS(FSBPTR)                   0618 00554000
*             ELSE;                /* NO SENSE DATA AVAILABLE        */ 00555000
@RF00617 DS    0H                                                  0620 00556000
*             ELSE;                                                0620 00557000
*                                                                  0620 00558000
@RF00616 DS    0H                                                  0621 00559000
*             /*******************************************/        0621 00560000
*             /*    MOVE DATA TO USER'S BUFFERS          */        0621 00561000
*             /*******************************************/        0621 00562000
*                                                                  0621 00563000
*             CALL MOVEPROC;       /* MOVE DATA TO USER'S BUFFER     */ 00564000
         BAL   @14,MOVEPROC                                        0621 00565000
*             END;                                                 0622 00566000
*                                                                  0622 00567000
*                                                                  0622 00568000
*   /*****************************************************************/ 00569000
*   /* IF CRRSW1 IS OFF, THEN ALL THE DATA ASSOCIATED WITH THE       */ 00570000
*   /* FSB HAS BEEN MOVED SO THE FSB CAN BE FREED.  OTHERWISE,       */ 00571000
*   /* HOLDDATA WILL BE INVOKED IN THE MAIN PROCEEDURE TO SAVE       */ 00572000
*   /* THE FSB.                                                      */ 00573000
*   /*****************************************************************/ 00574000
*                                                                  0623 00575000
*   IF CRRSW1=OFF THEN                                             0623 00576000
@RC00600 TM    CRRSW1(CRRPTR),B'01000000'                          0623 00577000
         BNZ   @RF00623                                            0623 00578000
*         CALL FREEFSB;           /* FREE STORAGE OCCUPIED BY FSB    */ 00579000
         BAL   @14,FREEFSB                                         0624 00580000
*   ELSE;                         /* FSB WILL BE SAVED               */ 00581000
@RF00623 DS    0H                                                  0626 00582000
*  DO; /*MODESET  EXTKEY(VTAM)*/                                   0626 00583000
*    SPKA (96); /*SET PSW KEY*/                                    0627 00584000
         LA    @15,96                                              0627 00585000
         SPKA  0(@15)                                              0627 00586000
*  END; /*MODESET  EXTKEY(VTAM)*//*GET VTAM KEY @Z30APGE*/         0628 00587000
*   RETPTR = CRRRTN2;             /* GET RETURN ADDRESS              */ 00588000
         L     RETPTR,CRRRTN2(,CRRPTR)                             0629 00589000
*END LDOPROC;                      /* RETURN TO CALLER                * 00590000
@EL00004 DS    0H                                                  0630 00591000
@EF00004 DS    0H                                                  0630 00592000
@ER00004 BR    @14                                                 0630 00593000
*/********************************************************************/ 00594000
*/*                                                                  */ 00595000
*/* ROUTINE NAME : MOVEPROC - MOVE PROCEDURE                         */ 00596000
*/*                                                                  */ 00597000
*/* FUNCTION : THIS RTN SETS UP THE INPUT PARAMETERS TO THE MOVE     */ 00598000
*/* ROUTINE.  IF A PREVIOUS MOVE HAD OCCURRED (RESUME DATA ADDRESS   */ 00599000
*/* IN LCPB NOT ZERO) THEN THE PARAMETERS ARE REINITIALIZED FROM     */ 00600000
*/* THE LCPB WHERE THEY WERE SAVED.  OTHERWISE, THEY ARE             */ 00601000
*/* INITIALIZED FROM FIELDS IN THE CRR AND LDO.  THE MOVE            */ 00602000
*/* ROUTINE IS INVOKED TO MOVE THE DATA, AND UPON RETURN,            */ 00603000
*/* MOVEFDBK IS CALLED TO ANALYZE THE RESULTS.                       */ 00604000
*/*                                                                  */ 00605000
*/* INPUT : LCPBPTR (REG 7) - PTR TO THE LCPB                        */ 00606000
*/*                                                                  */ 00607000
*/* OUTPUT : DATA MOVED                                              */ 00608000
*/*                                                                  */ 00609000
*/* SUBROUTINES CALLED : MOVEFDBK , ISTDCC60(EXTERNAL)               */ 00610000
*/*                                                                  */ 00611000
*/* MACROS USED : VMDESET(2) , VSETLCK(2)                            */ 00612000
*/*                                                                  */ 00613000
*/********************************************************************/ 00614000
*MOVEPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0631 00615000
MOVEPROC DS    0H                                                  0632 00616000
*RFY ATCVTPTR RSTD;                                                0632 00617000
*                                                                  0632 00618000
*   CRRRTN3 = RETPTR;             /* SAVE RETURN ADDRESS             */ 00619000
         ST    RETPTR,CRRRTN3(,CRRPTR)                             0633 00620000
*   IF DCLIOBUF ^= 0 THEN                                          0634 00621000
         L     @15,DCLIOBUF(,LCPBPTR)                              0634 00622000
         LTR   @15,@15                                             0634 00623000
         BZ    @RF00634                                            0634 00624000
*         DO;                     /* CONTINUE PREVIOUS MOVE          */ 00625000
*                                                                  0635 00626000
*         /**********************************************/         0636 00627000
*         /*   DATA PARTIALLY MOVED.  SET UP PARAMETERS */         0636 00628000
*         /*   FOR MOVE FROM LCPB.                      */         0636 00629000
*         /**********************************************/         0636 00630000
*                                                                  0636 00631000
*         CRRBARAD=DCLIOBUF;      /* RESUME BUFFER ADDRESS           */ 00632000
         ST    @15,CRRBARAD(,CRRPTR)                               0636 00633000
*         CRRBLEN=DCLIOLEN;       /* REMAINING LENGTH                */ 00634000
         LH    @08,DCLIOLEN(,LCPBPTR)                              0637 00635000
         STH   @08,CRRBLEN(,CRRPTR)                                0637 00636000
*         CRRBLDAD=DCLCLDOA;      /* RESUME LDO ADDRESS              */ 00637000
         L     @04,DCLCLDOA(,LCPBPTR)                              0638 00638000
         ST    @04,CRRBLDAD(,CRRPTR)                               0638 00639000
*         CRRBTLEN=DCLBTLEN;      /* TOTAL LENGTH MOVED SO FAR       */ 00640000
         L     @02,DCLBTLEN(,LCPBPTR)                              0639 00641000
         ST    @02,CRRBTLEN(,CRRPTR)                               0639 00642000
*         DCLIOBUF=DCLIOBUF&&DCLIOBUF;  /* CLEAR OUT DATA ADDRESS    */ 00643000
         LR    @00,@15                                             0640 00644000
         XR    @00,@15                                             0640 00645000
         ST    @00,DCLIOBUF(,LCPBPTR)                              0640 00646000
*         DCLIOLEN=DCLIOLEN&&DCLIOLEN;  /* ZERO RESUME LENGTH        */ 00647000
         LR    @15,@08                                             0641 00648000
         XR    @15,@08                                             0641 00649000
         STH   @15,DCLIOLEN(,LCPBPTR)                              0641 00650000
*         DCLCLDOA=DCLCLDOA&&DCLCLDOA;  /* ZERO RESUME LENGTH        */ 00651000
         LR    @15,@04                                             0642 00652000
         XR    @15,@04                                             0642 00653000
         ST    @15,DCLCLDOA(,LCPBPTR)                              0642 00654000
*         DCLBTLEN=DCLBTLEN&&DCLBTLEN;  /* CLEAR LENGTH ACCUMULATOR */  00655000
         LR    @15,@02                                             0643 00656000
         XR    @15,@02                                             0643 00657000
         ST    @15,DCLBTLEN(,LCPBPTR)                              0643 00658000
*         END;                                                     0644 00659000
*   ELSE DO;                      /* SET UP FOR FIRST MOVE           */ 00660000
*                                                                  0645 00661000
         B     @RC00634                                            0645 00662000
@RF00634 DS    0H                                                  0646 00663000
*        /**********************************************/          0646 00664000
*        /*   NO DATA YET MOVED.  SET UP PARAMETERS    */          0646 00665000
*        /*   FOR MOVE FROM LDO.                       */          0646 00666000
*        /**********************************************/          0646 00667000
*                                                                  0646 00668000
*        CRRBLDAD=CRRCLDO;        /* LDO ADDRESS FOR MOVE            */ 00669000
         L     @15,CRRCLDO(,CRRPTR)                                0646 00670000
         ST    @15,CRRBLDAD(,CRRPTR)                               0646 00671000
*        CRRBLEN=LDOLEN;          /* SET BUFFER LENGTH FROM LDO      */ 00672000
         LH    @15,LDOLEN(,LDOPTR)                                 0647 00673000
         N     @15,@CF04418                                        0647 00674000
         STH   @15,CRRBLEN(,CRRPTR)                                0647 00675000
*        CRRBARAD=LDOADDR;        /* SET BUFFER ADDR FROM LDO        */ 00676000
         L     @15,LDOADDR(,LDOPTR)                                0648 00677000
         ST    @15,CRRBARAD(,CRRPTR)                               0648 00678000
*        CRRBTLEN=CRRBTLEN&&CRRBTLEN;   /* SET TOTAL LEN MOVED TO 0  */ 00679000
         L     @15,CRRBTLEN(,CRRPTR)                               0649 00680000
         LR    @08,@15                                             0649 00681000
         XR    @08,@15                                             0649 00682000
         ST    @08,CRRBTLEN(,CRRPTR)                               0649 00683000
*        END;                                                      0650 00684000
*ATCVTPTR=CRRATCVT;/*REFERENCE VTAM CVT*/                          0651 00685000
@RC00634 L     ATCVTPTR,CRRATCVT(,CRRPTR)                          0651 00686000
*RFY (GPR10F,GPR14F) RSTD;/*@Z30APGE*/                             0652 00687000
*STM (GPR10F,GPR14F,CRRSAVE);/*SAVE REGS ACROSS SETLCK @Z30APGE*/  0653 00688000
         STM   GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0653 00689000
*GPR10F=CRRPTR;/*SAVE REG 13 @Z30APGE*/                            0654 00690000
*                                                                  0654 00691000
         LR    GPR10F,CRRPTR                                       0654 00692000
* /*****************************************************************/   00693000
* /*    CHANGE TO KEY ZERO AND OBTAIN LOCAL MEMORY LOCK            */   00694000
* /*****************************************************************/   00695000
*                                                                  0655 00696000
*                                                                  0655 00697000
*C40LOCK:  DO; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) RELATED(LOC 00698000
*L,ISTDCC40(C40UNLCK)) */                                          0655 00699000
C40LOCK  DS    0H                                                  0656 00700000
*            RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;               0656 00701000
*            GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE TABLE. */   00702000
         L     GPR13P,PSALITA                                      0657 00703000
*            GEN(LM GPR11P,GPR13P,576(GPR13P)); /* LOAD LOCKWORD, CLHT  00704000
*ND ENTRY ADRS. */                                                 0658 00705000
         LM GPR11P,GPR13P,576(GPR13P)                                   00706000
*            GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN. */   0659 00707000
         BALR GPR14P,GPR13P                                             00708000
*            RFY (GPR11P,GPR12P,GPR13P,GPR14P) UNRSTD;             0660 00709000
*          END; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) RELATED(LO 00710000
*AL,ISTDCC40(C40UNLCK)) *//*@Z30APGE*/                             0661 00711000
* CRRPTR=GPR10F; /*RESTORE REG 13 @Z30APGE*/                       0662 00712000
         LR    CRRPTR,GPR10F                                       0662 00713000
*  LM(GPR10F,GPR14F,CRRSAVE);/*RESTORE REGS @Z30APGE*/             0663 00714000
         LM    GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0663 00715000
*  RFY (GPR10F,GPR14F) UNRSTD;/*@Z30APGE*/                         0664 00716000
*  CRRPTR=ADDR(CRRSAVE1);/*GET SAVEAREA FOR DCC60 @Z30APGE*/       0665 00717000
*                                                                  0665 00718000
         LA    @15,CRRSAVE1(,CRRPTR)                               0665 00719000
         LR    CRRPTR,@15                                          0665 00720000
*   /*********************************************/                0666 00721000
*   /*   CALL ISTDCC60 TO MOVE DATA TO USER'S    */                0666 00722000
*   /*   BUFFER.                                 */                0666 00723000
*   /*********************************************/                0666 00724000
*                                                                  0666 00725000
*   CALL ATCDCC60;                /* MOVE THE DATA                   */ 00726000
         L     @15,ATCDCC60(,ATCVTPTR)                             0666 00727000
         BALR  @14,@15                                             0666 00728000
*   CRRPTR=ADDR(CRRSPACE); /* REESTABLISH BASE FOR CRR       */    0667 00729000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0667 00730000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0667 00731000
*   RPLRLEN=CRRBTLEN;             /* SAVE AMT MOVED IN USER'S RPL    */ 00732000
*                                                                  0668 00733000
         L     @15,CRRARPL(,CRRPTR)                                0668 00734000
         L     @08,CRRBTLEN(,CRRPTR)                               0668 00735000
         ST    @08,RPLRLEN(,@15)                                   0668 00736000
*  /**********************************************************/    0669 00737000
*  /*  RELEASE LOCAL MEMORY LOCK */                                0669 00738000
*  /**********************************************************/    0669 00739000
*                                                                  0669 00740000
* RFY (GPR10F,GPR14F) RSTD;/*@Z30APGE*/                            0669 00741000
* STM(GPR10F,GPR14F,CRRSAVE);/*SAVE REGS ACROSS SETLCK @Z30APGE*/  0670 00742000
         STM   GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0670 00743000
* GPR10F=CRRPTR;/*SAVE REG 13 @Z30APGE*/                           0671 00744000
*                                                                  0671 00745000
         LR    GPR10F,CRRPTR                                       0671 00746000
*C40UNLCK:  DO; /* SETLOCK (RELEASE) TYPE(LOCAL) RELATED(LOCAL,ISTDCC40 00747000
*C40LOCK)) */ RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;              0672 00748000
C40UNLCK DS    0H                                                  0673 00749000
*             GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE TABLE. */  00750000
         L     GPR13P,PSALITA                                      0674 00751000
*             GEN(LM GPR11P,GPR13P,588(GPR13P)); /* LOAD LOCKWORD, CLHT 00752000
*AND ENTRY ADRS. */                                                0675 00753000
         LM GPR11P,GPR13P,588(GPR13P)                                   00754000
*             GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN. */  0676 00755000
