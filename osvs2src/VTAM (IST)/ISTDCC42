         CL    @11,WKECHAIN(,GPR14P)                               0530 00389000
         BNE   @DL00527                                            0530 00390000
*           GPR00P=FMZCA030;                                       0531 00391000
         LR    GPR00P,@11                                          0531 00392000
*           CS(GPR14P,GPR00P,GPR15P->PABWEQA);                     0532 00393000
         CS    GPR14P,@00,PABWEQA(GPR15P)                          0532 00394000
*           BC(7,FMZL1034);                                        0533 00395000
         BC    7,FMZL1034                                          0533 00396000
*           GPR14P->WKECHAIN=0;                                    0534 00397000
         SLR   @11,@11                                             0534 00398000
         ST    @11,WKECHAIN(,GPR14P)                               0534 00399000
*         END;                                                     0535 00400000
*ISTA0034:                                                         0536 00401000
*       ;                                                          0536 00402000
@RT00523 DS    0H                                                  0536 00403000
ISTA0034 DS    0H                                                  0537 00404000
*       GPR14P=GPR14P&FMZCB030;                                    0537 00405000
         N     GPR14P,@CF02465                                     0537 00406000
*       GPR01P->RPHWEA=GPR14P;                                     0538 00407000
         ST    GPR14P,RPHWEA(,GPR01P)                              0538 00408000
*       RFY                                                        0539 00409000
*        (GPR00P,                                                  0539 00410000
*         GPR01P,                                                  0539 00411000
*         GPR14P,                                                  0539 00412000
*         GPR15P) UNRSTD;                                          0539 00413000
*     END;                          /* END TPDEQ GENERATION DEQUE RPL*/ 00414000
*   END;                                                           0541 00415000
@DE00513 L     @15,RPHWEA(,ADDRRPH)                                0541 00416000
         LTR   @15,@15                                             0541 00417000
         BZ    @DC00513                                            0541 00418000
         CL    @15,@CF02372                                        0541 00419000
         BNE   @DL00513                                            0541 00420000
@DC00513 DS    0H                                                  0542 00421000
*   REG14=CRRRTN1;                  /* RESTORE RETURN ADDRESS        */ 00422000
         L     REG14,CRRRTN1(,ADDRCRR)                             0542 00423000
*   END POSTRPL;                    /* RETURN TO MAIN PROCEDURE      */ 00424000
@EL00002 DS    0H                                                  0543 00425000
@EF00002 DS    0H                                                  0543 00426000
@ER00002 BR    @14                                                 0543 00427000
*                                                                  0544 00428000
*/********************************************************************/ 00429000
*/*                                                                  */ 00430000
*/* ROUTINE NAME : REQUE - RE- QUEUE THE RESET                       */ 00431000
*/*                                                                  */ 00432000
*/* FUNCTION : THE RESET UNCONDITIONAL OPERATION HAS FAILED, AND MUST*/ 00433000
*/*      BE RE-SCHEDULED. THE LCPB FLAGS ARE CLEARED AND THE LCPB IS */ 00434000
*/*      QUED TO THE TPIOS OUTBOUND PAB.                             */ 00435000
*/*                                                                  */ 00436000
*/* INPUT : ADDRCRR (REG 13) - PTR TO CRR                            */ 00437000
*/*         ADDRFSB (REG 3) - PTR TO THE FSB                         */ 00438000
*/*                                                                  */ 00439000
*/* OUTPUT : THE LCPB QUEUED TO THE TOP                              */ 00440000
*/*                                                                  */ 00441000
*/* SUBROUTINES CALLED : NONE                                        */ 00442000
*/*                                                                  */ 00443000
*/* MACROS USED : TPQUE                                              */ 00444000
*/*                                                                  */ 00445000
*/********************************************************************/ 00446000
*                                                                  0544 00447000
*REQUE:                                                            0544 00448000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0544 00449000
REQUE    DS    0H                                                  0545 00450000
*   CRRRTN4=REG14;                  /* SAVE RETURN ADDR      @D31KKNL*/ 00451000
         ST    REG14,CRRRTN4(,ADDRCRR)                             0545 00452000
*   LCPFLAGS='00'X;                 /* CLEAR THE LCPB FLAGS          */ 00453000
         L     @15,CRRLCPB(,ADDRCRR)                               0546 00454000
         MVI   LCPFLAGS(@15),X'00'                                 0546 00455000
*   LCPMNRCD='00'X;                 /* CLEAR THE MINOR RET. CODE     */ 00456000
         MVI   LCPMNRCD(@15),X'00'                                 0547 00457000
*   LCPNXLCW=ADDR(DCLLCCW2);        /* GET ADDR OF SECOND LCCW       */ 00458000
         LA    @11,DCLLCCW2(,@15)                                  0548 00459000
         ST    @11,LCPNXLCW(,@15)                                  0548 00460000
*   LCPNXLCW->LCCWOP=LXI;           /* SET RESET IMMEDIATE CMD       */ 00461000
*                                                                  0549 00462000
         MVI   LCCWOP(@11),X'2B'                                   0549 00463000
*   /*****************************************************************/ 00464000
*   /*                                                               */ 00465000
*   /* THE RESET OPERATION MUST BE RESCHEDULED                       */ 00466000
*   /*                                                               */ 00467000
*   /*****************************************************************/ 00468000
*                                                                  0550 00469000
*/*  TPQUE (CRRLCPB,FMCPAB3) RESET                                   */ 00470000
*                                                                  0550 00471000
*   DO;                                                            0550 00472000
*     RFY                                                          0551 00473000
*      (GPR00P,                                                    0551 00474000
*       GPR01P,                                                    0551 00475000
*       GPR02P,                                                    0551 00476000
*       GPR15P) RSTD;                                              0551 00477000
*     GEN REFS(GPR01P) SETS(RPHWORK)                               0552 00478000
*         (STM   GPR14P,GPR09P,RPHWORK(GPR01P));                   0552 00479000
         STM   GPR14P,GPR09P,RPHWORK(GPR01P)                            00480000
*     GPR00P=ADDR(FMCPAB3);                                        0553 00481000
         L     @11,CRRFMCB(,ADDRCRR)                               0553 00482000
         LA    GPR00P,FMCPAB3(,@11)                                0553 00483000
*     GPR02P=CRRLCPB;                                              0554 00484000
         L     GPR02P,CRRLCPB(,ADDRCRR)                            0554 00485000
*     CALL ATCLCPTR->ATCAP34->ISTAPC34;                            0555 00486000
         L     @11,ATCLCPTR                                        0555 00487000
         L     @15,ATCAP34(,@11)                                   0555 00488000
         BALR  @14,@15                                             0555 00489000
*     GEN REFS(RPHWORK,GPR01P) NOSETS                              0556 00490000
*         (LM    GPR14P,GPR09P,RPHWORK(GPR01P));                   0556 00491000
         LM    GPR14P,GPR09P,RPHWORK(GPR01P)                            00492000
*     RFY                                                          0557 00493000
*      (GPR00P,                                                    0557 00494000
*       GPR01P,                                                    0557 00495000
*       GPR02P,                                                    0557 00496000
*       GPR15P) UNRSTD;                                            0557 00497000
*   END;                            /* *** END OF TPQUE GENERATION   */ 00498000
*   CRRENDLW=OFF;                   /* TURN OFF END OF I/O FLAG      */ 00499000
         NI    CRRENDLW(ADDRCRR),B'11011111'                       0559 00500000
*   REG14=CRRRTN4;                  /* RESTORE RTN ADDR@D31KKNL      */ 00501000
         L     REG14,CRRRTN4(,ADDRCRR)                             0560 00502000
*   END REQUE;                                                     0561 00503000
*                                                                  0561 00504000
@EL00003 DS    0H                                                  0561 00505000
@EF00003 DS    0H                                                  0561 00506000
@ER00003 BR    @14                                                 0561 00507000
@PB00003 DS    0H                                                  0562 00508000
*/* TPEPILOG                                                         */ 00509000
*                                                                  0562 00510000
*   GEN(PRINT OFF);                                                0562 00511000
         PRINT OFF                                                      00512000
*/* TEMPLATE FOR CHAIN FIELD OF WORK ELEMENT */                    0563 00513000
*DCL  1  ISTWKE BDY(WORD) BASED(WKEPTR),                           0563 00514000
*        2  * CHAR(4),                 /* SPACE                       * 00515000
*        2  WKECHAIN PTR(31),          /* CHAIN POINTER               * 00516000
*           3 WKEFLAGS BIT(8),         /* FLAGS                       * 00517000
*             4 WKEGATE  BIT(1),       /* GATE BIT                    * 00518000
*             4 *        BIT(7),       /* RESERVED                    * 00519000
*           3 WKECHNGP PTR(24);        /* PTR ADDRESS                 * 00520000
*   DCL                                                            0564 00521000
*     ISTAPC34 ENTRY BASED(ATCAP34) VALRG(*);/* RESET ROUTINE        */ 00522000
*   DCL                                                            0565 00523000
*     ISTAPC37 ENTRY BASED(ATCAP37) VALRG(*);/* TPQUE ROUTINE        */ 00524000
*   GEN(PRINT ON);                                                 0566 00525000
*                                                                  0566 00526000
         PRINT ON                                                       00527000
*/* END OF TPEPILOG                                                  */ 00528000
*                                                                  0567 00529000
*   END ISTDCC42                                                   0567 00530000
