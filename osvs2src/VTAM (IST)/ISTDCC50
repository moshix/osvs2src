*/********************************************************************/ 01449000
*/*                                                                  */ 01450000
*/* ROUTINE NAME : XFRFSB                                            */ 01451000
*/*                                                                  */ 01452000
*/* FUNCTION :                                                       */ 01453000
*/*  THE FSB IS MOVED TO PAGEABLE STORAGE AND QUEUED TO THE FMCB     */ 01454000
*/*   DATA QUEUE AND THE NUMBER OF BUFFERS IN THE CHAIN IS ADDED     */ 01455000
*/*   TO THE BUFFER COUNT IN THE FMCB.  A RESET COMMAND IS ISSUED    */ 01456000
*/*   IF SUFFICIENT BUFFERS EXIST AND NO PURGE IS IN PROGRESS.       */ 01457000
*/*   DATA WILL BE THROWN AWAY IF FLUSH IS INDICATED.                */ 01458000
*/*                                                                  */ 01459000
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */ 01460000
*/*                                                                  */ 01461000
*/* OUTPUT : SAME                                                    */ 01462000
*/*                                                                  */ 01463000
*/* SUBROUTINES CALLED : EOBANAL  - CHECK FOR ENDING STATUS          */ 01464000
*/*                      ISTDCC61 - MOVE FSB TO PAGEABLE STORAGE     */ 01465000
*/*                      LCPBINIT - BUILD AND SCHEDULE RESET LCPB    */ 01466000
*/*                      FLUSH    - FREE DATA BUFFERS                */ 01467000
*/*                                                                  */ 01468000
*/* MACROS USED : TPQUE                                              */ 01469000
*/*                                                                  */ 01470000
*/********************************************************************/ 01471000
*                                                                  0897 01472000
*XFRFSB:                                                           0897 01473000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0897 01474000
XFRFSB   DS    0H                                                  0898 01475000
*   CRRRTN9=RTNREG;                 /* SAVE RETURN REG               */ 01476000
         ST    RTNREG,CRRRTN9(,CRRPTR)                             0898 01477000
*   CALL EOBANAL;                   /* CHECK FOR LAST FSB            */ 01478000
         BAL   @14,EOBANAL                                         0899 01479000
*   FSBELI=OFF;                     /* CLEAR UNSOL INPUT FLAG        */ 01480000
*   FSBECI=OFF;                     /* CLEAR UNUSED FLAG             */ 01481000
*                                                                  0901 01482000
         NI    FSBELI(FSBPTR),B'00111111'                          0901 01483000
*   /*****************************************************************/ 01484000
*   /*                                                               */ 01485000
*   /* MOVE FSB TO PAGEABLE BUFFER                                   */ 01486000
*   /*                                                               */ 01487000
*   /*****************************************************************/ 01488000
*                                                                  0902 01489000
*   RFY                                                            0902 01490000
*     CVTPTR RSTD;                  /* RESTRICT PTR TO ATCVT         */ 01491000
*   CVTPTR=CRRATCVT;                /* SET PTR FOR ATCVT             */ 01492000
         L     CVTPTR,CRRATCVT(,CRRPTR)                            0903 01493000
*   CRRPTR=ADDR(CRRSAVE1);          /* SAVEAREA FOR MOVE ROUTINE     */ 01494000
         LA    @15,CRRSAVE1(,CRRPTR)                               0904 01495000
         LR    CRRPTR,@15                                          0904 01496000
*   CALL MOVE61;                    /* MOVE FSB TO PAGEABLE BUFFER   */ 01497000
         L     @15,ATCDCC61(,CVTPTR)                               0905 01498000
         BALR  @14,@15                                             0905 01499000
*   RFY                                                            0906 01500000
*     CVTPTR UNRSTD;                /* UNRESTRICT PTR TO ATCVT       */ 01501000
*   CRRPTR=ADDR(CRRSPACE);          /* REESTABLISH BASE              */ 01502000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0907 01503000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0907 01504000
*   FSBPTR=CRRFSB;                  /* GET NEW FSB (PAGEABLE) ADDRESS*/ 01505000
*                                                                  0908 01506000
         L     FSBPTR,CRRFSB(,CRRPTR)                              0908 01507000
*   /*****************************************************************/ 01508000
*   /*                                                               */ 01509000
*   /* ACCUMULATE COUNT OF BUFFERS IN USE. IF BUFFER LIMIT REACHED,  */ 01510000
*   /* RESET ANY OUTSTANDING I/O.                                    */ 01511000
*   /*                                                               */ 01512000
*   /*****************************************************************/ 01513000
*                                                                  0909 01514000
*   RFY                                                            0909 01515000
*    (COUNTER,                                                     0909 01516000
*     FDBPTR) RSTD;                                                0909 01517000
*   FDBPTR=ADDR(FSBFDB);            /* REFERENCE FIRST FDB           */ 01518000
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0910 01519000
*   COUNTER=0;                      /* INITIALIZE BUFFER ACCUMULATOR */ 01520000
         SLR   COUNTER,COUNTER                                     0911 01521000
*   DO WHILE FDBPTR^=ZEROWORD;      /* LOOP THROUGH ALL FDB'S        */ 01522000
         B     @DE00912                                            0912 01523000
@DL00912 DS    0H                                                  0913 01524000
*     COUNTER=COUNTER+1;            /* COUNT BUFFERS USED            */ 01525000
         AL    COUNTER,@CF00067                                    0913 01526000
*     FDBPTR=FDBFDBA;               /* REFERENCE NEXT FDB            */ 01527000
         L     FDBPTR,FDBFDBA(,FDBPTR)                             0914 01528000
*   END;                                                           0915 01529000
@DE00912 LTR   FDBPTR,FDBPTR                                       0915 01530000
         BNZ   @DL00912                                            0915 01531000
*   RFY                                                            0916 01532000
*    (REG2,                                                        0916 01533000
*     REG3) RSTD;                                                  0916 01534000
*CS4:                                                              0917 01535000
*   REG2=FMCCMPS1;                  /* GET CURRENT BUFFER COUNT      */ 01536000
CS4      L     @15,CRRFMCB(,CRRPTR)                                0917 01537000
         L     REG2,FMCCMPS1(,@15)                                 0917 01538000
*   REG3=REG2+COUNTER;              /* UPDATE BUFFER COUNT           */ 01539000
         LR    REG3,REG2                                           0918 01540000
         ALR   REG3,COUNTER                                        0918 01541000
*   CS(REG2,REG3,FMCCMPS1);         /* SET NEW COUNT IN FMCB         */ 01542000
         CS    REG2,@03,FMCCMPS1(@15)                              0919 01543000
*   BC(7,CS4);                      /* TRY AGAIN IF FAILURE          */ 01544000
         BC    7,CS4                                               0920 01545000
*   RFY                                                            0921 01546000
*    (COUNTER,                                                     0921 01547000
