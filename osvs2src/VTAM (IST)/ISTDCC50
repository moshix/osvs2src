*         REG3=REG2&FLGA3OFF;       /* RESET FLUSH FLAG      @ZA33102*/ 00133000
         LR    REG3,REG2                                           0516 00134000
         N     REG3,@CF01563                                       0516 00135000
*         CS(REG2,REG3,FMCCMPS2);   /* SET NEW VAL IN FMCB   @ZA33102*/ 00136000
         CS    REG2,@03,FMCCMPS2(@15)                              0517 00137000
*         BC(7,CS10);               /* TRY AGAIN IF FAILURE  @ZA33102*/ 00138000
         BC    7,CS10                                              0518 00139000
*         RFY                                                      0519 00140000
*          (REG2,                                                  0519 00141000
*           REG3) UNRSTD;           /*                       @ZA33102*/ 00142000
*       END;                        /* TURN OFF FLUSH BIT    @ZA33102*/ 00143000
*   ELSE                                                           0521 00144000
*     ;                                                            0521 00145000
@RF00510 DS    0H                                                  0522 00146000
*   IF CRRSW4=ON THEN               /* IF NOTHING FLUSHED            */ 00147000
@RC00510 TM    CRRSW4(CRRPTR),B'00001000'                          0522 00148000
         BNO   @RF00522                                            0522 00149000
*     DO;                                                          0523 00150000
*       IF DCLRPLA=ZEROWORD THEN    /* RPL PTR IN DCLCP      @DM9272K*/ 00151000
         L     @15,CRRLCPB(,CRRPTR)                                0524 00152000
         L     @15,DCLRPLA(,@15)                                   0524 00153000
         LTR   @15,@15                                             0524 00154000
         BNZ   @RF00524                                            0524 00155000
*         CRRSW0=ON;                /* REQUEST IS SOLICIT            */ 00156000
         OI    CRRSW0(CRRPTR),B'10000000'                          0525 00157000
*       ELSE                                                       0526 00158000
*         ;                                                        0526 00159000
@RF00524 DS    0H                                                  0527 00160000
*       IF DCLRQTP=DCLREAD|CRRSW0=ON THEN                          0527 00161000
*                                                                  0527 00162000
         L     @15,CRRLCPB(,CRRPTR)                                0527 00163000
