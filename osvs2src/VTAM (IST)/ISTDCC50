*   END;                            /* **** END OF TPESC             */ 00215000
*                                                                  0542 00216000
*/********************************************************************/ 00217000
*/*                                                                  */ 00218000
*/* ROUTINE NAME : READMAC                                           */ 00219000
*/*                                                                  */ 00220000
*/* FUNCTION :                                                       */ 00221000
*/*  READMAC RECEIVES CONTROL ON READ OR SOLICIT FEEDBACK.           */ 00222000
*/*  IF CRRSW0 IS SET, READMAC WILL QUEUE THE DATA OR ATTEMPT TO     */ 00223000
*/*  SCHEDULE AN ASYNCHRONOUS INPUT EXIT.                            */ 00224000
*/*                                                                  */ 00225000
*/*  IF ENTRY WAS FOR A READ, THE FEEDBACK STATUS IS MOVED FROM      */ 00226000
*/*  THE FSB TO THE CRR.  THE PARM LIST FOR THE MOVE ROUTINE-        */ 00227000
*/*  ISTDCC60 - IS BUILT IN THE CRR USING THE INFORMATION IN THE     */ 00228000
*/*  RPL OR IN THE DCLCP.  THE DCLCP IS USED IF THE DATA TO BE       */ 00229000
*/*  MOVED WAS SAVED FROM A PREVIOUS REQUEST.                        */ 00230000
*/*                                                                  */ 00231000
*/* INPUT :  FSBPTR REG(4) - ADDRESS OF FSB                          */ 00232000
*/*                                                                  */ 00233000
*/* OUTPUT : SAME                                                    */ 00234000
*/*                                                                  */ 00235000
*/* SUBROUTINES CALLED : SETCRR   - COPY STATUS TO CRR               */ 00236000
*/*                      MOVEDATA - MOVE DATA TO USER AREA           */ 00237000
*/*                      XFRFSB   - QUEUE FSB TO DATA QUEUE          */ 00238000
*/*                      SOLMAC1  - CHECK FOR ASYNCH INPUT EXIT      */ 00239000
*/*                                                                  */ 00240000
*/* MACROS USED :  NONE                                              */ 00241000
*/*                                                                  */ 00242000
*/********************************************************************/ 00243000
*                                                                  0542 00244000
*READMAC:                                                          0542 00245000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0542 00246000
         B     @PB00002                                            0542 00247000
READMAC  DS    0H                                                  0543 00248000
*   CRRRTN1=RTNREG;                 /* SAVE RETURN REGISTER          */ 00249000
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0543 00250000
*   DO WHILE(CRRSW1=OFF);           /*                       @D31KKNL*/ 00251000
         B     @DE00544                                            0544 00252000
@DL00544 DS    0H                                                  0545 00253000
*     IF CRRSW0=OFF THEN            /* IF RPL EXISTS         @D31KKNL*/ 00254000
         TM    CRRSW0(CRRPTR),B'10000000'                          0545 00255000
         BNZ   @RF00545                                            0545 00256000
*       DO;                         /* PROCESS THE DATA      @D31KKNL*/ 00257000
*         DCLRPLA=ZEROWORD;         /* CLEAR RPL ADDRESS     @D31KKNL*/ 00258000
         L     @15,CRRLCPB(,CRRPTR)                                0547 00259000
         SLR   @05,@05                                             0547 00260000
         ST    @05,DCLRPLA(,@15)                                   0547 00261000
*         CALL SETCRR;              /* COPY FEEDBACK STATUS  @D31KKNL*/ 00262000
         BAL   @14,SETCRR                                          0548 00263000
*         CALL MOVEDATA;            /* MOVE DATA TO USER BUFFERS   0549 00264000
*                                                            @D31KKNL*/ 00265000
         BAL   @14,MOVEDATA                                        0549 00266000
*         CRRSW1=ON;                /* END THIS LOOP         @D31KKNL*/ 00267000
         OI    CRRSW1(CRRPTR),B'01000000'                          0550 00268000
*       END;                        /* END-RPL EXISTS        @D31KKNL*/ 00269000
*     ELSE                                                         0552 00270000
*       DO;                         /* NO RPL EXISTS         @D31KKNL*/ 00271000
         B     @RC00545                                            0552 00272000
@RF00545 DS    0H                                                  0553 00273000
*         IF(FMCFBA=ZEROWORD|FMCFBA=GATEBIT)&/* NO DATA              */ 00274000
*             FMCFLGA1=OFF THEN     /* AND SPECIFIC MODE             */ 00275000
         L     @15,CRRFMCB(,CRRPTR)                                0553 00276000
         L     @15,FMCFBA(,@15)                                    0553 00277000
         LTR   @15,@15                                             0553 00278000
         BZ    @GL00002                                            0553 00279000
         CL    @15,@CF01547                                        0553 00280000
         BNE   @RF00553                                            0553 00281000
@GL00002 L     @15,CRRFMCB(,CRRPTR)                                0553 00282000
         TM    FMCFLGA1(@15),B'01000000'                           0553 00283000
         BNZ   @RF00553                                            0553 00284000
*           CALL SOLMAC1;           /* CHECK ASY. INPUT              */ 00285000
         BAL   @14,SOLMAC1                                         0554 00286000
*         ELSE                                                     0555 00287000
*           DO;                     /* THERE IS DATA                 */ 00288000
         B     @RC00553                                            0555 00289000
@RF00553 DS    0H                                                  0556 00290000
*             CRRSW6=ON;            /* SET DATA EXISTS FLG           */ 00291000
         OI    CRRSW6(CRRPTR),B'00000010'                          0556 00292000
*             CALL XFRFSB;          /* QUEUE PAGEABLE FSB            */ 00293000
         BAL   @14,XFRFSB                                          0557 00294000
*             CRRSW1=ON;            /* SET EXIT SWITCH       @DM9361K*/ 00295000
         OI    CRRSW1(CRRPTR),B'01000000'                          0558 00296000
*           END;                                                   0559 00297000
*       END;                        /* END - NO RPL EXISTS           */ 00298000
@RC00553 DS    0H                                                  0561 00299000
*   END;                            /* END DO WHILE                  */ 00300000
@RC00545 DS    0H                                                  0561 00301000
@DE00544 TM    CRRSW1(CRRPTR),B'01000000'                          0561 00302000
         BZ    @DL00544                                            0561 00303000
*   RTNREG=CRRRTN1;                 /* RESTORE RETURN REG            */ 00304000
         L     RTNREG,CRRRTN1(,CRRPTR)                             0562 00305000
*   END READMAC;                                                   0563 00306000
@EL00002 DS    0H                                                  0563 00307000
@EF00002 DS    0H                                                  0563 00308000
@ER00002 BR    @14                                                 0563 00309000
*                                                                  0564 00310000
*/********************************************************************/ 00311000
*/*                                                                  */ 00312000
*/* ROUTINE NAME : WRTMAC                                            */ 00313000
*/*                                                                  */ 00314000
*/* FUNCTION :                                                       */ 00315000
*/*  WRTMAC RECEIVES CONTROL FROM THE MAINLINE ROUTINE               */ 00316000
*/*  TO PROCESS A WRITE FEEDBACK.  FEEDBACK STATUS IS                */ 00317000
*/*  MOVED FROM THE FSB TO THE CRR.  IF THE WRITE WAS                */ 00318000
*/*  CONVERSATIONAL AND INPUT WAS RECEIVED, THE MOVEDATA             */ 00319000
*/*  SUBROUTINE IS CALLED TO PROCESS THE DATA.  IF THE               */ 00320000
*/*  OPERATION WAS A NORMAL WRITE AND DATA WAS RECEIVED              */ 00321000
*/*  THE FSB IS QUEUED IS THE FMCB DATA QUEUE AND THE                */ 00322000
*/*  UNSOLICITED INPUT FLAG IS SET IN THE CRR.                       */ 00323000
*/*                                                                  */ 00324000
*/* INPUT : FSBPTR REG(4) - ADDRESS FSB                              */ 00325000
*/*                                                                  */ 00326000
*/* OUTPUT : SAME                                                    */ 00327000
*/*                                                                  */ 00328000
*/* SUBROUTINES CALLED : SETCRR   - COPY STATUS TO CRR               */ 00329000
*/*                      MOVEDATA - MOVE DATA TO USER AREA           */ 00330000
*/*                      FREEBUF  - FREE FSB CHAIN                   */ 00331000
*/*                      XFRFSB   - QUEUE FSB TO DATA QUEUE          */ 00332000
*/* MACROS USED : NONE                                               */ 00333000
*/*                                                                  */ 00334000
*/********************************************************************/ 00335000
*                                                                  0564 00336000
*WRTMAC:                                                           0564 00337000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0564 00338000
WRTMAC   DS    0H                                                  0565 00339000
*   CRRRTN1=RTNREG;                 /* SAVE RETURN REG               */ 00340000
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0565 00341000
*   CALL SETCRR;                    /* MOVE STATUS TO CRR    @D31KKNL*/ 00342000
         BAL   @14,SETCRR                                          0566 00343000
*   RFY                                                            0567 00344000
*     FDBPTR RSTD;                                                 0567 00345000
*   FDBPTR=ADDR(FSBFDB);            /* COVER THE FDB DSECT           */ 00346000
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0568 00347000
*   IF RPLWROPT=ON THEN             /* WRITE CONVERSATIONAL          */ 00348000
         L     @15,CRRARPL(,CRRPTR)                                0569 00349000
         TM    RPLWROPT(@15),B'00000001'                           0569 00350000
         BNO   @RF00569                                            0569 00351000
*     IF FDBDATCT>ZEROWORD THEN     /* DATA RECEIVED                 */ 00352000
*                                                                  0570 00353000
         SLR   @15,@15                                             0570 00354000
         CH    @15,FDBDATCT(,FDBPTR)                               0570 00355000
         BNL   @RF00570                                            0570 00356000
*       /*************************************************************/ 00357000
*       /*                                                           */ 00358000
*       /* WRITE CONVERSATIONAL WITH DATA RECEIVED                   */ 00359000
*       /*                                                           */ 00360000
*       /*************************************************************/ 00361000
*                                                                  0571 00362000
*       DO;                         /* PROCESS IT            @DM9258K*/ 00363000
*         DCLRPLA=ZEROWORD;         /* CLEAR RPL ADDRESS     @DM9258K*/ 00364000
         L     @09,CRRLCPB(,CRRPTR)                                0572 00365000
         ST    @15,DCLRPLA(,@09)                                   0572 00366000
*         CALL MOVEDATA;            /* MOVE DATA TO USER BUFFERS   0573 00367000
*                                                            @D31KKNL*/ 00368000
         BAL   @14,MOVEDATA                                        0573 00369000
*       END;                        /* END-DATA REC'D        @DM9258K*/ 00370000
*     ELSE                                                         0575 00371000
*       DO;                                                        0575 00372000
*                                                                  0575 00373000
         B     @RC00570                                            0575 00374000
@RF00570 DS    0H                                                  0576 00375000
*         /***********************************************************/ 00376000
*         /*                                                         */ 00377000
*         /* FINISHED PROCESSING-WRITE CONVERSATIONAL BUT NO DATA    */ 00378000
*         /* RECEIVED                                                */ 00379000
*         /*                                                         */ 00380000
*         /***********************************************************/ 00381000
*                                                                  0576 00382000
*         CRRPOST=ON;               /* SET DO POST SWITCH            */ 00383000
         OI    CRRPOST(CRRPTR),B'00010000'                         0576 00384000
*         CALL EOBANAL;             /*                       @ZA28667*/ 00385000
         BAL   @14,EOBANAL                                         0577 00386000
*         DCLRPLA=ZEROWORD;         /* CLEAR RPL ADDRESS     @ZA28667*/ 00387000
         L     @15,CRRLCPB(,CRRPTR)                                0578 00388000
         SLR   @09,@09                                             0578 00389000
         ST    @09,DCLRPLA(,@15)                                   0578 00390000
*         IF CRRRTNCD=ZEROWORD THEN /* IF NO ERROR                   */ 00391000
         CLI   CRRRTNCD(CRRPTR),0                                  0579 00392000
         BNE   @RF00579                                            0579 00393000
*           CRRRTNCD=CRRNGRCC;      /* SET CONDITIONAL OPERATION     */ 00394000
         MVI   CRRRTNCD(CRRPTR),X'10'                              0580 00395000
*         CALL FREEBUF;                                            0581 00396000
@RF00579 BAL   @14,FREEBUF                                         0581 00397000
*       END;                                                       0582 00398000
*   ELSE                                                           0583 00399000
*     DO;                                                          0583 00400000
*                                                                  0583 00401000
         B     @RC00569                                            0583 00402000
@RF00569 DS    0H                                                  0584 00403000
*       /*************************************************************/ 00404000
*       /*                                                           */ 00405000
*       /* NONCONVERSATIONAL WRITE                                   */ 00406000
*       /*                                                           */ 00407000
*       /*************************************************************/ 00408000
*                                                                  0584 00409000
*       CRRPOST=ON;                 /* SET DO POST SWITCH            */ 00410000
*       CRRENDLW=ON;                /* SET END OF IO LCCW            */ 00411000
         OI    CRRPOST(CRRPTR),B'00110000'                         0585 00412000
*       IF FDBDATCT>ZEROWORD THEN   /* DATA RECEIVED                 */ 00413000
         SLR   @15,@15                                             0586 00414000
         CH    @15,FDBDATCT(,FDBPTR)                               0586 00415000
         BNL   @RF00586                                            0586 00416000
*         DO;                                                      0587 00417000
*                                                                  0587 00418000
*           /*********************************************************/ 00419000
*           /*                                                       */ 00420000
*           /* DATA RECEIVED-TREAT AS UNSOLICITED INPUT              */ 00421000
*           /*                                                       */ 00422000
*           /*********************************************************/ 00423000
*                                                                  0588 00424000
*           IF FSBELI=OFF THEN      /* CHECK FOR END OF LCP          */ 00425000
         TM    FSBELI(FSBPTR),B'10000000'                          0588 00426000
         BNZ   @RF00588                                            0588 00427000
*             DO;                                                  0589 00428000
*               DCLRPLA=ZEROWORD;                                  0590 00429000
         L     @09,CRRLCPB(,CRRPTR)                                0590 00430000
         ST    @15,DCLRPLA(,@09)                                   0590 00431000
*               CRRENDLW=OFF;       /* RESET END OF LCP FLAG         */ 00432000
         NI    CRRENDLW(CRRPTR),B'11011111'                        0591 00433000
*             END;                                                 0592 00434000
*           ELSE                                                   0593 00435000
*             ;                     /* LEAVE END OF LCP SW SET       */ 00436000
@RF00588 DS    0H                                                  0594 00437000
*           FSBECI=OFF;                                            0594 00438000
         NI    FSBECI(FSBPTR),B'10111111'                          0594 00439000
*           CRRUINPT=ON;            /* INDICATE UNSOLICITED INPUT    */ 00440000
*           CRRRDEOB=OFF;           /* RESET EOB                     */ 00441000
*           CRRRDEOM=OFF;           /* RESET EOM                     */ 00442000
*           CRRRDEOT=OFF;           /* RESET EOT                     */ 00443000
*           CRRRLG=OFF;             /* RESET LEADING GRAPHICS        */ 00444000
         OI    CRRUINPT(CRRPTR),B'10000000'                        0599 00445000
         NI    CRRRDEOB(CRRPTR),B'11000101'                        0599 00446000
*           CALL XFRFSB;            /* SAVE FSB ON FMCB DATA Q       */ 00447000
         BAL   @14,XFRFSB                                          0600 00448000
*           FSBELI=ON;                                             0601 00449000
         OI    FSBELI(FSBPTR),B'10000000'                          0601 00450000
*         END;                                                     0602 00451000
*       ELSE                                                       0603 00452000
*                                                                  0603 00453000
*         /***********************************************************/ 00454000
*         /*                                                         */ 00455000
*         /* NORMAL WRITE COMPLETED                                  */ 00456000
*         /*                                                         */ 00457000
*         /***********************************************************/ 00458000
*                                                                  0603 00459000
*         CALL FREEBUF;                                            0603 00460000
         B     @RC00586                                            0603 00461000
@RF00586 BAL   @14,FREEBUF                                         0603 00462000
*     END;                                                         0604 00463000
@RC00586 DS    0H                                                  0605 00464000
*   RFY                                                            0605 00465000
*     FDBPTR UNRSTD;                                               0605 00466000
@RC00569 DS    0H                                                  0606 00467000
*   RTNREG=CRRRTN1;                 /* RESTORE RETURN REG            */ 00468000
         L     RTNREG,CRRRTN1(,CRRPTR)                             0606 00469000
*   END WRTMAC;                                                    0607 00470000
@EL00003 DS    0H                                                  0607 00471000
@EF00003 DS    0H                                                  0607 00472000
@ER00003 BR    @14                                                 0607 00473000
*                                                                  0608 00474000
*/********************************************************************/ 00475000
*/*                                                                  */ 00476000
*/* ROUTINE NAME : SETCRR                                            */ 00477000
*/*                                                                  */ 00478000
*/* FUNCTION :                                                       */ 00479000
*/*  SETCRR COPIES THE FEEDBACK STATUS FROM THE FSB TO THE CRR AS    */ 00480000
*/*   THE FSB  WILL BE EVENTUALLY BE FREED UP                        */ 00481000
*/*                                                                  */ 00482000
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */ 00483000
*/*                                                                  */ 00484000
*/* OUTPUT : SAME                                                    */ 00485000
*/*                                                                  */ 00486000
*/* SUBROUTINES CALLED :  NONE                                       */ 00487000
*/*                                                                  */ 00488000
*/* MACROS USED : NONE                                               */ 00489000
*/*                                                                  */ 00490000
*/********************************************************************/ 00491000
*                                                                  0608 00492000
*SETCRR:                                                           0608 00493000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @D31KKNL*/ 00494000
SETCRR   DS    0H                                                  0609 00495000
*   CRRRTNCD=FSBSRCAT;              /* COPY SYS RESPONSE CATEGORY  0609 00496000
*                                                            @D31KKNL*/ 00497000
         MVC   CRRRTNCD(1,CRRPTR),FSBSRCAT(FSBPTR)                 0609 00498000
*   CRRFDB2=FSBMNRCD;               /* COPY MINOR RETURN CODE      0610 00499000
*                                                            @D31KKNL*/ 00500000
         MVC   CRRFDB2(1,CRRPTR),FSBMNRCD(FSBPTR)                  0610 00501000
*   CRRFDB3=FSBERC;                 /* EXTENDED RESPONSE     @D31KKNL*/ 00502000
         MVC   CRRFDB3(1,CRRPTR),FSBERC(FSBPTR)                    0611 00503000
*   CRRUINPT=OFF;                   /* UNUSED FIELD          @D31KKNL*/ 00504000
*   CRRUNUSD=OFF;                   /* CLEAR UNUSED FIELD    @D31KKNL*/ 00505000
         NI    CRRUINPT(CRRPTR),B'00111111'                        0613 00506000
*   CRRESR1=FSBSYSRS;               /* SYSTEM RESPONSE       @D31KKNL*/ 00507000
         MVC   CRRESR1(1,CRRPTR),FSBSYSRS(FSBPTR)                  0614 00508000
*   CRRESR2=FSBEXTRS;               /* EXTENDED RESPONSE     @D31KKNL*/ 00509000
         MVC   CRRESR2(1,CRRPTR),FSBEXTRS(FSBPTR)                  0615 00510000
*   IF CRRRTNCD=CRRPHYSC&           /* IF PHYSICAL CONDITION @D31KKNL*/ 00511000
*       CRRSTSAV=ON THEN            /* SENSE DATA AVAILABLE  @D31KKNL*/ 00512000
         CLI   CRRRTNCD(CRRPTR),X'0C'                              0616 00513000
         BNE   @RF00616                                            0616 00514000
         TM    CRRSTSAV(CRRPTR),B'00000001'                        0616 00515000
         BNO   @RF00616                                            0616 00516000
*     CRRDSB=FSBDMGRS;              /* COPY SENSE DATA       @D31KKNL*/ 00517000
         MVC   CRRDSB(2,CRRPTR),FSBDMGRS(FSBPTR)                   0617 00518000
*   ELSE                                                           0618 00519000
*     ;                             /*                       @D31KKNL*/ 00520000
@RF00616 DS    0H                                                  0619 00521000
*   RETURN;                         /*                       @D31KKNL*/ 00522000
@EL00004 DS    0H                                                  0619 00523000
@EF00004 DS    0H                                                  0619 00524000
@ER00004 BR    @14                                                 0619 00525000
*   END SETCRR;                     /*                       @D31KKNL*/ 00526000
*                                                                  0621 00527000
*/********************************************************************/ 00528000
*/*                                                                  */ 00529000
*/* ROUTINE NAME : SOLMAC1                                           */ 00530000
*/*                                                                  */ 00531000
*/* FUNCTION :                                                       */ 00532000
*/*  SOLMAC1 RECEIVES CONTROL FROM SOLMAC TO CHECK FOR ASYNCHRONOUS  */ 00533000
*/*  INPUT.  THE SUBROUTINE ASYNCH IS CALLED TO CHECK FOR AN ACTIVE  */ 00534000
*/*  ASYNCHRONOUS INPUT EXIT.  IF THERE IS AN EXIT ACTIVE, THE EXIT  */ 00535000
*/*  SWITCH, CRRSW1, IS SET AND THE ROUTINE RETURNS TO SOLMAC.       */ 00536000
*/*                                                                  */ 00537000
*/*  IF THERE IS NO ACTIVE EXIT AN THE RPL IS CHAINED OFF THE        */ 00538000
*/*  ACBDEB, THE RPL IS DEQUEUED, THE RPL IS INITIALIZED, AND        */ 00539000
*/*  THE RPL PTR IS STORED IN THE CRR.  THE EXIT SWITCH IS NOT       */ 00540000
*/*  SET  THE DATA WILL BE PROCESSED BY RECYCLING THROUGH THE        */ 00541000
*/*  READ LOGIC.                                                     */ 00542000
*/*                                                                  */ 00543000
*/*  IF THE RPL IS NOT CHAINED OFF THE ACBDEB, THE FMCB IS           */ 00544000
*/*  QUEUED TO THE READ ANY QUEUE AND SUBROUTINE XFRFSB              */ 00545000
*/*  IS CALLED TO QUEUE THE FSB TO THE DATA QUEUE.                   */ 00546000
*/*                                                                  */ 00547000
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */ 00548000
*/*                                                                  */ 00549000
*/* OUTPUT : SAME                                                    */ 00550000
*/*                                                                  */ 00551000
*/* SUBROUTINES:         XFRFSB   - QUEUE FSB TO DATA QUEUE          */ 00552000
*/*                                                                  */ 00553000
*/* MACROS USED :  TPFEL, TPQUE, VMDESET, TPDEQ                      */ 00554000
*/*                                                                  */ 00555000
*/********************************************************************/ 00556000
*                                                                  0621 00557000
*SOLMAC1:                                                          0621 00558000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0621 00559000
SOLMAC1  DS    0H                                                  0622 00560000
*   CRRRTN3=RTNREG;                                                0622 00561000
         ST    RTNREG,CRRRTN3(,CRRPTR)                             0622 00562000
*   CRRBARAD=ZEROWORD;                                             0623 00563000
         SLR   @15,@15                                             0623 00564000
         ST    @15,CRRBARAD(,CRRPTR)                               0623 00565000
*   ACBPTR=FMCDEBA;                 /* SET UP ACDEB POINTER  @VS40357*/ 00566000
         L     @15,CRRFMCB(,CRRPTR)                                0624 00567000
         L     ACBPTR,FMCDEBA(,@15)                                0624 00568000
*   CRRSW7=ON;                      /* TURN ON SW TO GET OUT OF LOOP    00569000
*                                                            @ZA17037*/ 00570000
         OI    CRRSW7(CRRPTR),B'00000001'                          0625 00571000
*   IF CRRFMCLK=ON THEN             /* IS THE FMCB LOCKED?   @ZA30066*/ 00572000
         TM    CRRFMCLK(CRRPTR),B'10000000'                        0626 00573000
         BNO   @RF00626                                            0626 00574000
*     DO;                           /* YES, UNLOCK IT        @ZA30066*/ 00575000
*       CRRFMCLK=OFF;               /* TURN OFF FMCB LOCK FLAG     0628 00576000
*                                                            @ZA30066*/ 00577000
         NI    CRRFMCLK(CRRPTR),B'01111111'                        0628 00578000
*/*** START -TPUNLOCK (CTLLOCK)- **                                  */ 00579000
*                                                                  0629 00580000
*       DO;                                                        0629 00581000
*                                                                  0629 00582000
*/* ATBUILD (ISTRPH,ISTLOK,ISTCRA)                                   */ 00583000
*/* END OF ATBUILD                                                   */ 00584000
*                                                                  0630 00585000
*         RFY                                                      0630 00586000
*          (GPR00P,                                                0630 00587000
*           GPR01P,                                                0630 00588000
*           GPR02P,                                                0630 00589000
*           GPR03P,                                                0630 00590000
*           GPR04P,                                                0630 00591000
*           GPR15P) RSTD;                                          0630 00592000
*         GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(RPHWORK);    00593000
         STM GPR00P,GPR15P,RPHWORK(GPR01P)                              00594000
*         GPR02P=ADDR(FMCDCLOK);    /* SET PARAM - LOCKWORD PTR      */ 00595000
         L     GPR02P,CRRFMCB(,CRRPTR)                             0632 00596000
