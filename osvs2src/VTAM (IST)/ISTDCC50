*         GPR04P=GPR01P->RPHCRR;    /* SET PARAM - ISTCRA PTR        */ 00598000
         L     GPR04P,RPHCRR(,GPR01P)                              0633 00599000
*         GPR00P=CRALMS08;          /* SET PARAM - LOCK LEVEL FLAG   */ 00600000
         LA    GPR00P,128                                          0634 00601000
*         GPR03P=ADDR(GPR04P->CRAL8PTR);/* SET PARAM - LOCK LEVEL PTR*/ 00602000
