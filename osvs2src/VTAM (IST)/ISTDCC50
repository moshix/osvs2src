*                                                                  1199 02640000
*/********************************************************************/ 02641000
*/*                                                                  */ 02642000
*/* ROUTINE NAME : KEEP                                              */ 02643000
*/*                                                                  */ 02644000
*/* FUNCTION :                                                       */ 02645000
*/* KEEP IS CALLED BY MOVEDATA IF THE USER HAS NOT SPECIFIED THE     */ 02646000
*/* TRUNC OPTION.  IF THERE WERE NO ERRORS DURING THE MOVE KEEP1 IS  */ 02647000
*/* CALLED.  OTHERWISE, THE EOB, EOM, EOT INDICATORS ARE TURNED OFF  */ 02648000
*/* IN THE CRR, THE DO POST FLAG IS SET AND THE FSB AND FDBS ARE     */ 02649000
*/* QUEUED TO THE FMCB DATA QUEUE.  RETURN IS TO MOVEDATA VIA        */ 02650000
*/* REG 14.                                                          */ 02651000
*/*                                                                  */ 02652000
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */ 02653000
*/*                                                                  */ 02654000
*/* OUTPUT : SAME                                                    */ 02655000
*/*                                                                  */ 02656000
*/* SUBROUTINES CALLED : KEEP1    - FURTHER KEEP PROCESSING          */ 02657000
*/*                      XFRFSB   - QUEUE FSB ON FMCB DATA QUEUE     */ 02658000
*/*                      FREEBUF  - RELEASE FSB AND FDB BUFFERS      */ 02659000
*/*                                                                  */ 02660000
*/* MACROS USED :                                                    */ 02661000
*/*                                                                  */ 02662000
*/********************************************************************/ 02663000
*                                                                  1199 02664000
*KEEP:                                                             1199 02665000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1199 02666000
KEEP     DS    0H                                                  1200 02667000
*   CRRRTN3=RTNREG;                 /* SAVE RETURN REG               */ 02668000
         ST    RTNREG,CRRRTN3(,CRRPTR)                             1200 02669000
*   IF CRRBRTCD=CRRBRSUC THEN       /* 0 RETURN CODE FROM MOVE       */ 02670000
         CLI   CRRBRTCD(CRRPTR),0                                  1201 02671000
         BNE   @RF01201                                            1201 02672000
*     DO;                                                          1202 02673000
*                                                                  1202 02674000
*       /*************************************************************/ 02675000
*       /*                                                           */ 02676000
*       /* ALL DATA FROM ONE FSB MOVED- CHECK IF MORE DATA EXPECTED  */ 02677000
*       /*                                                           */ 02678000
*       /*************************************************************/ 02679000
*                                                                  1203 02680000
*       CALL KEEP1;                                                1203 02681000
         BAL   @14,KEEP1                                           1203 02682000
*       CALL FREEBUF;               /* FREE FSB AND FDBS             */ 02683000
         BAL   @14,FREEBUF                                         1204 02684000
*     END;                                                         1205 02685000
*   ELSE                                                           1206 02686000
*     DO;                                                          1206 02687000
*                                                                  1206 02688000
         B     @RC01201                                            1206 02689000
@RF01201 DS    0H                                                  1207 02690000
*       /*************************************************************/ 02691000
*       /*                                                           */ 02692000
*       /* BUFFER WAS FILLED BEFORE ALL DATA WAS MOVED. RESET ANY    */ 02693000
*       /* ENDING STATUS IN THE FEEDBACK TO INFORM USER OF THIS FACT.*/ 02694000
*       /* SAVE FSB TO BE PROCESSED DURING NEXT READ.                */ 02695000
*       /*                                                           */ 02696000
*       /*************************************************************/ 02697000
*                                                                  1207 02698000
*       CRRPOST=ON;                 /* SET DO POST SWITCH            */ 02699000
         OI    CRRPOST(CRRPTR),B'00010000'                         1207 02700000
*       CRRRDEOB=OFF;               /* TURN OFF EOB,EOM, EOT         */ 02701000
*       CRRRDEOM=OFF;                                              1209 02702000
*       CRRRDEOT=OFF;                                              1210 02703000
         NI    CRRRDEOB(CRRPTR),B'11000111'                        1210 02704000
*       CALL XFRFSB;                /* PUT FSB ON FMCB DATA Q        */ 02705000
         BAL   @14,XFRFSB                                          1211 02706000
*     END;                                                         1212 02707000
*   RTNREG=CRRRTN3;                 /* RESTORE RETURN REG            */ 02708000
@RC01201 L     RTNREG,CRRRTN3(,CRRPTR)                             1213 02709000
*   END KEEP;                                                      1214 02710000
@EL00013 DS    0H                                                  1214 02711000
@EF00013 DS    0H                                                  1214 02712000
@ER00013 BR    @14                                                 1214 02713000
*                                                                  1215 02714000
*/********************************************************************/ 02715000
*/*                                                                  */ 02716000
*/* ROUTINE NAME : KEEP1                                             */ 02717000
*/*                                                                  */ 02718000
*/* FUNCTION :                                                       */ 02719000
*/* KEEP1 IS CALLED BY KEEP IF THERE WERE NO ERRORS DURING THE MOVE. */ 02720000
*/* IF EOB WAS RECEIVED IN THE FSB, THE DO POST SWITCH IS SET AND    */ 02721000
*/* THE SUBROUTINE RETURNS TO KEEP.                                  */ 02722000
*/*                                                                  */ 02723000
*/* OTHERWISE, IF THERE IS MORE SPACE LEFT IN THE USER AREA, THE     */ 02724000
*/* BUFFER INFORMATION IS MOVED FROM THE CRR TO THE DCLCP AND THE    */ 02725000
*/* RPL PTR IS PUT BACK INTO THE DCLCP.  IF THERE IS NO MORE SPACE   */ 02726000
*/* IN THE USER AREA THE EOB, EOM, EOT INDICATORS ARE TURNED OFF IN  */ 02727000
*/* THE CRR AND THE DO POST FLAG IS SET.  THE SUBROUTINE RETURNS TO  */ 02728000
*/* KEEP VIA REG 14.                                                 */ 02729000
*/*                                                                  */ 02730000
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */ 02731000
*/*                                                                  */ 02732000
*/* OUTPUT : SAME                                                    */ 02733000
*/*                                                                  */ 02734000
*/* SUBROUTINES CALLED : EOBANAL  - CHECK FOR ENDING STATUS          */ 02735000
*/*                                                                  */ 02736000
*/* MACROS USED : NONE                                               */ 02737000
*/*                                                                  */ 02738000
*/********************************************************************/ 02739000
*                                                                  1215 02740000
*KEEP1:                                                            1215 02741000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1215 02742000
KEEP1    DS    0H                                                  1216 02743000
*   CRRRTN4=RTNREG;                 /* SAVE RETURN REG               */ 02744000
         ST    RTNREG,CRRRTN4(,CRRPTR)                             1216 02745000
*   CALL EOBANAL;                   /* CHECK FOR EOM, EOB, EOT       */ 02746000
         BAL   @14,EOBANAL                                         1217 02747000
*   IF FSBEBI=ON|FSBECI=ON THEN     /* CHECK EOB & END CHAN PGM    1218 02748000
*                                                            @OX11109*/ 02749000
*                                                                  1218 02750000
         TM    FSBEBI(FSBPTR),B'01100000'                          1218 02751000
         BZ    @RF01218                                            1218 02752000
*     /***************************************************************/ 02753000
*     /*                                                             */ 02754000
*     /* FSB JUST PROCESSED WAS NOT A SUBBLOCK SO MOVE PROCESSING    */ 02755000
*     /* COMPLETE.                                                   */ 02756000
*     /*                                                             */ 02757000
*     /***************************************************************/ 02758000
*                                                                  1219 02759000
*     CRRPOST=ON;                   /* SET DO POST SWITCH            */ 02760000
         OI    CRRPOST(CRRPTR),B'00010000'                         1219 02761000
*   ELSE                                                           1220 02762000
*     IF CRRBLEN>ZEROWORD THEN      /* MORE SPACE LEFT IN USER AREA  */ 02763000
         B     @RC01218                                            1220 02764000
@RF01218 LH    @15,CRRBLEN(,CRRPTR)                                1220 02765000
         LTR   @15,@15                                             1220 02766000
         BNP   @RF01220                                            1220 02767000
*       IF CRRERLK=ON|              /* CHECK FOR ERROR LOCK          */ 02768000
*           FSBMJRCD^=FRNORM|       /* ERROR RETURN CODE             */ 02769000
*           FSBLGI=ON THEN          /* LEADING GRAPHICS RECEIVED     */ 02770000
*                                                                  1221 02771000
         TM    CRRERLK(CRRPTR),B'10000000'                         1221 02772000
         BO    @RT01221                                            1221 02773000
         CLI   FSBMJRCD(FSBPTR),X'00'                              1221 02774000
         BNE   @RT01221                                            1221 02775000
         TM    FSBLGI(FSBPTR),B'00000010'                          1221 02776000
         BNO   @RF01221                                            1221 02777000
@RT01221 DS    0H                                                  1222 02778000
*         /***********************************************************/ 02779000
*         /*                                                         */ 02780000
*         /* ERROR RETURN CODE OR LEADING GRAPHICS RECEIVED SO MOVE  */ 02781000
*         /* PROCESSING COMPLETE.                                    */ 02782000
*         /*                                                         */ 02783000
*         /***********************************************************/ 02784000
*                                                                  1222 02785000
*         CRRPOST=ON;               /* POST USER WITH ERROR LOCK     */ 02786000
         OI    CRRPOST(CRRPTR),B'00010000'                         1222 02787000
*       ELSE                        /* ERROR LOCK NOT SET-CONTINUE   */ 02788000
*         DO;                                                      1223 02789000
*                                                                  1223 02790000
         B     @RC01221                                            1223 02791000
@RF01221 DS    0H                                                  1224 02792000
*           /*********************************************************/ 02793000
*           /*                                                       */ 02794000
*           /* FSB JUST PROCESSED WAS A SUBBLOCK AND THERE IS ROOM IN*/ 02795000
*           /* THE USER'S BUFFER FOR MORE DATA. SAVE STATUS IN THE   */ 02796000
*           /* DCLCP FOR WHEN ANOTHER FSB ARRIVES SO THE MOVE CAN BE */ 02797000
*           /* RESUMED.                                              */ 02798000
*           /*                                                       */ 02799000
*           /*********************************************************/ 02800000
*                                                                  1224 02801000
*           DCLIOBUF=CRRBARAD;      /* MOVE ADDR OF NEXT AVAIL SPACE    02802000
*                                      IN USER BUFFER TO DCLCP     1224 02803000
*                                      EXTENSION                     */ 02804000
         L     @15,CRRLCPB(,CRRPTR)                                1224 02805000
         L     @05,CRRBARAD(,CRRPTR)                               1224 02806000
         ST    @05,DCLIOBUF(,@15)                                  1224 02807000
*           DCLIOLEN=CRRBLEN;       /* MOVE RESIDUAL COUNT TO DCLCP  */ 02808000
         LH    @05,CRRBLEN(,CRRPTR)                                1225 02809000
         STH   @05,DCLIOLEN(,@15)                                  1225 02810000
*           DCLBTLEN=CRRBTLEN;      /* PUT LENGTH MOVED IN DCLCP     */ 02811000
         L     @05,CRRBTLEN(,CRRPTR)                               1226 02812000
         ST    @05,DCLBTLEN(,@15)                                  1226 02813000
*           DCLRPLA=CRRARPL;        /* PUT ADR OF RPL IN DCLCP       */ 02814000
         L     @05,CRRARPL(,CRRPTR)                                1227 02815000
         ST    @05,DCLRPLA(,@15)                                   1227 02816000
*         END;                                                     1228 02817000
*     ELSE                                                         1229 02818000
*       DO;                                                        1229 02819000
*                                                                  1229 02820000
         B     @RC01220                                            1229 02821000
@RF01220 DS    0H                                                  1230 02822000
*         /***********************************************************/ 02823000
*         /*                                                         */ 02824000
*         /* FSB JUST PROCESSED WAS A SUBBLOCK BUT THE USER'S BUFFER */ 02825000
*         /* IS FILLED. RESET FEEDBACK STATUS TO INDICATE NOT ALL    */ 02826000
*         /* DATA WAS RECEIVED. SAVE FSB TO BE USED FOR NEXT READ.   */ 02827000
*         /*                                                         */ 02828000
*         /***********************************************************/ 02829000
*                                                                  1230 02830000
*         CRRPOST=ON;               /* SET DO POST SWITCH            */ 02831000
         OI    CRRPOST(CRRPTR),B'00010000'                         1230 02832000
*         CRRRDEOB=OFF;             /* TURN OFF EOB,EOM,EOT          */ 02833000
*         CRRRDEOM=OFF;                                            1232 02834000
*         CRRRDEOT=OFF;                                            1233 02835000
         NI    CRRRDEOB(CRRPTR),B'11000111'                        1233 02836000
*       END;                                                       1234 02837000
*   RTNREG=CRRRTN4;                 /* RESTORE RETURN REG            */ 02838000
@RC01220 DS    0H                                                  1235 02839000
@RC01218 L     RTNREG,CRRRTN4(,CRRPTR)                             1235 02840000
*   END KEEP1;                                                     1236 02841000
@EL00014 DS    0H                                                  1236 02842000
@EF00014 DS    0H                                                  1236 02843000
@ER00014 BR    @14                                                 1236 02844000
*LCPBINIT:                                                         1237 02845000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1237 02846000
*                                                                  1237 02847000
LCPBINIT DS    0H                                                  1238 02848000
*/*****************************************************************  */ 02849000
*/*                                                                  */ 02850000
*/* ROUTINE NAME : LCPBINIT                                          */ 02851000
*/*                                                                  */ 02852000
*/* FUNCTION :                                                       */ 02853000
*/*                                                                  */ 02854000
*/*  THIS ROUTINE IS ENTERED TO GET SPACE FOR AND TO INITIALIZE      */ 02855000
*/*  A LCPB.  THIS IS CALLED BY XFRFSB WHEN BUFFER THRESHOLD HAS     */ 02856000
*/*  BEEN EXCEEDED AND I/O IS TO BE RESET.  IT IS ALSO CALLED BY     */ 02857000
*/*  FLUSH IF THE FSB BEING FLUSHED HAS THE ERROR LOCK SET AND AN    */ 02858000
*/*  INTERNAL RESET ERROR LOCK IS TO BE ISSUED.  THE DCLCP IS        */ 02859000
*/*  INITIALIZED TO BE USED FOR RESET OPERATIONS.                    */ 02860000
*/*                                                                  */ 02861000
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */ 02862000
*/*                                                                  */ 02863000
*/* OUTPUT : SAME                                                    */ 02864000
*/*                                                                  */ 02865000
*/* SUBROUTINES CALLED : NONE                                        */ 02866000
*/*                                                                  */ 02867000
*/* MACROS USED :  REQSTORE, TPWAIT, TPQUE                           */ 02868000
*/*                                                                  */ 02869000
*/********************************************************************/ 02870000
*                                                                  1238 02871000
*   CRRRTN8=RTNREG;                 /* SAVE RETURN ADDRESS           */ 02872000
*                                                                  1238 02873000
         ST    RTNREG,CRRRTN8(,CRRPTR)                             1238 02874000
*   /*****************************************************************/ 02875000
*   /*                                                               */ 02876000
*   /* GET STORAGE FOR DCLCPB AND WAIT IF STORAGE NOT AVAILABLE      */ 02877000
*   /*                                                               */ 02878000
*   /*****************************************************************/ 02879000
*                                                                  1239 02880000
*   RFY                                                            1239 02881000
*     RTCODREG RSTD;                                               1239 02882000
*                                                                  1239 02883000
*   /*****************************************************************/ 02884000
*   /*                                                               */ 02885000
*   /* REQSTORE CBID(DCLCP)RPH(RPHPTR)CLEAR(YES)PRIORITY(YES)        */ 02886000
*   /*                                                               */ 02887000
*   /*****************************************************************/ 02888000
*                                                                  1240 02889000
*   DO;                                                            1240 02890000
*                                                                  1240 02891000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)                               */ 02892000
*/* END OF ATBUILD                                                   */ 02893000
*                                                                  1241 02894000
*     RFY                                                          1241 02895000
*      (GPR02P,                                                    1241 02896000
*       GPR03P,                                                    1241 02897000
*       GPR04P,                                                    1241 02898000
*       GPR15P) RSTD;                                              1241 02899000
*     RFY                                                          1242 02900000
*      (GPR01P,                                                    1242 02901000
*       GPR13P) RSTD;                                              1242 02902000
*     GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);           1243 02903000
           STM GPR02P,GPR13P,RPHWORK(GPR01P)                            02904000
*     RFY                                                          1244 02905000
*      (GPR01P,                                                    1244 02906000
*       GPR13P) UNRSTD;                                            1244 02907000
*     DO;                           /* LISATCVT                    1245 02908000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 02909000
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 02910000
         L     GPR15P,ATCLCPTR                                     1246 02911000
*     END;                          /* LISATCVT                      */ 02912000
*     GPR02P=GPR15P->ATCBPDA|'80000000'X;                          1248 02913000
         L     GPR02P,ATCBPDA(,GPR15P)                             1248 02914000
         O     GPR02P,@CF01547                                     1248 02915000
*     DCL                                                          1249 02916000
*       1 FMZA0150 FIXED(31) STATIC LOCAL,                         1249 02917000
*        2 * FIXED(8) INIT(248),                                   1249 02918000
*        2 * PTR(24) INIT(0);                                      1249 02919000
*     GPR03F=FMZA0150;                                             1250 02920000
         L     GPR03F,FMZA0150                                     1250 02921000
*     GPR04F=5;                                                    1251 02922000
         LA    GPR04F,5                                            1251 02923000
*     GPR15P=GPR15P->ATCSMRQ;                                      1252 02924000
         L     GPR15P,ATCSMRQ(,GPR15P)                             1252 02925000
*     DCL                                                          1253 02926000
*       FMZL0150 BASED(GPR15P) LABEL VALUERANGE(*);                1253 02927000
*     CALL FMZL0150;                                               1254 02928000
         BALR  @14,@15                                             1254 02929000
*     RFY                                                          1255 02930000
*      (GPR13P) RSTD;                                              1255 02931000
*     GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);           1256 02932000
           LM GPR02P,GPR13P,RPHWORK(GPR01P)                             02933000
*     RFY                                                          1257 02934000
*      (GPR13P) UNRSTD;                                            1257 02935000
*     RFY                                                          1258 02936000
*      (GPR02P,                                                    1258 02937000
*       GPR03P,                                                    1258 02938000
*       GPR04P,                                                    1258 02939000
*       GPR15P) UNRSTD;                                            1258 02940000
*   END;                            /* ******** END OF REQSTORE    1259 02941000
*                                      EXPANSION                     */ 02942000
*   IF RTCODREG=X8 THEN             /* WAS STORAGE AVAILABLE?        */ 02943000
         C     RTCODREG,@CF00036                                   1260 02944000
         BNE   @RF01260                                            1260 02945000
*     CALL WAITUP;                  /* NO, WAIT FOR STORAGE          */ 02946000
         BAL   @14,WAITUP                                          1261 02947000
*   ELSE                                                           1262 02948000
*     ;                             /* YES                           */ 02949000
@RF01260 DS    0H                                                  1263 02950000
*   RFY                                                            1263 02951000
*     RTCODREG UNRSTD;                                             1263 02952000
*                                                                  1263 02953000
*   /*****************************************************************/ 02954000
*   /*                                                               */ 02955000
*   /* INITIALIZE ISTDCLCP FOR RESET OPERATION                       */ 02956000
*   /*                                                               */ 02957000
*   /*****************************************************************/ 02958000
*                                                                  1264 02959000
*   RFY                                                            1264 02960000
*     ISTDCLCP BASED(LCPBPTR);                                     1264 02961000
*   RFY                                                            1265 02962000
*     LCPBPTR RSTD;                 /* RESTRICT DCLCP BASE REG       */ 02963000
*   LCPBPTR=RPHSRPRM;               /* REFERENCE STORAGE             */ 02964000
         L     LCPBPTR,RPHSRPRM(,RPHPTR)                           1266 02965000
*   LCPTYPE=DCLTYPE;                /* INDICATE DCLCP                */ 02966000
         MVI   LCPTYPE(LCPBPTR),X'12'                              1267 02967000
*   LCPLNGTH=LENGTH(ISTDCLCP);      /* INITAILIZE LENGTH             */ 02968000
         MVI   LCPLNGTH(LCPBPTR),X'64'                             1268 02969000
*   LCPNXLCW=ADDR(DCLLCCW2);        /* POINT TO RESET LCCW SLOT      */ 02970000
         LA    @15,DCLLCCW2(,LCPBPTR)                              1269 02971000
         ST    @15,LCPNXLCW(,LCPBPTR)                              1269 02972000
*   LCPDAF=NCBDAF;                  /* INITIALIZE DESTINATION ID     */ 02973000
         L     @15,CRRFMCB(,CRRPTR)                                1270 02974000
         L     @08,FMCDNCBA(,@15)                                  1270 02975000
         MVC   LCPDAF(2,LCPBPTR),NCBDAF(@08)                       1270 02976000
*   LCPSAF=FMCSAF;                  /* INITIALIZE SOURCE ID          */ 02977000
         MVC   LCPSAF(2,LCPBPTR),FMCSAF(@15)                       1271 02978000
*   DCLRQTP=DCLRESET;               /* INDICATE LCPB FOR RESET OP    */ 02979000
         MVI   DCLRQTP(LCPBPTR),X'12'                              1272 02980000
*   RFY                                                            1273 02981000
*     ISTLCCW BASED(ADDR(DCLLCCW2));/* COVER 2ND LCCW SLOT           */ 02982000
*   IF CRRSW5=ON THEN               /* IF THRESHOLD EXCEEDED         */ 02983000
         TM    CRRSW5(CRRPTR),B'00000100'                          1274 02984000
         BNO   @RF01274                                            1274 02985000
*     LCCWOP=LXEL;                  /* BUILD RESET ERR LOCK          */ 02986000
         MVI   LCCWOP+56(LCPBPTR),X'0B'                            1275 02987000
*   ELSE                                                           1276 02988000
*     IF DEVCHAR2>=DEV2980&DEVCHAR2<=DEV3275 THEN/* 2980 OR 3270?    */ 02989000
         B     @RC01274                                            1276 02990000
@RF01274 L     @15,CRRFMCB(,CRRPTR)                                1276 02991000
         LA    @08,36                                              1276 02992000
         AL    @08,FMCDNCBA(,@15)                                  1276 02993000
         CLI   DEVCHAR2(@08),X'18'                                 1276 02994000
         BL    @RF01276                                            1276 02995000
         CLI   DEVCHAR2(@08),X'1C'                                 1276 02996000
         BH    @RF01276                                            1276 02997000
*       LCCWOP=LXEC;                /* RESET AT END OF CMD           */ 02998000
         MVI   LCCWOP+56(LCPBPTR),X'4B'                            1277 02999000
*     ELSE                                                         1278 03000000
*       IF DEVCRVB=ON THEN          /* START-STOP REVERSE BREAK      */ 03001000
         B     @RC01276                                            1278 03002000
@RF01276 L     @15,CRRFMCB(,CRRPTR)                                1278 03003000
         L     @15,FMCDNCBA(,@15)                                  1278 03004000
         TM    DEVCRVB+36(@15),B'00100000'                         1278 03005000
         BNO   @RF01278                                            1278 03006000
*         LCCWOP=LXI;               /* RESET IMMEDIATE LCCW          */ 03007000
         MVI   LCCWOP+56(LCPBPTR),X'2B'                            1279 03008000
*       ELSE                                                       1280 03009000
*         IF DEVCBSC=ON THEN        /* BSC DEVICE                    */ 03010000
         B     @RC01278                                            1280 03011000
@RF01278 L     @15,CRRFMCB(,CRRPTR)                                1280 03012000
         L     @15,FMCDNCBA(,@15)                                  1280 03013000
         TM    DEVCBSC+36(@15),B'10000000'                         1280 03014000
         BNO   @RF01280                                            1280 03015000
*           LCCWOP=LXO;             /* RESET ORDERLY                 */ 03016000
         MVI   LCCWOP+56(LCPBPTR),X'5B'                            1281 03017000
*         ELSE                      /* START-STOP W/O REVERSE BK     */ 03018000
*           LCCWOP=LXEC;            /* RESET END OF COMMAND LCCW     */ 03019000
         B     @RC01280                                            1282 03020000
@RF01280 MVI   LCCWOP+56(LCPBPTR),X'4B'                            1282 03021000
*   DCLSRESW=ON;                    /* INDICATE INTERNAL RESET       */ 03022000
@RC01280 DS    0H                                                  1283 03023000
@RC01278 DS    0H                                                  1283 03024000
@RC01276 DS    0H                                                  1283 03025000
@RC01274 OI    DCLSRESW(LCPBPTR),B'10000000'                       1283 03026000
*/*  TPQUE ((LCPBPTR),FMCPAB3) RESET                                 */ 03027000
*                                                                  1284 03028000
*   DO;                                                            1284 03029000
*     RFY                                                          1285 03030000
*      (GPR00P,                                                    1285 03031000
*       GPR01P,                                                    1285 03032000
*       GPR02P,                                                    1285 03033000
*       GPR15P) RSTD;                                              1285 03034000
*     GEN REFS(GPR01P) SETS(RPHWORK)                               1286 03035000
*         (STM   GPR14P,GPR09P,RPHWORK(GPR01P));                   1286 03036000
         STM   GPR14P,GPR09P,RPHWORK(GPR01P)                            03037000
*     GPR00P=ADDR(FMCPAB3);                                        1287 03038000
         L     @08,CRRFMCB(,CRRPTR)                                1287 03039000
         LA    GPR00P,FMCPAB3(,@08)                                1287 03040000
*     GPR02P=(LCPBPTR);                                            1288 03041000
         LR    GPR02P,LCPBPTR                                      1288 03042000
*     CALL ATCLCPTR->ATCAP34->ISTAPC34;                            1289 03043000
         L     @08,ATCLCPTR                                        1289 03044000
         L     @15,ATCAP34(,@08)                                   1289 03045000
         BALR  @14,@15                                             1289 03046000
*     GEN REFS(RPHWORK,GPR01P) NOSETS                              1290 03047000
*         (LM    GPR14P,GPR09P,RPHWORK(GPR01P));                   1290 03048000
         LM    GPR14P,GPR09P,RPHWORK(GPR01P)                            03049000
*     RFY                                                          1291 03050000
*      (GPR00P,                                                    1291 03051000
*       GPR01P,                                                    1291 03052000
*       GPR02P,                                                    1291 03053000
*       GPR15P) UNRSTD;                                            1291 03054000
*   END;                            /* *** END OF TPQUE GENERATION 1292 03055000
*                                      SHIP TO TPIOS                 */ 03056000
*   RFY                                                            1293 03057000
*     LCPBPTR UNRSTD;                                              1293 03058000
*   RFY                                                            1294 03059000
*     ISTDEVCH;                                                    1294 03060000
*   RFY                                                            1295 03061000
*     DEVCHPTR UNRSTD;                                             1295 03062000
*   RTNREG=CRRRTN8;                 /* PICK UP RETURN ADDRESS        */ 03063000
         L     RTNREG,CRRRTN8(,CRRPTR)                             1296 03064000
*   END LCPBINIT;                   /* RETURN TO CALLER              */ 03065000
@EL00015 DS    0H                                                  1297 03066000
@EF00015 DS    0H                                                  1297 03067000
@ER00015 BR    @14                                                 1297 03068000
*WAITUP:                                                           1298 03069000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1298 03070000
WAITUP   DS    0H                                                  1299 03071000
*   RFY                                                            1299 03072000
*     RTNREG RSTD;                                                 1299 03073000
*   CRRRTN2=RTNREG;                                                1300 03074000
         ST    RTNREG,CRRRTN2(,CRRPTR)                             1300 03075000
*   RFY                                                            1301 03076000
*     RTNREG UNRSTD;                                               1301 03077000
*                                                                  1301 03078000
*/*  TPWAIT                                                          */ 03079000
*                                                                  1302 03080000
*   DO;                             /* BEGIN TPWAIT GENERATION       */ 03081000
*     RFY                                                          1303 03082000
*      (GPR01P,                                                    1303 03083000
*       GPR14P,                                                    1303 03084000
*       GPR15P) RSTD;                                              1303 03085000
*     RFY                                                          1304 03086000
*      (GPR00P) RSTD;                                              1304 03087000
*     GEN(STM   GPR00P,GPR15P,RPHWORK(GPR01P))                     1305 03088000
*     REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);                    1305 03089000
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)                            03090000
*     RFY                                                          1306 03091000
*      (GPR00P) UNRSTD;                                            1306 03092000
*     GPR15P=ADDR(LHAA0175);                                       1307 03093000
         LA    GPR15P,LHAA0175                                     1307 03094000
*     GPR14P=0;                                                    1308 03095000
         SLR   GPR14P,GPR14P                                       1308 03096000
*     GPR15P=GPR15P&'00FFFFFF'X;                                   1309 03097000
         N     GPR15P,@CF04830                                     1309 03098000
*     GPR15P=GPR15P|RPHWT1;                                        1310 03099000
         O     GPR15P,@CF01571                                     1310 03100000
*     CS(GPR14P,GPR15P,GPR01P->RPHRESMA);                          1311 03101000
