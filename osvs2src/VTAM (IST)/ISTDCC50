*/*** END -TPLOCK (CTLLOCK)TYPE(XCLSIVE)- **                         */ 00100000
*                                                                  0508 00101000
*                                   /* GET LOCK              @ZA33377*/ 00102000
*       CRRFMCLK=ON;                /* FMCB LOCK HELD        @ZA33377*/ 00103000
         OI    CRRFMCLK(CRRPTR),B'10000000'                        0508 00104000
*     END;                          /* LOCK FMCB             @ZA33377*/ 00105000
*   IF FMCFLGA3=ON THEN             /* FLUSH BIT ON IN FMCB          */ 00106000
@RF00493 L     @15,CRRFMCB(,CRRPTR)                                0510 00107000
         TM    FMCFLGA3(@15),B'00010000'                           0510 00108000
         BNO   @RF00510                                            0510 00109000
*     IF FSBMJRCD='00'X THEN        /* RETURN CODE NORMAL?   @ZA33102*/ 00110000
*                                                                  0511 00111000
         CLI   FSBMJRCD(FSBPTR),X'00'                              0511 00112000
         BNE   @RF00511                                            0511 00113000
*       /*************************************************************/ 00114000
*       /*                                                           */ 00115000
