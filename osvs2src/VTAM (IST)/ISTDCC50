*                                                                  1012 01851000
*/********************************************************************/ 01852000
*/*                                                                  */ 01853000
*/* ROUTINE NAME : MOVEDATA                                          */ 01854000
*/*                                                                  */ 01855000
*/* FUNCTION :                                                       */ 01856000
*/*  MOVEDATA IS CALLED BY EITH READMAC OR WRTMAC TO MOVE DATA TO    */ 01857000
*/*   USER BUFFERS. THE PARM LIST FOR THE MOVE ROUTINE, ISTDCC60,    */ 01858000
*/*   HAS BEEN BUILT IN THE CRR BY THE CALLER.  IF THERE WAS NO      */ 01859000
*/*   IS BUILT IN THE CRR USING INFORMATION FROM EITHER THE RPL      */ 01860000
*/*   OR THE DCLCP.  THE DCLCP IS USED IF DATA TO BE MOVED WAS       */ 01861000
*/*   SAVED FROM AN EARLIER REQUEST.                                 */ 01862000
*/*                                                                  */ 01863000
*/*  IF THERE WAS AN ERROR DURING THE MOVE, THE FSB IS QUEUED        */ 01864000
*/*   TO THE FMCB DATA QUEUE.                                        */ 01865000
*/*                                                                  */ 01866000
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */ 01867000
*/*                                                                  */ 01868000
*/* OUTPUT : FSBPTR REG(4) - ADDRESS OF FSB                          */ 01869000
*/*                                                                  */ 01870000
*/* SUBROUTINES CALLED : ISTDCC60 - MOVE DATA TO USER BUFFERS        */ 01871000
*/*                      XFRFSB   - QUEUE FSB TO FMCB DATA QUEUE     */ 01872000
*/*                      TRUNC    _ PROCESS USER TRUNC OPTION        */ 01873000
*/*                      KEEP     - PROCESS USER KEEP OPTION         */ 01874000
*/*                                                                  */ 01875000
*/* MACROS USED : VSETLCK, VMDESET                                   */ 01876000
*/*                                                                  */ 01877000
*/********************************************************************/ 01878000
*                                                                  1012 01879000
*MOVEDATA:                                                         1012 01880000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1012 01881000
MOVEDATA DS    0H                                                  1013 01882000
*   CRRRTN6=RTNREG;                 /* SAVE RETURN REG               */ 01883000
         ST    RTNREG,CRRRTN6(,CRRPTR)                             1013 01884000
*   CRRBLDAD=ZEROWORD;              /* ZERO DATA LENGTH              */ 01885000
         SLR   @15,@15                                             1014 01886000
         ST    @15,CRRBLDAD(,CRRPTR)                               1014 01887000
*   IF DCLIOBUF=ZEROWORD THEN       /* NO USER BUFFER                */ 01888000
         L     @05,CRRLCPB(,CRRPTR)                                1015 01889000
         C     @15,DCLIOBUF(,@05)                                  1015 01890000
         BNE   @RF01015                                            1015 01891000
*     DO;                           /* USE RPL BUFFER                */ 01892000
*       IF DCLRQTP=DCLWRITE         /* WRITE REQUEST         @DM09829*/ 01893000
*           &RPLWROPT=ON THEN       /* WRITE CONV?           @DM09257*/ 01894000
         CLI   DCLRQTP(@05),X'11'                                  1017 01895000
         BNE   @RF01017                                            1017 01896000
         L     @15,CRRARPL(,CRRPTR)                                1017 01897000
         TM    RPLWROPT(@15),B'00000001'                           1017 01898000
         BNO   @RF01017                                            1017 01899000
*         DO;                                                      1018 01900000
*           CRRBARAD=RPLAAREA;      /* ALT ADDRESS           @DM09257*/ 01901000
         L     @05,RPLAAREA(,@15)                                  1019 01902000
         ST    @05,CRRBARAD(,CRRPTR)                               1019 01903000
*           CRRBLEN=RPLAARLN;       /* LENGTH OF ALT ADDR    @DM09257*/ 01904000
         L     @15,RPLAARLN(,@15)                                  1020 01905000
         STH   @15,CRRBLEN(,CRRPTR)                                1020 01906000
*         END;                                                     1021 01907000
*       ELSE                                                       1022 01908000
*         DO;                                                      1022 01909000
         B     @RC01017                                            1022 01910000
@RF01017 DS    0H                                                  1023 01911000
*           CRRBARAD=RPLAREA;       /* ADDRESS OF USER AREA          */ 01912000
         L     @15,CRRARPL(,CRRPTR)                                1023 01913000
         L     @05,RPLAREA(,@15)                                   1023 01914000
         ST    @05,CRRBARAD(,CRRPTR)                               1023 01915000
*           CRRBLEN=RPLBUFL;        /* LENGTH OF USER BUFFER         */ 01916000
         L     @15,RPLBUFL(,@15)                                   1024 01917000
         STH   @15,CRRBLEN(,CRRPTR)                                1024 01918000
*         END;                                                     1025 01919000
*       CRRBTLEN=ZEROWORD;          /* ZERO DATA LENGTH              */ 01920000
@RC01017 SLR   @15,@15                                             1026 01921000
         ST    @15,CRRBTLEN(,CRRPTR)                               1026 01922000
*     END;                                                         1027 01923000
*   ELSE                                                           1028 01924000
*     DO;                           /* DATA IS PARTIALLY MOVED       */ 01925000
         B     @RC01015                                            1028 01926000
@RF01015 DS    0H                                                  1029 01927000
*       CRRBARAD=DCLIOBUF;          /* ADDRESS OF OLD BUFFER         */ 01928000
         L     @15,CRRLCPB(,CRRPTR)                                1029 01929000
         L     @05,DCLIOBUF(,@15)                                  1029 01930000
         ST    @05,CRRBARAD(,CRRPTR)                               1029 01931000
*       CRRBLEN=DCLIOLEN;           /* LENGTH OF BUFFER              */ 01932000
         LH    @05,DCLIOLEN(,@15)                                  1030 01933000
