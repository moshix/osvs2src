         L     REG2,FMCCMPS1(,@15)                                 1138 02341000
*       REG3=REG2&BUFNB0;           /* SET COUNT TO ZERO             */ 02342000
         LR    REG3,REG2                                           1139 02343000
         N     REG3,@CF04668                                       1139 02344000
*       CS(REG2,REG3,FMCCMPS1);     /* SET NEW COUNT IN FMCB         */ 02345000
         CS    REG2,@03,FMCCMPS1(@15)                              1140 02346000
*       BC(7,CS7);                  /* TRY AGAIN IF FAILURE          */ 02347000
         BC    7,CS7                                               1141 02348000
*       RFY                                                        1142 02349000
*        (REG2,                                                    1142 02350000
*         REG3) UNRSTD;                                            1142 02351000
*       IF FSBPTR^=ZEROWORD&FSBPTR^=GATEBIT THEN/* WAS FSB DEQUED?   */ 02352000
         LTR   FSBPTR,FSBPTR                                       1143 02353000
         BZ    @RF01143                                            1143 02354000
         CL    FSBPTR,@CF01547                                     1143 02355000
         BE    @RF01143                                            1143 02356000
*         CALL FREEBUF;             /* YES, RELEASE IT               */ 02357000
         BAL   @14,FREEBUF                                         1144 02358000
*       ELSE                                                       1145 02359000
*         ;                         /* NO FSB ON DATA Q              */ 02360000
@RF01143 DS    0H                                                  1146 02361000
*     END;                                                         1146 02362000
*   ELSE                                                           1147 02363000
*     CALL FREEBUF;                 /* FREE CURRENT FSB              */ 02364000
         B     @RC01113                                            1147 02365000
@RF01113 BAL   @14,FREEBUF                                         1147 02366000
*   IF CRRSW5=ON THEN               /* WAS ERROR LOCK SET?           */ 02367000
*                                                                  1148 02368000
@RC01113 TM    CRRSW5(CRRPTR),B'00000100'                          1148 02369000
         BNO   @RF01148                                            1148 02370000
*     /***************************************************************/ 02371000
*     /*                                                             */ 02372000
*     /* IF THE ERROR LOCK IS SET IN THE FSB BEING FLUSHED, THE ERROR*/ 02373000
*     /* LOCK MUST BE RESET INTERNALLY SINCE THE USER WILL NOT KNOW  */ 02374000
*     /* IT IS SET. CALL LCPBINIT TO GET STORAGE FOR A LCPB. BUILD   */ 02375000
*     /* RESET ERROR LOCK AND QUEUE TO TPIOS OUTBOUND PAB WITH RESET */ 02376000
*     /* OPTION.                                                     */ 02377000
*     /*                                                             */ 02378000
*     /***************************************************************/ 02379000
*                                                                  1149 02380000
*     CALL LCPBINIT;                /* GET LCPB FOR RESET            */ 02381000
         BAL   @14,LCPBINIT                                        1149 02382000
*   ELSE                                                           1150 02383000
*     ;                             /* ERROR LOCK NOT SET            */ 02384000
@RF01148 DS    0H                                                  1151 02385000
*   RTNREG=CRRRTN1;                 /* RESTORE RETURN REG            */ 02386000
         L     RTNREG,CRRRTN1(,CRRPTR)                             1151 02387000
*   END FLUSH;                                                     1152 02388000
@EL00010 DS    0H                                                  1152 02389000
@EF00010 DS    0H                                                  1152 02390000
@ER00010 BR    @14                                                 1152 02391000
*                                                                  1153 02392000
*/********************************************************************/ 02393000
*/*                                                                  */ 02394000
*/* ROUTINE NAME : TRUNC                                             */ 02395000
*/*                                                                  */ 02396000
*/*  FUNCTITION :                                                    */ 02397000
*/* TRUNC IS CALLED BY MOVEDATA TO PROCESS THE USER TRUNCATE         */ 02398000
*/* OPTION.  IF THERE WAS NO ERROR IN THE MOVE, SUBROUTINE           */ 02399000
*/* TRUNC1 IS CALLED.   ON RETURN FROM TRUNC1, THE FSB AND FDBS      */ 02400000
*/* ARE FREED AND THE SUBROUTINE RETURNS TO MOVEDATA.                */ 02401000
*/*                                                                  */ 02402000
*/* IF THERE WAS AN ERROR DURING THE MOVE, ERROR FLGAS ARE SET IN    */ 02403000
*/* THE CRR(CRRTRUN AND CRRLOGIC).  THE DO POST SWITCH,              */ 02404000
*/* CRRPOST, IS SET AND SUBROUTINE EOBANAL IS CALLED.  IF            */ 02405000
*/* EOB WAS NOT RECEIVED, THE FLUSH BIT IS SET.  IN EITHER           */ 02406000
*/* CASE, THE FSB AND FDBS ARE FREED AND THE ROUTINE                 */ 02407000
*/* RETURNS TO MOVEDATA VIA REG 14.                                  */ 02408000
*/*                                                                  */ 02409000
*/* INPUT :  FSBPTR REG(4) - ADDRESS OF FSB                          */ 02410000
*/*                                                                  */ 02411000
*/* OUTPUT : SAME                                                    */ 02412000
*/*                                                                  */ 02413000
*/* SUBROUTINES CALLED : EOBANAL  - CHECK FOR ENDING STATUS          */ 02414000
*/*                      TRUNC1   - MORE TRUNC PROCESSING NECESSARY  */ 02415000
*/* MACROS USED : NONE                                               */ 02416000
*/*                                                                  */ 02417000
*/********************************************************************/ 02418000
*                                                                  1153 02419000
*TRUNC:                                                            1153 02420000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1153 02421000
TRUNC    DS    0H                                                  1154 02422000
*   CRRRTN3=RTNREG;                 /* SAVE RETURN REG               */ 02423000
         ST    RTNREG,CRRRTN3(,CRRPTR)                             1154 02424000
*   IF CRRBRTCD=CRRBRSUC THEN       /* RETURN CODE FROM MOVE OK      */ 02425000
*                                                                  1155 02426000
         CLI   CRRBRTCD(CRRPTR),0                                  1155 02427000
         BNE   @RF01155                                            1155 02428000
*     /***************************************************************/ 02429000
*     /*                                                             */ 02430000
*     /* ALL THE DATA FROM ONE FSB MOVED                             */ 02431000
*     /*                                                             */ 02432000
*     /***************************************************************/ 02433000
*                                                                  1156 02434000
*     CALL TRUNC1;                                                 1156 02435000
         BAL   @14,TRUNC1                                          1156 02436000
*   ELSE                                                           1157 02437000
*     DO;                                                          1157 02438000
*                                                                  1157 02439000
         B     @RC01155                                            1157 02440000
@RF01155 DS    0H                                                  1158 02441000
*       /*************************************************************/ 02442000
*       /*                                                           */ 02443000
*       /* DATA TRUNCATED                                            */ 02444000
*       /*                                                           */ 02445000
*       /*************************************************************/ 02446000
*                                                                  1158 02447000
*                                   /* D31JKMQ                       */ 02448000
*       CRRPOST=ON;                 /* SET DO POST SWITCH            */ 02449000
         OI    CRRPOST(CRRPTR),B'00010000'                         1158 02450000
*       CALL EOBANAL;               /* CHECK FOR EOB, EOM, EOT       */ 02451000
         BAL   @14,EOBANAL                                         1159 02452000
*       IF FSBEBI=ON|               /* EOM,EOT,EOB                   */ 02453000
*           FSBLGI=ON|              /* LEADING GRAPHICS?             */ 02454000
*           FSBSOHI=ON|             /* START OF HEADING (SOH)      1160 02455000
*                                      INDICATOR                     */ 02456000
*           FSBMJRCD^=FRNORM THEN                                  1160 02457000
         TM    FSBEBI(FSBPTR),B'00100011'                          1160 02458000
         BNZ   @RT01160                                            1160 02459000
         CLI   FSBMJRCD(FSBPTR),X'00'                              1160 02460000
         BNE   @RT01160                                            1160 02461000
*         ;                         /* ERROR CODE IN FSB?            */ 02462000
*       ELSE                                                       1162 02463000
*         DO;                                                      1162 02464000
*                                                                  1162 02465000
*           /*********************************************************/ 02466000
*           /*                                                       */ 02467000
*           /* SUBBLOCK-INDICATE REMAINING SUBBLOCKS SHOULD BE       */ 02468000
*           /* FLUSHED AS THEY ARRIVE                                */ 02469000
*           /*                                                       */ 02470000
*           /*********************************************************/ 02471000
*                                                                  1163 02472000
*           RFY                                                    1163 02473000
*            (REG2,                                                1163 02474000
*             REG3) RSTD;                                          1163 02475000
*CS8:                                                              1164 02476000
*           REG2=FMCCMPS2;          /* REFERENCE FLAG VALUE          */ 02477000
CS8      L     @15,CRRFMCB(,CRRPTR)                                1164 02478000
         L     REG2,FMCCMPS2(,@15)                                 1164 02479000
*           REG3=REG2|FLGA3ON;      /* SET FLUSH INDICATOR           */ 02480000
         LR    REG3,REG2                                           1165 02481000
         O     REG3,@CF01565                                       1165 02482000
*           CS(REG2,REG3,FMCCMPS2); /* SET NEW VALUE IN FMCB         */ 02483000
         CS    REG2,@03,FMCCMPS2(@15)                              1166 02484000
*           BC(7,CS8);              /* TRY AGAIN IF FAILURE          */ 02485000
         BC    7,CS8                                               1167 02486000
*           RFY                                                    1168 02487000
*            (REG2,                                                1168 02488000
*             REG3) UNRSTD;                                        1168 02489000
*         END;                                                     1169 02490000
*     END;                                                         1170 02491000
@RT01160 DS    0H                                                  1171 02492000
*   CALL FREEBUF;                   /* FREE FSB AND FDBS             */ 02493000
@RC01155 BAL   @14,FREEBUF                                         1171 02494000
*   RTNREG=CRRRTN3;                 /* RESTORE RETURN REG            */ 02495000
         L     RTNREG,CRRRTN3(,CRRPTR)                             1172 02496000
*   END TRUNC;                                                     1173 02497000
@EL00011 DS    0H                                                  1173 02498000
@EF00011 DS    0H                                                  1173 02499000
@ER00011 BR    @14                                                 1173 02500000
*                                                                  1174 02501000
*/********************************************************************/ 02502000
*/*                                                                  */ 02503000
*/* ROUTINE NAME : TRUNC1                                            */ 02504000
*/*                                                                  */ 02505000
*/* FUNCTION:                                                        */ 02506000
*/* TRUNC1 IS CALLED BY TRUNC IF THE USER SPECIFIED THE TRUNC        */ 02507000
*/* OPTION AND NO ERROR CODE WAS RETURNED BY THE MOVE ROUTINE.       */ 02508000
*/* IF EOB WAS NOT RECEIVED IN THE FSB AND THERE IS MORE SPACE       */ 02509000
*/* IN THE USER AREA, THE BUFFER INFORMATION IS MOVED FROM THE       */ 02510000
*/* CRR TO THE DCLCP AND THE RPL PTR IS PUT BACK IN THE DCLCP.       */ 02511000
*/* IF THERE IS NO SPACE LEFT IN THE USER AREA, ERROR INDICATORS     */ 02512000
*/* (CRRTRUN AND CRRLOGIC) ARE SET IN THE CRR.  THE FLUSH BIT        */ 02513000
*/* IS SET TO FREE THE FSB AND FDBS.  THE SUBROUTINE RETURNS TO      */ 02514000
*/*                                                                  */ 02515000
*/* INPUT :  FSBPTR REG(4) - ADDRESS OF FSB                          */ 02516000
*/*                                                                  */ 02517000
*/* OUTPUT : SAME                                                    */ 02518000
*/*                                                                  */ 02519000
*/* SUBROUTINES CALLED : EOBANAL  - CHECK FOR ENDING STATUS          */ 02520000
*/*                                                                  */ 02521000
*/* MACROS USED :  NONE                                              */ 02522000
*/*                                                                  */ 02523000
*/********************************************************************/ 02524000
*                                                                  1174 02525000
*TRUNC1:                                                           1174 02526000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1174 02527000
TRUNC1   DS    0H                                                  1175 02528000
*   CRRRTN4=RTNREG;                 /* SAVE RETURN REG               */ 02529000
         ST    RTNREG,CRRRTN4(,CRRPTR)                             1175 02530000
*   CALL EOBANAL;                   /* CHECK FOR EOM,EOB,EOT         */ 02531000
         BAL   @14,EOBANAL                                         1176 02532000
*   IF FSBEBI=ON|FSBECI=ON|         /* EOM,EOB,EOT           @OX11109*/ 02533000
*       FSBLGI=ON|                  /* LEADING GRAPHICS IN FSB       */ 02534000
*       FSBSOHI=ON|                 /* START OF HEADING(SOH)INDICATOR*/ 02535000
*       FSBMJRCD^=FRNORM THEN       /* ERROR RETURN CODE IN FSB      */ 02536000
*                                                                  1177 02537000
         TM    FSBEBI(FSBPTR),B'01100011'                          1177 02538000
         BNZ   @RT01177                                            1177 02539000
         CLI   FSBMJRCD(FSBPTR),X'00'                              1177 02540000
         BE    @RF01177                                            1177 02541000
@RT01177 DS    0H                                                  1178 02542000
*     /***************************************************************/ 02543000
*     /*                                                             */ 02544000
*     /* ENDING STATUS IN FSB-DO NOT EXPECT ANOTHER FSB. PROCESSING  */ 02545000
*     /* FINISHED                                                    */ 02546000
*     /*                                                             */ 02547000
*     /***************************************************************/ 02548000
*                                                                  1178 02549000
*     CRRPOST=ON;                   /* SET DO POST SWITCH            */ 02550000
         OI    CRRPOST(CRRPTR),B'00010000'                         1178 02551000
*   ELSE                                                           1179 02552000
*     IF CRRBLEN>ZEROWORD THEN      /* MORE SPACE IN USER DATA AREA  */ 02553000
         B     @RC01177                                            1179 02554000
@RF01177 LH    @15,CRRBLEN(,CRRPTR)                                1179 02555000
         LTR   @15,@15                                             1179 02556000
         BNP   @RF01179                                            1179 02557000
*       IF CRRERLK=ON THEN          /* TEST FOR ERROR LOCK           */ 02558000
*                                                                  1180 02559000
         TM    CRRERLK(CRRPTR),B'10000000'                         1180 02560000
         BNO   @RF01180                                            1180 02561000
*         /***********************************************************/ 02562000
*         /*                                                         */ 02563000
*         /* ERROR LOCK SET IN FSB JUST PROCESSED. PROCESSING        */ 02564000
*         /* FINISHED                                                */ 02565000
*         /*                                                         */ 02566000
*         /***********************************************************/ 02567000
*                                                                  1181 02568000
*         CRRPOST=ON;               /* POST USET WITH ERR LOCK       */ 02569000
         OI    CRRPOST(CRRPTR),B'00010000'                         1181 02570000
*       ELSE                                                       1182 02571000
*         DO;                       /* ERROR LOCK NOT SET            */ 02572000
*                                                                  1182 02573000
         B     @RC01180                                            1182 02574000
@RF01180 DS    0H                                                  1183 02575000
*           /*********************************************************/ 02576000
*           /*                                                       */ 02577000
*           /* FSB JUST PROCESSED WAS A SUBBLOCK AND THERE IS MORE   */ 02578000
*           /* ROOM IN THE USER'S BUFFER. SAVE STATUS IN LCPB TO     */ 02579000
*           /* RESUME MOVE WHEN NEXT FSB ARRIVES                     */ 02580000
*           /*                                                       */ 02581000
*           /*********************************************************/ 02582000
*                                                                  1183 02583000
*           DCLIOBUF=CRRBARAD;      /* MOVE ADDR OF NEXT AVAIL SPACE    02584000
*                                      IN USER BUFFER TO DCLCP     1183 02585000
*                                      EXTENSION                     */ 02586000
         L     @15,CRRLCPB(,CRRPTR)                                1183 02587000
         L     @05,CRRBARAD(,CRRPTR)                               1183 02588000
         ST    @05,DCLIOBUF(,@15)                                  1183 02589000
*           DCLIOLEN=CRRBLEN;       /* MOVE RESIDUAL COUNT TO DCLCP  */ 02590000
         LH    @05,CRRBLEN(,CRRPTR)                                1184 02591000
         STH   @05,DCLIOLEN(,@15)                                  1184 02592000
*           DCLBTLEN=CRRBTLEN;      /* PUT LENGTH MOVED IN DCLCP     */ 02593000
         L     @05,CRRBTLEN(,CRRPTR)                               1185 02594000
         ST    @05,DCLBTLEN(,@15)                                  1185 02595000
*           DCLRPLA=CRRARPL;        /* PUT ADR OF RPL IN DCLCP       */ 02596000
         L     @05,CRRARPL(,CRRPTR)                                1186 02597000
         ST    @05,DCLRPLA(,@15)                                   1186 02598000
*         END;                                                     1187 02599000
*     ELSE                                                         1188 02600000
*       DO;                                                        1188 02601000
*                                                                  1188 02602000
         B     @RC01179                                            1188 02603000
@RF01179 DS    0H                                                  1189 02604000
*         /***********************************************************/ 02605000
*         /*                                                         */ 02606000
*         /* FSB JUST PROCESSED WAS A SUBBLOCK BUT THE USER'S BUFFER */ 02607000
*         /* IS FILLED.                                              */ 02608000
*         /*                                                         */ 02609000
*         /***********************************************************/ 02610000
*                                                                  1189 02611000
*                                   /* D31JKMQ                       */ 02612000
*         CRRPOST=ON;               /* SET DO POST SWITCH            */ 02613000
