         TITLE 'ISTDCC50-- SOLICIT,READ,WRITE FEEDBACK                 *00001000
                        '                                               00002000
ISTDCC50 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC50  78.173'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*                                                                  0486 00013000
*/********************************************************************/ 00014000
*/*                                                                  */ 00015000
*/*                                                                  */ 00016000
*/*  ROUTINE NAME : ISTDCC50 MAINLINE ROUTINE                        */ 00017000
*/*                                                                  */ 00018000
*/*  FUNCTION : THE MAIN ROUTINE CALLS THE APPROPIATE PROCESSING     */ 00019000
*/*   SUBROUTINES:                                                   */ 00020000
*/*    READMAC - READ AND SOLICIT FEEDBACK                           */ 00021000
*/*    WRTMAC - WRITE FEEDBACK                                       */ 00022000
*/*    FLUSH - FREES INCOMING DATA                                   */ 00023000
*/*                                                                  */ 00024000
*/*  IF FEEDBACK IS FOR A SOLICIT REQUEST, DCLRPLA                   */ 00025000
*/*  WILL BE ZERO ON ENTRY.  CONTROL IS THEN TRANSFERRED             */ 00026000
*/*  TO READMAC WITH CONTROL SWITCH, CRRSW0, SET.                    */ 00027000
*/*                                                                  */ 00028000
*/* INPUT :  RPHPTR REG(1) - ADDRESS OF RPH                          */ 00029000
*/*                                                                  */ 00030000
*/* OUTPUT :  RPHPTR REG(1) - ADDRESS OF RPH                         */ 00031000
*/*                                                                  */ 00032000
*/* SUBROUTINES CALLED : READMAC - READ AND SOLICIT FEEDBACK         */ 00033000
*/*                      WRTMAC  - WRITE FEEDBACK PROCESSING         */ 00034000
*/*                      FLUSH   - FREE UP DATA BUFFERS              */ 00035000
*/* MACROS USED : TPESC - NORMAL EXIT                                */ 00036000
*/*                                                                  */ 00037000
*/********************************************************************/ 00038000
*                                                                  0486 00039000
*   CRRPTR=ADDR(CRRSPACE);          /* INITIALIZE TO MAIN CRR        */ 00040000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0486 00041000
