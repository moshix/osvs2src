*       DCL                                                        1321 03126000
*         FMZL0180 BASED(GPR15P) LABEL VALUERANGE(*);              1321 03127000
*       CALL FMZL0180;                                             1322 03128000
         BALR  @14,@15                                             1322 03129000
*       RESPECIFY                                                  1323 03130000
*        (GPR00F,                                                  1323 03131000
*         GPR15P) UNRSTD;                                          1323 03132000
*     END;                          /* **** END OF TPEXIT            */ 03133000
*LHAB0175:                                                         1325 03134000
*     ;                                                            1325 03135000
LHAB0175 DS    0H                                                  1326 03136000
*     GPR01P->RPHRESMA=0;                                          1326 03137000
         SLR   @15,@15                                             1326 03138000
         ST    @15,RPHRESMA(,GPR01P)                               1326 03139000
*     RFY                                                          1327 03140000
*      (GPR00P) RSTD;                                              1327 03141000
*LHAA0175:                                                         1328 03142000
*     ;                                                            1328 03143000
LHAA0175 DS    0H                                                  1329 03144000
*     GEN(LM    GPR00P,GPR15P,RPHWORK(GPR01P))                     1329 03145000
*     REFS(RPHWORK,GPR01P) SETS(GPR00P,GPR15P);                    1329 03146000
