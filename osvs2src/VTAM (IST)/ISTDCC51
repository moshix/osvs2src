*     RPLFDB2=ZEROBYTE;             /* INDICATES NO ERRORS IN RTN  0513 00357000
*                                      CODES                         */ 00358000
         MVI   RPLFDB2(@15),X'00'                                  0513 00359000
*     RPLFDB3=ZEROBYTE;                                            0514 00360000
