*   END POSTRPL;                    /* RETURN TO MAIN PROCEDURE      */ 00434000
@EL00002 DS    0H                                                  0540 00435000
@EF00002 DS    0H                                                  0540 00436000
@ER00002 BR    @14                                                 0540 00437000
@PB00002 DS    0H                                                  0540 00438000
*   GEN NOSETS NOREFS(EJECT);                                      0541 00439000
*                                                                  0541 00440000
         EJECT                                                          00441000
*/********************************************************************/ 00442000
*/*                                                                  */ 00443000
*/* ROUTINE NAME : REQUE - RE- QUEUE THE RESET                       */ 00444000
*/*                                                                  */ 00445000
*/* FUNCTION : THE RESET UNCONDITIONAL OPERATION HAS FAILED, AND MUST*/ 00446000
*/*      BE RE-SCHEDULED. THE LCPB FLAGS ARE CLEARED AND THE LCPB IS */ 00447000
*/*      QUED TO THE TPIOS OUTBOUND PAB.                             */ 00448000
*/*                                                                  */ 00449000
*/* INPUT : ADDRCRR (REG 13) - PTR TO CRR                            */ 00450000
*/*         ADDRFSB (REG 3) - PTR TO THE FSB                         */ 00451000
*/*                                                                  */ 00452000
*/* OUTPUT : THE LCPB QUEUED TO THE TOP                              */ 00453000
*/*                                                                  */ 00454000
*/* SUBROUTINES CALLED : NONE                                        */ 00455000
*/*                                                                  */ 00456000
*/* MACROS USED : TPQUE                                              */ 00457000
*/*                                                                  */ 00458000
*/********************************************************************/ 00459000
*                                                                  0542 00460000
*REQUE:                                                            0542 00461000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0542 00462000
         B     @PB00003                                            0542 00463000
REQUE    DS    0H                                                  0543 00464000
*   CRRRTN2=REG14;                  /* SAVE THE RETURN REG           */ 00465000
         ST    REG14,CRRRTN2(,ADDRCRR)                             0543 00466000
*   LCPFLAGS='00'X;                 /* CLEAR THE LCPB FLAGS          */ 00467000
         L     @15,CRRLCPB(,ADDRCRR)                               0544 00468000
         MVI   LCPFLAGS(@15),X'00'                                 0544 00469000
*   LCPMNRCD='00'X;                 /* CLEAR THE MINOR RET. CODE     */ 00470000
         MVI   LCPMNRCD(@15),X'00'                                 0545 00471000
*   LCPNXLCW=ADDR(DCLLCCW2);        /* GET ADDR OF SECOND LCCW       */ 00472000
         LA    @11,DCLLCCW2(,@15)                                  0546 00473000
         ST    @11,LCPNXLCW(,@15)                                  0546 00474000
*   LCCWOP=LXI;                     /* SET RESET IMMEDIATE CMD       */ 00475000
*                                                                  0547 00476000
         MVI   LCCWOP(@11),X'2B'                                   0547 00477000
*   /*****************************************************************/ 00478000
*   /*                                                               */ 00479000
*   /* THE RESET OPERATION MUST BE RESCHEDULED                       */ 00480000
*   /*                                                               */ 00481000
*   /*****************************************************************/ 00482000
*                                                                  0548 00483000
*/*  TPQUE (CRRLCPB,FMCPAB3) RESET                                   */ 00484000
*                                                                  0548 00485000
*   DO;                                                            0548 00486000
*     RFY                                                          0549 00487000
*      (GPR00P,                                                    0549 00488000
*       GPR01P,                                                    0549 00489000
*       GPR02P,                                                    0549 00490000
*       GPR15P) RSTD;                                              0549 00491000
*     GEN REFS(GPR01P) SETS(RPHWORK)                               0550 00492000
*         (STM   GPR14P,GPR09P,RPHWORK(GPR01P));                   0550 00493000
         STM   GPR14P,GPR09P,RPHWORK(GPR01P)                            00494000
*     GPR00P=ADDR(FMCPAB3);                                        0551 00495000
         L     @11,CRRFMCB(,ADDRCRR)                               0551 00496000
         LA    GPR00P,FMCPAB3(,@11)                                0551 00497000
*     GPR02P=CRRLCPB;                                              0552 00498000
         L     GPR02P,CRRLCPB(,ADDRCRR)                            0552 00499000
*     CALL ATCLCPTR->ATCAP34->ISTAPC34;                            0553 00500000
         L     @11,ATCLCPTR                                        0553 00501000
         L     @15,ATCAP34(,@11)                                   0553 00502000
         BALR  @14,@15                                             0553 00503000
*     GEN REFS(RPHWORK,GPR01P) NOSETS                              0554 00504000
*         (LM    GPR14P,GPR09P,RPHWORK(GPR01P));                   0554 00505000
         LM    GPR14P,GPR09P,RPHWORK(GPR01P)                            00506000
*     RFY                                                          0555 00507000
*      (GPR00P,                                                    0555 00508000
*       GPR01P,                                                    0555 00509000
*       GPR02P,                                                    0555 00510000
*       GPR15P) UNRSTD;                                            0555 00511000
*   END;                            /* *** END OF TPQUE GENERATION   */ 00512000
*   CRRENDLW=OFF;                   /* TURN OFF END OF I/O FLAG      */ 00513000
         NI    CRRENDLW(ADDRCRR),B'11011111'                       0557 00514000
*   REG14=CRRRTN2;                  /* RESTORE THE RETURN REG        */ 00515000
         L     REG14,CRRRTN2(,ADDRCRR)                             0558 00516000
*   RETURN;                                                        0559 00517000
@EL00003 DS    0H                                                  0559 00518000
@EF00003 DS    0H                                                  0559 00519000
@ER00003 BR    @14                                                 0559 00520000
*   END REQUE;                                                     0560 00521000
*                                                                  0560 00522000
@PB00003 DS    0H                                                  0560 00523000
*/* TPEPILOG                                                         */ 00524000
*                                                                  0561 00525000
*   GEN(PRINT OFF);                                                0561 00526000
         PRINT OFF                                                      00527000
*/* TEMPLATE FOR CHAIN FIELD OF WORK ELEMENT */                    0562 00528000
*DCL  1  ISTWKE BDY(WORD) BASED(WKEPTR),                           0562 00529000
*        2  * CHAR(4),                 /* SPACE                       * 00530000
*        2  WKECHAIN PTR(31),          /* CHAIN POINTER               * 00531000
*           3 WKEFLAGS BIT(8),         /* FLAGS                       * 00532000
*             4 WKEGATE  BIT(1),       /* GATE BIT                    * 00533000
*             4 *        BIT(7),       /* RESERVED                    * 00534000
*           3 WKECHNGP PTR(24);        /* PTR ADDRESS                 * 00535000
*   DCL                                                            0563 00536000
*     ISTAPC34 ENTRY BASED(ATCAP34) VALRG(*);/* RESET ROUTINE        */ 00537000
*   DCL                                                            0564 00538000
*     ISTAPC37 ENTRY BASED(ATCAP37) VALRG(*);/* TPQUE ROUTINE        */ 00539000
*   GEN(PRINT ON);                                                 0565 00540000
*                                                                  0565 00541000
         PRINT ON                                                       00542000
*/* END OF TPEPILOG                                                  */ 00543000
