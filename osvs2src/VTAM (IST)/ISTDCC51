*       IF GPR14P=FMZCA030 THEN                                    0520 00381000
         CLR   GPR14P,@11                                          0520 00382000
         BE    @RT00520                                            0520 00383000
*         ;                                                        0521 00384000
*       ELSE                                                       0522 00385000
*         DO;                                                      0522 00386000
*FMZL1034:                                                         0523 00387000
*           ;                                                      0523 00388000
FMZL1034 DS    0H                                                  0524 00389000
*           DO WHILE(GPR14P->WKECHAIN^=FMZCA030);                  0524 00390000
         B     @DE00524                                            0524 00391000
@DL00524 DS    0H                                                  0525 00392000
*             GPR15P=ADDR(GPR14P->WKECHAIN);                       0525 00393000
         LA    GPR15P,WKECHAIN(,GPR14P)                            0525 00394000
*             GPR14P=GPR15P->PABWEQA;                              0526 00395000
         L     GPR14P,PABWEQA(,GPR15P)                             0526 00396000
*           END;                                                   0527 00397000
@DE00524 L     @11,@CF01735                                        0527 00398000
         CL    @11,WKECHAIN(,GPR14P)                               0527 00399000
         BNE   @DL00524                                            0527 00400000
*           GPR00P=FMZCA030;                                       0528 00401000
         LR    GPR00P,@11                                          0528 00402000
*           CS(GPR14P,GPR00P,GPR15P->PABWEQA);                     0529 00403000
         CS    GPR14P,@00,PABWEQA(GPR15P)                          0529 00404000
*           BC(7,FMZL1034);                                        0530 00405000
         BC    7,FMZL1034                                          0530 00406000
*           GPR14P->WKECHAIN=0;                                    0531 00407000
         SLR   @11,@11                                             0531 00408000
         ST    @11,WKECHAIN(,GPR14P)                               0531 00409000
*         END;                                                     0532 00410000
*ISTA0034:                                                         0533 00411000
*       ;                                                          0533 00412000
@RT00520 DS    0H                                                  0533 00413000
ISTA0034 DS    0H                                                  0534 00414000
*       GPR14P=GPR14P&FMZCB030;                                    0534 00415000
         N     GPR14P,@CF01828                                     0534 00416000
*       GPR01P->RPHWEA=GPR14P;                                     0535 00417000
         ST    GPR14P,RPHWEA(,GPR01P)                              0535 00418000
*       RFY                                                        0536 00419000
*        (GPR00P,                                                  0536 00420000
*         GPR01P,                                                  0536 00421000
*         GPR14P,                                                  0536 00422000
*         GPR15P) UNRSTD;                                          0536 00423000
*     END;                          /* END TPDEQ GENERATION DEQUE RPL*/ 00424000
*   END;                                                           0538 00425000
@DE00510 L     @15,RPHWEA(,ADDRRPH)                                0538 00426000
         LTR   @15,@15                                             0538 00427000
         BZ    @DC00510                                            0538 00428000
