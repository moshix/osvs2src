*     DCL                                                          0486 00278000
*       FMZCA030 BIT(32) CONSTANT('80000000'X);                    0486 00279000
*     DCL                                                          0487 00280000
*       FMZCB030 BIT(32) CONSTANT('7FFFFFFF'X);                    0487 00281000
*     RFY                                                          0488 00282000
*      (GPR00P,                                                    0488 00283000
*       GPR01P,                                                    0488 00284000
*       GPR14P,                                                    0488 00285000
*       GPR15P) RSTD;                                              0488 00286000
*     RFY                                                          0489 00287000
*       ISTWKE;                                                    0489 00288000
*     GPR15P=ADDR(FMCRPLA);         /* ADDRESS QUEUE POINTER         */ 00289000
         L     GPR15P,CRRFMCB(,ADDRCRR)                            0490 00290000
         LA    GPR15P,FMCRPLA(,GPR15P)                             0490 00291000
*     GPR14P=GPR15P->PABWEQA|FMZCA030;                             0491 00292000
         L     @11,@CF01735                                        0491 00293000
         L     GPR14P,PABWEQA(,GPR15P)                             0491 00294000
