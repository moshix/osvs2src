         TITLE 'ISTDCC60 - CONTROL LAYER INPUT BUFFER MOVE             *00001000
                        '                                               00002000
ISTDCC60 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC60  76.110'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*   CRRPTR=ADDR(CRACRR->CRRSPACE);  /* GET @ SECOND PORTION CRR      */ 00014000
         L     @15,RPHCRR(,PARAM)                                  0342 00015000
         L     CRRPTR,CRACRR(,@15)                                 0342 00016000
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0342 00017000
*   RESPECIFY                                                      0343 00018000
*     ISTDCCRR;                     /* DROP COVER FOR FIRST PART OF     00019000
*                                      CRR                           */ 00020000
*   RESPECIFY                                                      0344 00021000
*     CRRSAREA BASED(CRRPTR);       /* COVER SECOND PORTION OF CRR   */ 00022000
*   CRRSAVE(5:8)=SAVEPTR;           /* SAVE CALLER'S SAVEAREA ADDRESS   00023000
*                                      IN BACKWARD PTR OF MY SAVEAREA*/ 00024000
         ST    SAVEPTR,CRRSAVE+4(,CRRPTR)                          0345 00025000
*   SAVEPTR=ADDR(CRRSAVE);          /* GET ADDRESS OF MY SAVE- AREA     00026000
*                                      INTO REG 13                   */ 00027000
         LR    SAVEPTR,CRRPTR                                      0346 00028000
*   CRRSAVE1(9:12)=SAVEPTR;         /* STORE MY SAVEAREA @ INTO    0347 00029000
*                                      CALLER'S FORWARD PTR          */ 00030000
         ST    SAVEPTR,CRRSAVE1+8(,CRRPTR)                         0347 00031000
*   FDBPTR=ADDR(FSBFDB);            /* GET FSB FDB @ INTO REG        */ 00032000
         L     FDBPTR,CRRFSB(,CRRPTR)                              0348 00033000
         LA    FDBPTR,FSBFDB(,FDBPTR)                              0348 00034000
*   CRRM60=ON;                      /* SET AUDIT TRAIL SWITCH 1,   0349 00035000
*                                      DON'T CLEAR FIRST             */ 00036000
         OI    CRRM60(CRRPTR),B'10000000'                          0349 00037000
*   CRRC60=ON;                      /* SET AUDIT TRAIL SWITCH 2      */ 00038000
         OI    CRRC60(CRRPTR),B'10000000'                          0350 00039000
*                                                                  0351 00040000
*/********************************************************************/ 00041000
*/* 1. FIND FDB TO BE PROCESSED                                      */ 00042000
*/* 2. INITIALIZE BUFFER AREA AND LENGTH REGS AND LDO REG            */ 00043000
*/* 3. CALL PROCESSA - THE NEXT PHASE                                */ 00044000
*/*    ERROR CONDITIONS -                                            */ 00045000
*/*          -- NO FDB TO BE SERVICED, SET ZERO RETURN CODE AND      */ 00046000
*/*             RETURN                                               */ 00047000
*/*          -- BUFFER PARAMETER LENGTH LESS THAN OR EQUAL TO        */ 00048000
*/*             ZERO - SET RETURN CODE FOR ERROR RETURN, AND CRR     */ 00049000
*/*             ERROR BIT FOR INVALID LENGTH                         */ 00050000
*/********************************************************************/ 00051000
*                                                                  0351 00052000
*   CRRBFLG1=ON;                    /* TURN SWITCH ON                */ 00053000
         OI    CRRBFLG1(CRRPTR),B'10000000'                        0351 00054000
*FINDFDB:                                                          0352 00055000
*   IF FDBDATCT=0                   /* IF FDB ALREADY PROCESSED      */ 00056000
*     THEN                                                         0352 00057000
FINDFDB  SLR   @15,@15                                             0352 00058000
         CH    @15,FDBDATCT(,FDBPTR)                               0352 00059000
         BNE   @RF00352                                            0352 00060000
*     DO;                           /* THEN                          */ 00061000
*       FDBPTR=FDBFDBA;             /* GET CHAIN PTR TO NEXT FDB     */ 00062000
         L     FDBPTR,FDBFDBA(,FDBPTR)                             0354 00063000
*       IF FDBPTR=0|FDBPTR=GATEBIT  /* IF NO MORE FDBS               */ 00064000
*         THEN                                                     0355 00065000
         CR    FDBPTR,@15                                          0355 00066000
         BE    @RT00355                                            0355 00067000
         CL    FDBPTR,GATEBIT                                      0355 00068000
         BNE   @RF00355                                            0355 00069000
@RT00355 DS    0H                                                  0356 00070000
*         CRRBFLG1=OFF;             /* THEN TURN OFF SWITCH          */ 00071000
         NI    CRRBFLG1(CRRPTR),B'01111111'                        0356 00072000
*       ELSE                                                       0357 00073000
*         ;                         /* IF MORE FDBS,NULL ELSE        */ 00074000
@RF00355 DS    0H                                                  0358 00075000
*     END;                          /* END DO FOR FDB ALREADY      0358 00076000
*                                      PROCESSED                     */ 00077000
*   ELSE                                                           0359 00078000
*     CRRBFLG1=OFF;                 /* IF FDB NOT YET PROCESSED TURN    00079000
*                                      OFF SWITCH                    */ 00080000
         B     @RC00352                                            0359 00081000
@RF00352 NI    CRRBFLG1(CRRPTR),B'01111111'                        0359 00082000
*   IF CRRBFLG1=ON                  /* IF SWITCH IS ON THEN          */ 00083000
*     THEN                                                         0360 00084000
@RC00352 TM    CRRBFLG1(CRRPTR),B'10000000'                        0360 00085000
         BO    @RT00360                                            0360 00086000
*     GOTO FINDFDB;                 /* GO FIND FDB AGAIN GOTO USED IN   00087000
*                                      LIEU OF DO UNTIL              */ 00088000
*   ELSE                                                           0362 00089000
*     ;                             /* NULL ELSE IF SWITCH OFF       */ 00090000
*   IF FDBPTR=0|FDBPTR=GATEBIT      /* IF NO MORE FDBS               */ 00091000
*     THEN                                                         0363 00092000
         LTR   FDBPTR,FDBPTR                                       0363 00093000
         BZ    @RT00363                                            0363 00094000
         CL    FDBPTR,GATEBIT                                      0363 00095000
         BNE   @RF00363                                            0363 00096000
@RT00363 DS    0H                                                  0364 00097000
*     CRRBRTCD=CRRBRSUC;            /* THEN SET GOOD RETURN CODE     */ 00098000
         MVI   CRRBRTCD(CRRPTR),X'00'                              0364 00099000
*   ELSE                                                           0365 00100000
*     DO;                           /* IF MORE FDBS                  */ 00101000
         B     @RC00363                                            0365 00102000
@RF00363 DS    0H                                                  0366 00103000
*       BCTR=CRRBLEN;               /* GET PARAM LIST LENGTH INTO  0366 00104000
*                                      BUFFER LENGTH REG             */ 00105000
         LH    BCTR,CRRBLEN(,CRRPTR)                               0366 00106000
*       IF BCTR>0                   /* IF PARAM LENGTH VALID         */ 00107000
*         THEN                      /* THEN                          */ 00108000
         LTR   BCTR,BCTR                                           0367 00109000
         BNP   @RF00367                                            0367 00110000
*         CALL PROCESSA;            /* CALL NEXT PHASE               */ 00111000
         BAL   @14,PROCESSA                                        0368 00112000
*       ELSE                                                       0369 00113000
*         DO;                       /* IF PARM LENGTH INVALID        */ 00114000
         B     @RC00367                                            0369 00115000
@RF00367 DS    0H                                                  0370 00116000
*           CRRRTNCD=CRRLOGIC;      /* SET MAJOR RC=LOGIC ERROR IN 0370 00117000
*                                      CRR                           */ 00118000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0370 00119000
*           CRRFDB2=CRRIDA;         /* SET MINOR RC=INVALID LENGTH IN   00120000
*                                      CRR                           */ 00121000
         MVI   CRRFDB2(CRRPTR),X'17'                               0371 00122000
*           CRRBRTCD=CRRBEROR;      /* SET ERROR RETURN CODE LOGIC 0372 00123000
*                                      ERROR                         */ 00124000
         MVI   CRRBRTCD(CRRPTR),X'08'                              0372 00125000
*         END;                      /* END DO FOR INVALID PARM LENGTH*/ 00126000
*     END;                          /* END DO FOR MORE FDBS          */ 00127000
@RC00367 DS    0H                                                  0375 00128000
*   SAVEPTR=CRRSAVE(5:8);           /* GET @ CALLER'S SAVEAREA       */ 00129000
@RC00363 L     SAVEPTR,CRRSAVE+4(,CRRPTR)                          0375 00130000
*   RETURN;                         /* RETURN TO CALLER              */ 00131000
@EL00001 DS    0H                                                  0376 00132000
@EF00001 DS    0H                                                  0376 00133000
@ER00001 LM    @14,@12,12(@13)                                     0376 00134000
         BR    @14                                                 0376 00135000
*   GEN NOSETS NOREFS(EJECT);                                      0377 00136000
         EJECT                                                          00137000
*                                                                  0378 00138000
*/********************************************************************/ 00139000
*/*                                                                  */ 00140000
*/* ROUTINE NAME : PROCESSA - CHECK DATA AREA                        */ 00141000
*/*                                                                  */ 00142000
*/* FUNCTION : THIS ROUTINE DOES THE FOLLOWING:                      */ 00143000
*/*             - VALIDITY CHECK BUFFER AREA ADDRESS & LENGTH        */ 00144000
*/*             - SET FDBK DATA & LENGTH REGS & LENGTH MOVED REG     */ 00145000
*/*             - CALL PROCESSB TO MOVE DATA FROM FDB TO DATA AREA   */ 00146000
*/*               UNTIL EITHER OR BOTH ARE EXHAUSTED.                */ 00147000
*/*             - CALL PROCESSC TO FIND NEXT LDO & DATA AREA TO BE   */ 00148000
*/*               MOVED INTO, IF ONE EXIST.                          */ 00149000
*/*             - REPEAT THIS PROCESS AS MANY TIMES AS NECESSARY     */ 00150000
*/*                                                                  */ 00151000
*/* INPUT : FDBPTR (REG 5) - PTR TO FBD TO BE PROCESSED              */ 00152000
*/*         BCTR (REG 7) - PTR TO LENGTH OF DATA AREA                */ 00153000
*/*         CRRPTR (REG 11) - PTR TO THE CRR                         */ 00154000
*/*         SAVEPTR (REG 13) - PTR TO THE SAVE AREA IN THE CRR       */ 00155000
*/*                                                                  */ 00156000
*/* OUTPUT : SEE TABLE                                               */ 00157000
*/*                                                                  */ 00158000
*/* SUBROUTINES CALLED : PROCESSB , PROCESSC , VALIDATE              */ 00159000
*/*                                                                  */ 00160000
*/* MACROS USED : NONE                                               */ 00161000
*/*                                                                  */ 00162000
*/********************************************************************/ 00163000
*/* TABLE FOR PROCESSA                                               */ 00164000
*/*------------------------------------------------------------------*/ 00165000
*/* CB       FIELD      R/S    MEANING                               */ 00166000
*/*------------------------------------------------------------------*/ 00167000
*/* CRR      CRRRTN7    R/S    USED AS A TEMPORARY SAVEAREA FOR      */ 00168000
*/*                            RETURN ADDRESS IN REGISTER RETPTR     */ 00169000
*/*          CRRBFLG1   R/S    SEARCH FOR MORE FDBS FLAG, IF ON      */ 00170000
*/*                            SEARCH FOR ANOTHER FDB TO TRANSFER    */ 00171000
*/*                            DATA FROM, IF OFF,                    */ 00172000
*/*                            DON'T, SET IN THIS ROUTINE AND IN     */ 00173000
*/*                            PROCESSB                              */ 00174000
*/*          CRRBTLEN   R/S    PARAMETER LIST CUMULATIVE TOTAL OF    */ 00175000
*/*                            DATA BYTES MOVED, IS NOT INITIALIZED  */ 00176000
*/*                            IN THIS ENTIRE MODULE, SET BY USER OR */ 00177000
*/*                            UPDATED BY PROCESSA, SET TO ITS       */ 00178000
*/*                            CURRENT VALUE + REGISTER RCTR (LENGTH */ 00179000
*/*                            MOVED REGISTER)                       */ 00180000
*/*          CRRBRTCD   R/S    PARAMETER LIST RETURN CODE, SET TO    */ 00181000
*/*                            CRRBEROR (ERROR RETURN CODE) IF BUFFER*/ 00182000
*/*                            AREA ADDRESS OR LENGTH  REGISTERS ARE */ 00183000
*/*                            INVALID, TESTED AGAINST CRRBMDAT TO   */ 00184000
*/*                            DETERMINE IF MORE DATA SHOULD BE      */ 00185000
*/*                            MOVED FROM FDB TO USER AREA (SET IN   */ 00186000
*/*                            THIS CASE BY PROCESSB)                */ 00187000
*/*          CRRRTNCD    S     CRR MAJOR RETURN CODE, SET TO CRRLOGIC*/ 00188000
*/*                            (LOGIC ERROR), IF BUFFER AREA         */ 00189000
*/*                            OR LENGTH REGISTERS ARE INVALID       */ 00190000
*/*          CRRFDB2     S     CRR MINOR RETURN CODE, SET TO         */ 00191000
*/*                            CRRIDA(INVALID AREA ADDRESS OR LENGTH)*/ 00192000
*/*                            IF BUFFER AREA ADDRESS OR LENGTH REGS */ 00193000
*/*                            ARE INVALID                           */ 00194000
*/*                                                                  */ 00195000
*/* FDB      FDBDATAA    R     FDB DATA ADDRESS, USED TO SET (FPTR)  */ 00196000
*/*                            FDB DATA ADDRESS REGISTER             */ 00197000
*/*          FDBDATCT    R     FDB DATA COUNT, USED TO SET (FCTR)    */ 00198000
*/*                            FDB DATA LENGTH REGISTER              */ 00199000
*/*                                                                  */ 00200000
*/* REGS     BASE        R     IMPLICIT MODULE BASE ADDRESS, SET IN  */ 00201000
*/*                            MAIN PROCEDURE                        */ 00202000
*/*          CRRPTR      R     IMPLICIT BASE FOR CRR, SET IN MAIN    */ 00203000
*/*                            PROCEDURE                             */ 00204000
*/*          FDBPTR      R     IMPLICIT BASE FOR FDB, SET EITHER IN  */ 00205000
*/*                            MAIN PROCEDURE OR PROCESSB            */ 00206000
*/*          BCTR        R     USER BUFFER LENGTH REGISTER, USED     */ 00207000
*/*                            AS THE LENGTH PARAMETER TO THE        */ 00208000
*/*                            ?VALCHECK MACRO, COULD BE SET BY ANY  */ 00209000
*/*                            OF THE PROCS INCLUDING THIS ONE,      */ 00210000
*/*                            DEPENDING ON THE CIRCUMSTANCES        */ 00211000
*/*          ENTER       R     RETURN CODE REGISTER FROM ?VALCHECK,  */ 00212000
*/*                            IF NON-ZERO, MEANS THAT EITHER THE    */ 00213000
*/*                            BUFFER AREA ADDRESS OR LENGTH REGS    */ 00214000
*/*                            ARE INVALID                           */ 00215000
*/*          BPTR       R/S    USER BUFFER AREA ADDRESS REGISTER,    */ 00216000
*/*                            USED AS INPUT TO ?VALCHECK MACRO AS   */ 00217000
*/*                            THE BEGINNING ADDRESS TO BE VALI-     */ 00218000
*/*                            DATED, SET BY ANY                     */ 00219000
*/*                            OF THE PROCS INCLUDING THIS ONE,      */ 00220000
*/*                            DEPENDING ON THE CIRCUMSTANCES        */ 00221000
*/*          RETPTR     R/S    RETURN ADDRESS REGISTER FOR PROC,     */ 00222000
*/*                            SAVED AND RESTORED FROM CRRRTN7       */ 00223000
*/*                            WITHIN PROCESSA                       */ 00224000
*/*          PARAM       S     IMPLICIT BASE FOR THE RPH, USED AS    */ 00225000
*/*                            INPUT TO ?VALCHECK, SET EXTERNALLY TO */ 00226000
*/*                            ISTDCC60                              */ 00227000
*/*          RCTR        S     TOTAL LENGTH MOVED REGISTER, SET TO   */ 00228000
*/*                            ZERO TO INITIALIZE IT, AND USED TO    */ 00229000
*/*                            UPDATE CRRBTLEN                       */ 00230000
*/*          FPTR        S     FDB DATA ADDRESS REGISTER, SET TO     */ 00231000
*/*                            THE FDB DATA ADDRESS (FDBDATAA)       */ 00232000
*/*          FCTR        S     FDB DATA LENGTH REGISTER, SET TO      */ 00233000
*/*                            THE FDB DATA LENGTH (FDBDATCT)        */ 00234000
*/*                                                                  */ 00235000
*/* PROCS.   PROCESSB    -     CALLED TO ACTUALLY MOVE THE DATA FROM */ 00236000
*/*                            THE FDB TO THE USER BUFFER AREA, MAY  */ 00237000
*/*                            BE CALLED SEVERAL TIMES AS LONG AS    */ 00238000
*/*                            THERE IS BOTH FDB DATA TO MOVE AND    */ 00239000
*/*                            USER BUFFER AREA TO MOVE IT INTO, AND */ 00240000
*/*                            PROVIDED NO ERROR CONDITIONS HAVE     */ 00241000
*/*                            ARISEN                                */ 00242000
*/*          PROCESSC    -     CALLED TO FIND THE NEXT VALID LDO AND */ 00243000
*/*                            BUFFER AREA ADDRESS IF ANY EXISTS,    */ 00244000
*/*                            MAY BE CALLED SEVERAL TIMES AS LONG   */ 00245000
*/*                            AS THERE IS BOTH FDB DATA TO MOVE     */ 00246000
*/*                            AND USER BUFFER AREA TO MOVE IT INTO  */ 00247000
*/*                            AND PROVIDED NO ERROR CONDITIONS      */ 00248000
*/*                            HAVE ARISEN                           */ 00249000
*/*------------------------------------------------------------------*/ 00250000
*                                                                  0378 00251000
*PROCESSA:                                                         0378 00252000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0378 00253000
         B     @PB00002                                            0378 00254000
PROCESSA DS    0H                                                  0379 00255000
*   RESPECIFY                                                      0379 00256000
*    (RETPTR) RESTRICTED;           /* RESTRICT RETURN @ REG         */ 00257000
*   CRRRTN7=RETPTR;                 /* SAVE RETURN ADDRESS           */ 00258000
         ST    RETPTR,CRRRTN7(,CRRPTR)                             0380 00259000
*   RESPECIFY                                                      0381 00260000
*    (RETPTR) UNRESTRICTED;         /* UNRESTRICT RETURN @ REG       */ 00261000
*   RFY                                                            0382 00262000
*    (ENTER) RSTD;                                                 0382 00263000
*                                                                  0382 00264000
*   /*****************************************************************/ 00265000
*   /*                                                               */ 00266000
*   /* SET UP TO CALL THE VALIDATE ROUTINE TO CHECK THE DATA AREA    */ 00267000
*   /*                                                               */ 00268000
*   /*****************************************************************/ 00269000
*                                                                  0383 00270000
*ADLNCHK:                                                          0383 00271000
*   ;                                                              0383 00272000
ADLNCHK  DS    0H                                                  0384 00273000
*   CRRVALCK=BCTR;                  /* GET LENGTH IN CRRVALCK        */ 00274000
         ST    BCTR,CRRVALCK(,CRRPTR)                              0384 00275000
*   VALPTR=CRRBARAD;                /* GET AREA ADDR                 */ 00276000
         L     VALPTR,CRRBARAD(,CRRPTR)                            0385 00277000
*   ENTER=0;                        /* ZERO REG 15           @VC40441*/ 00278000
         SLR   ENTER,ENTER                                         0386 00279000
*   IF RPLAUTUS=OFF THEN            /* AUTHORIZED USER       @VC40441   00280000
*                                                            @ZA08948*/ 00281000
         L     @14,CRRARPL(,CRRPTR)                                0387 00282000
         TM    RPLAUTUS+16(@14),B'00100000'                        0387 00283000
         BNZ   @RF00387                                            0387 00284000
*     CALL VALIDATE;                /* GO DO THE VALCHEK             */ 00285000
*                                                                  0388 00286000
         BAL   @14,VALIDATE                                        0388 00287000
*   /*****************************************************************/ 00288000
*   /*                                                               */ 00289000
*   /* IF THE AREA IS VALID, SET UP REGS AND GO MOVE THE DATA        */ 00290000
*   /*                                                               */ 00291000
*   /*****************************************************************/ 00292000
*                                                                  0389 00293000
*   IF ENTER=0                      /* IF AREA @ AND LENGTH VAL      */ 00294000
*     THEN                                                         0389 00295000
@RF00387 SLR   @14,@14                                             0389 00296000
         CR    ENTER,@14                                           0389 00297000
         BNE   @RF00389                                            0389 00298000
*     DO;                           /* THEN                          */ 00299000
*       RCTR=0;                     /* ZERO LENGTH MOVED REG         */ 00300000
         LR    RCTR,@14                                            0391 00301000
*MOVEDATA:                                                         0392 00302000
*       BPTR=CRRBARAD;              /* GET PARAM LIST AREA @ INTO  0392 00303000
*                                      BUFFER AREA REG               */ 00304000
MOVEDATA L     BPTR,CRRBARAD(,CRRPTR)                              0392 00305000
*       FPTR=FDBDATAA;              /* GET FDB DATA @ INTO FDB       */ 00306000
         L     FPTR,FDBDATAA(,FDBPTR)                              0393 00307000
*       FCTR=FDBDATCT;              /* GET FDB DATA LEN INTO FDB   0394 00308000
*                                      LENGTH REG                    */ 00309000
         LH    FCTR,FDBDATCT(,FDBPTR)                              0394 00310000
*       CRRBFLG1=OFF;               /* TURN SWITCH OFF               */ 00311000
         NI    CRRBFLG1(CRRPTR),B'01111111'                        0395 00312000
*       CALL PROCESSB;              /* GO MOVE DATA                  */ 00313000
         BAL   @14,PROCESSB                                        0396 00314000
*       IF CRRBFLG1=ON              /* IF SWITCH ON, MORE DATA TO  0397 00315000
*                                      MOVE                          */ 00316000
*         THEN                                                     0397 00317000
         TM    CRRBFLG1(CRRPTR),B'10000000'                        0397 00318000
         BO    @RT00397                                            0397 00319000
*         GOTO MOVEDATA;            /* LOOP BACK GOTO USED IN LIEU OF   00320000
*                                      DO UNTIL                      */ 00321000
*                                                                  0398 00322000
*       /*************************************************************/ 00323000
*       /*                                                           */ 00324000
*       /* NO MORE DATA TO MOVE, CHECK FOR LDO DATA AREAS            */ 00325000
*       /*                                                           */ 00326000
*       /*************************************************************/ 00327000
*                                                                  0399 00328000
*       ELSE                                                       0399 00329000
*         DO;                       /* IF SWITCH OFF, NO MORE DATA TO   00330000
*                                      MOVE NOW                      */ 00331000
*           CALL PROCESSC;          /* SEE IF MORE LDOS AND DATA AREA   00332000
*                                      TO FILL                       */ 00333000
         BAL   @14,PROCESSC                                        0400 00334000
*           CRRBTLEN=CRRBTLEN+RCTR; /* ADD LENGTH MOVED TO PARM LIST    00335000
*                                      CUMULATIVE TOTAL              */ 00336000
         LR    @14,RCTR                                            0401 00337000
         AL    @14,CRRBTLEN(,CRRPTR)                               0401 00338000
         ST    @14,CRRBTLEN(,CRRPTR)                               0401 00339000
*         END;                      /* END DO FOR SWITCH OFF         */ 00340000
*     END;                          /* END DO FOR VALID @ AND LENGTH */ 00341000
*                                                                  0403 00342000
*   /*****************************************************************/ 00343000
*   /*                                                               */ 00344000
*   /* THE DATA AREA WAS INVALID, SET THE AREA CODES                 */ 00345000
*   /*                                                               */ 00346000
*   /*****************************************************************/ 00347000
*                                                                  0404 00348000
*   ELSE                                                           0404 00349000
*     DO;                           /* IF @ AND LENGTH INVALID       */ 00350000
         B     @RC00389                                            0404 00351000
@RF00389 DS    0H                                                  0405 00352000
*       CRRBRTCD=CRRBEROR;          /* SET ERROR RETURN CODE         */ 00353000
         MVI   CRRBRTCD(CRRPTR),X'08'                              0405 00354000
*       CRRRTNCD=CRRLOGIC;          /* SET MAJOR RC=LOGIC ERROR IN 0406 00355000
*                                      CRR                           */ 00356000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0406 00357000
*       CRRFDB2=CRRIDA;             /* SET MINOR RC=INVALID @ OR   0407 00358000
*                                      LENGTH IN CRR                 */ 00359000
         MVI   CRRFDB2(CRRPTR),X'17'                               0407 00360000
*       CRRBFLG1=OFF;               /* TURN OFF SWITCH               */ 00361000
         NI    CRRBFLG1(CRRPTR),B'01111111'                        0408 00362000
*     END;                          /* END DO FOR INVALID AREA @ AND    00363000
*                                      LENGTH                        */ 00364000
*   RESPECIFY                                                      0410 00365000
*    (ENTER) UNRESTRICTED;          /* REG CAN NOW BE USED           */ 00366000
@RC00389 DS    0H                                                  0411 00367000
*   IF CRRBFLG1=ON&CRRBRTCD=CRRBMDAT/* IF MORE DATA AREA FOUND FROM     00368000
*                                      LDO PROCESSING, AND MORE    0411 00369000
*                                      FEEDBACK DATA TO MOVE INTO IT,   00370000
*                                      LOOP BACK                     */ 00371000
*     THEN                                                         0411 00372000
         TM    CRRBFLG1(CRRPTR),B'10000000'                        0411 00373000
         BNO   @RF00411                                            0411 00374000
         CLI   CRRBRTCD(CRRPTR),4                                  0411 00375000
         BE    @RT00411                                            0411 00376000
*     GOTO ADLNCHK;                 /* TO DO IT. GOTO USED IN LIEU OF   00377000
*                                      DO UNTIL                      */ 00378000
*   ELSE                                                           0413 00379000
*     ;                             /* NULL ELSE IF 2 CONDITIONS NOT    00380000
*                                      TRUE                          */ 00381000
@RF00411 DS    0H                                                  0414 00382000
*   RESPECIFY                                                      0414 00383000
*    (RETPTR) RESTRICTED;           /* RESTRICT RETURN @ REG         */ 00384000
*   RETPTR=CRRRTN7;                 /* RESTORE RETURN POINTER        */ 00385000
         L     RETPTR,CRRRTN7(,CRRPTR)                             0415 00386000
*   RESPECIFY                                                      0416 00387000
*    (RETPTR) UNRESTRICTED;         /* UNRESTRICT RETURN @ REG       */ 00388000
*   RETURN;                         /* RETURN TO CALLER              */ 00389000
@EL00002 DS    0H                                                  0417 00390000
@EF00002 DS    0H                                                  0417 00391000
@ER00002 BR    @14                                                 0417 00392000
*   GEN NOSETS NOREFS(EJECT);                                      0418 00393000
         EJECT                                                          00394000
*                                                                  0419 00395000
*/********************************************************************/ 00396000
*/*                                                                  */ 00397000
*/* ROUTINE NAME : PROCESSB - MOVE THE DATA                          */ 00398000
*/*                                                                  */ 00399000
*/* FUNCTION : THIS ROUTINE DOES THE FOLLOWING:                      */ 00400000
*/*             - MOVE FDB DATA TO USER DATA AREA                    */ 00401000
*/*             - LOCATE MORE FDB(S) IF ANY EXEIST                   */ 00402000
*/*             - IF FDB DATA >= BUFER DATA AREA, SET GOOD RETURN    */ 00403000
*/*             - IF FDB DATA < BUF DATA AREA, SET MORE DATA REMAIN  */ 00404000
*/*             - UPDATE PARAMETER LIST OR FDB , IF NECESSARY        */ 00405000
*/*                                                                  */ 00406000
*/* INPUT : SAME AS INPUT TO PROCESSA                                */ 00407000
*/*         BPTR (REG 6) - ADDRESS OF DATA AREA                      */ 00408000
*/*         FPTR (REG 8) - PTR TO FDB DATA AREA                      */ 00409000
*/*         FCTR (REG 9) - FDB DATA LENGTH                           */ 00410000
*/*                                                                  */ 00411000
*/* OUTPUT : SEE TABLE FOR PROCESSB                                  */ 00412000
*/*                                                                  */ 00413000
*/* SUBROUTINES CALLED : NONE                                        */ 00414000
*/*                                                                  */ 00415000
*/* MACROS USED : NONE                                               */ 00416000
*/*                                                                  */ 00417000
*/********************************************************************/ 00418000
*/* TABLE FOR PROCESSB                                               */ 00419000
*/*------------------------------------------------------------------*/ 00420000
*/* CB       FIELD      R/S    MEANING                               */ 00421000
*/*------------------------------------------------------------------*/ 00422000
*/* CRR      CRRRTN8    R/S    USED AS A TEMPORARY SAVEAREA FOR      */ 00423000
*/*                            RETURN ADDRESS IN REGISTER RETPTR     */ 00424000
*/*          CRRSAVE     S     THE FOLLOWING FOUR REGS ARE SAVED     */ 00425000
*/*                            BEFORE AND RESTORED AFTER DATA MOVES  */ 00426000
*/*                            FROM FDB TO USER BUFFER AREA IN THIS  */ 00427000
*/*                            ISTDCC60 SAVEAREA: BPTR,BCTR,FPTR,    */ 00428000
*/*                            FCTR                                  */ 00429000
*/*          CRRBLEN     S     PARAMETER LIST LENGTH LEFT IN THE     */ 00430000
*/*                            USER BUFFER AREA AFTER MOVES OF DATA, */ 00431000
*/*                            IS SET TO ZERO IF UPON ENTRY TO THIS  */ 00432000
*/*                            PROC EITHER FCTR>BCTR OR FCTR=BCTR, IS*/ 00433000
*/*                            SET TO BCTR IF UPON ENTRY TO THIS     */ 00434000
*/*                            PROC, FCTR<BCTR                       */ 00435000
*/*          CRRBARAD    S     PARAMETER LIST BUFFER AREA ADDRESS,   */ 00436000
*/*                            SET TO BPTR REGISTER, IF UPON ENTRY   */ 00437000
*/*                            TO THIS PROC FCTR<BCTR                */ 00438000
*/*          CRRBRTCD    S     PARAMETER LIST RETURN CODE, SET TO    */ 00439000
*/*                            CRRBRSUC (SUCCESSFUL DATA MOVE) IF,   */ 00440000
*/*                            UPON ENTRY TO THIS PROC,              */ 00441000
*/*                            EITHER FCTR=BCTR AND THIS IS THE LAST */ 00442000
*/*                            FDB (FDBFDBA=0) OR FCTR<BCTR AND THIS */ 00443000
*/*                            IS THE LAST FDB, OR SET TO CRRBMDAT   */ 00444000
*/*                            (MORE DATA LEFT IN FDB(S) TO BE MOVED)*/ 00445000
*/*                            IF, UPON ENTRY TO THIS PROC,          */ 00446000
*/*                            EITHER FCTR=BCTR AND THERE ARE MORE   */ 00447000
*/*                            FDBS TO PROCESS, OR FCTR>BCTR         */ 00448000
*/*          CRRBFLG1    S     PARAMETER LIST SEARCH FOR MORE FDBS   */ 00449000
*/*                            FLAG, SET TO ON IF UPON ENTRY TO THIS */ 00450000
*/*                            PROC, FCTR<BCTR AND THERE ARE MORE    */ 00451000
*/*                            FDBS TO BE PROCESSED,                 */ 00452000
*/*                            TESTED IN PROCESSA                    */ 00453000
*/* FDB      FDBFDBA     R     FDB CHAIN POINTER TO NEXT FDB, USED   */ 00454000
*/*                            CHECKED AGAINST ZERO OR ZERO PLUS     */ 00455000
*/*                            GATEBIT                               */ 00456000
*/*                            FCTR<BCTR AND FCTR=BCTR TO SEE IF     */ 00457000
*/*                            THERE ARE ANY MORE FDBS, ALSO USED TO */ 00458000
*/*                            SET REGISTER FDBPTR IF, AFTER THE     */ 00459000
*/*                            ABOVE CHECKS ARE MADE, IT IS FOUND    */ 00460000
*/*                            THAT THERE ARE MORE FDBS (FDBFDBA     */ 00461000
*/*                            NOT ZERO OR NOT ZERO PLUS GATEBIT)    */ 00462000
*/*          FDBDATCT    S     FDB DATA COUNT, SET TO FCTR IF, UPON  */ 00463000
*/*                            ENTRY TO THIS PROC, FCTR>BCTR, OR     */ 00464000
*/*                            SET TO ZERO (MARKED ALREADY PROCESSED)*/ 00465000
*/*                            IF UPON ENTRY TO THIS PROC, EITHER    */ 00466000
*/*                            FCTR<BCTR OR FCTR=BCTR                */ 00467000
*/*          FDBDATAA    S     FDB DATA ADDRESS, SET TO FPTR IF,     */ 00468000
*/*                            UPON ENTRY TO THIS PROC, FCTR>BCTR    */ 00469000
*/*                                                                  */ 00470000
*/* REGS     BASE        R     IMPLICIT MODULE BASE ADDRESS, SET IN  */ 00471000
*/*                            MAIN PROCEDURE                        */ 00472000
*/*          CRRPTR      R     IMPLICIT BASE FOR CRR, SET IN MAIN    */ 00473000
*/*                            PROCEDURE                             */ 00474000
*/*          RETPTR     R/S    RETURN ADDRESS REGISTER, SAVED AND    */ 00475000
*/*                            RESTORED WITHIN THIS PROC FROM        */ 00476000
*/*                            CRRRTN8                               */ 00477000
*/*          BPTR       R/S    USER BUFFER AREA ADDRESS REGISTER,    */ 00478000
*/*                            USED IN DATA MOVE FROM FDB TO USER    */ 00479000
*/*                            AREA, SAVED AND RESTORED FROM CRRSAVE,*/ 00480000
*/*                            UPDATED IF UPON ENTRY TO PROC,        */ 00481000
*/*                            FCTR<BCTR, TO ITSELF PLUS FCTR, USED  */ 00482000
*/*                            TO SET CRRBARAD IN PARAMETER LIST, MAY*/ 00483000
*/*                            HAVE BEEN SET BY ANY OF THE PROCS     */ 00484000
*/*                            INCLUDING THIS ONE, PRIOR TO ENTRY    */ 00485000
*/*          BCTR       R/S    USER BUFFER LENGTH REGISTER, USED     */ 00486000
*/*                            TO DETERMINE WHICH OF THREE PATHS TO  */ 00487000
*/*                            TAKE BY COMPARISON TO FCTR, USED IN   */ 00488000
*/*                            DATA MOVE FROM FDB TO USER AREA FOR   */ 00489000
*/*                            FCTR>=BCTR AS DATA LENGTH TO MOVE,    */ 00490000
*/*                            SAVED AND RESTORED FROM CRRSAVE,      */ 00491000
*/*                            IF, UPON ENTRY, FCTR>=BCTR, USED TO   */ 00492000
*/*                            UPDATE FPTR, RCTR AND FCTR, IF UPON   */ 00493000
*/*                            ENTRY, FCTR<BCTR, IS SET TO ITSELF    */ 00494000
*/*                            MINUS FCTR, AND IS USED TO SET CRRBLEN*/ 00495000
*/*                            IN PARAMETER LIST, MAY HAVE BEEN SET  */ 00496000
*/*                            BY ANY OF THE PROCS INCLUDING THIS    */ 00497000
*/*                            ONE PRIOR TO ENTRY                    */ 00498000
*/*          FPTR       R/S    FDB DATA ADDRESS REGISTER, USED IN    */ 00499000
*/*                            DATA MOVE FROM FDB TO USER AREA, SAVED*/ 00500000
*/*                            AND RESTORED FROM CRRSAVE, IF UPON    */ 00501000
*/*                            ENTRY, FCTR>BCTR, SET TO ITSELF PLUS  */ 00502000
*/*                            BCTR, OR IF UPON ENTRY, FCTR=BCTR,    */ 00503000
*/*                            SET TO ITSELF PLUS BCTR AND USED TO   */ 00504000
*/*                            UPDATE FDBDATAA IN FDB, MAY HAVE BEEN */ 00505000
*/*                            SET BY ANY OF THE PROCS INCLUDING THIS*/ 00506000
*/*                            PRIOR TO ENTRY                        */ 00507000
*/*          FCTR       R/S    FDB DATA LENGTH REGISTER, USED TO     */ 00508000
*/*                            DETERMINE WHICH OF THREE PATHS TO     */ 00509000
*/*                            TAKE BY COMPARISON WITH BCTR, USED    */ 00510000
*/*                            IN DATA MOVE FROM FDB TO USER AREA    */ 00511000
*/*                            FOR FCTR<BCTR AS DATA LENGTH TO MOVE, */ 00512000
*/*                            SAVED AND RESTORED FROM CRRSAVE,      */ 00513000
*/*                            IF UPON ENTRY FCTR>BCTR,  SET TO      */ 00514000
*/*                            ITSELF MINUS BCTR                     */ 00515000
*/*                            IF UPON ENTRY FCTR=BCTR,              */ 00516000
*/*                            IS SET TO ITSELF MINUS BCTR AND USED  */ 00517000
*/*                            TO SET FDBDATCT                       */ 00518000
*/*                            IN FDB, IF UPON ENTRY FCTR<           */ 00519000
*/*                            BCTR, IS USED TO UPDATE BPTR,RCTR,    */ 00520000
*/*                            AND BCTR, MAY HAVE BEEN SET BY ANY    */ 00521000
*/*                            OF THE PROCS INCLUDING THIS ONE PRIOR */ 00522000
*/*                            TO ENTRY                              */ 00523000
*/*          FDBPTR     R/S    COVER REGISTER FOR FDB, IMPLICIT BASE */ 00524000
*/*                            FRO FDB SET IN PROCESSA OR SET BY THIS*/ 00525000
*/*                            PROC TO FDBFDBA, FDB CHAIN POINTER TO */ 00526000
*/*                            NEXT FDB                              */ 00527000
*/*          RCTR       R/S    TOTAL LENGTH MOVED REGISTER, IF UPON  */ 00528000
*/*                            ENTRY FCTR<BCTR SET TO ITSELF PLUS    */ 00529000
*/*                            FCTR, OR IF UPON ENTRY FCTR>=BCTR,    */ 00530000
*/*                            SET TO ITSELF PLUS BCTR               */ 00531000
*/*------------------------------------------------------------------*/ 00532000
