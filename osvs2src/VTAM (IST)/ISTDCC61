*        (GPR01P,                                                  0447 00259000
*         GPR13P) RSTD;                                            0447 00260000
*       GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);         0448 00261000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00262000
*       RFY                                                        0449 00263000
*         GPR13P UNRSTD;                                           0449 00264000
*       GPR04P=FSBPTR|'80000000'X;                                 0450 00265000
         LR    GPR04P,FSBPTR                                       0450 00266000
         O     GPR04P,@CF00809                                     0450 00267000
*       DCL                                                        0451 00268000
*         1 FMZA0006 FIXED(31) STATIC LOCAL,                       0451 00269000
*          2 * FIXED(8) INIT(80),                                  0451 00270000
*          2 * PTR(24) INIT(0);                                    0451 00271000
*       GPR03F=FMZA0006;                                           0452 00272000
         L     GPR03F,FMZA0006                                     0452 00273000
*       DO;                         /* LISATCVT                    0453 00274000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00275000
*         GPR15P=ATCLCPTR;          /* SET UP ATCVT POINTER          */ 00276000
         L     GPR15P,ATCLCPTR                                     0454 00277000
*       END;                        /* LISATCVT                      */ 00278000
*       GPR02P=GPR15P->ATCBPDA;                                    0456 00279000
         L     GPR02P,ATCBPDA(,GPR15P)                             0456 00280000
*       GPR15P=GPR15P->ATCSMRS;                                    0457 00281000
         L     GPR15P,ATCSMRS(,GPR15P)                             0457 00282000
