@ER00002 BR    @14                                                 0487 00364000
*   END FREEFSB;                                                   0488 00365000
*                                                                  0489 00366000
*/********************************************************************/ 00367000
*/*                                                                  */ 00368000
*/* ROUTINE NAME :  GETFSB - GET STORAGE FOR A FSB OR FDB            */ 00369000
*/*                                                                  */ 00370000
*/* FUNCTION :                                                       */ 00371000
*/*     GETFSB REQUESTS PAGEABLE STORAGE FOR A FSB AND WAITS         */ 00372000
*/*     AND WAITS FOR IT IF STORAGE IS NOT AVAILABLE                 */ 00373000
*/*                                                                  */ 00374000
*/* INPUT :  FMCBREG (REG 9) - POINTS TO FMCB                        */ 00375000
*/*                                                                  */ 00376000
*/* OUTPUT :  RPHSRPRM - POINTS TO NEW BLOCK OF STORAGE              */ 00377000
*/* SUBROUTINES CALLED :  NONE                                       */ 00378000
*/*                                                                  */ 00379000
*/* MACROS USED : REQSTORE, TPWAIT, TPLOCK, TPUNLOCK                 */ 00380000
*/*                                                                  */ 00381000
*/********************************************************************/ 00382000
*                                                                  0489 00383000
*GETFSB:                                                           0489 00384000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);                0489 00385000
GETFSB   DS    0H                                                  0490 00386000
*   RFY                                                            0490 00387000
*    (REG2,                                                        0490 00388000
*     RCREG) RSTD;                  /* RESTRICT RTN REG@D31KKNL      */ 00389000
*   REG2=RTNREG;                    /* SAVE RTN REG          @D31KKNL*/ 00390000
*                                                                  0491 00391000
         LR    REG2,RTNREG                                         0491 00392000
*   /*****************************************************************/ 00393000
*   /*                                                               */ 00394000
*   /* REQSTORE CBID(PDBUF)RPH(RPHPTR)CLEAR(YES)PRIORITY(YES)        */ 00395000
*   /*                                                               */ 00396000
*   /*****************************************************************/ 00397000
*                                                                  0492 00398000
*   DO;                                                            0492 00399000
*                                                                  0492 00400000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)                               */ 00401000
*/* END OF ATBUILD                                                   */ 00402000
*                                                                  0493 00403000
*     RFY                                                          0493 00404000
*      (GPR02P,                                                    0493 00405000
*       GPR03P,                                                    0493 00406000
*       GPR04P,                                                    0493 00407000
*       GPR15P) RSTD;                                              0493 00408000
*     RFY                                                          0494 00409000
*      (GPR01P,                                                    0494 00410000
