*       GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);         0461 00289000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              00290000
*       RFY                                                        0462 00291000
*        (GPR01P,                                                  0462 00292000
*         GPR13P) UNRSTD;                                          0462 00293000
*       RFY                                                        0463 00294000
*        (GPR02P,                                                  0463 00295000
*         GPR03P,                                                  0463 00296000
*         GPR04P,                                                  0463 00297000
*         GPR15P) UNRSTD;                                          0463 00298000
*     END;                          /* *********END OF RELSTORE    0464 00299000
*                                      EXPANSION*********            */ 00300000
*   ELSE                            /* RELSTORE                    0465 00301000
*                                      BUFADDR(FSBPTR)CHAIN(YES)     */ 00302000
*     DO;                                                          0465 00303000
*                                                                  0465 00304000
         B     @RC00444                                            0465 00305000
@RF00444 DS    0H                                                  0466 00306000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00307000
*/* END OF ATBUILD                                                   */ 00308000
*                                                                  0466 00309000
