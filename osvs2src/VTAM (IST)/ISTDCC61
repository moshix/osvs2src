         TITLE 'ISTDCC61-MOVE FROM FIXED TO PAGEABLE STORAGE           *00001000
                        '                                               00002000
ISTDCC61 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC61  75.325'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0387 00014000
*   /*****************************************************************/ 00015000
*   /*                                                               */ 00016000
*   /* ******************************************************* *     */ 00017000
*   /* REGISTER 9 IS USED TO COVER THE CRR UNTIL THE CALLER'S * *    */ 00018000
*   /* SAVE AREA POINTER(REG 13) CAN BE SAVE IN MY SAVE AREA * * REG */ 00019000
*   /* 13 IS THEN USED TO COVER THE CRR *                            */ 00020000
*   /*                                                               */ 00021000
*   /*****************************************************************/ 00022000
*                                                                  0388 00023000
*   CRRPTR=ADDR(CRACRR->CRRSPACE);  /* GET SA ADDR IN R13            */ 00024000
         L     @15,RPHCRR(,RPHPTR)                                 0388 00025000
         L     CRRPTR,CRACRR(,@15)                                 0388 00026000
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0388 00027000
*   RESPECIFY                                                      0389 00028000
*     CRRSAREA BASED(CRRPTR);       /* COVER CRR AT SAVE WHENEVER  0389 00029000
*                                      THIS MODU LE SAVES REGS IT  0389 00030000
*                                      WILLUSE THE FIRST CRR SAVEAREA*/ 00031000
*   CRRSAVE(5:8)=SAVREG;            /* SAVE CALLER'S R13             */ 00032000
         STCM  SAVREG,15,CRRSAVE+4(CRRPTR)                         0390 00033000
*   CRRSAVE1(9:12)=CRRPTR;          /* SAVE MY SAVE AREA PTR IN HIS     00034000
*                                      AREA                          */ 00035000
         STCM  CRRPTR,15,CRRSAVE1+8(CRRPTR)                        0391 00036000
*   SAVREG=CRRPTR;                  /* COVER CRR WITH R13            */ 00037000
         LR    SAVREG,CRRPTR                                       0392 00038000
*   RFY                                                            0393 00039000
*     CRRSAREA BASED(SAVREG);                                      0393 00040000
*                                                                  0393 00041000
*   /*****************************************************************/ 00042000
*   /*                                                               */ 00043000
*   /* THE FIXED FSB/FDB CHAIN TO BE MOVED IS FOUND IN THE * * CRR,  */ 00044000
*   /* PAGEABLE STORAGE IS REQUESTED FOR THE NEW FSB * * AND THE     */ 00045000
*   /* FSB/FDB IS MOVED                                              */ 00046000
*   /*                                                               */ 00047000
*   /*****************************************************************/ 00048000
*                                                                  0394 00049000
*   CRRM61=ON;                      /* SET AUDIT TRAILS              */ 00050000
         OI    CRRM61(SAVREG),B'01000000'                          0394 00051000
*   CRRC61=ON;                                                     0395 00052000
         OI    CRRC61(SAVREG),B'01000000'                          0395 00053000
*   FSBPTR=CRRFSB;                  /* GET OLD FSB ADR               */ 00054000
         L     FSBPTR,CRRFSB(,SAVREG)                              0396 00055000
*   FDBREG=ADDR(FSBFDB);            /* ADDR FDB IN FSB               */ 00056000
         LA    FDBREG,FSBFDB(,FSBPTR)                              0397 00057000
*   CALL GETFSB;                    /* GET A FSB(PDBUF)      @D31KKNL*/ 00058000
         BAL   @14,GETFSB                                          0398 00059000
*   PAGEFSB=RPHSRPRM;               /* ADDR PAGEABLE STOR            */ 00060000
         L     PAGEFSB,RPHSRPRM(,RPHPTR)                           0399 00061000
*   FSBSIZE=LENGTH(ISTFSB)-LENGTH(ISTFDB);/* LENGTH OF FSB           */ 00062000
         LA    FSBSIZE,32                                          0400 00063000
*   PAGEFDB=PAGEFSB+FSBSIZE;        /* PSTOR FDB ADDR                */ 00064000
         LR    PAGEFDB,PAGEFSB                                     0401 00065000
         ALR   PAGEFDB,FSBSIZE                                     0401 00066000
*   PAGEFSB->ISTFSB(1:LENGTH(ISTFSB))=ISTFSB;/* MOVE FSB             */ 00067000
         MVC   ISTFSB(44,PAGEFSB),ISTFSB(FSBPTR)                   0402 00068000
*   RESPECIFY                                                      0403 00069000
*    (REG2,                                                        0403 00070000
*     REG3,                                                        0403 00071000
*     REG4,                                                        0403 00072000
*     REG5) RSTD;                   /* TEMP TEST REGS                */ 00073000
*                                                                  0403 00074000
*   /*****************************************************************/ 00075000
*   /*                                                               */ 00076000
*   /* ANY DATA FOR THE FSB/FDB IS NOW MOVED INTO THE PAGEABLE* *    */ 00077000
*   /* BUFFER FOLLOWING THE FSB/FDB CONTROL BLOCK                    */ 00078000
*   /*                                                               */ 00079000
*   /*****************************************************************/ 00080000
*                                                                  0404 00081000
*   REG2=PAGEFSB+FSBLNGTH;          /* DATA TO ADDRESS               */ 00082000
         LR    REG2,PAGEFSB                                        0404 00083000
         SLR   @15,@15                                             0404 00084000
         IC    @15,FSBLNGTH(,FSBPTR)                               0404 00085000
         ALR   REG2,@15                                            0404 00086000
*   REG3=FDBDATCT;                  /* AMOUNT OF DATA                */ 00087000
         LH    REG3,FDBDATCT(,FDBREG)                              0405 00088000
*   REG5=REG3;                      /* LENGTH FOR MVCL               */ 00089000
         LR    REG5,REG3                                           0406 00090000
*   REG4=FDBDATAA;                  /* DATA FROM ADDR                */ 00091000
         L     REG4,FDBDATAA(,FDBREG)                              0407 00092000
*   MVCL(REG2,REG4);                /* MOVE DATA                     */ 00093000
         MVCL  REG2,REG4                                           0408 00094000
*   RESPECIFY                                                      0409 00095000
*    (REG2,                                                        0409 00096000
*     REG3,                                                        0409 00097000
*     REG4,                                                        0409 00098000
*     REG5) UNRSTD;                                                0409 00099000
*   RPHWEA=PAGEFSB;                 /* NEW FSB ADR TO RPH            */ 00100000
         ST    PAGEFSB,RPHWEA(,RPHPTR)                             0410 00101000
*   PAGEFDB->FDBOFSET=FSBSIZE;      /* PAGEABLD FDB OFFST            */ 00102000
         STH   FSBSIZE,FDBOFSET(,PAGEFDB)                          0411 00103000
*   PAGEFDB->FDBDATAA=PAGEFDB+LENGTH(ISTFDB);                      0412 00104000
*                                                                  0412 00105000
         LA    @15,12                                              0412 00106000
         ALR   @15,PAGEFDB                                         0412 00107000
         ST    @15,FDBDATAA(,PAGEFDB)                              0412 00108000
*   /*****************************************************************/ 00109000
*   /*                                                               */ 00110000
*   /* ****************************************************** * ANY  */ 00111000
*   /* OTHER FDBS ARE MOVED IN THE SAME MANNER. FIRST THE * CONTROL  */ 00112000
*   /* BLOCK ITSELF THEN ITS ASSOCIATED DATA. THE * * CONTROL BLOCK  */ 00113000
*   /* AND ITS DATA NEED NOT BE CONTIGUOUS IN * * FIXED STORAGE BUT  */ 00114000
*   /* IS CONTIGUOUS AFTER MOVED TO PAGEABLE * STORAGE * *******     */ 00115000
*   /*                                                               */ 00116000
*   /*****************************************************************/ 00117000
*                                                                  0413 00118000
*   DO WHILE PAGEFDB->FDBFDBA^=0&   /* ANOTHER FDB EXIST             */ 00119000
*         PAGEFDB->FDBFDBA^=GATEWD; /* NOT GATED@DM12976             */ 00120000
         B     @DE00413                                            0413 00121000
@DL00413 DS    0H                                                  0414 00122000
*     FDBREG=PAGEFDB->FDBFDBA;      /* COVER NEXT FDB                */ 00123000
         L     FDBREG,FDBFDBA(,PAGEFDB)                            0414 00124000
*     CALL GETFSB;                  /* GET A FSB(PDBUF)      @D31KKNL*/ 00125000
         BAL   @14,GETFSB                                          0415 00126000
*     PAGEFSB=RPHSRPRM;             /* GET ADDR OF PAGEABLE FDB AREA */ 00127000
         L     PAGEFSB,RPHSRPRM(,RPHPTR)                           0416 00128000
*     PAGEFDB->FDBFDBA=PAGEFSB;     /* SET NEXT FDB PTR              */ 00129000
         ST    PAGEFSB,FDBFDBA(,PAGEFDB)                           0417 00130000
*     PAGEFDB=PAGEFSB;              /* COVER NEXT FDB PLUS OFFSET  0418 00131000
*                                      MOVE FDB,DATA AND CONTROL BLK    00132000
*                                      TO PAGE STORAGE               */ 00133000
         LR    PAGEFDB,PAGEFSB                                     0418 00134000
*     PAGEFDB->ISTFDB(1:LENGTH(ISTFDB))=ISTFDB;/* MOVE FDB CB        */ 00135000
*                                                                  0419 00136000
         MVC   ISTFDB(12,PAGEFDB),ISTFDB(FDBREG)                   0419 00137000
*     /***************************************************************/ 00138000
*     /*                                                             */ 00139000
*     /* MOVE DATA FOR THIS FDB                                      */ 00140000
*     /*                                                             */ 00141000
*     /***************************************************************/ 00142000
*                                                                  0420 00143000
*     RESPECIFY                                                    0420 00144000
*      (REG2,                                                      0420 00145000
*       REG3,                                                      0420 00146000
*       REG4,                                                      0420 00147000
*       REG5) RSTD;                 /* TEMP REST REGS                */ 00148000
*     REG2=PAGEFSB+LENGTH(ISTFDB);  /* DATA TO ADDRESS               */ 00149000
         LA    REG2,12                                             0421 00150000
         ALR   REG2,PAGEFSB                                        0421 00151000
*     REG3=FDBDATCT;                /* AMOUNT OF DATA                */ 00152000
         LH    REG3,FDBDATCT(,FDBREG)                              0422 00153000
*     REG5=REG3;                    /* 2ND LEN REG                   */ 00154000
         LR    REG5,REG3                                           0423 00155000
*     REG4=FDBDATAA;                /* DATA FROM ADDRESS             */ 00156000
         L     REG4,FDBDATAA(,FDBREG)                              0424 00157000
*     MVCL(REG2,REG4);              /* MOVE DATA                     */ 00158000
         MVCL  REG2,REG4                                           0425 00159000
*     RESPECIFY                                                    0426 00160000
*      (REG2,                                                      0426 00161000
*       REG3,                                                      0426 00162000
*       REG4,                                                      0426 00163000
*       REG5) UNRSTD;                                              0426 00164000
*     PAGEFDB->FDBOFSET=ZERO;       /* PAGEABLE OFFSET               */ 00165000
         SLR   @15,@15                                             0427 00166000
         STH   @15,FDBOFSET(,PAGEFDB)                              0427 00167000
*     PAGEFDB->FDBDATAA=PAGEFSB+LENGTH(ISTFDB);/* PAGE DATA ADDR     */ 00168000
         LA    @15,12                                              0428 00169000
         ALR   @15,PAGEFSB                                         0428 00170000
         ST    @15,FDBDATAA(,PAGEFDB)                              0428 00171000
*   END;                            /* FDB MOVED TO PSTR             */ 00172000
*                                                                  0429 00173000
@DE00413 L     @15,FDBFDBA(,PAGEFDB)                               0429 00174000
         LTR   @15,@15                                             0429 00175000
         BZ    @DC00413                                            0429 00176000
         CL    @15,@CF00809                                        0429 00177000
         BNE   @DL00413                                            0429 00178000
@DC00413 DS    0H                                                  0430 00179000
*   /*****************************************************************/ 00180000
*   /*                                                               */ 00181000
*   /* FSB/FDB CHAIN IS MOVED-FREE FIXED BUFFERS                     */ 00182000
*   /*                                                               */ 00183000
*   /*****************************************************************/ 00184000
*                                                                  0430 00185000
*   CALL FREEFSB;                   /* FREE UP FSB FDB CH            */ 00186000
         BAL   @14,FREEFSB                                         0430 00187000
*   CRRFSB=RPHWEA;                  /* NEW FSB ADDR TO CRR           */ 00188000
         L     @15,RPHWEA(,RPHPTR)                                 0431 00189000
         ST    @15,CRRFSB(,SAVREG)                                 0431 00190000
*   SAVREG=CRRSAVE(5:8);            /* CALLER'S REG 13               */ 00191000
         ICM   @15,15,CRRSAVE+4(SAVREG)                            0432 00192000
         LR    SAVREG,@15                                          0432 00193000
*EXIT:                                                             0433 00194000
*   RETURN;                                                        0433 00195000
@EL00001 DS    0H                                                  0433 00196000
@EF00001 DS    0H                                                  0433 00197000
@ER00001 LM    @14,@12,12(@13)                                     0433 00198000
         BR    @14                                                 0433 00199000
*FREEFSB:                                                          0434 00200000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);                     0434 00201000
*                                                                  0434 00202000
FREEFSB  DS    0H                                                  0435 00203000
*/********************************************************************/ 00204000
*/*                                                                  */ 00205000
*/* ROUTINE NAME : FREEFSB - FREE STORAGE FOR FSB AND FDB'S          */ 00206000
*/*                                                                  */ 00207000
*/* FUNCTION :                                                       */ 00208000
*/*      STORAGE FOR THE FIXED FSB/FDB CHAIN IS RELEASED. IF THE     */ 00209000
*/*      DATA IS CONFIDENTIAL THE BUFFERS ARE CLEARED TO ZEROES      */ 00210000
*/*                                                                  */ 00211000
*/* INPUT :  FMCBREG (REG 9) - POINTS TO THE FSB                     */ 00212000
*/*                                                                  */ 00213000
*/* OUTPUT : SAME                                                    */ 00214000
*/*                                                                  */ 00215000
*/* SUBROUTINES CALLED :  NONE                                       */ 00216000
*/*                                                                  */ 00217000
*/* MACROS USED : RELSTORE                                           */ 00218000
*/*                                                                  */ 00219000
*/********************************************************************/ 00220000
*                                                                  0435 00221000
*   RFY                                                            0435 00222000
*     REG2 RSTD;                    /* SAVE RETURN REG               */ 00223000
*   REG2=RTNREG;                    /* FOR FREEFSB                   */ 00224000
         LR    REG2,RTNREG                                         0436 00225000
*   FSBPTR=CRRFSB;                  /* GET ADDR OF OLD CHAIN TO    0437 00226000
*                                      DELETE                        */ 00227000
         L     FSBPTR,CRRFSB(,SAVREG)                              0437 00228000
*   FDBREG=ADDR(FSBFDB);            /* COVER FDB                     */ 00229000
         LA    FDBREG,FSBFDB(,FSBPTR)                              0438 00230000
*   FSBPTR=FDBREG-FDBOFSET;         /* PT TO BEGIN OF CONTROL BLOCK  */ 00231000
         LR    FSBPTR,FDBREG                                       0439 00232000
         SH    FSBPTR,FDBOFSET(,FDBREG)                            0439 00233000
*   FSBPTR->FDBFDBA=FDBFDBA;        /* SET UP A CHAIN PTR            */ 00234000
         L     @15,FDBFDBA(,FDBREG)                                0440 00235000
         ST    @15,FDBFDBA(,FSBPTR)                                0440 00236000
*   RFY                                                            0441 00237000
*     ISTFMCB BASED(FMCBPTR);       /* BASE THE FMCB                 */ 00238000
*   FMCBPTR=CRRFMCB;                /* SET UP PTR TO FMCB            */ 00239000
         L     FMCBPTR,CRRFMCB(,SAVREG)                            0442 00240000
*   RFY                                                            0443 00241000
*     ISTPROCD BASED(ADDR(FMCPROCD));                              0443 00242000
*   IF PROCFTX=ON                   /* IF CONFIDENTIAL               */ 00243000
*     THEN                          /* RELSTORE BUFADDR(FSBPTR)CLEAR(   00244000
*                                      YES)CHAIN(YES)                */ 00245000
         TM    PROCFTX+48(FMCBPTR),B'00000001'                     0444 00246000
         BNO   @RF00444                                            0444 00247000
*     DO;                                                          0445 00248000
*                                                                  0445 00249000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00250000
*/* END OF ATBUILD                                                   */ 00251000
*                                                                  0446 00252000
*       RFY                                                        0446 00253000
