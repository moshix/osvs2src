*                                                                  0864 01063000
*/********************************************************************/ 01064000
*/*                                                                  */ 01065000
*/* ROUTINE NAME : LCPBINIT - BUILD A LCPB                           */ 01066000
*/*                                                                  */ 01067000
*/* FUNCTION :                                                       */ 01068000
*/*                                                                  */ 01069000
*/* INPUT :  FMCBPTR (REG 11) - POINTS TO THE FMCB                   */ 01070000
*/*                                                                  */ 01071000
*/* OUTPUT : LCPBPTR (REG 9) - POINTS TO NEW LCPB                    */ 01072000
*/*                                                                  */ 01073000
*/* SUBROUTINES CALLED : NONE                                        */ 01074000
*/*                                                                  */ 01075000
*/* MACROS USED : REQSTORE, TPWAIT                                   */ 01076000
*/*                                                                  */ 01077000
*/********************************************************************/ 01078000
*                                                                  0864 01079000
*LCPBINIT:                                                         0864 01080000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);                     0864 01081000
*                                                                  0864 01082000
LCPBINIT DS    0H                                                  0865 01083000
*   /*****************************************************************/ 01084000
*   /*                                                               */ 01085000
*   /* ***************************************************** * THIS  */ 01086000
*   /* ROUTINE GETS STORAGE FOR AND INITIALIZES * * A PURGE LCPB *   */ 01087000
*   /*                                                               */ 01088000
*   /*****************************************************************/ 01089000
*                                                                  0865 01090000
*   CRRRTN4=RTNREG;                 /* SAVE RETURN REG               */ 01091000
         ST    RTNREG,CRRRTN4(,CRRPTR)                             0865 01092000
*   RFY                                                            0866 01093000
*     RCREG RSTD;                   /* SAVE RETURN CODE REG          */ 01094000
*                                                                  0866 01095000
*   /*****************************************************************/ 01096000
*   /*                                                               */ 01097000
*   /* REQSTORE CBID(DCLCP)CLEAR(YES)RPH(RPHPTR)                     */ 01098000
*   /*                                                               */ 01099000
*   /*****************************************************************/ 01100000
*                                                                  0867 01101000
*   DO;                                                            0867 01102000
*                                                                  0867 01103000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)                               */ 01104000
*/* END OF ATBUILD                                                   */ 01105000
*                                                                  0868 01106000
*     RFY                                                          0868 01107000
*      (GPR02P,                                                    0868 01108000
*       GPR03P,                                                    0868 01109000
*       GPR04P,                                                    0868 01110000
*       GPR15P) RSTD;                                              0868 01111000
*     RFY                                                          0869 01112000
*      (GPR01P,                                                    0869 01113000
*       GPR13P) RSTD;                                              0869 01114000
