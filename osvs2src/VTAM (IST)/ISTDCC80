*      (GPR02P,                                                    0885 01149000
*       GPR03P,                                                    0885 01150000
*       GPR04P,                                                    0885 01151000
*       GPR15P) UNRSTD;                                            0885 01152000
*   END;                                                           0886 01153000
*                                                                  0886 01154000
*   /*****************************************************************/ 01155000
*   /*                                                               */ 01156000
*   /* ******** END OF REQSTORE EXPANSION                            */ 01157000
*   /*                                                               */ 01158000
*   /*****************************************************************/ 01159000
*                                                                  0887 01160000
*   IF RCREG=8                      /* IF STORAGE NOT AVAIL          */ 01161000
*     THEN                          /* WAIT FOR IT                   */ 01162000
         C     RCREG,@CF00059                                      0887 01163000
         BNE   @RF00887                                            0887 01164000
*/*  TPWAIT                                                          */ 01165000
*                                                                  0888 01166000
*     DO;                           /* BEGIN TPWAIT GENERATION       */ 01167000
*       RFY                                                        0889 01168000
*        (GPR01P,                                                  0889 01169000
*         GPR14P,                                                  0889 01170000
*         GPR15P) RSTD;                                            0889 01171000
*       RFY                                                        0890 01172000
