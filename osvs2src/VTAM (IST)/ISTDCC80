*       GPR13P) UNRSTD;                                            0871 01119000
*     DO;                           /* LISATCVT                    0872 01120000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 01121000
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 01122000
         L     GPR15P,ATCLCPTR                                     0873 01123000
*     END;                          /* LISATCVT                      */ 01124000
*     GPR02P=GPR15P->ATCBPDA|'80000000'X;                          0875 01125000
         L     GPR02P,ATCBPDA(,GPR15P)                             0875 01126000
         O     GPR02P,@CF01579                                     0875 01127000
*     DCL                                                          0876 01128000
*       1 FMZA0113 FIXED(31) STATIC LOCAL,                         0876 01129000
*        2 * FIXED(8) INIT(184),                                   0876 01130000
*        2 * PTR(24) INIT(0);                                      0876 01131000
*     GPR03F=FMZA0113;                                             0877 01132000
