*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00379000
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00380000
         L     GPR15P,ATCLCPTR                                     0625 00381000
*     END;                          /* LISATCVT                      */ 00382000
*     RESPECIFY                                                    0627 00383000
*       GPR00F RSTD;                                               0627 00384000
*     GPR00F=4;                     /* SET PARAMETER REGISTER TO 4   */ 00385000
         LA    GPR00F,4                                            0628 00386000
*     GPR15P=GPR15P->ATCAP31;                                      0629 00387000
         L     GPR15P,ATCAP31(,GPR15P)                             0629 00388000
*     DCL                                                          0630 00389000
*       FMZL0041 BASED(GPR15P) LABEL VALUERANGE(*);                0630 00390000
*     CALL FMZL0041;                                               0631 00391000
         BALR  @14,@15                                             0631 00392000
*     RESPECIFY                                                    0632 00393000
*      (GPR00F,                                                    0632 00394000
*       GPR15P) UNRSTD;                                            0632 00395000
*   END;                            /* **** END OF TPEXIT EXIT TO PSS*/ 00396000
*                                                                  0634 00397000
*/********************************************************************/ 00398000
*/*                                                                  */ 00399000
*/* ROUTINE NAME : FREEFSB -  BUFFER RELEASE                         */ 00400000
*/*                                                                  */ 00401000
*/* FUNCTION :                                                       */ 00402000
*/*     FREEFSB LOOPS DOWN THE FMCB DATA QUEUE FREEING UP ALL        */ 00403000
*/*     FSB/FDB CHAINS FOUND THERE.  IF VARY SOFT PURGE IS IN        */ 00404000
*/*     PROGRESS THE ERROR LOCK IS SET IN THE CLOP TO STOP FURTHER   */ 00405000
*/*     REQUESTS TO CONTROL LAYER OUTBOUND.                          */ 00406000
*/*                                                                  */ 00407000
*/* INPUT : FMCBPTR (REG 11) - POINTS TO THE FMCB                    */ 00408000
*/*                                                                  */ 00409000
*/* OUTPUT : SAME                                                    */ 00410000
*/*                                                                  */ 00411000
*/* SUBROUTINES CALLED : NONE                                        */ 00412000
*/*                                                                  */ 00413000
*/* MACROS USED : TPDEQ, RELSTORE                                    */ 00414000
*/*                                                                  */ 00415000
*/********************************************************************/ 00416000
*                                                                  0634 00417000
*FREEFSB:                                                          0634 00418000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);                0634 00419000
         B     @PB00002                                            0634 00420000
FREEFSB  DS    0H                                                  0635 00421000
*   CRRRTN1=RTNREG;                 /* SAVE RETURN REG               */ 00422000
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0635 00423000
*   RFY                                                            0636 00424000
*     ISTDCLCP BASED(CRRARPL);      /* COVER WRK ELT LCPB            */ 00425000
*   IF LCPMNRCD^='05'X&             /* NOT APPL. ABEND               */ 00426000
*       LCPMNRCD^='06'X THEN        /* NOT CLOSE DEST                */ 00427000
         L     @15,CRRARPL(,CRRPTR)                                0637 00428000
         CLI   LCPMNRCD(@15),X'05'                                 0637 00429000
         BE    @RF00637                                            0637 00430000
         CLI   LCPMNRCD(@15),X'06'                                 0637 00431000
         BE    @RF00637                                            0637 00432000
*     DO;                           /* IS A VARY CASE                */ 00433000
*       IF LCPMNRCD=FRPRARSS THEN   /* IF VARY SOFT PURGE IN PROG    */ 00434000
         CLI   LCPMNRCD(@15),X'01'                                 0639 00435000
         BNE   @RF00639                                            0639 00436000
*         DO;                                                      0640 00437000
*           FMCVSP1=ON;             /* SET VARY SOFT PURGE FLAG      */ 00438000
         OI    FMCVSP1(FMCBPTR),B'00000001'                        0641 00439000
*           ADDR(FMCPAB1)->PABERRLK=ON;/* TURN ON CLOP ERR LOCK      */ 00440000
         OI    PABERRLK+64(FMCBPTR),B'00000100'                    0642 00441000
*         END;                      /* END-VARY SOFT PURGE           */ 00442000
*       ELSE                                                       0644 00443000
*         ;                         /* NOT SOFT PURGE                */ 00444000
@RF00639 DS    0H                                                  0645 00445000
*     END;                          /* END VARY SPECIFIED            */ 00446000
*   ELSE                                                           0646 00447000
*     DO;                           /* IF VARY NOT SPECIFIED         */ 00448000
         B     @RC00637                                            0646 00449000
@RF00637 DS    0H                                                  0647 00450000
*       RFY                                                        0647 00451000
*        (FSBPTR,                                                  0647 00452000
*         FDBREG) RSTD;                                            0647 00453000
*                                                                  0647 00454000
*/* TPDEQ (,FMCFBA) RETPTR(FSBPTR)                                   */ 00455000
*                                                                  0648 00456000
*       DO;                         /* BEGIN TPDEQ CODE GENERATION   */ 00457000
*         DCL                                                      0649 00458000
*           FMZCA049 BIT(32) CONSTANT('80000000'X);                0649 00459000
*         DCL                                                      0650 00460000
*           FMZCB049 BIT(32) CONSTANT('7FFFFFFF'X);                0650 00461000
*         RFY                                                      0651 00462000
*          (GPR00P,                                                0651 00463000
*           GPR01P,                                                0651 00464000
*           GPR14P,                                                0651 00465000
*           GPR15P) RSTD;                                          0651 00466000
*         RFY                                                      0652 00467000
*           ISTWKE;                                                0652 00468000
*         GPR15P=ADDR(FMCFBA);      /* ADDRESS QUEUE POINTER         */ 00469000
         LA    GPR15P,FMCFBA(,FMCBPTR)                             0653 00470000
*         GPR14P=GPR15P->PABWEQA|FMZCA049;                         0654 00471000
         L     @08,@CF01579                                        0654 00472000
         L     GPR14P,PABWEQA(,GPR15P)                             0654 00473000
         OR    GPR14P,@08                                          0654 00474000
*         IF GPR14P=FMZCA049 THEN                                  0655 00475000
         CLR   GPR14P,@08                                          0655 00476000
         BE    @RT00655                                            0655 00477000
*           ;                                                      0656 00478000
*         ELSE                                                     0657 00479000
*           DO;                                                    0657 00480000
*FMZL1049:                                                         0658 00481000
*             ;                                                    0658 00482000
FMZL1049 DS    0H                                                  0659 00483000
*             DO WHILE(GPR14P->WKECHAIN^=FMZCA049);                0659 00484000
         B     @DE00659                                            0659 00485000
@DL00659 DS    0H                                                  0660 00486000
*               GPR15P=ADDR(GPR14P->WKECHAIN);                     0660 00487000
         LA    GPR15P,WKECHAIN(,GPR14P)                            0660 00488000
*               GPR14P=GPR15P->PABWEQA;                            0661 00489000
