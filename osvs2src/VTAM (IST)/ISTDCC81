         L     GPR15P,ATCLCPTR                                     0535 00195000
*             END; /*LISATCVT*/                                    0536 00196000
*         GPR02P = GPR15P->ATCBPDA;                                0537 00197000
         L     GPR02P,ATCBPDA(,GPR15P)                             0537 00198000
*         GPR15P=GPR15P->ATCSMRS;                                  0538 00199000
