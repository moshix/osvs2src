         L     GPR15P,CURRPTRB(,CRRPTR)                            1336 02105000
         LA    GPR15P,LCPRSTQH(,GPR15P)                            1336 02106000
*      GPR14P=GPR15P->PABWEQA|FMZCA108;                            1337 02107000
         L     @10,@CF01149                                        1337 02108000
         L     GPR14P,PABWEQA(,GPR15P)                             1337 02109000
         OR    GPR14P,@10                                          1337 02110000
*      IF GPR14P=FMZCA108 THEN;                                    1338 02111000
         CLR   GPR14P,@10                                          1338 02112000
         BE    @RT01338                                            1338 02113000
*      ELSE DO;                                                    1340 02114000
*FMZL1262:;                                                        1341 02115000
FMZL1262 DS    0H                                                  1342 02116000
*        DO WHILE(GPR14P->WKECHAIN^=FMZCA108);                     1342 02117000
