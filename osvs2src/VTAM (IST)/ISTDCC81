         L     GPR15P,ATCLCPTR                                     0719 00659000
*             END; /*LISATCVT*/                                    0720 00660000
*         GPR02P = GPR15P->ATCBPDA;                                0721 00661000
         L     GPR02P,ATCBPDA(,GPR15P)                             0721 00662000
*         GPR15P=GPR15P->ATCSMRS;                                  0722 00663000
         L     GPR15P,ATCSMRS(,GPR15P)                             0722 00664000
*         DCL FMZL0076 BASED(GPR15P) LABEL VALUERANGE(*);          0723 00665000
