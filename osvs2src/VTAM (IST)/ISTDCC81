*       END;                              /* END-LCPB HAS NO RPL   */   00311000
*       END;                               /* END LCPB NOT FOR PURGE*/  00312000
*      END;                                /* END FSB HAS LCPB */  0596 00313000
*     ELSE CALL FREEFSB;                 /* NO LCPB-FREE FSB/FDBS     * 00314000
         B     @RC00498                                            0597 00315000
@RF00498 BAL   @14,FREEFSB                                         0597 00316000
*     RTNREG=CRRRTN1;                      /* RESTORE RETURN REG      * 00317000
