*        GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/               0483 00050000
         L     GPR15P,ATCLCPTR                                     0483 00051000
*      END; /*LISATCVT*/                                           0484 00052000
*        RESPECIFY GPR00F RSTD;                                    0485 00053000
*        GPR00F=4;    /* SET PARAMETER REGISTER TO 4 */            0486 00054000
         LA    GPR00F,4                                            0486 00055000
*        GPR15P=GPR15P->ATCAP31;                                   0487 00056000
         L     GPR15P,ATCAP31(,GPR15P)                             0487 00057000
*        DCL FMZL0007 BASED(GPR15P) LABEL VALUERANGE(*);           0488 00058000
*        CALL FMZL0007;                                            0489 00059000
         BALR  @14,@15                                             0489 00060000
*        RESPECIFY (GPR00F,GPR15P) UNRSTD;                         0490 00061000
