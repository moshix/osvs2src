@ER00011 BR    @14                                                 1409 02289000
*END PSTRPL;                                                       1410 02290000
*/********************************************************************/ 02291000
*/*                                                                  */ 02292000
*/* ROUTINE NAME :  FREELCPB - FREE UP LCPB AND LCCW'S               */ 02293000
*/*                                                                  */ 02294000
*/* FUNCTION :                                                       */ 02295000
*/*    THIS ROUTINE FREES ALL LCCW AREAS ASSOCIATED WITH THIS LCPB   */ 02296000
*/*    AND THEN THE LCPB ISSELF.  LCCW AREAS ARE THOSE WHICH ARE     */ 02297000
*/*    (TIC)'D TO FROM THE CURRENT LCCW.                             */ 02298000
*/*                                                                  */ 02299000
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */ 02300000
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */ 02301000
*/*                                                                  */ 02302000
*/* OUTPUT :  LCPB AND LCCW BUFFER SPACE RELEASED                    */ 02303000
*/*                                                                  */ 02304000
*/* SUBROUTINES CALLED : NONE                                        */ 02305000
*/*                                                                  */ 02306000
*/*  MACROS USED :  RELSTORE                                         */ 02307000
*/*                                                                  */ 02308000
*/********************************************************************/ 02309000
*FREELCPB: PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);              1411 02310000
FREELCPB DS    0H                                                  1412 02311000
*     CRRRTN11=RTNREG;                     /* SAVE RETURN ADDRESS    */ 02312000
         ST    RTNREG,CRRRTN11(,CRRPTR)                            1412 02313000
*     RFY ISTDCLCP BASED(FSBLCPB);                                 1413 02314000
*     RFY ISTLCCW BASED(ADDR(LCPLCCW));                            1414 02315000
*     RFY (LCCWPTR,STRTLCCW) RSTD;                                 1415 02316000
*     IF FSBELI=ON THEN                    /* IS LCPB USELESS        */ 02317000
         TM    FSBELI(FSBPTR),B'10000000'                          1416 02318000
         BNO   @RF01416                                            1416 02319000
*        DO;                               /* FREE IT UP             */ 02320000
*        IF LCCWOP=LTIC THEN                                       1418 02321000
         LA    @15,48                                              1418 02322000
         AL    @15,FSBLCPB(,FSBPTR)                                1418 02323000
         CLI   LCCWOP(@15),X'08'                                   1418 02324000
         BNE   @RF01418                                            1418 02325000
*          DO;                             /* MORE LCCWS TO FREE     */ 02326000
*          LCCWPTR=LCCWDATA;               /* GET NEXT LCCW AREA     */ 02327000
         L     LCCWPTR,LCCWDATA(,@15)                              1420 02328000
*          STRTLCCW=LCCWPTR;               /* THIS IS CURRENT LCCW   */ 02329000
         LR    STRTLCCW,LCCWPTR                                    1421 02330000
*          CRRSW1=ON;                      /* MORE LCCWS              * 02331000
         OI    CRRSW1(CRRPTR),B'01000000'                          1422 02332000
*          DO WHILE CRRSW1=ON;             /* WHILE MORE LCCWS       */ 02333000
         B     @DE01423                                            1423 02334000
@DL01423 DS    0H                                                  1424 02335000
*             DO WHILE LCCWPTR->LCCWOP=LTIC;                       1424 02336000
         B     @DE01424                                            1424 02337000
@DL01424 DS    0H                                                  1425 02338000
*               LCCWPTR=LCCWPTR->LCCWDATA; /* POINT TO NEXT LCCW     */ 02339000
         L     LCCWPTR,LCCWDATA(,LCCWPTR)                          1425 02340000
*                /*RELSTORE BUFADDR(STRTLCCW)*/                    1426 02341000
*                 DO;                                              1426 02342000
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  1427 02343000
*/* END OF ATBUILD */                                              1427 02344000
*                RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;           1427 02345000
*         RFY (GPR01P,GPR13P) RSTD;                                1428 02346000
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       1429 02347000
*                                                                  1429 02348000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             02349000
*         RFY GPR13P UNRSTD;                                       1430 02350000
*         GPR04P=STRTLCCW;                                         1431 02351000
         LR    GPR04P,STRTLCCW                                     1431 02352000
*         DCL 1 FMZA0274 FIXED(31) STATIC LOCAL,                   1432 02353000
*             2 * FIXED(8) INIT(64),                               1432 02354000
*             2 * PTR(24) INIT(0);                                 1432 02355000
*         GPR03F = FMZA0274;                                       1433 02356000
*                                                                  1433 02357000
         L     GPR03F,FMZA0274                                     1433 02358000
*                DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */     1434 02359000
*                  GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/     1435 02360000
         L     GPR15P,ATCLCPTR                                     1435 02361000
*                END; /*LISATCVT*/                                 1436 02362000
*         GPR02P = GPR15P->ATCBPDA;                                1437 02363000
         L     GPR02P,ATCBPDA(,GPR15P)                             1437 02364000
*         GPR15P=GPR15P->ATCSMRS;                                  1438 02365000
         L     GPR15P,ATCSMRS(,GPR15P)                             1438 02366000
*         DCL FMZL0274 BASED(GPR15P) LABEL VALUERANGE(*);          1439 02367000
*         CALL FMZL0274;                                           1440 02368000
         BALR  @14,@15                                             1440 02369000
*         RFY GPR13P RSTD;                                         1441 02370000
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       1442 02371000
*                                                                  1442 02372000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              02373000
*         RFY (GPR01P,GPR13P) UNRSTD;                              1443 02374000
*                RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;         1444 02375000
*                END ;/**********END OF RELSTORE EXPANSION**********/   02376000
*/* FREE PREVIOUS LCCW*/                                           1446 02377000
*               STRTLCCW=LCCWPTR;          /* UPDATE TO NEW LCCW     */ 02378000
         LR    STRTLCCW,LCCWPTR                                    1446 02379000
*               END;                                               1447 02380000
@DE01424 CLI   LCCWOP(LCCWPTR),X'08'                               1447 02381000
         BE    @DL01424                                            1447 02382000
*               /* END DO WHILD TICS      */                       1448 02383000
*             IF LCCWPTR->LCCWCC=ON |  /* COMMAND CHAINED            */ 02384000
*                LCCWPTR->LCCWCD=ON THEN  /* OR DATA CHAINED         */ 02385000
         TM    LCCWCC(LCCWPTR),B'11000000'                         1448 02386000
         BZ    @RF01448                                            1448 02387000
*               DO;                        /*THERE ARE MORE LCCWS    */ 02388000
*               CRRSW1=ON;                                         1450 02389000
         OI    CRRSW1(CRRPTR),B'01000000'                          1450 02390000
*               LCCWPTR=LCCWPTR+8;         /* BUMP TO NEXT LCCW      */ 02391000
         AL    LCCWPTR,@CF00060                                    1451 02392000
*               END;                                               1452 02393000
*             ELSE CRRSW1=OFF;             /* NO MORE LCCWS          */ 02394000
         B     @RC01448                                            1453 02395000
@RF01448 NI    CRRSW1(CRRPTR),B'10111111'                          1453 02396000
*             END;                         /* END DO WHILE MORE LCCWS   02397000
*             */                                                   1454 02398000
@RC01448 DS    0H                                                  1454 02399000
@DE01423 TM    CRRSW1(CRRPTR),B'01000000'                          1454 02400000
         BO    @DL01423                                            1454 02401000
*            /*RELSTORE BUFADDR(STRTLCCW)*/                        1455 02402000
*             DO;                                                  1455 02403000
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  1456 02404000
*/* END OF ATBUILD */                                              1456 02405000
*            RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;               1456 02406000
*         RFY (GPR01P,GPR13P) RSTD;                                1457 02407000
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       1458 02408000
*                                                                  1458 02409000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             02410000
*         RFY GPR13P UNRSTD;                                       1459 02411000
*         GPR04P=STRTLCCW;                                         1460 02412000
         LR    GPR04P,STRTLCCW                                     1460 02413000
*         DCL 1 FMZA0290 FIXED(31) STATIC LOCAL,                   1461 02414000
*             2 * FIXED(8) INIT(64),                               1461 02415000
*             2 * PTR(24) INIT(0);                                 1461 02416000
*         GPR03F = FMZA0290;                                       1462 02417000
*                                                                  1462 02418000
         L     GPR03F,FMZA0290                                     1462 02419000
*            DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */         1463 02420000
*              GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/         1464 02421000
         L     GPR15P,ATCLCPTR                                     1464 02422000
*            END; /*LISATCVT*/                                     1465 02423000
*         GPR02P = GPR15P->ATCBPDA;                                1466 02424000
         L     GPR02P,ATCBPDA(,GPR15P)                             1466 02425000
*         GPR15P=GPR15P->ATCSMRS;                                  1467 02426000
         L     GPR15P,ATCSMRS(,GPR15P)                             1467 02427000
*         DCL FMZL0290 BASED(GPR15P) LABEL VALUERANGE(*);          1468 02428000
*         CALL FMZL0290;                                           1469 02429000
         BALR  @14,@15                                             1469 02430000
*         RFY GPR13P RSTD;                                         1470 02431000
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       1471 02432000
*                                                                  1471 02433000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              02434000
*         RFY (GPR01P,GPR13P) UNRSTD;                              1472 02435000
*            RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;             1473 02436000
*            END ;/**********END OF RELSTORE EXPANSION**********/  /* F 02437000
*EE LAST ADD. LCCW     */                                          1474 02438000
*           END;                       /* END-LCCW IS A TIC          */ 02439000
*        ELSE                                                      1476 02440000
*            /*RELSTORE BUFADDR(FSBLCPB)*/                         1476 02441000
*             DO;                                                  1476 02442000
         B     @RC01418                                            1476 02443000
@RF01418 DS    0H                                                  1477 02444000
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  1477 02445000
*/* END OF ATBUILD */                                              1477 02446000
*            RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;               1477 02447000
*         RFY (GPR01P,GPR13P) RSTD;                                1478 02448000
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       1479 02449000
*                                                                  1479 02450000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             02451000
*         RFY GPR13P UNRSTD;                                       1480 02452000
*         GPR04P=FSBLCPB;                                          1481 02453000
         L     GPR04P,FSBLCPB(,FSBPTR)                             1481 02454000
*         DCL 1 FMZA0306 FIXED(31) STATIC LOCAL,                   1482 02455000
*             2 * FIXED(8) INIT(64),                               1482 02456000
*             2 * PTR(24) INIT(0);                                 1482 02457000
*         GPR03F = FMZA0306;                                       1483 02458000
*                                                                  1483 02459000
         L     GPR03F,FMZA0306                                     1483 02460000
*            DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */         1484 02461000
*              GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/         1485 02462000
         L     GPR15P,ATCLCPTR                                     1485 02463000
*            END; /*LISATCVT*/                                     1486 02464000
*         GPR02P = GPR15P->ATCBPDA;                                1487 02465000
         L     GPR02P,ATCBPDA(,GPR15P)                             1487 02466000
*         GPR15P=GPR15P->ATCSMRS;                                  1488 02467000
         L     GPR15P,ATCSMRS(,GPR15P)                             1488 02468000
*         DCL FMZL0306 BASED(GPR15P) LABEL VALUERANGE(*);          1489 02469000
*         CALL FMZL0306;                                           1490 02470000
         BALR  @14,@15                                             1490 02471000
*         RFY GPR13P RSTD;                                         1491 02472000
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       1492 02473000
*                                                                  1492 02474000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              02475000
*         RFY (GPR01P,GPR13P) UNRSTD;                              1493 02476000
*            RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;             1494 02477000
*            END ;/**********END OF RELSTORE EXPANSION**********/   /*  02478000
*REE THE LCPB           */                                         1495 02479000
*        END;                              /*END- FSBELI IS ON       */ 02480000
*     ELSE;                                                        1497 02481000
