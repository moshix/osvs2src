*TO PSS     */                                                     0491 00063000
*PRGEPROC: PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);         0492 00064000
         B     @PB00002                                            0492 00065000
PRGEPROC DS    0H                                                  0493 00066000
*/********************************************************************/ 00067000
*/*                                                                  */ 00068000
*/* ROUTINE NAME :  PRGEPROC - PROCESS PURGE FEEDBACK                */ 00069000
*/*                                                                  */ 00070000
*/* FUNCTION :                                                       */ 00071000
*/*    PURGEPROC PROCESSES THE FEEDBACK OF A PURGE REQUEST           */ 00072000
*/*   --IF THE LCPB IS FOR A BASIC I/O REQUEST, ITS RPL,IF ONE       */ 00073000
*/*    EXIST, IS POSTED COMPLETE. IF NO RPL EXISTS THE DATA IS       */ 00074000
*/*    QUEUED ONTO THE DATA QUEUE IF THIS IS A VARY PURGE.           */ 00075000
*/*   --IF THE LCPB IS A PURGE LCPB BUT IS NOT THE LAST PURGE        */ 00076000
*/*    LCPB COMING FROM TPIOS (DCLSRESW SWITCH ON), THE FSB AND      */ 00077000
*/*    LCPB ARE FREED UP AND CONTROL RETURNED TO MAINLINE            */ 00078000
*/*   --IF THE LCPB IS THE FINAL PURGE LCPB(DCLRESW SWITCH OFF)      */ 00079000
*/*    THE PURGE OPERATION AND ELEMENTS ON THE PURGE RESET           */ 00080000
*/*    QUEUE ARE POSTED COMPLETE.                                    */ 00081000
*/*                                                                  */ 00082000
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */ 00083000
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */ 00084000
*/*                                                                  */ 00085000
*/* OUTPUT : PURGE FEEDBACK PROCESSED                                */ 00086000
*/*                                                                  */ 00087000
*/* SUBROUTINES CALLED : SETRPL   - SET UP RPL FOR POSTING           */ 00088000
*/*                      PSTRPL  - POST RPLS ON THE FMCB WAIT QUEUE  */ 00089000
*/*                      ISTAICPT - POST USER RPL COMPLETE           */ 00090000
*/*                      POSTELEM - POST ELEMENTS ON LCPB RESET Q    */ 00091000
*/*                      PRGEPOST - POST PURGE OPERATION COMPLETE    */ 00092000
*/*                      FREELCPB - FREE LCPB AND LCCW AREAS         */ 00093000
*/*                      FREEFSB - FREE FSB/FDB CHAIN BUFFERS        */ 00094000
*/*                      XFRFSB  - QUEUE PAGEABLE FSB TO DATA QUE    */ 00095000
*/*                                                                  */ 00096000
*/* MACROS USED :  TPQUE,RELSTORE                                    */ 00097000
*/*                                                                  */ 00098000
*/********************************************************************/ 00099000
*     CRRRTN1=RTNREG;                      /* SAVE RETURN REG         * 00100000
*                                                                  0493 00101000
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0493 00102000
*      DO; /* LISATCVT CVTBASE(PAGEABLE,(CRRATCVT)) */             0494 00103000
*        CRRATCVT = ATCLCPTR; /*SET UP ATCVT POINTER*/             0495 00104000
         L     @15,ATCLCPTR                                        0495 00105000
         ST    @15,CRRATCVT(,CRRPTR)                               0495 00106000
*      END; /*LISATCVT*/  /* GET VTAM CVT */                       0496 00107000
*     RFY ISTATCVT BASED(CRRATCVT);        /* COVER VTAM CVT          * 00108000
*     IF  FSBLCPB^=ZERO THEN                                       0498 00109000
         L     @15,FSBLCPB(,FSBPTR)                                0498 00110000
         LTR   @15,@15                                             0498 00111000
         BZ    @RF00498                                            0498 00112000
*       DO;                                /* IF FSB HAS AN LCPB      * 00113000
*       IF FSBLCPB->DCLRQTP=DCLPURGE THEN                          0500 00114000
         CLI   DCLRQTP(@15),X'15'                                  0500 00115000
         BNE   @RF00500                                            0500 00116000
*         DO;                              /* YES, IF LCPB FOR PURGE  * 00117000
*         IF FSBLCPB->DCLSRESW=OFF THEN      /* YES,IF SPEC RESET OFF*/ 00118000
         TM    DCLSRESW(@15),B'10000000'                           0502 00119000
         BNZ   @RF00502                                            0502 00120000
*            DO;                                                   0503 00121000
*            CALL PSTRPL;                   /* POST RPLS ON THE WAIT Q* 00122000
         BAL   @14,PSTRPL                                          0504 00123000
*            CRRARPL=FSBLCPB->DCLRPLA;      /* GET REQUESTOR LCPB */    00124000
         L     @15,FSBLCPB(,FSBPTR)                                0505 00125000
         L     @15,DCLRPLA(,@15)                                   0505 00126000
         ST    @15,CRRARPL(,CRRPTR)                                0505 00127000
*            RFY ISTDCLCP BASED(FSBLCPB);  /* COVER DCLCPB            * 00128000
*            RFY ISTLCPB BASED(ADDR(DCLLCPB));                     0507 00129000
*            CALL POSTELEM;                 /* POST ALL RPLS LCPB QUE * 00130000
         BAL   @14,POSTELEM                                        0508 00131000
*            IF CRRSW0=ON THEN           /* A NON-PURGE LCPB FOUND   *  00132000
*                                         * ON THE LCPB RESET QUE    */ 00133000
*                                                                  0509 00134000
         TM    CRRSW0(CRRPTR),B'10000000'                          0509 00135000
         BNO   @RF00509                                            0509 00136000
*/*  TPQUE (CRRFSB,FMCPAB2) NORM */                                0510 00137000
*                                                                  0510 00138000
*               DO;                                                0510 00139000
*               RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;     0511 00140000
*               GEN REFS(GPR01P) SETS(RPHWORK)                     0512 00141000
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0512 00142000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00143000
*               GPR00P=0;                                          0513 00144000
         SLR   GPR00P,GPR00P                                       0513 00145000
*               GPR15P=ADDR(FMCPAB2); /*HOLD PAB PTR*/             0514 00146000
         L     GPR15P,CRRFMCB(,CRRPTR)                             0514 00147000
         LA    GPR15P,FMCPAB2(,GPR15P)                             0514 00148000
*               GPR04P=CRRFSB; /*WORK ELE PTR*/                    0515 00149000
         L     GPR04P,CRRFSB(,CRRPTR)                              0515 00150000
*               GPR05P=GPR15P; /*PASS PAB PTR*/                    0516 00151000
         LR    GPR05P,GPR15P                                       0516 00152000
*               CALL ATCLCPTR->ATCAP37->ISTAPC37;                  0517 00153000
         L     @10,ATCLCPTR                                        0517 00154000
         L     @15,ATCAP37(,@10)                                   0517 00155000
         BALR  @14,@15                                             0517 00156000
*               GEN(NOPR  0)NOSETS NOREFS;                         0518 00157000
         NOPR  0                                                        00158000
*               RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;   0519 00159000
*               END; /**** END OF TPQUE GENERATION ****/  /* QUE FSB TO 00160000
*CLIP     */                                                       0520 00161000
*            ELSE DO;                                              0521 00162000
         B     @RC00509                                            0521 00163000
@RF00509 DS    0H                                                  0522 00164000
*            FSBPTR=CRRFSB;                /* GET ORIGINAL FSB PTR    * 00165000
         L     FSBPTR,CRRFSB(,CRRPTR)                              0522 00166000
*            CRRLCPB=FSBLCPB;              /* SAVE ORIGINAL LCPB  */    00167000
         L     @15,FSBLCPB(,FSBPTR)                                0523 00168000
         ST    @15,CRRLCPB(,CRRPTR)                                0523 00169000
*            FSBLCPB=CRRARPL;              /* POINT TO REQUSTOR LCPB */ 00170000
         L     @15,CRRARPL(,CRRPTR)                                0524 00171000
         ST    @15,FSBLCPB(,FSBPTR)                                0524 00172000
*            CALL PRGEPOST;                /* POST PURGE COMPLETE     * 00173000
         BAL   @14,PRGEPOST                                        0525 00174000
*             /*RELSTORE BUFADDR(CRRLCPB)CLEAR(NO)*/               0526 00175000
*              DO;                                                 0526 00176000
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  0527 00177000
*/* END OF ATBUILD */                                              0527 00178000
*             RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;              0527 00179000
*         RFY (GPR01P,GPR13P) RSTD;                                0528 00180000
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       0529 00181000
*                                                                  0529 00182000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             00183000
*         RFY GPR13P UNRSTD;                                       0530 00184000
*         GPR04P=CRRLCPB;                                          0531 00185000
         L     GPR04P,CRRLCPB(,CRRPTR)                             0531 00186000
