*/*                                                                  */ 01887000
*/* ROUTINE NAME : SETRPL - SET UP RPL FOR POSTING                   */ 01888000
*/*                                                                  */ 01889000
*/* FUNCTION :                                                       */ 01890000
*/*    IF A VARY SOFT PURGE IS IN PROGRESS THE RPL IS POSTED         */ 01891000
*/*    COMPLETE IF NO OTHER RPL IS CURRENTLY BEING HELD IN THE       */ 01892000
*/*    FMCB FOR POSTING. IF THERE IS ONE BEING HELD, IT IS POSTED    */ 01893000
*/*    AND THE CURRENT RPL IS THEN HELD. IF THIS IS NOT VARY SOFT    */ 01894000
*/*    PURGE, THE CURRENT RPL IS POSTED WITH PURGE RETURN CODES      *. 01895000
*/*                                                                  */ 01896000
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */ 01897000
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */ 01898000
*/*                                                                  */ 01899000
*/* OUTPUT :  THE OLDEST RPL IS POSTED COMPLETE                      */ 01900000
*/*                                                                  */ 01901000
*/* SUBROUTINES CALLED :                                             */ 01902000
*/*                      ISTAICPT - POST USER RPL COMPLETE           */ 01903000
*/*                                                                  */ 01904000
*/*  MACROS USED :  NONE                                             */ 01905000
*/*                                                                  */ 01906000
*/********************************************************************/ 01907000
*SETRPL:  PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);               1260 01908000
SETRPL   DS    0H                                                  1261 01909000
*     CRRRTN8=RTNREG;                      /* SAVE RETURN ADDRESS     * 01910000
         ST    RTNREG,CRRRTN8(,CRRPTR)                             1261 01911000
*    IF FMCVSP1=ON                            /* IF A VARY SOFT PURGE * 01912000
*    THEN DO;                               /*CHECK FOR AN OLDER RPL */ 01913000
         L     @15,CRRFMCB(,CRRPTR)                                1262 01914000
         TM    FMCVSP1(@15),B'00000001'                            1262 01915000
         BNO   @RF01262                                            1262 01916000
*     FMCVSP2=ON;                          /*SET REQUEST SOFT PURGED  * 01917000
         OI    FMCVSP2(@15),B'10000000'                            1264 01918000
*     IF FMCRPLH=ZERO                      /*IF NO RPL IN FMCB        * 01919000
*        THEN FMCRPLH=RPLPTR;  /* PUT THIS RPL IN FMCB                * 01920000
         L     @10,FMCRPLH(,@15)                                   1265 01921000
         LTR   @10,@10                                             1265 01922000
         BNZ   @RF01265                                            1265 01923000
         ST    RPLPTR,FMCRPLH(,@15)                                1266 01924000
*     ELSE DO;                            /* IF FMCB HAS AN RPL       * 01925000
         B     @RC01265                                            1267 01926000
@RF01265 DS    0H                                                  1268 01927000
*        RFY IFGRPL BASED(RPLPTR);        /* COVER THIS RPL          */ 01928000
*        WRKREGD=RPLPTR;                   /* SAVE CURRENT RPL        * 01929000
         LR    WRKREGD,RPLPTR                                      1269 01930000
*        RPLPTR=FMCRPLH;                   /* GET HELD RPL            * 01931000
         L     @15,CRRFMCB(,CRRPTR)                                1270 01932000
         L     RPLPTR,FMCRPLH(,@15)                                1270 01933000
*        FMCRPLH=WRKREGD;                  /* HOLD THIS CURRENT RPL   * 01934000
         ST    WRKREGD,FMCRPLH(,@15)                               1271 01935000
*        IF FMCPRMRC='81'X THEN RPLRTNCD='18'X;/*            @XM7981K*/ 01936000
         CLI   FMCPRMRC(@15),X'81'                                 1272 01937000
         BNE   @RF01272                                            1272 01938000
         MVI   RPLRTNCD(RPLPTR),X'18'                              1273 01939000
*         ELSE                             /*                @XM7981K*/ 01940000
*        RPLRTNCD='1C'X;                   /* SET RTN CODE TO PURGE   * 01941000
         B     @RC01272                                            1274 01942000
@RF01272 MVI   RPLRTNCD(RPLPTR),X'1C'                              1274 01943000
*        RPLFDB2=FMCPRMRC;                 /*  MINOR CODE FROM FMCB   * 01944000
@RC01272 L     @15,CRRFMCB(,CRRPTR)                                1275 01945000
         IC    @10,FMCPRMRC(,@15)                                  1275 01946000
         STC   @10,RPLFDB2(,RPLPTR)                                1275 01947000
*        RPLUSFLD=FMCUSFLD;                /* GET USER INFO   @XM09597* 01948000
         MVC   RPLUSFLD(4,RPLPTR),FMCUSFLD(@15)                    1276 01949000
*        RPHWEA=RPLPTR;                   /* SET UP RPH FOR POST    */  01950000
         ST    RPLPTR,RPHWEA(,RPHPTR)                              1277 01951000
*        CALL ATCAPOST;                    /* CALL USER POST RTN      * 01952000
         L     @10,CRRATCVT(,CRRPTR)                               1278 01953000
         L     @15,ATCAPOST(,@10)                                  1278 01954000
         BALR  @14,@15                                             1278 01955000
*        END;                                                      1279 01956000
*       END;                                     /* END SOFT PURGE   */ 01957000
*    ELSE DO;                                   /*NOT SOFT PURGE    */  01958000
         B     @RC01262                                            1281 01959000
@RF01262 DS    0H                                                  1282 01960000
*      RPLUSFLD=FMCUSFLD;                  /* GET USER INFO   @XM09597* 01961000
         L     @15,CRRFMCB(,CRRPTR)                                1282 01962000
         MVC   RPLUSFLD(4,RPLPTR),FMCUSFLD(@15)                    1282 01963000
*      IF RPLFDB2='05'X THEN ;            /* USER ABEND-DON'T SET CODE* 01964000
         CLI   RPLFDB2(RPLPTR),5                                   1283 01965000
         BE    @RT01283                                            1283 01966000
*      ELSE DO;                                                    1285 01967000
*        IF FMCPRMRC='81'X THEN RPLRTNCD='18'X;/*            @XM7981K*/ 01968000
         L     @15,CRRFMCB(,CRRPTR)                                1286 01969000
         CLI   FMCPRMRC(@15),X'81'                                 1286 01970000
         BNE   @RF01286                                            1286 01971000
         MVI   RPLRTNCD(RPLPTR),X'18'                              1287 01972000
*         ELSE                             /*                @XM7981K*/ 01973000
*       RPLRTNCD='1C'X;                    /* SET PURGE RETURN CODE   * 01974000
         B     @RC01286                                            1288 01975000
@RF01286 MVI   RPLRTNCD(RPLPTR),X'1C'                              1288 01976000
*       RPLFDB2=FMCPRMRC;                  /* COPY REASON CODE        * 01977000
@RC01286 L     @15,CRRFMCB(,CRRPTR)                                1289 01978000
         IC    @10,FMCPRMRC(,@15)                                  1289 01979000
         STC   @10,RPLFDB2(,RPLPTR)                                1289 01980000
*       END;                                                       1290 01981000
*     RPHWEA=RPLPTR;                     /* PUT RPL ADDR TO RPH     */  01982000
@RT01283 ST    RPLPTR,RPHWEA(,RPHPTR)                              1291 01983000
*     CALL ATCAPOST;                     /* CALL USER POST ROUTINE  */  01984000
         L     @10,CRRATCVT(,CRRPTR)                               1292 01985000
         L     @15,ATCAPOST(,@10)                                  1292 01986000
         BALR  @14,@15                                             1292 01987000
*     END;                               /* END -NOT SOFT PURGE     */  01988000
*     RTNREG=CRRRTN8;                      /* RESTORE RETURN ADDRESS  * 01989000
@RC01262 L     RTNREG,CRRRTN8(,CRRPTR)                             1294 01990000
*     RETURN;                                                      1295 01991000
@EL00009 DS    0H                                                  1295 01992000
@EF00009 DS    0H                                                  1295 01993000
@ER00009 BR    @14                                                 1295 01994000
*END  SETRPL;                                                      1296 01995000
*/********************************************************************/ 01996000
*/*                                                                  */ 01997000
*/* ROUTINE NAME :  POSTRPL - POST RPL COMPLETE OR HOLD IT.          */ 01998000
*/*                                                                  */ 01999000
*/* FUNCTION :                                                       */ 02000000
*/*    POSTRPL POSTS THE RPL COMPLETE UNLESS A VARY SOFT PURGE       */ 02001000
*/*    IS IN PROGRESS, IN WHICH CASE THE RPL ADDRESS IS MOVED        */ 02002000
*/*    TO THE FMCB HOLD QUEUE                                        */ 02003000
*/*                                                                  */ 02004000
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */ 02005000
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */ 02006000
*/*                                                                  */ 02007000
*/* OUTPUT :  RPL IS POSTED OR PUT IN THE FMCB HOLD QUEUE            */ 02008000
*/*                                                                  */ 02009000
*/* SUBROUTINES CALLED :                                             */ 02010000
*/*                      ISTAICPT - USER POST ROUTINE                */ 02011000
*/*                                                                  */ 02012000
*/*  MACROS USED : TPDEQ                                             */ 02013000
*/*                                                                  */ 02014000
*/********************************************************************/ 02015000
*POSTRPL:  PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);              1297 02016000
POSTRPL  DS    0H                                                  1298 02017000
*     CRRRTN9=RTNREG;                      /* SAVE RETURN ADDRESS     * 02018000
         ST    RTNREG,CRRRTN9(,CRRPTR)                             1298 02019000
*     RFY IFGRPL BASED(WRKREGD);           /* WRKREGD HAS CURR RPL    * 02020000
*     IF FMCPRMRC='05'X                    /* APPLICATION ABEND SPEC  * 02021000
*     THEN DO;                             /*POST THIS RPL            * 02022000
         L     @15,CRRFMCB(,CRRPTR)                                1300 02023000
         CLI   FMCPRMRC(@15),X'05'                                 1300 02024000
         BNE   @RF01300                                            1300 02025000
*        RPLUSFLD=FMCUSFLD;                /* GET USER INFO   @XM09597* 02026000
         MVC   RPLUSFLD(4,WRKREGD),FMCUSFLD(@15)                   1302 02027000
*        RPHWEA=WRKREGD;                   /* SET UP RPLH FOR POST    * 02028000
         ST    WRKREGD,RPHWEA(,RPHPTR)                             1303 02029000
*        CALL ATCAPOST;                    /* CALL USER POST ROUTINE  * 02030000
         L     @10,CRRATCVT(,CRRPTR)                               1304 02031000
         L     @15,ATCAPOST(,@10)                                  1304 02032000
         BALR  @14,@15                                             1304 02033000
*        END;                              /* END-APPL. ABEND         * 02034000
*     ELSE DO;                             /* APPL ABEND NOT SPECIFIED* 02035000
         B     @RC01300                                            1306 02036000
@RF01300 DS    0H                                                  1307 02037000
*        IF FMCVSP2=ON                     /* RPL WAS SOFT PURGED     * 02038000
*        THEN DO;                          /*  SAVE IT-POST OLD ONE   * 02039000
         L     @15,CRRFMCB(,CRRPTR)                                1307 02040000
         TM    FMCVSP2(@15),B'10000000'                            1307 02041000
         BNO   @RF01307                                            1307 02042000
*          RPLPTR=FMCRPLH;                 /* GET ADDR SAVED RPL      * 02043000
         L     RPLPTR,FMCRPLH(,@15)                                1309 02044000
*          FMCRPLH=WRKREGD;                /*  SAVE THE CURR ONE      * 02045000
         ST    WRKREGD,FMCRPLH(,@15)                               1310 02046000
*          RPLPTR->RPLRTNCD='1C'X;         /* SET RTN CODE-NEW RPL    * 02047000
         MVI   RPLRTNCD(RPLPTR),X'1C'                              1311 02048000
*          RPLPTR->RPLFDB2=FMCPRMRC;       /* AND THE REASON CODE     * 02049000
         IC    @10,FMCPRMRC(,@15)                                  1312 02050000
         STC   @10,RPLFDB2(,RPLPTR)                                1312 02051000
*          RPLPTR->RPLUSFLD=FMCUSFLD;      /* GET USER INFO   @XM09579* 02052000
         MVC   RPLUSFLD(4,RPLPTR),FMCUSFLD(@15)                    1313 02053000
*          RPHWEA=RPLPTR;                  /* SET RPH FOR POSTING     * 02054000
         ST    RPLPTR,RPHWEA(,RPHPTR)                              1314 02055000
*          CALL ATCAPOST;                  /* CALL USER POST          * 02056000
         L     @10,CRRATCVT(,CRRPTR)                               1315 02057000
         L     @15,ATCAPOST(,@10)                                  1315 02058000
         BALR  @14,@15                                             1315 02059000
*          END;                            /* END-RPL SOFT PURGED     * 02060000
*        ELSE DO;                          /* RPL NOT SOFT PURGED     * 02061000
         B     @RC01307                                            1317 02062000
@RF01307 DS    0H                                                  1318 02063000
*          IF FMCVSP1=ON                   /* SOFT PRG IN PROGRESS    * 02064000
*          THEN DO;                        /* DON'T POST COMPLETE     * 02065000
         L     @15,CRRFMCB(,CRRPTR)                                1318 02066000
         TM    FMCVSP1(@15),B'00000001'                            1318 02067000
         BNO   @RF01318                                            1318 02068000
*            FMCRPLH=WRKREGD;              /* HOLD THIS RPL           * 02069000
         ST    WRKREGD,FMCRPLH(,@15)                               1320 02070000
*            FMCVSP2=ON;                   /* SOFT PURGED FLAG        * 02071000
         OI    FMCVSP2(@15),B'10000000'                            1321 02072000
*            END;                          /*END-SOFT PRG IP PROG     * 02073000
*          ELSE DO;                        /* NO SOFT PRG IN PROG     * 02074000
         B     @RC01318                                            1323 02075000
@RF01318 DS    0H                                                  1324 02076000
*            RFY IFGRPL BASED(WRKREGD);    /* COVER CURRENT RPL       * 02077000
*            RPLRTNCD='1C'X;               /* SET RTN CODE TO PURGE   * 02078000
         MVI   RPLRTNCD(WRKREGD),X'1C'                             1325 02079000
*            RPLFDB2=FMCPRMRC;             /* SET REASON CODE         * 02080000
         L     @15,CRRFMCB(,CRRPTR)                                1326 02081000
         IC    @10,FMCPRMRC(,@15)                                  1326 02082000
         STC   @10,RPLFDB2(,WRKREGD)                               1326 02083000
*            RPLUSFLD=FMCUSFLD;            /* GET USER INFO   @XM09597* 02084000
         MVC   RPLUSFLD(4,WRKREGD),FMCUSFLD(@15)                   1327 02085000
*            RPHWEA=WRKREGD;               /* PUT RPL ADDR IN RPH     * 02086000
         ST    WRKREGD,RPHWEA(,RPHPTR)                             1328 02087000
*            CALL ATCAPOST;                /* CALL USER POST RTN      * 02088000
         L     @10,CRRATCVT(,CRRPTR)                               1329 02089000
         L     @15,ATCAPOST(,@10)                                  1329 02090000
         BALR  @14,@15                                             1329 02091000
*            END;                          /* END-NO SOFT PRG IN PROG*/ 02092000
*          END;                            /* END-RPL NOT S PRGED     * 02093000
@RC01318 DS    0H                                                  1332 02094000
*        END;                              /* END NO APPL ABEND       * 02095000
@RC01307 DS    0H                                                  1333 02096000
*     RFY ISTDCLCP BASED(CURRPTRB);        /* COVER CURRENT LCPB      * 02097000
*                                                                  1333 02098000
@RC01300 DS    0H                                                  1334 02099000
*/* TPDEQ (,LCPRSTQH) RETPTR(WRKREGD) */                           1334 02100000
*      DO; /* BEGIN TPDEQ CODE GENERATION */                       1334 02101000
*      RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                     1335 02102000
*                                                                  1335 02103000
