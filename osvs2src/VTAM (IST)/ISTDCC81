         L     GPR15P,ATCLCPTR                                     0558 00236000
*             END; /*LISATCVT*/                                    0559 00237000
*         GPR02P = GPR15P->ATCBPDA;                                0560 00238000
         L     GPR02P,ATCBPDA(,GPR15P)                             0560 00239000
*         GPR15P=GPR15P->ATCSMRS;                                  0561 00240000
         L     GPR15P,ATCSMRS(,GPR15P)                             0561 00241000
*         DCL FMZL0042 BASED(GPR15P) LABEL VALUERANGE(*);          0562 00242000
*         CALL FMZL0042;                                           0563 00243000
         BALR  @14,@15                                             0563 00244000
*         RFY GPR13P RSTD;                                         0564 00245000
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       0565 00246000
*                                                                  0565 00247000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              00248000
*         RFY (GPR01P,GPR13P) UNRSTD;                              0566 00249000
*             RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;            0567 00250000
*             END ;/**********END OF RELSTORE EXPANSION**********/ 0568 00251000
*            CALL FREEFSB;                 /* FREE FSB AND FDBS       * 00252000
         BAL   @14,FREEFSB                                         0569 00253000
*            END;                          /* END SP RESET NOT ON */    00254000
*         END;                             /* END-IF PURGE LCPB       * 00255000
*         ELSE DO;                        /* LCPB IS NOT FOR PURGE   */ 00256000
         B     @RC00500                                            0572 00257000
@RF00500 DS    0H                                                  0573 00258000
*           RPLPTR=DCLRPLA;              /* SAVE RPL POINTER         */ 00259000
         L     @15,FSBLCPB(,FSBPTR)                                0573 00260000
         L     RPLPTR,DCLRPLA(,@15)                                0573 00261000
*           CALL FREELCPB;                /* FREE LCPB AND LCCWS*/ 0574 00262000
         BAL   @14,FREELCPB                                        0574 00263000
*           IF RPLPTR^=ZERO THEN                                   0575 00264000
         LTR   RPLPTR,RPLPTR                                       0575 00265000
         BZ    @RF00575                                            0575 00266000
*            DO;                           /* IF LCPB HAS AN RPL      * 00267000
*            RFY IFGRPL BASED(RPLPTR);                             0577 00268000
*            CALL FREEFSB;                 /* FREE FSB AND FDBS       * 00269000
         BAL   @14,FREEFSB                                         0578 00270000
*            IF FMCPRMRC^='05'X THEN      /* IF NO APPLICATION ABEND *  00271000
*                                           * WE'RE THROUGH           * 00272000
         L     @15,CRRFMCB(,CRRPTR)                                0579 00273000
         CLI   FMCPRMRC(@15),X'05'                                 0579 00274000
         BE    @RF00579                                            0579 00275000
*               CALL SETRPL;               /* SET UP RPL FOR POST     * 00276000
         BAL   @14,SETRPL                                          0580 00277000
*            ELSE                          /* IF ABEND SPECIFIED     */ 00278000
*               DO;                        /* MUSTS GO TO USER POST   * 00279000
         B     @RC00579                                            0581 00280000
@RF00579 DS    0H                                                  0582 00281000
*               RPHWEA=RPLPTR;             /* RPL ADDR TO RPH         * 00282000
         ST    RPLPTR,RPHWEA(,RPHPTR)                              0582 00283000
*               CALL ATCAPOST;             /* CALL USER POST ROUTINE */ 00284000
         L     @10,CRRATCVT(,CRRPTR)                               0583 00285000
         L     @15,ATCAPOST(,@10)                                  0583 00286000
         BALR  @14,@15                                             0583 00287000
*               END;                       /* END IF NO ABEND SPEC.   * 00288000
*            END;                          /* END IF LCPB HAS   RPL  */ 00289000
*         ELSE DO;                         /* LCPB HAS NO RPL POINTER * 00290000
         B     @RC00575                                            0586 00291000
@RF00575 DS    0H                                                  0587 00292000
*            IF FMCPRMRC^= '05'X  &       /* IF VARY SPECIFIED */  0587 00293000
*              FMCPRMRC^= '06'X THEN                               0587 00294000
         L     @15,CRRFMCB(,CRRPTR)                                0587 00295000
         CLI   FMCPRMRC(@15),X'05'                                 0587 00296000
         BE    @RF00587                                            0587 00297000
         CLI   FMCPRMRC(@15),X'06'                                 0587 00298000
         BE    @RF00587                                            0587 00299000
*         DO;                                                      0588 00300000
*         FSBSRCAT=FRPURGE;                /* PURGE CODE TO FSB    */   00301000
         MVI   FSBSRCAT(FSBPTR),X'1C'                              0589 00302000
*         FSBMNRCD=FMCPRMRC;               /* MINOR RET CODE IS PURGE*/ 00303000
         MVC   FSBMNRCD(1,FSBPTR),FMCPRMRC(@15)                    0590 00304000
*         CALL XFRFSB;                   /* QUE PAGEABLE FSB TO DATA Q* 00305000
         BAL   @14,XFRFSB                                          0591 00306000
*         END;                             /* END VARY SPECIFIED */     00307000
*       ELSE CALL FREEFSB;                /* IF NO VARY FREE FSB/FDBS*/ 00308000
         B     @RC00587                                            0593 00309000
