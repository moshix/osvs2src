*/********************************************************************/ 02154000
*/*                                                                  */ 02155000
*/* ROUTINE NAME : PSTRPL - POST RPLS ON THE FMCB WAIT QUEUE         */ 02156000
*/*                                                                  */ 02157000
*/* FUNCTION :                                                       */ 02158000
*/*    PSTRPL DEQUEUES EACH RPL FROM THE FMCB WAIT QUEUE AND CALLS   */ 02159000
*/*    SETRPL TO HANDLE THE ACTUAL POSTING OF EACH ONE.              */ 02160000
*/*                                                                  */ 02161000
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */ 02162000
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */ 02163000
*/*                                                                  */ 02164000
*/* OUTPUT :   ALL RPLS ON THE WAIT QUEUE POSTED                     */ 02165000
*/*                                                                  */ 02166000
*/* SUBROUTINES CALLED :                                             */ 02167000
*/*                      SETRPL   - SET UP RPL FOR POSTING           */ 02168000
*/*                                                                  */ 02169000
*/*  MACROS USED :  TPDEQ                                            */ 02170000
*/*                                                                  */ 02171000
*/********************************************************************/ 02172000
*PSTRPL: PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);                1359 02173000
PSTRPL   DS    0H                                                  1360 02174000
*     CRRRTN10=RTNREG;                     /* SAVE RETURN ADDRESS    */ 02175000
*                                                                  1360 02176000
         ST    RTNREG,CRRRTN10(,CRRPTR)                            1360 02177000
*/* TPDEQ (,FMCRPLA) RETPTR(RPLPTR) */                             1361 02178000
*      DO; /* BEGIN TPDEQ CODE GENERATION */                       1361 02179000
*      RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                     1362 02180000
*                                                                  1362 02181000
*      GPR15P=ADDR(FMCRPLA); /*ADDRESS QUEUE POINTER */            1363 02182000
         L     GPR15P,CRRFMCB(,CRRPTR)                             1363 02183000
         LA    GPR15P,FMCRPLA(,GPR15P)                             1363 02184000
*      GPR14P=GPR15P->PABWEQA|FMZCA108;                            1364 02185000
         L     @10,@CF01149                                        1364 02186000
         L     GPR14P,PABWEQA(,GPR15P)                             1364 02187000
         OR    GPR14P,@10                                          1364 02188000
*      IF GPR14P=FMZCA108 THEN;                                    1365 02189000
         CLR   GPR14P,@10                                          1365 02190000
         BE    @RT01365                                            1365 02191000
*      ELSE DO;                                                    1367 02192000
*FMZL1266:;                                                        1368 02193000
FMZL1266 DS    0H                                                  1369 02194000
*        DO WHILE(GPR14P->WKECHAIN^=FMZCA108);                     1369 02195000
         B     @DE01369                                            1369 02196000
@DL01369 DS    0H                                                  1370 02197000
*          GPR15P=ADDR(GPR14P->WKECHAIN);                          1370 02198000
         LA    GPR15P,WKECHAIN(,GPR14P)                            1370 02199000
*          GPR14P=GPR15P->PABWEQA;                                 1371 02200000
         L     GPR14P,PABWEQA(,GPR15P)                             1371 02201000
*        END;                                                      1372 02202000
@DE01369 L     @10,@CF01149                                        1372 02203000
         CL    @10,WKECHAIN(,GPR14P)                               1372 02204000
         BNE   @DL01369                                            1372 02205000
*        GPR00P=FMZCA108;                                          1373 02206000
         LR    GPR00P,@10                                          1373 02207000
*        CS(GPR14P,GPR00P,GPR15P->PABWEQA);                        1374 02208000
         CS    GPR14P,@00,PABWEQA(GPR15P)                          1374 02209000
*        BC(7,FMZL1266);                                           1375 02210000
         BC    7,FMZL1266                                          1375 02211000
*        GPR14P->WKECHAIN=0;                                       1376 02212000
         SLR   @10,@10                                             1376 02213000
         ST    @10,WKECHAIN(,GPR14P)                               1376 02214000
*      END;                                                        1377 02215000
*ISTA0266:;                                                        1378 02216000
@RT01365 DS    0H                                                  1378 02217000
ISTA0266 DS    0H                                                  1379 02218000
*      GPR14P=GPR14P&FMZCB108;                                     1379 02219000
         N     GPR14P,@CF01147                                     1379 02220000
*      RPLPTR=GPR14P;                                              1380 02221000
         LR    RPLPTR,GPR14P                                       1380 02222000
*      RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;                   1381 02223000
*      END; /* END TPDEQ GENERATION */                             1382 02224000
*     DO WHILE RPLPTR^=ZERO & RPLPTR^=GATE;                        1383 02225000
         B     @DE01383                                            1383 02226000
@DL01383 DS    0H                                                  1384 02227000
*        CALL SETRPL;                      /* POST THE RPL           */ 02228000
*                                                                  1384 02229000
         BAL   @14,SETRPL                                          1384 02230000
*/* TPDEQ (,FMCRPLA) RETPTR(RPLPTR) */                             1385 02231000
*         DO; /* BEGIN TPDEQ CODE GENERATION */                    1385 02232000
*         RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                  1386 02233000
*                                                                  1386 02234000
*         GPR15P=ADDR(FMCRPLA); /*ADDRESS QUEUE POINTER */         1387 02235000
         L     GPR15P,CRRFMCB(,CRRPTR)                             1387 02236000
         LA    GPR15P,FMCRPLA(,GPR15P)                             1387 02237000
*         GPR14P=GPR15P->PABWEQA|FMZCA108;                         1388 02238000
         L     @10,@CF01149                                        1388 02239000
         L     GPR14P,PABWEQA(,GPR15P)                             1388 02240000
         OR    GPR14P,@10                                          1388 02241000
*         IF GPR14P=FMZCA108 THEN;                                 1389 02242000
         CLR   GPR14P,@10                                          1389 02243000
         BE    @RT01389                                            1389 02244000
*         ELSE DO;                                                 1391 02245000
*FMZL1270:;                                                        1392 02246000
FMZL1270 DS    0H                                                  1393 02247000
*           DO WHILE(GPR14P->WKECHAIN^=FMZCA108);                  1393 02248000
         B     @DE01393                                            1393 02249000
@DL01393 DS    0H                                                  1394 02250000
*             GPR15P=ADDR(GPR14P->WKECHAIN);                       1394 02251000
         LA    GPR15P,WKECHAIN(,GPR14P)                            1394 02252000
*             GPR14P=GPR15P->PABWEQA;                              1395 02253000
         L     GPR14P,PABWEQA(,GPR15P)                             1395 02254000
*           END;                                                   1396 02255000
@DE01393 L     @10,@CF01149                                        1396 02256000
         CL    @10,WKECHAIN(,GPR14P)                               1396 02257000
         BNE   @DL01393                                            1396 02258000
*           GPR00P=FMZCA108;                                       1397 02259000
         LR    GPR00P,@10                                          1397 02260000
*           CS(GPR14P,GPR00P,GPR15P->PABWEQA);                     1398 02261000
         CS    GPR14P,@00,PABWEQA(GPR15P)                          1398 02262000
*           BC(7,FMZL1270);                                        1399 02263000
         BC    7,FMZL1270                                          1399 02264000
*           GPR14P->WKECHAIN=0;                                    1400 02265000
         SLR   @10,@10                                             1400 02266000
         ST    @10,WKECHAIN(,GPR14P)                               1400 02267000
*         END;                                                     1401 02268000
*ISTA0270:;                                                        1402 02269000
@RT01389 DS    0H                                                  1402 02270000
ISTA0270 DS    0H                                                  1403 02271000
*         GPR14P=GPR14P&FMZCB108;                                  1403 02272000
         N     GPR14P,@CF01147                                     1403 02273000
*         RPLPTR=GPR14P;                                           1404 02274000
