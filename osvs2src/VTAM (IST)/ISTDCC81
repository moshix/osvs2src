         AL    @15,RPHCRR(,RPHPTR)                                 1124 01572000
         LA    CRRPTR,CRRSAVE1(,@15)                               1124 01573000
*     CALL ATCDCC61;                   /* CALL MOVE ROUTINE          */ 01574000
         L     @10,CRRATCVT(,@15)                                  1125 01575000
         L     @15,ATCDCC61(,@10)                                  1125 01576000
         BALR  @14,@15                                             1125 01577000
*     CRRPTR=ADDR(CRRSPACE);                /* ORIGINAL CRR COVER    */ 01578000
         L     CRRPTR,RPHCRR(,RPHPTR)                              1126 01579000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        1126 01580000
*     RFY CRRSAREA BASED(CRRPTR);                                  1127 01581000
*     FSBPTR=CRRFSB;               /* GET NEW (PAGEABLE) FSB ADDRESS */ 01582000
         L     FSBPTR,CRRFSB(,CRRPTR)                              1128 01583000
*     RFY COUNTER RSTD;                                            1129 01584000
*     COUNTER=ZERO;                        /* TO COUNT FDS'IN CHAIN   * 01585000
