*/********************************************************************/ 00594000
*/*                                                                  */ 00595000
*/* ROUTINE NAME : FREEFSB - FREE FSB/FDB STORAGE                    */ 00596000
*/*                                                                  */ 00597000
*/* FUNCTION :                                                       */ 00598000
*/*    FREEFSB FREES ALL STORAGE FOR FSB AND FDB BUFFERS             */ 00599000
*/*                                                                  */ 00600000
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */ 00601000
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */ 00602000
*/*                                                                  */ 00603000
*/* OUTPUT :   FSB/FDB STORAGE FREED                                 */ 00604000
*/*                                                                  */ 00605000
*/* SUBROUTINES CALLED : NONE                                        */ 00606000
*/*                                                                  */ 00607000
*/*  MACROS USED :  RELSTORE                                         */ 00608000
*/*                                                                  */ 00609000
*/********************************************************************/ 00610000
*FREEFSB: PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);          0700 00611000
FREEFSB  DS    0H                                                  0701 00612000
*     CRRRTN3=RTNREG;                      /*SAVE RETURN REG          * 00613000
*                                                                  0701 00614000
         ST    RTNREG,CRRRTN3(,CRRPTR)                             0701 00615000
*   /********************************************************      0702 00616000
*    * THIS ROUTINE RELEASES THE STORAGE FOR THE FSB AND ALL*      0702 00617000
*    * FDBS IN THE CHAIN                                    *      0702 00618000
*    *******************************************************/      0702 00619000
*                                                                  0702 00620000
*         RFY FDBREG RSTD;                                         0702 00621000
*         FDBREG=ADDR(FSBFDB);             /* GET ADDR FIRST FDB      * 00622000
         LA    FDBREG,FSBFDB(,FSBPTR)                              0703 00623000
*         RFY ISTFDB BASED(FDBREG);        /* COVER FDB               * 00624000
*         FSBPTR=FDBREG-FDBOFSET;          /* POINT TO BEGINNING OF   * 00625000
*                                           * BLOCK TO BE FREED   */    00626000
         LR    FSBPTR,FDBREG                                       0705 00627000
         SH    FSBPTR,FDBOFSET(,FDBREG)                            0705 00628000
*         FSBPTR->FDBFDBA=FDBFDBA;         /* MAKE 1ST WORD NXT FDB @ * 00629000
         L     @15,FDBFDBA(,FDBREG)                                0706 00630000
