         TITLE 'ISTDCC81 PURGE INBOUND                                 *00001000
                        '                                               00002000
ISTDCC81 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTDCC81  76.322'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  LA    @11,4095(,@12)                                      0001 00011000
         USING @PSTART,@12                                         0001 00012000
         USING @PSTART+4095,@11                                    0001 00013000
*     CRRPTR=ADDR(CRRSPACE);              /* COVER CRR SAVE AREA   */   00014000
         L     CRRPTR,RPHCRR(,RPHPTR)                              0467 00015000
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0467 00016000
*     CRRMAS=CRRMAS && CRRMAS;             /* CLEAR AUDIT TRAIL SW    * 00017000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0468 00018000
*     CRRM81=ON;                           /* SET AUDIT TRAIL SW1     * 00019000
         OI    CRRM81(CRRPTR),B'00000001'                          0469 00020000
*     CRRC81=ON;                           /* SET AUDIT TRAIL SW2     * 00021000
         OI    CRRC81(CRRPTR),B'00000001'                          0470 00022000
*     CRRFMCB=RPHMAJCB;                                            0471 00023000
