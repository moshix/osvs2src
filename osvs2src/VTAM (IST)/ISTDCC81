         ST    @15,CRRFSB(,CRRPTR)                                 0472 00029000
*     FSBPTR=CRRFSB;                       /* REGISTER BASE FOR FSB  */ 00030000
         LR    FSBPTR,@15                                          0473 00031000
*     RFY ISTFMCB BASED(CRRFMCB);          /* COVER FMCB              * 00032000
*     RFY ISTDNCB BASED(FMCDNCBA);         /*COVER DNCB               * 00033000
*     RFY ISTNCB BASED(ADDR(DNCNCB));      /*COVER NCB                * 00034000
*     RFY ISTFSB BASED(FSBPTR);            /* COVER FSB               * 00035000
*     CALL PRGEPROC;                       /* PROCESS PURGE FEEDBACK  * 00036000
