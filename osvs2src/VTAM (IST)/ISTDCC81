         TM    PROCFTX+48(@15),B'00000001'                         0709 00636000
         BNO   @RF00709                                            0709 00637000
*             /*RELSTORE BUFADDR(FSBPTR)CLEAR(YES)CHAIN(YES)*/     0710 00638000
*              DO;                                                 0710 00639000
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  0711 00640000
*/* END OF ATBUILD */                                              0711 00641000
*             RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;              0711 00642000
*         RFY (GPR01P,GPR13P) RSTD;                                0712 00643000
