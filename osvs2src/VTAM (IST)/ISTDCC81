*          DCL FMZL0222 BASED(GPR15P) LABEL VALUERANGE(*);         1183 01718000
*          CALL FMZL0222;                                          1184 01719000
*                                                                  1184 01720000
         BALR  @14,@15                                             1184 01721000
* RFY (GPR13P) RSTD;                                               1185 01722000
*          GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);      1186 01723000
*                                                                  1186 01724000
           LM GPR02P,GPR13P,RPHWORK(GPR01P)                             01725000
* RFY (GPR13P) UNRSTD;                                             1187 01726000
*                                                                  1187 01727000
* RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;                        1188 01728000
* END ;    /********* END OF REQSTORE EXPANSION **********/        1189 01729000
*        IF RCREG=FAIL                     /* NO STORAGE              * 01730000
*         THEN                             /* WAIT FOR IT             * 01731000
*                                                                  1190 01732000
         C     RCREG,@CF00060                                      1190 01733000
         BNE   @RF01190                                            1190 01734000
*/*  TPWAIT   */                                                   1191 01735000
*           DO; /* BEGIN TPWAIT GENERATION*/                       1191 01736000
*           RFY (GPR01P,GPR14P,GPR15P) RSTD;                       1192 01737000
*           RFY (GPR00P) RSTD;                                     1193 01738000
*           GEN (STM   GPR00P,GPR15P,RPHWORK(GPR01P))              1194 01739000
*           REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);              1194 01740000
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)                            01741000
*           RFY (GPR00P) UNRSTD;                                   1195 01742000
*           GPR15P=ADDR(LHAA0241);                                 1196 01743000
         LA    GPR15P,LHAA0241                                     1196 01744000
*           GPR14P=0;                                              1197 01745000
         SLR   GPR14P,GPR14P                                       1197 01746000
*           GPR15P=GPR15P & '00FFFFFF'X;                           1198 01747000
         N     GPR15P,@CF04346                                     1198 01748000
*           GPR15P=GPR15P | RPHWT1;                                1199 01749000
         O     GPR15P,@CF01173                                     1199 01750000
*           CS (GPR14P,GPR15P,GPR01P->RPHRESMA);                   1200 01751000
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         1200 01752000
*           BC (7,LHAB0241);                                       1201 01753000
*                                                                  1201 01754000
