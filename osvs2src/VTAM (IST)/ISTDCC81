*             END ;/**********END OF RELSTORE EXPANSION**********/ 0749 00713000
*     RTNREG=CRRRTN3;                      /*RESTORE RETURN REG       * 00714000
@RC00709 L     RTNREG,CRRRTN3(,CRRPTR)                             0750 00715000
*     RETURN;                                                      0751 00716000
@EL00004 DS    0H                                                  0751 00717000
@EF00004 DS    0H                                                  0751 00718000
@ER00004 BR    @14                                                 0751 00719000
*END FREEFSB;                                                      0752 00720000
*/********************************************************************/ 00721000
*/*                                                                  */ 00722000
*/* ROUTINE NAME : CLNPURGQ - CLEAN OFF THE PURGE RESET QUEUE        */ 00723000
*/*                                                                  */ 00724000
*/* FUNCTION :                                                       */ 00725000
*/*    THE LCPB RESET QUEUE MAY HAVE RPLS OR LCPBS ON IT. RPLS ARE   */ 00726000
*/*    POSTED WITH PURGE RETURN CODES AND THE LCPBS ARE QUEUED AND   */ 00727000
*/*    SCHEDULED ON THE CONTROL LAYER INBOUND PAB. IF THE LCPB IS    */ 00728000
*/*    A PURGE LCPB IS'S OWN LCPB PURGE RESET QUEUE (LCPRSTQH) IS    */ 00729000
*/*    PROCESSED. FSB'S ARE BUILT FOR ANY LCPB THAT IS POSTED OR FED */ 00730000
*/*    BACK TO CONTROL LAYER.                                        */ 00731000
*/*                                                                  */ 00732000
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */ 00733000
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */ 00734000
*/*                                                                  */ 00735000
*/* OUTPUT :  ALL ELEMENTS ON THE RESET QUEUE PROCESSED              */ 00736000
*/*                                                                  */ 00737000
*/* SUBROUTINES CALLED :                                             */ 00738000
*/*                      PRGEPOST - POST PURGE OPERATION COMPLETE    */ 00739000
*/*                      POSTRPL - - POST RPL STATUS                 */ 00740000
*/*                                                                  */ 00741000
*/*  MACROS USED :   TPDEQ,TPFEL,REQSTORE,TPWAIT,TPQUE               */ 00742000
*/*                                                                  */ 00743000
*/*******************************************************************/  00744000
*CLNPURGQ: PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);              0753 00745000
*                                                                  0753 00746000
CLNPURGQ DS    0H                                                  0754 00747000
*     CRRRTN4=RTNREG;                      /*SAVE RETURN ADDRESS      * 00748000
         ST    RTNREG,CRRRTN4(,CRRPTR)                             0754 00749000
*     DO WHILE CRRSW1=OFF;                 /* ONE COMPLETE CYCLE SW   * 00750000
         B     @DE00755                                            0755 00751000
@DL00755 DS    0H                                                  0756 00752000
*      CRRSW1=ON;                          /* SWITCH TO STOP DO WHILE*/ 00753000
*      CRRSW2=OFF;                         /*INITIALIZE IS-PURGE SW   * 00754000
         OI    CRRSW1(CRRPTR),B'01000000'                          0757 00755000
         NI    CRRSW2(CRRPTR),B'11011111'                          0757 00756000
*      IF WRKREGD->RPLID='00'X             /* IS THIS ELEMENT AN RPL  * 00757000
*        THEN;                             /* DON'T CHECK LCCW        * 00758000
         CLI   RPLID(WRKREGD),X'00'                                0758 00759000
         BE    @RT00758                                            0758 00760000
*      ELSE DO;                                                    0760 00761000
*        IF WRKREGD->LCPNXLCW->LCCWOP=LCCWPRGE /* IF A PURGE LCPB     * 00762000
*          THEN CRRSW2=ON;                 /*SET PURGE INDICATOR      * 00763000
         L     @15,LCPNXLCW(,WRKREGD)                              0761 00764000
         CLI   LCCWOP(@15),X'6B'                                   0761 00765000
         BNE   @RF00761                                            0761 00766000
         OI    CRRSW2(CRRPTR),B'00100000'                          0762 00767000
*        ELSE;                             /* NOT A PURGE LCPB        * 00768000
@RF00761 DS    0H                                                  0764 00769000
*        END;                              /*END-ELEMENT NOT RPL      * 00770000
*      IF CRRSW2=ON                        /* IF A PURGE LCPB         * 00771000
*        THEN DO;                                                  0765 00772000
*                                                                  0765 00773000
@RT00758 TM    CRRSW2(CRRPTR),B'00100000'                          0765 00774000
         BNO   @RF00765                                            0765 00775000
*   /* THIS IS A PURGE LCPB. ITS PURGED QUEUE MUST BE CLEANED OFF *     00776000
*    * ALSO   */                                                   0767 00777000
*                                                                  0767 00778000
*          PRGPTRC=CURRPTRB;               /*SET UP PTR FOR PURGE LCPB* 00779000
         L     @15,CURRPTRB(,CRRPTR)                               0767 00780000
         ST    @15,PRGPTRC(,CRRPTR)                                0767 00781000
*          CURRPTRB=WRKREGD;               /* CURRENT ELT IS WORK ELT * 00782000
         ST    WRKREGD,CURRPTRB(,CRRPTR)                           0768 00783000
*          RFY ISTDCLCP  BASED(CURRPTRB);  /*COVER CURRENT LCPB       * 00784000
*                                                                  0769 00785000
*/* TPFEL (,LCPRSTQH) RETPTR(WRKREGD) */                           0770 00786000
*           DO; /* BEGIN TPFEL CODE GENERATION */                  0770 00787000
*           DCL FMZCA108 BIT(32) CONSTANT('80000000'X);            0771 00788000
*           DCL FMZCB108 BIT(32) CONSTANT('7FFFFFFF'X);            0772 00789000
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                0773 00790000
*                                                                  0773 00791000
*           GPR15P=ADDR(LCPRSTQH); /*ADDRESS QUEUE POINTER */      0774 00792000
         L     @10,CURRPTRB(,CRRPTR)                               0774 00793000
         LA    GPR15P,LCPRSTQH(,@10)                               0774 00794000
*           GPR14P=GPR15P->PABWEQA|FMZCA108;                       0775 00795000
         L     @10,@CF01149                                        0775 00796000
         L     GPR14P,PABWEQA(,GPR15P)                             0775 00797000
         OR    GPR14P,@10                                          0775 00798000
*           IF GPR14P=FMZCA108 THEN;                               0776 00799000
         CLR   GPR14P,@10                                          0776 00800000
         BE    @RT00776                                            0776 00801000
*           ELSE DO;                                               0778 00802000
*           GPR00P=FMZCA108; /* INITIALIZE QUIT FLAG */            0779 00803000
         L     GPR00P,@CF01149                                     0779 00804000
*           DO WHILE (GPR00P ^= GPR14P->WKECHAIN);                 0780 00805000
         B     @DE00780                                            0780 00806000
@DL00780 DS    0H                                                  0781 00807000
*              GPR14P=GPR14P->WKECHAIN; /* SEARCH FOR END OF CHAIN */   00808000
         L     GPR14P,WKECHAIN(,GPR14P)                            0781 00809000
*              END;                                                0782 00810000
@DE00780 C     GPR00P,WKECHAIN(,GPR14P)                            0782 00811000
         BNE   @DL00780                                            0782 00812000
*           END;                                                   0783 00813000
*           GPR14P=GPR14P&FMZCB108;                                0784 00814000
@RT00776 N     GPR14P,@CF01147                                     0784 00815000
*           WRKREGD=GPR14P;                                        0785 00816000
         LR    WRKREGD,GPR14P                                      0785 00817000
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;              0786 00818000
*           END; /* END TPFEL GENERATION */ /*GET LAST ELEMENT ADDR*/   00819000
*          IF WRKREGD^=ZERO               /* IF Q IS EMPTY           */ 00820000
*            THEN CRRSW1=OFF;            /*ANOTHER CYCLE IS NEEDED*/    00821000
         LTR   WRKREGD,WRKREGD                                     0788 00822000
         BZ    @RF00788                                            0788 00823000
         NI    CRRSW1(CRRPTR),B'10111111'                          0789 00824000
*          ELSE DO;                        /* RESET Q IS EMPTY        * 00825000
         B     @RC00788                                            0790 00826000
@RF00788 DS    0H                                                  0791 00827000
*            RFY ISTDCLCP BASED(PRGPTRC);                          0791 00828000
*            WRKREGD=CURRPTRB;              /*            @DM09165*/    00829000
         L     WRKREGD,CURRPTRB(,CRRPTR)                           0792 00830000
*            /*GET PURGE LCPB */                                   0793 00831000
*                                                                  0793 00832000
*/* TPDEQ (WRKREGD,LCPRSTQH)  */                                   0793 00833000
*             DO; /* BEGIN TPDEQ CODE GENERATION */                0793 00834000
*             RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;              0794 00835000
*                                                                  0794 00836000
*             GPR15P=ADDR(LCPRSTQH); /*ADDRESS QUEUE POINTER */    0795 00837000
         L     GPR15P,PRGPTRC(,CRRPTR)                             0795 00838000
         LA    GPR15P,LCPRSTQH(,GPR15P)                            0795 00839000
*             RFY (GPR02P,GPR03P) RSTD;                            0796 00840000
*             STM(GPR02P,GPR03P,GPR01P->RPHWORK(5:12));            0797 00841000
         STM   GPR02P,GPR03P,RPHWORK+4(GPR01P)                     0797 00842000
*             GPR03P=WRKREGD | FMZCA108;                           0798 00843000
         L     @10,@CF01149                                        0798 00844000
         LR    GPR03P,WRKREGD                                      0798 00845000
         OR    GPR03P,@10                                          0798 00846000
*             GPR14P=FMZCA108;                                     0799 00847000
         LR    GPR14P,@10                                          0799 00848000
*             GPR15P=ADDR(GPR15P->PABWEQA);                        0800 00849000
*             GPR02P=GPR15P->PABWEQA|FMZCA108;                     0801 00850000
         O     @10,PABWEQA(,GPR15P)                                0801 00851000
         LR    GPR02P,@10                                          0801 00852000
*             DO WHILE(GPR02P^=GPR14P);                            0802 00853000
         B     @DE00802                                            0802 00854000
@DL00802 DS    0H                                                  0803 00855000
*FMZL2112:;                                                        0803 00856000
FMZL2112 DS    0H                                                  0804 00857000
*               IF GPR02P=GPR03P                                   0804 00858000
*               THEN DO;                                           0804 00859000
         CR    GPR02P,GPR03P                                       0804 00860000
         BNE   @RF00804                                            0804 00861000
*                 GPR00P=GPR02P->WKECHAIN;                         0806 00862000
         L     GPR00P,WKECHAIN(,GPR02P)                            0806 00863000
*                 CS(GPR02P,GPR00P,GPR15P->PABWEQA);               0807 00864000
         CS    GPR02P,@00,PABWEQA(GPR15P)                          0807 00865000
*                 BC(7,FMZL3112);                                  0808 00866000
         BC    7,FMZL3112                                          0808 00867000
*                 GPR14P=GPR02P;                                   0809 00868000
         LR    GPR14P,GPR02P                                       0809 00869000
*                 GPR14P->WKECHAIN=0;                              0810 00870000
         SLR   @10,@10                                             0810 00871000
         ST    @10,WKECHAIN(,GPR14P)                               0810 00872000
*                 END;                                             0811 00873000
*               ELSE DO;                                           0812 00874000
         B     @RC00804                                            0812 00875000
@RF00804 DS    0H                                                  0813 00876000
*                 IF (GPR02P->WKECHAIN)=GPR14P                     0813 00877000
*                 THEN                                             0813 00878000
         L     @10,WKECHAIN(,GPR02P)                               0813 00879000
         CR    @10,GPR14P                                          0813 00880000
         BE    @RT00813                                            0813 00881000
*                   GOTO ISTA0112;                                 0814 00882000
*                 ELSE DO;                                         0815 00883000
*                   GPR15P=ADDR(GPR02P->WKECHAIN);                 0816 00884000
         LA    GPR15P,WKECHAIN(,GPR02P)                            0816 00885000
*                   GPR02P=GPR02P->WKECHAIN;                       0817 00886000
         LR    GPR02P,@10                                          0817 00887000
*                   GOTO FMZL2112;                                 0818 00888000
         B     FMZL2112                                            0818 00889000
*                   END;                                           0819 00890000
*                 END;                                             0820 00891000
*FMZL3112:;                                                        0821 00892000
@RC00804 DS    0H                                                  0821 00893000
FMZL3112 DS    0H                                                  0822 00894000
*             END;                                                 0822 00895000
@DE00802 CR    GPR02P,GPR14P                                       0822 00896000
         BNE   @DL00802                                            0822 00897000
*ISTA0112:;                                                        0823 00898000
ISTA0112 DS    0H                                                  0824 00899000
*             LM (GPR02P,GPR03P,GPR01P->RPHWORK(5:12));            0824 00900000
         LM    GPR02P,GPR03P,RPHWORK+4(GPR01P)                     0824 00901000
*             RFY (GPR02P,GPR03P) UNRSTD;                          0825 00902000
*             GPR14P=GPR14P&FMZCB108;                              0826 00903000
         N     GPR14P,@CF01147                                     0826 00904000
*             GPR01P->RPHWEA=GPR14P;                               0827 00905000
         ST    GPR14P,RPHWEA(,GPR01P)                              0827 00906000
*             RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;            0828 00907000
*             END; /* END TPDEQ GENERATION */                      0829 00908000
*     /*REQSTORE CBID(FSB)RPH(RPHPTR)CLEAR(YES)SAVE(YES)PRIORITY(YES)*/ 00909000
*      DO;                                                         0830 00910000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)  */                         0831 00911000
*/* END OF ATBUILD */                                              0831 00912000
*                                                                  0831 00913000
*     RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;                      0831 00914000
*                                                                  0831 00915000
*     RFY (GPR01P,GPR13P) RSTD;                                    0832 00916000
*          GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);      0833 00917000
*                                                                  0833 00918000
           STM GPR02P,GPR13P,RPHWORK(GPR01P)                            00919000
*     RFY (GPR01P,GPR13P) UNRSTD;                                  0834 00920000
*                                                                  0834 00921000
*     DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */                0835 00922000
*       GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/                0836 00923000
         L     GPR15P,ATCLCPTR                                     0836 00924000
*     END; /*LISATCVT*/                                            0837 00925000
*          GPR02P=GPR15P->ATCBPDA | '80000000'X;                   0838 00926000
         L     GPR02P,ATCBPDA(,GPR15P)                             0838 00927000
         O     GPR02P,@CF01149                                     0838 00928000
*          DCL 1 FMZA0118 FIXED(31) STATIC LOCAL,                  0839 00929000
*              2 *        FIXED(8) INIT(248),                      0839 00930000
*              2 *        PTR(24) INIT(0);                         0839 00931000
*          GPR03F=FMZA0118;                                        0840 00932000
         L     GPR03F,FMZA0118                                     0840 00933000
*          GPR04F=8;                                               0841 00934000
         LA    GPR04F,8                                            0841 00935000
*          GPR15P=GPR15P->ATCSMRQ;                                 0842 00936000
         L     GPR15P,ATCSMRQ(,GPR15P)                             0842 00937000
*          DCL FMZL0118 BASED(GPR15P) LABEL VALUERANGE(*);         0843 00938000
*          CALL FMZL0118;                                          0844 00939000
*                                                                  0844 00940000
         BALR  @14,@15                                             0844 00941000
*     RFY (GPR13P) RSTD;                                           0845 00942000
*          GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);      0846 00943000
*                                                                  0846 00944000
           LM GPR02P,GPR13P,RPHWORK(GPR01P)                             00945000
*     RFY (GPR13P) UNRSTD;                                         0847 00946000
*                                                                  0847 00947000
*     RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;                    0848 00948000
*     END ;/********* END OF REQSTORE EXPANSION **********/        0849 00949000
*F RCREG=FAIL                 /* IF NO STORAGE AVAILABLE*/         0849 00950000
*             THEN                         /* WAIT FOR IT             * 00951000
*                                                                  0850 00952000
         C     RCREG,@CF00060                                      0850 00953000
         BNE   @RF00850                                            0850 00954000
*/*  TPWAIT   */                                                   0851 00955000
*               DO; /* BEGIN TPWAIT GENERATION*/                   0851 00956000
*               RFY (GPR01P,GPR14P,GPR15P) RSTD;                   0852 00957000
*               RFY (GPR00P) RSTD;                                 0853 00958000
*               GEN (STM   GPR00P,GPR15P,RPHWORK(GPR01P))          0854 00959000
*               REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);          0854 00960000
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)                            00961000
*               RFY (GPR00P) UNRSTD;                               0855 00962000
