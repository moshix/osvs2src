*                                                         FOR VS2    */ 00627000
*           3  ATCTPMMI PTR(31),        /* PTR TO ISTCFMMI - MSG ISSUER 00628000
*                                          FOR SRB MODE ROUTINES     */ 00629000
*           3  ATCVIOPM PTR(31),       /* PTR TO ISTVIOPM            */ 00630000
*        2  ATCAOS CHAR(0) BDY(WORD);  /* VS2 ONLY FIELDS            */ 00631000
*/* END OF ATBUILD */                                              0045 00632000
*                                                                  0045 00633000
*         DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */            0045 00634000
*           GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/            0046 00635000
         L     GPR15P,ATCLCPTR                                     0046 00636000
*         END; /*LISATCVT*/                                        0047 00637000
*           RESPECIFY GPR00F RSTD;                                 0048 00638000
*           GPR00F=4;    /* SET PARAMETER REGISTER TO 4 */         0049 00639000
