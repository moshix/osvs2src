I007C02  DS    0H                                                  0086 00135000
         OPEN  (REMCDS,(OUTPUT))        /*OPEN REMOTE DATA SET       */ 00136000
         SPACE                                                          00137000
         LA    WKREG1,REMCDS            /*SET UP PTR                 */ 00138000
         TM    OPENFLAG(WKREG1),GOODOPEN /*WAS OPEN GOOD             */ 00139000
         BZ    I007CER1                 /*NO-OPEN ERROR                 00140000
         MVC   CDSMEMBR(L'CDSMEMBR),CDSSYMBN(R8) SET UP NME IN BLDL  */ 00141000
         SPACE                                                          00142000
         STOW  REMCDS,CDSBLDL,D         /*DELETE CDS                 */ 00143000
         SPACE                                                          00144000
         LTR R15,R15                    /*IF RET CODE EQ ZERO THEN   */ 00145000
         BZ  I007CEX                    /*PRT MSG AND RET TO CALLER  */ 00146000
         B   I007CER2                   /*OTERWISE PRT ERR & RETURN  */ 00147000
*   GEN(EJECT);                                                    0087 00148000
         EJECT                                                          00149000
