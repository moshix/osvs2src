*   NCSPLVCD=NCSPLVC4;                 /*INDICATE DISPLAY COMMAND     * 01447000
         L     @08,NCSPLPTR                                        1022 01448000
         MVI   NCSPLVCD(@08),X'04'                                 1022 01449000
*   RETCODE=4;                         /*SET ERROR RETURN CODE FOR NOW* 01450000
         MVI   RETCODE,X'04'                                       1023 01451000
*   DSPLFLGS=0;                        /*INIT CONFLICT BYTE  @X50BHYB*/ 01452000
         MVI   DSPLFLGS,X'00'                                      1024 01453000
*   LOOPSW=ON;                         /*INITILIZE DO WHILE           * 01454000
         OI    LOOPSW,B'00000010'                                  1025 01455000
*   R1=ADDR(CSCTEXT);                  /*PT TO BEGINING OF TEXT       * 01456000
         L     R1,CSCBPTR                                          1026 01457000
         LA    R1,CSCTEXT(,R1)                                     1026 01458000
*   DO WHILE LOOPSW=ON;                /*DO WHILE TO PROCESS PARMS    * 01459000
         B     @DE01027                                            1027 01460000
@DL01027 DS    0H                                                  1028 01461000
*   R0=ADDR(DISPLYTB);                 /*PT TO DISPLAY TABLE          * 01462000
         LA    R0,DISPLYTB                                         1028 01463000
*   CALL PHRBR;                        /*PROCESS DISPLAY PARMS        * 01464000
         BAL   @14,PHRBR                                           1029 01465000
*   IF R15^=4                          /*WAS RETURN CODE FOUR         * 01466000
*        THEN DO;                      /*YES ARGUMENT FOUND           * 01467000
         C     R15,@CF00055                                        1030 01468000
         BE    @RF01030                                            1030 01469000
*             IF R15=8                 /*WAS IT END OF PARMS          * 01470000
*                  THEN DO;            /*END OF PARMS                 * 01471000
         C     R15,@CF00061                                        1032 01472000
         BNE   @RF01032                                            1032 01473000
*                       RETCODE=0;     /*SET RETURN CODE TO ZERO      * 01474000
         MVI   RETCODE,X'00'                                       1034 01475000
*                  END;                /*END END OF PARMS CODE        * 01476000
*                  ELSE DO;            /*NOT END OF PARMS             * 01477000
         B     @RC01032                                            1036 01478000
@RF01032 DS    0H                                                  1037 01479000
*                       IF R15=0       /*WAS THERE ANY SYNTAX ERROR   * 01480000
*                            THEN DO;  /*NO SYNTAX ERROR              * 01481000
         LTR   R15,R15                                             1037 01482000
         BNZ   @RF01037                                            1037 01483000
*                                 CALL DKWDRTN;/*GO TO RTN PT BY R0   * 01484000
         LR    @15,R0                                              1039 01485000
         BALR  @14,@15                                             1039 01486000
*                            END;      /*END NO SYNTAX ERROR CODE     * 01487000
*                            ELSE DO;  /*SYNTAX ERROR                 * 01488000
         B     @RC01037                                            1041 01489000
@RF01037 DS    0H                                                  1042 01490000
*                                 MSGCODE='037I';/*SET MSG ID         * 01491000
         MVC   MSGCODE(4),@CC03490                                 1042 01492000
*                                 CALL MSGS;/*CALL MSG ROUTIN         * 01493000
         BAL   @14,MSGS                                            1043 01494000
*                            END;      /*END SYNTAX ERROR CODE        * 01495000
*                  END;                /*END OF NOT END OF PARMS CODE * 01496000
*        END;                          /*END OF ARGUMENT FOUND CODE   * 01497000
*        ELSE DO;                      /*ERROR IN PARM PROCESSING     * 01498000
         B     @RC01030                                            1047 01499000
@RF01030 DS    0H                                                  1048 01500000
*             MSGCODE='039I';          /*SET MSG ID                   * 01501000
         MVC   MSGCODE(4),@CC03481                                 1048 01502000
*             CALL MSGS;               /*ISSUE UNIDENTIFIABLE KWD MSG * 01503000
         BAL   @14,MSGS                                            1049 01504000
*        END;                          /*END OF RC NOT ZERO CODE      * 01505000
*   END;                               /*END OF DO WHILE              * 01506000
@RC01030 DS    0H                                                  1051 01507000
@DE01027 TM    LOOPSW,B'00000010'                                  1051 01508000
         BO    @DL01027                                            1051 01509000
*   R14=R14SVSCN;                     /*RESTORE RETURN REG   @D32CKBN*/ 01510000
         L     R14,R14SVSCN                                        1052 01511000
*   END DSPPARM;                                                   1053 01512000
@EL00012 DS    0H                                                  1053 01513000
@EF00012 DS    0H                                                  1053 01514000
@ER00012 BR    @14                                                 1053 01515000
*/********************************************************************/ 01516000
*/*                                                                  */ 01517000
*/*             TYPRTN:  THIS ROUTINE HANDLES THE TYPE PARM OF THE   */ 01518000
*/*                      TRACE COMMAND.  IT TEST FOR MULTIPLE TYPE   */ 01519000
*/*                      PARM SPECIFIED AND ISSUES A MSG IF SO.      */ 01520000
*/*                      OTHERWISE IT TEST FOR A VALID TYPE SPECIFIED*/ 01521000
*/*                      AND SETS THE APPROPRIATE BIT IN THE NCSPL   */ 01522000
*/*                      OR ISSUES A MSG IF THE TYPE IS NOT VALID.   */ 01523000
*/*                      IT ALSO TURNS ON THE TYPE SPECIFIED BIT.    */ 01524000
*/*                                                                  */ 01525000
*/********************************************************************/ 01526000
*                                                                  1054 01527000
*TYPRTN: PROC OPTIONS(NOSAVE,NOSAVEAREA);                          1054 01528000
TYPRTN   DS    0H                                                  1055 01529000
*   R14SVKWD=R14;                      /*SAVE REG 14         @D32CKBN*/ 01530000
         ST    R14,R14SVKWD                                        1055 01531000
*   IF PHRF2NVL > 1                                        /*@X50BHEE*/ 01532000
*      THEN DO;                                            /*@X50BHEE*/ 01533000
         L     @08,RPHSAVE                                         1056 01534000
         L     @08,PHRF2NVL(,@08)                                  1056 01535000
         C     @08,@CF00063                                        1056 01536000
         BNH   @RF01056                                            1056 01537000
*        MSGCODE='403I';                                   /*@X50BHEE*/ 01538000
         MVC   MSGCODE(4),@CC03494                                 1058 01539000
*        MSGTXT(9:16)='TYPE    ';                          /*@X50BHEE*/ 01540000
         MVC   MSGTXT+8(8),@CC03448                                1059 01541000
*        CALL MSGS;                                        /*@X50BHEE*/ 01542000
         BAL   @14,MSGS                                            1060 01543000
*      END;                                                /*@X50BHEE*/ 01544000
*    ELSE DO;                                              /*@X50BHEE*/ 01545000
         B     @RC01056                                            1062 01546000
@RF01056 DS    0H                                                  1063 01547000
*   TYPESPC=ON;                        /*INIT TYPE SPECIFIED ON*/  1063 01548000
*   TRREQ=ON;                          /*REQUIRES TRACE KWD*/      1064 01549000
         OI    TYPESPC,B'01001000'                                 1064 01550000
*   IF NCSVBF05=ON |                   /*PRIOR TYPE=LINE */        1065 01551000
*      NCSVBF04=ON |                   /*OR PRIOR TYPE=SMS   @X50BHYB*/ 01552000
*      NCSVBF06=ON |                   /*OR PRIOR TYPE=IO*/        1065 01553000
*      NCSVBF07=ON                     /*OR PRIOR TYPE=BUF*/       1065 01554000
*      THEN DO;                        /*YES PRIOR TYPE*/          1065 01555000
         L     @08,NCSPLPTR                                        1065 01556000
         TM    NCSVBF05(@08),B'00011110'                           1065 01557000
         BZ    @RF01065                                            1065 01558000
*         MSGCODE='096I';              /*SET ID FOR MULTI MSG*/    1067 01559000
         MVC   MSGCODE(4),@CC03495                                 1067 01560000
*         MSGTXT(9:16)='TYPE    ';     /*SET MSG TEXT*/            1068 01561000
         MVC   MSGTXT+8(8),@CC03448                                1068 01562000
*         CALL MSGS;                   /*CALL MSG ROUTINE*/        1069 01563000
         BAL   @14,MSGS                                            1069 01564000
*      END;                            /*END PRIOR TYPE*/          1070 01565000
*      ELSE DO;                        /*NO PRIOR TYPE*/           1071 01566000
         B     @RC01065                                            1071 01567000
@RF01065 DS    0H                                                  1072 01568000
*               IF PHRF2VAL(1)=1             /*IS IT TYPE=IO*/     1072 01569000
*                  THEN DO;                  /* YES */             1072 01570000
         L     @08,RPHSAVE                                         1072 01571000
         CLC   PHRF2VAL(4,@08),@CF00063                            1072 01572000
         BNE   @RF01072                                            1072 01573000
*                     NCSVBF06=ON;           /*SET IO BIT*/        1074 01574000
         L     @08,NCSPLPTR                                        1074 01575000
         OI    NCSVBF06(@08),B'00000100'                           1074 01576000
*                     IDREQ=ON; /*SET ID REQUIRED            @DL32HBN*/ 01577000
*                     LOOPSW=ON;             /*CONTINUE SCAN*/     1076 01578000
         OI    IDREQ,B'00000011'                                   1076 01579000
*                  END;                      /*END TYPE=IO*/       1077 01580000
*                  ELSE DO;                  /*NOT TYPE=IO*/       1078 01581000
         B     @RC01072                                            1078 01582000
@RF01072 DS    0H                                                  1079 01583000
*                     IF PHRF2VAL(1)=2       /*IS IT TYPE=BUF*/    1079 01584000
*                        THEN DO;            /* YES */             1079 01585000
         L     @08,RPHSAVE                                         1079 01586000
         CLC   PHRF2VAL(4,@08),@CF00075                            1079 01587000
         BNE   @RF01079                                            1079 01588000
*                           NCSVBF07=ON;     /*SET BUF BIT*/       1081 01589000
         L     @08,NCSPLPTR                                        1081 01590000
         OI    NCSVBF07(@08),B'00000010'                           1081 01591000
*                           IDREQ=ON; /*SET ID REQRD         @DL32HBN*/ 01592000
*                           LOOPSW=ON;       /*CONTINUE SCAN*/     1083 01593000
         OI    IDREQ,B'00000011'                                   1083 01594000
*                        END;                /*END TYPE=BUF*/      1084 01595000
*                        ELSE DO;            /*NOT TYPE=BUF*/      1085 01596000
         B     @RC01079                                            1085 01597000
@RF01079 DS    0H                                                  1086 01598000
*                           IF PHRF2VAL(1)=3 /*IS IT TYPE=LINE*/   1086 01599000
*                              THEN DO;      /*YES */              1086 01600000
         L     @08,RPHSAVE                                         1086 01601000
         CLC   PHRF2VAL(4,@08),@CF00290                            1086 01602000
         BNE   @RF01086                                            1086 01603000
*                                 NCSVBF05=ON;/*TURN ON LINE BIT*/ 1088 01604000
         L     @08,NCSPLPTR                                        1088 01605000
         OI    NCSVBF05(@08),B'00001000'                           1088 01606000
*                                 IDREQ=ON;/*SET ID REQD     @DL32HBN*/ 01607000
*                                 LOOPSW=ON; /*CONTINUE SCAN*/     1090 01608000
         OI    IDREQ,B'00000011'                                   1090 01609000
*                              END;          /*END TYPE=LINE*/     1091 01610000
*                              ELSE DO;      /*NOT TYPE=LINE*/     1092 01611000
         B     @RC01086                                            1092 01612000
@RF01086 DS    0H                                                  1093 01613000
*                                 IF PHRF2VAL(1)=4 &/*IS IT VTAM TR*/   01614000
*                                    ATCDVTPT^=0 /*AND TR ALLOWED*/     01615000
*                                    THEN DO;  /*VALID VTAM TR*/   1093 01616000
         L     @08,RPHSAVE                                         1093 01617000
         CLC   PHRF2VAL(4,@08),@CF00055                            1093 01618000
         BNE   @RF01093                                            1093 01619000
         L     @08,ISTACVT(,AVTPTR)                                1093 01620000
         L     @08,ATCDVTPT(,@08)                                  1093 01621000
         LTR   @08,@08                                             1093 01622000
         BZ    @RF01093                                            1093 01623000
*                                      NCSVBF03=ON;/*SET VTAM TR BIT*/  01624000
         L     @08,NCSPLPTR                                        1095 01625000
         OI    NCSVBF03(@08),B'00100000'                           1095 01626000
*                                      NCSPLRID='VTAM';/*SET ID*/  1096 01627000
         MVI   NCSPLRID+4(@08),C' '                                1096 01628000
         MVC   NCSPLRID+5(3,@08),NCSPLRID+4(@08)                   1096 01629000
         MVC   NCSPLRID(4,@08),@CC03102                            1096 01630000
*                                      LOOPSW=ON;/*CONT      @DL32HBN*/ 01631000
         OI    LOOPSW,B'00000010'                                  1097 01632000
*                                    END;   /*END VTAM TR    @DL32HBN*/ 01633000
*                                                                  1098 01634000
*                                    ELSE DO;/*NOT VTAM TR   @X50BHYB*/ 01635000
         B     @RC01093                                            1099 01636000
@RF01093 DS    0H                                                  1100 01637000
*                                     IF PHRF2VAL(1)=5  /*IS TYPE=SMS*/ 01638000
*                                      THEN DO;   /*YES*/          1100 01639000
         L     @08,RPHSAVE                                         1100 01640000
         CLC   PHRF2VAL(4,@08),@CF00701                            1100 01641000
         BNE   @RF01100                                            1100 01642000
*                                       NCSVBF04=ON;  /*SET SMS BIT*/   01643000
         L     @08,NCSPLPTR                                        1102 01644000
         OI    NCSVBF04(@08),B'00010000'                           1102 01645000
*                                       IDREQ=ON;   /*SET ID REQRD*/    01646000
*                                       LOOPSW=ON; /*TO CONTINUE SCAN*/ 01647000
         OI    IDREQ,B'00000011'                                   1104 01648000
*                                       END;      /*END TYPE=SMS*/ 1105 01649000
*                                                                  1105 01650000
*                                      ELSE DO;       /*NOT SMS TRACE*/ 01651000
         B     @RC01100                                            1106 01652000
@RF01100 DS    0H                                                  1107 01653000
*                                       TYPESPC=OFF;/*NO VALID TYPE*/   01654000
         NI    TYPESPC,B'10111111'                                 1107 01655000
*                                       MSGCODE='043I';/*SET MSGID*/    01656000
         MVC   MSGCODE(4),@CC03496                                 1108 01657000
*                                       MSGTXT(9:16)='TYPE    ';   1109 01658000
         MVC   MSGTXT+8(8),@CC03448                                1109 01659000
*                                       CALL MSGS; /*CALL MSG RTN*/     01660000
         BAL   @14,MSGS                                            1110 01661000
*                                    END;       /*END NOT TR @DL32HBN*/ 01662000
*                                 END;    /*TYPE NOT VTAM TR @X50BHYB*/ 01663000
@RC01100 DS    0H                                                  1113 01664000
*                              END;          /*END TYPE NOT LINE*/ 1113 01665000
@RC01093 DS    0H                                                  1114 01666000
*                        END;                /*END TYPE NOT BUF*/  1114 01667000
@RC01086 DS    0H                                                  1115 01668000
*                  END;                      /*END TYPE NOT IO*/   1115 01669000
@RC01079 DS    0H                                                  1116 01670000
*      END;                            /*END NO PRIOR TYPE*/       1116 01671000
@RC01072 DS    0H                                                  1117 01672000
*   END;                                                   /*@X50BHEE*/ 01673000
@RC01065 DS    0H                                                  1118 01674000
*   R14=R14SVKWD;                      /*RESTORE RETURN REG  @D32CKBN*/ 01675000
@RC01056 L     R14,R14SVKWD                                        1118 01676000
*END TYPRTN;                                                       1119 01677000
@EL00013 DS    0H                                                  1119 01678000
@EF00013 DS    0H                                                  1119 01679000
@ER00013 BR    @14                                                 1119 01680000
*MDFPARM: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         1120 01681000
MDFPARM  DS    0H                                                  1121 01682000
*   R14SVSCN=R14;                     /*SAVE RETURN REG      @D32CKBN*/ 01683000
         ST    R14,R14SVSCN                                        1121 01684000
*   RETCODE=4;                         /*SET RETURN CODE TO 4         * 01685000
         MVI   RETCODE,X'04'                                       1122 01686000
*   LOOPSW=ON;                         /*INITIALIZE DO WHILE          * 01687000
         OI    LOOPSW,B'00000010'                                  1123 01688000
*   NCSPLVCD=NCSPLVC2;                 /*INDICATE MODIFY COMMAND      * 01689000
         L     @08,NCSPLPTR                                        1124 01690000
         MVI   NCSPLVCD(@08),X'02'                                 1124 01691000
*   R1=ADDR(CSCTEXT);                  /*PT TO BEGINING OF TEXT       * 01692000
         L     R1,CSCBPTR                                          1125 01693000
         LA    R1,CSCTEXT(,R1)                                     1125 01694000
*   DO WHILE LOOPSW=ON;                /*DO WHILE TO PROCESS PARMS    * 01695000
         B     @DE01126                                            1126 01696000
@DL01126 DS    0H                                                  1127 01697000
*   R0=ADDR(MODTABLE);                 /*PT TO MODIFY TABLE           * 01698000
         LA    R0,MODTABLE                                         1127 01699000
*   CALL PHRBR;                        /*SET UP AND CALL PARM HANDLER * 01700000
         BAL   @14,PHRBR                                           1128 01701000
*   IF R15=4                           /*WAS IT ARGUMENT NOT FOUND    * 01702000
*        THEN DO;                      /*ARGUMENT NOT FOUND           * 01703000
         C     R15,@CF00055                                        1129 01704000
         BNE   @RF01129                                            1129 01705000
*             MSGCODE='039I';          /*SET MSG ID                   * 01706000
         MVC   MSGCODE(4),@CC03481                                 1131 01707000
*             CALL MSGS;               /*ISSUE INVALID KWD WRD MSG    * 01708000
         BAL   @14,MSGS                                            1132 01709000
*        END;                          /*END ARG NOT FOUND            * 01710000
*        ELSE DO;                      /*ARGUMENT FOUND               * 01711000
         B     @RC01129                                            1134 01712000
@RF01129 DS    0H                                                  1135 01713000
*             IF R15=8                 /*WAS IT END OF PARMS          * 01714000
*                  THEN                /*YES END OF PARMS             * 01715000
         C     R15,@CF00061                                        1135 01716000
         BNE   @RF01135                                            1135 01717000
*                       RETCODE=0;     /*SET RETURN CODE TO ZERO      * 01718000
         MVI   RETCODE,X'00'                                       1136 01719000
*                  ELSE DO;            /*NOT END OF PARMS             * 01720000
         B     @RC01135                                            1137 01721000
@RF01135 DS    0H                                                  1138 01722000
*                       IF R15=0       /*WAS THERE AN ERROR           * 01723000
*                            THEN      /*NO ERRORS                    * 01724000
         LTR   R15,R15                                             1138 01725000
         BNZ   @RF01138                                            1138 01726000
*                                 CALL MKWDRTN; /*GO TO RTN PT BY R0  * 01727000
         LR    @15,R0                                              1139 01728000
         BALR  @14,@15                                             1139 01729000
*                            ELSE DO;  /*THERE WAS AN ERROR           * 01730000
         B     @RC01138                                            1140 01731000
@RF01138 DS    0H                                                  1141 01732000
*                                 MSGCODE='037I'; /*SET MSG ID        * 01733000
         MVC   MSGCODE(4),@CC03490                                 1141 01734000
*                                 CALL MSGS;/*ISSUE SYNTAX ERROR MSG  * 01735000
         BAL   @14,MSGS                                            1142 01736000
*                            END;      /*END OF ERROR CODE            * 01737000
*                  END;                /*END OF NOT END OF PARMS CODE * 01738000
@RC01138 DS    0H                                                  1145 01739000
*        END;                          /*END ARGUMENT FOUND CODE      * 01740000
@RC01135 DS    0H                                                  1146 01741000
*   END;                               /*END OF DO WHILE              * 01742000
@RC01129 DS    0H                                                  1146 01743000
@DE01126 TM    LOOPSW,B'00000010'                                  1146 01744000
         BO    @DL01126                                            1146 01745000
*   R14=R14SVSCN;                     /*RESTORE RETURN REG   @D32CKBN*/ 01746000
         L     R14,R14SVSCN                                        1147 01747000
*   END MDFPARM;                                                   1148 01748000
@EL00014 DS    0H                                                  1148 01749000
@EF00014 DS    0H                                                  1148 01750000
@ER00014 BR    @14                                                 1148 01751000
*PHRBR: PROC OPTIONS(NOSAVE,NOSAVEAREA);                           1149 01752000
PHRBR    DS    0H                                                  1150 01753000
*/********************************************************************/ 01754000
*/*                                                                  */ 01755000
*/*     PHRBR  THIS SUBRTN TURNS OFF THE SCAN CONTROL SW (LOOPSW,    */ 01756000
*/*            SETS REG 2 TO POINT TO THE OUTPUT AREA FOR ISTINCP,   */ 01757000
*/*            AND CALLS THE PARAMETER SCAN ROUTINE (ISTINCPH).      */ 01758000
*/*            NOTE THE LOOPSW IS TURNED BACK ON BY THE INDIVIDUAL   */ 01759000
*/*            KEYWORD ROUTINES ONLY AFTER A KEYWORD IS HANDLED      */ 01760000
*/*            SUCCESSFULLY                                          */ 01761000
*/*                                                                  */ 01762000
*/********************************************************************/ 01763000
*   R14SVPHR=R14;                     /*SAVE RETURN REG      @D32CKBN*/ 01764000
         ST    R14,R14SVPHR                                        1150 01765000
*   LOOPSW=OFF;                        /*RESET LOOPSW OFF       */ 1151 01766000
         NI    LOOPSW,B'11111101'                                  1151 01767000
*   R2=RPHSAVE;                        /*PT TO OUTPUT AREA            * 01768000
         L     R2,RPHSAVE                                          1152 01769000
*   CALL ISTINCPH;                     /*CALL PARM HANDLER            * 01770000
         L     @15,@CV03479                                        1153 01771000
         BALR  @14,@15                                             1153 01772000
*   R14=R14SVPHR;                     /*RESTORE RETURN REG   @D32CKBN*/ 01773000
         L     R14,R14SVPHR                                        1154 01774000
*   END PHRBR;                                                     1155 01775000
@EL00015 DS    0H                                                  1155 01776000
@EF00015 DS    0H                                                  1155 01777000
@ER00015 BR    @14                                                 1155 01778000
*MDFTR: PROC OPTIONS(NOSAVE,NOSAVEAREA);                           1156 01779000
MDFTR    DS    0H                                                  1157 01780000
*/********************************************************************/ 01781000
*/*                                                                  */ 01782000
*/*     MDFTR     THIS SUBRTN TEST FOR DUPLICATE TRACE KEYWORDS      */ 01783000
*/*               IT ISSUES A MSG AND RETURNS WITHOUT SETTING THE    */ 01784000
*/*               LOOPSW ON IF THERE IS A DUP OTHERWISE IT TURNS ON  */ 01785000
*/*               THE LOOPSW, THE IDREQ SW, THE TYPEREQ SW AND       */ 01786000
*/*               INDICATES TRACE IN THE NCSPL.                      */ 01787000
*/*                                                                  */ 01788000
*/********************************************************************/ 01789000
*   R14SVKWD=R14;                     /*SAVE RETURN REG      @D32CKBN*/ 01790000
         ST    R14,R14SVKWD                                        1157 01791000
*   IF NCSVBF11=ON                     /*WAS THERE A PRIOR TRACE      * 01792000
*        THEN DO;                      /*YES THERE WAS A PRIOR TRACE  * 01793000
         L     @08,NCSPLPTR                                        1158 01794000
         TM    NCSVBF11(@08),B'00100000'                           1158 01795000
         BNO   @RF01158                                            1158 01796000
*             MSGCODE='096I';          /*SET MSG CODE FOR MULTI TRACE * 01797000
         MVC   MSGCODE(4),@CC03495                                 1160 01798000
*             MSGTXT(9:16)='TRACE   '; /*SET UP MSG TEXT              * 01799000
         MVC   MSGTXT+8(8),@CC03447                                1161 01800000
*             CALL MSGS;               /*ISSUE MULTI TRACE MSG        * 01801000
         BAL   @14,MSGS                                            1162 01802000
*        END;                          /*END OF MULTI TRACE CODE      * 01803000
*        ELSE DO;                      /*NO PRIOR TRACE               * 01804000
         B     @RC01158                                            1164 01805000
@RF01158 DS    0H                                                  1165 01806000
*             TYPEREQ=ON;              /*INDICATE TYPE REQUIRED*/  1165 01807000
         OI    TYPEREQ,B'00100000'                                 1165 01808000
*             NCSVBF11=ON;             /*SET TRACE BIT ON             * 01809000
         L     @08,NCSPLPTR                                        1166 01810000
         OI    NCSVBF11(@08),B'00100000'                           1166 01811000
*             LOOPSW=ON;               /*SET SW TO STAY IN DO WHILE   * 01812000
         OI    LOOPSW,B'00000010'                                  1167 01813000
*        END;                          /*END NO PRIOR TRACE CODE      * 01814000
*   R14=R14SVKWD;                     /*RESTORE RETURN REG   @D32CKBN*/ 01815000
@RC01158 L     R14,R14SVKWD                                        1169 01816000
*   END MDFTR;                                                     1170 01817000
@EL00016 DS    0H                                                  1170 01818000
@EF00016 DS    0H                                                  1170 01819000
@ER00016 BR    @14                                                 1170 01820000
*MDFNTR: PROC OPTIONS(NOSAVE,NOSAVEAREA);                          1171 01821000
MDFNTR   DS    0H                                                  1172 01822000
*/********************************************************************/ 01823000
*/*                                                                  */ 01824000
*/*     MDFNTR    THIS SUBRTN TEST FOR DUPLICATE NOTRACE KEYWORDS    */ 01825000
*/*               IT ISSUES A MSG AND RETURNS WITHOUT SETTING THE    */ 01826000
*/*               LOOPSW ON IF THERE IS A DUP OTHERWISE IT TURNS ON  */ 01827000
*/*               THE LOOPSW, THE IDREQ SW, THE TYPEREQ SW, AND      */ 01828000
*/*               INDICATES NOTRACE IN THE NCSPL.                    */ 01829000
*/*                                                                  */ 01830000
*/********************************************************************/ 01831000
*   R14SVKWD=R14;                     /*SAVE RETURN REG      @D32CKBN*/ 01832000
         ST    R14,R14SVKWD                                        1172 01833000
*   IF NCSVBF10=ON                     /*WAS THERE A PRIOR NO-TRACE   * 01834000
*        THEN DO;                      /*YES THERE WAS A PRIOR NO TRCE* 01835000
         L     @08,NCSPLPTR                                        1173 01836000
         TM    NCSVBF10(@08),B'01000000'                           1173 01837000
         BNO   @RF01173                                            1173 01838000
*             MSGCODE='096I';          /*SET MSG CODE FOR MULTI NO-TRC* 01839000
         MVC   MSGCODE(4),@CC03495                                 1175 01840000
*             MSGTXT(9:16)='NOTRACE '; /*SET UP MSG TEXT              * 01841000
         MVC   MSGTXT+8(8),@CC03509                                1176 01842000
*             CALL MSGS;               /*ISSUE MULTI NOTRACE MSG      * 01843000
         BAL   @14,MSGS                                            1177 01844000
*        END;                          /*END OF MULTI NO-TRACE CODE   * 01845000
*        ELSE DO;                      /*NO PRIOR NO-TRACE            * 01846000
         B     @RC01173                                            1179 01847000
@RF01173 DS    0H                                                  1180 01848000
*             TYPEREQ=ON;              /*INDICATE TYPE REQUIRED*/  1180 01849000
         OI    TYPEREQ,B'00100000'                                 1180 01850000
*             NCSVBF10=ON;             /*SET NO-TRACE BIT ON          * 01851000
