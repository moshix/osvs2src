         OI    NCSVBF10(@08),B'01000000'                           1181 01853000
*             LOOPSW=ON;               /*SET SW TO STAY IN DO WHILE   * 01854000
         OI    LOOPSW,B'00000010'                                  1182 01855000
*        END;                          /*END NO PRIOR NO-TRACE CODE   * 01856000
*   R14=R14SVKWD;                     /*RESTORE RETURN REG   @D32CKBN*/ 01857000
