*/*                                                      */        1259 02516000
*/*     TRACE PARM ROUTINE - TO VALIDATE TRACE SUBPARMS, */        1259 02517000
*/*     GO TO PROPER KEYWORD RTN FOR EACH, AND BUILD THE */        1259 02518000
*/*     TRACE PARM LIST. CCCERROR IS SET ON EXIT IF ANY  */        1259 02519000
*/*     ERRORS WERE FOUND AND NO PARM LIST IS BUILT.     */        1259 02520000
*/*     R1 = ADDR(KEYWORD) FOLLOWING TRACE/NOTRACE       */        1259 02521000
*/*     R2 = ADDR(CCCOAREA) OUTPUT AREA FOR ISTINCPH     */        1259 02522000
*/*     TRACE/NOTRACE,ID=NODENAME,TYPE=IO|LINE|BUF.      */        1259 02523000
*/*                                                      */        1259 02524000
*/********************************************************/        1259 02525000
*                                                                  1259 02526000
*                                                                  1259 02527000
* TRCPARM: PROC OPTIONS(NOSAVE,NOSAVEAREA);                /*@X50BHBE*/ 02528000
*                                                                  1259 02529000
TRCPARM  DS    0H                                                  1260 02530000
*   SAV14D = R14;               /*SAVE RETURN REG            @X50BHBE*/ 02531000
         ST    R14,SAV14D                                          1260 02532000
*   CCCTRCFG = '0'B;               /* INITIALIZE TRACE FLAGS */    1261 02533000
         MVI   CCCTRCFG,B'00000000'                                1261 02534000
*   CCCTRCF1='00'X;      /*INITIALIZE TRACE FLAGS*/                1262 02535000
         MVI   CCCTRCF1,X'00'                                      1262 02536000
*   CCCSUBPM = OFF;                /* INITIALIZE LOOP SWITCH */    1263 02537000
*                                                                  1263 02538000
         NI    CCCSUBPM,B'10111111'                                1263 02539000
*   DO WHILE CCCSUBPM = OFF;     /* SCAN UNTIL REACH END OF SUBPARMS*/  02540000
*                                                                  1264 02541000
         B     @DE01264                                            1264 02542000
@DL01264 DS    0H                                                  1265 02543000
*   R0 = ADDR(CCCTRCPM);      /*POINT TO TRACE SUBPARM TABLE @X50BHBE*/ 02544000
         LA    R0,CCCTRCPM                                         1265 02545000
*   TRSUBPTR = CCCPHOUT;           /* SAVE PTR TO BEG CURRENT SUBPM*/   02546000
