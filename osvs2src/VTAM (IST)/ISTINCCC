*  BUFPROC:  PROC OPTIONS(NOSAVE,NOSAVEAREA);              /*@X50BHBE*/ 03648000
BUFPROC  DS    0H                                                  1677 03649000
*    SAV14D = R14;  /* SAVE CALLER'S RETURN ADDRESS          @X50BHBE*/ 03650000
*                                                                  1677 03651000
         ST    R14,SAV14D                                          1677 03652000
*          CCCBUFER = OFF;              /* INIT TO NO ERROR FOUND */    03653000
*                                                                  1678 03654000
         NI    CCCBUFER,B'11111011'                                1678 03655000
*          IF PHRF34NS = ZERO | PHRF34NS > CCCSIZE /*NO SUBPRM OR MORE* 03656000
*            THEN CCCBUFER = ON;            /*PARMS THAN ALLOWED */     03657000
*                                                                  1679 03658000
         L     @11,PHRF34NS(,R2)                                   1679 03659000
         LTR   @11,@11                                             1679 03660000
         BZ    @RT01679                                            1679 03661000
         SLR   @08,@08                                             1679 03662000
