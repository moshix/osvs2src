*/*        MAXNODE PARM HANDLING ROUTINE                             */ 04175000
*/*                                                                  */ 04176000
*/********************************************************************/ 04177000
*MAXNODR: PROC OPTIONS(NOSAVE,NOSAVEAREA);                 /*@X50BHBE*/ 04178000
MAXNODR  DS    0H                                                  1863 04179000
*   SAV14C = R14;  /* SAVE CALLER'S RETURN ADDRESS           @X50BHBE*/ 04180000
         ST    R14,SAV14C                                          1863 04181000
*  DCL MASK1 BIT(16) INIT('1000000000000000'B);/*MASK FOR XRANGE=1*/    04182000
*  DCL MASK2 BIT(16) INIT('1100000000000000'B);/*MASK FOR XRANGE=2*/    04183000
*  DCL MASK3 BIT(16) INIT('1110000000000000'B);/*MASK FOR XRANGE=3*/    04184000
*  DCL MASK4 BIT(16) INIT('1111000000000000'B);/*MASK FOR XRANGE=4*/    04185000
*  DCL MASK5 BIT(16) INIT('1111100000000000'B);/*MASK FOR XRANGE=5*/    04186000
*  DCL MASK6 BIT(16) INIT('1111110000000000'B);/*MASK FOR XRANGE=6*/    04187000
*  DCL MASK7 BIT(16) INIT('1111111000000000'B);/*MASK FOR XRANGE=7*/    04188000
*  DCL MASK8 BIT(16) INIT('1111111100000000'B);/*MASK FOR XRANGE=8*/    04189000
*    IF PHRF7NSP=1 & PHRF7NUM(1)^=0/*ONLY ONE SUB PARM AND NOT ZERO   * 04190000
*       THEN DO;                   /*YES CONTINUE                     * 04191000
         CLC   PHRF7NSP(4,R2),@CF00040                             1872 04192000
         BNE   @RF01872                                            1872 04193000
         CLI   PHRF7NUM(R2),0                                      1872 04194000
         BE    @RF01872                                            1872 04195000
*          IF PHRF7NUM(1)<='01'X   /*WAS THE MAXID PARM 1             * 04196000
*           THEN DO;               /*YES PROCESS MAXID OF ONE         * 04197000
         CLI   PHRF7NUM(R2),1                                      1874 04198000
         BH    @RF01874                                            1874 04199000
*             ATCXRANG='01'X;      /*SET XRANGE                       * 04200000
         L     @11,CCCVTPTR                                        1876 04201000
         MVI   ATCXRANG(@11),X'01'                                 1876 04202000
*             ATCMAXID='01'X;      /*SET MAXID                        * 04203000
         MVI   ATCMAXID(@11),X'01'                                 1877 04204000
*             ATCCIDM=MASK1;       /*SET MASK BITS                    * 04205000
         MVC   ATCCIDM(2,@11),MASK1                                1878 04206000
*           END;                   /*END MAXID ONE CODE               * 04207000
*           ELSE DO;                /*NOT ONE                         * 04208000
         B     @RC01874                                            1880 04209000
@RF01874 DS    0H                                                  1881 04210000
*            IF PHRF7NUM(1)<='03'X  /*WAS THE MAXID PARM 3 OR LESS    * 04211000
*             THEN DO;              /*YES PROCESS MAXID OF THREE      * 04212000
         CLI   PHRF7NUM(R2),3                                      1881 04213000
         BH    @RF01881                                            1881 04214000
*                ATCXRANG='02'X;    /*SET XRANGE                      * 04215000
         L     @11,CCCVTPTR                                        1883 04216000
         MVI   ATCXRANG(@11),X'02'                                 1883 04217000
*                ATCMAXID='03'X;    /*SET MAXID                       * 04218000
         MVI   ATCMAXID(@11),X'03'                                 1884 04219000
*                ATCCIDM=MASK2;     /*SET MASK BITS                   * 04220000
         MVC   ATCCIDM(2,@11),MASK2                                1885 04221000
*             END;                  /*END MAXID OF THREE CODE         * 04222000
*             ELSE DO;              /*NOT THREE                       * 04223000
         B     @RC01881                                            1887 04224000
@RF01881 DS    0H                                                  1888 04225000
*              IF PHRF7NUM(1)<='07'X /*WAS THE MAXID PARM 7 OR LESS   * 04226000
*               THEN DO;              /*YES PROCESS MAXID OF SEVEN    * 04227000
         CLI   PHRF7NUM(R2),7                                      1888 04228000
         BH    @RF01888                                            1888 04229000
*                 ATCXRANG='03'X;     /*SET XRANGE                    * 04230000
         L     @11,CCCVTPTR                                        1890 04231000
         MVI   ATCXRANG(@11),X'03'                                 1890 04232000
*                 ATCMAXID='07'X;     /*SET MAXID                     * 04233000
         MVI   ATCMAXID(@11),X'07'                                 1891 04234000
*                 ATCCIDM=MASK3;      /*SET MASK BITS                 * 04235000
         MVC   ATCCIDM(2,@11),MASK3                                1892 04236000
*               END;                /*END MAXID OF SEVEN CODE         * 04237000
*               ELSE DO;            /*NOT SEVEN                       * 04238000
