         BNH   @RF01679                                            1679 03665000
@RT01679 DS    0H                                                  1680 03666000
         OI    CCCBUFER,B'00000100'                                1680 03667000
*            ELSE                                                  1681 03668000
*             DO;                                                  1681 03669000
         B     @RC01679                                            1681 03670000
@RF01679 DS    0H                                                  1682 03671000
*              IF PHRF34NS=FOUR    /*WAS USERFIX SPECIFIED?*/      1682 03672000
*                THEN BUFPRMNO=THREE;/*YES-CHECK BUFF VALUES IN LOOP*/  03673000
         CLC   PHRF34NS(4,R2),@CF00191                             1682 03674000
         BNE   @RF01682                                            1682 03675000
         MVC   BUFPRMNO(4),@CF00128                                1683 03676000
*                ELSE BUFPRMNO=PHRF34NS; /*SET LOOP TO # VALS ENTERED*/ 03677000
         B     @RC01682                                            1684 03678000
@RF01682 L     @11,PHRF34NS(,R2)                                   1684 03679000
         ST    @11,BUFPRMNO                                        1684 03680000
*CCCDOVVV:        DO CCCNDEX = ONE TO BUFPRMNO /*PROCESS ALL SUBPRMS*/  03681000
*                 WHILE (CCCBUFER = OFF);      /*GIVEN OR UNTIL AN */   03682000
@RC01682 DS    0H                                                  1685 03683000
