*DCL ZCTYPE BIT(8) CONSTANT('00'X);   /* ZCR TYPE                   */  04799000
*DCL ZCLNG FIXED(15) CONSTANT(172);   /* ZCRR LENGTH                */  04800000
*DCL ZCRSAVNO BIT(8) CONSTANT('06'X); /* NUMBER OF SAVEAREAS IN ZCRR */ 04801000
*DCL ZCRLSVNO BIT(8) CONSTANT('04'X); /* NUMBER OF SAVE AREAS IN   2462 04802000
*                                         LOCAL SUPPORT              */ 04803000
*/* *** EQUATES FOR ZCRRCDSV *** */                                2463 04804000
*                                                                  2463 04805000
*DCL ZCRMRFSB BIT(8) CONSTANT('0C'X);  /* REQUEST COMPLETION MOVE FSB   04806000
*                                         TO PROCESS                 */ 04807000
*DCL ZCRSRCHQ BIT(8) CONSTANT('10'X);  /* REQUEST COMPLETION SEARCH     04808000
*                                         REQUEST QUEUE              */ 04809000
*DCL ZCREXIT  BIT(8) CONSTANT('14'X);  /* REQUEST COMPLETION EXIT    */ 04810000
*DCL ZCRSFSB  BIT(8) CONSTANT('18'X);  /* REQUEST COMPLETION SINGLE     04811000
*                                         FSB                        */ 04812000
*DCL ZCRDLFSB BIT(8) CONSTANT('1C'X);  /* REQUEST COMPLETION DUAL FSB * 04813000
*DCL ZCRNOSCH BIT (8) CONSTANT('20'X); /* REQUEST COMPLETION, EXIT WITH 04814000
*                                         SCHED NONE                 */ 04815000
*DCL ZCRNRSCH BIT (8) CONSTANT('24'X); /* REQUEST COMPLETION, EXIT WITH 04816000
*                                         SCHED NORM                 */ 04817000
