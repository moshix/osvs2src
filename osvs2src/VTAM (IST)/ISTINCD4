*    END;                                                       01S0093 00052000
*   GEN REFS(CD4WAPTR)(  USING @DATD,CD4WAPTR);                 01S0094 00053000
           USING @DATD,CD4WAPTR                                         00054000
*   RFY                                                         01S0095 00055000
*     CD4R15 RSTD;                                              01S0095 00056000
*   CD4R15=REGSAPTR;                /* SAVE THE CALLER'S REGISTER       00057000
*                                      SAVEAREA PTR                  */ 00058000
         LR    CD4R15,REGSAPTR                                  01S0096 00059000
*   REGSAPTR=REGSAPTR-LENGTH(CD4REGSV);/* DECREMENT REG 13 BY 72     */ 00060000
         SL    REGSAPTR,@CF00945                                01S0097 00061000
*   CD4SVBKP=CD4R15;                /* POINT THIS ROUTINE'S SAVEAREA    00062000
