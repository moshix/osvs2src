*     DO;                           /* YES - GROUP RDTE FOUND        */ 00226000
*      GROUPTR=CD4R0;               /* GET THE GROUP RDTE ADDRESS    */ 00227000
         ST    CD4R0,GROUPTR                                    03S0149 00228000
*      RFY (CD4R0,CD4R2,CD4R15) UNRSTD;                         03S0150 00229000
*      RFY (ISTRGP,ISTRPRE) BASED(GROUPTR);                     03S0151 00230000
