         L     R8SWRDT,RPRNSNEA(,R6PURDT)                          0118 00076000
*                                  /*SUBAREA HDR RDT - SAVE*/      0119 00077000
*                                                                  0119 00078000
*IF RPRENTED = ON                  /*IF GRP IS LAST ENTRY IN SEGMENT */ 00079000
*                                  /*NO LINKS TO USE-FAIL DIAL REQST*/  00080000
*  THEN CALL D5REDIAL;                                             0119 00081000
         TM    RPRENTED(R7WRKRDT),B'00000100'                      0119 00082000
         BNO   @RF00119                                            0119 00083000
         BAL   @14,D5REDIAL                                        0120 00084000
*  ELSE DO;                        /*GROUP HAS AT LEAST ONE LINK*/ 0121 00085000
         B     @RC00119                                            0121 00086000
@RF00119 DS    0H                                                  0122 00087000
*     CD5TRYNX = ON;               /*INSURE PROCESS FIRST ONE*/    0122 00088000
         OI    CD5TRYNX,B'10000000'                                0122 00089000
*     R7WRKRDT = R7WRKRDT + RPRELEN;  /*GET ADDR OF LINK*/         0123 00090000
*                                                                  0123 00091000
         AH    R7WRKRDT,RPRELEN(,R7WRKRDT)                         0123 00092000
*        /**********************************************/          0124 00093000
*        /*  LOOP THRU LINKS UNDER THIS GROUP UNTIL    */          0124 00094000
*        /*  FIND ONE TO USE OR ENCOUNTER FAIL         */          0124 00095000
*        /*  CONDITION OR REACH END OF THE GROUP       */          0124 00096000
*        /**********************************************/          0124 00097000
*                                                                  0124 00098000
*     DO WHILE CD5TRYNX = ON;                                      0124 00099000
         B     @DE00124                                            0124 00100000
