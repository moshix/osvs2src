SVTM     TITLE ' SSCP -- ISTINCD5 -- DIAL LINK START                   *00001000
                        '                                               00002000
ISTINCD5 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCD5  75.128'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*      /* MAINLINE CODE OF ISTINCD5 -- DIAL LINK START */          0090 00014000
*                                                                  0090 00015000
* DO;                                                              0090 00016000
*/***  CRRMAINT  PROCNM(ISTINCD5)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00017000
**/                                                                0091 00018000
*                                                                  0091 00019000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0091 00020000
* RFY ISTLCCW ;                                                    0091 00021000
* RFY ISTLCPB ;                                                    0092 00022000
* RFY ISTZSAVE;                                                    0093 00023000
* RFY ISTPFCRR;                                                    0094 00024000
* RFY ISTCFCRR;                                                    0095 00025000
* RFY ISTZCRR ;                                                    0096 00026000
* RFY ISTASCRR;                                                    0097 00027000
* RFY ISTAPCRR;                                                    0098 00028000
* RFY ISTCRA  ;                                                    0099 00029000
* RFY ISTCNSTA;                                                    0100 00030000
* RFY ISTVYCRR;                                                    0101 00031000
*/* END OF ATBUILD */                                              0102 00032000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0102 00033000
*NCSPLRPH->RPHCRR->CRACRR->NINCD5 = '1'B;                          0102 00034000
         L     @09,NCSPLRPH(,R1)                                   0102 00035000
         L     @09,RPHCRR(,@09)                                    0102 00036000
         L     @09,CRACRR(,@09)                                    0102 00037000
         OI    NINCD5(@09),B'00000010'                             0102 00038000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0103 00039000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0103 00040000
         XC    VYCCRT+1(11,@09),VYCCRT+1(@09)                      0103 00041000
         MVI   VYCCRT(@09),B'00000000'                             0103 00042000
*NCSPLRPH->RPHCRR->CRACRR->CINCD5 = '1'B;                          0104 00043000
         OI    CINCD5(@09),B'00000010'                             0104 00044000
* END;                                                             0105 00045000
*GEN REFS(CD5WAPTR) (   USING  @DATD,CD5WAPTR);                    0106 00046000
            USING  @DATD,CD5WAPTR                                       00047000
*R15=REGSAPTR;   /* SAVE THE CALLER'S REGISTER SAVEAREA POINTER */ 0107 00048000
         LR    R15,REGSAPTR                                        0107 00049000
*REGSAPTR=REGSAPTR-LENGTH(CD5REGSV); /* DECREMENT REG 13 BY 72 */  0108 00050000
         SL    REGSAPTR,@CF01005                                   0108 00051000
*CD5SVBKP=R15;   /* POINT THIS ROUTINE'S SAVEAREA TO THE CALLER'S */    00052000
