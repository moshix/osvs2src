         ST    REGSAPTR,CD5SVFWP(,R15)                             0111 00058000
*CD5WAPTR=NXTAVAIL;/* GET THE NEXT AVAILABLE ADDRESS IN THE WORKAREA*/  00059000
         L     @09,NCSPLWKA(,R1)                                   0112 00060000
         L     CD5WAPTR,NXTAVAIL(,@09)                             0112 00061000
*NXTAVAIL=CD5WAPTR+@SIZDATD; /* RESET NEXT AVAILABLE BEYOND THIS   0113 00062000
*                                   ROUTINE'S DYNAMIC AREA */      0113 00063000
         LR    @05,CD5WAPTR                                        0113 00064000
         AL    @05,@SIZDATD                                        0113 00065000
         ST    @05,NXTAVAIL(,@09)                                  0113 00066000
*NCSPLPTR=R1;                       /* GET THE NCSPL ADDRESS */    0114 00067000
         LR    NCSPLPTR,R1                                         0114 00068000
*RFY ISTNCSPL BASED(NCSPLPTR);                                     0115 00069000
*                                                                  0115 00070000
*R7WRKRDT = NCSPLRPT;              /*ENTERED WITH GROUP RDTE ADDR*/     00071000
         L     R7WRKRDT,NCSPLRPT(,NCSPLPTR)                        0116 00072000
