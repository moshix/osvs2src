SVTM     TITLE ' SSCP -- ISTINCD6 -- DIAL ALLOCATE                     *00001000
                        '                                               00002000
ISTINCD6 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTINCD6  75.123'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                     01S0001 00010000
         STM   @00,@12,20(@13)                                  01S0001 00011000
         BALR  @12,0                                            01S0001 00012000
@PSTART  DS    0H                                               01S0001 00013000
         USING @PSTART,@12                                      01S0001 00014000
*                                                               01S0044 00015000
*   /*****************************************************************/ 00016000
*   /*                                                               */ 00017000
*   /* MAINLINE CODE OF ISTINCD6 -- DIAL ALLOCATE                    */ 00018000
*   /*                                                               */ 00019000
*   /*****************************************************************/ 00020000
*                                                               01S0044 00021000
*                                                               01S0044 00022000
*    DO;                                                        01S0044 00023000
*/***  CRRMAINT  PROCNM(ISTINCD6)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00024000
**/                                                             01S0045 00025000
*                                                               01S0045 00026000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */               01S0045 00027000
*    RFY ISTRPH  ;                                              01S0045 00028000
*    RFY ISTLCCW ;                                              01S0046 00029000
*    RFY ISTLCPB ;                                              01S0047 00030000
*    RFY ISTZSAVE;                                              01S0048 00031000
*    RFY ISTPFCRR;                                              01S0049 00032000
*    RFY ISTCFCRR;                                              01S0050 00033000
*    RFY ISTZCRR ;                                              01S0051 00034000
*    RFY ISTASCRR;                                              01S0052 00035000
*    RFY ISTAPCRR;                                              01S0053 00036000
*    RFY ISTCRA  ;                                              01S0054 00037000
*    RFY ISTCNSTA;                                              01S0055 00038000
*    RFY ISTVYCRR;                                              01S0056 00039000
*/* END OF ATBUILD */                                           01S0057 00040000
*    /*** SET ENTERED BIT FOR THIS PROCEDURE ***/               01S0057 00041000
*NCSPLRPH->RPHCRR->CRACRR->NINCD6 = '1'B;                       01S0057 00042000
         L     @08,NCSPLRPH(,R1)                                01S0057 00043000
         L     @08,RPHCRR(,@08)                                 01S0057 00044000
         L     @08,CRACRR(,@08)                                 01S0057 00045000
         OI    NINCD6(@08),B'00000001'                          01S0057 00046000
*    /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/              01S0058 00047000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                       01S0058 00048000
         XC    VYCCRT+1(11,@08),VYCCRT+1(@08)                   01S0058 00049000
         MVI   VYCCRT(@08),B'00000000'                          01S0058 00050000
*NCSPLRPH->RPHCRR->CRACRR->CINCD6 = '1'B;                       01S0059 00051000
         OI    CINCD6(@08),B'00000001'                          01S0059 00052000
*    END;                                                       01S0060 00053000
*                                                               01S0060 00054000
*   /*****************************************************************/ 00055000
*   /*                                                               */ 00056000
*   /* NOTE: THIS ROUTINE DOES NOT ESTABLISH A REGISTER SAVEAREA     */ 00057000
*   /* BECAUSE NO EXTERNAL ROUTINES ARE CALLED                       */ 00058000
*   /*                                                               */ 00059000
*   /*****************************************************************/ 00060000
*                                                               01S0061 00061000
*   GEN REFS(CD6WAPTR)(   USING  @DATD,CD6WAPTR);               01S0061 00062000
            USING  @DATD,CD6WAPTR                                       00063000
*   CD6WAPTR=NXTAVAIL;              /* GET THE NEXT AVAILABLE ADDRESS   00064000
*                                      IN THE WORKAREA               */ 00065000
         L     @08,NCSPLWKA(,R1)                                01S0062 00066000
         L     CD6WAPTR,NXTAVAIL(,@08)                          01S0062 00067000
*   CD6WAPTR=(CD6WAPTR+SEVEN)&MINUS8;/* RESET POINTER TO A DOUBLE       00068000
*                                      WORD BOUNDRY                  */ 00069000
         LA    @06,7                                            01S0063 00070000
         ALR   @06,CD6WAPTR                                     01S0063 00071000
         N     @06,@CF00739                                     01S0063 00072000
         LR    CD6WAPTR,@06                                     01S0063 00073000
*   NXTAVAIL=CD6WAPTR+@SIZDATD;     /* RESET NEXT AVAILABLE BEYOND      00074000
*                                      THIS ROUTINE'S DYNAMIC AREA   */ 00075000
         LR    @06,CD6WAPTR                                     01S0064 00076000
         AL    @06,@SIZDATD                                     01S0064 00077000
         ST    @06,NXTAVAIL(,@08)                               01S0064 00078000
*   NCSPLPTR=R1;                    /* GET THE NCSPL ADDRESS         */ 00079000
         LR    NCSPLPTR,R1                                      01S0065 00080000
*   RFY                                                         01S0066 00081000
*     ISTNCSPL BASED(NCSPLPTR);                                 01S0066 00082000
*   PXPTR=NCSPLRPT+NCSPLRPT->RPRELEN;/* GET THE PX ENTRY ASSOCIATED     00083000
*                                      WITH THE LINK                 */ 00084000
         L     @08,NCSPLRPT(,NCSPLPTR)                          01S0067 00085000
         LH    PXPTR,RPRELEN(,@08)                              01S0067 00086000
         ALR   PXPTR,@08                                        01S0067 00087000
*   PUPTR=NCSPLRAD;                 /* GET THE ADDRESS OF PU THAT       00088000
*                                      DIALED IN                     */ 00089000
         L     PUPTR,NCSPLRAD(,NCSPLPTR)                        01S0068 00090000
*   IF RCCCTELU=ZERO                /* ANY LUS ASSOCIATED WITH THIS     00091000
*                                      PU?                           */ 00092000
*     THEN                                                      01S0069 00093000
         LH    @08,RCCCTELU(,PUPTR)                             01S0069 00094000
         LTR   @08,@08                                          01S0069 00095000
         BNZ   @RF00069                                         01S0069 00096000
*     RETCODE=ZERO;                 /* NO - SET ALLOCATED RESPONSE   */ 00097000
         MVI   RETCODE,X'00'                                    01S0070 00098000
*   ELSE                                                        01S0071 00099000
*     IF RPXMAXLU<RCCCTELU          /* THIS PX SUPPORT A SUFFICIENT     00100000
*                                      NUMBER OF LXS?                */ 00101000
*       THEN                                                    01S0071 00102000
         B     @RC00069                                         01S0071 00103000
@RF00069 SLR   @08,@08                                          01S0071 00104000
         IC    @08,RPXMAXLU(,PXPTR)                             01S0071 00105000
         CH    @08,RCCCTELU(,PUPTR)                             01S0071 00106000
         BNL   @RF00071                                         01S0071 00107000
*       RETCODE=FOUR;               /* NO - SET INSUFFICIENT MAXLU      00108000
*                                      RESPONSE                      */ 00109000
         MVI   RETCODE,X'04'                                    01S0072 00110000
*     ELSE                                                      01S0073 00111000
*       DO;                                                     01S0073 00112000
         B     @RC00071                                         01S0073 00113000
@RF00071 DS    0H                                               01S0074 00114000
*         LXPTR=NCSPLRPT;           /* GET THE LINK RDTE ADDRESS     */ 00115000
         L     LXPTR,NCSPLRPT(,NCSPLPTR)                        01S0074 00116000
*         DO WHILE LXPTR->RPRENTRY^=LXPTR->RPRHDTYP;/* LOOK FOR THE     00117000
*                                      370X                          */ 00118000
         B     @DE00075                                         01S0075 00119000
@DL00075 DS    0H                                               01S0076 00120000
*           LXPTR=LXPTR->RPRNSNEA;  /* GET NEXT HIGHER NODE          */ 00121000
         L     LXPTR,RPRNSNEA(,LXPTR)                           01S0076 00122000
*         END;                                                  01S0077 00123000
@DE00075 CLC   RPRENTRY(1,LXPTR),RPRHDTYP(LXPTR)                01S0077 00124000
         BNE   @DL00075                                         01S0077 00125000
*         LXPTR=LXPTR->RRNFLUX;     /* GET THE LX POOL ADDRESS       */ 00126000
         L     LXPTR,RRNFLUX(,LXPTR)                            01S0078 00127000
*         IF LXPTR=ZERO             /* LX POOL EXIST?                */ 00128000
*           THEN                                                01S0079 00129000
         LTR   LXPTR,LXPTR                                      01S0079 00130000
         BNZ   @RF00079                                         01S0079 00131000
*           RETCODE=EIGHT;          /* NO - SET INSUFFICIENT LX 01S0080 00132000
*                                      RESPONSE                      */ 00133000
         MVI   RETCODE,X'08'                                    01S0080 00134000
*         ELSE                                                  01S0081 00135000
*           DO;                                                 01S0081 00136000
         B     @RC00079                                         01S0081 00137000
@RF00079 DS    0H                                               01S0082 00138000
*/********************************************************************/ 00139000
*/*                                                                  */ 00140000
*/* START OF THE LU TO LUX ALLOCATION LOOP                           */ 00141000
*/*                                                                  */ 00142000
*/********************************************************************/ 00143000
*                                                               02S0082 00144000
*RETCODE=EF;                       /* INITIALIZE RETURN CODE OF X'EF'*/ 00145000
         MVI   RETCODE,X'EF'                                    02S0082 00146000
*LXALLOC=ZERO;                      /* INITIALIZE THE ALLOCATED LX      00147000
*                                      COUNTER TO ZERO               */ 00148000
         MVI   LXALLOC,X'00'                                    02S0083 00149000
*SAVLXPTR=ZERO;                     /* INITIALIZE PREVIOUS LX POINTER   00150000
*                                      TO ZERO                       */ 00151000
         SLR   @08,@08                                          02S0084 00152000
         ST    @08,SAVLXPTR                                     02S0084 00153000
*DO WHILE RETCODE=EF;               /* START OUTER ALLOCATION LOOP   */ 00154000
         B     @DE00085                                         02S0085 00155000
@DL00085 DS    0H                                               02S0086 00156000
* IF RLXINUSE=OFF                   /* LX AVAILABLE?                 */ 00157000
*  THEN                                                         02S0086 00158000
         TM    RLXINUSE(LXPTR),B'10000000'                      02S0086 00159000
         BNZ   @RF00086                                         02S0086 00160000
*   DO;                             /* YES - LX IS AVAILABLE         */ 00161000
*    RPXFLUX=LXPTR;                 /* PUT FIRST LX ADDRESS IN PX    */ 00162000
         ST    LXPTR,RPXFLUX(,PXPTR)                            02S0088 00163000
*    LUPTR=PUPTR+RPRELEN;           /* GET FIRST LU ASSOCIATED WITH     00164000
*                                      THE PU                        */ 00165000
         LR    LUPTR,PUPTR                                      02S0089 00166000
         AH    LUPTR,RPRELEN(,PUPTR)                            02S0089 00167000
*    DO WHILE RCCCTELU^=LXALLOC&RETCODE=EF; /* START INNER      02S0090 00168000
*                                      ALLOCATION LOOP               */ 00169000
         B     @DE00090                                         02S0090 00170000
@DL00090 DS    0H                                               02S0091 00171000
*     IF SAVLXPTR=ZERO              /* ANY LXS ALREADY ALLOCATED?    */ 00172000
*      THEN                                                     02S0091 00173000
         L     @08,SAVLXPTR                                     02S0091 00174000
         LTR   @08,@08                                          02S0091 00175000
         BNZ   @RF00091                                         02S0091 00176000
*       SAVLXPTR=LXPTR;             /* NO - SAVE THIS LX'S ADDRESS      00177000
*                                      IN CASE ANOTHER LX IS ALLOCATED  00178000
*                                      AND THIS LX WILL THEN HAVE TO    00179000
*                                      HAVE ITS FORWARD POINTER CHAINED 00180000
*                                      TO THE NEXT ALLOCATED LX      */ 00181000
         ST    LXPTR,SAVLXPTR                                   02S0092 00182000
*      ELSE                                                     02S0093 00183000
*       DO;                         /* YES - AN LX WAS PREVIOUSLY       00184000
*                                      ALLOCATED.  POINT THAT LX'S      00185000
*                                      FORWARD POINTER TO THIS LX    */ 00186000
         B     @RC00091                                         02S0093 00187000
@RF00091 DS    0H                                               02S0094 00188000
*        SAVLXPTR->RLXNLUX=LXPTR;   /* CHAIN THE PREVIOUSLY ALLOCATED   00189000
*                                      LX TO THIS LX                 */ 00190000
         L     @08,SAVLXPTR                                     02S0094 00191000
         ST    LXPTR,RLXNLUX(,@08)                              02S0094 00192000
*        SAVLXPTR=LXPTR;            /* SAVE THIS LX'S ADDRESS IN        00193000
*                                      CASE ANOTHER LX IS ALLOCATED  */ 00194000
         ST    LXPTR,SAVLXPTR                                   02S0095 00195000
*       END;                                                    02S0096 00196000
*     RLXINUSE=ON;                  /* MARK LX IN USE                */ 00197000
@RC00091 OI    RLXINUSE(LXPTR),B'10000000'                      02S0097 00198000
*     RLXNLUX=ZERO;                 /* ZERO OUT NEXT LX POINTER -       00199000
*                                      IN CASE THIS IS THE LAST LX TO   00200000
*                                      BE ALLOCATED                  */ 00201000
         SLR   @08,@08                                          02S0098 00202000
         ST    @08,RLXNLUX(,LXPTR)                              02S0098 00203000
*     LUPTR->RPRSWDAF=LXPTR->RPRDAF;/* COPY DAF OF LX TO LU          */ 00204000
         MVC   RPRSWDAF(2,LUPTR),RPRDAF(LXPTR)                  02S0099 00205000
*     LXPTR->RPRSWDAF=LUPTR->RPRDAF;/* COPY DAF OF LU TO LX          */ 00206000
         MVC   RPRSWDAF(2,LXPTR),RPRDAF(LUPTR)                  02S0100 00207000
*     LXALLOC=LXALLOC+ONE;          /* INCREMENT THE ALLOCATED  02S0101 00208000
*                                      LX COUNTER BY ONE             */ 00209000
         LA    @08,1                                            02S0101 00210000
         SLR   @04,@04                                          02S0101 00211000
         IC    @04,LXALLOC                                      02S0101 00212000
         ALR   @08,@04                                          02S0101 00213000
         STC   @08,LXALLOC                                      02S0101 00214000
*     IF RCCCTELU=LXALLOC           /* ALL LXS ALLOCATED?            */ 00215000
*      THEN                                                     02S0102 00216000
         CH    @08,RCCCTELU(,PUPTR)                             02S0102 00217000
         BNE   @RF00102                                         02S0102 00218000
*       RETCODE=ZERO;               /* YES - LXS ALLOCATED, EXIT        00219000
*                                      FROM LOOP                     */ 00220000
         MVI   RETCODE,X'00'                                    02S0103 00221000
*      ELSE                                                     02S0104 00222000
*       IF LXPTR->RPRENTED=ON       /* LAST LX IN THE SEGMENT?       */ 00223000
*        THEN                       /* YES - LAST LX IN THE SEGMENT  */ 00224000
         B     @RC00102                                         02S0104 00225000
@RF00102 TM    RPRENTED(LXPTR),B'00000100'                      02S0104 00226000
         BNO   @RF00104                                         02S0104 00227000
*         CALL DECHAIN;             /* DEALLOCATE ALL ALLOCATED LXS  */ 00228000
         BAL   @14,DECHAIN                                      02S0105 00229000
*        ELSE                                                   02S0106 00230000
*         DO;                       /* NOT LAST LX IN SEGMENT        */ 00231000
         B     @RC00104                                         02S0106 00232000
@RF00104 DS    0H                                               02S0107 00233000
*          DO WHILE RLXINUSE=ON&LXPTR->RPRENTED=OFF; /* LOOK FOR        00234000
*                                      NEXT AVAILABLE LX             */ 00235000
         B     @DE00107                                         02S0107 00236000
@DL00107 DS    0H                                               02S0108 00237000
*           LXPTR=LXPTR+LXPTR->RPRELEN;/* GET NEXT LX                */ 00238000
         AH    LXPTR,RPRELEN(,LXPTR)                            02S0108 00239000
*          END;                                                 02S0109 00240000
@DE00107 TM    RLXINUSE(LXPTR),B'10000000'                      02S0109 00241000
         BNO   @DC00107                                         02S0109 00242000
         TM    RPRENTED(LXPTR),B'00000100'                      02S0109 00243000
         BZ    @DL00107                                         02S0109 00244000
@DC00107 DS    0H                                               02S0110 00245000
*          IF RLXINUSE=ON           /* LX FOUND IN USE (I.E.: THE       00246000
*                                      LAST LX IN THE SEGMENT)?      */ 00247000
*           THEN                    /* YES - LAST LX FOUND IN USE    */ 00248000
         TM    RLXINUSE(LXPTR),B'10000000'                      02S0110 00249000
         BNO   @RF00110                                         02S0110 00250000
*            CALL DECHAIN;          /* DECHAIN THE LXS ALREADY  02S0111 00251000
*                                      ALLOCATED                     */ 00252000
         BAL   @14,DECHAIN                                      02S0111 00253000
*           ELSE                                                02S0112 00254000
*            LUPTR=LUPTR+LUPTR->RPRELEN;/* GET NEXT LU               */ 00255000
         B     @RC00110                                         02S0112 00256000
@RF00110 AH    LUPTR,RPRELEN(,LUPTR)                            02S0112 00257000
*         END;                      /* END OF NOT LAST LX IN SEGMENT */ 00258000
@RC00110 DS    0H                                               02S0114 00259000
*   END;                            /* END OF INNER ALLOCATION LOOP  */ 00260000
@RC00104 DS    0H                                               02S0114 00261000
@RC00102 DS    0H                                               02S0114 00262000
@DE00090 LH    @08,RCCCTELU(,PUPTR)                             02S0114 00263000
         SLR   @04,@04                                          02S0114 00264000
         IC    @04,LXALLOC                                      02S0114 00265000
         CR    @08,@04                                          02S0114 00266000
         BE    @DC00090                                         02S0114 00267000
         CLI   RETCODE,X'EF'                                    02S0114 00268000
         BE    @DL00090                                         02S0114 00269000
@DC00090 DS    0H                                               02S0115 00270000
*  END;                             /* END OF LX AVAILABLE           */ 00271000
*ELSE                               /* FIRST LX NOT AVAILABLE        */ 00272000
* IF LXPTR->RPRENTED=ON             /* THIS THE LAST LX?             */ 00273000
*  THEN                                                         02S0116 00274000
         B     @RC00086                                         02S0116 00275000
@RF00086 TM    RPRENTED(LXPTR),B'00000100'                      02S0116 00276000
         BNO   @RF00116                                         02S0116 00277000
*   RETCODE=EIGHT;                  /* YES - LAST LX - SET      02S0117 00278000
*                                      INSUFFICIENT LX RESPONSE      */ 00279000
         MVI   RETCODE,X'08'                                    02S0117 00280000
*  ELSE                                                         02S0118 00281000
*   LXPTR=LXPTR+LXPTR->RPRELEN;     /* GET THE NEXT LX               */ 00282000
         B     @RC00116                                         02S0118 00283000
@RF00116 AH    LXPTR,RPRELEN(,LXPTR)                            02S0118 00284000
*END;                               /* END OF OUTER ALLOCATION LOOP  */ 00285000
@RC00116 DS    0H                                               02S0119 00286000
@RC00086 DS    0H                                               02S0119 00287000
@DE00085 CLI   RETCODE,X'EF'                                    02S0119 00288000
         BE    @DL00085                                         02S0119 00289000
*           END;                                                01S0120 00290000
*       END;                        /* END OF PX SUPPORTS SUFFICIENT    00291000
*                                      NUMBER OF LXS                 */ 00292000
@RC00079 DS    0H                                               01S0122 00293000
*   IF RETCODE=ZERO                 /* LXS ALLOCATED?                */ 00294000
*     THEN                                                      01S0122 00295000
@RC00071 DS    0H                                               01S0122 00296000
@RC00069 CLI   RETCODE,0                                        01S0122 00297000
         BNE   @RF00122                                         01S0122 00298000
*     DO;                           /* YES - LXS ALLOCATED           */ 00299000
*       RPRNSNEA=NCSPLRPT;          /* CHANGE NSNEA OF PU TO POINT TO   00300000
*                                      THE LINK IN THE 370X SEGMENT     00301000
*                                      RATHER THAN THE SWITCHED 01S0124 00302000
*                                      SUBAREA HEADER                */ 00303000
         L     @08,NCSPLRPT(,NCSPLPTR)                          01S0124 00304000
         ST    @08,RPRNSNEA(,PUPTR)                             01S0124 00305000
*       RLNINUSE=ON;                /* MARK THE LINK IN USE          */ 00306000
         OI    RLNINUSE(@08),B'01000000'                        01S0125 00307000
*       PXPTR->RPRSWDAF=RPRDAF;     /* PUT PU'S DAF IN THE PX ENTRY  */ 00308000
         MVC   RPRSWDAF(2,PXPTR),RPRDAF(PUPTR)                  01S0126 00309000
*       RPRSWDAF=PXPTR->RPRDAF;     /* PUT PX'S DAF IN THE PU ENTRY  */ 00310000
         MVC   RPRSWDAF(2,PUPTR),RPRDAF(PXPTR)                  01S0127 00311000
*       RFY R2 RSTD;                /* RESTRICT REG 2 OVER CIDCTL    */ 00312000
*       R1=NCSPLRPH;                /* PUT RPH ADDRESS IN REG 1 FOR     00313000
*                                      CIDCTL                        */ 00314000
         L     R1,NCSPLRPH(,NCSPLPTR)                           01S0129 00315000
*       LOOP1=ON;                   /* INITIALIZE LOOP CONTROL SWITCH*/ 00316000
         OI    LOOP1,B'10000000'                                01S0130 00317000
*       DO WHILE LOOP1=ON;          /* LOOP THROUGH PU/LUS           */ 00318000
         B     @DE00131                                         01S0131 00319000
@DL00131 DS    0H                                               01S0132 00320000
*        R2=ZERO|RPRSWDAF;          /* GET DAF OF PUX/LUX            */ 00321000
         LH    R2,RPRSWDAF(,PUPTR)                              01S0132 00322000
         N     R2,@CF03722                                      01S0132 00323000
*        /* UPDATE PUX/LUX SNT ENTRY TO POINT TO PU/LU SNT ENTRY     */ 00324000
*                                                               01S0133 00325000
*         DO; /* CIDCTL(ADDSNTPT)  */                           01S0133 00326000
*                                                               01S0133 00327000
*         RFY (EMCR2,EMCR3) RSTD;                               01S0134 00328000
*                                                               01S0134 00329000
*         RFY ISTPAB  ;                                         01S0135 00330000
*         RFY ISTDYPAB;                                         01S0136 00331000
*         RFY ISTATCVT;                                         01S0137 00332000
*           GEN SETS(RPHWORK)                                   01S0138 00333000
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);   01S0138 00334000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00335000
*           EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                 01S0139 00336000
         SLR   EMCR3,EMCR3                                      01S0139 00337000
*           EMCR3 = '03'X; /*INDICATE ADDSNTPT*/                01S0140 00338000
         LA    EMCR3,3                                          01S0140 00339000
*           CALL ATCLCPTR->ATCCDADD->ISTSDCCA; /*PROCESS REQUEST*/      00340000
*                                                               01S0141 00341000
         L     @08,ATCLCPTR                                     01S0141 00342000
         L     @15,ATCCDADD(,@08)                               01S0141 00343000
         BALR  @14,@15                                          01S0141 00344000
*         RFY (EMCR2,EMCR3) UNRSTD;                             01S0142 00345000
*         END; /*CIDCTL*/                                       01S0143 00346000
*        IF RPRENTED=ON             /* LAST ENTRY IN SEGMENT?        */ 00347000
*         THEN                                                  01S0144 00348000
         TM    RPRENTED(PUPTR),B'00000100'                      01S0144 00349000
         BNO   @RF00144                                         01S0144 00350000
*          LOOP1=OFF;               /* YES, LAST ENTRY - QUIT        */ 00351000
         NI    LOOP1,B'01111111'                                01S0145 00352000
*         ELSE                                                  01S0146 00353000
*          DO;                      /* NOT LAST ENTRY - GET THE NEXT */ 00354000
         B     @RC00144                                         01S0146 00355000
@RF00144 DS    0H                                               01S0147 00356000
*           PUPTR=PUPTR+RPRELEN;                                01S0147 00357000
         AH    PUPTR,RPRELEN(,PUPTR)                            01S0147 00358000
*           IF RPRENTRY^=RPRENTLU   /* THIS A LU?                    */ 00359000
*            THEN                                               01S0148 00360000
         CLI   RPRENTRY(PUPTR),X'81'                            01S0148 00361000
         BE    @RF00148                                         01S0148 00362000
*             LOOP1=OFF;            /* THIS ENTRY IS NOT AN LU -        00363000
*                                      QUIT LOOPING                  */ 00364000
         NI    LOOP1,B'01111111'                                01S0149 00365000
*            ELSE;                                              01S0150 00366000
@RF00148 DS    0H                                               01S0151 00367000
*          END;                     /* END OF NOT LAST ENTRY         */ 00368000
*       END;                        /* END OF LOOP                   */ 00369000
@RC00144 DS    0H                                               01S0152 00370000
@DE00131 TM    LOOP1,B'10000000'                                01S0152 00371000
         BO    @DL00131                                         01S0152 00372000
*       RFY R2 UNRSTD;                                          01S0153 00373000
*     END;                          /* END OF LXS ALLOCATED          */ 00374000
*   ELSE                                                        01S0155 00375000
*     ;                                                         01S0155 00376000
@RF00122 DS    0H                                               01S0156 00377000
*   RFY                                                         01S0156 00378000
*     R15 RSTD;                                                 01S0156 00379000
*   R15=RETCODE;                    /* PUT RESPONSE CODE IN REGISTER    00380000
*                                      15                            */ 00381000
         SLR   R15,R15                                          01S0157 00382000
         IC    R15,RETCODE                                      01S0157 00383000
*   NXTAVAIL=CD6WAPTR;              /* RESET NCSPL NEXT AVAILABLE       00384000
*                                      POINTER IN WORKAREA           */ 00385000
         L     @14,NCSPLWKA(,NCSPLPTR)                          01S0158 00386000
         ST    CD6WAPTR,NXTAVAIL(,@14)                          01S0158 00387000
*   RFY                                                         01S0159 00388000
*     R15 UNRSTD;                                               01S0159 00389000
         EJECT                                                          00390000
*DECHAIN:                                                       01S0160 00391000
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                            01S0160 00392000
*                                                               01S0160 00393000
         B     @PB00002                                         01S0160 00394000
DECHAIN  DS    0H                                               01S0161 00395000
*   /*****************************************************************/ 00396000
*   /*                                                               */ 00397000
*   /* DECHAIN SUBROUTINE                                            */ 00398000
*   /*                                                               */ 00399000
*   /*****************************************************************/ 00400000
*                                                               01S0161 00401000
*   RFY                                                         01S0161 00402000
*     R14 RSTD;                                                 01S0161 00403000
*   LXPTR=RPXFLUX;                  /* GET ADDRESS OF FIRST ALLOCATED   00404000
*                                      LX                            */ 00405000
         L     LXPTR,RPXFLUX(,PXPTR)                            01S0162 00406000
*   LUPTR=PUPTR+RPRELEN;            /* GET ADDRESS OF FIRST LU       */ 00407000
         LR    LUPTR,PUPTR                                      01S0163 00408000
         AH    LUPTR,RPRELEN(,PUPTR)                            01S0163 00409000
*   RPXFLUX=ZERO;                   /* DECHAIN THE LX(S) FROM THE PX */ 00410000
         SLR   @15,@15                                          01S0164 00411000
         ST    @15,RPXFLUX(,PXPTR)                              01S0164 00412000
*   DO WHILE RETCODE^=EIGHT;        /* START DECHAIN LOOP            */ 00413000
         B     @DE00165                                         01S0165 00414000
@DL00165 DS    0H                                               01S0166 00415000
*     RLXINUSE=OFF;                 /* MARK LX NOT IN USE            */ 00416000
         NI    RLXINUSE(LXPTR),B'01111111'                      01S0166 00417000
*     LUPTR->RPRSWDAF=ZERO;         /* ZERO THE LX'S DAF ADDRESS IN     00418000
*                                      THE LU                        */ 00419000
         SLR   @15,@15                                          01S0167 00420000
         STH   @15,RPRSWDAF(,LUPTR)                             01S0167 00421000
*     LXPTR->RPRSWDAF=ZERO;         /* ZERO THE LU'S DAF ADDRESS IN     00422000
*                                      THE LX                        */ 00423000
         STH   @15,RPRSWDAF(,LXPTR)                             01S0168 00424000
*     IF RLXNLUX=ZERO               /* THIS THE LAST ALLOCATED LX?   */ 00425000
*       THEN                                                    01S0169 00426000
         C     @15,RLXNLUX(,LXPTR)                              01S0169 00427000
         BNE   @RF00169                                         01S0169 00428000
*       RETCODE=EIGHT;              /* YES - LAST LX - SET      01S0170 00429000
*                                      INSUFFICIENT LX RESPONSE TO      00430000
*                                      END DECHAIN LOOP              */ 00431000
         MVI   RETCODE,X'08'                                    01S0170 00432000
*     ELSE                                                      01S0171 00433000
*       DO;                         /* NOT LAST ALLOCATED LX -       */ 00434000
         B     @RC00169                                         01S0171 00435000
@RF00169 DS    0H                                               01S0172 00436000
*         SAVLXPTR=RLXNLUX;         /* SAVE THE ADDRESS OF THE NEXT     00437000
*                                      LX                            */ 00438000
         L     @15,RLXNLUX(,LXPTR)                              01S0172 00439000
         ST    @15,SAVLXPTR                                     01S0172 00440000
*         RLXNLUX=ZERO;             /* ZERO OUT THE FORWARD POINTER     00441000
*                                      TO THE NEXT LX                */ 00442000
         SLR   @08,@08                                          01S0173 00443000
         ST    @08,RLXNLUX(,LXPTR)                              01S0173 00444000
*         LXPTR=SAVLXPTR;           /* RESET LXPTR TO THE NEXT  01S0174 00445000
*                                      ALLOCATED LX                  */ 00446000
         LR    LXPTR,@15                                        01S0174 00447000
*         LUPTR=LUPTR+LUPTR->RPRELEN;/* GET THE NEXT LU              */ 00448000
         AH    LUPTR,RPRELEN(,LUPTR)                            01S0175 00449000
*       END;                                                    01S0176 00450000
*   END;                            /* END OF DECHAIN LOOP           */ 00451000
@RC00169 DS    0H                                               01S0177 00452000
@DE00165 CLI   RETCODE,8                                        01S0177 00453000
         BNE   @DL00165                                         01S0177 00454000
*   RFY                                                         01S0178 00455000
*     R14 UNRSTD;                                               01S0178 00456000
*   END DECHAIN;                                                01S0179 00457000
*                                                               01S0179 00458000
@EL00002 DS    0H                                               01S0179 00459000
@EF00002 DS    0H                                               01S0179 00460000
@ER00002 BR    @14                                              01S0179 00461000
@PB00002 DS    0H                                               01S0180 00462000
*/* TPEPILOG   */                                               01S0180 00463000
*    GEN (PRINT OFF);                                           01S0180 00464000
*                                                               01S0180 00465000
         PRINT OFF                                                      00466000
*/*   PROGRAM OPERATOR MESSAGE HEADER                                */ 00467000
*DCL  1  ISTPOHD  BDY(WORD) BASED,                              01S0181 00468000
*        2 POHRSVD1 CHAR(1),       /* RESERVED                       */ 00469000
*        2 POHSTAT,                /* HEADER STATUS BITS             */ 00470000
*          3 POHEND   BIT(1),      /* END LINE 1=YES                 */ 00471000
*          3 POHDATA  BIT(1),      /* DATA LINE 1=YES                */ 00472000
*          3 POHLBL   BIT(1),      /* LABEL LINE 1=YES               */ 00473000
*          3 POHCNTRL BIT(1),      /* CONTROL LINE 1=YES             */ 00474000
*          3 POHRSVD2 BIT(2),      /* RESERVED                       */ 00475000
*          3 POHRREQ  BIT(1),      /* REPLY REQUESTED 1=YES          */ 00476000
