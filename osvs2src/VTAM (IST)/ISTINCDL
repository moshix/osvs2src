         TITLE 'ISTINCDL - 370X L/D/R - CLUSTER CONTROLLER RESTART     *00001000
                        '                                               00002000
ISTINCDL CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCDL  75.281'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @08,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@08                                         0001 00012000
*   CDLR1=CDLR1;                                                   0471 00013000
*                                                                  0471 00014000
*/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  */ 00015000
