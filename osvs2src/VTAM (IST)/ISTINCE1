*                                       CHANNEL END PROGRAM SHOULD      00743000
*                                       END                          */ 00744000
*    2 IOBDBYTN FIXED(15),           /* NUMBER OF BYTES NEEDED ON A     00745000
*                                       TRACK TO WRITE A NEW BLOCK   */ 00746000
*    2 IOBRSV34 CHAR(2),             /* RESERVED                     */ 00747000
*    2 IOBDQPTR PTR(31),             /* ADDRESS OF IOB FOR NEXT I/O     00748000
*                                       OPERATION TO BE EXECUTED     */ 00749000
*    2 IOBRSV35 CHAR(8) BDY(WORD),   /* RESERVED                     */ 00750000
*    2 IOBDNCRF CHAR(8) BDY(WORD),   /* COUNT FIELD FOR NEW BLOCK    */ 00751000
*    2 IOBCHNPR CHAR(0) BDY(WORD);   /* CHANNEL PROGRAM USED TO 01S0051 00752000
*                                       TRANSFER DATA AS REQUESTED BY   00753000
*                                       THE READ OR WRITE MACRO 01S0051 00754000
*                                       INSTRUCTION STARTS HERE      */ 00755000
*                                                               01S0051 00756000
*                        /* **************************************** */ 00757000
*                        /*      VSAM EXTENSION *** 24 BYTES         */ 00758000
*                        /* **************************************** */ 00759000
*                                                               01S0052 00760000
* DECLARE 1 IOBORG10                                            01S0052 00761000
*       DEF(IOBEXTEN) POS(9) BDY(WORD),                         01S0052 00762000
*    2 IOBSEEK2 CHAR(8) BDY(WORD),   /* SEEK FIELD 2           ICB435*/ 00763000
*     3 IOBSK2M  CHAR(1),            /* EXTENT NUMBER          ICB435*/ 00764000
*     3 IOBSK2BB CHAR(2),            /* BIN NUMBER             ICB435*/ 00765000
*     3 IOBSK2CC CHAR(2),            /* CYLINDER NUMBER        ICB435*/ 00766000
*     3 IOBSK2HH CHAR(2),            /* HEAD NUMBER            ICB435*/ 00767000
*     3 IOBSK2R  CHAR(1),            /* RECORD NUMBER          ICB435*/ 00768000
*    2 IOBBUFC  PTR(31),             /* ADDRESS OF ASSOCIATED BUFFER    00769000
*                                       CONTROL BLOCK          ICB435*/ 00770000
*    2 IOBREADA PTR(31),             /* ADDRESS OF FIRST READ CHANNEL   00771000
*                                       PROGRAM SEGMENT THAT HAS NOT    00772000
*                                       BEEN PROCESSED         ICB435*/ 00773000
*    2 IOBNEXTA PTR(31),             /* ADDRESS OF NEXT ACTIVE IOB      00774000
*                                                              ICB435*/ 00775000
*    2 IOBRDCHP PTR(31);             /* ADDRESS OF READ CHANNEL 01S0052 00776000
*                                       PROGRAM                ICB435*/ 00777000
*                                                               01S0052 00778000
*                        /* **************************************** */ 00779000
*                        /*    VTAM EXTENSION (OS/VS1 ONLY)          */ 00780000
*                        /*        *** 24 BYTES                      */ 00781000
*                        /* **************************************** */ 00782000
*                                                               01S0053 00783000
* DECLARE 1 IOBORG11                                            01S0053 00784000
*       DEF(IOBEXTEN) BDY(WORD),                                01S0053 00785000
*    2 IOBERCT  PTR(31),             /* SAME AS IOBERCTA BELOW MDC001*/ 00786000
*     3 IOBUCBXV PTR(8),             /* UCB INDEX              MDC050*/ 00787000
*      4 IOBRTYPE BIT(8),            /* RECORD TYPE FOR OBR    MDC002*/ 00788000
*     3 IOBERCTA PTR(24),            /* POINTER TO COUNTERS FOR SIO     00789000
*                                       AND TEMPORARY ERRORS   MDC003*/ 00790000
*    2 IOBNAME  PTR(31),             /* SAME AS IOBNAMEA BELOW MDC004*/ 00791000
*     3 IOBNAMSZ BIT(8),             /* SIZE OF TERMINAL NAME  MDC005*/ 00792000
*     3 IOBNAMEA PTR(24),            /* POINTER TO TERMINAL NAME        00793000
*                                                              MDC006*/ 00794000
*    2 IOBMDREC PTR(31),             /* POINTER TO RECORD BEING 01S0053 00795000
*                                       PASSED TO MISCELLANEOUS DATA    00796000
*                                       RECORDER               MDC007*/ 00797000
*    2 IOBRCD   PTR(31),             /* POINTER TO QUEUE OF OBR 01S0053 00798000
*                                       RECORDS PASSED FROM 3705        00799000
*                                                              MDC008*/ 00800000
*    2 IOBSENSV BIT(8),              /* SENSE BYTE SAVE AREA   MDC009*/ 00801000
*    2 IOBCSWSV CHAR(7);             /* SAVE AREA FOR LAST 7 BYTES OF   00802000
*                                       CSW                    MDC010*/ 00803000
*                                                               01S0053 00804000
*                        /* **************************************** */ 00805000
*                        /*    3540 EXTENSION *** 4 BYTES    @Z30OP9A*/ 00806000
*                        /* **************************************** */ 00807000
*                                                               01S0054 00808000
* DECLARE 1 IOBORG13                                            01S0054 00809000
*       DEF(IOBEXTEN) BDY(WORD),     /*                      @Z30OP9A*/ 00810000
*    2 IOBSKADR CHAR(4) BDY(WORD),   /* 3540 SEEK ADDRESS       01S0054 00811000
*                                       (MDC300)             @Z30OP9A*/ 00812000
*     3 IOBSKRV  PTR(8),             /* RESERVED  (MDC301)   @Z30OP9A*/ 00813000
*     3 IOBSKTT  PTR(8),             /* TRACK NUMBER            01S0054 00814000
*                                       (MDC302)             @Z30OP9A*/ 00815000
*     3 IOBSK0   PTR(8),             /* MUST BE ZERO            01S0054 00816000
*                                       (MDC303)             @Z30OP9A*/ 00817000
*     3 IOBSKSS  PTR(8);             /* SECTOR NUMBER           01S0054 00818000
*                                       (MDC304)             @Z30OP9A*/ 00819000
*                                                               01S0054 00820000
*/*************************************************************/        00821000
*/*                                                           */        00822000
*/*  THIS STRUCTURE DESCRIBES THE CONTROL BLOCK PASSED TO THE */        00823000
*/*  I/O SUPERVISOR  AS A  PARAMETER OF THE SRB.  THE CONTROL */        00824000
*/*  BLOCK CONTAINS ALL INFORMATION NECESSARY TO INITIATE AND */        00825000
*/*  TERMINATE AN I/O OPERATION.  FIELDS OF THE IOSB ARE USED */        00826000
*/*  FOR COMMUNICATION BETWEEN THE:                           */        00827000
*/*                                                           */        00828000
*/*      1. I/O SUPERVISOR AND I/O REQUESTOR                  */        00829000
*/*      2. I/O SUPERVISOR AND ERROR RECOVERY PROCEDURES      */        00830000
*/*      3. COMPONENTS OF THE I/O SUPERVISOR                  */        00831000
*/*      4. SUCCESSIVE ENTRANCES TO THE ERP'S                 */        00832000
*/*      5. ERP AND COMMON WTO AND STATISTICS UPDATE MODULES  */        00833000
*/*                                                           */        00834000
*/*************************************************************/        00835000
*/*                                                           */        00836000
*/*      05/30/72   LEVEL=00                                  */        00837000
*/*                                                           */        00838000
*/*************************************************************/        00839000
*                                                               01S0055 00840000
*   DCL 1 IOSB  BDY(WORD)  BASED(IOSBPTR),                      01S0055 00841000
*       2 IOSFLA    CHAR(1),       /* FLAG BYTE A             */        00842000
*         3 IOSACHN BIT(2),        /* COMMAND & DATA CHAINING */        00843000
*           4 IOSDCHN BIT(1),      /* DATA CHAINING           */        00844000
*           4 IOSCCHN BIT(1),      /* COMMAND CHAINING        */        00845000
*         3 IOSERR  BIT(1),        /* ERROR ROUTINE IN CONTROL  01S0055 00846000
*                                     MUST BE ZEROED BY DRIVER  01S0055 00847000
*                                     IF ERP RETURNS WITH THIS  01S0055 00848000
*                                     BIT ON, A RETRY IS        01S0055 00849000
*                                     ASSUMED. IF THE ERP RE-   01S0055 00850000
*                                     TURNS WITH THIS BIT OFF,  01S0055 00851000
*                                     THE ERROR IS CONSIDERED   01S0055 00852000
*                                     PERMANENT OR CORRECTED    01S0055 00853000
*                                     DEPENDING ON THE SETTING  01S0055 00854000
*                                     OF IOSEX.               */        00855000
*         3 IOSSMDA BIT(1),        /* ERP STATUS MODFR BIT .    01S0055 00856000
*                                     MUST BE ZEROED BY DRIVER  01S0055 00857000
*                                      TAPE - REPOSITION DEVCE  01S0055 00858000
*                                      1052 - IMMEDIATE OPERA-  01S0055 00859000
*                                        TION, CCW OP CODE IN   01S0055 00860000
*                                        IOSMDB.              */        00861000
*         3 IOSSMDB BIT(1),        /* ERP STATUS MODFR BIT B    01S0055 00862000
*                                     MUST BE ZEROED BY DRIVER  01S0055 00863000
*                                     SET BY FETCH IN APPEN-    01S0055 00864000
*                                     DAGE FOR POSTING.         01S0055 00865000
*                                      TAPE - CRC NEEDED        01S0055 00866000
*                                      DASD - PCI FETCH STOP    01S0055 00867000
*                                           FLAG              */        00868000
*         3 IOSEX   BIT(1),        /* EXCEPTIONAL CONDITION.    01S0055 00869000
*                                     UPON RETURN FROM NORMAL   01S0055 00870000
*                                     OR ABNORMAL EXIT WITH     01S0055 00871000
*                                     THIS BIT ON, ERP PROCES-  01S0055 00872000
*                                     SING IS INITIATED IF IN-  01S0055 00873000
*                                     ITIAL ERROR CONDITION.    01S0055 00874000
*                                     IF BIT IS OFF, IT IS AS-  01S0055 00875000
*                                     SUMED THAT THE EXIT COR-  01S0055 00876000
*                                     RECTED THE CONDITION OR   01S0055 00877000
*                                     DID NOT CONSIDER IT AN    01S0055 00878000
*                                     ERROR. WHEN THE ERP RE-   01S0055 00879000
*                                     TURNS WITH THIS BIT ON,   01S0055 00880000
*                                     AND IOSERR OFF,THE ERROR  01S0055 00881000
*                                     IS CONSIDERED PERMANENT.  01S0055 00882000
*                                     WHEN THE ERP RETRNS WITH  01S0055 00883000
*                                     THIS BIT OFF, THE ERROR   01S0055 00884000
*                                     HAS BEEN CORRECTED.     */        00885000
*         3 IOSDOM  BIT(1),        /* DOM MACRO REQUIRED     @YM06828*/ 00886000
*         3 IOSIOSB BIT(1),        /* IOSB CREATED BY IOS.      01S0055 00887000
*                                     MUST BE ZEROED BY DRIVER*/        00888000
*                                                               01S0055 00889000
*       2 IOSFLB    CHAR(1),       /* FLAG BYTE B             */        00890000
*         3 IOSDIESE BIT(1),       /* SECOND ENTRY TO DIE     */        00891000
*         3 IOSSDR   BIT(1),       /* ERP DOESNT WANT OBR    @Z40MPLG*/ 00892000
*         3 IOSFLB2  BIT(1),       /* RESERVED                */        00893000
*         3 IOSFLB3  BIT(1),       /* RESERVED                */        00894000
*         3 IOSFLB4  BIT(1),       /* RESERVED                */        00895000
*         3 IOSMSG   BIT(1),       /* MESSAGE INDICATOR         01S0055 00896000
*                                     0 = INTRVNTN REQURD MSG   01S0055 00897000
*                                     1 = I/O ERROR MESSAGE   */        00898000
*         3 IOSBDCST BIT(1),       /* BROADCAST BIT           */        00899000
*         3 IOSLOG   BIT(1),       /* CREATE AN OBR RECORD    */        00900000
*                                                               01S0055 00901000
*       2 IOSFLC    CHAR(1),       /* DEVICE DEP ERP FLAGS    */        00902000
*         3 IOSDVMNT BIT(1),       /* DAVV ISSUED MOUNT       */        00903000
*         3 IOSDVALT BIT(1),       /* ALTERNATE TRACK PROCES-   01S0055 00904000
*                                     SING BY DAVV            */        00905000
*          4 IOSVERIF BIT(1),      /* UNSOLICITED DEVICE END    01S0055 00906000
*                                     VERIFICATION NEEDED       01S0055 00907000
*                                     (NON-DA)                */        00908000
*         3 IOSCC3WE BIT(1),       /* GDP REQ'RS CC3 POST X6D@DCRR054*/ 00909000
*         3 IOSTP    BIT(1),       /* NO SPECIAL CC3 HANDLINGTOBEFLGD*/ 00910000
*         3 IOSFLC4  BIT(1),       /* RESERVED                */        00911000
*         3 IOSGLC5  BIT(1),       /* RESERVED                */        00912000
*         3 IOSFLC6  BIT(1),       /* RESERVED                */        00913000
*         3 IOSFLC7  BIT(1),       /* RESERVED                */        00914000
*                                                               01S0055 00915000
*       2 IOSPROC   CHAR(1),       /* THIS BYTE INDICATES WHAT  01S0055 00916000
*                                     TYPE OF SPECIAL PROCES-   01S0055 00917000
*                                     SING IS TO BE PERFORMED   01S0055 00918000
*                                     BY IOS COMPONENTS OPER-   01S0055 00919000
*                                     ATING ASYNCHRONOUS TO     01S0055 00920000
*                                     MAINLINE                  01S0055 00921000
*                                     MUST BE ZEROED BY DRIVER  01S0055 00922000
*                                     DCLS ARE AT END OF IOSB */        00923000
*                                                               01S0055 00924000
*       2 IOSDVRID  CHAR(1),       /* DRIVER ID VALUE           01S0055 00925000
*                                     VALUES AT END OF IOSB   */        00926000
*                                                               01S0055 00927000
*       2 IOSPRLVL  CHAR(1),       /* PRIORITY LEVEL AT WHICH   01S0055 00928000
*                                     THE ADDRESS SPACE IS TO BE        00929000
*                                     SCHEDULED, 0 OR 1       */        00930000
*       2 IOSASID   CHAR(2),       /* ADDR SPACE ID OF MEMORY   01S0055 00931000
*                                     TO BE SCHEDULED AT TERM   01S0055 00932000
*                                     OF I/O REQUEST          */        00933000
*       2 IOSPGAD   PTR(31),       /* PGM ADDR TO BE DISPTCHD */        00934000
*       2 IOSPKEY BIT(8),          /* PROTECT KEY OF IOSPGAD  */        00935000
*        3 *       BIT(4),         /* PROTECT KEY             */        00936000
*        3 IOSLCL  BIT(1),         /* ASID SCHDLD AT LOCAL LVL*/        00937000
*        3 IOSIDR  BIT(1),         /* ASYNCHRONOUS SCHEDULING   01S0055 00938000
*                                     SHOULD BE USED FOR THIS   01S0055 00939000
*                                     REQUEST (INDIRECT RE-     01S0055 00940000
*                                     CORDING FOR PAGING I/O  */        00941000
*        3 IOSPGDPX BIT(1),        /* THIS REQUEST HAS A BACK   01S0055 00942000
*                                     UP COPY (DUPLEXED PAGE) */        00943000
*        3 IOSPKY7  BIT(1),        /* RESERVED                */        00944000
*       2 IOSCOD    CHAR(1),       /* I/O COMPLETION CODE       01S0055 00945000
*                                     USED AS TEMP SAVE FOR     01S0055 00946000
*                                     SYSTEM MASK BY CHAN SCHD  01S0055 00947000
*                                     VALUES AT END OF IOSB   */        00948000
*                                                               01S0055 00949000
*       2 IOSOPT    CHAR(1),       /* OPTIONS BYTE            */        00950000
*         3 IOSBYP  BIT(1),        /* BYPSS IOS CHAN PGM PRFX */        00951000
*         3 IOSDEP  BIT(1),        /* DEVICE END POST REQUSTD */        00952000
*         3 IOSQISCE BIT(1),       /* THIS REQUEST INITIATED    01S0055 00953000
*                                     BY FUNCTION WHICH HAS     01S0055 00954000
*                                     QUIESCED THE DEVICE     */        00955000
*         3 IOSPSLL BIT(1),        /* ON=LOCAL LOCK NOT WANTED  01S0055 00956000
*                                     FOR POST STATUS PROCSS    01S0055 00957000
*                                     OFF=LOCAL LOCK WANTED   */        00958000
*         3 IOSNERP BIT(1),        /* IBM ERPS NOT TO BE USED */        00959000
*         3 IOSTSLL BIT(1),        /* ON = LOCAL LOCK NOT       01S0055 00960000
*                                     WANTED BY TERMINATION     01S0055 00961000
*                                     ROUTINE                   01S0055 00962000
*                                     OFF = LOCAL LOCK WANTED   01S0055 00963000
*                                     BY TERMINATION ROUTINE    01S0055 00964000
*                                     IF IOSPSLL ALSO ON      */        00965000
*         3 IOSAPR  BIT(1),        /* ALT PATH RETRY ACTIVE.    01S0055 00966000
*                                     MUST BE ZEROED BY DRIVER*/        00967000
*         3 IOSRELSE BIT(1),       /* STAND-ALONE RELEASE CCW   01S0055 00968000
*                                     ISSUED BY IOS           */        00969000
*                                                               01S0055 00970000
*       2 IOSOPT2  CHAR(1),        /* OPTION BYTE 2           */        00971000
*         3 IOSHTP  BIT(1),        /* ELIGIBLE FOR SHOULDR TAP*/        00972000
*         3 IOSOPT2X BIT(7),       /* RESERVED BITS (UNUSED)  */        00973000
*       2 IOSUCB    PTR(31),       /* UNIT CONTROL BLOCK ADDR */        00974000
*       2 *         CHAR(8),                                    01S0055 00975000
*         3 IOSCC     BIT(8),      /* SIO CONDITION CODE      */        00976000
*         3 IOSCSW    CHAR(7),     /* LOW ORDR 7 BYTES OF CSW */        00977000
*          4 IOSCSWCA  PTR(24) BDY(BYTE), /*                    01S0055 00978000
*                                     LAST COMMAND ADDRESS    */        00979000
*          4 IOSTATUS  CHAR(2),    /* CSW STATUS              */        00980000
*            5 IOSTSA  BIT(8),     /* DEVICE STATUS           */        00981000
*            5 IOSTSB  BIT(8),     /* CHANNEL STATUS          */        00982000
*          4 IOSCSWRC PTR(16) BDY(BYTE), /*                     01S0055 00983000
*                                     RISIDUAL COUNT          */        00984000
*                                                               01S0055 00985000
*       2 IOSSRB    PTR(31),       /* POINTER BACK TO SRB     */        00986000
*       2 IOSUSE    PTR(31),       /* USER FIELD              */        00987000
*       2 IOSRES4A  PTR(31),       /* RESERVED                */        00988000
*       2 IOSAPMSK  CHAR(2),       /* EXCLUSIVE PATH MASK FOR   01S0055 00989000
*                                     APR                       01S0055 00990000
*                                     MUST BE ZEROED BY DRIVER*/        00991000
*       2 IOSSNS    BIT(16),       /* SENSE DATA              */        00992000
*                                                               01S0055 00993000
*  /***********************************************************/        00994000
*  /*  END OF COMMON SECTION AND START OF PROCESSING  DEPEN-  */        00995000
*  /*  DENT SECTIONS WHICH ARE: NORMAL I/O REQUEST, WTO, AND  */        00996000
*  /*  PCI SCHEDULING.                                        */        00997000
*  /***********************************************************/        00998000
*                                                               01S0055 00999000
*       2 IOSIPIB   PTR(31),       /* IOS/PURGE INTERFACE       01S0055 01000000
*                                     BLOCK ADDRESS             01S0055 01001000
*                                     MUST BE ZEROED BY DRIVER. 01S0055 01002000
*                                     OR CHAIN PTR FOR PCI      01S0055 01003000
*                                     SRB/IOSBS               */        01004000
*       2 IOSPCHN   PTR(31),       /* PTR TO ENDING STATUS IOSB 01S0055 01005000
*                                     FOR PCI SRB/IOSBS.        01S0055 01006000
*                                     PTR TO FIRST PCI SRB/IOSB 01S0055 01007000
*                                     FOR ENDING STATUS IOSB. */        01008000
*       2 IOSERP    PTR(31),       /* ERP DYNAMIC W.A. ADDR.    01S0055 01009000
*                                     MUST BE ZEROED BY DRIVER*/        01010000
*       2 IOSPCI    PTR(31),       /* PCI EXIT ADDRESS        */        01011000
*       2 IOSNRM    PTR(31),       /* NORMAL EXIT ADDRESS     */        01012000
*       2 IOSABN    PTR(31),       /* ABNORMAL EXIT ADDRESS   */        01013000
*       2 IOSDIE    PTR(31),       /* DISABLED INT. EXIT ADDR */        01014000
*       2 IOSRST    PTR(31),       /* REAL ADDRESS OF REAL      01S0055 01015000
*                                     CHANNEL PROGRAM         */        01016000
*       2 IOSVST    PTR(31),       /* VIRTUAL ADDRESS OF REAL   01S0055 01017000
*                                     CHANNEL PROGRAM         */        01018000
*       2 IOSDSID   PTR(31),       /* DATA SET ID FOR PURGE   */        01019000
*                                                               01S0055 01020000
*       2 IOSRSS1B  CHAR(1),       /* RESERVED                */        01021000
*       2 IOSAFF    BIT(8),        /* CPU AFFINITY INDICATOR    01S0055 01022000
*                                     FOR GUARANTEED DEVCE PTH*/        01023000
*       2 IOSPATH   CHAR(2),       /* PATH SPECIFICATION FOR    01S0055 01024000
*                                     GUARANTEED DEVICE PATH    01S0055 01025000
*                                     OR SPECIFIC EXPOSURE      01S0055 01026000
*                                     REQUESTS                */        01027000
*         3 IOSCHN  CHAR(1),                                    01S0055 01028000
*           4 IOSGDP    BIT(1),    /* GUARANTEED DEVICE PATH  */        01029000
*           4 IOSEXP    BIT(1),    /* SPECIFIC EXPOSURE REQST */        01030000
*           4 IOSPATH2  BIT(1),    /* RESERVED                */        01031000
*           4 IOSPATH3  BIT(1),    /* RESERVED                */        01032000
*           4 *         BIT(4),    /* CHANNEL NUMBER          */        01033000
*                                                               01S0055 01034000
*         3 IOSCUDEV CHAR(1),      /* CONTRL UNIT/DEVICE ADDR */        01035000
*                                                               01S0055 01036000
*       2 IOSFMSK   CHAR(1),       /* MODE SET/FILE MASK      */        01037000
*       2 IOSCKEY   BIT(8),        /* PROT. KEY OF CHAN PGM   */        01038000
*         3 *        BIT(4),       /* PROTECT KEY             */        01039000
*         3 IOSCKEY4 BIT(1),       /* RESERVED                */        01040000
*         3 IOSCKEY5 BIT(1),       /* RESERVED                */        01041000
*         3 IOSCKEY6 BIT(1),       /* RESERVED                */        01042000
*         3 IOSCKEY7 BIT(1),       /* RESERVED                */        01043000
*       2 IOSMDB    CHAR(1),       /* ERP IMMED. CCW OP CODE  */        01044000
*       2 IOSMDM    CHAR(1),       /* ERP MODIFIER MASK       */        01045000
*       2 IOSEEK    CHAR(8),       /* STATIC SEEK ADDRESS     */        01046000
*       2 IOSEEKA   CHAR(8),       /* DYNAMIC SEEK ADDRESS    */        01047000
*         3 IOSSKM  PTR(8),             /*   M                */        01048000
*         3 IOSSKBB PTR(16) BDY(BYTE),  /*   BB               */        01049000
*         3 IOSCCHH CHAR(4),            /*   CCHH             */        01050000
*          4 IOSSKCC PTR(16) BDY(BYTE), /*   CC               */        01051000
*          4 IOSSKHH PTR(16) BDY(BYTE), /*   HH               */        01052000
*             5 IOSSKH1 PTR(8),         /*   H                */        01053000
*             5 IOSSKH2  PTR(8),        /*   H                */        01054000
*         3 IOSSKR  PTR(8),             /*   R                */        01055000
*       2 IOSEND    CHAR(0);       /* END OF IOSB             */        01056000
*                                                               01S0055 01057000
*  /*  ATTENTION PROCESSING SECTION                           */        01058000
*   DCL 1 IOSATTSC  CHAR(64) DEF(IOSIPIB),                      01S0056 01059000
*         2 IOSATTSN CHAR(24),     /* ADDITIONAL SENSE IF ANY */        01060000
*         2 *        CHAR(16),     /* ATTN RTN RSVD AREA      */        01061000
*         2 IOSATTWA CHAR(24);     /* ATTN RTN WORK AREA      */        01062000
*                                                               01S0056 01063000
*  /*  WTO PROCESSING SECTION                                 */        01064000
*   DCL 1 IOSWTOWA  CHAR(64) DEF(IOSIPIB),                      01S0057 01065000
*         2 IOSWTOCH CHAR(2),      /* ADDR CC=3 OCCURRED ON   */        01066000
*         2 IOSWTOCP CHAR(1),      /* CPU CC=3 OCCURRED ON    */        01067000
*         2 IOSWTOPT CHAR(1),      /* PATH IND. FOR CC=3      */        01068000
*         2 IOSWRNDR CHAR(60);     /* REMAINDER OF WTO W.A.   */        01069000
*                                                               01S0057 01070000
*  /*  PCI SCHEDULING SECTION                                 */        01071000
*   DCL 1 IOSPCISC  CHAR(64)  DEF(IOSIPIB) BDY(WORD),           01S0058 01072000
*         2 *       PTR(31),       /* SAME AS IOSIPIB. MUST     01S0058 01073000
*                                     NOT BE USED             */        01074000
*         2 *       PTR(31),       /* SAME AS IOSPCHN. MUST     01S0058 01075000
*                                     NOT BE USED             */        01076000
*         2 IOSPCIRS CHAR(36),     /* PCI RSVD SECTION        */        01077000
*         2 IOSPCIWA CHAR(20);     /* PCI WORK AREA           */        01078000
*                                                               01S0058 01079000
* /************************************************************/        01080000
* /*                                                          */        01081000
* /*     FOLLOWING ARE THE PLS DECLARES WHICH REPLACE  THE    */        01082000
* /*     PREVIOUSLY DEFINED %DCLARES.  THE FIELDS TO WHICH    */        01083000
* /*     THE VALUES APPLY ARE REPRODUCED AS A COMMENT. THIS   */        01084000
* /*     CHANGE ALLOWS A CROSS REFERENCE OF THE NAMES  USED   */        01085000
* /*     AS FIELD VALUES.                                     */        01086000
* /*                                                          */        01087000
* /************************************************************/        01088000
*                                                               01S0059 01089000
*   /*  2 IOSPROC  CHAR(1),           SEE DCL FOR DESCRIPTION */        01090000
*                                                               01S0059 01091000
*   DCL IOSAPCI BIT(8) CONSTANT('04'X);     /* PCI            */        01092000
*   DCL IOSATTN BIT(8) CONSTANT('08'X);     /* ATTENTION      */        01093000
*   DCL IOSAPURG BIT(8) CONSTANT('0C'X);    /* PURGE          */        01094000
*   DCL IOSADAVV BIT(8) CONSTANT('10'X);    /* DAVV           */        01095000
*   DCL IOSAWTO BIT(8) CONSTANT('14'X);     /* WTO            */        01096000
*   DCL IOSADDR BIT(8) CONSTANT('18'X);     /* DDR            */        01097000
