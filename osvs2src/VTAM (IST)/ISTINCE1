SVTM     TITLE 'ISTINCE1 - LINK RESTART PROCEDURE                      *00001000
                        '                                               00002000
ISTINCE1 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTINCE1  75.171'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
*/* END OF ATBUILD */                                           01S0006 00014000
*                                                               01S0006 00015000
*          DO;                                                  01S0006 00016000
*/***  CRRMAINT  RPHADD((R01->NCSPLRPH))PROCNM(ISTINCE1)COMP(VY)FUNC(SU 00017000
*)  ***/                                                        01S0007 00018000
*                                                               01S0007 00019000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */               01S0007 00020000
*          RFY ISTLCCW ;                                        01S0007 00021000
*          RFY ISTLCPB ;                                        01S0008 00022000
*          RFY ISTZSAVE;                                        01S0009 00023000
*          RFY ISTPFCRR;                                        01S0010 00024000
*          RFY ISTCFCRR;                                        01S0011 00025000
*          RFY ISTZCRR ;                                        01S0012 00026000
*          RFY ISTASCRR;                                        01S0013 00027000
*          RFY ISTAPCRR;                                        01S0014 00028000
*          RFY ISTCRA  ;                                        01S0015 00029000
*          RFY ISTCNSTA;                                        01S0016 00030000
*          RFY ISTVYCRR;                                        01S0017 00031000
*/* END OF ATBUILD */                                           01S0018 00032000
*          /*** SET ENTERED BIT FOR THIS PROCEDURE ***/         01S0018 00033000
*R01->NCSPLRPH->RPHCRR->CRACRR->NINCE1 = '1'B;                  01S0018 00034000
         L     @14,NCSPLRPH(,R01)                               01S0018 00035000
         L     @14,RPHCRR(,@14)                                 01S0018 00036000
         L     @14,CRACRR(,@14)                                 01S0018 00037000
         OI    NINCE1(@14),B'00100000'                          01S0018 00038000
*          /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/        01S0019 00039000
*R01->NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                  01S0019 00040000
         XC    VYCCRT+1(11,@14),VYCCRT+1(@14)                   01S0019 00041000
         MVI   VYCCRT(@14),B'00000000'                          01S0019 00042000
*R01->NCSPLRPH->RPHCRR->CRACRR->CINCE1 = '1'B;                  01S0020 00043000
         OI    CINCE1(@14),B'00100000'                          01S0020 00044000
*          END;                                                 01S0021 00045000
*          DCL R01 PTR(31) REG(01) RSTD;/* INPUT PARAMETER REG    */    00046000
*          DCL R13 PTR(31) REG(13) RSTD;/* SAVE AREA PTR REGISTER */    00047000
*          DCL R15 PTR(31) REG(15) RSTD;/* WORK REGISTER          */    00048000
*          DCL R10 PTR(31) REG(10) RSTD;/* BASE REG FOR NCSPL     */    00049000
*          DCL R11 PTR(31) REG(11) RSTD;/* DATA AREA PTR REGISTER */    00050000
*          DCL @SIZDATD FIXED(31) GEND;/* size of DATA area       */    00051000
*          R10 = R01;                /* INIT NCSPL BASE REGISTER  */    00052000
         LR    R10,R01                                          01S0028 00053000
*          RFY ISTNCSPL BASED(R10);  /* ADDRESSABILITY FOR NCSPL  */    00054000
*          RFY ISTRPH BASED(NCSPLRPH);/* ADDRESSABILITY FOR RPH   */    00055000
*          R15 = R13;                /* SAVE CALLER SAVE AREA ADR */    00056000
         LR    R15,R13                                          01S0031 00057000
*          R13 = R13 - LENGTH(ISTRSA);/* SET SAVE AREA PTR REG    */    00058000
         SL    R13,@CF00917                                     01S0032 00059000
*          R15->RSAFPT = R13;        /* SET CALLERS FORWARD PTR   */    00060000
         ST    R13,RSAFPT(,R15)                                 01S0033 00061000
*          R13->RSABPT = R15;        /* SET MY BACK PTR           */    00062000
         ST    R15,RSABPT(,R13)                                 01S0034 00063000
*          R13->RSAFPT = 0;          /* SET MY FORWARD PTR        */    00064000
         SLR   @14,@14                                          01S0035 00065000
         ST    @14,RSAFPT(,R13)                                 01S0035 00066000
*          R11= VWANAV + 7 & 'FFFFFFF8'X;/* DATAREG = DWORD BDY   */    00067000
         L     @14,NCSPLWKA(,R10)                               01S0036 00068000
         LA    R11,7                                            01S0036 00069000
         AL    R11,VWANAV(,@14)                                 01S0036 00070000
         N     R11,@CF00073                                     01S0036 00071000
*          VWANAV = R11 + @SIZDATD;  /* ALLOCATE DATA AREA        */    00072000
         LR    @00,R11                                          01S0037 00073000
         AL    @00,@SIZDATD                                     01S0037 00074000
         ST    @00,VWANAV(,@14)                                 01S0037 00075000
*          GEN (USING @DATD,R11);    /* ESTAB BASE REG FOR DATA   */    00076000
         USING @DATD,R11                                                00077000
*/* END OF CPPROC EXPANSION */                                  01S0039 00078000
*                                                               01S0039 00079000
*                                                               01S0039 00080000
*/*            INPUT/OUTPUT BLOCK                                    */ 00081000
*/*                                                                  */ 00082000
*/*  OS/VS2 RELEASE 4, OS/VS1 RELEASE 5, 12/18/74, LEVEL=3           */ 00083000
*/*                                                                  */ 00084000
*/*  METHOD OF ACCESS                                                */ 00085000
*/*      BAL  - DSECT IS PRODUCED UNLESS DSECT=NO IS SPECIFIED.      */ 00086000
*/*             USING ON IOB GIVES ADDRESSABILITY FOR ALL SYMBOLS.   */ 00087000
*/*      PL/S - DCL (IOBPRPTR,IOBSTPTR) PTR                          */ 00088000
*/*                                                                  */ 00089000
*                                                               01S0039 00090000
* DECLARE                                                       01S0039 00091000
*   1 IOBPREFX BASED(IOBPRPTR) BDY(WORD),                       01S0039 00092000
*                                                               01S0039 00093000
* /* *************************************************************** */ 00094000
* /*                                                                 */ 00095000
* /*                 PREFIX SECTIONS OF THE IOB                      */ 00096000
* /*                                                                 */ 00097000
* /* *************************************************************** */ 00098000
*                                                               01S0039 00099000
*                        /* **************************************** */ 00100000
*                        /*    QSAM,BSAM,BPAM PREFIX                 */ 00101000
*                        /*    CHAINED SCHEDULING *** 16 BYTES       */ 00102000
*                        /* **************************************** */ 00103000
*                                                               01S0039 00104000
*    2 IOBCFLG1 CHAR(1),             /* I/O INDICATORS               */ 00105000
*     3 IOBRSV01 BIT(1),             /* RESERVED                     */ 00106000
*     3 IOBRSV02 BIT(1),             /* RESERVED                     */ 00107000
*     3 IOBRSV03 BIT(1),             /* RESERVED                     */ 00108000
*     3 IOBRSV04 BIT(1),             /* RESERVED                     */ 00109000
*     3 IOBPTST  BIT(1),             /* NOTE OR POINT OPERATION IS      00110000
*                                       IN PROCESS                   */ 00111000
*     3 IOBABAPP BIT(1),             /* ERROR HAS BEEN PROCESSED ONCE   00112000
*                                       BY ABNORMAL-END APPENDAGE       00113000
*                                       ROUTINE                      */ 00114000
*     3 IOBRSTCH BIT(1),             /* RESTART CHANNEL              */ 00115000
*     3 IOBPCI   BIT(1),             /* SET WHEN A PROGRAM-CONTROLLED   00116000
*                                       INTERRUPTION (PCI) OCCURS    */ 00117000
*    2 IOBRSV05 CHAR(1),             /* RESERVED                     */ 00118000
*    2 IOBCINOP CHAR(1),             /* OFFSET OF THE LAST I/O  01S0039 00119000
*                                       COMMAND FOR INPUT OPERATION     00120000
*                                       (NOP CCW) FROM THE ORIGIN OF    00121000
*                                       THE ICB                      */ 00122000
*    2 IOBCONOP CHAR(1),             /* OFFSET OF THE LAST I/O  01S0039 00123000
*                                       COMMAND FOR AN OUTPUT   01S0039 00124000
*                                       OPERATION (NOP CCW) FROM        00125000
*                                       THE ORIGIN OF THE ICB        */ 00126000
*    2 IOBCECB  FIXED(31),           /* EVENT CONTROL BLOCK USED BY     00127000
*                                       BSAM OR QSAM.  SHOWS THE        00128000
*                                       STATUS OF THE I/O OPERATION. */ 00129000
*    2 IOBCICB  PTR(31),             /* ADDRESS OF THE FIRST    01S0039 00130000
*                                       INTERRUPT CONTROL BLOCK (ICB)   00131000
*                                       ON THE ICB QUEUE             */ 00132000
*    2 IOBCNOPA PTR(31);             /* ADDRESS OF THE NOP COMMAND AT   00133000
*                                       THE END OF THE QUEUE         */ 00134000
*                                                               01S0039 00135000
*                        /* **************************************** */ 00136000
*                        /*    QSAM,BSAM,BPAM PREFIX                 */ 00137000
*                        /*    NORMAL SCHEDULING *** 8 BYTES         */ 00138000
*                        /* **************************************** */ 00139000
*                                                               01S0040 00140000
* DECLARE 1 IOBORG01                                            01S0040 00141000
*       DEF(IOBPREFX) POS(9) BDY(WORD),                         01S0040 00142000
*    2 IOBNIOBA PTR(31),             /* ADDRESS OF THE NEXT IOB 01S0040 00143000
*                                       ASSOCIATED WITH ONE PARTICULAR  00144000
*                                       DCB.  THE IOB'S ARE CHAINED     00145000
*                                       IN SEQUENTIAL ORDER.         */ 00146000
*     3 IOBNFLG1 CHAR(1),            /* FLAG BYTE                    */ 00147000
*      4 IOBPRTOV BIT(1),            /* PRTOV HAS OCCURRED           */ 00148000
*      4 IOBWRITE BIT(1),            /* A WRITE OPERATION IS IN 01S0040 00149000
*                                       PROCESS                      */ 00150000
*      4 IOBREAD  BIT(1),            /* A READ OPERATION IS IN  01S0040 00151000
*                                       PROCESS                      */ 00152000
*      4 IOBUPDAT BIT(1),            /* UPDATE FLAG.  SET ON TOGETHER   00153000
*                                       WITH BIT 1 OF THIS BYTE TO      00154000
*                                       SHOW THAT THE BLOCK IS TO BE    00155000
*                                       UPDATED.  CAN ONLY OCCUR IF     00156000
*                                       THE OPEN PARAMETER IS UPDAT. */ 00157000
*      4 IOBBKSPC BIT(1),            /* IOB BEING USED FOR BACKSPACE,   00158000
*                                       CONTROL OR NOTE/POINT   01S0040 00159000
*                                       OPERATION                    */ 00160000
