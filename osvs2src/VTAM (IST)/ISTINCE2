SVTM     TITLE ' SSCP -- ISTINCE2 -- PAB SWITCH I/O                    *00001000
                        '                                               00002000
ISTINCE2 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCE2  75.128'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0055 00014000
*   /*****************************************************************/ 00015000
*   /*                                                               */ 00016000
*   /* MAINLINE CODE ISTINCE2 -- PAB SWITCH I/O                      */ 00017000
*   /*                                                               */ 00018000
*   /*****************************************************************/ 00019000
*                                                                  0055 00020000
*/*  NOTE:  THIS ROUTINE DOES NOT REQUIRE A REGISTER SAVEAREA BECAUSE   00021000
*         NO EXTERNAL ROUTINES ARE INVOKED.                          */ 00022000
*                                                                  0055 00023000
*                                                                  0055 00024000
*    DO;                                                           0055 00025000
*/***  CRRMAINT  PROCNM(ISTINCE2)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00026000
**/                                                                0056 00027000
*                                                                  0056 00028000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0056 00029000
*    RFY ISTRPH  ;                                                 0056 00030000
*    RFY ISTLCCW ;                                                 0057 00031000
*    RFY ISTLCPB ;                                                 0058 00032000
*    RFY ISTZSAVE;                                                 0059 00033000
*    RFY ISTPFCRR;                                                 0060 00034000
*    RFY ISTCFCRR;                                                 0061 00035000
*    RFY ISTZCRR ;                                                 0062 00036000
*    RFY ISTASCRR;                                                 0063 00037000
*    RFY ISTAPCRR;                                                 0064 00038000
*    RFY ISTCRA  ;                                                 0065 00039000
*    RFY ISTCNSTA;                                                 0066 00040000
*    RFY ISTVYCRR;                                                 0067 00041000
*/* END OF ATBUILD */                                              0068 00042000
*    /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                  0068 00043000
*NCSPLRPH->RPHCRR->CRACRR->NINCE2 = '1'B;                          0068 00044000
         L     @15,NCSPLRPH(,R1)                                   0068 00045000
         L     @15,RPHCRR(,@15)                                    0068 00046000
         L     @15,CRACRR(,@15)                                    0068 00047000
         OI    NINCE2(@15),B'00001000'                             0068 00048000
*    /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                 0069 00049000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0069 00050000
         XC    VYCCRT+1(11,@15),VYCCRT+1(@15)                      0069 00051000
         MVI   VYCCRT(@15),B'00000000'                             0069 00052000
*NCSPLRPH->RPHCRR->CRACRR->CINCE2 = '1'B;                          0070 00053000
         OI    CINCE2(@15),B'00001000'                             0070 00054000
*    END;                                                          0071 00055000
*   NCSPLPTR=R1;                    /* GET THE NCSPL ADDRESS         */ 00056000
         LR    NCSPLPTR,R1                                         0072 00057000
*   RFY                                                            0073 00058000
*     ISTNCSPL BASED(NCSPLPTR);                                    0073 00059000
*   RDTEPTR=NCSPLRAD;               /* GET THE RDTE ADDRESS          */ 00060000
         L     RDTEPTR,NCSPLRAD(,NCSPLPTR)                         0074 00061000
*   DO WHILE RPRENTRY^=RPRHDTYP;    /* LOCATE THE CONTROLLING HEADER    00062000
*                                      RDTE                          */ 00063000
         B     @DE00075                                            0075 00064000
@DL00075 DS    0H                                                  0076 00065000
*     RDTEPTR=RPRNSNEA;             /* GET THE NEXT HIGHER NODE      */ 00066000
         L     RDTEPTR,RPRNSNEA(,RDTEPTR)                          0076 00067000
*   END;                                                           0077 00068000
@DE00075 CLC   RPRENTRY(1,RDTEPTR),RPRHDTYP(RDTEPTR)               0077 00069000
         BNE   @DL00075                                            0077 00070000
*   NCSPLSPL->NCSPLWEL=NCSPLRPH;    /* PUT THE CURRENT RPH ADDRESS IN   00071000
*                                      THE I/O NCSPL                 */ 00072000
         L     @15,NCSPLSPL(,NCSPLPTR)                             0078 00073000
         L     @14,NCSPLRPH(,NCSPLPTR)                             0078 00074000
         ST    @14,NCSPLWEL(,@15)                                  0078 00075000
*   R1=NCSPLRPH;                    /* GET THE CURRENT RPH ADDRESS   */ 00076000
*                                                                  0079 00077000
         LR    R1,@14                                              0079 00078000
*   /*****************************************************************/ 00079000
*   /*                                                               */ 00080000
*   /* QUEUE THE I/O NCSPL TO THE BOUNDRY NODE'S VARY PAB            */ 00081000
*   /*                                                               */ 00082000
*   /*****************************************************************/ 00083000
*                                                                  0080 00084000
*                                                                  0080 00085000
*/*  TPQUE (NCSPLSPL,DYPPAB) NORM */                               0080 00086000
*                                                                  0080 00087000
*    DO;                                                           0080 00088000
*                                                                  0080 00089000
*    RFY ISTATCVT;                                                 0081 00090000
*    RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;                0082 00091000
*    GEN REFS(GPR01P) SETS(RPHWORK)                                0083 00092000
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0083 00093000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00094000
*    GPR00P=0;                                                     0084 00095000
         SLR   GPR00P,GPR00P                                       0084 00096000
*    GPR15P=ADDR(DYPPAB); /*HOLD PAB PTR*/                         0085 00097000
         L     GPR15P,RDTVPAB(,RDTEPTR)                            0085 00098000
         LA    GPR15P,DYPPAB(,GPR15P)                              0085 00099000
*    GPR04P=NCSPLSPL; /*WORK ELE PTR*/                             0086 00100000
         L     GPR04P,NCSPLSPL(,NCSPLPTR)                          0086 00101000
*    GPR05P=GPR15P; /*PASS PAB PTR*/                               0087 00102000
         LR    GPR05P,GPR15P                                       0087 00103000
*    CALL ATCLCPTR->ATCAP37->ISTAPC37;                             0088 00104000
         L     @14,ATCLCPTR                                        0088 00105000
         L     @15,ATCAP37(,@14)                                   0088 00106000
         BALR  @14,@15                                             0088 00107000
*    GEN(NOPR  0)NOSETS NOREFS;                                    0089 00108000
         NOPR  0                                                        00109000
*    RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;              0090 00110000
*    END; /**** END OF TPQUE GENERATION ****/                      0091 00111000
*                                                                  0091 00112000
*   /*****************************************************************/ 00113000
*   /*                                                               */ 00114000
*   /* WAIT FOR THE I/O PROCESSING TO COMPLETE                       */ 00115000
*   /*                                                               */ 00116000
*   /*****************************************************************/ 00117000
*                                                                  0092 00118000
*                                                                  0092 00119000
*/*  TPWAIT   */                                                   0092 00120000
*    DO; /* BEGIN TPWAIT GENERATION*/                              0092 00121000
*    RFY (GPR01P,GPR14P,GPR15P) RSTD;                              0093 00122000
*    RFY (GPR00P) RSTD;                                            0094 00123000
*    GEN (STM   GPR00P,GPR15P,RPHWORK(GPR01P))                     0095 00124000
*    REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);                     0095 00125000
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)                            00126000
*    RFY (GPR00P) UNRSTD;                                          0096 00127000
*    GPR15P=ADDR(LHAA0015);                                        0097 00128000
         LA    GPR15P,LHAA0015                                     0097 00129000
*    GPR14P=0;                                                     0098 00130000
         SLR   GPR14P,GPR14P                                       0098 00131000
*    GPR15P=GPR15P & '00FFFFFF'X;                                  0099 00132000
         N     GPR15P,@CF00789                                     0099 00133000
*    GPR15P=GPR15P | RPHWT1;                                       0100 00134000
         O     GPR15P,@CF02166                                     0100 00135000
*    CS (GPR14P,GPR15P,GPR01P->RPHRESMA);                          0101 00136000
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         0101 00137000
*    BC (7,LHAB0015);                                              0102 00138000
*                                                                  0102 00139000
         BC    7,LHAB0015                                          0102 00140000
*/* TPEXIT  NONE */                                                0103 00141000
*    DO;                                                           0103 00142000
*      RESPECIFY GPR15P RSTD;                                      0104 00143000
*                                                                  0104 00144000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)  */                         0105 00145000
*/* END OF ATBUILD */                                              0105 00146000
*                                                                  0105 00147000
*    DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */                 0105 00148000
*      GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/                 0106 00149000
         L     GPR15P,ATCLCPTR                                     0106 00150000
*    END; /*LISATCVT*/                                             0107 00151000
*      RESPECIFY GPR00F RSTD;                                      0108 00152000
*      GPR00F=0;     /*SET PARAMETER REGISTER TO 0*/               0109 00153000
         SLR   GPR00F,GPR00F                                       0109 00154000
*      GPR15P=GPR15P->ATCAP31;                                     0110 00155000
         L     GPR15P,ATCAP31(,GPR15P)                             0110 00156000
*      DCL FMZL0020 BASED(GPR15P) LABEL VALUERANGE(*);             0111 00157000
*      CALL FMZL0020;                                              0112 00158000
         BALR  @14,@15                                             0112 00159000
*      RESPECIFY (GPR00F,GPR15P) UNRSTD;                           0113 00160000
*    END; /***** END OF TPEXIT  *****/                             0114 00161000
*LHAB0015:;                                                        0115 00162000
LHAB0015 DS    0H                                                  0116 00163000
*    GPR01P->RPHRESMA = 0;                                         0116 00164000
         SLR   @15,@15                                             0116 00165000
         ST    @15,RPHRESMA(,GPR01P)                               0116 00166000
*    RFY (GPR00P) RSTD;                                            0117 00167000
*LHAA0015:;                                                        0118 00168000
LHAA0015 DS    0H                                                  0119 00169000
*    GEN (LM    GPR00P,GPR15P,RPHWORK(GPR01P))                     0119 00170000
*    REFS(RPHWORK,GPR01P) SETS(GPR00P,GPR15P);                     0119 00171000
         LM    GPR00P,GPR15P,RPHWORK(GPR01P)                            00172000
*    RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;                     0120 00173000
*    END; /*****END OF TPWAIT *****/                               0121 00174000
*                                                                  0121 00175000
*/* TPEPILOG   */                                                  0122 00176000
*    GEN (PRINT OFF);                                              0122 00177000
*                                                                  0122 00178000
         PRINT OFF                                                      00179000
*/*   PROGRAM OPERATOR MESSAGE HEADER                                */ 00180000
*DCL  1  ISTPOHD  BDY(WORD) BASED,                                 0123 00181000
*        2 POHRSVD1 CHAR(1),       /* RESERVED                       */ 00182000
*        2 POHSTAT,                /* HEADER STATUS BITS             */ 00183000
*          3 POHEND   BIT(1),      /* END LINE 1=YES                 */ 00184000
*          3 POHDATA  BIT(1),      /* DATA LINE 1=YES                */ 00185000
*          3 POHLBL   BIT(1),      /* LABEL LINE 1=YES               */ 00186000
*          3 POHCNTRL BIT(1),      /* CONTROL LINE 1=YES             */ 00187000
*          3 POHRSVD2 BIT(2),      /* RESERVED                       */ 00188000
*          3 POHRREQ  BIT(1),      /* REPLY REQUESTED 1=YES          */ 00189000
*          3 POHGEN   BIT(1),      /* ID ORIGIN  0=GENERATED BY VTAM */ 00190000
*                                  /*            1=GENERATED BY POA  */ 00191000
*        2 POHID    FIXED(16);     /* MESSAGE IDENTIFIER             */ 00192000
*/*            VTAM PREFIX FOR QUEUE ELEMENTS                         * 00193000
*/*  INITIAL LEVEL                                     001 03/04/75  */ 00194000
*DECLARE                                                           0124 00195000
*  1 ISTPFX  BDY(WORD)  BASED,                                     0124 00196000
*   2 PFXID        CHAR(4),             /* QUEUE ACRONYM (SAME AS  0124 00197000
*                                          QABID)                     * 00198000
*     3 PFXCBID    BIT(8),              /* CONTROL BLOCK ID           * 00199000
*   2 PFXPRTY      CHAR(1),             /* PRIORITY OF ELEMENT        * 00200000
*   2 PFXLNG       CHAR(3),             /* ELEMENT LENGTH,         0124 00201000
*                                          INCLUDING PREFIX           * 00202000
*   2 PFXFWD       PTR(31),   /* POINTER TO NEXT QUEUE             0124 00203000
*                                          ELEMENT (IF LAST        0124 00204000
*                                          POINTER TO QAB)            * 00205000
*   2 PFXBCK       PTR(31) ;   /* POINTER TO PREVIOUS              0124 00206000
*                                          QUEUE ELEMENT (IF       0124 00207000
*                                          FIRST, POINTER TO QAB)     * 00208000
*DCL NCSPLTYP BIT(8) CONSTANT('60'X);   /* NCSPL CONTROL BLOCK ID     * 00209000
*   /* NETWORK CONFIGURATION SERVICES CONTROL BLOCK  */            0126 00210000
*/*  DELTA                                             13 02/08/75  */  00211000
*DECLARE                                                           0126 00212000
*  1 ISTNCSPL BASED BDY(DWORD),                                    0126 00213000
*   2 *      CHAR(16) BDY(WORD),                                   0126 00214000
*     3 NCSPFX CHAR(LENGTH(ISTPFX)), /* QUE ELEMENT PREFIX           */ 00215000
*       4 *        CHAR(12),                                       0126 00216000
*       4 NCSCRWKA PTR(31),       /* PTR TO WORK AREA FOR C/R I/O    */ 00217000
*   2 *           BIT(8),         /*RESERVED                         */ 00218000
*   2 NCSPLVCD    BIT(8),         /*VERB CODE                        */ 00219000
*   2 NCSPLVBF    CHAR(2),        /*VERB MODIFIER FLAGS              */ 00220000
*      3 NCSVBF01 BIT(1),         /* UNUSED                          */ 00221000
*      3 NCSVBF02 BIT(1),         /* UNUSED                          */ 00222000
*      3 NCSVBF03 BIT(1),         /* MODIFY MSG SUP                  */ 00223000
*      3 NCSVBF04 BIT(1),         /* TYPE=VTAM                       */ 00224000
*      3 NCSVBF05 BIT(1),         /* LINETRACE=1                     */ 00225000
*      3 NCSVBF06 BIT(1),         /* I/O TRACE=1                     */ 00226000
*      3 NCSVBF07 BIT(1),         /* BUFFER TRACE=1                  */ 00227000
*      3 NCSVBF08 BIT(1),         /* MODIFY TPRINT                   */ 00228000
*      3 NCSVBF09 BIT(1),         /* MODIFY TEST                     */ 00229000
*      3 NCSVBF10 BIT(1),         /* MODIFY TRACE=NO                 */ 00230000
*      3 NCSVBF11 BIT(1),         /* VARY INTERNAL ONLY & MODIFY     */ 00231000
*                                 /*  TRACE = YES                    */ 00232000
*      3 NCSVBF12 BIT(1),         /* VARY IMMEDIATE  & MODIFY        */ 00233000
*                                 /*  NETSOL=NO                      */ 00234000
*      3 NCSVBF13 BIT(1),         /* MODIFY NETSOL=YES               */ 00235000
*      3 NCSVBF14 BIT(1),         /* VARY LOGON & MODIFY             */ 00236000
*                                 /*  CHANGE                         */ 00237000
*      3 NCSVBF15 BIT(1),         /* VARY DEACTIVATE & VARY NORMAL   */ 00238000
*                                 /*  CLOSE(ERP)                     */ 00239000
*      3 NCSVBF16 BIT(1),         /* VARY ACTIVATE & ERP REQUEST     */ 00240000
*                                 /*   & MODIFY DUMP                 */ 00241000
*   2 NCSPLVFL    CHAR(1),        /*COMMAND FLAGS                    */ 00242000
*    3 NCSVNCSA   BIT(1),         /*NCSPL IS ACTIVE                  */ 00243000
*    3 NCSVDEL    BIT(1),         /*DELETE NCSPL                     */ 00244000
*    3 NCSVSTAT   BIT(1),         /*INDICATE NCSPL WAS NOT GETMAINED */ 00245000
*    3 NCSDCONT   BIT(1),         /* DO DISCONACT                    */ 00246000
*    3 NCSABCON   BIT(1),         /* DO ABANDON CONNECTION           */ 00247000
*    3 NCSDACLK   BIT(1),         /* DO ACTIVATE LINE                */ 00248000
*    3 NCSRSV02   BIT(2),         /* RESERVED                        */ 00249000
*   2 NCSPLVF1    CHAR(1),        /* SPECIAL PROC FLAGS              */ 00250000
*    3 NCSOVFMC   BIT(1),         /* PURGE ONLY VARY'S FMCB          */ 00251000
*   2 NCSPRMRC    CHAR(1),        /* PURGE MINOR RETURN CODE FIELD   */ 00252000
*   2 NCSIORC     CHAR(1),        /* I/O OR RESTART RETURN CODE      */ 00253000
*   2 NCSRRNPT    CHAR(4),        /* ADDR RN ENTRY FOR LINE TRACE    */ 00254000
*   2 NCSPLRID    CHAR(8),        /*ID = 'NODENAME'                  */ 00255000
*   2 NCSPLLID    CHAR(8),        /*LOGON = 'NODENAME'               */ 00256000
*   2 NCSDLID     CHAR(6),        /* OFFHOOK ID                      */ 00257000
*   2 NCSCRCMD    FIXED(8),       /* C/R I/O COMMAND CODE            */ 00258000
*   2 NCSCRPID    PTR(8),         /* C/R PATH ID (PID)               */ 00259000
*   2 NCSPLRPT    PTR(31),        /*ID='NODENAME' RDTE ADDRESS       */ 00260000
*   2 NCSPLLPT    PTR(31),        /*LOGON/LOGOFF='NODENAME' RDTE ADDR*/ 00261000
*   2 NCSPLCMD    CHAR(2),        /*3705 CONTROL COMMAND CODES       */ 00262000
*     3 NCSCMDA  BIT(8),          /*RH FLAGS                         */ 00263000
*       4 NCSPLSYS BIT(1),        /*ON-SYSTEM OFF-FM                 */ 00264000
*       4 NCSPLCTL BIT(1),        /*ON-CONTROL OFF-DATA              */ 00265000
*       4 NCSPLSYN BIT(1),        /*ON=SYNCHR OFF-ASYNCHR            */ 00266000
*       4 NCSPLFLO BIT(1),        /*ON-WITH FLOW  OFF-AGAINST FLOW   */ 00267000
*       4 NCSPLUFM BIT(1),        /*ON=UNFORMATTED RU,OFF=FORMATTED  */ 00268000
*       4 *        BIT(3),        /*RESERVED                         */ 00269000
*     3 NCSCMDB  BIT(8),          /*COMMAND FIELD                    */ 00270000
*   2 NCSNCPEP    BIT(8),         /* EP SUBCHANNEL ADDR              */ 00271000
*   2 NCSMSGSP    CHAR(1),        /* RESERVED FOR MESSAGE SURPRESSION*/ 00272000
*   2 NCSPLUAD    CHAR(3),        /*U = 'UNIT ADDRESS'               */ 00273000
*   2 NCSCRFLG    CHAR(1),        /* MISCELLANEOUS C/R FLAGS         */ 00274000
*     3 NCSPLWRM    BIT(1),       /* COMMAND SPECIFIED WARM PARAM,   */ 00275000
*                                 /* 1 = YES                         */ 00276000
*     3 NCSPLCLD    BIT(1),       /* COMMAND SPECIFIED COLD PARAM,   */ 00277000
*                                 /* 1 = YES                         */ 00278000
*     3 NCSFMCLD    BIT(1),       /* 370X RESPONDED COLD TO ACTPU    */ 00279000
*                                 /* (ERP), 1 = YES                  */ 00280000
*     3 NCSFMERP    BIT(1),       /* 370X RESPONDED ERP TO ACTPU     */ 00281000
*                                 /* (ERP), 1 = YES                  */ 00282000
*     3 NCSCRDSE    BIT(1),       /* 0=C/R DATA SET DOES NOT EXIST   */ 00283000
*                                 /* OR IS NULL                      */ 00284000
*                                 /* 1=C/R DATA SET EXISTS           */ 00285000
*                                 /* AND IS NOT NULL                 */ 00286000
*     3 NCSOPRQU    BIT(1),       /* OPERATOR REQUESTS REIPL, 1 = YES*/ 00287000
*     3 NCSUSRNF    BIT(1),       /* EARLY WARNING PURGE HAS ALREADY */ 00288000
*                                 /* OCCURED, 1 = YES                */ 00289000
*     3 NCSSTCMD    BIT(1),       /* ACTIVATE IS A RESULT OF         */ 00290000
*                                 /* START VTAM COMMAND, 1 = YES     */ 00291000
*   2 NCSPLWRE    BIT(32),        /* WTOR ECB                        */ 00292000
*   2 NCSPLWRF    CHAR(72),       /* WTOR REPLY AREA                 */ 00293000
*   2 NCSPLRAD    PTR(31),        /* RDTE PTR FOR ALLOCATE/DEALLOCATE*/ 00294000
*     3 NCSLSCMD BIT(8),          /* LINE SCHED COMMAND TYPE         */ 00295000
*     3 NCSPLRCD BIT(8),          /* HEX VALUE FOR LINE SCHED PARAM  */ 00296000
*   2 NCSIOCID    CHAR(4),        /* CURRENT CID FOR DAF LOCK HELD   */ 00297000
*     3  NCSPLCID CHAR(4),        /* ALIAS FOR ABOVE                 */ 00298000
*        4  NCSPLSAF  CHAR(2),    /* SOURCE ADDRESS                  */ 00299000
*        4  NCSPLDAF  CHAR(2),    /* DESTINATION ADDRESS             */ 00300000
*   2 NCSPLWKA    PTR(31),        /* NCS WORK AREA ADDRESS           */ 00301000
*   2 NCSPLCVT    PTR(31),        /* POINTER TO VTAM CVT             */ 00302000
*   2 NCSPLRPH    PTR(31),        /* POINTER TO RPH                  */ 00303000
*   2 NCSPLECB    BIT(32),        /* ECB FOR VARY PGS                */ 00304000
*   2 NCSPLFSB    PTR(31),        /* ADDRESS OF USS-FSS FSB OR RU    */ 00305000
*   2 NCSPLRCR    PTR(31),        /* ADDRESS RDTE FOR CR             */ 00306000
*   2 NCSPLFL1    CHAR(1),        /* VARY FLAGS                      */ 00307000
*     3 NCSPL2IN  BIT(1),         /* SECOND ENTRY                    */ 00308000
*     3 NCSPLSOM  BIT(1),         /* SUPRESS OP. MESSAGE             */ 00309000
*     3 NCSPLEBN  BIT(1),         /* ENTRY FROM BNN SSCP             */ 00310000
*     3 NCSPLREQ  BIT(1),         /* NCSPL FROM RESTORE              */ 00311000
*     3 NCSPLNON  BIT(1),         /* DO NOT ADDR RDT SEGMENT         */ 00312000
*     3 NCSPLAPU  BIT(1),         /* ACTIVATE PHYSICAL DONE BY LOAD  */ 00313000
*     3 NCSPL2CN  BIT(1),         /* SECOND CONTACT REQUEST          */ 00314000
*     3 NCSPLPOS  BIT(1),         /* BNN POSTED                      */ 00315000
*   2 NCSPLFL2    CHAR(1),        /* VARY FLAGS                      */ 00316000
*     3 NCSPLVDP  BIT(1),         /* NCSPL ON VARDEF PAB             */ 00317000
*     3 NCSPLRSF  BIT(1),         /* NO SICK CLEAR BEFORE I/O        */ 00318000
*     3 NCSPLRIO  BIT(1),         /* I/O = RESTART I/O               */ 00319000
*     3 NCSOUFMC  BIT(1),         /* PURGE ONLY USER FMCB            */ 00320000
*     3 NCSPLDS   BIT(1),         /* 1=CIO SAVED RESPONSE DATA       */ 00321000
*     3 NCSREMPO  BIT(1),         /* REMOTE POWER OFF SPEC BY OPER   */ 00322000
*     3 NCSPLVIS  BIT(1),         /* VARY IMM START CALLER OF CVP    */ 00323000
*     3 NCSPLVIT  BIT(1),         /* VARY IMM TERM CALLER OF CVP     */ 00324000
*   2 NCSPLFL3    CHAR(1),        /* FLAG BYTE                       */ 00325000
*     3 NCSPLMOM  BIT(1),         /* ERP CAUSED BY FAILURE OF      0126 00326000
*                                    HIGHER NODE                     */ 00327000
*     3 NCSEPP    BIT(1),         /* 1=EP SUBCHANNEL PRESENT         */ 00328000
*     3 NCSPLANS  BIT(1),         /* ON FOR ACT IN ANSWER MODE       */ 00329000
*                                 /* OFF FOR ACT IN NON-ANSWER MODE  */ 00330000
*     3 NCSGID    BIT(1),         /* ON FOR PATH WITH GID PARM       */ 00331000
*     3 NCSPID    BIT(1),         /* ON FOR PATH WITH PID PARM       */ 00332000
*     3 NCSANS    BIT(1),         /* ON FOR ACTIVATE WITH ANS PARM   */ 00333000
*     3 NCSFINAL  BIT(1),         /* FINAL PARM EXISTS               */ 00334000
*     3 NCSEND    BIT(1),         /* END PARM EXISTS                 */ 00335000
*   2 NCSPLFL4    CHAR(1),        /* FLAG BYTE                       */ 00336000
*     3 NCSDLNID  BIT(1),         /* ID PRESENT IN NCSPL - OFF       */ 00337000
*     3 NCSPLRSO  BIT(1),         /* RESET ONLY                      */ 00338000
*     3 NCSPLVID  BIT(1),         /* ORIG CMD WAS MODIFY DUMP        */ 00339000
*     3 NCSDSPE   BIT(1),         /* DISPLAY EVERY                   */ 00340000
*     3 NCSDSPA   BIT(1),         /* DISPLAY ACT                     */ 00341000
*     3 NCSDSPI   BIT(1),         /* DISPLAY INACT                   */ 00342000
*     3 NCSDSPN   BIT(1),         /* DISPLAY NONE                    */ 00343000
*     3 NCSPL2AP  BIT(1),         /* SECOND ACTPU TO 320X            */ 00344000
*   2 NCSDLRSN    CHAR(1),        /* DIAL REASON CODES               */ 00345000
*   2 NCSPLFL5    CHAR(1),        /* FLAG BYTE                       */ 00346000
