*CE3WAPTR=NXTAVAIL;/* GET THE NEXT AVAILABLE ADDRESS IN THE WORKAREA*/  00059000
         L     @14,NCSPLWKA(,R1)                                   0095 00060000
         L     CE3WAPTR,NXTAVAIL(,@14)                             0095 00061000
*CE3WAPTR=(CE3WAPTR+SEVEN)&MINUS8;       /*ALIGN ON DOUBLE WORD*/  0096 00062000
         LA    @00,7                                               0096 00063000
         ALR   @00,CE3WAPTR                                        0096 00064000
         N     @00,@CF00768                                        0096 00065000
         LR    CE3WAPTR,@00                                        0096 00066000
