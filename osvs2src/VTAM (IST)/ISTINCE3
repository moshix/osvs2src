         L     @14,RPHCRR(,@14)                                    0085 00036000
         L     @14,CRACRR(,@14)                                    0085 00037000
         OI    NINCE3(@14),B'00000100'                             0085 00038000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0086 00039000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0086 00040000
