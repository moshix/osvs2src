SVTM     TITLE ' SSCP -- ISTINCE3 -- PU CLEANUP                        *00001000
                        '                                               00002000
ISTINCE3 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCE3  75.128'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*      /* MAINLINE CODE OF ISTINCE3 -- PU CLEANUP */               0073 00014000
*                                                                  0073 00015000
* DO;                                                              0073 00016000
*/***  CRRMAINT  PROCNM(ISTINCE3)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00017000
**/                                                                0074 00018000
*                                                                  0074 00019000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0074 00020000
* RFY ISTLCCW ;                                                    0074 00021000
* RFY ISTLCPB ;                                                    0075 00022000
* RFY ISTZSAVE;                                                    0076 00023000
* RFY ISTPFCRR;                                                    0077 00024000
* RFY ISTCFCRR;                                                    0078 00025000
* RFY ISTZCRR ;                                                    0079 00026000
* RFY ISTASCRR;                                                    0080 00027000
* RFY ISTAPCRR;                                                    0081 00028000
* RFY ISTCRA  ;                                                    0082 00029000
* RFY ISTCNSTA;                                                    0083 00030000
* RFY ISTVYCRR;                                                    0084 00031000
*/* END OF ATBUILD */                                              0085 00032000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0085 00033000
*NCSPLRPH->RPHCRR->CRACRR->NINCE3 = '1'B;                          0085 00034000
