*NCSPLPTR=R1;                      /* GET THE NCSPL ADDRESS */     0098 00072000
         LR    NCSPLPTR,R1                                         0098 00073000
*RFY ISTNCSPL BASED(NCSPLPTR);                                     0099 00074000
*R1 = NCSPLRPH;                    /*GET RPH ADDR*/                0100 00075000
         L     R1,NCSPLRPH(,NCSPLPTR)                              0100 00076000
*RFY ISTRPH BASED(R1);                                             0101 00077000
*R9PURDT = NCSPLRPT;               /*GET ADDR SW PU RDTE*/         0102 00078000
         L     R9PURDT,NCSPLRPT(,NCSPLPTR)                         0102 00079000
*R8DNCSPL = RCCSWTOX;              /*GET ADDR SW TO 370X NCSPL*/   0103 00080000
         L     R8DNCSPL,RCCSWTOX(,R9PURDT)                         0103 00081000
*RFY ISTNCSPL BASED(R8DNCSPL);                                     0104 00082000
*                                  /*FORMAT DIAL DEALLOCATE NCSPL*/     00083000
*NCSPLVCD = NCSPLVC8;              /*VERB CODE = DIAL*/            0105 00084000
         MVI   NCSPLVCD(R8DNCSPL),X'08'                            0105 00085000
*NCSPLVBF = NCSDLK;                /*VERB MODIFIER = DEALLOCATE*/  0106 00086000
         MVC   NCSPLVBF(2,R8DNCSPL),@CB01073                       0106 00087000
*NCSPLRPT = RPRNSNEA;              /*ADDR OF LINK PU IS DIALED OVER*/   00088000
         L     @14,RPRNSNEA(,R9PURDT)                              0107 00089000
         ST    @14,NCSPLRPT(,R8DNCSPL)                             0107 00090000
*NCSPLRAD = R9PURDT;               /*ADDR OF SW PU RDTE*/          0108 00091000
         ST    R9PURDT,NCSPLRAD(,R8DNCSPL)                         0108 00092000
*NCSVSTAT = ON;                    /*SET STATIC ON-DO NOT FREE NCSPL*/  00093000
         OI    NCSVSTAT(R8DNCSPL),B'00100000'                      0109 00094000
*NCSPLWEL = R1;                    /*ADDR OF RPH TO USE*/          0110 00095000
         ST    R1,NCSPLWEL(,R8DNCSPL)                              0110 00096000
*RFY R7WORK RSTD;                                                  0111 00097000
*R7WORK = R9PURDT;                 /*ADDR PU*/                     0112 00098000
         LR    R7WORK,R9PURDT                                      0112 00099000
*                                  /*GO UP NSNEA CHAIN TO FIND RDTE */  00100000
*                                  /*OF RN IN WHICH LINK RESIDES*/ 0113 00101000
*DO WHILE R7WORK -> RPRENTRY ^= RPRENTRN;                          0113 00102000
         B     @DE00113                                            0113 00103000
@DL00113 DS    0H                                                  0114 00104000
*     R7WORK = R7WORK-> RPRNSNEA;    /*GET NEXT ENTRY*/            0114 00105000
         L     R7WORK,RPRNSNEA(,R7WORK)                            0114 00106000
*END;                                                              0115 00107000
@DE00113 CLI   RPRENTRY(R7WORK),X'01'                              0115 00108000
         BNE   @DL00113                                            0115 00109000
*                                  /*QUEUE DIAL DEALLOCATE NCSPL TO */  00110000
*                                  /*370X PAB OF LINK*/            0116 00111000
*/*  TPQUE (R8DNCSPL,DYPPAB) NORM */                               0116 00112000
*                                                                  0116 00113000
*  DO;                                                             0116 00114000
*  RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;                  0117 00115000
*  GEN REFS(GPR01P) SETS(RPHWORK)                                  0118 00116000
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0118 00117000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00118000
*  GPR00P=0;                                                       0119 00119000
         SLR   GPR00P,GPR00P                                       0119 00120000
*  GPR15P=ADDR(DYPPAB); /*HOLD PAB PTR*/                           0120 00121000
         L     GPR15P,RDTVPAB(,R7WORK)                             0120 00122000
         LA    GPR15P,DYPPAB(,GPR15P)                              0120 00123000
*  GPR04P=R8DNCSPL; /*WORK ELE PTR*/                               0121 00124000
         LR    GPR04P,R8DNCSPL                                     0121 00125000
*  GPR05P=GPR15P; /*PASS PAB PTR*/                                 0122 00126000
         LR    GPR05P,GPR15P                                       0122 00127000
*  CALL ATCLCPTR->ATCAP37->ISTAPC37;                               0123 00128000
         L     @06,ATCLCPTR                                        0123 00129000
         L     @15,ATCAP37(,@06)                                   0123 00130000
         BALR  @14,@15                                             0123 00131000
*  GEN(NOPR  0)NOSETS NOREFS;                                      0124 00132000
         NOPR  0                                                        00133000
*  RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;                0125 00134000
*  END; /**** END OF TPQUE GENERATION ****/                        0126 00135000
*                                  /*WAIT FOR PU AND LINK TO BE */ 0127 00136000
*                                  /*DISCONNECTED AND DEALLOCATED*/     00137000
*/*  TPWAIT   */                                                   0127 00138000
*  DO; /* BEGIN TPWAIT GENERATION*/                                0127 00139000
*  RFY (GPR01P,GPR14P,GPR15P) RSTD;                                0128 00140000
*  RFY (GPR00P) RSTD;                                              0129 00141000
*  GEN (STM   GPR00P,GPR15P,RPHWORK(GPR01P))                       0130 00142000
