SVTM     TITLE ' SSCP -- ISTINCE5 -- DIAL GIVE RESOURCES               *00001000
                        '                                               00002000
ISTINCE5 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTINCE5  75.265'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
*      /* MAINLINE CODE OF ISTINCE5 -- DIAL GIVE RESOURCES */   01S0037 00014000
* DO;                                                           01S0037 00015000
*/***  CRRMAINT  PROCNM(ISTINCE5)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00016000
**/                                                             01S0038 00017000
*                                                               01S0038 00018000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */               01S0038 00019000
* RFY ISTLCCW ;                                                 01S0038 00020000
* RFY ISTLCPB ;                                                 01S0039 00021000
* RFY ISTZSAVE;                                                 01S0040 00022000
* RFY ISTCFCRR;                                                 01S0041 00023000
* RFY ISTZCRR ;                                                 01S0042 00024000
* RFY ISTASCRR;                                                 01S0043 00025000
* RFY ISTAPCRR;                                                 01S0044 00026000
* RFY ISTCRA  ;                                                 01S0045 00027000
*/* END OF ATBUILD */                                           01S0046 00028000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                  01S0046 00029000
*NCSPLRPH->RPHCRR->CRACRR->NINCE5 = '1'B;                       01S0046 00030000
         L     @15,NCSPLRPH(,NCSPLPTR)                          01S0046 00031000
         L     @15,RPHCRR(,@15)                                 01S0046 00032000
         L     @15,CRACRR(,@15)                                 01S0046 00033000
         OI    NINCE5(@15),B'00010000'                          01S0046 00034000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                 01S0047 00035000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                       01S0047 00036000
         XC    VYCCRT+1(11,@15),VYCCRT+1(@15)                   01S0047 00037000
         MVI   VYCCRT(@15),B'00000000'                          01S0047 00038000
*NCSPLRPH->RPHCRR->CRACRR->CINCE5 = '1'B;                       01S0048 00039000
         OI    CINCE5(@15),B'00010000'                          01S0048 00040000
* END;                                                          01S0049 00041000
*RFY R15 RSTD;                                                  01S0050 00042000
*R15=REGSAPTR;   /* SAVE THE CALLER'S REGISTER SAVEAREA POINTER */      00043000
         LR    R15,REGSAPTR                                     01S0051 00044000
*REGSAPTR=REGSAPTR-LENGTH(CE5REGSV); /* DECREMENT REG 13 BY 72 */       00045000
         SL    REGSAPTR,@CF00851                                01S0052 00046000
*CE5SVBKP=R15;   /* POINT THIS ROUTINE'S SAVEAREA TO THE CALLER'S */    00047000
         ST    R15,CE5SVBKP(,REGSAPTR)                          01S0053 00048000
*CE5SVFWP=ZERO;  /* ZERO THIS ROUTINE'S SAVEAREA FORWARD POINTER */     00049000
         SLR   @14,@14                                          01S0054 00050000
         ST    @14,CE5SVFWP(,REGSAPTR)                          01S0054 00051000
*R15->CE5SVFWP=REGSAPTR; /* POINT CALLER'S SAVEAREA TO THIS SAVEAREA */ 00052000
         ST    REGSAPTR,CE5SVFWP(,R15)                          01S0055 00053000
*RFY R15 UNRSTD;                                                01S0056 00054000
*LINKPTR=NCSPLRPT;                  /* GET THE LINK RDTE ADDRESS     */ 00055000
         L     LINKPTR,NCSPLRPT(,NCSPLPTR)                      01S0057 00056000
*RLNOHIN=OFF;                       /* TURN OFF 'OFFHOOK IN' FLAG IN    00057000
*                                      THE LINK'S RDTE          01S0058 00058000
*                                      (MOVED HERE FOR VS4)  @VS41446*/ 00059000
         NI    RLNOHIN(LINKPTR),B'01111111'                     01S0058 00060000
*IF RLNTRAP=ON                      /* 'TRAP' SET IN THE LINK?       */ 00061000
* THEN                                                          01S0059 00062000
         TM    RLNTRAP(LINKPTR),B'10000000'                     01S0059 00063000
         BNO   @RF00059                                         01S0059 00064000
*  DO;                              /* YES - 'TRAP' SET IN LINK -    */ 00065000
*   CALL ISTINCE6;                  /* INVOKE HANG UP PROCESSOR      */ 00066000
*                                                               01S0061 00067000
         L     @15,@CV00744                                     01S0061 00068000
         BALR  @14,@15                                          01S0061 00069000
*    DO;                                                        01S0062 00070000
*/***  CRRMAINT  PROCNM(ISTINCE5)COMP(VY)FUNC(RET)RPHADD((NCSPLRPH))  * 00071000
**/                                                             01S0063 00072000
*    /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/              01S0063 00073000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                       01S0063 00074000
         L     @15,NCSPLRPH(,NCSPLPTR)                          01S0063 00075000
         L     @15,RPHCRR(,@15)                                 01S0063 00076000
         L     @15,CRACRR(,@15)                                 01S0063 00077000
         XC    VYCCRT+1(11,@15),VYCCRT+1(@15)                   01S0063 00078000
         MVI   VYCCRT(@15),B'00000000'                          01S0063 00079000
*NCSPLRPH->RPHCRR->CRACRR->CINCE5 = '1'B;                       01S0064 00080000
         OI    CINCE5(@15),B'00010000'                          01S0064 00081000
*    END;                                                       01S0065 00082000
*   NCSIORC=NCSIOERR;               /* SET 'NO GO' RESPONSE          */ 00083000
         MVI   NCSIORC(NCSPLPTR),X'04'                          01S0066 00084000
*  END;                                                         01S0067 00085000
* ELSE                                                          01S0068 00086000
*  DO;                              /* 'TRAP' NOT SET IN LINK -      */ 00087000
         B     @RC00059                                         01S0068 00088000
@RF00059 DS    0H                                               01S0069 00089000
*   CALL ISTINCD6;                  /* INVOKE RESOURCE ALLOCATE      */ 00090000
         L     @15,@CV00743                                     01S0069 00091000
         BALR  @14,@15                                          01S0069 00092000
*   RFY R15 RSTD;                                               01S0070 00093000
*                                                               01S0070 00094000
*    DO;                                                        01S0071 00095000
*/***  CRRMAINT  PROCNM(ISTINCE5)COMP(VY)FUNC(RET)RPHADD((NCSPLRPH))  * 00096000
**/                                                             01S0072 00097000
*    /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/              01S0072 00098000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                       01S0072 00099000
         L     @14,NCSPLRPH(,NCSPLPTR)                          01S0072 00100000
         L     @14,RPHCRR(,@14)                                 01S0072 00101000
         L     @14,CRACRR(,@14)                                 01S0072 00102000
         XC    VYCCRT+1(11,@14),VYCCRT+1(@14)                   01S0072 00103000
         MVI   VYCCRT(@14),B'00000000'                          01S0072 00104000
*NCSPLRPH->RPHCRR->CRACRR->CINCE5 = '1'B;                       01S0073 00105000
         OI    CINCE5(@14),B'00010000'                          01S0073 00106000
*    END;                                                       01S0074 00107000
*   IF R15=ZERO                     /* RESOURCE ALLOCATED?           */ 00108000
*    THEN                                                       01S0075 00109000
         LTR   R15,R15                                          01S0075 00110000
         BNZ   @RF00075                                         01S0075 00111000
*     NCSIORC=NCSIOSUC;             /* RESOURCE ALLOCATED - SET 'GO'    00112000
*                                      RESPONSE                      */ 00113000
         MVI   NCSIORC(NCSPLPTR),X'00'                          01S0076 00114000
*    ELSE                                                       01S0077 00115000
*     DO;                           /* RESOURCES NOT ALLOCATED -     */ 00116000
         B     @RC00075                                         01S0077 00117000
@RF00075 DS    0H                                               01S0078 00118000
*      RFY R15 UNRSTD;                                          01S0078 00119000
*      CALL ISTINCE6;               /* INVOKE DIAL HANG UP PROCESSOR */ 00120000
*                                                               01S0079 00121000
         L     @15,@CV00744                                     01S0079 00122000
         BALR  @14,@15                                          01S0079 00123000
*       DO;                                                     01S0080 00124000
*/***  CRRMAINT  PROCNM(ISTINCE5)COMP(VY)FUNC(RET)RPHADD((NCSPLRPH))  * 00125000
**/                                                             01S0081 00126000
*       /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/           01S0081 00127000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                       01S0081 00128000
         L     @15,NCSPLRPH(,NCSPLPTR)                          01S0081 00129000
         L     @15,RPHCRR(,@15)                                 01S0081 00130000
         L     @15,CRACRR(,@15)                                 01S0081 00131000
