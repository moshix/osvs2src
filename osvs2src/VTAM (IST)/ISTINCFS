         TITLE 'ISTINCFS - FREE RDT SEGMENT ROUTINE                    *00001000
                        '                                               00002000
ISTINCFS CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTINCFS  78.111'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @08,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@08                                      01S0001 00013000
*                                                               01S0005 00014000
*/* END OF ATBUILD                                                   */ 00015000
