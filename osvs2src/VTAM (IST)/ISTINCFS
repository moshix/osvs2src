*     /*                                                             */ 00049000
*     /* ** SET ENTERED BIT FOR THIS PROCEDURE                       */ 00050000
*     /*                                                             */ 00051000
*     /***************************************************************/ 00052000
*                                                               01S0018 00053000
*     R01->NCSPLRPH->RPHCRR->CRACRR->NINCFS='1'B;               01S0018 00054000
*                                                               01S0018 00055000
         L     @14,NCSPLRPH(,R01)                               01S0018 00056000
         L     @14,RPHCRR(,@14)                                 01S0018 00057000
         L     @14,CRACRR(,@14)                                 01S0018 00058000
         OI    NINCFS(@14),B'00100000'                          01S0018 00059000
*     /***************************************************************/ 00060000
*     /*                                                             */ 00061000
*     /* ** CLEAR AND SET CURRENT PROCEDURE BIT                      */ 00062000
*     /*                                                             */ 00063000
*     /***************************************************************/ 00064000
*                                                               01S0019 00065000
*     R01->NCSPLRPH->RPHCRR->CRACRR->VYCCRT='0'B;               01S0019 00066000
         XC    VYCCRT+1(11,@14),VYCCRT+1(@14)                   01S0019 00067000
         MVI   VYCCRT(@14),B'00000000'                          01S0019 00068000
*     R01->NCSPLRPH->RPHCRR->CRACRR->CINCFS='1'B;               01S0020 00069000
         OI    CINCFS(@14),B'00100000'                          01S0020 00070000
*   END;                                                        01S0021 00071000
*   DCL                                                         01S0022 00072000
*     R01 PTR(31) REG(01) RSTD;     /* INPUT PARAMETER REG           */ 00073000
*   DCL                                                         01S0023 00074000
*     R13 PTR(31) REG(13) RSTD;     /* SAVE AREA PTR REGISTER        */ 00075000
*   DCL                                                         01S0024 00076000
*     R15 PTR(31) REG(15) RSTD;     /* WORK REGISTER                 */ 00077000
*   DCL                                                         01S0025 00078000
*     R10 PTR(31) REG(10) RSTD;     /* BASE REG FOR NCSPL            */ 00079000
*   DCL                                                         01S0026 00080000
*     R11 PTR(31) REG(11) RSTD;     /* DATA AREA PTR REGISTER        */ 00081000
*   DCL                                                         01S0027 00082000
*     @SIZDATD FIXED(31) GEND;      /* size of DATA area             */ 00083000
*   R10=R01;                        /* INIT NCSPL BASE REGISTER      */ 00084000
         LR    R10,R01                                          01S0028 00085000
*   RFY                                                         01S0029 00086000
*     ISTNCSPL BASED(R10);          /* ADDRESSABILITY FOR NCSPL      */ 00087000
*   RFY                                                         01S0030 00088000
*     ISTRPH BASED(NCSPLRPH);       /* ADDRESSABILITY FOR RPH        */ 00089000
*   R15=R13;                        /* SAVE CALLER SAVE AREA ADR     */ 00090000
         LR    R15,R13                                          01S0031 00091000
*   R13=R13-LENGTH(ISTRSA);         /* SET SAVE AREA PTR REG         */ 00092000
         SH    R13,@CH01860                                     01S0032 00093000
*   R15->RSAFPT=R13;                /* SET CALLERS FORWARD PTR       */ 00094000
         ST    R13,RSAFPT(,R15)                                 01S0033 00095000
*   R13->RSABPT=R15;                /* SET MY BACK PTR               */ 00096000
         ST    R15,RSABPT(,R13)                                 01S0034 00097000
*   R13->RSAFPT=0;                  /* SET MY FORWARD PTR            */ 00098000
         SLR   @14,@14                                          01S0035 00099000
         ST    @14,RSAFPT(,R13)                                 01S0035 00100000
*   R11=VWANAV+7&'FFFFFFF8'X;       /* DATAREG = DWORD BDY           */ 00101000
         L     @14,NCSPLWKA(,R10)                               01S0036 00102000
         LA    R11,7                                            01S0036 00103000
         AL    R11,VWANAV(,@14)                                 01S0036 00104000
         N     R11,@CF00072                                     01S0036 00105000
*   VWANAV=R11+@SIZDATD;            /* ALLOCATE DATA AREA            */ 00106000
         LR    @12,R11                                          01S0037 00107000
         AL    @12,@SIZDATD                                     01S0037 00108000
         ST    @12,VWANAV(,@14)                                 01S0037 00109000
*   GEN(USING @DATD,R11);           /* ESTAB BASE REG FOR DATA       */ 00110000
         USING @DATD,R11                                                00111000
