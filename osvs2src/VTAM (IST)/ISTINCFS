*      2 CVTMDL CHAR(2) BDY(WORD,3),/* CPU MODEL NUMBER IN SIGNLESS     01423000
*                                      PACKED DECIMAL, I.E., A MODEL    01424000
*                                      145 WOULD BE REPRESENTED AS      01425000
*                                      0145 HEX                      */ 01426000
*      2 CVTRELNO CHAR(4) BDY(WORD),/* RELEASE NUMBER (EBCDIC)       */ 01427000
*       3 CVTNUMB CHAR(2) BDY(WORD),/* RELEASE NUMBER                */ 01428000
*       3 CVTLEVL CHAR(2) BDY(WORD,3);/* LEVEL NUMBER OF THIS RELEASE*/ 01429000
*/*                    END OF CVT MACRO                              */ 01430000
*                                                               01S0044 01431000
*   DCL                                                         01S0044 01432000
*     1 IFGRPL BASED,               /* LEVEL ONE DECLARE             */ 01433000
*      2 RPLCOMN,                   /* RPL COMMON SECTION X03004     */ 01434000
*       3 RPLIDWD CHAR(4),          /* ID WORD OF RPL                */ 01435000
*        4 RPLID BIT(8),            /* IDENT & HEADER TYPE           */ 01436000
*        4 RPLSTYP PTR(8),          /* SUBTYPE                       */ 01437000
*        4 RPLREQ PTR(8),           /* REQUEST TYPE                  */ 01438000
*        4 RPLLEN PTR(8),           /* LENGTH OF THIS BLOCK          */ 01439000
*         5 RPLLEN2 PTR(8),         /* LENGTH OF THIS BLOCK X03004   */ 01440000
*       3 RPLPLHPT PTR(31),         /* PTR TO PLACEHOLDER            */ 01441000
*       3 RPLECB PTR(31),           /* EXTERNAL ECB PTR OR INTERNAL     01442000
*                                      ECB                           */ 01443000
*         5 RPLWAIT BIT(1),         /* EVENT WAITED ON               */ 01444000
*         5 RPLPOST BIT(1),         /* EVENT COMPLETE                */ 01445000
*       3 RPLFDBWD PTR(31),         /* FEEDBACK WORD@X04SVHS         */ 01446000
*        4 RPLSTAT BIT(8),          /* CURRENT RPL STATUS            */ 01447000
