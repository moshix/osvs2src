@RF01591 DS    0H                                                  1598 02793000
*               IF NCSPLVCD^=NCSPLVC3 THEN                         1598 02794000
         CLI   NCSPLVCD(NCSPLPTR),X'03'                            1598 02795000
         BE    @RF01598                                            1598 02796000
*                 LCPINTER=OFF;                                    1599 02797000
         L     @04,CIOLCPTR                                        1599 02798000
         NI    LCPINTER(@04),B'10111111'                           1599 02799000
*               IF NCSPLCMD=NCSCNOTF THEN/* IF NOTIFY OP=NOTIFY      */ 02800000
@RF01598 CLC   NCSPLCMD(2,NCSPLPTR),@CB03218                       1600 02801000
         BNE   @RF01600                                            1600 02802000
*                 IF NCSPLVCD^=NCSPLVC3 THEN                       1601 02803000
         CLI   NCSPLVCD(NCSPLPTR),X'03'                            1601 02804000
         BE    @RF01601                                            1601 02805000
