*   FMCBPTR=NCBFMCBA;               /* SET UP PTR TO FMCB            */ 00087000
         L     @03,NCBPTR                                          0693 00088000
         L     @03,NCBFMCBA(,@03)                                  0693 00089000
         ST    @03,FMCBPTR                                         0693 00090000
*/* UPDATE NEXT AVAILABLE PTR                                        */ 00091000
*                                                                  0694 00092000
*   NXTAVPTR=NXTAVPTR+@SIZDATD;                                    0694 00093000
*                                                                  0694 00094000
