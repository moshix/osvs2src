         BNZ   @RF01489                                            1489 02468000
*                       CIOFDBLP=ON;                               1490 02469000
         OI    CIOFDBLP,B'00010000'                                1490 02470000
*                     ELSE                                         1491 02471000
*                       CIOFDBPT=FDBFDBA;/* GET NEXT FDB @           */ 02472000
         B     @RC01489                                            1491 02473000
@RF01489 L     @04,CIOFDBPT                                        1491 02474000
         L     @04,FDBFDBA(,@04)                                   1491 02475000
         ST    @04,CIOFDBPT                                        1491 02476000
*                   END;            /* END FOR DO WHILE              */ 02477000
@RC01489 DS    0H                                                  1492 02478000
@DE01484 TM    CIOFDBLP,B'00010000'                                1492 02479000
         BZ    @DL01484                                            1492 02480000
*                   RFY                                            1493 02481000
*                    (CIOFDBAD,                                    1493 02482000
*                     CIOFDBCT,                                    1493 02483000
*                     CIOINBUF,                                    1493 02484000
*                     CIOINCNT) UNRSTD;                            1493 02485000
*                 END;              /* END FOR SAVE RESPONSE IND     */ 02486000
*             END;                  /* END- CMD NOT POS OR NEG RESP  */ 02487000
*         END;                      /* END FOR MAJ RC=0              */ 02488000
*       ELSE                                                       1497 02489000
*         NCSIORC=NCSIOERR;         /* ELSE SET UP UNSUCCESS R.C.    */ 02490000
         B     @RC01464                                            1497 02491000
@RF01464 MVI   NCSIORC(NCSPLPTR),X'04'                             1497 02492000
*     END;                          /* END - MAJ R.C. NOT FRPURGE    */ 02493000
@RC01464 DS    0H                                                  1499 02494000
*   IF NCSPLCMD=NCSSNFMD            /* IF CMD IS SEND FMD            */ 02495000
*       &CIORESW=OFF THEN                                          1499 02496000
@RC01461 CLC   NCSPLCMD(2,NCSPLPTR),@CB03372                       1499 02497000
         BNE   @RF01499                                            1499 02498000
         TM    CIORESW,B'10000000'                                 1499 02499000
         BNZ   @RF01499                                            1499 02500000
*     DO;                                                          1500 02501000
*       TIETYPE=TIETYPE;            /* SET UP TIE FOR TOLTEP         */ 02502000
         LA    @04,CIOTIE                                          1501 02503000
         MVC   @TS00001(1),TIETYPE(@04)                            1501 02504000
         MVC   TIETYPE(1,@04),@TS00001                             1501 02505000
*       TIELNGTH=LENGTH(ISTTIE);                                   1502 02506000
         MVI   TIELNGTH(@04),X'10'                                 1502 02507000
*       TIEREQ=TIEREQ1;             /* IND. THIS IS A FSB TO TOLTEP  */ 02508000
         MVI   TIEREQ(@04),X'01'                                   1503 02509000
*       TIEPTR1=ISTVRFSB;           /* GIVE TOLTEP FSB PTR           */ 02510000
         L     @03,CIOLCPTR                                        1504 02511000
         L     @03,ISTVRFSB+72(,@03)                               1504 02512000
         ST    @03,TIEPTR1(,@04)                                   1504 02513000
*       TIEPTR2=NCSPLPTR;           /* PASS PTR TO NCSPL             */ 02514000
         ST    NCSPLPTR,TIEPTR2(,@04)                              1505 02515000
*       R1=NCSPLRPH;                /* SET UP RPH FOR TPQUE          */ 02516000
*                                                                  1506 02517000
         L     R1,NCSPLRPH(,NCSPLPTR)                              1506 02518000
*/*  TPQUE (ADDR(CIOTIE),ADDR(ATCTCIPB)->DYPPAB) NORM ENA */       1507 02519000
*                                                                  1507 02520000
*        DO;                                                       1507 02521000
*        RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;            1508 02522000
*        GEN REFS(GPR01P) SETS(RPHWORK)                            1509 02523000
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      1509 02524000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              02525000
*        GPR00P=0;                                                 1510 02526000
         SLR   GPR00P,GPR00P                                       1510 02527000
*        GPR15P=ADDR(ADDR(ATCTCIPB)->DYPPAB); /*HOLD PAB PTR*/     1511 02528000
         L     GPR15P,NCSPLCVT(,NCSPLPTR)                          1511 02529000
         LA    GPR15P,DYPPAB+624(,GPR15P)                          1511 02530000
*        GPR04P=ADDR(CIOTIE); /*WORK ELE PTR*/                     1512 02531000
         LA    GPR04P,CIOTIE                                       1512 02532000
*        GPR05P=GPR15P; /*PASS PAB PTR*/                           1513 02533000
         LR    GPR05P,GPR15P                                       1513 02534000
*        CALL ATCLCPTR->ATCAP37->ISTAPC37;                         1514 02535000
         L     @03,ATCLCPTR                                        1514 02536000
         L     @15,ATCAP37(,@03)                                   1514 02537000
         BALR  @14,@15                                             1514 02538000
*        GEN(NOPR  0)NOSETS NOREFS;                                1515 02539000
         NOPR  0                                                        02540000
*        RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;          1516 02541000
*        END; /**** END OF TPQUE GENERATION ****/                  1517 02542000
*                                                                  1517 02543000
*/* TPEXIT  NORM */                                                1518 02544000
*        DO;                                                       1518 02545000
*          RESPECIFY GPR15P RSTD;                                  1519 02546000
*                                                                  1519 02547000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)  */                         1520 02548000
*/* END OF ATBUILD */                                              1520 02549000
*                                                                  1520 02550000
*        DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */             1520 02551000
*          GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/             1521 02552000
         L     GPR15P,ATCLCPTR                                     1521 02553000
*        END; /*LISATCVT*/                                         1522 02554000
*          RESPECIFY GPR00F RSTD;                                  1523 02555000
*          GPR00F=4;    /* SET PARAMETER REGISTER TO 4 */          1524 02556000
         LA    GPR00F,4                                            1524 02557000
*          GPR15P=GPR15P->ATCAP31;                                 1525 02558000
         L     GPR15P,ATCAP31(,GPR15P)                             1525 02559000
*          DCL FMZL0160 BASED(GPR15P) LABEL VALUERANGE(*);         1526 02560000
*          CALL FMZL0160;                                          1527 02561000
         BALR  @14,@15                                             1527 02562000
*          RESPECIFY (GPR00F,GPR15P) UNRSTD;                       1528 02563000
*        END; /***** END OF TPEXIT  *****/                         1529 02564000
*     END;                          /* END FOR TOLTEP TPEXIT         */ 02565000
*   ELSE                            /* ELSE RELSTORE FSB             */ 02566000
*     IF ISTVRFSB^=0                /* RELSTORE IF FSB EXISTS      1531 02567000
*                                                            @XM7548K*/ 02568000
*       THEN                                                       1531 02569000
         B     @RC01499                                            1531 02570000
@RF01499 SLR   @04,@04                                             1531 02571000
         L     @03,CIOLCPTR                                        1531 02572000
         L     @03,ISTVRFSB+72(,@03)                               1531 02573000
         CR    @03,@04                                             1531 02574000
         BE    @RF01531                                            1531 02575000
*       DO;                         /*                       @XM7548K*/ 02576000
*         R1=NCSPLRPH;                                             1533 02577000
         L     R1,NCSPLRPH(,NCSPLPTR)                              1533 02578000
*         RFY                                                      1534 02579000
*           ISTFDB BASED(ADDR(FSBFDB));                            1534 02580000
*         IF FDBOFSET=0             /*                       @ZA18448*/ 02581000
*           THEN                    /*                       @ZA18448*/ 02582000
         LA    @09,32                                              1535 02583000
         LR    @08,@03                                             1535 02584000
         ALR   @08,@09                                             1535 02585000
         CH    @04,FDBOFSET(,@08)                                  1535 02586000
         BNE   @RF01535                                            1535 02587000
*           CIOBUFAD=ADDR(FSBFDB)-TWENTY;/*                  @ZA18448*/ 02588000
         LA    @04,FSBFDB(,@03)                                    1536 02589000
         SLR   @04,@09                                             1536 02590000
         ST    @04,CIOBUFAD                                        1536 02591000
*         ELSE                      /*                       @ZA18448*/ 02592000
*           CIOBUFAD=ADDR(FSBFDB)-FDBOFSET;/* BACK UP TO CORRECT ADD */ 02593000
         B     @RC01535                                            1537 02594000
@RF01535 L     @04,CIOLCPTR                                        1537 02595000
         L     @04,ISTVRFSB+72(,@04)                               1537 02596000
         LA    @03,FSBFDB(,@04)                                    1537 02597000
         SH    @03,FDBOFSET+32(,@04)                               1537 02598000
         ST    @03,CIOBUFAD                                        1537 02599000
*         CIOBUFAD->CIORLBUF=FDBFDBA;/* PUT FDB CHAIN PTR HERE       */ 02600000
@RC01535 L     @04,CIOLCPTR                                        1538 02601000
         L     @04,ISTVRFSB+72(,@04)                               1538 02602000
         L     @04,FDBFDBA+32(,@04)                                1538 02603000
         L     @03,CIOBUFAD                                        1538 02604000
         ST    @04,CIORLBUF(,@03)                                  1538 02605000
*          /*RELSTORE BUFADDR(CIOBUFAD)*/                          1539 02606000
*           DO;                                                    1539 02607000
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  1540 02608000
*/* END OF ATBUILD */                                              1540 02609000
*          RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;                 1540 02610000
*         RFY (GPR01P,GPR13P) RSTD;                                1541 02611000
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       1542 02612000
*                                                                  1542 02613000
          STM GPR02P,GPR13P,RPHWORK(GPR01P)                             02614000
*         RFY GPR13P UNRSTD;                                       1543 02615000
*         GPR04P=CIOBUFAD;                                         1544 02616000
         L     GPR04P,CIOBUFAD                                     1544 02617000
*         DCL 1 FMZA0168 FIXED(31) STATIC LOCAL,                   1545 02618000
*             2 * FIXED(8) INIT(64),                               1545 02619000
*             2 * PTR(24) INIT(0);                                 1545 02620000
*         GPR03F = FMZA0168;                                       1546 02621000
*                                                                  1546 02622000
         L     GPR03F,FMZA0168                                     1546 02623000
*          DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */           1547 02624000
*            GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/           1548 02625000
         L     GPR15P,ATCLCPTR                                     1548 02626000
*          END; /*LISATCVT*/                                       1549 02627000
*         GPR02P = GPR15P->ATCBPDA;                                1550 02628000
         L     GPR02P,ATCBPDA(,GPR15P)                             1550 02629000
*         GPR15P=GPR15P->ATCSMRS;                                  1551 02630000
         L     GPR15P,ATCSMRS(,GPR15P)                             1551 02631000
*         DCL FMZL0168 BASED(GPR15P) LABEL VALUERANGE(*);          1552 02632000
*         CALL FMZL0168;                                           1553 02633000
         BALR  @14,@15                                             1553 02634000
*         RFY GPR13P RSTD;                                         1554 02635000
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       1555 02636000
*                                                                  1555 02637000
          LM GPR02P,GPR13P,RPHWORK(GPR01P)                              02638000
*         RFY (GPR01P,GPR13P) UNRSTD;                              1556 02639000
*          RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;               1557 02640000
*          END ;/**********END OF RELSTORE EXPANSION**********/    1558 02641000
*       END;                        /* END FOR NOT TOLTEP EXIT       */ 02642000
*   R14=FSBSAVE;                    /* RESTORE REG 14                */ 02643000
@RF01531 DS    0H                                                  1560 02644000
@RC01499 L     R14,FSBSAVE                                         1560 02645000
*   END FSBCHECK;                                                  1561 02646000
@EL00002 DS    0H                                                  1561 02647000
@EF00002 DS    0H                                                  1561 02648000
@ER00002 BR    @14                                                 1561 02649000
*CALLPURG:                                                         1562 02650000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1562 02651000
*                                                                  1562 02652000
CALLPURG DS    0H                                                  1563 02653000
*/********************************************************************  02654000
*                                                                    */ 02655000
*/*   THE CALLPURG SUBROUTINE FORMATS A PURGE LCPB & SCHEDULES IT.   */ 02656000
*/* THERE ARE 2 CASES:                                               */ 02657000
*/*   1. ENTRY IS FOR AN FMCB WITH MODE OF RECORD, OR VARY'S SESSION */ 02658000
*/* WITH AN NEW CLUSTER.                                             */ 02659000
*/*   2. ALL OTHERS.                                                 */ 02660000
*/*   IN CASE # 1 THE LCPB IS FORMATTED AS AN EXTENDED LCPB & TPQUEED*/ 02661000
*/* TO THE ASYNCHRONOUS TPIOS OUTBOUND PAB IN THE FMCB.              */ 02662000
*/*   IN CASE # 2 THE TPPURGE MACRO IS ISSUED WHICH FORMATS A 'NORMAL'  02663000
*                                                                    */ 02664000
*/* LCPB & TPQUEUES IT TO THE CLOP IN THE FMCB.                      */ 02665000
*/*   IF THE ERP REQUESTED PURGE, NO CID FINISH IS DONE TO WAIT FOR  */ 02666000
*/* I/O COMPLETION. IF THE ERP REQUESTED THE PURGE, CALLPURG GOES TO */ 02667000
*/* RELSTORE FSB LOGIC & RETURNS.                                    */ 02668000
*/*   IF IT WASN'T THE ERP WHICH REQUESTED PURGE, A CID FIND IS DONE */ 02669000
*/* FOR THE DAF LOCK & IF NOT SUCCESSFUL, CALLPURG GOES TO RELSTORE  */ 02670000
*/* FSB LOGIC & RETURNS. IF SUCCESSFUL, A SEARCH IS MADE FOR THE FMCB*/ 02671000
*/* JUST PURGED. IF FOUND & THIS IS VARY'S SEESION WITH A NEW CLUSTER*/ 02672000
*/* THE CONNEXT DEAD FLAG IS TURNED OFF. OTHERWISE, IT IS LEFT ON.   */ 02673000
*/* THE DAF LOCK HELD SWITCH IS TURNED ON AND THE MODULE ENTERS REL- */ 02674000
*/* STORE FSB LOGIC & RETURNS.                                       */ 02675000
*/********************************************************************  02676000
*                                                                    */ 02677000
*                                                                  1563 02678000
*   RFY                                                            1563 02679000
*     ISTRPRE BASED(NCBRDTE);       /*                       @ZA25517*/ 02680000
*   CALLPGSV=R14;                   /* SAVE REG 14                   */ 02681000
         ST    R14,CALLPGSV                                        1564 02682000
*   CIOLCPB=CIOLCPB&&CIOLCPB;       /* CLEAR LCPB                    */ 02683000
         L     @04,CIOLCPTR                                        1565 02684000
         XC    CIOLCPB(80,@04),CIOLCPB(@04)                        1565 02685000
*   IF FMCMODE(1:6)^='RECORD'&      /* IF NOT RECORD                 */ 02686000
*       FMCMODE(1:8)^='ISTYTCM3'&   /* OR TCAM                       */ 02687000
*       NCSPLCMD=NCSCNOTF           /* AND COMMAND IS NOTIFY @XM9709K*/ 02688000
*     THEN                                                         1566 02689000
         L     @04,FMCBPTR                                         1566 02690000
         CLC   FMCMODE(6,@04),@CC04787                             1566 02691000
         BE    @RF01566                                            1566 02692000
         CLC   FMCMODE(8,@04),@CC04788                             1566 02693000
         BE    @RF01566                                            1566 02694000
         CLC   NCSPLCMD(2,NCSPLPTR),@CB03218                       1566 02695000
         BE    @RT01566                                            1566 02696000
*     ;                             /* DO NOTHING            @DM95990*/ 02697000
*   ELSE                                                           1568 02698000
*     DO;                           /* ELSE NOT NOTIFY       @DM95990*/ 02699000
@RF01566 DS    0H                                                  1569 02700000
*       IF FMCMODE(1:6)='RECORD'|   /* MODE EQ RECORD        @D31JKMD*/ 02701000
*           FMCMODE(1:8)='ISTYTCM3'|/* OR TCAM, OR           @D31JKMD*/ 02702000
*           FMCMODE(1:8)='ISTYSSCP'|/* OR SSCP, OR           @ZA18924*/ 02703000
*           FMCVSNA=ON THEN         /* VARY'S SESSION IN SNA?      1569 02704000
*                                                            @D31JKMD*/ 02705000
         L     @04,FMCBPTR                                         1569 02706000
         CLC   FMCMODE(6,@04),@CC04787                             1569 02707000
         BE    @RT01569                                            1569 02708000
         CLC   FMCMODE(8,@04),@CC04788                             1569 02709000
         BE    @RT01569                                            1569 02710000
         CLC   FMCMODE(8,@04),@CC04808                             1569 02711000
         BE    @RT01569                                            1569 02712000
         TM    FMCVSNA(@04),B'01000000'                            1569 02713000
         BNO   @RF01569                                            1569 02714000
@RT01569 DS    0H                                                  1570 02715000
*         DO;                                                      1570 02716000
*           LCPTYPE=LCPETYPE;       /* FLAG AS SYSTEM CONTROL        */ 02717000
         L     @04,CIOLCPTR                                        1571 02718000
         MVI   LCPTYPE(@04),X'40'                                  1571 02719000
*           LCPLNGTH=LENGTH(CIOLCPB);                              1572 02720000
         MVI   LCPLNGTH(@04),X'50'                                 1572 02721000
*           IF NCSPRMRC=LCPMNSF&    /* IF SOFT ERROR                 */ 02722000
*               (FMCMODE(1:6)='RECORD'|FMCMODE(1:8)='ISTYTCM3') THEN    02723000
         CLI   NCSPRMRC(NCSPLPTR),X'01'                            1573 02724000
         BNE   @RF01573                                            1573 02725000
         L     @04,FMCBPTR                                         1573 02726000
         CLC   FMCMODE(6,@04),@CC04787                             1573 02727000
         BE    @RT01573                                            1573 02728000
         CLC   FMCMODE(8,@04),@CC04788                             1573 02729000
         BNE   @RF01573                                            1573 02730000
@RT01573 DS    0H                                                  1574 02731000
*             LCPMNRCD=LCPBFIRM;    /* & MODE=RECORD OR TCAM @D31JKMD*/ 02732000
         L     @04,CIOLCPTR                                        1574 02733000
         MVI   LCPMNRCD(@04),X'10'                                 1574 02734000
*           ELSE                                                   1575 02735000
*             LCPMNRCD=NCSPRMRC;    /* GET MINOR RC FROM NCSPL       */ 02736000
         B     @RC01573                                            1575 02737000
@RF01573 L     @04,CIOLCPTR                                        1575 02738000
         MVC   LCPMNRCD(1,@04),NCSPRMRC(NCSPLPTR)                  1575 02739000
*           LCPNXLCW=ADDR(LCPLCCW)+LENGTH(ISTLCCW);/* SET NEXT LCCW @   02740000
*                                      TO 1ST SSCP DOESN'T USE 2ND   */ 02741000
@RC01573 L     @04,CIOLCPTR                                        1576 02742000
         LA    @03,LCPLCCW(,@04)                                   1576 02743000
         AL    @03,@CF00060                                        1576 02744000
         ST    @03,LCPNXLCW(,@04)                                  1576 02745000
*           LCPRPHA=NCSPLRPH;       /* SET RPH @ FROM NCSPL          */ 02746000
         L     @00,NCSPLRPH(,NCSPLPTR)                             1577 02747000
         ST    @00,LCPRPHA(,@04)                                   1577 02748000
*           LCPFTYPE=ON;            /* FLAG THIS AS AN RPH @ TO BE 1578 02749000
*                                      TPPOSTED                      */ 02750000
         OI    LCPFTYPE(@04),B'10000000'                           1578 02751000
*           LCPINTER=ON;            /* INDICATE INTERNAL OPERATION   */ 02752000
         OI    LCPINTER(@04),B'01000000'                           1579 02753000
*           LCPSAF=FMCSAF;          /* GET SAF FROM FMCB             */ 02754000
         L     @09,FMCBPTR                                         1580 02755000
         MVC   LCPSAF(2,@04),FMCSAF(@09)                           1580 02756000
*           RFY                                                    1581 02757000
*             ISTLCCW BASED(LCPNXLCW);                             1581 02758000
*           LCPDAF=NCSPLDAF;        /* GET DAF FROM NCSPL            */ 02759000
         MVC   LCPDAF(2,@04),NCSPLDAF(NCSPLPTR)                    1582 02760000
*           RFY                                                    1583 02761000
*             ISTRH BASED(ADDR(LCCWDATA));                         1583 02762000
*           RHTYPE=ON;              /* INDICATE CONTROL COMMAND      */ 02763000
*           RHSCI=ON;               /* INDICATE SYSTEM CONTROL       */ 02764000
*           RHFORMAT=ON;            /* INDICATE FORMATTED            */ 02765000
         LA    @08,4                                               1586 02766000
         LR    @07,@03                                             1586 02767000
         ALR   @07,@08                                             1586 02768000
         OI    RHTYPE(@07),B'01101000'                             1586 02769000
*           RHFME=ON;               /* FUNCTION MGMT. END            */ 02770000
         OI    RHFME(@07),B'10000000'                              1587 02771000
*           RHCHAIN=RHONLY;         /* INDICATE NO CHAINING          */ 02772000
         OI    RHCHAIN(@07),B'00000011'                            1588 02773000
*           LCCWIDAT=ON;            /* TURN ON IMMEDIATE DATA FLAG   */ 02774000
         OI    LCCWIDAT(@03),B'00001000'                           1589 02775000
*           LCPASYN=ON;             /* INDICATE ASYNCHRONOUS REQUEST */ 02776000
         OI    LCPASYN(@04),B'01000000'                            1590 02777000
*           IF FMCSAF=ATCVTMID THEN /* IF VARY'S FMCB                */ 02778000
         L     @04,NCSPLCVT(,NCSPLPTR)                             1591 02779000
         CLC   FMCSAF(2,@09),ATCVTMID(@04)                         1591 02780000
         BNE   @RF01591                                            1591 02781000
*             DO;                                                  1592 02782000
*               LCCWOP=LCLEAR;      /* FLAG AS A 'CLEAR' LCCW        */ 02783000
         MVI   LCCWOP(@03),X'F2'                                   1593 02784000
*               LCCWCNT=CIOPGCNT;   /* SET COUNT TO 4                */ 02785000
         STH   @08,LCCWCNT(,@03)                                   1594 02786000
*               CIOLCWOP='A1'X;                                    1595 02787000
