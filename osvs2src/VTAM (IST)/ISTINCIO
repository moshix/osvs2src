@RC00949 DS    0H                                                  0956 00946000
@RC00944 L     @04,CIOLCPTR                                        0956 00947000
         MVI   LCPLNGTH(@04),X'50'                                 0956 00948000
*     LCPSAF=ATCVTMID; /* GET SAF FROM VTAM CVT FOR VARY'S FMCB */ 0957 00949000
         L     @03,NCSPLCVT(,NCSPLPTR)                             0957 00950000
