         B     @RC01601                                            1608 02820000
@RF01601 DS    0H                                                  1609 02821000
*                     LCCWOP=LCLRNFY;/* SET OP TO CLEAR & NOTIFY     */ 02822000
         L     @04,CIOLCPTR                                        1609 02823000
         L     @04,LCPNXLCW(,@04)                                  1609 02824000
         MVI   LCCWOP(@04),X'F8'                                   1609 02825000
*                     CIOFRFSB=ON;  /* INDICATE FSB TO BE FREED    1610 02826000
*                                                            @XM7541K*/ 02827000
         OI    CIOFRFSB,B'00001000'                                1610 02828000
*                     CIOLCWOP='A1'X;                              1611 02829000
