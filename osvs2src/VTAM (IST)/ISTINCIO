*        RFY (EMCR2,EMCR3) UNRSTD;                                 1448 02279000
*        END; /*CIDCTL*//* ISSUE CID FINISH              */        1449 02280000
*     END;                          /* SKIPCTL END CID CTL           */ 02281000
*   R1=NCSPLRPH;                    /* SET UP RPH            @XM19047*/ 02282000
@RT01438 L     @04,NCSPLRPH(,NCSPLPTR)                             1451 02283000
         LR    R1,@04                                              1451 02284000
*   NCSPLRPH->RPHWEA=CIOWEASV;      /* RESTORE RPHWEA        @DM12063*/ 02285000
         L     @03,CIOWEASV                                        1452 02286000
