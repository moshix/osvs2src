*IF NCSOVFMC=ON                   /* IF ONLY VARY SWITCH ON   */   0724 00222000
*  THEN;                          /* FALL THRU -FMCBPTR IS SET UP */    00223000
         TM    NCSOVFMC(NCSPLPTR),B'10000000'                      0724 00224000
         BO    @RT00724                                            0724 00225000
*  ELSE IF NCSOUFMC=ON            /* IS ONLY USER'S SWITCH ON */   0726 00226000
