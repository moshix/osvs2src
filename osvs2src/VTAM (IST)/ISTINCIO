*                     LCCWCNT=CIOCNT;/*                      @DL31HGH*/ 02811000
         MVC   LCCWCNT(2,@04),@CH00289                             1604 02812000
*                     LCCWCD=OFF;   /*                       @DL31HGH*/ 02813000
         NI    LCCWCD(@04),B'01111111'                             1605 02814000
*                     CIONORES=ON;  /*                       @DL31HGH*/ 02815000
         OI    CIONORES,B'01000000'                                1606 02816000
