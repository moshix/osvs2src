         MVI   NCSIORC(NCSPLPTR),X'00'                             0907 00752000
*         IF FMCDEVOF=ON&NCBFORCE=OFF/* IF BOX DEAD AND NOT  @XA10628*/ 00753000
*           THEN                                                   0908 00754000
         L     @04,FMCBPTR                                         0908 00755000
         TM    FMCDEVOF(@04),B'00010000'                           0908 00756000
         BNO   @RF00908                                            0908 00757000
         L     @04,NCBPTR                                          0908 00758000
         TM    NCBFORCE(@04),B'00000100'                           0908 00759000
         BNZ   @RF00908                                            0908 00760000
*           NCSIORC=NCSIOPRG;       /* FORCE DEACT,FAIL I/O  @XA10628*/ 00761000
         MVI   NCSIORC(NCSPLPTR),X'08'                             0909 00762000
*         ELSE                                                     0910 00763000
*           DO;                     /* CHECK DEVOF IN LOCAL RN     0910 00764000
*                                                            @XM9620K*/ 00765000
         B     @RC00908                                            0910 00766000
@RF00908 DS    0H                                                  0911 00767000
*             RDTEPTR=NCBRDTE;      /* GET RDTE FOR IO       @XM9620K*/ 00768000
         L     @04,NCBPTR                                          0911 00769000
         L     @04,NCBRDTE(,@04)                                   0911 00770000
         ST    @04,RDTEPTR                                         0911 00771000
*             DO WHILE RDTEPTR->RPRNSNEA^=0;/* LOOK FOR LOCAL RN   0912 00772000
*                                                            @XM9620K*/ 00773000
         B     @DE00912                                            0912 00774000
@DL00912 DS    0H                                                  0913 00775000
*               RDTEPTR=RDTEPTR->RPRNSNEA;/* UP NSNEA CHAIN  @XM9620K*/ 00776000
         L     @04,RDTEPTR                                         0913 00777000
         L     @04,RPRNSNEA(,@04)                                  0913 00778000
         ST    @04,RDTEPTR                                         0913 00779000
*             END;                  /*                       @XM9620K*/ 00780000
@DE00912 L     @04,RDTEPTR                                         0914 00781000
         L     @03,RPRNSNEA(,@04)                                  0914 00782000
         LTR   @03,@03                                             0914 00783000
         BNZ   @DL00912                                            0914 00784000
*             RFY                                                  0915 00785000
*               R2 RSTD;            /*                       @XM11585*/ 00786000
*             IF RDTEPTR->RPRENTRY=RPRENTRN/* IF ITS A370X   @XM9620K*/ 00787000
*               THEN                                               0916 00788000
         CLI   RPRENTRY(@04),X'01'                                 0916 00789000
         BNE   @RF00916                                            0916 00790000
*               DO;                 /*                       @XM11585*/ 00791000
*                 R2=RDTEPTR->RPRNCBA->NCBFMCBA;/* GET 1ST FMCB    0918 00792000
*                                                            @DM12996*/ 00793000
         L     @04,RPRNCBA(,@04)                                   0918 00794000
         L     R2,NCBFMCBA(,@04)                                   0918 00795000
*                 DO WHILE R2->FMCMODE(1:8)^='ISTYSSCP';/*   @XM11585*/ 00796000
         B     @DE00919                                            0919 00797000
@DL00919 DS    0H                                                  0920 00798000
*                   R2=R2->FMCFMCBA;/* GET NEXT FMCB         @XM11585*/ 00799000
         L     R2,FMCFMCBA(,R2)                                    0920 00800000
*                 END;              /*                       @XM11585*/ 00801000
@DE00919 CLC   FMCMODE(8,R2),@CC04808                              0921 00802000
         BNE   @DL00919                                            0921 00803000
*                 IF R2->FMCDEVOF='1'B/* IF DEV OFFLINE      @XM11585*/ 00804000
*                   THEN                                           0922 00805000
         TM    FMCDEVOF(R2),B'00010000'                            0922 00806000
         BNO   @RF00922                                            0922 00807000
*                   NCSIORC=NCSIOPRG;/* TURN IO AROUND       @XM11585*/ 00808000
         MVI   NCSIORC(NCSPLPTR),X'08'                             0923 00809000
*                 ELSE                                             0924 00810000
*                   ;               /*                       @XM11585*/ 00811000
@RF00922 DS    0H                                                  0925 00812000
*               END;                /*                       @XM11585*/ 00813000
*             ELSE                                                 0926 00814000
*               ;                   /*                       @XM11585*/ 00815000
@RF00916 DS    0H                                                  0927 00816000
*           END;                    /* NOTDEAD               #XM9620K*/ 00817000
*         IF NCSIORC^=NCSIOSUC THEN                                0928 00818000
@RC00908 CLI   NCSIORC(NCSPLPTR),X'00'                             0928 00819000
         BNE   @RT00928                                            0928 00820000
*           ;                       /* DONT DO IO            #XM9620K*/ 00821000
*         ELSE                                                     0930 00822000
*           DO;                     /*                       #XM9620K*/ 00823000
*DO; /* DO FOR IOFRMAT SEGMENT */                                  0931 00824000
*/* IOFRMAT SUBROUTINE */                                          0932 00825000
*/********************************************************************* 00826000
*/*   THE IOFRMAT SUBROUTINE FORMATS THE LCPB TO SEND THE I/O REQUEST * 00827000
*/* TO TPIOS.                                                         * 00828000
*/*   IF THE NCSPLCMD IS A SEND FMD(TOLTEP) & THIS IS A LOCAL DEVICE  * 00829000
*/* A 'BASIC' LCPB IS BUILT WITHOUT CHECKING FOR NCSPL FIELDS SUCH AS * 00830000
*/* SYNCHRONOUS WITH THE FLOW.                                        * 00831000
*/*   ALL OTHER CASES BUILD AN LCBP DEPENDING ON BITS SET UP IN THE   * 00832000
*/* NCSPL:                                                            * 00833000
*/*   IF THE NCSPLCMD IS A SEND POS. RESPONSE OR SEND NEG. RESPONSE   * 00834000
*/* (I.E. RESPONSES TO REQUESTS FROM THE NETWORK), A SEQUENCE # IS    * 00835000
*/* PUT IN THE LCPB & THE TYPE FIELD IS SET TO STANDARD LCPB TYPE.    * 00836000
*/*   OTHERWISE, A CHECK IS MADE FOR SYSTEM CONTROL & IF SYSTEM CON-  * 00837000
*/* TROL, THE LCPTYPE IS SET TO SESSION CONTROL, & THE PAB @ SET TO   * 00838000
*/* @ CLIP IN VARY'S FMCB AND THE LCPB POST FLAG TURNED OFF. THIS     * 00839000
*/* INDICATES THAT FEEDBACK WILL COME BACK THRU THE CLIP AND NOT      * 00840000
*/* DIRECTLY VIA TPPOST.                                              * 00841000
*/*   NEXT, STANDARD FIELDS IN THE LCPB ARE INITIALIZED. (I.E. SAF,   * 00842000
*/* DAF & LCPNXLCW). IF THIS IS RESTART I/O OR THE REQUEST IS ASYN-   * 00843000
*/* CHRONOUS, THE APPROPRIATE FLAGS(LCPRSTRT & LCPASYNC) ARE SET.     * 00844000
*/*   IF THE COMMAND IS QUIESCE OR RECORD DEVICE STATS, THE APPRO-    * 00845000
*/* PRIATE FIELDS IN THE LCCW(LCCWOP, LCCWCNT, & RH OP) ARE SET UP.   * 00846000
*/* OTHERWISE, THE OP CODE IN THE LCCW IS SET TO SEND, CHAIN DATA &   * 00847000
*/* IMMEDIATE DATA FLAGS TURNED ON, & COUNT SET.                      * 00848000
*/*   THE REQUEST HEADER IS NOW FORMATTED. THERE ARE 4 POSSIBILITEIS  * 00849000
*/* FOR INFO SENT TO THE NCP: 1. SYSTEM CONTROL                       * 00850000
*/*                           2. SYSTEM DATA                          * 00851000
*/*                           3. FUNCTION MGMT CONTROL                * 00852000
*/*                           4. FUNCTION MGMT DATA                   * 00853000
*/*   AFTER SETTING THE NO CHAINING BITS AND FORMAT BIT IN THE RH,    * 00854000
*/* IOFRMAT SETS THE APPROPRIATE BITS TO INDICATE THE ABOVE POSS-     * 00855000
*/* IBILITIES.                                                        * 00856000
*/*   IF THE COMMAND IS A POS RESPONSE OR NEG RESPONSE, THE BUFFER    * 00857000
*/* SYNCHRONOUS FLAG(LCCWLSI) AND THE RESPONSE INDICATOR(RHQS) ARE    * 00858000
*/* SET. IF THE CMD IS A NEG RESPONSE, THE SENSE INDICATOR(RHSENSE)   * 00859000
*/* & EXCEPTION RESPONSE INDICATOR(RHEXCEPT) ARE TURNED ON.           * 00860000
*/*    IF THE COMMAND IS SET TIME & DATE, IOFRMAT GOES TO THE STIMDAT * 00861000
*/* SUBROUTINE TO FORMAT THE SET TIME & DATE RU.                      * 00862000
*/*   IF THE SKIP SWITCH IS ON, IOFRMAT EXITS. OTHERWISE, IF THE CMD  * 00863000
*/* IS QUIESCE OR RDS, NCSPLRUI IS SET TO 0 INDICATING DON'T SAVE FDB * 00864000
*/* DATA, THE RPH @ IS PUT IN THE VRLCP & IOFRMAT EXITS.              * 00865000
*/*   OTHERWISE, THE 2ND LCCW IS FORMATTED WITH COUNT FROM THE NCSPL  * 00866000
*/* & LCCWDATA POINTING TO OUTBOUND RU(NCSPLRUO HAS @), ISTVRRPH SET  * 00867000
*/* UP, AND EXIT.                                                     * 00868000
*/********************************************************************* 00869000
*CIOLCPB=CIOLCPB&&CIOLCPB; /* CLEAR LCPB */                        0932 00870000
         L     @04,CIOLCPTR                                        0932 00871000
         XC    CIOLCPB(80,@04),CIOLCPB(@04)                        0932 00872000
*IF NCSPLCMD=NCSSNFMD&NCBTYPE=LDTYPE THEN /* IF CMD IS SEND FMD */ 0933 00873000
         CLC   NCSPLCMD(2,NCSPLPTR),@CB03372                       0933 00874000
         BNE   @RF00933                                            0933 00875000
         L     @04,NCBPTR                                          0933 00876000
         CLI   NCBTYPE(@04),X'07'                                  0933 00877000
         BNE   @RF00933                                            0933 00878000
*   DO;                                   /* & DEVICE IS LOCSL */  0934 00879000
*   LCPLNGTH=LENGTH(CIOLCPB); /* SET UP LCPB FIELDS */             0935 00880000
         L     @04,CIOLCPTR                                        0935 00881000
         MVI   LCPLNGTH(@04),X'50'                                 0935 00882000
*   LCPTYPE=LCTYPE;                                                0936 00883000
         MVI   LCPTYPE(@04),X'04'                                  0936 00884000
*   LCPSAF=ATCVTMID; /* GET SSCP'S SAF FROM ATCVT */               0937 00885000
         L     @03,NCSPLCVT(,NCSPLPTR)                             0937 00886000
         MVC   LCPSAF(2,@04),ATCVTMID(@03)                         0937 00887000
*   LCPDAF=NCSPLDAF; /* GET DAF FROM NCSPL */                      0938 00888000
         MVC   LCPDAF(2,@04),NCSPLDAF(NCSPLPTR)                    0938 00889000
*   ISTVRRPH=NCSPLPTR; /* GET NCSPL @ IN VRLCP */                  0939 00890000
         LA    @03,72                                              0939 00891000
         ALR   @03,@04                                             0939 00892000
         ST    NCSPLPTR,ISTVRRPH(,@03)                             0939 00893000
*   CIORPHHI=ON; /* TURN ON HI BIT TO INDICATE A NCSPL */          0940 00894000
         OI    CIORPHHI(@03),B'10000000'                           0940 00895000
*   LCPNXLCW=NCSPLRUO; /* GET RU @ FROM NCSPL */                   0941 00896000
         L     @03,NCSPLRUO(,NCSPLPTR)                             0941 00897000
         ST    @03,LCPNXLCW(,@04)                                  0941 00898000
*   END;                                                           0942 00899000
*ELSE DO;                                                          0943 00900000
         B     @RC00933                                            0943 00901000
@RF00933 DS    0H                                                  0944 00902000
*     IF NCSPLCMD=NCSCFME | NCSCMD2='06'X                  /*@DM13713*/ 00903000
*       | NCSPLCMD=NCSCEXCP | NCSCMD2='07'X                /*@DM13713*/ 00904000
*     THEN                               /* IS NCSPL CMD     @DM13713   00905000
*                                                   SEND POS RESPONSE   00906000
