@RF01415 DS    0H                                                  1429 02235000
*     RFY (CIOVRRPH,CIOSFMD) UNRSTD;                       /*@XM09596*/ 02236000
*     CALL FSBCHECK; /* CALL TO PROCESS FSB */                     1430 02237000
         BAL   @14,FSBCHECK                                        1430 02238000
*     CIOSKIP=ON; /* SET SKIP FLAG ON */                           1431 02239000
         OI    CIOSKIP,B'01000000'                                 1431 02240000
*  END;            /* END OF NOT SICK OR RESTART IO          @XM7543K*/ 02241000
*END; /* END FOR QIOPROC SEGMENT */                                1433 02242000
@RC01157 DS    0H                                                  1434 02243000
*               END;                /* NOTSKIP                       */ 02244000
*           END;                    /* DONT DO IO END        @XM9620K*/ 02245000
@RC01153 DS    0H                                                  1436 02246000
*       END;                        /* NOTPURGE                      */ 02247000
@RT00928 DS    0H                                                  1437 02248000
*   END;                            /* RESWON                        */ 02249000
@RC00715 DS    0H                                                  1437 02250000
@DE00713 TM    CIORESW,B'10000000'                                 1437 02251000
         BO    @DL00713                                            1437 02252000
*   IF CIOSKIP=ON THEN                                             1438 02253000
         TM    CIOSKIP,B'01000000'                                 1438 02254000
         BO    @RT01438                                            1438 02255000
*     ;                             /* IF SKIP FLAG ON, THEN SKIP CID   02256000
*                                      FINISH                        */ 02257000
*   ELSE                                                           1440 02258000
*     DO;                                                          1440 02259000
*SKIPCTL:                                                          1441 02260000
*       CIDREG=NCSIOCID;            /* SET UP CID IN REG 2           */ 02261000
SKIPCTL  L     CIDREG,NCSIOCID(,NCSPLPTR)                          1441 02262000
*       R1=NCSPLRPH;                /* SET UP REG 1 TO RPH           */ 02263000
*                                                                  1442 02264000
         L     R1,NCSPLRPH(,NCSPLPTR)                              1442 02265000
*        DO; /* CIDCTL(FINISH) CID(CIDREG) */                      1443 02266000
*                                                                  1443 02267000
*        RFY (EMCR2,EMCR3) RSTD;                                   1444 02268000
*          GEN SETS(RPHWORK)                                       1445 02269000
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      1445 02270000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              02271000
*          EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                     1446 02272000
         SLR   EMCR3,EMCR3                                         1446 02273000
*          CALL ATCLCPTR->ATCCDFIN->ISTSDCCR; /*PROCESS REQUEST*/  1447 02274000
