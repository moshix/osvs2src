         L     @04,CIOLCPTR                                        1615 02839000
         L     @03,LCPNXLCW(,@04)                                  1615 02840000
         MVI   LCCWOP(@03),X'F4'                                   1615 02841000
*                   LCCWCNT=CIOCNT; /* COUNT=3                       */ 02842000
         MVC   LCCWCNT(2,@03),@CH00289                             1616 02843000
*                   LCCWCD=ON;      /* TURN ON CHAIN DATA FLAG       */ 02844000
         OI    LCCWCD(@03),B'10000000'                             1617 02845000
*                   ADDR(CIOLCW2)->LCCWIDAT=ON;/* TURN ON IMMEDIATE     02846000
*                                      DATA                          */ 02847000
         AL    @04,@CF01109                                        1618 02848000
         OI    LCCWIDAT(@04),B'00001000'                           1618 02849000
*                   ADDR(CIOLCW2)->LCCWCNT=CIOPCNT2;               1619 02850000
         MVC   LCCWCNT(2,@04),@CH00074                             1619 02851000
