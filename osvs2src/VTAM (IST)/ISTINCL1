*        RDTPTR->RPRONLIN = B1;/* MARK THE PU ONLINE                 */ 00186000
         OI    RPRONLIN(RDTPTR),B'00001000'                        0080 00187000
*        RDTPTR->RPRLIVE = B1;/* AND MARK THE PU ALIVE, TOO          */ 00188000
         OI    RPRLIVE(RDTPTR),B'00000001'                         0081 00189000
