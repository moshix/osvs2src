*     END;                                                         0072 00171000
*     END; /* CPCALL(ISTINCL0) */       /* ACTIVATE DIRECT ATTACHMENT N 00172000
*DE     */                                                         0073 00173000
*    RTCODE = R15;            /* PRESERVE THE RETURN CODE            */ 00174000
         LR    RTCODE,R15                                          0074 00175000
*    NCSPLRAD = WRKREG;       /* RESTORE ORIG CONTENTS OF NCSPLRAD   */ 00176000
         ST    WRKREG,NCSPLRAD(,R10)                               0075 00177000
*    RFY WRKREG UNRSTD;       /* UNRESTRICT WORK REGISTER            */ 00178000
