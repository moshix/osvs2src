*          GEN (USING @DATD,R11);    /* ESTAB BASE REG FOR DATA   */    00076000
         USING @DATD,R11                                                00077000
*/* END OF CPPROC EXPANSION */                                     0039 00078000
*                                                                  0039 00079000
*/* ATBUILD (ISTNCSPL,ISTRDT)  */                                  0039 00080000
* RFY ISTLOK  ;                                                    0039 00081000
* RFY ISTDEVCH;                                                    0040 00082000
* RFY (ISTRPRE,ISTRCPRE,ISTRDT,ISTRAP,ISTRLC,ISTRRN,ISTRGP,ISTRLN,ISTRP 00083000
*,ISTRCL,ISTRTR,ISTRIN,ISTRDS,ISTDOT,ISTRCC,ISTRLU,ISTRPX,ISTRLX,ISTRSW 00084000
*ISTRDA,ISTRLS,ISTDNT,ISTGNT,ISTPAT);                              0041 00085000
*/* END OF ATBUILD */                                              0042 00086000
*DCL WRKREG PTR(31) REG(05);  /* A WORK REGISTER                     */ 00087000
*DCL RDTPTR PTR(31) REG(07) RSTD;/* BASE REG FOR ISTRDT              */ 00088000
*DCL RTCODE PTR(31) REG(09) RSTD;/* INTERNAL RETURN CODE             */ 00089000
*DCL B0 BIT(1) CONSTANT('0'B);/* BINARY 0 CONSTANT                   */ 00090000
*DCL B1 BIT(1) CONSTANT('1'B);/* BINARY 1 CONSTANT                   */ 00091000
*DCL D0 FIXED(31) CONSTANT(0);/* DECIMAL 0 CONSTANT                  */ 00092000
*DCL D1 FIXED(31) CONSTANT(1);/* DECIMAL 1 CONSTANT                  */ 00093000
*IF (NCSPLRAD->RPRONLIN = B1) /* IF PU IS (ALREADY) ONLINE           */ 00094000
*    | ((NCSPLRPT->RPRENTRY = RPRENTLS) /* OR IF CMD FOR LCL SUBAREA */ 00095000
*    & (NCSPL2IN = B1)        /* AND IF 2ND ENTRY (AFTER SYSDEF)     */ 00096000
*    & (NCSPLRAD->RCCRBISA = B0)) /* AND PU ISTATUS IS INACTIVE      */ 00097000
*  THEN RTCODE = 4;           /* THEN ACTIVATE NOT NECESSARY         */ 00098000
         L     @14,NCSPLRAD(,R10)                                  0049 00099000
         TM    RPRONLIN(@14),B'00001000'                           0049 00100000
         BO    @RT00049                                            0049 00101000
         L     @08,NCSPLRPT(,R10)                                  0049 00102000
         CLI   RPRENTRY(@08),X'05'                                 0049 00103000
         BNE   @RF00049                                            0049 00104000
         TM    NCSPL2IN(R10),B'10000000'                           0049 00105000
         BNO   @RF00049                                            0049 00106000
         TM    RCCRBISA(@14),B'10000000'                           0049 00107000
         BNZ   @RF00049                                            0049 00108000
@RT00049 DS    0H                                                  0050 00109000
         LA    RTCODE,4                                            0050 00110000
*  ELSE DO;                   /* OTHERWISE, ACTIVATE DAN             */ 00111000
         B     @RC00049                                            0051 00112000
@RF00049 DS    0H                                                  0052 00113000
*    RDTPTR = NCSPLRAD;       /* GET ADDR OF PU RDTE                 */ 00114000
*                                                                  0052 00115000
         L     RDTPTR,NCSPLRAD(,R10)                               0052 00116000
*/*   DOINF  */                                                    0053 00117000
*                                                                  0053 00118000
*     DO  UNTIL(1=2);                  /* LOOP TO MARK PU, ALL LU RDTES 00119000
*      */                                                          0053 00120000
@DL00053 DS    0H                                                  0054 00121000
*      RDTPTR->RPRPTHCT = D1; /* SET CURRENT RDTE PATH COUNT TO 1    */ 00122000
*                                                                  0054 00123000
         MVI   RPRPTHCT(RDTPTR),X'01'                              0054 00124000
*/*     LEAVE ON(RDTPTR->RPRENTED=B1) */                           0055 00125000
*       IF (RDTPTR->RPRENTED=B1) THEN                              0055 00126000
         TM    RPRENTED(RDTPTR),B'00000100'                        0055 00127000
         BO    @RT00055                                            0055 00128000
*         GOTO #DC00009;/* EXIT IF LAST RDT ENTRY      */          0056 00129000
*      RDTPTR = RDTPTR + RDTPTR->RPRELEN;/* GET ADDR OF NEXT RDTE    */ 00130000
*                                                                  0057 00131000
         AH    RDTPTR,RPRELEN(,RDTPTR)                             0057 00132000
*/*     LEAVE ON(RDTPTR->RPRENTRY^=RPRENTLU) */                    0058 00133000
*       IF (RDTPTR->RPRENTRY^=RPRENTLU) THEN                       0058 00134000
         CLI   RPRENTRY(RDTPTR),X'81'                              0058 00135000
         BNE   @RT00058                                            0058 00136000
*         GOTO #DC00009;/* EXIT IF NOT AN LU    */                 0059 00137000
*                                                                  0059 00138000
*       END; /* END  */                                            0060 00139000
@DE00053 B     @DL00053                                            0060 00140000
*#DC00009:;                  /* CONTINUE UNTIL OUT OF LUS           */  00141000
#DC00009 DS    0H                                                  0062 00142000
*    NCSPLRAD->RPRNSNEA->RPRPTHCT = B1;/* SET DAN PATH COUNT TO 1    */ 00143000
         L     @14,NCSPLRAD(,R10)                                  0062 00144000
         L     @08,RPRNSNEA(,@14)                                  0062 00145000
         MVI   RPRPTHCT(@08),X'01'                                 0062 00146000
*    RFY WRKREG RSTD;         /* RESTRICT WORK REGISTER              */ 00147000
*    WRKREG = NCSPLRAD;       /* SAVE ADDRESS OF PU RDTE             */ 00148000
         LR    WRKREG,@14                                          0064 00149000
*    NCSPLRAD = NCSPLRAD->RPRNSNEA;/* PT NCSPL TO DAN RDTE           */ 00150000
         ST    @08,NCSPLRAD(,R10)                                  0065 00151000
*     DO; /* CPCALL(ISTINCL0) */                                   0066 00152000
*     R01 = ADDR(ISTNCSPL);     /* SET INPUT REGISTER        */    0067 00153000
         LR    R01,R10                                             0067 00154000
