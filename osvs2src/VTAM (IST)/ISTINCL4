*DCL ZCTYPE BIT(8) CONSTANT('00'X);   /* ZCR TYPE                   */  02745000
*DCL ZCLNG FIXED(15) CONSTANT(172);   /* ZCRR LENGTH                */  02746000
*DCL ZCRSAVNO BIT(8) CONSTANT('06'X); /* NUMBER OF SAVEAREAS IN ZCRR */ 02747000
*DCL ZCRLSVNO BIT(8) CONSTANT('04'X); /* NUMBER OF SAVE AREAS IN   0715 02748000
*                                         LOCAL SUPPORT              */ 02749000
*/* *** EQUATES FOR ZCRRCDSV *** */                                0716 02750000
*                                                                  0716 02751000
*DCL ZCRMRFSB BIT(8) CONSTANT('0C'X);  /* REQUEST COMPLETION MOVE FSB   02752000
*                                         TO PROCESS                 */ 02753000
*DCL ZCRSRCHQ BIT(8) CONSTANT('10'X);  /* REQUEST COMPLETION SEARCH     02754000
*                                         REQUEST QUEUE              */ 02755000
*DCL ZCREXIT  BIT(8) CONSTANT('14'X);  /* REQUEST COMPLETION EXIT    */ 02756000
*DCL ZCRSFSB  BIT(8) CONSTANT('18'X);  /* REQUEST COMPLETION SINGLE     02757000
*                                         FSB                        */ 02758000
*DCL ZCRDLFSB BIT(8) CONSTANT('1C'X);  /* REQUEST COMPLETION DUAL FSB * 02759000
*DCL ZCRNOSCH BIT (8) CONSTANT('20'X); /* REQUEST COMPLETION, EXIT WITH 02760000
*                                         SCHED NONE                 */ 02761000
