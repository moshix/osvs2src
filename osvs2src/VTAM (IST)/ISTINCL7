*     3 IOBDYNBF BIT(1),             /* DYNAMIC BUFFERING            */ 01100000
*     3 IOBRDEXC BIT(1),             /* READ EXCLUSIVE               */ 01101000
*     3 IOBRELBL BIT(1),             /* RELATIVE BLOCK ADDRESSING    */ 01102000
*    2 IOBDTYP2 CHAR(1),             /* SECOND BYTE OF OPTIONS AND 0101 01103000
*                                       REQUESTS                     */ 01104000
*     3 IOBSKEY  BIT(1),             /* KEY ADDRESS CODED AS 'S'     */ 01105000
*     3 IOBSBLKL BIT(1),             /* BLOCK LENGTH CODED AS 'S'    */ 01106000
*     3 IOBSUFFX BIT(2),             /* IF BITS 2 AND 3 ARE ONE, RU     01107000
*                                       IS SUFFIXED TO THE TYPE,   0101 01108000
*                                       INDICATING THAT THE FEEDBACK    01109000
*                                       ADDRESS IN DECNXADR CAN BE 0101 01110000
*                                       THE ADDRESS OF EITHER THE  0101 01111000
*                                       NEXT DATA RECORD OR THE NEXT    01112000
*                                       CAPACITY RECORD, WHICHEVER 0101 01113000
*                                       OCCURS FIRST.              0101 01114000
*                                       IF BIT 2 IS ZERO AND BIT 3 IS   01115000
*                                       ONE, R IS SUFFIXED TO THE  0101 01116000
*                                       TYPE, INDICATING THAT THE  0101 01117000
*                                       FEEDBACK ADDRESS IN DECNXADR    01118000
*                                       IS THE ADDRESS OF THE NEXT 0101 01119000
*                                       DATA RECORD.                 */ 01120000
*     3 IOBRQUST BIT(1),             /* IF 1, READ REQUEST.        0101 01121000
*                                       IF 0, WRITE REQUEST.         */ 01122000
*     3 IOBTYPE  BIT(1),             /* IF 1, KEY TYPE.            0101 01123000
*                                       IF 0, ID TYPE.               */ 01124000
*     3 IOBADDTY BIT(1),             /* ADD TYPE                     */ 01125000
*     3 IOBRELEX BIT(1),             /* RELEX MACRO ISSUED           */ 01126000
*    2 IOBDSTAT CHAR(2),             /* STATUS OF THE I/O REQUEST    */ 01127000
