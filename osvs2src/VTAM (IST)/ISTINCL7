*         3 *        BIT(4),       /* PROTECT KEY             */   0039 00298000
*         3 IOSCKEY4 BIT(1),       /* RESERVED                */   0039 00299000
*         3 IOSCKEY5 BIT(1),       /* RESERVED                */   0039 00300000
*         3 IOSCKEY6 BIT(1),       /* RESERVED                */   0039 00301000
*         3 IOSCKEY7 BIT(1),       /* RESERVED                */   0039 00302000
*       2 IOSMDB    CHAR(1),       /* ERP IMMED. CCW OP CODE  */   0039 00303000
*       2 IOSMDM    CHAR(1),       /* ERP MODIFIER MASK       */   0039 00304000
*       2 IOSEEK    CHAR(8),       /* STATIC SEEK ADDRESS     */   0039 00305000
*       2 IOSEEKA   CHAR(8),       /* DYNAMIC SEEK ADDRESS    */   0039 00306000
*         3 IOSSKM  PTR(8),             /*   M                */   0039 00307000
*         3 IOSSKBB PTR(16) BDY(BYTE),  /*   BB               */   0039 00308000
*         3 IOSCCHH CHAR(4),            /*   CCHH             */   0039 00309000
*          4 IOSSKCC PTR(16) BDY(BYTE), /*   CC               */   0039 00310000
*          4 IOSSKHH PTR(16) BDY(BYTE), /*   HH               */   0039 00311000
*             5 IOSSKH1 PTR(8),         /*   H                */   0039 00312000
*             5 IOSSKH2  PTR(8),        /*   H                */   0039 00313000
*         3 IOSSKR  PTR(8),             /*   R                */   0039 00314000
*       2 IOSEND    CHAR(0);       /* END OF IOSB             */   0039 00315000
*                                                                  0039 00316000
*  /*  ATTENTION PROCESSING SECTION                           */   0040 00317000
*   DCL 1 IOSATTSC  CHAR(64) DEF(IOSIPIB),                         0040 00318000
*         2 IOSATTSN CHAR(24),     /* ADDITIONAL SENSE IF ANY */   0040 00319000
*         2 *        CHAR(16),     /* ATTN RTN RSVD AREA      */   0040 00320000
*         2 IOSATTWA CHAR(24);     /* ATTN RTN WORK AREA      */   0040 00321000
*                                                                  0040 00322000
*  /*  WTO PROCESSING SECTION                                 */   0041 00323000
*   DCL 1 IOSWTOWA  CHAR(64) DEF(IOSIPIB),                         0041 00324000
*         2 IOSWTOCH CHAR(2),      /* ADDR CC=3 OCCURRED ON   */   0041 00325000
*         2 IOSWTOCP CHAR(1),      /* CPU CC=3 OCCURRED ON    */   0041 00326000
*         2 IOSWTOPT CHAR(1),      /* PATH IND. FOR CC=3      */   0041 00327000
*         2 IOSWRNDR CHAR(60);     /* REMAINDER OF WTO W.A.   */   0041 00328000
*                                                                  0041 00329000
*  /*  PCI SCHEDULING SECTION                                 */   0042 00330000
*   DCL 1 IOSPCISC  CHAR(64)  DEF(IOSIPIB) BDY(WORD),              0042 00331000
*         2 *       PTR(31),       /* SAME AS IOSIPIB. MUST        0042 00332000
*                                     NOT BE USED             */   0042 00333000
*         2 *       PTR(31),       /* SAME AS IOSPCHN. MUST        0042 00334000
*                                     NOT BE USED             */   0042 00335000
*         2 IOSPCIRS CHAR(36),     /* PCI RSVD SECTION        */   0042 00336000
*         2 IOSPCIWA CHAR(20);     /* PCI WORK AREA           */   0042 00337000
*                                                                  0042 00338000
* /************************************************************/   0043 00339000
* /*                                                          */   0043 00340000
* /*     FOLLOWING ARE THE PLS DECLARES WHICH REPLACE  THE    */   0043 00341000
* /*     PREVIOUSLY DEFINED %DCLARES.  THE FIELDS TO WHICH    */   0043 00342000
* /*     THE VALUES APPLY ARE REPRODUCED AS A COMMENT. THIS   */   0043 00343000
* /*     CHANGE ALLOWS A CROSS REFERENCE OF THE NAMES  USED   */   0043 00344000
* /*     AS FIELD VALUES.                                     */   0043 00345000
* /*                                                          */   0043 00346000
* /************************************************************/   0043 00347000
*                                                                  0043 00348000
*   /*  2 IOSPROC  CHAR(1),           SEE DCL FOR DESCRIPTION */   0043 00349000
*                                                                  0043 00350000
*   DCL IOSAPCI BIT(8) CONSTANT('04'X);     /* PCI            */   0043 00351000
*   DCL IOSATTN BIT(8) CONSTANT('08'X);     /* ATTENTION      */   0044 00352000
*   DCL IOSAPURG BIT(8) CONSTANT('0C'X);    /* PURGE          */   0045 00353000
*   DCL IOSADAVV BIT(8) CONSTANT('10'X);    /* DAVV           */   0046 00354000
*   DCL IOSAWTO BIT(8) CONSTANT('14'X);     /* WTO            */   0047 00355000
*   DCL IOSADDR BIT(8) CONSTANT('18'X);     /* DDR            */   0048 00356000
*   DCL IOSACRH BIT(8) CONSTANT('1C'X);     /* CRH            Y30CQLG*/ 00357000
*                                                                  0049 00358000
*   /*  2 IOSDVRID CHAR(1),           SEE DCL FOR DESCRIPTION */   0050 00359000
*                                                                  0050 00360000
*   DCL IOSIOSID BIT(8) CONSTANT('00'X); /*                        0050 00361000
