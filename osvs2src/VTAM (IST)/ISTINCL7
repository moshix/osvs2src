SVTM     TITLE 'ISTINCL7 - ABANDON CONNECTION PROCESSOR FOR LOCAL PU   *00001000
                        '                                               00002000
ISTINCL7 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCL7  75.129'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*/* END OF ATBUILD */                                              0006 00014000
*                                                                  0006 00015000
*          DO;                                                     0006 00016000
*/***  CRRMAINT  RPHADD((R01->NCSPLRPH))PROCNM(ISTINCL7)COMP(VY)FUNC(SU 00017000
*)  ***/                                                           0007 00018000
*                                                                  0007 00019000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0007 00020000
*          RFY ISTLCCW ;                                           0007 00021000
*          RFY ISTLCPB ;                                           0008 00022000
*          RFY ISTZSAVE;                                           0009 00023000
*          RFY ISTPFCRR;                                           0010 00024000
*          RFY ISTCFCRR;                                           0011 00025000
*          RFY ISTZCRR ;                                           0012 00026000
*          RFY ISTASCRR;                                           0013 00027000
*          RFY ISTAPCRR;                                           0014 00028000
*          RFY ISTCRA  ;                                           0015 00029000
*          RFY ISTCNSTA;                                           0016 00030000
*          RFY ISTVYCRR;                                           0017 00031000
*/* END OF ATBUILD */                                              0018 00032000
*          /*** SET ENTERED BIT FOR THIS PROCEDURE ***/            0018 00033000
*R01->NCSPLRPH->RPHCRR->CRACRR->NINCL7 = '1'B;                     0018 00034000
         L     @14,NCSPLRPH(,R01)                                  0018 00035000
         L     @14,RPHCRR(,@14)                                    0018 00036000
         L     @14,CRACRR(,@14)                                    0018 00037000
         OI    NINCL7(@14),B'00100000'                             0018 00038000
*          /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/           0019 00039000
*R01->NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                     0019 00040000
         XC    VYCCRT+1(11,@14),VYCCRT+1(@14)                      0019 00041000
         MVI   VYCCRT(@14),B'00000000'                             0019 00042000
*R01->NCSPLRPH->RPHCRR->CRACRR->CINCL7 = '1'B;                     0020 00043000
         OI    CINCL7(@14),B'00100000'                             0020 00044000
*          END;                                                    0021 00045000
*          DCL R01 PTR(31) REG(01) RSTD;/* INPUT PARAMETER REG    */    00046000
*          DCL R13 PTR(31) REG(13) RSTD;/* SAVE AREA PTR REGISTER */    00047000
*          DCL R15 PTR(31) REG(15) RSTD;/* WORK REGISTER          */    00048000
*          DCL R10 PTR(31) REG(10) RSTD;/* BASE REG FOR NCSPL     */    00049000
*          DCL R11 PTR(31) REG(11) RSTD;/* DATA AREA PTR REGISTER */    00050000
*          DCL @SIZDATD FIXED(31) GEND;/* size of DATA area       */    00051000
*          R10 = R01;                /* INIT NCSPL BASE REGISTER  */    00052000
         LR    R10,R01                                             0028 00053000
*          RFY ISTNCSPL BASED(R10);  /* ADDRESSABILITY FOR NCSPL  */    00054000
*          RFY ISTRPH BASED(NCSPLRPH);/* ADDRESSABILITY FOR RPH   */    00055000
*          R15 = R13;                /* SAVE CALLER SAVE AREA ADR */    00056000
         LR    R15,R13                                             0031 00057000
*          R13 = R13 - LENGTH(ISTRSA);/* SET SAVE AREA PTR REG    */    00058000
         SL    R13,@CF00862                                        0032 00059000
*          R15->RSAFPT = R13;        /* SET CALLERS FORWARD PTR   */    00060000
         ST    R13,RSAFPT(,R15)                                    0033 00061000
*          R13->RSABPT = R15;        /* SET MY BACK PTR           */    00062000
         ST    R15,RSABPT(,R13)                                    0034 00063000
*          R13->RSAFPT = 0;          /* SET MY FORWARD PTR        */    00064000
         SLR   @14,@14                                             0035 00065000
         ST    @14,RSAFPT(,R13)                                    0035 00066000
*          R11= VWANAV + 7 & 'FFFFFFF8'X;/* DATAREG = DWORD BDY   */    00067000
         L     @14,NCSPLWKA(,R10)                                  0036 00068000
         LA    R11,7                                               0036 00069000
         AL    R11,VWANAV(,@14)                                    0036 00070000
         N     R11,@CF00073                                        0036 00071000
*          VWANAV = R11 + @SIZDATD;  /* ALLOCATE DATA AREA        */    00072000
         LR    @00,R11                                             0037 00073000
         AL    @00,@SIZDATD                                        0037 00074000
         ST    @00,VWANAV(,@14)                                    0037 00075000
*          GEN (USING @DATD,R11);    /* ESTAB BASE REG FOR DATA   */    00076000
         USING @DATD,R11                                                00077000
*/* END OF CPPROC EXPANSION */                                     0039 00078000
*                                                                  0039 00079000
*/*************************************************************/   0039 00080000
*/*                                                           */   0039 00081000
*/*  THIS STRUCTURE DESCRIBES THE CONTROL BLOCK PASSED TO THE */   0039 00082000
*/*  I/O SUPERVISOR  AS A  PARAMETER OF THE SRB.  THE CONTROL */   0039 00083000
*/*  BLOCK CONTAINS ALL INFORMATION NECESSARY TO INITIATE AND */   0039 00084000
*/*  TERMINATE AN I/O OPERATION.  FIELDS OF THE IOSB ARE USED */   0039 00085000
*/*  FOR COMMUNICATION BETWEEN THE:                           */   0039 00086000
*/*                                                           */   0039 00087000
*/*      1. I/O SUPERVISOR AND I/O REQUESTOR                  */   0039 00088000
*/*      2. I/O SUPERVISOR AND ERROR RECOVERY PROCEDURES      */   0039 00089000
*/*      3. COMPONENTS OF THE I/O SUPERVISOR                  */   0039 00090000
*/*      4. SUCCESSIVE ENTRANCES TO THE ERP'S                 */   0039 00091000
*/*      5. ERP AND COMMON WTO AND STATISTICS UPDATE MODULES  */   0039 00092000
*/*                                                           */   0039 00093000
*/*************************************************************/   0039 00094000
*/*                                                           */   0039 00095000
*/*      05/30/72   LEVEL=00                                  */   0039 00096000
*/*                                                           */   0039 00097000
*/*************************************************************/   0039 00098000
*                                                                  0039 00099000
*   DCL 1 IOSB  BDY(WORD)  BASED(IOSBPTR),                         0039 00100000
*       2 IOSFLA    CHAR(1),       /* FLAG BYTE A             */   0039 00101000
*         3 IOSACHN BIT(2),        /* COMMAND & DATA CHAINING */   0039 00102000
*           4 IOSDCHN BIT(1),      /* DATA CHAINING           */   0039 00103000
*           4 IOSCCHN BIT(1),      /* COMMAND CHAINING        */   0039 00104000
*         3 IOSERR  BIT(1),        /* ERROR ROUTINE IN CONTROL     0039 00105000
*                                     MUST BE ZEROED BY DRIVER     0039 00106000
*                                     IF ERP RETURNS WITH THIS     0039 00107000
*                                     BIT ON, A RETRY IS           0039 00108000
*                                     ASSUMED. IF THE ERP RE-      0039 00109000
*                                     TURNS WITH THIS BIT OFF,     0039 00110000
*                                     THE ERROR IS CONSIDERED      0039 00111000
*                                     PERMANENT OR CORRECTED       0039 00112000
*                                     DEPENDING ON THE SETTING     0039 00113000
*                                     OF IOSEX.               */   0039 00114000
*         3 IOSSMDA BIT(1),        /* ERP STATUS MODFR BIT .       0039 00115000
*                                     MUST BE ZEROED BY DRIVER     0039 00116000
*                                      TAPE - REPOSITION DEVCE     0039 00117000
*                                      1052 - IMMEDIATE OPERA-     0039 00118000
*                                        TION, CCW OP CODE IN      0039 00119000
*                                        IOSMDB.              */   0039 00120000
*         3 IOSSMDB BIT(1),        /* ERP STATUS MODFR BIT B       0039 00121000
*                                     MUST BE ZEROED BY DRIVER     0039 00122000
*                                     SET BY FETCH IN APPEN-       0039 00123000
*                                     DAGE FOR POSTING.            0039 00124000
*                                      TAPE - CRC NEEDED           0039 00125000
*                                      DASD - PCI FETCH STOP       0039 00126000
*                                           FLAG              */   0039 00127000
*         3 IOSEX   BIT(1),        /* EXCEPTIONAL CONDITION.       0039 00128000
*                                     UPON RETURN FROM NORMAL      0039 00129000
*                                     OR ABNORMAL EXIT WITH        0039 00130000
*                                     THIS BIT ON, ERP PROCES-     0039 00131000
*                                     SING IS INITIATED IF IN-     0039 00132000
*                                     ITIAL ERROR CONDITION.       0039 00133000
*                                     IF BIT IS OFF, IT IS AS-     0039 00134000
*                                     SUMED THAT THE EXIT COR-     0039 00135000
*                                     RECTED THE CONDITION OR      0039 00136000
*                                     DID NOT CONSIDER IT AN       0039 00137000
*                                     ERROR. WHEN THE ERP RE-      0039 00138000
*                                     TURNS WITH THIS BIT ON,      0039 00139000
*                                     AND IOSERR OFF,THE ERROR     0039 00140000
*                                     IS CONSIDERED PERMANENT.     0039 00141000
*                                     WHEN THE ERP RETRNS WITH     0039 00142000
*                                     THIS BIT OFF, THE ERROR      0039 00143000
*                                     HAS BEEN CORRECTED.     */   0039 00144000
*         3 IOSDOM  BIT(1),        /* DOM MACRO REQUIRED     @YM06828*/ 00145000
*         3 IOSIOSB BIT(1),        /* IOSB CREATED BY IOS.         0039 00146000
*                                     MUST BE ZEROED BY DRIVER*/   0039 00147000
*                                                                  0039 00148000
*       2 IOSFLB    CHAR(1),       /* FLAG BYTE B             */   0039 00149000
*         3 IOSDIESE BIT(1),       /* SECOND ENTRY TO DIE     */   0039 00150000
*         3 IOSFLB1  BIT(1),       /* RESERVED                */   0039 00151000
*         3 IOSFLB2  BIT(1),       /* RESERVED                */   0039 00152000
*         3 IOSFLB3  BIT(1),       /* RESERVED                */   0039 00153000
*         3 IOSFLB4  BIT(1),       /* RESERVED                */   0039 00154000
*         3 IOSMSG   BIT(1),       /* MESSAGE INDICATOR            0039 00155000
*                                     0 = INTRVNTN REQURD MSG      0039 00156000
*                                     1 = I/O ERROR MESSAGE   */   0039 00157000
*         3 IOSBDCST BIT(1),       /* BROADCAST BIT           */   0039 00158000
*         3 IOSLOG   BIT(1),       /* CREATE AN OBR RECORD    */   0039 00159000
*                                                                  0039 00160000
*       2 IOSFLC    CHAR(1),       /* DEVICE DEP ERP FLAGS    */   0039 00161000
*         3 IOSDVMNT BIT(1),       /* DAVV ISSUED MOUNT       */   0039 00162000
*         3 IOSDVALT BIT(1),       /* ALTERNATE TRACK PROCES-      0039 00163000
*                                     SING BY DAVV            */   0039 00164000
*          4 IOSVERIF BIT(1),      /* UNSOLICITED DEVICE END       0039 00165000
*                                     VERIFICATION NEEDED          0039 00166000
*                                     (NON-DA)                */   0039 00167000
*         3 IOSFLC2  BIT(1),       /* RESERVED                */   0039 00168000
*         3 IOSFLC3  BIT(1),       /* RESERVED                */   0039 00169000
*         3 IOSFLC4  BIT(1),       /* RESERVED                */   0039 00170000
*         3 IOSGLC5  BIT(1),       /* RESERVED                */   0039 00171000
*         3 IOSFLC6  BIT(1),       /* RESERVED                */   0039 00172000
*         3 IOSFLC7  BIT(1),       /* RESERVED                */   0039 00173000
*                                                                  0039 00174000
*       2 IOSPROC   CHAR(1),       /* THIS BYTE INDICATES WHAT     0039 00175000
*                                     TYPE OF SPECIAL PROCES-      0039 00176000
*                                     SING IS TO BE PERFORMED      0039 00177000
*                                     BY IOS COMPONENTS OPER-      0039 00178000
*                                     ATING ASYNCHRONOUS TO        0039 00179000
*                                     MAINLINE                     0039 00180000
*                                     MUST BE ZEROED BY DRIVER     0039 00181000
*                                     DCLS ARE AT END OF IOSB */   0039 00182000
*                                                                  0039 00183000
*       2 IOSDVRID  CHAR(1),       /* DRIVER ID VALUE              0039 00184000
*                                     VALUES AT END OF IOSB   */   0039 00185000
*                                                                  0039 00186000
*       2 IOSPRLVL  CHAR(1),       /* PRIORITY LEVEL AT WHICH      0039 00187000
*                                     THE ADDRESS SPACE IS TO BE   0039 00188000
*                                     SCHEDULED, 0 OR 1       */   0039 00189000
*       2 IOSASID   CHAR(2),       /* ADDR SPACE ID OF MEMORY      0039 00190000
*                                     TO BE SCHEDULED AT TERM      0039 00191000
*                                     OF I/O REQUEST          */   0039 00192000
*       2 IOSPGAD   PTR(31),       /* PGM ADDR TO BE DISPTCHD */   0039 00193000
*       2 IOSPKEY BIT(8),          /* PROTECT KEY OF IOSPGAD  */   0039 00194000
*        3 *       BIT(4),         /* PROTECT KEY             */   0039 00195000
*        3 IOSLCL  BIT(1),         /* ASID SCHDLD AT LOCAL LVL*/   0039 00196000
*        3 IOSIDR  BIT(1),         /* ASYNCHRONOUS SCHEDULING      0039 00197000
*                                     SHOULD BE USED FOR THIS      0039 00198000
*                                     REQUEST (INDIRECT RE-        0039 00199000
*                                     CORDING FOR PAGING I/O  */   0039 00200000
*        3 IOSPGDPX BIT(1),        /* THIS REQUEST HAS A BACK      0039 00201000
*                                     UP COPY (DUPLEXED PAGE) */   0039 00202000
*        3 IOSPKY7  BIT(1),        /* RESERVED                */   0039 00203000
*       2 IOSCOD    CHAR(1),       /* I/O COMPLETION CODE          0039 00204000
*                                     USED AS TEMP SAVE FOR        0039 00205000
*                                     SYSTEM MASK BY CHAN SCHD     0039 00206000
*                                     VALUES AT END OF IOSB   */   0039 00207000
*                                                                  0039 00208000
*       2 IOSOPT    CHAR(1),       /* OPTIONS BYTE            */   0039 00209000
*         3 IOSBYP  BIT(1),        /* BYPSS IOS CHAN PGM PRFX */   0039 00210000
*         3 IOSDEP  BIT(1),        /* DEVICE END POST REQUSTD */   0039 00211000
*         3 IOSQISCE BIT(1),       /* THIS REQUEST INITIATED       0039 00212000
*                                     BY FUNCTION WHICH HAS        0039 00213000
*                                     QUIESCED THE DEVICE     */   0039 00214000
*         3 IOSPSLL BIT(1),        /* ON=LOCAL LOCK NOT WANTED     0039 00215000
*                                     FOR POST STATUS PROCSS       0039 00216000
*                                     OFF=LOCAL LOCK WANTED   */   0039 00217000
*         3 IOSNERP BIT(1),        /* IBM ERPS NOT TO BE USED */   0039 00218000
*         3 IOSTSLL BIT(1),        /* ON = LOCAL LOCK NOT          0039 00219000
*                                     WANTED BY TERMINATION        0039 00220000
*                                     ROUTINE                      0039 00221000
*                                     OFF = LOCAL LOCK WANTED      0039 00222000
