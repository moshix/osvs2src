*                                     FOR ENDING STATUS IOSB. */   0039 00267000
*       2 IOSERP    PTR(31),       /* ERP DYNAMIC W.A. ADDR.       0039 00268000
*                                     MUST BE ZEROED BY DRIVER*/   0039 00269000
*       2 IOSPCI    PTR(31),       /* PCI EXIT ADDRESS        */   0039 00270000
*       2 IOSNRM    PTR(31),       /* NORMAL EXIT ADDRESS     */   0039 00271000
*       2 IOSABN    PTR(31),       /* ABNORMAL EXIT ADDRESS   */   0039 00272000
*       2 IOSDIE    PTR(31),       /* DISABLED INT. EXIT ADDR */   0039 00273000
*       2 IOSRST    PTR(31),       /* REAL ADDRESS OF REAL         0039 00274000
*                                     CHANNEL PROGRAM         */   0039 00275000
*       2 IOSVST    PTR(31),       /* VIRTUAL ADDRESS OF REAL      0039 00276000
*                                     CHANNEL PROGRAM         */   0039 00277000
*       2 IOSDSID   PTR(31),       /* DATA SET ID FOR PURGE   */   0039 00278000
*                                                                  0039 00279000
*       2 IOSRSS1B  CHAR(1),       /* RESERVED                */   0039 00280000
*       2 IOSAFF    BIT(8),        /* CPU AFFINITY INDICATOR       0039 00281000
*                                     FOR GUARANTEED DEVCE PTH*/   0039 00282000
*       2 IOSPATH   CHAR(2),       /* PATH SPECIFICATION FOR       0039 00283000
*                                     GUARANTEED DEVICE PATH       0039 00284000
*                                     OR SPECIFIC EXPOSURE         0039 00285000
*                                     REQUESTS                */   0039 00286000
*         3 IOSCHN  CHAR(1),                                       0039 00287000
*           4 IOSGDP    BIT(1),    /* GUARANTEED DEVICE PATH  */   0039 00288000
*           4 IOSEXP    BIT(1),    /* SPECIFIC EXPOSURE REQST */   0039 00289000
*           4 IOSPATH2  BIT(1),    /* RESERVED                */   0039 00290000
