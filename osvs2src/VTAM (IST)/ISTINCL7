*                                     IF IOSPSLL ALSO ON      */   0039 00224000
*         3 IOSAPR  BIT(1),        /* ALT PATH RETRY ACTIVE.       0039 00225000
*                                     MUST BE ZEROED BY DRIVER*/   0039 00226000
*         3 IOSRELSE BIT(1),       /* STAND-ALONE RELEASE CCW      0039 00227000
*                                     ISSUED BY IOS           */   0039 00228000
*                                                                  0039 00229000
*       2 IOSOPT2  CHAR(1),        /* OPTION BYTE 2           */   0039 00230000
*         3 IOSHTP  BIT(1),        /* ELIGIBLE FOR SHOULDR TAP*/   0039 00231000
*         3 IOSOPT2X BIT(7),       /* RESERVED BITS (UNUSED)  */   0039 00232000
*       2 IOSUCB    PTR(31),       /* UNIT CONTROL BLOCK ADDR */   0039 00233000
*       2 *         CHAR(8),                                       0039 00234000
*         3 IOSCC     BIT(8),      /* SIO CONDITION CODE      */   0039 00235000
*         3 IOSCSW    CHAR(7),     /* LOW ORDR 7 BYTES OF CSW */   0039 00236000
*          4 IOSCSWCA  PTR(24) BDY(BYTE), /*                       0039 00237000
*                                     LAST COMMAND ADDRESS    */   0039 00238000
*          4 IOSTATUS  CHAR(2),    /* CSW STATUS              */   0039 00239000
*            5 IOSTSA  BIT(8),     /* DEVICE STATUS           */   0039 00240000
*            5 IOSTSB  BIT(8),     /* CHANNEL STATUS          */   0039 00241000
*          4 IOSCSWRC PTR(16) BDY(BYTE), /*                        0039 00242000
*                                     RISIDUAL COUNT          */   0039 00243000
*                                                                  0039 00244000
*       2 IOSSRB    PTR(31),       /* POINTER BACK TO SRB     */   0039 00245000
