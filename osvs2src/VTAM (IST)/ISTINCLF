         ST    @15,CLFWKPT5                                        1307 02038000
*        END;                                                      1308 02039000
*                                                                  1308 02040000
@DE01306 L     @15,CLFWKPT5                                        1308 02041000
         C     CLFWKPT2,RPRLGONF(,@15)                             1308 02042000
         BNE   @DL01306                                            1308 02043000
*        CLFWKPT5 -> RPRLGONF          /*CHAIN PRECEDING RDTE TO RDTE*/ 02044000
*         = CLFWKPT2 -> RPRLGONF;      /*FOLLOWING OUR NODE */     1309 02045000
         L     @07,RPRLGONF(,CLFWKPT2)                             1309 02046000
         ST    @07,RPRLGONF(,@15)                                  1309 02047000
*        CLFWKPT2 -> RPRLGONF =CLFWKPT2; /*COMPLETE REMOVAL OF NODE*/   02048000
         ST    CLFWKPT2,RPRLGONF(,CLFWKPT2)                        1310 02049000
*        CLFWKPT2 -> RPRLGONB =CLFWKPT2; /*FROM CHAIN BY PT TO SELF*/   02050000
*                                                                  1311 02051000
         ST    CLFWKPT2,RPRLGONB(,CLFWKPT2)                        1311 02052000
* /*SEGMENT(CLFDCHN)FINISH*/DO;GOTO CLFDCHN#;CLFDCHN@:END;         1312 02053000
