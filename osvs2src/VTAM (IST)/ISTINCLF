         TITLE 'ISTINCLF VARY LOGON PROCESSOR                          *00001000
                        '                                               00002000
ISTINCLF CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCLF  76.009'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*DCL R10     REG(10);                                              0490 00014000
*DCL R11     REG(11);                                              0491 00015000
*DCL R12     REG(12);           /* BASE REG */                     0492 00016000
*DCL R13SAVE REG(13) PTR(31);    /*SAVE AREA ADDRESS */            0493 00017000
*DCL R14RTNAD REG(14) PTR(31);   /* RETURN ADDRESS */              0494 00018000
*DCL R15RTNCD REG(15) PTR(31);   /* RETURN CODE */                 0495 00019000
*                                                                  0495 00020000
*                                                                  0495 00021000
*RFY ISTATCVT BASED (NCSPLCVT);  /* SET BASE FOR CVT */            0496 00022000
*RFY ISTVYCRR BASED(CRACRR);                               /*@D32CKKU*/ 00023000
*RFY ISTCRA   BASED(RPHCRR);                               /*@D32CKKU*/ 00024000
*RFY ISTRPH   BASED(NCSPLRPH);                             /*@D32CKKU*/ 00025000
*                                                                  0499 00026000
*RFY (R1INPUT,R4MSGID,R5NCSPL,R6WKA,R13SAVE,CLFWKPT2) RSTD;        0500 00027000
*/*  DATA AREA OVERLAYS */                                         0501 00028000
*                                                                  0501 00029000
*                                                                  0501 00030000
*DCL 1 CLFSVMAP BASED,            /*SAVE AREA OVERLAY */           0501 00031000
*    2 * FIXED (31),                                               0501 00032000
