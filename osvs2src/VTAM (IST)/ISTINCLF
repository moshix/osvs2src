*           ELSE DO;                  /*LID PRESENT OR NAME IN ENTRY */ 01860000
@RF01227 DS    0H                                                  1230 01861000
*              IF CLFWKPT2->RPRLGONB=CLFWKPT2       /*ON ANY QUEUE*/    01862000
*                 THEN;               /*NOT ON QUEUE */            1230 01863000
         C     CLFWKPT2,RPRLGONB(,CLFWKPT2)                        1230 01864000
         BE    @RT01230                                            1230 01865000
*                 ELSE DO;            /*ON A QUEUE */              1232 01866000
*                     /*SEGMENT(CLFDCHN)REQUEST*/DO;GOTO CLFDCHN;CLFDCH 01867000
*#:END;/*CALL DECHAIN SEG*/                                        1233 01868000
         B     CLFDCHN                                             1234 01869000
CLFDCHN# DS    0H                                                  1236 01870000
*                 END;                /*END ON QUEUE CODE*/        1236 01871000
*              CALL CLFONLY;          /*CALL LOGON RTN*/           1237 01872000
@RT01230 BAL   @14,CLFONLY                                         1237 01873000
*           END;                      /*END LID PRESENT */         1238 01874000
*         END;                   /*END ERROR SW OFF          @D32CKKU*/ 01875000
*     END;                            /*END ONE OF THE ABOVE*/     1240 01876000
*     ELSE;                           /*NOT TERM,COM,OR UTERM*/    1241 01877000
