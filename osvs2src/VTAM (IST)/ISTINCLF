*/********************************************************************* 01803000
*/*                                                                   * 01804000
*/*        CLFENTPR SUBRTN TO PROCESS INDIVIDUAL ENTRIES PASSED BY    * 01805000
*/*                 THE MAIN LINE CODE.  THIS ROUTINE WILL TEST FOR   * 01806000
*/*                 AN ID (IN THE NCSPL OR RDTE), TEST IF ON A CHAIN  * 01807000
*/*                 AND CALL CLFONLY TO LOGON ON THE ENTRY.  ALSO     * 01808000
*/*                 CALLS DECHAIN IF NECESSARY.  IF NO NAME IS PRESENT* 01809000
*/*                 NO PROCESSING OCCURS.                             * 01810000
*/********************************************************************* 01811000
*                                                                  1214 01812000
*CLFENTPR: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        1214 01813000
*                                                                  1214 01814000
CLFENTPR DS    0H                                                  1215 01815000
*  CLFENTRA=R14RTNAD;              /*SAVE RETURN ADDRESS */        1215 01816000
         ST    R14RTNAD,CLFENTRA                                   1215 01817000
*  RFY ISTDEVCH BASED(ADDR(RPRDEVCH));                             1216 01818000
*  IF (CLFWKPT2->RPRENTRY=RPRENTTR  &           /*TERM WITH     */ 1217 01819000
*      DEVSUBND='0'B) |     /*SCHEDUABLE SUBNODES*/                1217 01820000
*      CLFWKPT2->RPRENTRY=RPRENTCM |                  /*OR COMPONET*/   01821000
*      CLFWKPT2->RPRENTRY=RPRENTUT |                  /*OR UTERM*/ 1217 01822000
*      CLFWKPT2->RPRENTRY=RPRENTLU                  /*OR LOGICAL UNIT*/ 01823000
*     THEN DO;                        /*ONE OF THE ABOVE*/         1217 01824000
         CLI   RPRENTRY(CLFWKPT2),X'80'                            1217 01825000
         BNE   @GL00023                                            1217 01826000
         TM    DEVSUBND+44(CLFWKPT2),B'00010000'                   1217 01827000
         BZ    @RT01217                                            1217 01828000
@GL00023 CLI   RPRENTRY(CLFWKPT2),X'90'                            1217 01829000
         BE    @RT01217                                            1217 01830000
         CLI   RPRENTRY(CLFWKPT2),X'75'                            1217 01831000
         BE    @RT01217                                            1217 01832000
         CLI   RPRENTRY(CLFWKPT2),X'81'                            1217 01833000
         BNE   @RF01217                                            1217 01834000
@RT01217 DS    0H                                                  1218 01835000
*        IF CLFWKPT2->RPRENTRY=RPRENTLU       /*LOGICAL UNIT @D32CKKU*/ 01836000
*          THEN                                            /*@D32CKKU*/ 01837000
         CLI   RPRENTRY(CLFWKPT2),X'81'                            1219 01838000
         BNE   @RF01219                                            1219 01839000
*          /*SEGMENT(CKLOGMD)REQUEST*/DO;GOTO CKLOGMD;CKLOGMD#:END;     01840000
**VALIDATE LOGMODE   @D32CKKU*/                                    1220 01841000
         B     CKLOGMD                                             1221 01842000
*          ELSE;                                           /*@D32CKKU*/ 01843000
@RF01219 DS    0H                                                  1224 01844000
*           IF CLFLGMER = ON    /*ERROR SW ON                @D32CKKU*/ 01845000
*             THEN CLFLGMER = OFF;      /*RESET ERROR SW     @D32CKKU*/ 01846000
@RC01219 TM    CLFLGMER,B'00000001'                                1224 01847000
         BNO   @RF01224                                            1224 01848000
         NI    CLFLGMER,B'11111110'                                1225 01849000
*             ELSE DO;        /*ERROR  SW OFF                @D32CKKU*/ 01850000
         B     @RC01224                                            1226 01851000
@RF01224 DS    0H                                                  1227 01852000
*        IF NCSPLLID=CLFNOID &        /*LOGON ID PRESENT     @DM14341*/ 01853000
*           CLFWKPT2->RCPLGNAP=BLANKS /*NAME IN ENTRY        @DM14341*/ 01854000
*           THEN;                     /*NO ID AND NAME IN ENTRY      */ 01855000
         CLC   NCSPLLID(8,R5NCSPL),@CB03853                        1227 01856000
         BNE   @RF01227                                            1227 01857000
         CLC   RCPLGNAP(8,CLFWKPT2),@CC03851                       1227 01858000
