*    2 CLFWKPT5 PTR(31),          /* SAVE AREA FOR PTRS */         0504 00049000
*    2 CLFWKPT6 PTR(31),          /*USED IN DIT/DIVT/DOT RTN*/     0504 00050000
*    2   NEWNCSPL PTR(31),       /* BASE FOR NEW NCSPL       @D32CKKU*/ 00051000
*    2 FNDSP CHAR(LENGTH(ISTFNDSP)),                               0504 00052000
*    2 CWCSL CHAR(LENGTH(ISTCSL)),                                 0504 00053000
*    2 MSGTXT CHAR(16),           /*TEXT OF MESSAGES*/             0504 00054000
*    2 CLFCNT FIXED(8),           /*COUNT FOR LOOPS */             0504 00055000
*    2 N FIXED(8),                /*COUNT CONTROL FOR LOOPS*/      0504 00056000
*    2 CLFSWTCH BIT(8),           /*INTERNAL SWITCHES */           0504 00057000
*       3 CLFABORT BIT(1),        /*NO CONNECTION DONE */          0504 00058000
*       3 CLFNEWDV BIT(1),        /*NEW DEVICE SW */               0504 00059000
*       3 CLFPICSW BIT(1),        /* CONTROL FOR PICTYPE LOOP*/    0504 00060000
*       3 CLFOUTSW BIT(1),        /*USED TO INDICATE BYPASS IN CLFONLY* 00061000
