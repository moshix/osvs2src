*             IF RPRHDTYP=RPRENTRN & /* IS RDTE IN A 370X SEGMENT   */  00174000
*                (RPRENTRY=RPRENTTR | /* IS RDTE A TERMINAL         */  00175000
*                 RPRENTRY=RPRENTCM)  /* IS RDTE A COMPONENT @DM14121*/ 00176000
*         THEN                      /* GET LINE RDTE ADDR    @DM94180*/ 00177000
