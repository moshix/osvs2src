         LA    @14,0(,@14)                                         0207 00392000
         LR    COQR01,@14                                          0207 00393000
*                        GEN (POST (1));                           0208 00394000
         POST (1)                                                       00395000
*                        END;                                      0209 00396000
*            ELSE;  /* NOTIFICATION FLAG IS NOTHING */             0210 00397000
*                                                                  0210 00398000
@RF00205 DS    0H                                                  0211 00399000
*/* TPEPILOG   */                                                  0211 00400000
*   GEN (PRINT OFF);                                               0211 00401000
*                                                                  0211 00402000
@RC00194 DS    0H                                                  0211 00403000
@RC00136 DS    0H                                                  0211 00404000
         PRINT OFF                                                      00405000
*   DCL ISTAPC34 ENTRY BASED(ATCAP34) VALRG(*); /*RESET ROUTINE*/  0212 00406000
