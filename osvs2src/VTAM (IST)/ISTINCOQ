@RF00136 CLI   DLRPLRPF(COQR01),4                                  0194 00358000
         BNE   @RF00194                                            0194 00359000
*/*  TPQUE (COQR01,DLRPLRNF) NORM NSTR */                          0195 00360000
*                                                                  0195 00361000
*                                                                  0195 00362000
*/*                          CMZVER = OS/VS2                          * 00363000
*           DO;                                                    0195 00364000
*           RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;         0196 00365000
*           GPR00P=0;                                              0197 00366000
         SLR   GPR00P,GPR00P                                       0197 00367000
*           GPR15P=ADDR(DLRPLRNF); /*HOLD PAB PTR*/                0198 00368000
         LA    GPR15P,DLRPLRNF(,COQR01)                            0198 00369000
*           GPR04P=COQR01; /*WORK ELE PTR*/                        0199 00370000
         LR    GPR04P,COQR01                                       0199 00371000
*           GPR05P=GPR15P; /*PASS PAB PTR*/                        0200 00372000
         LR    GPR05P,GPR15P                                       0200 00373000
*           CALL ATCLCPTR->ATCAP37->ISTAPC37;                      0201 00374000
         L     @11,ATCLCPTR                                        0201 00375000
         L     @15,ATCAP37(,@11)                                   0201 00376000
         BALR  @14,@15                                             0201 00377000
*           GEN(NOPR  8)NOSETS NOREFS;                             0202 00378000
         NOPR  8                                                        00379000
*           RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;       0203 00380000
*           END; /**** END OF TPQUE GENERATION ****/             /*@VS4 00381000
*726*/                                                             0204 00382000
*        ELSE                                                      0205 00383000
*          IF DLRPLRPF = DLRPLECB  /* IS FLAG AN ECB?        */    0205 00384000
*            THEN      /* YES */                                   0205 00385000
         B     @RC00194                                            0205 00386000
@RF00194 CLI   DLRPLRPF(COQR01),3                                  0205 00387000
