         TITLE 'ISTINCPD                                               *00001000
                        '                                               00002000
ISTINCPD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCPD  78.100'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
         ST    @13,@SA00001+4                                      0001 00015000
         LA    @00,@SA00001                                        0001 00016000
         ST    @00,8(,@13)                                         0001 00017000
         LR    @13,@00                                             0001 00018000
*                                                                  0209 00019000
*    DO; /* LISATCVT CVTBASE(PAGEABLE,(ATCVTPTR)) */               0210 00020000
*      ATCVTPTR = ATCLCPTR; /*SET UP ATCVT POINTER*/               0211 00021000
         L     ATCVTPTR,ATCLCPTR                                   0211 00022000
*    END; /*LISATCVT*/                                             0212 00023000
*   CPDFLAG1='1'B;                  /* VARIABLE TXT EXISTS   @ZM32266*/ 00024000
         OI    CPDFLAG1,B'10000000'                                0213 00025000
*   CONFTPTR=ATCCONFT;              /* INIT. CONFTPTR                */ 00026000
         L     CONFTPTR,ATCCONFT(,ATCVTPTR)                        0214 00027000
*   IF CONSTOR=CPDON                /* IF CONSTOR IS ON THEN GO TO   */ 00028000
*     THEN                                                         0215 00029000
         TM    CONSTOR(CONFTPTR),B'00010000'                       0215 00030000
         BNO   @RF00215                                            0215 00031000
*     CALL CPDDELET;                /* STORAGE DELETE PROCEDURE      */ 00032000
         BAL   @14,CPDDELET                                        0216 00033000
*   ELSE                                                           0217 00034000
*     CALL CPDBUILD;                /* OTHERWISE GO TO STORAGE       */ 00035000
         B     @RC00215                                            0217 00036000
@RF00215 BAL   @14,CPDBUILD                                        0217 00037000
*                                                                  0218 00038000
*/*------------------------------------------------------------------*/ 00039000
*/* THIS PROCEDURE IS ENTERED AT VTAM TERMINATION TIME TO DELETE ALL */ 00040000
*/* VTAM BUFFER POOLS (EXCEPT SFBUF&LFBUF-VS2 DELETED BY ISTRAMA2    */ 00041000
*/* THE MPLT CHAIN MUST BE FREED                             @ZA29244*/ 00042000
*/*------------------------------------------------------------------*/ 00043000
*                                                                  0218 00044000
*CPDDELET:                                                         0218 00045000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0218 00046000
         B     @PB00002                                            0218 00047000
CPDDELET DS    0H                                                  0219 00048000
*   DCL                                                            0219 00049000
*     CPDSAVE1 FIXED(31);           /* CALLER REG. 14 SAVE AREA      */ 00050000
*   CPDSAVE1=CPDR14;                /* SAVE CALLER RETURN ADDRESS    */ 00051000
         ST    CPDR14,CPDSAVE1                                     0220 00052000
*   IF CONSPEX=CPDON                /* IF CONSPEX IS ON THEN         */ 00053000
*     THEN                                                         0221 00054000
         TM    CONSPEX(CONFTPTR),B'01000000'                       0221 00055000
         BNO   @RF00221                                            0221 00056000
*     DO;                           /* DO AS FOLLOWS                 */ 00057000
*       SBFPTR=ADDR(CONSPSBF);      /* SP BUF POOL ADDR              */ 00058000
         LA    SBFPTR,CONSPSBF(,CONFTPTR)                          0223 00059000
*       CALL CPDDP;                 /* TO DELETE BUF POOL            */ 00060000
         BAL   @14,CPDDP                                           0224 00061000
*       CONSPEX=CPDOFF;             /* TURN CONSPEX OFF              */ 00062000
         NI    CONSPEX(CONFTPTR),B'10111111'                       0225 00063000
*     END;                                                         0226 00064000
*   ELSE                                                           0227 00065000
*     ;                             /* ELSE NO-OP                    */ 00066000
@RF00221 DS    0H                                                  0228 00067000
*/*  7 LINES OF CODE DELETED BY APAR OZ29244                  @ZA29244  00068000
*                                                                    */ 00069000
*                                                                  0228 00070000
*   IF CONLPEX=CPDON                /* IF CONLPEX IS ON              */ 00071000
*     THEN                                                         0228 00072000
         TM    CONLPEX(CONFTPTR),B'00010000'                       0228 00073000
         BNO   @RF00228                                            0228 00074000
*     DO;                           /* THEN DO FOLLOWS               */ 00075000
*       SBFPTR=ADDR(CONLPSBF);      /* LP BUF POOL ADDR              */ 00076000
         LA    SBFPTR,CONLPSBF(,CONFTPTR)                          0230 00077000
*       CALL CPDDP;                 /* TO DELETE BUF POOL            */ 00078000
         BAL   @14,CPDDP                                           0231 00079000
*       CONLPEX=CPDOFF;             /* TURN CONLPEX FLAG OFF         */ 00080000
         NI    CONLPEX(CONFTPTR),B'11101111'                       0232 00081000
*     END;                                                         0233 00082000
*   ELSE                                                           0234 00083000
*     ;                             /* ELSE, NO-OP                   */ 00084000
@RF00228 DS    0H                                                  0235 00085000
