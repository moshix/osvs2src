         TITLE 'ISTINCR1 - RESTART CONT                                *00001000
                        '                                               00002000
ISTINCR1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCR1  75.171'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*/********************************************************************/ 00014000
*/*  ROUTINE NAME: MAIN LINE                                         */ 00015000
*/*  FUNCTION:                                                       */ 00016000
*/*            MAINLINE DETERMINES IF THE GIVEN RDTE IS FOR A 370X OR*/ 00017000
*/*            A LU CLUSTER. IF IT IS FOR A 370X, ISTINCR2 IS CALLED */ 00018000
*/*            TO ISSUE A SET TIME AND DATE AND DOLNKACT IS INCLUDED */ 00019000
*/*            TO RESTART LINKS. IF FOR A CLUSTER, DOLUACT IS        */ 00020000
*/*            INCLUDED TO RESTART LOGICAL UNITS. A MESSAGE IS       */ 00021000
*/*            ISSUED TO SAY IF THE RESTART IS SUCCESSFUL OR NOT     */ 00022000
*/*  SEGMENTS INCLUDED: DOLNKACT,DODVACT                             */ 00023000
*/*  ROUTINES CALLED: ISTINCR2                                       */ 00024000
*/*  MACROS USED: TPMSG                                              */ 00025000
*/*               CRRMAINT                                           */ 00026000
*/********************************************************************/ 00027000
*                                                                  0394 00028000
*  DO;                                                             0394 00029000
*/***  CRRMAINT  PROCNM(ISTINCR1)COMP(VY)FUNC(SUB)RPHADD(ISTRPH)  ***/  00030000
*                                                                  0395 00031000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0395 00032000
*/* LOGICAL CHANNEL COMMAND WORD                                      * 00033000
*/* ADD A DECLARE                                      003 10/18/74   * 00034000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0395 00035000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 00036000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 00037000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 00038000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 00039000
*              4  * BIT(1),            /* RESERVED                    * 00040000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0395 00041000
*                                         ERASE OR READ CONT          * 00042000
*              4  LCCWCTL BIT(4),      /* CONTROL TYPE                * 00043000
*                 5  LCCWMODE BIT(2),  /* MODE TYPE                   * 00044000
*                 5  LCCWTYPE BIT(2),  /* OPERATION TYPE              * 00045000
*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00046000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00047000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00048000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00049000
*                                         RECORD                      * 00050000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00051000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00052000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00053000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00054000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00055000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00056000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00057000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00058000
*/* LOGICAL COMMAND CODES */                                       0396 00059000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00060000
