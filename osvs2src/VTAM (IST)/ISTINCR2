*            CALL CR2INCIO;            /*DO I/O              @D32CKKG*/ 00361000
*                                                                  0239 00362000
         BAL   @14,CR2INCIO                                        0239 00363000
*            IF NCSIORC^=NCSIOSUC      /*IF I/O UNSUCC*/           0240 00364000
*             THEN                                                 0240 00365000
         CLI   NCSIORC(NCSPLPTR),X'00'                             0240 00366000
         BE    @RF00240                                            0240 00367000
*              DO;                                                 0241 00368000
*               MSGIDPTR = ADDR(MSG646I); /*DEV TRANS LIMIT FAILED*/    00369000
         LA    MSGIDPTR,@CC00830                                   0242 00370000
*               CALL DOMSG;            /*ISSUE MSG*/               0243 00371000
         BAL   @14,DOMSG                                           0243 00372000
*              END;                                                0244 00373000
*             ELSE;                                                0245 00374000
@RF00240 DS    0H                                                  0246 00375000
*           END;                                                   0246 00376000
*          ELSE                                                    0247 00377000
*           DO;                                                    0247 00378000
         B     @RC00230                                            0247 00379000
@RF00230 DS    0H                                                  0248 00380000
*/********************************************************************/ 00381000
*/*  SEGMENT NAME - DOLINK (CALLED BY MAINLINE)                      */ 00382000
*/*  FUNCTION:                                                       */ 00383000
*/*       THIS SEGMENT ISSUES COMMANDS VIA ISTINCIO IN ORDER TO DO   */ 00384000
*/*       THE FOLLOWING:                                             */ 00385000
*/*         1 - SWITCH THE LINK TO EP OR NCP                         */ 00386000
*/*         2 - ACTIVATE THE LINK                                    */ 00387000
*/*         3 - CHANGE THE FOLLOWING LINE SCHED PARMS                */ 00388000
*/*           A - NEG POLL LIMIT                                     */ 00389000
*/*           B - SERV SEEKING PAUSE                                 */ 00390000
*/*           C - SESSION LIMIT                                      */ 00391000
*/*       THE RET CODE IN NCSIORC IS CHECKED. IF NOT SUCCESSFUL, THE */ 00392000
*/*       THE APPROPIATE MSG IS POINTED TO AND DOMSG IS CALLED TO    */ 00393000
*/*       ISSUE THE MESSAGE                                          */ 00394000
*/********************************************************************/ 00395000
*                                                                  0248 00396000
*/********************************************************************/ 00397000
*/*  THIS SECTION DETERMINES IF THE LINK IS TO BE SWITCHED TO THE    */ 00398000
*/*  NCP OR THE EP IF EITHER.                                        */ 00399000
*/********************************************************************/ 00400000
*IF NCSFMERP = '1'B                  /*IF ACTPU RESP ERP     @X50BHKG*/ 00401000
* THEN;                              /* SKIP SW TO EP        @X50BHKG*/ 00402000
         TM    NCSFMERP(NCSPLPTR),B'00010000'                      0248 00403000
         BO    @RT00248                                            0248 00404000
* ELSE                               /* DO SW TO EP          @X50BHKG*/ 00405000
*IF (RLNRBUSE=COFF & RLNSWNCP=CON) | /*IF INITIALLY EP & NOW NCP*/ 0250 00406000
*  (RLNRBUSE=CON & RLNSWEP=CON)      /*OR IF INITIALLY NCP & NOW EP*/   00407000
* THEN                                                             0250 00408000
         L     @03,NCSPLRCR(,NCSPLPTR)                             0250 00409000
         TM    RLNRBUSE(@03),B'10000000'                           0250 00410000
         BNZ   @GL00001                                            0250 00411000
         TM    RLNSWNCP(@03),B'01000000'                           0250 00412000
         BO    @RT00250                                            0250 00413000
@GL00001 L     @03,NCSPLRCR(,NCSPLPTR)                             0250 00414000
         TM    RLNRBUSE(@03),B'10000000'                           0250 00415000
         BNO   @RF00250                                            0250 00416000
         TM    RLNSWEP(@03),B'00000100'                            0250 00417000
         BNO   @RF00250                                            0250 00418000
@RT00250 DS    0H                                                  0251 00419000
*  DO;                                                             0251 00420000
*   IF RLNPORT = CON                  /*DAF OF LINE INTO RU  @XA12208*/ 00421000
*     THEN CSRUDATA = RPTLNDAF;                            /*@XA12208*/ 00422000
