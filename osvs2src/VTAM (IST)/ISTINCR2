         MVI   SRUQSREQ(@03),X'51'                                 0259 00445000
*     END;                                                         0260 00446000
*    ELSE                                                          0261 00447000
*     DO;                                                          0261 00448000
         B     @RC00256                                            0261 00449000
@RF00256 DS    0H                                                  0262 00450000
*      NCSPLCMD = NCSCSEP;              /*SET CMD IN NCSPL & RU*/  0262 00451000
         MVC   NCSPLCMD(2,NCSPLPTR),@CB02181                       0262 00452000
*      SRUQSREQ = '52'X;              /*TO SWITCH TO EP*/          0263 00453000
         L     @03,NCSPLRUO(,NCSPLPTR)                             0263 00454000
         MVI   SRUQSREQ(@03),X'52'                                 0263 00455000
*     END;                                                         0264 00456000
*   CALL CR2INCIO;                     /*DO I/O              @D32CKKG*/ 00457000
*                                                                  0265 00458000
@RC00256 BAL   @14,CR2INCIO                                        0265 00459000
*   IF NCSIORC^=NCSIOSUC               /*IF I/O UNSUCC*/           0266 00460000
*    THEN                                                          0266 00461000
         CLI   NCSIORC(NCSPLPTR),X'00'                             0266 00462000
