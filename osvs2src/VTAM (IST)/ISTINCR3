*CR3FMERP:PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0497 00288000
CR3FMERP DS    0H                                                  0498 00289000
*/* THIS ROUTINE WILL PROCESS AN RN RDT SEGMENT WHEN AN*/          0498 00290000
*/*   FME(ERP) RESPONSE IS RECEIVED TO THE ACTPU.  IF THE CALL TO */    00291000
*/*   CR3 IS FOR ACTIVATE PROCESSING, THIS SECTION WILL ISSUE DEACT.*/  00292000
*/*   LINK RU'S TO ANY LINE WHOSE ISTATUS FLAG IS NOT SET.  IF THE  */  00293000
*/*   COMMAND IS COLD, LINE SCHEDULING PARAMETERS ARE REISSUED AS   */  00294000
*/*   NEEDED.  IF THE CALL WAS FOR ERP PROCESSING, DEACTIVATE LINK  */  00295000
*/*   RU'S ARE ISSUED TO ALL LINES WHOSE ONLINE STATUS IS           */  00296000
*/*   'DEACTIVATED'                                                 */  00297000
*/*   NOTE THAT ALL LINE SCHEDULING PARAMETERS APPLY ONLY TO NON-   */  00298000
*/*   SWITCHED, POLLED, BASIC LINES OR THE TERMINALS OR COMPONENTS  */  00299000
*/*   ON SUCH LINES.                                                */  00300000
*/*                                                                 */  00301000
*                                                                  0498 00302000
* CR3RSERP=CR3R14;        /*SAVE RETURN ADDRESS ACROSS CALL          */ 00303000
         ST    CR3R14,CR3RSERP                                     0498 00304000
* IF NCSPLVCD=NCSPLVC1                                             0499 00305000
*  THEN                   /*PREPARE TO PROCESS FOR ACT CMD.          */ 00306000
         CLI   NCSPLVCD(NCSPLSAV),X'01'                            0499 00307000
         BNE   @RF00499                                            0499 00308000
*   DO WHILE (CR3RDTE^=NCSPLRPT) & NCSIORC < NCSIOPRG;             0500 00309000
         B     @DE00500                                            0500 00310000
@DL00500 DS    0H                                                  0501 00311000
*   NCSIORC=NCSIOSUC;     /*INITIALIZE R.C. FOR EXT. CALLS*/       0501 00312000
         MVI   NCSIORC(NCSPLSAV),X'00'                             0501 00313000
*   IF (RPRENTRY=RPRENTLN)                                         0502 00314000
*    THEN                                                          0502 00315000
         CLI   RPRENTRY(CR3RDTE),X'50'                             0502 00316000
         BNE   @RF00502                                            0502 00317000
*     DO;                                                          0503 00318000
*      IF (RLNRBISA^=CR3ON)                                        0504 00319000
*       THEN                                                       0504 00320000
         TM    RLNRBISA(CR3RDTE),B'01000000'                       0504 00321000
         BO    @RF00504                                            0504 00322000
*        DO;     /*DEACT THE LINE SINCE ISTATUS IS OFF               */ 00323000
*          CALL CR3DEACT;                                          0506 00324000
         BAL   @14,CR3DEACT                                        0506 00325000
*        END;                                                      0507 00326000
*       ELSE;                                                      0508 00327000
*                                                                  0508 00328000
*                                                                  0508 00329000
@RF00504 DS    0H                                                  0509 00330000
*      IF NCSPLWRM^=CR3ON & NCSIORC < NCSIOPRG                     0509 00331000
*                    /*IF THE COMMAND IS ACT(COLD) AND NO ERROR*/  0509 00332000
*      & RLNRACAT=CR3OFF                                   /*@X50BHF9*/ 00333000
*      & RLNRACAN=CR3OFF  /*LINE MUST BE NON-SWITCHED,BASIC  @X50BHF9*/ 00334000
*      & RLNRAPOL=CR3ON   /*AND POLLED TO HAVE L.S.P.'S      @XM18659*/ 00335000
*      & RLNRATRN=CR3OFF                                   /*@X50BHF9*/ 00336000
*         THEN DO;                                                 0509 00337000
         TM    NCSPLWRM(NCSPLSAV),B'10000000'                      0509 00338000
         BO    @RF00509                                            0509 00339000
         CLI   NCSIORC(NCSPLSAV),X'08'                             0509 00340000
         BNL   @RF00509                                            0509 00341000
         TM    RLNRACAT(CR3RDTE),B'11000000'                       0509 00342000
         BNZ   @RF00509                                            0509 00343000
         TM    RLNRAPOL(CR3RDTE),B'00000100'                       0509 00344000
         BNO   @RF00509                                            0509 00345000
         TM    RLNRATRN(CR3RDTE),B'00000010'                       0509 00346000
         BNZ   @RF00509                                            0509 00347000
*          NCSIORC=NCSIOSUC;   /*INSURE ZERO RC FOR CALL TO CR2*/  0511 00348000
         MVI   NCSIORC(NCSPLSAV),X'00'                             0511 00349000
*          CALL CR3LSP;    /*REISSUE L.S.P. AS NECESSARY             */ 00350000
         BAL   @14,CR3LSP                                          0512 00351000
*          END;                                                    0513 00352000
*         ELSE;                                                    0514 00353000
@RF00509 DS    0H                                                  0515 00354000
*      END;               /* LINE IS PROCESSED.                      */ 00355000
*                                                                  0515 00356000
*    ELSE                                                          0516 00357000
