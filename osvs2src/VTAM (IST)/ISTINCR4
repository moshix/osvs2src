*        END; /*LISATCVT*/              /*PUT POINTER TO ATCVT IN */    02419000
*                                       /*  IN REG 15             */    02420000
*          CALL GPR15P->ATCAP32;        /* CALL ISTAPC32: TPSCHED */    02421000
         L     @09,ATCAP32(,GPR15P)                                0917 02422000
         LR    @15,@09                                             0917 02423000
         BALR  @14,@15                                             0917 02424000
*FMZT0091:                                                         0918 02425000
*        DO; /*MODESET  KEYADDR(GPR06F) WORKREG(GPR05F)*/          0918 02426000
FMZT0091 DS    0H                                                  0919 02427000
*          GPR05F = GPR06F; /*GET KEY FORM SAVE LOCATION*/         0919 02428000
         LR    GPR05F,GPR06F                                       0919 02429000
*          SPKA (GPR05F); /*SET PSW KEY*/                          0920 02430000
         SPKA  0(GPR05F)                                           0920 02431000
*        END; /*MODESET  KEYADDR(GPR06F) WORKREG(GPR05F)*/         0921 02432000
*          RESPECIFY (GPR00F,GPR01F,GPR04F,GPR05F,GPR06F,          0922 02433000
*          GPR14F,GPR15F) UNRSTD;       /*UNRESTRICT REGS 0,1,    */    02434000
*                                       /*4,5,14, AND 15          */    02435000
*        END;  /****END OF TPPOST GENERATION****/               /*TPPOS 02436000
* THE RPH*/                                                        0923 02437000
*RETURN;                                /*RETURN             @ZA10991*/ 02438000
*                                                                  0924 02439000
@EL00001 DS    0H                                                  0924 02440000
@EF00001 DS    0H                                                  0924 02441000
