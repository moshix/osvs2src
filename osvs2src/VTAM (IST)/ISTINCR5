*                                     /*AVAILABLE BYTE IN VARY WORK  */ 03203000
*                                     /*AREA TO DOUBLE WORD BOUNDRY  */ 03204000
*      NCSCRWKA=VWANAV;               /*POINT TO WKA FOR ISTINCR4    */ 03205000
         ST    @04,NCSCRWKA(,R10)                                  0490 03206000
*      VWANAV=VWANAV+                 /*POINT TO NEXT VARY WORK AREA */ 03207000
*             LENGTH(ISTCR4WA);                                    0491 03208000
