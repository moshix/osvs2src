         BALR  @14,@15                                             0136 02184000
*    IF CR5MSGID^=CR5NOMSG                /*IF SUBROUTINE SET MSGID  */ 02185000
*      THEN DO;                           /*THEN SEND MESSAGE        */ 02186000
         CLC   CR5MSGID(4),@CC01103                                0137 02187000
         BE    @RF00137                                            0137 02188000
*                                         /*NOTE THAT THE MESSAGE IS */ 02189000
*                                         /*AGAINST A SEGMENT ONLY   */ 02190000
*        CR5MSGTX(1:8)=RPRNAME;           /*SEGMENT NAME             */ 02191000
         MVC   CR5MSGTX(8),RPRNAME(CR5RDTE)                        0139 02192000
*        CR5MSGTX(9:16)=RDTCRSDS;         /*CHECKPOINT DATA SET NAME */ 02193000
         MVC   CR5MSGTX+8(8),RDTCRSDS(CR5RDTE)                     0140 02194000
*                                                                  0141 02195000
*         DO; /* CPMSG MSGID(CR5MSGID)MSG(CR5MSGTX)OPTIONS(NCSPLSID) */ 02196000
*                                                                  0141 02197000
*/* ATBUILD (ISTDLRPL,ISTATCVT)  */                                0142 02198000
