SVTM     TITLE 'ISTINCR6-CONFIGURATION RESTART VSAM IO                 *00001000
                        '                                               00002000
ISTINCR6 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCR6  76.278'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         EJECT                                                          00014000
*/*-----------------------------------------------------------     0121 00015000
*    EXECTUABLE CODE -MAIN ROUTINE                                 0121 00016000
*---------------------------------------------------------------*/ 0121 00017000
*/*    CHAIN SAVEAREAS */                                          0121 00018000
*                                                                  0121 00019000
* DO; /* BLOCK MAIN */                                             0121 00020000
*/********************************************************************/ 00021000
*/*                                                                  */ 00022000
*/*   MACRO NAME = IDAAMB                                            */ 00023000
*/*                                                                  */ 00024000
*/*   DESCRIPTIVE NAME =ACCESS METHOD BLOCK                          */ 00025000
*/*                                                                  */ 00026000
*/*   FUNCTION = DESCRIBES DATA SET ATTRIBUTES AND SUPPLIES          */ 00027000
*/*              IOS COMPATIBLE FIELDS FOR I/O                       */ 00028000
*/*                                                                  */ 00029000
*/*   CONTROL BLOCK STRUCTURE = THE IDAAMB IS POINTED TO BY THE      */ 00030000
*/*              IDAAMBL                                             */ 00031000
*/*                                                                  */ 00032000
*/*   INCLUDED MACROS = NONE                                         */ 00033000
*/*                                                                  */ 00034000
*/*   METHOD OF ACCESS = PL/S - NO DECLARES NECESSARY                */ 00035000
*/*                                                                  */ 00036000
*/*   CREATED BY = VSAM OPEN                                         */ 00037000
*/*                                                                  */ 00038000
*/*   STATUS = VS/1 RELEASE 4  (CHANGE FLAG @X04SVXX)                */ 00039000
*/*               VS/2 RELEASE 3  (CHANGE FLAG @Y30SDXX)             */ 00040000
*/*                                                                  */ 00041000
*/*   DATE OF LAST CHANGE = 8 AUG 74                                 */ 00042000
*/*                                                                  */ 00043000
*/********************************************************************/ 00044000
