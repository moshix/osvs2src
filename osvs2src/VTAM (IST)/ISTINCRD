@EL00002 DS    0H                                                  0431 00555000
@EF00002 DS    0H                                                  0431 00556000
@ER00002 BR    @14                                                 0431 00557000
*PARMSCAN:                                                         0432 00558000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0432 00559000
*                                                                  0432 00560000
PARMSCAN DS    0H                                                  0433 00561000
*/********************************************************************  00562000
*                                                                    */ 00563000
*/*   THE PARMSCAN SUBROUTINE IS CALLED TO SCAN THE PARAMETERS OF THE*/ 00564000
*/* RECORD. ALL IMBEDDED BLANKS, NOT INSIDE A QUOTE, ARE TREATED AS  */ 00565000
*/* EXTRANEOUS & NOT MOVED TO THE OUTPUT BUFFER.                     */ 00566000
*/*   CHK72 IS CALLED AT THE END OF THE CURRENT RECORD TO DETERMINE  */ 00567000
*/* IF THE PARAMETERS ARE CONTINUED ON ANOTHER CARD.                 */ 00568000
*/********************************************************************  00569000
*                                                                    */ 00570000
*/* THE FOLLOWING DECLARES ARE LOCAL TO PARMSCAN                     */ 00571000
*                                                                  0433 00572000
*   DCL                                                            0433 00573000
*     PARMSAVE;                     /* SAVE VAR. FOR REG 14          */ 00574000
*   DCL                                                            0434 00575000
*     FUNCBYTE CHAR(1) BASED(STOPPTR);/* BYTE WHERE TRT STOPS        */ 00576000
*   DCL                                                            0435 00577000
*     QUOTECHR CHAR(1) INIT('7D'X); /* QUOTE FOR QUOTE CHECK         */ 00578000
*   DCL                                                            0436 00579000
*     1 PARMFLGS CHAR(1),                                          0436 00580000
*      2 * BIT(1),                                                 0436 00581000
*      2 CC0FLAG BIT(1),            /* FLAG INDICATING C.C=0         */ 00582000
*      2 * BIT(6);                                                 0436 00583000
*   DCL                                                            0437 00584000
*     CONS7 CONSTANT(7);            /* USED FOR C.C. CHECK           */ 00585000
*   DCL                                                            0438 00586000
*     PREVBYTE CHAR(1) BASED(STOPPTR-CONS1);/* BYTE BEFORE TRT STOPS */ 00587000
*   DCL                                                            0439 00588000
*     CHARCNT;                      /* USED TO CALCULATE # CHARS   0439 00589000
*                                      SCANNED                       */ 00590000
*   DCL                                                            0440 00591000
*     CONS13 CONSTANT(13);          /* USE FOR C.C. CHECK            */ 00592000
*   DCL                                                            0441 00593000
*     1 PARMTAB CHAR(256),          /* TRANSLATE & TEST TABLE FOR  0441 00594000
*                                      PARMSCAN                      */ 00595000
*      2 * CHAR(64),                                               0441 00596000
*       3 *(64) CHAR(1) INIT((64)'00'X),                           0441 00597000
*      2 BLANKSTP CHAR(1) INIT('04'X),/* BLANK STOP                  */ 00598000
*      2 * CHAR(60),                                               0441 00599000
*       3 *(60) CHAR(1) INIT((60)'00'X),                           0441 00600000
*      2 QUOTESTP CHAR(1) INIT('08'X),/* QUOTE STOP                  */ 00601000
*      2 * CHAR(130),                                              0441 00602000
*       3 *(130) CHAR(1) INIT((130)'00'X);                         0441 00603000
*                                                                  0441 00604000
*/* THE FOLLOWING TRT WILL STOP ON A BLANK OR QUOTE                  */ 00605000
*                                                                  0442 00606000
*   GEN DATA REFS(CURRENTP,PARMTAB);                               0442 00607000
*   PARMSAVE=R14;                   /* SAVE REG 14                   */ 00608000
         ST    R14,PARMSAVE                                        0443 00609000
*   CRDENDSP=CRDOFF;                /* TURN OFF ALL LOOP CONTROL   0444 00610000
*                                      FLAGS                         */ 00611000
*   CRDENDST=CRDOFF;                                               0445 00612000
*   CRDENTRT=CRDOFF;                                               0446 00613000
