         SLR   TRTREG,CURRENTP                                     0619 01101000
         BCTR  TRTREG,0                                            0619 01102000
*   GEN(EX   TRTREG,MOVETRT);       /* EXECUTE TRT TO GET BLANK      */ 01103000
         EX   TRTREG,MOVETRT                                            01104000
*   STOPPTR=R1;                     /* SAVE STOPPTR                  */ 01105000
