*STNB:                                                             0658 01194000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0658 01195000
*                                                                  0658 01196000
STNB     DS    0H                                                  0659 01197000
*/********************************************************************  01198000
*                                                                    */ 01199000
*/*   THE SCAN TO NON BLANKS SUBROUTINE SCANS FOR A NON BLANK CHAR-  */ 01200000
*/* ACTER. IF FOUND, THE CURRENT PTR TO THE NEXT BYTE IN THE INPUT   */ 01201000
*/* AREA IS UPDATED.                                                 */ 01202000
*/*   IF NO NON BLANK CHARACTER IS FOUND & THIS IS NOT THE FIRST CARD*/ 01203000
*/* A NON ZERO R.C. IS PASSED BACK. IF THIS IS THE FIRST CARD, CHK72 */ 01204000
*/* IS CALLED TO PROCESS CONTINUATION.                               */ 01205000
*/********************************************************************  01206000
*                                                                    */ 01207000
*/* THE FOLLOWING DECLARES ARE LOCAL TO STNB SUBRTN                  */ 01208000
*                                                                  0659 01209000
*   DCL                                                            0659 01210000
*     STNBSAVE;                     /* SAVE VAR. FOR REG 14          */ 01211000
*   DCL                                                            0660 01212000
*     BLNKCONS FIXED(31) INIT('40000000'X);/* PAD CHAR FOR CLCL      */ 01213000
*   STNBSAVE=R14;                   /* SAVE REG 14                   */ 01214000
         ST    R14,STNBSAVE                                        0661 01215000
*   R5=ADDR(INBUF)+ENDCARD-CURRENTP;/* SET UP REG5 FOR CLCL          */ 01216000
         LA    R5,INBUF                                            0662 01217000
         AL    R5,@CF02360                                         0662 01218000
         SLR   R5,CURRENTP                                         0662 01219000
*   R3=BLNKCONS;                    /* SET UP REG 3 FOR BLANK SCAN   */ 01220000
         L     R3,BLNKCONS                                         0663 01221000
*   GEN SETS(CURRENTP) REFS(R2,CODEOK);                            0664 01222000
         CLCL  CURRENTP,R2          SCAN LOOKING FOR NON BLANK          01223000
         BNZ   CODEOK               IF NOT FOUND, BRANCH AROUND 12 R.C. 01224000
*   IF NOPARMS=CRDOFF THEN                                         0665 01225000
         TM    NOPARMS,B'00001000'                                 0665 01226000
         BNZ   @RF00665                                            0665 01227000
*     RCREG=RETURN12;               /* CHECK IF 1ST CALL WITH NO   0666 01228000
*                                      PARMS                         */ 01229000
         LA    RCREG,12                                            0666 01230000
*   ELSE                                                           0667 01231000
*     CALL CHK72;                   /* IF 1ST CALL SEE IF CONT.      */ 01232000
         B     @RC00665                                            0667 01233000
@RF00665 BAL   @14,CHK72                                           0667 01234000
*CODEOK:                                                           0668 01235000
*   ;                                                              0668 01236000
@RC00665 DS    0H                                                  0668 01237000
CODEOK   DS    0H                                                  0669 01238000
*   R14=STNBSAVE;                   /* RESTORE REG 14                */ 01239000
         L     R14,STNBSAVE                                        0669 01240000
*   END STNB;                                                      0670 01241000
@EL00006 DS    0H                                                  0670 01242000
@EF00006 DS    0H                                                  0670 01243000
@ER00006 BR    @14                                                 0670 01244000
*CHK72:                                                            0671 01245000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0671 01246000
*                                                                  0671 01247000
CHK72    DS    0H                                                  0672 01248000
*/********************************************************************  01249000
*                                                                    */ 01250000
*/*   THE CHK72 SUBROUTINE PROCESSES COLUMN 72 TO DETERMINE IF A     */ 01251000
*/* CONTINUATION HAS BEEN INDICATED. IF COL. 72 IS NON BLANK, THE    */ 01252000
*/* CONTINUE SUBROUTINE IS CALLED TO PREPARE ANOTHER CARD FOR SCAN   */ 01253000
*/*   IF COL. 72 IS BLANK & QUOTES ARE PAIRED, REG. 15 IS SET TO 0   */ 01254000
*/* AND THE END OF STRING INDICATOR TURNED ON. IF QUOTES ARE NOT     */ 01255000
*/* PAIRED, A R.C. OF 12 IS SET.                                     */ 01256000
*/********************************************************************  01257000
*                                                                    */ 01258000
*                                                                  0672 01259000
*   DCL                                                            0672 01260000
*     CHKSAVE FIXED(31);            /* SAVE VAR. FOR REG 14          */ 01261000
*   CHKSAVE=R14;                    /* SAVE REG 14                   */ 01262000
         ST    R14,CHKSAVE                                         0673 01263000
*   IF COL72=BLANK THEN             /* CHECK FOR CONTINUATION        */ 01264000
         LA    @11,INBUF                                           0674 01265000
         CLI   COL72+71(@11),C' '                                  0674 01266000
         BNE   @RF00674                                            0674 01267000
*     DO;                                                          0675 01268000
*       IF QUOTECNT//2=CRDCLEAR THEN/* CHECK IF QUOTES PAIRED        */ 01269000
         SLR   @11,@11                                             0676 01270000
         L     @06,QUOTECNT                                        0676 01271000
         SRDA  @06,32                                              0676 01272000
         D     @06,@CF00073                                        0676 01273000
         CR    @06,@11                                             0676 01274000
         BNE   @RF00676                                            0676 01275000
*         DO;                                                      0677 01276000
*           RCREG=CRDCLEAR;                                        0678 01277000
         LR    RCREG,@11                                           0678 01278000
*           CRDENDST=CRDON;         /* GET OUT OF PARMSCAN LOOP      */ 01279000
         OI    CRDENDST,B'01000000'                                0679 01280000
*         END;                                                     0680 01281000
*       ELSE                                                       0681 01282000
*         RCREG=RETURN12;           /* ELSE SET 12 R.C.              */ 01283000
         B     @RC00676                                            0681 01284000
@RF00676 LA    RCREG,12                                            0681 01285000
*     END;                                                         0682 01286000
*   ELSE                                                           0683 01287000
*     DO;                                                          0683 01288000
         B     @RC00674                                            0683 01289000
@RF00674 DS    0H                                                  0684 01290000
*       CALL CONTINUE;              /* CALL CONTINUE TO READ NEXT  0684 01291000
*                                      CARD                          */ 01292000
         BAL   @14,CONTINUE                                        0684 01293000
*       CRDENTRT=CRDOFF;            /* STAY IN TRT LOOP              */ 01294000
         NI    CRDENTRT,B'11011111'                                0685 01295000
*     END;                                                         0686 01296000
*   R14=CHKSAVE;                    /* RESTORE REG 14                */ 01297000
@RC00674 L     R14,CHKSAVE                                         0687 01298000
*   END CHK72;                                                     0688 01299000
@EL00007 DS    0H                                                  0688 01300000
@EF00007 DS    0H                                                  0688 01301000
@ER00007 BR    @14                                                 0688 01302000
*CONTINUE:                                                         0689 01303000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0689 01304000
*                                                                  0689 01305000
CONTINUE DS    0H                                                  0690 01306000
*/********************************************************************  01307000
*                                                                    */ 01308000
*/*   THE CONTINUE SUBROUTINE HANDLES PROCESSING OF CONTINUATION     */ 01309000
*/* CARDS FOR CURRENT PARAMETERS BEING PROCESSED.                    */ 01310000
*/*   IT FIRST CHECKS TO DETERMINE IF THE REMAINING SPACE IN THE OUT-*/ 01311000
*/* BUFFER IS LARGE ENOUGH TO ACCOMODATE ANOTHER RECORD. IF NOT, A   */ 01312000
*/* VGTMAIN & SUBSEQUENT VFRMAIN ARE DONE TO GET ANOTHER OUTPUT BUF. */ 01313000
*/*   GOREAD IS CALLED TO READ IN THE CONTINUATION CARD.             */ 01314000
*/*   IF THE CONTINUATION CARD IS A CONTINUATION FOR AN ASSEMBLER    */ 01315000
*/* LANGUAGE MACRO THEN COLUMNS 1-15 MUST BE BLANK. IF NOT BLANK, A  */ 01316000
*/* NON ZERO R.C. IS PASSED BACK. IF COL. 16 IS BLANK, THERE ARE NO  */ 01317000
*/* MORE PARAMETERS.                                                 */ 01318000
*/*   FOR START PARAMETERS OR CONFIGURATION PARAMETERS, STNB IS      */ 01319000
*/* CALLED TO GET THE ADDRESS OF THE NEXT PARAMETER.                 */ 01320000
*/********************************************************************  01321000
*                                                                    */ 01322000
*                                                                  0690 01323000
*   DCL                                                            0690 01324000
*     CONTSAVE FIXED(31);           /* SAVE VAR. FOR REG 14          */ 01325000
*/* THE FOLLOWING DECLARES ARE LOCAL TO SUBRTN CONTINUE              */ 01326000
*                                                                  0691 01327000
*   DCL                                                            0691 01328000
*     COLS15 CHAR(15) BASED(ADDR(INBUF));/* USED TO CHECK COLS.1-15  */ 01329000
*   DCL                                                            0692 01330000
*     CONS15 CONSTANT(15);                                         0692 01331000
*   DCL                                                            0693 01332000
*     FIRST15 CONSTANT(15);         /* USED TO REFERENCE 1ST 15 CHARS   01333000
*                                      IN CARD                       */ 01334000
*   DCL                                                            0694 01335000
*     BLANKS15 CHAR(15) CONSTANT('               ');/* CHECKS COLS.     01336000
*                                      1-15 FOR BLANKS               */ 01337000
*   DCL                                                            0695 01338000
*     COL16 CHAR(1) BASED(ADDR(INBUF)+CONS15);/* USED TO CHECK COL 16   01339000
*                                      IN CARD                       */ 01340000
*   DCL                                                            0696 01341000
*     REMSPACE;                     /* REMAINING SPACE IN OUTPUT   0696 01342000
*                                      BUFFER                        */ 01343000
*   DCL                                                            0697 01344000
*     BUFENUFF CONSTANT(71);        /* USED TO CHECK IF 71 BYTES LEFT   01345000
*                                      IN OUTPUT BUFFER              */ 01346000
*   DCL                                                            0698 01347000
*     TEMPSIZE;                     /* USED FOR TEMPORARY AREA FOR 0698 01348000
*                                      BUFFER SIZE. IF GETSTOR FAILS    01349000
*                                      WE DON'T WANT TO UPDATE     0698 01350000
*                                      CONBUFSZ                      */ 01351000
*   DCL                                                            0699 01352000
*     TEMPBUFF;                     /* TEMP SAVE AREA FOR CONRDBUF   */ 01353000
*   DCL                                                            0700 01354000
*     NEXTSIZE;                     /* TEMP AREA FOR NEXT BUFFER SIZE*/ 01355000
*   DCL                                                            0701 01356000
*     R15SAVE;                      /* AREA TO SAVE REG 15 FROM    0701 01357000
*                                      FREESTOR                      */ 01358000
*   DCL                                                            0702 01359000
*     OLDADDR REG(6);               /* CONTAINS OLD OUTPUT AREA ADD. */ 01360000
*   DCL                                                            0703 01361000
*     NEWADDR REG(8);               /* CONTAINS NEW OUTPUT AREA ADD. */ 01362000
*   DCL                                                            0704 01363000
*     OLDCOUNT REG(7);              /* CONTAINS COUNT OF BYTES USED  */ 01364000
*   DCL                                                            0705 01365000
*     NEWCOUNT REG(9);              /* SAME COUNT AS OLD             */ 01366000
*   CONTSAVE=R14;                   /* SAVE REG 14                   */ 01367000
         ST    R14,CONTSAVE                                        0706 01368000
*   REMSPACE=CONRDBUF+CONBUFSZ-CRDOBPTR;/* COMPUTE REMAINING SPACE IN   01369000
