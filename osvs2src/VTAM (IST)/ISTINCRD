*               CRDOBPTR=CRDOBPTR+CHARCNT;/* UPDATE OUTPUT PTR       */ 00770000
         L     @00,CRDOBPTR                                        0490 00771000
         AL    @00,CHARCNT                                         0490 00772000
         ST    @00,CRDOBPTR                                        0490 00773000
*             END;                                                 0491 00774000
*           ELSE                                                   0492 00775000
*             ;                                                    0492 00776000
@RF00479 DS    0H                                                  0493 00777000
*         END;                      /* END ELSE CLAUSE FOR CRDENDSP     00778000
*                                      CHECK                         */ 00779000
*     END;                          /* END MINOR LOOP                */ 00780000
@RC00451 DS    0H                                                  0494 00781000
@DE00449 LTR   RCREG,RCREG                                         0494 00782000
         BNZ   @DC00449                                            0494 00783000
         TM    CRDENTRT,B'00100000'                                0494 00784000
         BZ    @DL00449                                            0494 00785000
@DC00449 DS    0H                                                  0495 00786000
*     CALL CHK72;                   /* CALL SUBRTN. TO PROCESS COL.     00787000
*                                      72                            */ 00788000
         BAL   @14,CHK72                                           0495 00789000
*   END;                            /* END MAJOR LOOP                */ 00790000
@DE00448 LTR   RCREG,RCREG                                         0496 00791000
         BNZ   @DC00448                                            0496 00792000
         TM    CRDENDST,B'01000000'                                0496 00793000
         BZ    @DL00448                                            0496 00794000
@DC00448 DS    0H                                                  0497 00795000
*   R14=PARMSAVE;                   /* RESTORE REG 14                */ 00796000
         L     R14,PARMSAVE                                        0497 00797000
*   END PARMSCAN;                                                  0498 00798000
@EL00003 DS    0H                                                  0498 00799000
@EF00003 DS    0H                                                  0498 00800000
@ER00003 BR    @14                                                 0498 00801000
*GOREAD:                                                           0499 00802000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0499 00803000
*                                                                  0499 00804000
GOREAD   DS    0H                                                  0500 00805000
*/********************************************************************  00806000
*                                                                    */ 00807000
*/*   THE GOREAD SUBROUTINE CALLS ISTINCRS TO READ A RECORD FROM THE */ 00808000
*/* VTAMLST DATA SET. IF THE RETURN CODE IS 0, THE CARD IS CHECKED   */ 00809000
*/* FOR A COMMENT CARD.                                              */ 00810000
*/*   IF IT IS A COMMENT CARD, THE ONLY PROCESSING DONE IS TO CHECK  */ 00811000
*/* THAT IT IS NOT A COMMENT CARD IMBEDDED IN THE MIDDLE OF PARAMETERS  00812000
*                                                                    */ 00813000
*/* IF THIS IS THE CASE, A MESSAGE IS PUT OUT.                       */ 00814000
*/*   IF IT IS NOT A COMMENT CARD, THE SUBROUTINE EXITS.             */ 00815000
*/*   IF THE RETURN CODE FROM ISTINCRS IS NON ZERO, AN APPROPRIATE   */ 00816000
*/* MESSAGE IS PUT OUT.                                              */ 00817000
*/********************************************************************  00818000
*                                                                    */ 00819000
*                                                                  0500 00820000
*   DCL                                                            0500 00821000
*     ON BIT(8) CONSTANT('01'X);    /* USED TO SET COMMFLAG ON       */ 00822000
*   DCL                                                            0501 00823000
*     OFF BIT(8) CONSTANT('00'X);   /* USED TO SET COMMFLAG OFF      */ 00824000
*/* LOCAL DECLARE FOR GOREAD                                         */ 00825000
*                                                                  0502 00826000
*   DCL                                                            0502 00827000
*     GORDSAVE;                     /* SAVE VAR. FOR REG 14          */ 00828000
*   DCL                                                            0503 00829000
*     COMMFLAG BIT(8) INIT('01'X);  /* FLAG INDICATIONG A COMMENT    */ 00830000
*   GORDSAVE=R14;                   /* SAVE REG 14                   */ 00831000
         ST    R14,GORDSAVE                                        0504 00832000
*   COMMFLAG=ON;                    /* TURN ON COMMENT FLAG FOR LOOP */ 00833000
         MVI   COMMFLAG,X'01'                                      0505 00834000
*COMMCHK:                                                          0506 00835000
*   DO WHILE COMMFLAG=ON;           /* GO THRU LOOP WHILE COMMENT  0506 00836000
*                                      FLAG IS ON                    */ 00837000
COMMCHK  B     @DE00506                                            0506 00838000
@DL00506 DS    0H                                                  0507 00839000
*     COMMFLAG=OFF;                 /* TURN COMMENT FLAG OFF         */ 00840000
         MVI   COMMFLAG,X'00'                                      0507 00841000
*     R1=ADDR(ISTRSPRM);            /* SET UP REG 1 FOR CRS          */ 00842000
         LA    R1,ISTRSPRM                                         0508 00843000
*     CALL ISTINCRS;                /* GO TO READ IN A RECORD        */ 00844000
         L     @15,@CV02466                                        0509 00845000
