         LTR   @11,@11                                             0484 00749000
         BZ    @RF00484                                            0484 00750000
*                 DO;                                              0485 00751000
*                                                                  0485 00752000
*                   /*************************************************/ 00753000
*                   /*                                               */ 00754000
*                   /* MOVE CHARS TO OUTPUT AREA                     */ 00755000
*                   /*                                               */ 00756000
*                   /*************************************************/ 00757000
*                                                                  0486 00758000
*                   OUTAREA(START:CHARCNT)=INBUFNOW(START:CHARCNT);     00759000
         L     @10,CRDOBPTR                                        0486 00760000
         BCTR  @11,0                                               0486 00761000
         EX    @11,@SM02630                                        0486 00762000
*                 END;                                             0487 00763000
*               ELSE                                               0488 00764000
*                 ;                                                0488 00765000
@RF00484 DS    0H                                                  0489 00766000
*               CURRENTP=STOPPTR+CONS1;/* UPDATE CURRENT PTR         */ 00767000
         LA    CURRENTP,1                                          0489 00768000
