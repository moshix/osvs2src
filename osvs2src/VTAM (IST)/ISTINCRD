         L     @11,ATCCONFT(,@11)                                  0707 01372000
         LH    @00,CONBUFSZ(,@11)                                  0707 01373000
         AL    @00,CONRDBUF(,@11)                                  0707 01374000
         SL    @00,CRDOBPTR                                        0707 01375000
         ST    @00,REMSPACE                                        0707 01376000
*   IF REMSPACE>=BUFENUFF THEN                                     0708 01377000
         C     @00,@CF02360                                        0708 01378000
         BL    @RF00708                                            0708 01379000
*     RCREG=CRDCLEAR;               /* IF SPACE >=71 THEN BUFFER   0709 01380000
*                                      STILL BIG ENOUGH              */ 01381000
         SLR   RCREG,RCREG                                         0709 01382000
*   ELSE                                                           0710 01383000
*     DO;                                                          0710 01384000
         B     @RC00708                                            0710 01385000
@RF00708 DS    0H                                                  0711 01386000
*       TEMPBUFF=CONRDBUF;          /* SAVE CONRDBUF                 */ 01387000
         L     @11,VCVTPTR                                         0711 01388000
         L     @11,ATCCONFT(,@11)                                  0711 01389000
         L     @00,CONRDBUF(,@11)                                  0711 01390000
         ST    @00,TEMPBUFF                                        0711 01391000
*       TEMPSIZE=CONBUFSZ;          /* SAVE CONBUFSZ                 */ 01392000
         LH    @11,CONBUFSZ(,@11)                                  0712 01393000
         ST    @11,TEMPSIZE                                        0712 01394000
*       NEXTSIZE=CONBUFSZ+BUFSIZE;  /* GET NEXT BUFFER SIZE          */ 01395000
         AL    @11,BUFSIZE                                         0713 01396000
         ST    @11,NEXTSIZE                                        0713 01397000
*       GEN SETS(SMSREGS)(STM    2,4,SMSREGS);/* SAVE SMS-CLOBBERRED    01398000
*                                      REGS                          */ 01399000
*                                                                  0714 01400000
         STM    2,4,SMSREGS                                             01401000
*/*VGTMAIN SIZE(NEXTSIZE)STORAGE((ADDR(CONRDBUF)))SUBPOOL((ADDR(CRDPOOL 01402000
