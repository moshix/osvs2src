*           GEN(EX    TRTREG,TRANSLT);/* EXECUTE THE TRT FOR PARMSCAN*/ 00642000
         EX    TRTREG,TRANSLT                                           00643000
*           STOPPTR=R1;             /* SAVE STOP POSITION            */ 00644000
         ST    R1,STOPPTR                                          0456 00645000
*           BC(CONS7,CONCODE2);     /* IF C.C IS NOT ZERO BRANCH     */ 00646000
         BC    7,CONCODE2                                          0457 00647000
*           CC0FLAG=CRDON;          /* INDICATE C.C. =0              */ 00648000
         OI    CC0FLAG,B'01000000'                                 0458 00649000
*           CRDENTRT=CRDON;         /* SET FLAG TO GET OUT OF TRT  0459 00650000
*                                      LOOP                          */ 00651000
         OI    CRDENTRT,B'00100000'                                0459 00652000
*           GOTO RCCHECK;           /* THIS IS NEEDED BECAUSE OF C.C.   00653000
*                                      CHECK                         */ 00654000
         B     RCCHECK                                             0460 00655000
*CONCODE2:                                                         0461 00656000
*           BC(CONS13,QUOTECHK);    /* CHECK FOR C.C. = 2            */ 00657000
CONCODE2 BC    13,QUOTECHK                                         0461 00658000
*           CRDENTRT=CRDON;         /* SET END TRT FLAG              */ 00659000
         OI    CRDENTRT,B'00100000'                                0462 00660000
*QUOTECHK:                                                         0463 00661000
*           IF FUNCBYTE=QUOTECHR THEN/* IS THE STOP BYTE A QUOTE?    */ 00662000
QUOTECHK L     @11,STOPPTR                                         0463 00663000
         CLC   FUNCBYTE(1,@11),QUOTECHR                            0463 00664000
         BNE   @RF00463                                            0463 00665000
*             DO;                                                  0464 00666000
*               QUOTECNS=CONS1;     /* WE WANT TO ADD QUOTE          */ 00667000
         LA    @11,1                                               0465 00668000
         ST    @11,QUOTECNS                                        0465 00669000
*               QUOTECNT=QUOTECNT+CONS1;/* IF YES, ADD 1 TO QUOTE CNT*/ 00670000
         AL    @11,QUOTECNT                                        0466 00671000
         ST    @11,QUOTECNT                                        0466 00672000
*             END;                                                 0467 00673000
*           ELSE                                                   0468 00674000
*             DO;                                                  0468 00675000
         B     @RC00463                                            0468 00676000
@RF00463 DS    0H                                                  0469 00677000
*               IF QUOTECNT//2=CRDCLEAR THEN/* CHECK FOR PAIRED    0469 00678000
*                                      QUOTES                        */ 00679000
         SLR   @11,@11                                             0469 00680000
         L     @06,QUOTECNT                                        0469 00681000
         SRDA  @06,32                                              0469 00682000
         D     @06,@CF00073                                        0469 00683000
         CR    @06,@11                                             0469 00684000
         BNE   @RF00469                                            0469 00685000
*                 DO;                                              0470 00686000
*                   QUOTECNS=CRDCLEAR;/* SET CONS TO 0 SO BLANK NOT     00687000
*                                      INCL.                         */ 00688000
         ST    @11,QUOTECNS                                        0471 00689000
*                   IF PREVBYTE=',' THEN                           0472 00690000
         L     @11,STOPPTR                                         0472 00691000
         AL    @11,@CF02629                                        0472 00692000
         CLI   PREVBYTE(@11),C','                                  0472 00693000
         BE    @RT00472                                            0472 00694000
*                     ;             /* CHECK IF PREVIOUS BYTE WAS A     00695000
*                                      COMMA                         */ 00696000
*                   ELSE                                           0474 00697000
*                     CRDENDSP=CRDON;/* OUT OF SUB-PARM SCAN         */ 00698000
         OI    CRDENDSP,B'10000000'                                0474 00699000
*                   CRDENTRT=CRDON; /* OUT OF TRT LOOP               */ 00700000
@RT00472 OI    CRDENTRT,B'00100000'                                0475 00701000
*                 END;                                             0476 00702000
*               ELSE                                               0477 00703000
*                 QUOTECNS=CONS1;   /* WANT TO ADD BLANK IF INSIDE 0477 00704000
*                                      QUOTES                        */ 00705000
         B     @RC00469                                            0477 00706000
@RF00469 MVC   QUOTECNS(4),@CF00061                                0477 00707000
*             END;                                                 0478 00708000
@RC00469 DS    0H                                                  0479 00709000
*RCCHECK:                                                          0479 00710000
*           IF RCREG=CRDCLEAR THEN                                 0479 00711000
@RC00463 DS    0H                                                  0479 00712000
