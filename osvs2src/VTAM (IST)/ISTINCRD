@ER00001 L     @14,12(,@13)                                        0397 00455000
         LM    @00,@12,20(@13)                                     0397 00456000
         BR    @14                                                 0397 00457000
*EDITRCD:                                                          0398 00458000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0398 00459000
*                                                                  0398 00460000
EDITRCD  DS    0H                                                  0399 00461000
*/********************************************************************  00462000
*                                                                    */ 00463000
*/*   THE EDITRCD SUBROUTINE DETERMINES HOW ISTINCRD WAS ENTERED.    */ 00464000
*/* I.E. WAS IT ENTERED FOR START PARAMETERS, CONFIGURATION PARAMETERS  00465000
*                                                                    */ 00466000
*/* OR ASSEMBLER LANGUAGE MACRO STATEMENTS. IF IT WAS ENTERED FOR    */ 00467000
*/* ASSEMBLER LANGUAGE MACRO STATEMENTS, THE NAME & OP CODE FIELDS   */ 00468000
*/* ARE PROPERLY EDITED. PARMSCAN IS CALLED FOR BOTH TYPES OF ENTRIES*/ 00469000
*/* TO SCAN PARAMETERS.                                              */ 00470000
*/********************************************************************  00471000
*                                                                    */ 00472000
*                                                                  0399 00473000
*   DCL                                                            0399 00474000
*     EDITSAVE;                     /* SAVE FOR REG 14               */ 00475000
*   EDITSAVE=R14;                   /* SAVE REG 14                   */ 00476000
         ST    R14,EDITSAVE                                        0400 00477000
*   CRDENDST=CRDOFF;                /* SET FLAG OFF FOR LOOP         */ 00478000
         NI    CRDENDST,B'10111111'                                0401 00479000
*   IF MEMBSAVE(START:FIRST6)='ATCCON'|MEMBSAVE(START:FIRST6)='ATCSTR'  00480000
*     THEN                          /* IF 1ST 6 CHARS OF MEMBER NAME    00481000
*                                      ARE ATCCON OR ATCSTR          */ 00482000
         CLC   MEMBSAVE(6),@CC02418                                0402 00483000
         BE    @RT00402                                            0402 00484000
         CLC   MEMBSAVE(6),@CC02419                                0402 00485000
         BNE   @RF00402                                            0402 00486000
@RT00402 DS    0H                                                  0403 00487000
*     CALL PARMSCAN;                /* CALL ROUTINE TO SCAN        0403 00488000
*                                      PARAMETERS                    */ 00489000
         BAL   @14,PARMSCAN                                        0403 00490000
*   ELSE                                                           0404 00491000
*     DO;                                                          0404 00492000
         B     @RC00402                                            0404 00493000
@RF00402 DS    0H                                                  0405 00494000
*       IF INBUF(START:START)=BLANK THEN/* CHECK FOR BLANK IN COL. 1 */ 00495000
         CLI   INBUF,C' '                                          0405 00496000
         BNE   @RF00405                                            0405 00497000
*         DO;                                                      0406 00498000
*           OUTAREA(START:START)=BLANK;/* MOVE A BLANK TO OUTPUT AREA*/ 00499000
         L     @11,CRDOBPTR                                        0407 00500000
         MVI   OUTAREA(@11),C' '                                   0407 00501000
*           CRDOBPTR=CRDOBPTR+CONS1;/* BUMP OUTPUT PTR               */ 00502000
         AL    @11,@CF00061                                        0408 00503000
         ST    @11,CRDOBPTR                                        0408 00504000
*         END;                                                     0409 00505000
*       ELSE                                                       0410 00506000
*         DO;                       /* IF NOT BLANK, THEN PROCESS  0410 00507000
*                                      NAME FIELD                    */ 00508000
         B     @RC00405                                            0410 00509000
@RF00405 DS    0H                                                  0411 00510000
*           CALL MTB;               /* MOVE TO BLANKS                */ 00511000
         BAL   @14,MTB                                             0411 00512000
*           CALL STNB;              /* SCAN TO NON-BLANKS            */ 00513000
         BAL   @14,STNB                                            0412 00514000
*         END;                                                     0413 00515000
*       IF RCREG=CRDCLEAR THEN      /* IF R.C. =0 THEN SCAN REST OF     00516000
*                                      CARD                          */ 00517000
@RC00405 LTR   RCREG,RCREG                                         0414 00518000
         BNZ   @RF00414                                            0414 00519000
*         DO;                                                      0415 00520000
*           CALL MTB;               /* MOVE TO BLANKS                */ 00521000
         BAL   @14,MTB                                             0416 00522000
*           NOPARMS=CRDON;          /* INDICATE NO PARAMS ON CARD IS    00523000
*                                      OK                            */ 00524000
         OI    NOPARMS,B'00001000'                                 0417 00525000
*           CALL STNB;              /* SCAN TO NON-BLANKS            */ 00526000
         BAL   @14,STNB                                            0418 00527000
*           IF RCREG=CRDCLEAR THEN  /* CHECK IF NON-BLANK FOUND      */ 00528000
         LTR   RCREG,RCREG                                         0419 00529000
         BNZ   @RF00419                                            0419 00530000
*             DO;                                                  0420 00531000
*               NOPARMS=CRDOFF;     /* TURN OFF NOPARMS FLAG         */ 00532000
         NI    NOPARMS,B'11110111'                                 0421 00533000
*               CALL PARMSCAN;      /* IF NON-BLANK FOUND, PROCESS 0422 00534000
*                                      PARMS                         */ 00535000
         BAL   @14,PARMSCAN                                        0422 00536000
*             END;                                                 0423 00537000
*           ELSE                                                   0424 00538000
*             ;                     /* RETURN CODE ^= 0 FROM PARMSCAN*/ 00539000
@RF00419 DS    0H                                                  0425 00540000
*         END;                                                     0425 00541000
*       ELSE                                                       0426 00542000
*         ;                         /* RETURN CODE ^= 0 FROM MTB     */ 00543000
@RF00414 DS    0H                                                  0427 00544000
*     END;                                                         0427 00545000
*   IF RCREG=CRDCLEAR THEN                                         0428 00546000
