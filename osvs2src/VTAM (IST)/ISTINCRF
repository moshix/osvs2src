@EF00001 DS    0H                                                  0420 00120000
@ER00001 LM    @14,@12,12(@13)                                     0420 00121000
         BR    @14                                                 0420 00122000
*                                                                  0421 00123000
*/* OPTCK SUBROUTINE                                                 */ 00124000
*/********************************************************************  00125000
*                                                                    */ 00126000
*/* THE OPTCK SUBROUTINE HANDLES MISCELLANEOUS MODIFY OPTIONS & PUTS */ 00127000
*/* OUT A MESSAGE FOR ANY OPTIONS WHICH AREN'T VALID. THE OPTIONS    */ 00128000
*/* HANDLED ARE: NETWORK SOLICITOR, DUMP, & CHANGE POLL DELAY.       */ 00129000
*/*   IF ISTINCMR IS THE ROUTINE CALLED IT WILL FREE THE NCSPL AND   */ 00130000
*/* AND A SWITCH IS TURNED ON TO INDICATE THE NCSPL WAS FREED.       */ 00131000
*/********************************************************************  00132000
*                                                                    */ 00133000
*                                                                  0421 00134000
*OPTCK:                                                            0421 00135000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0421 00136000
OPTCK    DS    0H                                                  0422 00137000
*   R14SAVE1=R14;                   /* SAVE REG 14                   */ 00138000
         LR    R14SAVE1,R14                                        0422 00139000
