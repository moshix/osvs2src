SVTM     TITLE 'ISTINCRF-MODIFY ROUTER                                 *00001000
                        '                                               00002000
ISTINCRF CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCRF  75.281'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @08,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@08                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @00,@SA00001                                        0001 00015000
         ST    @00,8(,@13)                                         0001 00016000
         LR    @13,@00                                             0001 00017000
*   NCSPLPTR=R1;                    /* SAVE NCSPL PTR                */ 00018000
*                                                                  0389 00019000
         LR    NCSPLPTR,R1                                         0389 00020000
*    DO; /* LISATCVT CVTBASE(PAGEABLE,(VCVTPTR)) */                0390 00021000
*      VCVTPTR = ATCLCPTR; /*SET UP ATCVT POINTER*/                0391 00022000
         L     VCVTPTR,ATCLCPTR                                    0391 00023000
*    END; /*LISATCVT*/                                             0392 00024000
*                                                                  0393 00025000
*/* CODE BEGINS HERE FOR MODIFY                                      */ 00026000
*/********************************************************************  00027000
*                                                                    */ 00028000
*/* MAINLINE ISTINCRF(MODIFY ROUTER) BREAKS DOWN THE MODIFY COMMAND  */ 00029000
*/* INTO ONE OF FOUR AREAS(TRACE PRINT, TEST, TRACE, & MISCELLANEOUS */ 00030000
*/* OPTIONS). IT ROUTES EACH RESPECTIVE OPTION TO AN APPROPRIATE SUB-*/ 00031000
*/* ROUTINE FOR PROCESSING.                                          */ 00032000
*/*   ON RETURN FROM THE APPROPRIATE SUBROUTINE, MAINLINE DETER-     */ 00033000
*/* MINES IF THE NCSPL EXISTS, AND IF SO, FREES IT.                  */ 00034000
*/********************************************************************  00035000
*                                                                    */ 00036000
*                                                                  0393 00037000
*   CRFNCSPL=CRFON;                 /* TURN ON SWITCH INDICATING   0393 00038000
*                                      NCSPL EXISTS                  */ 00039000
         OI    CRFNCSPL,B'10000000'                                0393 00040000
*   IF NCSPLVBF=NCSMTPR THEN                                       0394 00041000
         CLC   NCSPLVBF(2,NCSPLPTR),@CB01368                       0394 00042000
         BNE   @RF00394                                            0394 00043000
*     CALL TPRINT;                  /* IF VERB CODE IS TRACE PRINT 0395 00044000
*                                      TRACE PRINT                   */ 00045000
         BAL   @14,TPRINT                                          0395 00046000
*   ELSE                                                           0396 00047000
*     DO;                                                          0396 00048000
         B     @RC00394                                            0396 00049000
@RF00394 DS    0H                                                  0397 00050000
*       IF NCSPLVBF=NCSMTST THEN                                   0397 00051000
         CLC   NCSPLVBF(2,NCSPLPTR),@CB01350                       0397 00052000
         BNE   @RF00397                                            0397 00053000
*         CALL TEST;                /* CALL TEST IF VERB INDICATES   */ 00054000
         BAL   @14,TEST                                            0398 00055000
*       ELSE                                                       0399 00056000
*         DO;                       /* CHECK FOR MODIFY TRACE OR   0399 00057000
*                                      NOTRACE                       */ 00058000
         B     @RC00397                                            0399 00059000
@RF00397 DS    0H                                                  0400 00060000
*           IF(CRFTRCON&NCSPLVBF)=NCSMTRY|(CRFTRCON&NCSPLVBF)=NCSMTRN   00061000
*             THEN                                                 0400 00062000
         MVC   @TS00001(2),CRFTRCON                                0400 00063000
         NC    @TS00001(2),NCSPLVBF(NCSPLPTR)                      0400 00064000
         CLC   @TS00001(2),@CB01354                                0400 00065000
         BE    @RT00400                                            0400 00066000
         MVC   @TS00001(2),CRFTRCON                                0400 00067000
         NC    @TS00001(2),NCSPLVBF(NCSPLPTR)                      0400 00068000
         CLC   @TS00001(2),@CB01366                                0400 00069000
         BNE   @RF00400                                            0400 00070000
@RT00400 DS    0H                                                  0401 00071000
*             CALL TRACE;                                          0401 00072000
         BAL   @14,TRACE                                           0401 00073000
*           ELSE                                                   0402 00074000
*             CALL OPTCK;           /* IF NOT CALL OPTCK             */ 00075000
         B     @RC00400                                            0402 00076000
@RF00400 BAL   @14,OPTCK                                           0402 00077000
*         END;                                                     0403 00078000
@RC00400 DS    0H                                                  0404 00079000
*     END;                                                         0404 00080000
@RC00397 DS    0H                                                  0405 00081000
*   IF CRFNCSPL=CRFOFF THEN                                        0405 00082000
@RC00394 TM    CRFNCSPL,B'10000000'                                0405 00083000
         BZ    @RT00405                                            0405 00084000
*     ;                             /* DON'T DO FREESTOR IF SWITCH 0406 00085000
*                                      OFF                           */ 00086000
*   ELSE                            /* FREESTOR NCSPL. DON'T SAVE  0407 00087000
*                                      REGS SINCE MAY NOT HAVE RPH   */ 00088000
*      /*FREESTOR AREA(NCSPLPTR)SIZE(LENGTH(ISTNCSPL))SAVE(NO)FETCHP(NO 00089000
**/                                                                0407 00090000
*       DO;                                                        0407 00091000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0408 00092000
*/* END OF ATBUILD */                                              0408 00093000
*      RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                     0408 00094000
*      GPR00F = LENGTH(ISTNCSPL);                      /*SIZE IN REG0*/ 00095000
         LA    GPR00F,280                                          0409 00096000
*      DCL 1 CMZA0012 FIXED(31) STATIC LOCAL,                      0410 00097000
*            2 * FIXED(8) INIT(0),                   /* OPTION FLAGS */ 00098000
*            2 * FIXED(8) INIT(96),                   /* STORAGE KEY */ 00099000
*            2 * FIXED(8) INIT(241),               /* SUBPOOL NUMBER */ 00100000
*            2 * FIXED(8) INIT('01'X);               /* REQUEST TYPE */ 00101000
*      GPR03F = CMZA0012;                                          0411 00102000
         L     GPR03F,CMZA0012                                     0411 00103000
*      RFY (GPR04P) RSTD;                                          0412 00104000
