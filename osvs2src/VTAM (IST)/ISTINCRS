*                                                                  0075 00080000
*ISTINFRU:                          /*                       @XM06409*/ 00081000
*   RTNCDE=4;                       /* INDICATE EODAD        @ZA03720*/ 00082000
ISTINFRU MVC   RTNCDE(4),@CF00040                                  0075 00083000
*EXIT:                                                             0076 00084000
*   RETURN CODE(RTNCDE);            /* RETURN TO CALLER      @ZA03720*/ 00085000
EXIT     L     @15,RTNCDE                                          0076 00086000
         L     @13,4(,@13)                                         0076 00087000
         L     @14,12(,@13)                                        0076 00088000
         LM    @00,@12,20(@13)                                     0076 00089000
         BR    @14                                                 0076 00090000
*READRTN:                                                          0077 00091000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);/*                @ZA03720*/ 00092000
*                                                                  0077 00093000
READRTN  DS    0H                                                  0078 00094000
*   /*****************************************************************/ 00095000
*   /*                                                               */ 00096000
*   /* THIS ROUTINE READS A BLOCK FROM VTAMLST. IT DEBLOCKS THE DATA */ 00097000
*   /* AND PASSES ONE RECORD AT A TIME TO INCRD.                     */ 00098000
*   /*                                                               */ 00099000
*   /*****************************************************************/ 00100000
*                                                                  0078 00101000
*   R14SAVE=R14;                    /* SAVE RETURN ADDRESS   @ZA03720*/ 00102000
         ST    R14,R14SAVE                                         0078 00103000
*   IF DONTREAD='0'B THEN           /* IS RECORD IN BUFFER   @ZA03720*/ 00104000
         TM    DONTREAD,B'10000000'                                0079 00105000
         BNZ   @RF00079                                            0079 00106000
*     DO;                           /* NO                    @ZA03720*/ 00107000
*       R3=BUFRADR;                 /* BUFFER ADDR FOR READ  @ZA03720*/ 00108000
         L     @09,@CA00654                                        0081 00109000
         L     R3,BUFRADR-BUFRADR(,@09)                            0081 00110000
*       GEN FLOWS(ISTINFRT,ISTINFRU);                              0082 00111000
*                                           /*               @ZA03720*/ 00112000
              READ DECB,SF,(2),(3),'S'                                  00113000
              CHECK DECB                                                00114000
*       IF IOERR='1'B THEN          /* WAS THERE AN I/O ERROR      0083 00115000
*                                                            @ZA03721*/ 00116000
         TM    IOERR,B'01000000'                                   0083 00117000
         BNO   @RF00083                                            0083 00118000
*         DO;                       /* YES                   @ZA03721*/ 00119000
*           IOERR='0'B;             /* RESET ERROR FLAG      @ZA03721*/ 00120000
         NI    IOERR,B'10111111'                                   0085 00121000
*           RTNCDE=8;               /* INDICATE I/O ERROR    @ZA03721*/ 00122000
         MVC   RTNCDE(4),@CF00042                                  0086 00123000
*           GOTO READOUT;           /* EXIT READ ROUTINE     @ZA03721*/ 00124000
         B     READOUT                                             0087 00125000
*         END;                      /*                       @ZA03721*/ 00126000
*       RECNUMBR=((DCBBLKSI-RESIDCT)/RCDSIZE);/* COMPUTE NUMBER OF 0089 00127000
*                                      RECORDS IN EACK BLOCK @ZA03720*/ 00128000
@RF00083 LH    @04,DCBBLKSI+52(,DCBPTR)                            0089 00129000
         L     @09,IOBSTPTR                                        0089 00130000
         SH    @04,RESIDCT(,@09)                                   0089 00131000
         SRDA  @04,32                                              0089 00132000
         D     @04,@CF00302                                        0089 00133000
         STH   @05,RECNUMBR                                        0089 00134000
*       DATAPTR=BUFRADR;            /* POINT TO FIRST RECORD @ZA03720*/ 00135000
         L     @09,@CA00654                                        0090 00136000
         L     @09,BUFRADR-BUFRADR(,@09)                           0090 00137000
         ST    @09,DATAPTR                                         0090 00138000
*       DONTREAD='1'B;              /* INDICATE DATA AVAIL   @ZA03720*/ 00139000
         OI    DONTREAD,B'10000000'                                0091 00140000
*     END;                          /*                       @ZA03720*/ 00141000
*   RECNUMBR=RECNUMBR-1;            /* DECREMENT RECORD CNT  @ZA03720*/ 00142000
@RF00079 LH    @09,RECNUMBR                                        0093 00143000
         BCTR  @09,0                                               0093 00144000
         STH   @09,RECNUMBR                                        0093 00145000
*   CRDBUFR=RECORD;                 /* MOVE RECORD INTO BUFFER     0094 00146000
*                                      PROVIDED BY INCRD     @ZA03720*/ 00147000
         L     @11,RSPBUFA(,RDPRMPTR)                              0094 00148000
         L     @10,DATAPTR                                         0094 00149000
         MVC   CRDBUFR(80,@11),RECORD(@10)                         0094 00150000
*   IF RECNUMBR^=0 THEN             /* ANY MORE RECORDS LEFT @ZA03720*/ 00151000
         LTR   @09,@09                                             0095 00152000
         BZ    @RF00095                                            0095 00153000
*     DATAPTR=DATAPTR+RCDSIZE;      /* YES - POINT TO NEXT RECORD  0096 00154000
*                                                            @ZA03720*/ 00155000
         AL    @10,@CF00302                                        0096 00156000
         ST    @10,DATAPTR                                         0096 00157000
*   ELSE                            /* NO - NO RECORDS LEFT  @ZA03720*/ 00158000
*     DONTREAD='0'B;                /* INDICATE TO READ NEXT BLOCK 0097 00159000
*                                                            @ZA03720*/ 00160000
         B     @RC00095                                            0097 00161000
@RF00095 NI    DONTREAD,B'01111111'                                0097 00162000
*   RTNCDE=0;                       /* INDICATE SUCCESSFUL READ    0098 00163000
*                                                            @ZA03720*/ 00164000
@RC00095 SLR   @11,@11                                             0098 00165000
         ST    @11,RTNCDE                                          0098 00166000
*READOUT:                                                          0099 00167000
