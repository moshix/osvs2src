ISTINCRS CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCRS  75.231'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @09,@SA00001                                        0001 00015000
         ST    @09,8(,@13)                                         0001 00016000
         LR    @13,@09                                             0001 00017000
*   RDPRMPTR=R1;                    /* REBASE PARM LIST      @ZA03720*/ 00018000
         LR    RDPRMPTR,R1                                         0056 00019000
*   DCBPTR=RSPDCBA;                 /* ADDR OF DCB           @ZA03720*/ 00020000
         L     DCBPTR,RSPDCBA(,RDPRMPTR)                           0057 00021000
*   IF RSPMEMBN=XZEROS THEN         /* IF ZERO NO NEED TO FIND       */ 00022000
         CLC   RSPMEMBN(8,RDPRMPTR),XZEROS                         0058 00023000
         BNE   @RF00058                                            0058 00024000
*     RTNCDE=0;                     /* INDICATE FIND NOT NEEDED      */ 00025000
         SLR   @09,@09                                             0059 00026000
         ST    @09,RTNCDE                                          0059 00027000
*   ELSE                                                           0060 00028000
*     DO;                                                          0060 00029000
         B     @RC00058                                            0060 00030000
@RF00058 DS    0H                                                  0061 00031000
*       R3=ADDR(RSPMEMBN);          /* ADDR OF MEMBER NAME           */ 00032000
         LA    R3,RSPMEMBN(,RDPRMPTR)                              0061 00033000
*       GEN REFS(DCBPTR,R3)(FIND (2),(3),D);/*               @ZA03720*/ 00034000
         FIND (2),(3),D                                                 00035000
*       IF R15^=0 THEN              /* TEST FOR ERROR FROM FIND      */ 00036000
         LTR   R15,R15                                             0063 00037000
         BZ    @RF00063                                            0063 00038000
*         RTNCDE=12;                /* MEMBER NOT FOUND      @ZA03720*/ 00039000
         MVC   RTNCDE(4),@CF00032                                  0064 00040000
*       ELSE                        /* MEMBER FOUND          @ZA03720*/ 00041000
*         DO;                       /*                       @ZA03720*/ 00042000
         B     @RC00063                                            0065 00043000
@RF00063 DS    0H                                                  0066 00044000
*           DONTREAD='0'B;          /* INDICATE READ REQUIRD @ZA03720*/ 00045000
         NI    DONTREAD,B'01111111'                                0066 00046000
*           RTNCDE=0;               /* INDICATE FIND SUCCESS @ZA03720*/ 00047000
         SLR   @09,@09                                             0067 00048000
         ST    @09,RTNCDE                                          0067 00049000
*         END;                      /*                       @ZA03720*/ 00050000
*     END;                                                         0069 00051000
@RC00063 DS    0H                                                  0070 00052000
*   IF RTNCDE=0 THEN                /* READ A RECORD         @ZA03720*/ 00053000
@RC00058 L     @09,RTNCDE                                          0070 00054000
         LTR   @09,@09                                             0070 00055000
         BNZ   @RF00070                                            0070 00056000
*     CALL READRTN;                 /* YES - READ SUBRTN     @ZA03720*/ 00057000
         BAL   @14,READRTN                                         0071 00058000
*   GOTO EXIT;                      /* RETURN                @ZA03720*/ 00059000
*                                                                  0072 00060000
         B     EXIT                                                0072 00061000
*   /*****************************************************************/ 00062000
*   /*                                                               */ 00063000
*   /* SYNAD ROUTINE                                                 */ 00064000
*   /*                                                               */ 00065000
*   /*****************************************************************/ 00066000
*                                                                  0073 00067000
*ISTINFRT:                          /*                       @XM06409*/ 00068000
*   IOERR='1'B;                     /* INDICATE I/O ERROR    @ZA03721*/ 00069000
ISTINFRT OI    IOERR,B'01000000'                                   0073 00070000
*   GEN EXIT NOSEQFLOW(BR  R14);    /* RETURN TO SYSTEM TO CLEAN UP     00071000
*                                      DCB                   @ZA03721*/ 00072000
*                                                                  0074 00073000
         BR  R14                                                        00074000
*   /*****************************************************************/ 00075000
*   /*                                                               */ 00076000
*   /* EODAD ROUTINE                                                 */ 00077000
*   /*                                                               */ 00078000
