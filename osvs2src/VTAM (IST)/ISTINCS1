*  /*SEGMENT(CS1HKEP)START*/DO;GOTO CS1HKEP@;CS1HKEP:END;          0416 00105000
CS1HKEP  DS    0H                                                  0419 00106000
*/********************************************************************/ 00107000
*/*   HOUSEKEEPING                                                   */ 00108000
*/*   INPUT: R13 HAS THE ADDR OF THE NEXT AVAILABLE SAVE AREA WHICH  */ 00109000
*/*           IS AT THE END OF THE NCSPL WORK AREA AND IS ALLOCATED  */ 00110000
*/*           FROM BOTTOM TO TOP                                     */ 00111000
*/*          CS1NXTAV HAS THE ADDR OF THE NEXT AVAILABLE WORK SPACE  */ 00112000
*/*           WHICH IS AT THE TOP OF THE NCSPL WORK AREA AND IS      */ 00113000
*/*           ALLOCATED TOP TO BOTTOM                                */ 00114000
*/*                                                                  */ 00115000
*/*  OUTPUT: R13 HAS UPDATED ADDR OF NEXT AVAILABLE SAVE AREA        */ 00116000
*/*          CS1NXTAV HAS UPDATED ADDR OF NEXT AVAILABLE WORK SPACE  */ 00117000
*/*                                                                  */ 00118000
*/********************************************************************/ 00119000
*/********************************************************************/ 00120000
*/*   SAVE AREA -                                                    */ 00121000
*/*     DO STANDARD LINKAGE USING NEXT AVAILABLE SAVE AREA           */ 00122000
*/*     UPDATE NEXT AVAILABLE SAVE AREA ACCORDING TO NCSPL WORK AREA */ 00123000
*/*      ALLOCATION RULES                                            */ 00124000
*/********************************************************************/ 00125000
*RFY CS1R2 RSTD;                                                   0419 00126000
*CS1R2=CS1R13;                                                     0420 00127000
         LR    CS1R2,CS1R13                                        0420 00128000
*CS1R13=CS1R13-LENGTH(CS1SVMAP);                                   0421 00129000
         SL    CS1R13,@CF01981                                     0421 00130000
*CS1R2->CS1FWPTR=CS1R13;                                           0422 00131000
         ST    CS1R13,CS1FWPTR(,CS1R2)                             0422 00132000
*CS1R13->CS1BKPTR=CS1R2;                                           0423 00133000
         ST    CS1R2,CS1BKPTR(,CS1R13)                             0423 00134000
*RFY CS1R2 UNRSTD;                                                 0424 00135000
*CS1R13->CS1FWPTR=CS10;                                            0425 00136000
         SLR   @04,@04                                             0425 00137000
         ST    @04,CS1FWPTR(,CS1R13)                               0425 00138000
*/********************************************************************/ 00139000
*/*   WORK SPACE                                                     */ 00140000
*/*     SET BASE FOR WORK SPACE FROM NEXT AVAILABLE POINTER WHICH IS */ 00141000
*/*      THE FIRST WORD OF THE NCSPL WORK AREA                       */ 00142000
*/*     CLEAR WORK SPACE                                             */ 00143000
*/*     ESTABLISH BASE FOR DYNAMIC AREA (FOLLOWS CS1 WORK SPACE)     */ 00144000
*/*     UPDATE 'NEXT AVAILABLE POINTER' IN THE NCSPL WORK AREA TO    */ 00145000
*/*      TO POINT TO THE AVAILABLE SPACE FOLLOWING THE ISTINCS1 WORK */ 00146000
*/*      SPACE                                                       */ 00147000
*/********************************************************************/ 00148000
*CS1WKA=NCSPLWKA->CS1NXTAV;                                        0426 00149000
