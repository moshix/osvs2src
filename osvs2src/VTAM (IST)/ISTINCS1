*/* LOGICAL CHANNEL COMMAND WORD                                      * 00164000
*/* ADD A DECLARE                                      003 10/18/74   * 00165000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0432 00166000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 00167000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 00168000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 00169000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 00170000
*              4  * BIT(1),            /* RESERVED                    * 00171000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0432 00172000
*                                         ERASE OR READ CONT          * 00173000
*              4  LCCWCTL BIT(4),      /* CONTROL TYPE                * 00174000
*                 5  LCCWMODE BIT(2),  /* MODE TYPE                   * 00175000
*                 5  LCCWTYPE BIT(2),  /* OPERATION TYPE              * 00176000
*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00177000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00178000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00179000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00180000
*                                         RECORD                      * 00181000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00182000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00183000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00184000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00185000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00186000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00187000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00188000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00189000
*/* LOGICAL COMMAND CODES */                                       0433 00190000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00191000
*     LXEC     BIT(8) CONSTANT('4B'X), /* RESET AT END OF COMMAND     * 00192000
*     LXO      BIT(8) CONSTANT('5B'X), /* RESET ORDERLY               * 00193000
*     LCCWPRGE BIT(8) CONSTANT('6B'X), /* PURGE                       * 00194000
