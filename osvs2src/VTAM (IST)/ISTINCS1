*            4 LCPZF003 BIT(1),        /* LCP PROCESSING COMPLETE FLAG* 00280000
*              5 LCPZRT04 BIT(1),      /* STPR SET IN BDU FLAG       */ 00281000
*                                      /* (REQ. TAG OVERLAY)         */ 00282000
*                6 LCPZRTL4 BIT(1),    /* FEEDBACK REQUESTED         */ 00283000
*            4 LCPZF004 BIT(1),        /* SINGLE LCCW/LCP FLAG       */ 00284000
