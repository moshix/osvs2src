*  RFY ISTCNSTA;                                                   0393 00028000
*  RFY ISTVYCRR;                                                   0394 00029000
*/* END OF ATBUILD */                                              0395 00030000
*  /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                    0395 00031000
*NCSPLRPH->RPHCRR->CRACRR->NINCS1 = '1'B;                          0395 00032000
         L     @04,NCSPLRPH(,CS1R1)                                0395 00033000
         L     @04,RPHCRR(,@04)                                    0395 00034000
         L     @04,CRACRR(,@04)                                    0395 00035000
         OI    NINCS1(@04),B'00001000'                             0395 00036000
*  /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                   0396 00037000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0396 00038000
