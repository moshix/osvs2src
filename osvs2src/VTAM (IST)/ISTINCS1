*  END;                                                            0398 00043000
*GEN REFS(CS1WKA) (USING @DATD,CS1WKA);                            0399 00044000
         USING @DATD,CS1WKA                                             00045000
*  /*SEGMENT(CS1HKEP)REQUEST*/DO;GOTO CS1HKEP;CS1HKEP#:END;        0400 00046000
*                                                                  0400 00047000
         B     CS1HKEP                                             0401 00048000
CS1HKEP# DS    0H                                                  0403 00049000
*RFY ISTRLU BASED(NCSPLRPT);                                       0403 00050000
*RFY ISTMGL BASED(NCSMGLPT);                                       0404 00051000
*RFY ISTRH BASED(ADDR(MGLRH));                                     0405 00052000
*                                                                  0405 00053000
*IF RLUSSCFM=RLUFSS                                                0406 00054000
*  & (MGLREQ='1'B | RHFORMAT='1'B)                                 0406 00055000
*  THEN CALL ISTINCF1;                                             0406 00056000
         L     @04,NCSPLRPT(,CS1R1)                                0406 00057000
         CLI   RLUSSCFM(@04),X'01'                                 0406 00058000
         BNE   @RF00406                                            0406 00059000
         L     @04,NCSMGLPT(,CS1R1)                                0406 00060000
         TM    MGLREQ(@04),B'10000000'                             0406 00061000
         BO    @RT00406                                            0406 00062000
         TM    RHFORMAT(@04),B'00001000'                           0406 00063000
         BNO   @RF00406                                            0406 00064000
@RT00406 DS    0H                                                  0407 00065000
         L     @15,@CV02566                                        0407 00066000
