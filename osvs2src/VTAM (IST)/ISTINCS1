SVTM     TITLE 'ISTINCS1 FEEDBACK STATUS ROUTER                        *00001000
                        '                                               00002000
ISTINCS1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCS1  75.129'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*  DO;                                                             0383 00014000
*/***  CRRMAINT  PROCNM(ISTINCS1)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00015000
**/                                                                0384 00016000
*                                                                  0384 00017000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0384 00018000
*  RFY ISTLCCW ;                                                   0384 00019000
*  RFY ISTLCPB ;                                                   0385 00020000
