*                                      /* ICNCB FMCB (REQ. TAG       */ 00286000
*                                      /* OVERLAY)                   */ 00287000
*                6 LCPZRTL5 BIT(1),    /* DECREMENT SCHED COUNT ON DEQ* 00288000
*            4 LCPZRT06 BIT(1),        /* CRITICAL TEXT BLOCK        */ 00289000
*                                      /* FLAG (REQ. TAG OVERLAY)    */ 00290000
*              5 LCPZRTL6 BIT(1),      /* WRITE REQUEST              */ 00291000
*            4 LCPZRT07 BIT(1),        /* BUFFER TRACE ACTIVE        */ 00292000
*                                      /* FLAG (REQ. TAG OVERLAY)    */ 00293000
*              5 LCPZRTL7 BIT(1),      /* SCHEDULING FLAG            */ 00294000
*          3 LCPMNRCD CHAR(1),         /* LCP MINOR REJECTION CODE   */ 00295000
*            4 LCPZRT08 BIT(1),        /* I/O TRACE ACTIVE FLAG      */ 00296000
*                                      /* (REQ. TAG OVERLAY)         */ 00297000
*              5 LCPZRTL8 BIT(1),      /* DEVICE END NOTIFICATION    */ 00298000
*            4 LCPZRT09 BIT(1),        /* DIALOGUE SCHED DISABLED    */ 00299000
*              5 LCPZRTL9 BIT(1),      /* CRITICAL TEXT FLAG         */ 00300000
*            4 LCPZRT0A BIT(1),        /* SIMULATED CONTACT FLAG     */ 00301000
*            4 LCPZRT0B BIT(1),        /* SIMULATED DISCONNECT FLAG  */ 00302000
*        2 LCPCHAIN PTR(31),           /* APS CHAIN                  */ 00303000
*          3 LCPCHAI1 PTR(8),                                      0440 00304000
*            4 LCPCHNG BIT(1),         /* GATE BIT                   */ 00305000
*          3 LCPCHAI2 PTR(24),         /* ADDRESS FIELD              */ 00306000
*        2 LCPRUCNT PTR(8),            /* NUMBER OF RU FOR THIS LCPB */ 00307000
*          3 LCPZMIRC CHAR(1),         /* MINOR RETURN CODE          */ 00308000
*        2 LCPFLAG2 CHAR(1),           /* SECOND FLAG BYTE           */ 00309000
*          3 LCPPRFLG BIT(1),          /* PACING REQUIRED            */ 00310000
*          3 LCPASYN  BIT(1),          /* ASYNCHRONOUS LCPB          */ 00311000
*          3 LCPRSTRT BIT(1),          /* RESTART LCPB               */ 00312000
*          3 LCPTLBPI BIT(1),          /* TPIOS BLOCK LCCW           */ 00313000
*            4 LCPZRIMM BIT(1),        /* RESET IMMED REQUIRED ON ERR*/ 00314000
*          3 LCPFID0  BIT(1),          /* FID0 TH INDICATOR FOR RECORD* 00315000
*            4 LCPEXRES BIT(1),        /* EXCEP. RESPONSE SPEC. 3270R*/ 00316000
*          3 LCPDSTAT BIT(1),          /* DIALOGUE STATE BIT, 3270R  */ 00317000
*                                      /* 0=REQ. REJECT,1=STATE ERROR*/ 00318000
*          3 LCPFREE  BIT(1),          /* DEALLOCATE LCPB 3270R      */ 00319000
*          3 LCPATOP  BIT(1),          /* ATOP LCPB  3270R           */ 00320000
*        2 LCPSEQ   FIXED(16),         /* OUTBOUND SEQUENCE NUMBER   */ 00321000
*          3 LCPRRCCT PTR(8),          /* COUNT OF RESPONSES RECEIVED*/ 00322000
*          3 LCPRRCNT PTR(8),          /* COUNT OF RESPONSES REQUESTED* 00323000
*        2 LCPLCCW1 PTR(31),           /* ADDR FIRST LCCW FOR FID1   */ 00324000
*        2 LCPCID CHAR(4) BDY(WORD),   /* COMMUNICATION ID           */ 00325000
*          3 LCPSAF BIT(16),           /* SOURCE IDENTIFICATION      */ 00326000
*          3 LCPDAF BIT(16),           /* DESTINATION IDENTIFICATION */ 00327000
*        2 LCPNXLCW PTR(31),           /* ADDR NEXT LCCW             */ 00328000
*        2 LCPFDBA PTR(31),            /* BTU ADDRESS                */ 00329000
*          3 LCPFDB1 PTR(8),                                       0440 00330000
