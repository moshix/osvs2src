         TITLE 'ISTINCTR - TRACE INITIALIZATION                        *00001000
                        '                                               00002000
ISTINCTR CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTINCTR  75.323'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         ST    @13,@SA00001+4                                   01S0001 00014000
         LA    @14,@SA00001                                     01S0001 00015000
         ST    @14,8(,@13)                                      01S0001 00016000
         LR    @13,@14                                          01S0001 00017000
*WRKELMNT = WRKELMNT&&WRKELMNT;/*INIT CTR'S WORK ELMNT*/        01S0451 00018000
         XC    WRKELMNT(100),WRKELMNT                           01S0451 00019000
*CTRPARMS = CTRPARMS&&CTRPARMS;/*INIT TRACE PARM COPY AREA*/    01S0452 00020000
         XC    CTRPARMS(24),CTRPARMS                            01S0452 00021000
*SWITCHES = SWITCHES&&SWITCHES;        /* RESET ALL FLAGS*/     01S0453 00022000
         XC    SWITCHES(2),SWITCHES                             01S0453 00023000
*RFY R2 RSTD;                                                   01S0454 00024000
*                                                               01S0454 00025000
*                                                               01S0454 00026000
* DO; /* LISATCVT CVTBASE(PAGEABLE,(R2)) */                     01S0455 00027000
*   R2 = ATCLCPTR; /*SET UP ATCVT POINTER*/                     01S0456 00028000
         L     R2,ATCLCPTR                                      01S0456 00029000
* END; /*LISATCVT*/     /* VTAM CVT ADDRESSABILITY   */         01S0457 00030000
*                                                               01S0457 00031000
*CONID = CONID && CONID;               /* CLEAR CONSOLE ID   @X05CKZA*/ 00032000
         XC    CONID(9),CONID                                   01S0458 00033000
*VTAMCVTA = R2;                        /* SAVE VTAM CVT PTR           * 00034000
         ST    R2,VTAMCVTA                                      01S0459 00035000
*BPDPTR = ATCBPDA;                     /* BUF POOL DIR ADDR */  01S0460 00036000
