*RFY R2 UNRSTD;                                                 01S0461 00040000
*IF R1 > 0                             /* NCSPL PTR TEST              * 00041000
*  THEN DO;                            /* ENTERED FOR MODIFY COMMAND  * 00042000
         LTR   R1,R1                                            01S0462 00043000
         BNP   @RF00462                                         01S0462 00044000
*                                                               02S0464 00045000
*/********************************************************************* 00046000
*/* INITIALIZE TO PROCESS MODIFY TRACE COMMAND                        * 00047000
*/*  1. SAVE NCSPLPTR FROM REG.1                                      * 00048000
*/*  2. SET SW TO INDICATE MODIFY TRACE                               * 00049000
*/*  3. COPY TRACE PARAMETERS FROM NCSPL                              * 00050000
*/*  4. SAVE ADDR OF NODE NAME COPY                                   * 00051000
*/*  5. SAVE CONSOLE ID                                      @X05CKZA * 00052000
*/********************************************************************* 00053000
*                                                               02S0464 00054000
*NCSPLPTR = R1;                        /* SAVE NCSPL PTR              * 00055000
         ST    R1,NCSPLPTR                                      02S0464 00056000
*RFY R1 UNRSTD;                        /* RELEASE REG.1               * 00057000
*MODSW = ON;                           /* SET MODIFY COMMAND SW       * 00058000
         OI    MODSW,B'10000000'                                02S0466 00059000
*IF NCSVBF05 = ON                                               02S0467 00060000
*  THEN LINETR = ON;                   /* LINE TRACE                  * 00061000
