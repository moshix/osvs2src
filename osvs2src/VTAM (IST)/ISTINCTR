         BNO   @RF00469                                         02S0469 00071000
         OI    IOTR,B'01000000'                                 02S0470 00072000
*         ELSE IF NCSVBF07 = ON                                 02S0471 00073000
*                THEN BUFRTR = ON;     /* BUFFER TRACE                * 00074000
