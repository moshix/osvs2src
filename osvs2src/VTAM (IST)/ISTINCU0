*DCL 1 ISTMGL BASED(NCSMGLPT),/* MESSAGE PARAMETER LIST              */ 03638000
*      2 MGLRH CHAR(LENGTH(ISTRH)),/* ORIG RH IF MGLREQ = 0          */ 03639000
*      2 MGLUSSFL CHAR(1),  /* FLAGS                                 */ 03640000
*        3 MGLREQ   BIT(1), /* 0 = SEND RESPONSE                     */ 03641000
*                           /* 1 = SEND REQUEST                      */ 03642000
*        3 MGLCMPAT BIT(1), /* COMPATIBLE LOGON                      */ 03643000
*        3 * BIT(6),        /* RESERVED                              */ 03644000
*      2 MGLRUPTR PTR(31),  /* ADDR OF REQ RU IF MGLREQ = 1          */ 03645000
*        3 MGLSEQNO FIXED(15),/* SEQUENCE NUMBER OF ORIG RU          */ 03646000
*        3 MGLSENSE CHAR(2),/* 0 TO SEND POSITIVE FME                */ 03647000
*/*  SET FOLLOWING SENSE BYTES USING ISTSRU CONSTANTS                */ 03648000
*          4 MGLNS1 CHAR(1),/* NON-ZERO SENSE DATA TO SEND           */ 03649000
