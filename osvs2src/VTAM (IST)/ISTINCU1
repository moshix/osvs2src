         BNL   @RF00104                                            0104 00095000
*    CUREG15 = CUZERO;                   /*RETURN CODE = ZERO*/    0105 00096000
         LR    CUREG15,@11                                         0105 00097000
*   ELSE                                 /*RH INVALID*/            0106 00098000
*    DO;                                 /*ERROR MSG*/             0106 00099000
         B     @RC00104                                            0106 00100000
@RF00104 DS    0H                                                  0107 00101000
*     CUREG15 = CUFOUR;                  /*CODE = INVALID*/        0107 00102000
         LA    CUREG15,4                                           0107 00103000
*     MGLNS1 = SRUFIE;                   /*REQUEST ERROR*/         0108 00104000
         L     @14,NCSMGLPT(,CUNCSPLR)                             0108 00105000
         MVI   MGLNS1(@14),X'10'                                   0108 00106000
*     MGLNS2 = SRUFUNC;                  /*FUNC UNKNOWN*/          0109 00107000
         MVI   MGLNS2(@14),X'03'                                   0109 00108000
*    END;                                /*END REQUEST ERROR*/     0110 00109000
* ELSE                                   /*RH NOT ONLY IN CHAIN*/  0111 00110000
*  DO;                                   /*NOT ONLY IN CHAIN*/     0111 00111000
