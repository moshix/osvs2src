         L     UDTTABPT,UDTTPT(,@10)                               0598 01292000
*     END;                          /* END NO USER TABLE             */ 01293000
*   ELSE                            /* THERE IS A USER TABLE         */ 01294000
*     UDTTABPT=NCSPLRPT->RLUUSSPT->UDTTPT;/* GET ITS ADDRESS         */ 01295000
*                                                                  0600 01296000
         B     @RC00594                                            0600 01297000
@RF00594 L     @10,NCSPLRPT(,NCSPLPTR)                             0600 01298000
         L     @10,RLUUSSPT(,@10)                                  0600 01299000
         L     UDTTABPT,UDTTPT(,@10)                               0600 01300000
*   /*****************************************************************/ 01301000
*   /*                                                               */ 01302000
*   /* SEGMENT(TRANS)REQUEST                                         */ 01303000
*   /*                                                               */ 01304000
*   /*****************************************************************/ 01305000
*                                                                  0601 01306000
*   DO;                                                            0601 01307000
@RC00594 DS    0H                                                  0602 01308000
*     GOTO TRANS;                                                  0602 01309000
         B     TRANS                                               0602 01310000
*TRANS#:                                                           0603 01311000
*   END;                            /* GO TO TRANSLATE AND CHECK     */ 01312000
TRANS#   DS    0H                                                  0604 01313000
*   RFY                                                            0604 01314000
*    (RET15,                                                       0604 01315000
*     GPR03F) RSTD;                                                0604 01316000
*   GPR03F=RET15;                   /* SAVE RETURN CODE              */ 01317000
         LR    GPR03F,RET15                                        0605 01318000
*   IF RET15=ZERO                   /* TRANS AND CHECK GOOD          */ 01319000
*     THEN                                                         0606 01320000
         LTR   RET15,RET15                                         0606 01321000
         BZ    @RT00606                                            0606 01322000
*     ;                             /* YES                           */ 01323000
*   ELSE                                                           0608 01324000
*     DO;                           /* NO THERE WAS AN ERROR         */ 01325000
*       RFY                                                        0609 01326000
*         RET15 UNRSTD;                                            0609 01327000
*       MGLREQ=OFF;                 /* INDICATE RESPONSE             */ 01328000
         L     @10,NCSMGLPT(,NCSPLPTR)                             0610 01329000
         NI    MGLREQ(@10),B'01111111'                             0610 01330000
*       MGLNS1=SRUFIE;              /* SET FUNC INTERPRET ERROR      */ 01331000
         MVI   MGLNS1(@10),X'10'                                   0611 01332000
*       MGLNS2=SRURUDE;             /* SET RU DATA ERROR             */ 01333000
         MVI   MGLNS2(@10),X'01'                                   0612 01334000
*       MGLVDATA=ZERO;              /* NO VARIABLE TEXT              */ 01335000
         SLR   @02,@02                                             0613 01336000
         ST    @02,MGLVDATA(,@10)                                  0613 01337000
*       MGLMSGAD=ADDR(MSG1);        /* PT TO MSG NUMBER              */ 01338000
         LA    @02,@CB02305                                        0614 01339000
         ST    @02,MGLMSGAD(,@10)                                  0614 01340000
*       GPR01P=NCSPLPTR;            /* PT TO NCSPL                   */ 01341000
         LR    GPR01P,NCSPLPTR                                     0615 01342000
*       CALL ISTINCS1;              /* CALL CS1 TO SEND MSG          */ 01343000
         L     @15,@CV03464                                        0616 01344000
         BALR  @14,@15                                             0616 01345000
*       DO;                                                        0617 01346000
*                                                                  0617 01347000
*/***  CRRMAINT  PROCNM(ISTINCU2)COMP(VY)FUNC(RET)RPHADD((NCSPLRPH))  * 01348000
*                                                                    */ 01349000
*                                                                  0618 01350000
*         /***********************************************************/ 01351000
*         /*                                                         */ 01352000
*         /* ** CLEAR AND SET CURRENT PROCEDURE BIT                  */ 01353000
*         /*                                                         */ 01354000
