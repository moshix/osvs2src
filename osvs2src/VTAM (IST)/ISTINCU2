*     GPR03F UNRSTD;                                               0625 01378000
*                                                                  0626 01379000
*   /*****************************************************************/ 01380000
*   /*                                                               */ 01381000
*   /* SEGMENT(TRANS)START                                           */ 01382000
*   /*                                                               */ 01383000
*   /*****************************************************************/ 01384000
*                                                                  0626 01385000
*   DO;                                                            0626 01386000
*     GOTO TRANS@;                                                 0627 01387000
*TRANS:                                                            0628 01388000
*   END;                                                           0628 01389000
*                                                                  0628 01390000
TRANS    DS    0H                                                  0629 01391000
*/********************************************************************/ 01392000
*/*                                                                  */ 01393000
*/*  SUBRTN TRANS  THIS SETCTION SCANS THE INPUT STRING.  IT CHECKS  */ 01394000
*/*              FOR ESC CHARS FOLLOWED BY SSR CHAR(F4 HEX), IT      */ 01395000
*/*              CHECKS FOR END OF ESCAPE SEQUENCE (IRS HEX 1E) AND  */ 01396000
*/*              IT CHECKS FOR QUOTES.  IF IT FINDS ESC WITHOUT SSR  */ 01397000
*/*              A SYNTAX ERROR CODE IS PUT INTO REG 15.  IT FLIPS   */ 01398000
*/*              THE QUOTE BIT FOR EACH QUOTE  ANY OF THESE CHARS    */ 01399000
*/*              ARE INDEXED PAST AND THE NEXT CHAR IS CHECKED IF    */ 01400000
*/*              IT IS NOT ONE OF THESE SUBRTN CHARCK IS CALLED TO   */ 01401000
*/*              CHECK FOR A VALID CHAR.  IF AN INVALID CHAR IS      */ 01402000
*/*              FOUND IN CHARCK THIS ROUTINE GETS OUT OF THE SCAN   */ 01403000
*/*              AND RETURNS TO NAINLINE WITH A RETCODE IN REG 15    */ 01404000
*/*              IF THE END OF THE STRING IS FOUND AND ANY SWS ARE   */ 01405000
*/*              STILL ON A RETURN CODE OF SYNTAX ERROR IS SET AND   */ 01406000
*/*              CONTROL IS RETURNED TO MAINLINE.  OTHERWISE A RET   */ 01407000
*/*              CODE OF ZERO IS RETURNED.                           */ 01408000
*/*                                                                  */ 01409000
*/********************************************************************/ 01410000
*                                                                  0629 01411000
*   RFY                                                            0629 01412000
*    (CU2INPTR,                                                    0629 01413000
*     CPWREG,                                                      0629 01414000
*     RET15) RSTD;                                                 0629 01415000
*   CPWREG=NCSCPWPT;                /* GET ADDR OF CPWA              */ 01416000
         L     CPWREG,NCSCPWPT(,NCSPLPTR)                          0630 01417000
*   CU2INPTR=ADDR(NCSRUITS);        /* PT TO STRING                  */ 01418000
         L     CU2INPTR,NCSPLFSB(,NCSPLPTR)                        0631 01419000
         LA    CU2INPTR,NCSRUITS(,CU2INPTR)                        0631 01420000
*   CU2RMLNG=CPWILN;                /* GET LENGTH OF INPUT   @DM14029*/ 01421000
         LH    CU2RMLNG,CPWILN(,CPWREG)                            0632 01422000
*   CPWLCL=0;                       /* CLEAR SWITCHES                */ 01423000
         SLR   @14,@14                                             0633 01424000
         STH   @14,CPWLCL(,CPWREG)                                 0633 01425000
*   RET15=CPWSUC;                   /* INIT RET CODE TO ZERO         */ 01426000
*                                                                  0634 01427000
         LR    RET15,@14                                           0634 01428000
*   /*****************************************************************/ 01429000
*   /*                                                               */ 01430000
*   /* SCAN UNTIL END OF STRING OR ERROR                             */ 01431000
*   /*                                                               */ 01432000
*   /*****************************************************************/ 01433000
*                                                                  0635 01434000
*   DO CU2RMLNG TO ONE BY-1 WHILE RET15=CPWSUC;                    0635 01435000
         B     @DE00635                                            0635 01436000
@DL00635 LTR   RET15,RET15                                         0635 01437000
         BNZ   @DC00635                                            0635 01438000
*     IF CPWSEL=CPWONE              /* WAS LAST CHAR ESC     @D32CKKG*/ 01439000
*       THEN                                                       0636 01440000
         TM    CPWSEL(CPWREG),B'10000000'                          0636 01441000
         BNO   @RF00636                                            0636 01442000
*       DO;                         /* YES                           */ 01443000
*         IF THISCHAR='50'X         /* IS THIS CHAR SSR      @D32CKKG*/ 01444000
*           THEN                                                   0638 01445000
         CLI   THISCHAR(CU2INPTR),X'50'                            0638 01446000
         BNE   @RF00638                                            0638 01447000
*           DO;                     /* YES                           */ 01448000
*             CPWSEL=CPWZRO;        /* RESET SEL SW          @D32CKKG*/ 01449000
*             CPWSSR=CPWONE;        /* INDICATE SSR FOUND            */ 01450000
         OI    CPWSSR(CPWREG),B'01000000'                          0641 01451000
         NI    CPWSEL(CPWREG),B'01111111'                          0641 01452000
*           END;                    /* END SSR CHAR                  */ 01453000
*         ELSE                      /* ESC NOT FOLLOWED BY SSR       */ 01454000
*           RET15=CPWERR;           /* SET SYNTAX ERROR              */ 01455000
         B     @RC00638                                            0643 01456000
@RF00638 LA    RET15,8                                             0643 01457000
*       END;                        /* END LAST CHAR ESC             */ 01458000
*     ELSE                                                         0645 01459000
*       DO;                         /* NOT ESC CHAR                  */ 01460000
         B     @RC00636                                            0645 01461000
@RF00636 DS    0H                                                  0646 01462000
*         IF THISCHAR='04'X         /* IS THIS CHAR ESC              */ 01463000
*           THEN                    /* YES                           */ 01464000
         CLI   THISCHAR(CU2INPTR),X'04'                            0646 01465000
         BNE   @RF00646                                            0646 01466000
*           CPWSEL=CPWONE;          /* SET SEL SW            @D32CKKG*/ 01467000
         OI    CPWSEL(CPWREG),B'10000000'                          0647 01468000
*         ELSE                                                     0648 01469000
*           DO;                     /* NOT ESC                       */ 01470000
         B     @RC00646                                            0648 01471000
@RF00646 DS    0H                                                  0649 01472000
*             IF CPWSSR=CPWONE      /* IS SSR SW ON                  */ 01473000
*               THEN                                               0649 01474000
         TM    CPWSSR(CPWREG),B'01000000'                          0649 01475000
         BNO   @RF00649                                            0649 01476000
*               DO;                 /* YES                           */ 01477000
*                 IF THISCHAR='1E'X /* IS THIS CHAR IRS              */ 01478000
*                   THEN            /* YES END OF SSR DATA           */ 01479000
         CLI   THISCHAR(CU2INPTR),X'1E'                            0651 01480000
         BNE   @RF00651                                            0651 01481000
*                   CPWSSR=CPWZRO;  /* RESET SSR SW                  */ 01482000
         NI    CPWSSR(CPWREG),B'10111111'                          0652 01483000
*                 ELSE                                             0653 01484000
*                   ;               /* NOT IRS CHAR                  */ 01485000
@RF00651 DS    0H                                                  0654 01486000
*               END;                /* END SSR ON                    */ 01487000
*             ELSE                                                 0655 01488000
*               DO;                 /* NOT SSR SW ON                 */ 01489000
         B     @RC00649                                            0655 01490000
@RF00649 DS    0H                                                  0656 01491000
*                 IF THISCHAR='7D'X /* IS THIS CHAR A QUOTE          */ 01492000
*                   THEN                                           0656 01493000
         CLI   THISCHAR(CU2INPTR),X'7D'                            0656 01494000
         BNE   @RF00656                                            0656 01495000
*                   DO;             /* YES QUOTE                     */ 01496000
*                     IF CPWQUO=ON  /* QUOTE SW ON                   */ 01497000
*                       THEN        /* YES SW ON                     */ 01498000
         TM    CPWQUO(CPWREG),B'00100000'                          0658 01499000
         BNO   @RF00658                                            0658 01500000
*                       CPWQUO=OFF; /* TURN IT OFF                   */ 01501000
         NI    CPWQUO(CPWREG),B'11011111'                          0659 01502000
*                     ELSE          /* NOT ON                        */ 01503000
*                       CPWQUO=ON;  /* TURN IT ON                    */ 01504000
         B     @RC00658                                            0660 01505000
@RF00658 OI    CPWQUO(CPWREG),B'00100000'                          0660 01506000
*                   END;            /* END THIS CAHR QUOTE           */ 01507000
*                 ELSE              /* NOT SPECIAL CHAR              */ 01508000
*                                                                  0662 01509000
*                   /*************************************************/ 01510000
*                   /*                                               */ 01511000
*                   /* SEGMENT(CHARCK)REQUEST                        */ 01512000
*                   /*                                               */ 01513000
*                   /*************************************************/ 01514000
*                                                                  0662 01515000
*                   DO;                                            0662 01516000
         B     @RC00656                                            0662 01517000
@RF00656 DS    0H                                                  0663 01518000
*                     GOTO CHARCK;                                 0663 01519000
         B     CHARCK                                              0663 01520000
*CHARCK#:                                                          0664 01521000
*                   END;            /* CALL CK RTN                   */ 01522000
CHARCK#  DS    0H                                                  0665 01523000
*               END;                /* END NOT SSR CODE              */ 01524000
@RC00656 DS    0H                                                  0666 01525000
*           END;                    /* END NOT ESC CODE              */ 01526000
@RC00649 DS    0H                                                  0667 01527000
*       END;                        /* END NOT ESC ON                */ 01528000
@RC00646 DS    0H                                                  0668 01529000
*     CU2INPTR=CU2INPTR+1;          /* PT TO NEXT CHAR               */ 01530000
@RC00636 AL    CU2INPTR,@CF00039                                   0668 01531000
*   END;                            /* END DO LOOP                   */ 01532000
         BCTR  CU2RMLNG,0                                          0669 01533000
@DE00635 LTR   CU2RMLNG,CU2RMLNG                                   0669 01534000
         BP    @DL00635                                            0669 01535000
@DC00635 DS    0H                                                  0670 01536000
*   CPWQUO=OFF;                     /* RESET QUOTE SWITCH AFTER ALL     01537000
*                                      INPUT TRANSLATED @DM14439F9   */ 01538000
         NI    CPWQUO(CPWREG),B'11011111'                          0670 01539000
*   IF CPWLCL=0                     /* ANY SWITCHES LEFT ON          */ 01540000
*     THEN                                                         0671 01541000
         LH    @14,CPWLCL(,CPWREG)                                 0671 01542000
         N     @14,@CF03483                                        0671 01543000
         LTR   @14,@14                                             0671 01544000
         BZ    @RT00671                                            0671 01545000
*     ;                             /* NO                            */ 01546000
*   ELSE                            /* YES                           */ 01547000
*     RET15=CPWERR;                 /* SET SYNTAX ERROR              */ 01548000
         LA    RET15,8                                             0673 01549000
*   CPWRTC=RET15;                   /* SET RETURN CODE IN CPWA       */ 01550000
*                                                                  0674 01551000
@RT00671 STC   RET15,CPWRTC(,CPWREG)                               0674 01552000
*   /*****************************************************************/ 01553000
*   /*                                                               */ 01554000
*   /* SEGMENT(TRANS)FINISH                                          */ 01555000
*   /*                                                               */ 01556000
*   /*****************************************************************/ 01557000
*                                                                  0675 01558000
*   DO;                                                            0675 01559000
*     GOTO TRANS#;                                                 0676 01560000
         B     TRANS#                                              0676 01561000
*TRANS@:                                                           0677 01562000
*   END;                                                           0677 01563000
TRANS@   DS    0H                                                  0678 01564000
*                                                                  0678 01565000
*   /*****************************************************************/ 01566000
*   /*                                                               */ 01567000
*   /* SEGMENT(CHARCK)START                                          */ 01568000
*   /*                                                               */ 01569000
*   /*****************************************************************/ 01570000
*                                                                  0678 01571000
*   DO;                                                            0678 01572000
*     GOTO CHARCK@;                                                0679 01573000
         B     CHARCK@                                             0679 01574000
*CHARCK:                                                           0680 01575000
*   END;                                                           0680 01576000
*                                                                  0680 01577000
CHARCK   DS    0H                                                  0681 01578000
*/********************************************************************/ 01579000
*/*                                                                  */ 01580000
*/*     SUBRTN CHARCK   THE SECTION CHECKS FOR A VALID INPUT CHAR    */ 01581000
*/*                 AND SET SYNTAX ERROR IF NOT.  IF INPUT CHAR IS   */ 01582000
*/*                 VALID IT CHECKS TO SEE IF QUOTE SW IS ON IF SO   */ 01583000
*/*                 IT RETURNS TO TRANS WITHOUT TRANSLATION IF NOT   */ 01584000
*/*                 THE CHAR IS TRANSLATED USING THE TABLE FOUND IN  */ 01585000
*/*                 MAINLINE.  THE TRANSLATED CHAR IS CHECKED FOR    */ 01586000
*/*                 A VALID VALUE IF NOT A SYNTAX ERROR IS RETURNED  */ 01587000
*/*                 OTHERWISE CONTROL IS RETURNED TO TRANS WITH THE  */ 01588000
*/*                 RETURN CODE UNCHANGED.                           */ 01589000
*/*                                                                  */ 01590000
*/********************************************************************/ 01591000
*                                                                  0681 01592000
*   RFY                                                            0681 01593000
*     GPR02F RSTD;                                                 0681 01594000
*   GPR02F=ZERO;                    /* SET REG 2 TO ZERO             */ 01595000
         SLR   GPR02F,GPR02F                                       0682 01596000
*   TRT(THISCHAR,CU2UDTAB);         /* TRANS CHAR USING MY TAB       */ 01597000
         TRT   THISCHAR(1,CU2INPTR),CU2UDTAB                       0683 01598000
*   IF GPR02F=ZERO                  /* VALID CHAR                    */ 01599000
*     THEN                          /* NO                            */ 01600000
         LTR   GPR02F,GPR02F                                       0684 01601000
         BNZ   @RF00684                                            0684 01602000
*     RET15=CPWERR;                 /* SET SYNTAX ERROR              */ 01603000
         LA    RET15,8                                             0685 01604000
*   ELSE                                                           0686 01605000
*     DO;                           /* YES INPUT VALID               */ 01606000
         B     @RC00684                                            0686 01607000
@RF00684 DS    0H                                                  0687 01608000
*       IF CPWQUO=CPWONE            /* IN QUOTED STRING              */ 01609000
*         THEN                                                     0687 01610000
         TM    CPWQUO(CPWREG),B'00100000'                          0687 01611000
         BO    @RT00687                                            0687 01612000
*         ;                         /* YES DO NOT TRANSLATE          */ 01613000
*       ELSE                                                       0689 01614000
*         DO;                       /* NOT QUOTED STRING             */ 01615000
*           TR(THISCHAR,UDTTAB);    /* TRANS USING MAINLINE TABLE    */ 01616000
         TR    THISCHAR(1,CU2INPTR),UDTTAB(UDTTABPT)               0690 01617000
*           GPR02F=ZERO;            /* RESET REG 2                   */ 01618000
         SLR   GPR02F,GPR02F                                       0691 01619000
*           TRT(THISCHAR,CU2UDTAB); /* CHECK IF OUTPUT CHAR VALID    */ 01620000
         TRT   THISCHAR(1,CU2INPTR),CU2UDTAB                       0692 01621000
*           IF GPR02F=ZERO          /* VALID CHAR                    */ 01622000
*             THEN                  /* NO                            */ 01623000
         LTR   GPR02F,GPR02F                                       0693 01624000
         BNZ   @RF00693                                            0693 01625000
*             RET15=CPWERR;         /* SET SYNTAX ERROR              */ 01626000
         LA    RET15,8                                             0694 01627000
*           ELSE                                                   0695 01628000
*             ;                     /* YES A VALID OUTPUT CHAR       */ 01629000
@RF00693 DS    0H                                                  0696 01630000
*         END;                      /* END NOT QUOTED STRING         */ 01631000
*     END;                          /* END NOT INVALID INPUT         */ 01632000
*                                                                  0697 01633000
@RT00687 DS    0H                                                  0698 01634000
*   /*****************************************************************/ 01635000
*   /*                                                               */ 01636000
*   /* SEGMENT(CHARCK)FINISH                                         */ 01637000
*   /*                                                               */ 01638000
*   /*****************************************************************/ 01639000
*                                                                  0698 01640000
