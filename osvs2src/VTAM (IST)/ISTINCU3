*           3  ATCRPT30 PTR(31),       /* RESERVED                   */ 02814000
*           3  ATCRPT31 PTR(31),       /* RESERVED                   */ 02815000
*           3  ATCRPT32 PTR(31),       /* RESERVED                   */ 02816000
*           3  ATCRPT33 PTR(31),       /* RESERVED                   */ 02817000
*           3  ATCRPT34 PTR(31),       /* RESERVED                   */ 02818000
*           3  ATCRPT35 PTR(31),       /* RESERVED                   */ 02819000
*           3  ATCRPT36 PTR(31),       /* RESERVED                   */ 02820000
*           3  ATCRPT37 PTR(31),       /* RESERVED                   */ 02821000
*           3  ATCRPT38 PTR(31),       /* RESERVED                   */ 02822000
*           3  ATCRPT39 PTR(31),       /* RESERVED                   */ 02823000
*           3  ATCRPT40 PTR(31),       /* RESERVED                   */ 02824000
*/* END OF VS1/VS2 COMMON CODE.                                      */ 02825000
*/* START OF VS2 UNIQUE CODE.                                        */ 02826000
*           3  ATCVTFLG BIT(32),                                   0726 02827000
*              4 ATCVTACT BIT(1),      /* VTT ACTIVE                 */ 02828000
*              4 *        BIT(31),     /* RESERVED                   */ 02829000
*           3  ATCHLTCB PTR(31),       /* ECB FOR HALTING VTAM       */ 02830000
*           3  ATCATA10 PTR(31),       /* PTR TO ISTATM10            */ 02831000
*           3  ATCZLAAB PTR(31),       /* PTR TO ISTZLFAB            */ 02832000
*           3  ATCCACXA PTR(31),       /* PTR TO ISTCFCXA            */ 02833000
*           3  ATCSMARA PTR(31),       /* PTR TO ISTORFRA            */ 02834000
*           3  ATCSMAID PTR(31),       /* PTR TO ISTORFID            */ 02835000
*           3  ATCTPWQA PTR(31),       /* WAIT Q ANCHOR FOR TPIOS    */ 02836000
*           3  ATCSMAPR PTR(31),       /* PTR TO ISTORFPR            */ 02837000
*           3  ATCSMABF PTR(31),       /* PTR TO ISTORFBF            */ 02838000
*           3  ATCPSTA  PTR(31),       /* ACTIVE PST QUEUE           */ 02839000
*           3  ATCTRMPB CHAR(LENGTH(ISTDYPAB)) BDY(DWORD),         0726 02840000
*                                      /* PAB FOR VTAM TERM TASK     */ 02841000
*           3  ATCTPSPB CHAR(LENGTH(ISTDYPAB)) BDY(DWORD),         0726 02842000
*                                      /* PAB FOR TPIOS              */ 02843000
*           3  ATCAP23  PTR(31),       /* PTR TO ISTAPC23            */ 02844000
*           3  ATCAPC25 PTR(31),       /* PTR TO ISTAPC25            */ 02845000
*           3  ATCAP41  PTR(31),       /* PTR TO ISTAPC41            */ 02846000
*           3  ATCRCFY0 PTR(31),       /* PTR TO ISTRCFY0            */ 02847000
*           3  ATCAP55  PTR(31),       /* PTR TO ISTAPC55            */ 02848000
*           3  ATCAP56  PTR(31),       /* PTR TO ISTAPC56            */ 02849000
*           3  ATCAPC59 PTR(31),       /* PTR TO ISTAPC59            */ 02850000
*           3  ATCAP62  PTR(31),       /* PTR TO ISTAPC62            */ 02851000
*           3  ATCAP66  PTR(31),       /* PTR TO ISTAPC66            */ 02852000
*           3  ATCAP69  PTR(31),       /* PTR TO ISTAPM69            */ 02853000
*           3  ATCAP82  PTR(31),       /* PTR TO ISTAPC82            */ 02854000
*           3  ATCAP83  PTR(31),       /* PTR TO ISTAPC83            */ 02855000
*           3  ATCAP84  PTR(31),       /* PTR TO ISTAPC84            */ 02856000
*           3  ATCAP85  PTR(31),       /* PTR TO ISTAPC85            */ 02857000
*           3  ATCAP86  PTR(31),       /* PTR TO ISTAPC86            */ 02858000
*           3  ATCASCB  PTR(31),       /* PTR TO VTAM ASCB           */ 02859000
*           3  ATCMPLTA  PTR(31),      /* PTR TO MPST LIST TABLE     */ 02860000
*           3  ATCZR01  PTR(31),       /* PTR TO ISTZRM01            */ 02861000
*           3  ATCAICTN PTR(31),       /* PTR TO ISTAICTN            */ 02862000
*           3  ATCTPCP0 PTR(31),       /* PTR TO ISTZFSAV            */ 02863000
*           3  ATCTPCP1 PTR(31),       /* PTR TO ISTZ19SV            */ 02864000
*           3  ATCZFLAG BIT(8),        /* FLAG BYTE                  */ 02865000
*              4 ATCZF001 BIT(1),      /* MDR RECORDING SUSPENDED    */ 02866000
*              4 ATCRSV66 BIT(7),      /* RESERVED                   */ 02867000
*           3  ATCRSV75 BIT(8),        /* RESERVED                   */ 02868000
*           3  ATCSRBCT FIXED(16),     /* COUNT OF PREALLOCATED SRB'S*/ 02869000
*           3  ATCCRACT FIXED(16),     /* COUNT OF PREALLOCATED CRA'S*/ 02870000
*           3  ATCRPLCT FIXED(16),     /* COUNT OF PREALLOCATED CRPL */ 02871000
*           3  ATCTPMPB CHAR(LENGTH(ISTDYPAB)) BDY(DWORD), /*TP MSG PAB 02872000
*                                                         FOR VS2    */ 02873000
*           3  ATCTPMMI PTR(31),        /* PTR TO ISTCFMMI - MSG ISSUER 02874000
*                                          FOR SRB MODE ROUTINES     */ 02875000
*           3  ATCVIOPM PTR(31),       /* PTR TO ISTVIOPM            */ 02876000
*           3  ATCPOPAB CHAR(LENGTH(ISTDYPAB)) BDY(DWORD),         0726 02877000
*                       /* POPAB FOR REPLY PROCESSING, POI TERMINATION* 02878000
*                       /* AND VTAM MESSAGE PROCESSOR                 * 02879000
*        2  ATCAOS CHAR(0) BDY(WORD);  /* VS2 ONLY FIELDS            */ 02880000
*DCL  1  ISTRH     BDY(WORD) BASED,      /* REQUEST HEADER           */ 02881000
*       2  RHFLAGS  CHAR(3),             /* REQUEST HEADER FLAGS     */ 02882000
*        3  RH1    BIT(8),               /* FIRST RH BYTE            */ 02883000
*           4  RHQS     BIT(1),          /* 0-REQUEST,1-RESPONSE     */ 02884000
*           4  RHTYPE   BIT(1),          /* 0-DATA,1-CONTROL         */ 02885000
*           4  RHSCI    BIT(1),          /* 0-FM,1-SYSTEM CONTROL    */ 02886000
