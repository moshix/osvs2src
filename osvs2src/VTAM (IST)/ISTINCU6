*IF MGLREQ = '0'B                      /* IF MGL FOR 'SEND RESPONSE' */ 00109000
* THEN DO;                             /* CHECK IF RESPONSE NEEDED   */ 00110000
         TM    MGLREQ(CU6MGL),B'10000000'                       02S0517 00111000
         BNZ   @RF00517                                         02S0517 00112000
*  RFY ISTRH BASED(ADDR(MGLRH));       /* MAP RH IN ISTMGL           */ 00113000
*  IF RHFME = '1'B                     /* IF FME RESPONSE WANTED     */ 00114000
*   THEN                                                        02S0520 00115000
