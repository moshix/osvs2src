* SRUSENS2 = SRUSENS2 && SRUSENS2;     /* NO USER SENSE IN RESPONSE  */ 00157000
         XC    SRUSENS2(2,@10),SRUSENS2(@10)                    03S0532 00158000
* NCSPLOLN = LENGTH(SRUSENSE);         /* OUTBOUND RU LENGTH         */ 00159000
