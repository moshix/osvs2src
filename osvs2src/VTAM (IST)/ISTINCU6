         LR    @15,GPR02P                                       02S0767 00776000
         N     @15,@CF00072                                     02S0767 00777000
         LTR   @15,@15                                          02S0767 00778000
         BZ    @RT00767                                         02S0767 00779000
*          ELSE                                                 02S0769 00780000
*             DO;                                               02S0769 00781000
*                IF GPR02P->GPBIPT->CPGLEN^=GPR01P->CPGLEN|     02S0770 00782000
*                   GPR02P->GPBIPT->CPGSTRNG(1:GPR01P->CPGLEN)^=GPR01P- 00783000
