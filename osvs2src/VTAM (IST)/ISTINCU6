         L     @07,RPHCRR(,@07)                                 03S0494 00048000
         L     @07,CRACRR(,@07)                                 03S0494 00049000
         OI    NINCU6(@07),B'00001000'                          03S0494 00050000
*  /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                03S0495 00051000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                       03S0495 00052000
         XC    VYCCRT+1(11,@07),VYCCRT+1(@07)                   03S0495 00053000
