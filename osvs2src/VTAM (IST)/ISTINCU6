SVTM     TITLE 'ISTINCU6-VTAM USS STATUS FEEDBACK PROCESSOR            *00001000
                        '                                               00002000
ISTINCU6 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTINCU6  75.231'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
*/********************************************************************/ 00014000
*/*                                                                  */ 00015000
*/* ISTINCU6 MAINLINE PROCESSING -                                   */ 00016000
*/*                                                                  */ 00017000
*/********************************************************************/ 00018000
*CU6NCSPL = CU6INPUT;                  /* SET NCSPL BASE FROM INPUT  */ 00019000
         LR    CU6NCSPL,CU6INPUT                                02S0480 00020000
*RFY ISTNCSPL BASED(CU6NCSPL);                                  02S0481 00021000
*/********************************************************************/ 00022000
*/*                                                                  */ 00023000
*/* UPDATE COMPONENT RECOVERY RECORD AUDIT TRAIL TO INDICATE THAT    */ 00024000
*/* SUBROUTINE ISTINCU6 HAS BEEN ENTERED.                            */ 00025000
*/*                                                                  */ 00026000
*/********************************************************************/ 00027000
*  DO;                                                          03S0482 00028000
*/***  CRRMAINT  PROCNM(ISTINCU6)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00029000
**/                                                             03S0483 00030000
*                                                               03S0483 00031000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */               03S0483 00032000
*  RFY ISTLCCW ;                                                03S0483 00033000
*  RFY ISTLCPB ;                                                03S0484 00034000
