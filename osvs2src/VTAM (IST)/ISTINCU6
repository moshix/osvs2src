*NCSPLRPH->RPHCRR->CRACRR->CINCU6 = '1'B;                       03S0496 00055000
         OI    CINCU6(@07),B'00001000'                          03S0496 00056000
*  END;                                                         03S0497 00057000
*/********************************************************************/ 00058000
*/*                                                                  */ 00059000
*/* PERFORM STANDARD SSCP ROUTINE ENTRY LINKAGE. ESTABLISH CURRENT   */ 00060000
*/* ROUTINE'S SAVE AREA IN THE NCSPL WORK AREA BY DECREMENTING THE   */ 00061000
*/* ADDRESS OF THE CALLER'S SAVE AREA BY THE LENGTH OF THE SAVE AREA.*/ 00062000
*/* DO FORWARD AND BACKWARD CHAINING OF SAVE AREAS.                  */ 00063000
*/*                                                                  */ 00064000
*/********************************************************************/ 00065000
*DCL 1 SSCPSAVE BASED,                 /* SAVE AREA OVERLAY          */ 00066000
*      2 * FIXED(31),                                           03S0498 00067000
*      2 SSCPBPTR PTR(31),             /* BACK POINTER               */ 00068000
*      2 SSCPFPTR PTR(31),             /* FORWARD POINTER            */ 00069000
*      2 *(15) FIXED(31);              /* SAVE AREA REGS 14-12       */ 00070000
*DCL SSCPNXAV PTR(31) BASED(NCSPLWKA); /* NEXT AVAILABLE POINTER     */ 00071000
*                                      /* IN NCSPL WORK AREA         */ 00072000
*DCL SSCPSAVR PTR(31) REG(13) RSTD;    /* SAVE AREA POINTER          */ 00073000
*RFY SSCPWORK RSTD;                                             03S0501 00074000
*SSCPWORK = SSCPSAVR;                  /* PRESERVE SAVE AREA PTR     */ 00075000
         LR    SSCPWORK,SSCPSAVR                                03S0502 00076000
*SSCPSAVR = SSCPSAVR-LENGTH(SSCPSAVE); /* SAVE AREA BASE             */ 00077000
         SL    SSCPSAVR,@CF00777                                03S0503 00078000
*SSCPWORK->SSCPFPTR=SSCPSAVR;          /* SET CALLER'S FORWARD PTR   */ 00079000
         ST    SSCPSAVR,SSCPFPTR(,SSCPWORK)                     03S0504 00080000
*SSCPSAVR->SSCPBPTR=SSCPWORK;          /* SET BACK POINTER           */ 00081000
         ST    SSCPWORK,SSCPBPTR(,SSCPSAVR)                     03S0505 00082000
*RFY SSCPWORK UNRSTD;                                           03S0506 00083000
*SSCPSAVR->SSCPFPTR=0;                 /* ZERO FORWARD POINTER       */ 00084000
         SLR   @07,@07                                          03S0507 00085000
         ST    @07,SSCPFPTR(,SSCPSAVR)                          03S0507 00086000
*RFY CU6WKA RSTD;                                               02S0508 00087000
*CU6WKA = SSCPNXAV;                    /* NEXT AVAILABLE WORK AREA   */ 00088000
         L     @07,NCSPLWKA(,CU6NCSPL)                          02S0509 00089000
         L     CU6WKA,SSCPNXAV(,@07)                            02S0509 00090000
*CU6WKA=(CU6WKA+SEVEN)&MINUS8;         /* SET ON DWORD BDY   @D32CKXK*/ 00091000
