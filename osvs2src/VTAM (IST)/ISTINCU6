         BNE   @RF00752                                         02S0752 00742000
*       DO;                                                     02S0753 00743000
*CPGA0034:                                                      02S0754 00744000
*          GPR00P=GPR02P;                                       02S0754 00745000
CPGA0034 LR    GPR00P,GPR02P                                    02S0754 00746000
*          GPR02P=GPR02P->GPBFWD;                               02S0755 00747000
         L     GPR02P,GPBFWD(,GPR02P)                           02S0755 00748000
*          IF (GPR02P&'7FFFFFFF'X)='00000000'X                  02S0756 00749000
*          THEN;                                                02S0756 00750000
         LR    @15,GPR02P                                       02S0756 00751000
         N     @15,@CF00072                                     02S0756 00752000
         LTR   @15,@15                                          02S0756 00753000
         BZ    @RT00756                                         02S0756 00754000
*          ELSE                                                 02S0758 00755000
*             DO;                                               02S0758 00756000
*                IF GPR02P->GPBFLG='1'B                         02S0759 00757000
*                THEN                                           02S0759 00758000
         TM    GPBFLG(GPR02P),B'10000000'                       02S0759 00759000
