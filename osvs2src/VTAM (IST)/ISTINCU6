*                   GO TO CPGA0034;                             02S0760 00761000
*                ELSE;                                          02S0761 00762000
*             END;                                              02S0762 00763000
*       END;                                                    02S0763 00764000
*    ELSE                                                       02S0764 00765000
*       DO;                                                     02S0764 00766000
         B     @RC00752                                         02S0764 00767000
@RF00752 DS    0H                                               02S0765 00768000
*CPGB0034:                                                      02S0765 00769000
*          GPR00P=GPR02P;                                       02S0765 00770000
CPGB0034 LR    GPR00P,GPR02P                                    02S0765 00771000
*          GPR02P=GPR02P->GPBFWD;                               02S0766 00772000
         L     GPR02P,GPBFWD(,GPR02P)                           02S0766 00773000
