         ST    @00,MGLMSGAD(,CU6MGL)                            03S0562 00253000
*  MGLVDATA = 0;                       /* NO VARIABLE DATA   @D32CKLB*/ 00254000
         ST    @10,MGLVDATA(,CU6MGL)                            03S0563 00255000
*  CPWSPT = 0;                         /* NO VERB SYNONYM    @D32CKLB*/ 00256000
