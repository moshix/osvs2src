         PRINT OFF                                                      00123000
*/*   PROGRAM OPERATOR MESSAGE HEADER                                */ 00124000
*DCL  1  ISTPOHD  BDY(WORD) BASED,                                 0084 00125000
*        2 POHRSVD1 CHAR(1),       /* RESERVED                       */ 00126000
*        2 POHSTAT,                /* HEADER STATUS BITS             */ 00127000
*          3 POHEND   BIT(1),      /* END LINE 1=YES                 */ 00128000
*          3 POHDATA  BIT(1),      /* DATA LINE 1=YES                */ 00129000
*          3 POHLBL   BIT(1),      /* LABEL LINE 1=YES               */ 00130000
*          3 POHCNTRL BIT(1),      /* CONTROL LINE 1=YES             */ 00131000
*          3 POHRSVD2 BIT(2),      /* RESERVED                       */ 00132000
*          3 POHRREQ  BIT(1),      /* REPLY REQUESTED 1=YES          */ 00133000
*          3 POHGEN   BIT(1),      /* ID ORIGIN  0=GENERATED BY VTAM */ 00134000
*                                  /*            1=GENERATED BY POA  */ 00135000
*        2 POHID    FIXED(16);     /* MESSAGE IDENTIFIER             */ 00136000
*/*            VTAM PREFIX FOR QUEUE ELEMENTS                         * 00137000
*/*  INITIAL LEVEL                                     001 03/04/75  */ 00138000
*DECLARE                                                           0085 00139000
*  1 ISTPFX  BDY(WORD)  BASED,                                     0085 00140000
*   2 PFXID        CHAR(4),             /* QUEUE ACRONYM (SAME AS  0085 00141000
*                                          QABID)                     * 00142000
*     3 PFXCBID    BIT(8),              /* CONTROL BLOCK ID           * 00143000
*   2 PFXPRTY      CHAR(1),             /* PRIORITY OF ELEMENT        * 00144000
*   2 PFXLNG       CHAR(3),             /* ELEMENT LENGTH,         0085 00145000
*                                          INCLUDING PREFIX           * 00146000
*   2 PFXFWD       PTR(31),   /* POINTER TO NEXT QUEUE             0085 00147000
*                                          ELEMENT (IF LAST        0085 00148000
*                                          POINTER TO QAB)            * 00149000
*   2 PFXBCK       PTR(31) ;   /* POINTER TO PREVIOUS              0085 00150000
*                                          QUEUE ELEMENT (IF       0085 00151000
*                                          FIRST, POINTER TO QAB)     * 00152000
*DCL NCSPLTYP BIT(8) CONSTANT('60'X);   /* NCSPL CONTROL BLOCK ID     * 00153000
*   /* NETWORK CONFIGURATION SERVICES CONTROL BLOCK  */            0087 00154000
*/*  REVISIONS                                         14 07/01/75  */  00155000
*DECLARE                                                           0087 00156000
