         TITLE 'ISTINCU7 - USS 3270 OUTPUT MEDIA CONTROL               *00001000
                        '                                               00002000
ISTINCU7 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCU7  78.110'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*/********************************************************************/ 00014000
*/*                 STANDARD LINKAGE                                 */ 00015000
*/********************************************************************/ 00016000
*                                                                  0055 00017000
* DO;                                                              0055 00018000
*/***  CRRMAINT  PROCNM(ISTINCU7)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00019000
**/                                                                0056 00020000
*                                                                  0056 00021000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0056 00022000
* RFY ISTLCCW ;                                                    0056 00023000
* RFY ISTLCPB ;                                                    0057 00024000
* RFY ISTZSAVE;                                                    0058 00025000
* RFY ISTPFCRR;                                                    0059 00026000
* RFY ISTCFCRR;                                                    0060 00027000
* RFY ISTZCRR ;                                                    0061 00028000
* RFY ISTASCRR;                                                    0062 00029000
* RFY ISTAPCRR;                                                    0063 00030000
* RFY ISTCRA  ;                                                    0064 00031000
* RFY ISTCNSTA;                                                    0065 00032000
* RFY ISTVYCRR;                                                    0066 00033000
*/* END OF ATBUILD */                                              0067 00034000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0067 00035000
*NCSPLRPH->RPHCRR->CRACRR->NINCU7 = '1'B;                          0067 00036000
         L     @15,NCSPLRPH(,CUNCSPLR)                             0067 00037000
         L     @15,RPHCRR(,@15)                                    0067 00038000
         L     @15,CRACRR(,@15)                                    0067 00039000
         OI    NINCU7(@15),B'00000100'                             0067 00040000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0068 00041000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0068 00042000
         XC    VYCCRT+1(11,@15),VYCCRT+1(@15)                      0068 00043000
         MVI   VYCCRT(@15),B'00000000'                             0068 00044000
*NCSPLRPH->RPHCRR->CRACRR->CINCU7 = '1'B;                          0069 00045000
         OI    CINCU7(@15),B'00000100'                             0069 00046000
* END;                                                             0070 00047000
*CUFWDPTR = CUSAVERG - 72;               /*SAVE AREA CHAIN*/       0071 00048000
         LA    @15,72                                              0071 00049000
         LR    @00,CUSAVERG                                        0071 00050000
         SLR   @00,@15                                             0071 00051000
         ST    @00,CUFWDPTR(,CUSAVERG)                             0071 00052000
*CUSAVERG = CUSAVERG - 72;               /*PT TO OWN SAVE AREA*/   0072 00053000
         SLR   CUSAVERG,@15                                        0072 00054000
*CUBCKPTR = CUSAVERG + 72;               /*COMPLETE CHAING*/       0073 00055000
         ALR   @15,CUSAVERG                                        0073 00056000
         ST    @15,CUBCKPTR(,CUSAVERG)                             0073 00057000
*CUFWDPTR = 0;                           /*ZERO OWN FWD PTR*/      0074 00058000
         SLR   @00,@00                                             0074 00059000
         ST    @00,CUFWDPTR(,CUSAVERG)                             0074 00060000
*/********************************************************************/ 00061000
*/*                 STANDARD LINKAGE END                             */ 00062000
*/********************************************************************/ 00063000
*                                                                  0075 00064000
*/********************************************************************/ 00065000
*/*       THIS CODE ADDS THE CONTROL CHARS APPROPIATE FOR PRESENTING */ 00066000
*/*       THE MESSAGE ON A 3270 DISPLAY.                             */ 00067000
*/*       THE CONTROL CHARS ARE AS FOLLOWS;                          */ 00068000
*/*            LEADING                                               */ 00069000
*/*            'F5'X ERASE/WRITE CMD                                 */ 00070000
*/*            'C2'X WRITE CONTROL CHAR -RESTORE KEYBOARD            */ 00071000
*/*            '1D'X START FIELD ORDER                               */ 00072000
*/*            '40'X ATTRIBUT CHAR                           @ZA27731*/ 00073000
*/*            TRAILING                                              */ 00074000
*/*            '11' SET BUFFER ADDRESS                       @ZA12764*/ 00075000
*/*            'C4' =LINE 9, CHAR. 40 (ON 40X20 DEVICE), OR  @ZA12764*/ 00076000
*/*            '7F' =LINE 5, CHAR. 80 (ON 80X20 DEVICE)      @ZA12764*/ 00077000
*/*            '1D' START FIELD ORDER                                */ 00078000
*/*            '40' ATTRIBUT CHAR                            @ZA27731*/ 00079000
*/*            '13' INSERT CURSOR ORDER                              */ 00080000
*/********************************************************************/ 00081000
*CUMSGTXT(NCSPLOLN+1:NCSPLOLN+7) = '11C47F1D4013'X;        /*@ZA27731*/ 00082000
         LH    @15,NCSPLOLN(,CUNCSPLR)                             0075 00083000
         L     @14,NCSPLRUO(,CUNCSPLR)                             0075 00084000
         ALR   @14,@15                                             0075 00085000
         MVI   CUMSGTXT+6(@14),X'00'                               0075 00086000
         MVC   CUMSGTXT(6,@14),@CB00770                            0075 00087000
*                        /* NEW TRAILING CONTROL CHARACTERS  @ZA12764*/ 00088000
*NCSPLOLN = NCSPLOLN + 10;               /*INCLUDE ALL CHAR IN LEN*/    00089000
         AL    @15,@CF00772                                        0076 00090000
         STH   @15,NCSPLOLN(,CUNCSPLR)                             0076 00091000
*                                        /*                  @ZA12764*/ 00092000
*DO CUINDEX=NCSPLOLN BY -1 TO 5;         /*SHIFT RIGHT*/           0077 00093000
         LR    CUINDEX,@15                                         0077 00094000
         B     @DE00077                                            0077 00095000
@DL00077 DS    0H                                                  0078 00096000
* CUMSGTXT(CUINDEX)=CUMSGTXT(CUINDEX-4);/*SHIFT RIGHT*/            0078 00097000
         L     @02,NCSPLRUO(,CUNCSPLR)                             0078 00098000
         LR    @15,@02                                             0078 00099000
         ALR   @15,CUINDEX                                         0078 00100000
         AL    @15,@CF00773                                        0078 00101000
         ALR   @02,CUINDEX                                         0078 00102000
         AL    @02,@CF02946                                        0078 00103000
         MVC   CUMSGTXT(1,@15),CUMSGTXT(@02)                       0078 00104000
*END;                                                              0079 00105000
         BCTR  CUINDEX,0                                           0079 00106000
@DE00077 C     CUINDEX,@CF00774                                    0079 00107000
         BNL   @DL00077                                            0079 00108000
*CUMSGTXT(1:4) = 'F5C21D40'X;            /*LEADING CONTROL   @ZA27731*/ 00109000
         L     @15,NCSPLRUO(,CUNCSPLR)                             0080 00110000
         MVC   CUMSGTXT(4,@15),@CB00775                            0080 00111000
*CUSAVERG = CUSAVERG + 72;               /*RESTORE CALLER'S SVAREA @*/  00112000
         AL    CUSAVERG,@CF00767                                   0081 00113000
*RETURN ;                                                          0082 00114000
*                                                                  0082 00115000
@EL00001 DS    0H                                                  0082 00116000
@EF00001 DS    0H                                                  0082 00117000
@ER00001 LM    @14,@12,12(@13)                                     0082 00118000
         BR    @14                                                 0082 00119000
*/* TPEPILOG   */                                                  0083 00120000
