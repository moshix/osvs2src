*   2 NCSPLVCD    BIT(8),         /*VERB CODE                        */ 00163000
*   2 NCSPLVBF    CHAR(2),        /*VERB MODIFIER FLAGS              */ 00164000
*      3 NCSVBF01 BIT(1),         /* UNUSED                          */ 00165000
*      3 NCSVBF02 BIT(1),         /* UNUSED                          */ 00166000
*      3 NCSVBF03 BIT(1),         /* MODIFY MSG SUP                  */ 00167000
*      3 NCSVBF04 BIT(1),         /* TYPE=VTAM                       */ 00168000
*      3 NCSVBF05 BIT(1),         /* LINETRACE=1                     */ 00169000
*      3 NCSVBF06 BIT(1),         /* I/O TRACE=1                     */ 00170000
*      3 NCSVBF07 BIT(1),         /* BUFFER TRACE=1                  */ 00171000
*      3 NCSVBF08 BIT(1),         /* MODIFY TPRINT                   */ 00172000
*      3 NCSVBF09 BIT(1),         /* MODIFY TEST                     */ 00173000
*      3 NCSVBF10 BIT(1),         /* MODIFY TRACE=NO                 */ 00174000
*      3 NCSVBF11 BIT(1),         /* VARY INTERNAL ONLY & MODIFY     */ 00175000
*                                 /*  TRACE = YES                    */ 00176000
*      3 NCSVBF12 BIT(1),         /* VARY IMMEDIATE  & MODIFY        */ 00177000
*                                 /*  NETSOL=NO                      */ 00178000
*      3 NCSVBF13 BIT(1),         /* MODIFY NETSOL=YES               */ 00179000
*      3 NCSVBF14 BIT(1),         /* VARY LOGON & MODIFY             */ 00180000
*                                 /*  CHANGE                         */ 00181000
*      3 NCSVBF15 BIT(1),         /* VARY DEACTIVATE & VARY NORMAL   */ 00182000
*                                 /*  CLOSE(ERP)                     */ 00183000
*      3 NCSVBF16 BIT(1),         /* VARY ACTIVATE & ERP REQUEST     */ 00184000
*                                 /*   & MODIFY DUMP                 */ 00185000
*   2 NCSPLVFL    CHAR(1),        /*COMMAND FLAGS                    */ 00186000
*    3 NCSVNCSA   BIT(1),         /*NCSPL IS ACTIVE                  */ 00187000
*    3 NCSVDEL    BIT(1),         /*DELETE NCSPL                     */ 00188000
*    3 NCSVSTAT   BIT(1),         /*INDICATE NCSPL WAS NOT GETMAINED */ 00189000
*    3 NCSDCONT   BIT(1),         /* DO DISCONACT                    */ 00190000
*    3 NCSABCON   BIT(1),         /* DO ABANDON CONNECTION           */ 00191000
*    3 NCSDACLK   BIT(1),         /* DO ACTIVATE LINE                */ 00192000
*    3 NCSRSV02   BIT(2),         /* RESERVED                        */ 00193000
*   2 NCSPLVF1    CHAR(1),        /* SPECIAL PROC FLAGS              */ 00194000
*    3 NCSOVFMC   BIT(1),         /* PURGE ONLY VARY'S FMCB          */ 00195000
*   2 NCSPRMRC    CHAR(1),        /* PURGE MINOR RETURN CODE FIELD   */ 00196000
