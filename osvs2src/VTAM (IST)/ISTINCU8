*/* THIS CODE PUTS NEW LINE CHARS AT THE BEGINNING AND END           */ 00066000
*/* OF A MSG TEXT POINTED TO BY NCSPLRUO & UPDATES THE LENGTH        */ 00067000
*/********************************************************************/ 00068000
*NCSPLOLN = NCSPLOLN + 2;                /*INCREASE COUNT BY 2*/   0076 00069000
         LA    @00,2                                               0076 00070000
         AH    @00,NCSPLOLN(,CUNCSPLR)                             0076 00071000
         STH   @00,NCSPLOLN(,CUNCSPLR)                             0076 00072000
*DO CUINDEX=NCSPLOLN BY -1 TO 2;         /* RIGHT ONE*/            0077 00073000
         LR    CUINDEX,@00                                         0077 00074000
         B     @DE00077                                            0077 00075000
@DL00077 DS    0H                                                  0078 00076000
* CUMSGTXT(CUINDEX)= CUMSGTXT(CUINDEX-1); /*SHIFT*/                0078 00077000
         L     @02,NCSPLRUO(,CUNCSPLR)                             0078 00078000
         LR    @15,@02                                             0078 00079000
         ALR   @15,CUINDEX                                         0078 00080000
         AL    @15,@CF00761                                        0078 00081000
         ALR   @02,CUINDEX                                         0078 00082000
         AL    @02,@CF02883                                        0078 00083000
         MVC   CUMSGTXT(1,@15),CUMSGTXT(@02)                       0078 00084000
*END;                                                              0079 00085000
         BCTR  CUINDEX,0                                           0079 00086000
@DE00077 C     CUINDEX,@CF00074                                    0079 00087000
         BNL   @DL00077                                            0079 00088000
*CUMSGTXT(1)  = '15'X;                   /*PUT NEW LINE CHAR AT BEGIN*/ 00089000
         L     @15,NCSPLRUO(,CUNCSPLR)                             0080 00090000
         MVI   CUMSGTXT(@15),X'15'                                 0080 00091000
*CUMSGTXT(NCSPLOLN) = '15'X;             /*PUT NEW LINE CHAR AT END */  00092000
         LH    @14,NCSPLOLN(,CUNCSPLR)                             0081 00093000
         ALR   @15,@14                                             0081 00094000
         AL    @15,@CF00761                                        0081 00095000
         MVI   CUMSGTXT(@15),X'15'                                 0081 00096000
*CUSAVERG = CUSAVERG + 72;               /*RESTORE CALLER'S SVAREA @*/  00097000
         AL    CUSAVERG,@CF00759                                   0082 00098000
*RETURN ;                               /*RETURN WITH R15 = CPWRTC*/    00099000
