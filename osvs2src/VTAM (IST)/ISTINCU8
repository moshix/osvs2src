* END;                                                             0071 00047000
*CUFWDPTR = CUSAVERG - 72;               /*SAVE AREA CHAIN*/       0072 00048000
         LA    @15,72                                              0072 00049000
         LR    @00,CUSAVERG                                        0072 00050000
         SLR   @00,@15                                             0072 00051000
         ST    @00,CUFWDPTR(,CUSAVERG)                             0072 00052000
