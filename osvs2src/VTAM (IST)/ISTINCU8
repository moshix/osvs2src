         TITLE 'ISTINCU8 - USS STAND CHAR OUTPUT MEDIA CONTROL         *00001000
                        '                                               00002000
ISTINCU8 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCU8  75.129'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*/********************************************************************/ 00014000
*/*                 STANDARD LINKAGE                                 */ 00015000
*/********************************************************************/ 00016000
*                                                                  0056 00017000
* DO;                                                              0056 00018000
*/***  CRRMAINT  PROCNM(ISTINCU8)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00019000
**/                                                                0057 00020000
*                                                                  0057 00021000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0057 00022000
* RFY ISTLCCW ;                                                    0057 00023000
* RFY ISTLCPB ;                                                    0058 00024000
* RFY ISTZSAVE;                                                    0059 00025000
* RFY ISTPFCRR;                                                    0060 00026000
* RFY ISTCFCRR;                                                    0061 00027000
* RFY ISTZCRR ;                                                    0062 00028000
* RFY ISTASCRR;                                                    0063 00029000
* RFY ISTAPCRR;                                                    0064 00030000
* RFY ISTCRA  ;                                                    0065 00031000
* RFY ISTCNSTA;                                                    0066 00032000
* RFY ISTVYCRR;                                                    0067 00033000
*/* END OF ATBUILD */                                              0068 00034000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0068 00035000
*NCSPLRPH->RPHCRR->CRACRR->NINCU8 = '1'B;                          0068 00036000
         L     @15,NCSPLRPH(,CUNCSPLR)                             0068 00037000
         L     @15,RPHCRR(,@15)                                    0068 00038000
         L     @15,CRACRR(,@15)                                    0068 00039000
         OI    NINCU8(@15),B'00000010'                             0068 00040000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0069 00041000
