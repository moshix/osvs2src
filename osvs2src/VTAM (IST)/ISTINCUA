         B     @RC00187                                            0189 00297000
@RF00187 DS    0H                                                  0190 00298000
*         CPWTYP = CPWNPP;               /*POSIT PARMS NOT PERMITTED*/  00299000
         OI    CPWTYP(CUCPWPTR),B'01000000'                        0190 00300000
*DO;                                     /*UPDINPUT*/              0191 00301000
*CPWILN = CPWILN - CU1;                  /*DECREMENT INPUT LENGTH*/     00302000
         LH    @04,CPWILN(,CUCPWPTR)                               0192 00303000
         BCTR  @04,0                                               0192 00304000
         STH   @04,CPWILN(,CUCPWPTR)                               0192 00305000
*CPWIPT = CPWIPT + CUONE;                /*INCREMENT INPUT POINTER*/    00306000
         LA    @00,1                                               0193 00307000
         AL    @00,CPWIPT(,CUCPWPTR)                               0193 00308000
         ST    @00,CPWIPT(,CUCPWPTR)                               0193 00309000
*IF CPWILN = CUZEROBT                   /*END OF INPUT*/           0194 00310000
* THEN                                                             0194 00311000
         LTR   @04,@04                                             0194 00312000
         BNZ   @RF00194                                            0194 00313000
*  CPWEOR = CUON;                        /*END OF RECORD SW ON*/   0195 00314000
         OI    CPWEOR(CUCPWPTR),B'00100000'                        0195 00315000
* ELSE;                                  /*LENGTH NOT ZERO*/       0196 00316000
@RF00194 DS    0H                                                  0197 00317000
*END;                                    /*END UPDINPUT*/          0197 00318000
*         CUREG1 = CUNCSPLR;             /*R1 = ADDR OF NCSPL*/    0198 00319000
         LR    CUREG1,CUNCSPLR                                     0198 00320000
