         TITLE 'ISTINCUA - USS VERB && KEYWORD PROCESSOR               *00001000
                         '                                              00002000
ISTINCUA CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCUA  76.257'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
* DO;                                                              0100 00014000
*/***  CRRMAINT  PROCNM(ISTINCUA)COMP(VY)FUNC(SUB)RPHADD((CUREG1->NCSPL 00015000
*PH))  ***/                                                        0101 00016000
*                                                                  0101 00017000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0101 00018000
* RFY ISTLCCW ;                                                    0101 00019000
* RFY ISTLCPB ;                                                    0102 00020000
* RFY ISTZSAVE;                                                    0103 00021000
* RFY ISTPFCRR;                                                    0104 00022000
* RFY ISTCFCRR;                                                    0105 00023000
* RFY ISTZCRR ;                                                    0106 00024000
* RFY ISTASCRR;                                                    0107 00025000
* RFY ISTAPCRR;                                                    0108 00026000
* RFY ISTCRA  ;                                                    0109 00027000
* RFY ISTCNSTA;                                                    0110 00028000
* RFY ISTVYCRR;                                                    0111 00029000
*/* END OF ATBUILD */                                              0112 00030000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0112 00031000
*CUREG1->NCSPLRPH->RPHCRR->CRACRR->NINCUA = '1'B;                  0112 00032000
         L     @04,NCSPLRPH(,CUREG1)                               0112 00033000
         L     @04,RPHCRR(,@04)                                    0112 00034000
         L     @04,CRACRR(,@04)                                    0112 00035000
