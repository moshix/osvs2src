         BNE   @RF00127                                            0127 00089000
*  DO;                                   /*PROCESS GETVERB*/       0128 00090000
*/********************************************************************/ 00091000
*/*  FUNCTION                                                        */ 00092000
*/*       THIS CODE GETS THE VERB FROM THE INPUT USS COMMAND. CPWIPT */ 00093000
*/*       CONTAINS THE ADDRESS OF THE CMD AND CPWILN CONTAINS THE    */ 00094000
*/*       LENGTH                                                     */ 00095000
*/*  PROCEDURES CALLED                                               */ 00096000
*/*       BLKSUP - SUPPRESS BLANKS                                   */ 00097000
*/*       NAMECKEK - OBTAIN AND VALIDATE THE VERB                    */ 00098000
*/********************************************************************/ 00099000
*   CPWGBL = CUZEROHL;                   /*CLEAR GLOBAL SW*/       0129 00100000
         STH   @04,CPWGBL(,CUCPWPTR)                               0129 00101000
*   CALL BLKSUP;                         /*SUPPRESS LEADING BLANKS*/    00102000
         BAL   @14,BLKSUP                                          0130 00103000
*   IF CPWEOR = CUON                     /*IF END OF RECORD*/      0131 00104000
*    THEN                                                          0131 00105000
         TM    CPWEOR(CUCPWPTR),B'00100000'                        0131 00106000
         BNO   @RF00131                                            0131 00107000
*     CPWRTC = CPWEOV;                   /*CODE = END OF VLAUE*/   0132 00108000
         MVI   CPWRTC(CUCPWPTR),X'0C'                              0132 00109000
*    ELSE                                /*NOT END OF RECORD*/     0133 00110000
*     DO;                                /*GET THE NAME*/          0133 00111000
         B     @RC00131                                            0133 00112000
@RF00131 DS    0H                                                  0134 00113000
*      CUMAX = CU8;                      /*MAX LN OF NAME = 8*/    0134 00114000
         LA    CUMAX,8                                             0134 00115000
*      CALL NAMECHEK;                    /*CHECK FOR VALID NAME*/  0135 00116000
         BAL   @14,NAMECHEK                                        0135 00117000
*      IF CPWRTC = CPWUCH                /*IF NXT CHAR UNRECOG*/   0136 00118000
*       THEN                                                       0136 00119000
         CLI   CPWRTC(CUCPWPTR),40                                 0136 00120000
         BNE   @RF00136                                            0136 00121000
*        IF CURCHAR = CUBLNK |           /*IF CHAR A BLANK*/       0137 00122000
*           CURCHAR = CUHTAB             /*OR HORIZ TAB*/          0137 00123000
*         THEN                                                     0137 00124000
         L     @04,CPWIPT(,CUCPWPTR)                               0137 00125000
         CLI   CURCHAR(@04),C' '                                   0137 00126000
         BE    @RT00137                                            0137 00127000
         CLI   CURCHAR(@04),B'00000101'                            0137 00128000
         BNE   @RF00137                                            0137 00129000
@RT00137 DS    0H                                                  0138 00130000
*          DO;                                                     0138 00131000
*           CALL BLKSUP;                 /*SUPPRESS BLANKS*/       0139 00132000
         BAL   @14,BLKSUP                                          0139 00133000
*           CPWRTC = CPWEOV;             /*CODE = END OF VALUE*/   0140 00134000
         MVI   CPWRTC(CUCPWPTR),X'0C'                              0140 00135000
*          END;                                                    0141 00136000
*         ELSE                           /*CHAR NOT BLANK OR HTAB*/     00137000
*          IF CURCHAR = CUSEMI |         /*IF CHAR A SEMICOLON*/   0142 00138000
*              CURCHAR = CULPAR          /*OR A LEFT PAREN*/       0142 00139000
*           THEN                                                   0142 00140000
         B     @RC00137                                            0142 00141000
@RF00137 L     @04,CPWIPT(,CUCPWPTR)                               0142 00142000
         CLI   CURCHAR(@04),C';'                                   0142 00143000
         BE    @RT00142                                            0142 00144000
         CLI   CURCHAR(@04),C'('                                   0142 00145000
         BNE   @RF00142                                            0142 00146000
@RT00142 DS    0H                                                  0143 00147000
*            DO;                                                   0143 00148000
*              CPWSPE = CUON;            /* FORMAT MUST BE PL1*/   0144 00149000
         OI    CPWSPE(CUCPWPTR),B'00000100'                        0144 00150000
*              CPWRTC = CPWEOV;          /*CODE = END OF VALUE*/   0145 00151000
         MVI   CPWRTC(CUCPWPTR),X'0C'                              0145 00152000
*            END;                                                  0146 00153000
*           ELSE                         /*CHAR NOT SMICLN OR LFT PAR*/ 00154000
*            CPWRTC = CPWERR;            /*CODE = SYNTAX ERROR*/   0147 00155000
         B     @RC00142                                            0147 00156000
@RF00142 MVI   CPWRTC(CUCPWPTR),X'08'                              0147 00157000
*       ELSE;                            /*CHAR NOT UNRECOG*/      0148 00158000
@RF00136 DS    0H                                                  0149 00159000
*     END;                               /*END GET THE NAME*/      0149 00160000
*  END;                                  /*END PROCESS GETVERB*/   0150 00161000
* ELSE                                   /*GET THE ENTERED PARMAMETER*/ 00162000
*  DO UNTIL(CPWRTC^=CPWCON);             /*PROCESS GETPARM*/       0151 00163000
         B     @RC00127                                            0151 00164000
@RF00127 DS    0H                                                  0151 00165000
@DL00151 DS    0H                                                  0152 00166000
*/********************************************************************/ 00167000
*/* FUNCTION                                                         */ 00168000
*/*       THIS CODE GETS THE NEXT PARAMETER FROM AN ENTERED USS CMD  */ 00169000
*/*       LOCAL SWITCHES ARE INITIALIZES AND THE PROPER ROUTINES ARE */ 00170000
*/*       INVOKED.                                                   */ 00171000
*/*  PROCEDURES CALLED                                               */ 00172000
*/*       PGTPARM - WILL GET THE NEXT PL/1 PARM                      */ 00173000
*/*       BGTPARM - WILL GET THE NEXT BAL PARM                       */ 00174000
*/********************************************************************/ 00175000
*   CPWSPE = CUOFF;                      /*RESET SPECIAL SW*/      0152 00176000
         NI    CPWSPE(CUCPWPTR),B'11111011'                        0152 00177000
*   IF CPWEOR = CUOFF                    /*IF NOT END OF RECORD*/  0153 00178000
*    THEN                                                          0153 00179000
         TM    CPWEOR(CUCPWPTR),B'00100000'                        0153 00180000
         BNZ   @RF00153                                            0153 00181000
*     IF CPWFMT = CPWBAL                 /*IF FORMAT BAL*/         0154 00182000
*      THEN                                                        0154 00183000
         TM    CPWFMT(CUCPWPTR),B'10000000'                        0154 00184000
         BNO   @RF00154                                            0154 00185000
*        /*SEGMENT(BGTPARM)REQUEST*/DO;GOTO BGTPARM;BGTPARM#:END;  0155 00186000
         B     BGTPARM                                             0156 00187000
*/*GET NEXT BAL PARM*/                                             0158 00188000
*      ELSE                              /*FORMAT IS PL/1*/        0158 00189000
*        /*SEGMENT(PGTPARM)REQUEST*/DO;GOTO PGTPARM;PGTPARM#:END;  0158 00190000
**GET NEXT PL/1 PARM*/                                             0158 00191000
         B     @RC00154                                            0158 00192000
@RF00154 DS    0H                                                  0159 00193000
         B     PGTPARM                                             0159 00194000
*    ELSE                                /*END OF RECORD SW ON*/   0161 00195000
*     CPWRTC = CPWEOC;                   /*RET CODE = END OF CMD*/ 0161 00196000
         B     @RC00153                                            0161 00197000
@RF00153 MVI   CPWRTC(CUCPWPTR),X'04'                              0161 00198000
*  END;                                  /*END PROCESS GETPARM*/   0162 00199000
@RC00153 DS    0H                                                  0162 00200000
@DE00151 CLI   CPWRTC(CUCPWPTR),44                                 0162 00201000
         BE    @DL00151                                            0162 00202000
*IF CPWRTC = CPWEOV                      /*IF CODE = END OF VALUE*/     00203000
* THEN                                                             0163 00204000
@RC00127 CLI   CPWRTC(CUCPWPTR),12                                 0163 00205000
         BNE   @RF00163                                            0163 00206000
*  IF CPWKLN = CUZEROBT                  /*IF KEYWORD LENGTH 0*/   0164 00207000
*   THEN                                                           0164 00208000
         CLI   CPWKLN(CUCPWPTR),0                                  0164 00209000
         BNE   @RF00164                                            0164 00210000
*    IF CPWVLN = CUZEROBT &              /*IF VALUE LENGTH = 0*/   0165 00211000
