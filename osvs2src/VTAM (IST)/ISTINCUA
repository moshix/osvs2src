*NAMECHEK: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0340 00710000
NAMECHEK DS    0H                                                  0341 00711000
*/********************************************************************/ 00712000
*/*  PROCEDURE NAMECHEK                                              */ 00713000
*/*  FUNCTION                                                        */ 00714000
*/*       THIS CODE BUILDS A NAME IN CPWKCH AND ENSURES THAT THE     */ 00715000
*/*       FIRST CHAR IS ALPHAMERIC AND THE NEXT N-1 CHARS ARE ALPHA- */ 00716000
*/*       NUMERIC. NAMECHEK ALSO ENSURES THAT THE NAME IS NOT >      */ 00717000
*/*       THAN N CHARS IN LENGTH (N IS SUPPLIED BY CALLER).          */ 00718000
*/********************************************************************/ 00719000
*DCL NAMCK14 PTR(31);                    /*SAVE AREA FOR R14*/     0341 00720000
*RFY CUREG14 RSTD;                                                 0342 00721000
*NAMCK14 = CUREG14;                      /*SAVE R14*/              0343 00722000
         LR    NAMCK14,CUREG14                                     0343 00723000
*RFY CUREG14 UNRSTD;                                               0344 00724000
*IF CUTRANTB(CURCHAR+1) = ALPHABET       /*IF FIRST CHAR ALPH*/    0345 00725000
* THEN                                                             0345 00726000
         L     @04,CPWIPT(,CUCPWPTR)                               0345 00727000
         SLR   @15,@15                                             0345 00728000
         IC    @15,CURCHAR(,@04)                                   0345 00729000
         SLR   @00,@00                                             0345 00730000
         IC    @00,CUTRANTB(@15)                                   0345 00731000
         C     @00,@CF00054                                        0345 00732000
         BNE   @RF00345                                            0345 00733000
*  DO;                                   /*CHECK REST OF NAME*/    0346 00734000
*   CPWRTC = CPWCON;                     /*CODE = CONTINUE*/       0347 00735000
         MVI   CPWRTC(CUCPWPTR),X'2C'                              0347 00736000
*   CPWKLN = CPWKLN + CU1;               /*INCREMENT OUTPUT LN*/   0348 00737000
         LA    @00,1                                               0348 00738000
         SLR   @15,@15                                             0348 00739000
         IC    @15,CPWKLN(,CUCPWPTR)                               0348 00740000
         ALR   @00,@15                                             0348 00741000
         STC   @00,CPWKLN(,CUCPWPTR)                               0348 00742000
*   CPWKCH(CPWKLN) = CURCHAR;            /*CHAR INTO OUTPUT AREA*/ 0349 00743000
