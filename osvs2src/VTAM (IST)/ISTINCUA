         AL    CUSAVERG,@CF00873                                   0174 00239000
*RETURN;                                 /*RETURN TO CALLER*/      0175 00240000
@EL00001 DS    0H                                                  0175 00241000
@EF00001 DS    0H                                                  0175 00242000
@ER00001 LM    @14,@12,12(@13)                                     0175 00243000
         BR    @14                                                 0175 00244000
* /*SEGMENT(BGTPARM)START*/DO;GOTO BGTPARM@;BGTPARM:END;           0176 00245000
BGTPARM  DS    0H                                                  0179 00246000
*/********************************************************************/ 00247000
*/* PROCEDURE     BGTPARM (CALLED BY MAINLINE)                       */ 00248000
*/*                                                                  */ 00249000
*/* FUNCTION      GET THE NEXT BAL PARAMETER                         */ 00250000
*/*       IF A KEYWORD IS FOUND, ISTINCUB IS CALLED TO OBTAIN THE    */ 00251000
*/*       VALUE.  IF A KEYWORD IS NOT FOUND, BPOSPARM IS CALLED TO   */ 00252000
*/*       CHECK FOR POSITIONAL PARARMETERS.                          */ 00253000
*/* PROCEDURES CALLED                                                */ 00254000
*/*       NAMECHEK - OBTAIN AND VALIDATE THE KEYWORD                 */ 00255000
*/*       BPOSPARM - PROCESS POSSIBLE POSITIONAL PARMS               */ 00256000
*/*       UPDINPUT - UPDATE INPUT PTRS AND LENGTH                    */ 00257000
*/*       ISTINCUB - WILL GET THE PARAMETER VALUE                    */ 00258000
*/********************************************************************/ 00259000
*CUSAVILN = CPWILN;                      /*SAVE THE INPUT LENGTH*/ 0179 00260000
         LH    CUSAVILN,CPWILN(,CUCPWPTR)                          0179 00261000
*CUSAVIPT = CPWIPT;                      /*SAVE THE INPUT PTR        */ 00262000
         L     CUSAVIPT,CPWIPT(,CUCPWPTR)                          0180 00263000
*CUMAX = CU7;                            /*MAX NAME IS 7 CHAR*/    0181 00264000
         LA    CUMAX,7                                             0181 00265000
*CALL NAMECHEK;                          /*CHECK FOR VALID NAME*/  0182 00266000
         BAL   @14,NAMECHEK                                        0182 00267000
*IF CPWRTC = CPWERR |                    /*IF SYNTAX ERROR*/       0183 00268000
*   CPWRTC = CPWEOV                      /*OR END OF VALUE*/       0183 00269000
* THEN                                                             0183 00270000
         CLI   CPWRTC(CUCPWPTR),8                                  0183 00271000
         BE    @RT00183                                            0183 00272000
         CLI   CPWRTC(CUCPWPTR),12                                 0183 00273000
         BNE   @RF00183                                            0183 00274000
@RT00183 DS    0H                                                  0184 00275000
*  CALL BPOSPARM;                         /*PROCESS POSS POSIT PARM*/   00276000
         BAL   @14,BPOSPARM                                        0184 00277000
* ELSE                                   /*NOT SYNTAX OR END OF VAL*/   00278000
*  IF CPWRTC = CPWUCH                    /*IF NEXT CHAR UNRECOG*/  0185 00279000
*   THEN                                                           0185 00280000
         B     @RC00183                                            0185 00281000
@RF00183 CLI   CPWRTC(CUCPWPTR),40                                 0185 00282000
         BNE   @RF00185                                            0185 00283000
*    IF CURCHAR = CUEQUAL                /*IF NEXT CHAR AN EQ SIGN*/    00284000
*     THEN                                                         0186 00285000
         L     @04,CPWIPT(,CUCPWPTR)                               0186 00286000
         CLI   CURCHAR(@04),C'='                                   0186 00287000
         BNE   @RF00186                                            0186 00288000
*      IF CPWKLN = CUZEROBT              /*IF KEYWORD LN ZERO*/    0187 00289000
*       THEN                                                       0187 00290000
         CLI   CPWKLN(CUCPWPTR),0                                  0187 00291000
