*BLKSUP: PROC OPTIONS(NOSAVE,NOSAVEAREA);                          0373 00830000
BLKSUP   DS    0H                                                  0374 00831000
*/********************************************************************/ 00832000
*/*       THIS CODE CHECKS THE NEXT CHAR POINTED TO BY CPWIPT FOR    */ 00833000
*/*       A BLANK OR A HORIZONTAL TAB. IF EITHER, CPWIPT IS INCREASED*/ 00834000
*/*       AND CPWILN IS DECREASED.                                   */ 00835000
*/********************************************************************/ 00836000
*RFY CUREG14 RSTD;                                                 0374 00837000
*IF CURCHAR = CUBLNK |                   /*IF CHAR BLANK*/         0375 00838000
*  CURCHAR = CUHTAB                      /*OR TAB*/                0375 00839000
* THEN                                                             0375 00840000
         L     @15,CPWIPT(,CUCPWPTR)                               0375 00841000
         CLI   CURCHAR(@15),C' '                                   0375 00842000
         BE    @RT00375                                            0375 00843000
         CLI   CURCHAR(@15),B'00000101'                            0375 00844000
         BNE   @RF00375                                            0375 00845000
@RT00375 DS    0H                                                  0376 00846000
*  DO;                                   /*DO BLANKS*/             0376 00847000
*   IF CPWSIG = CUON                     /*IF SIGNIFICANE STARTED ON*/  00848000
*    THEN                                                          0377 00849000
         TM    CPWSIG(CUCPWPTR),B'00000010'                        0377 00850000
         BNO   @RF00377                                            0377 00851000
*     DO;                                /*BLS ON*/                0378 00852000
*      CPWSIG = CUOFF;                   /*TURN IT OFF*/           0379 00853000
*      CPWBLS = CUON;                    /*TURN ON BLANKS SUPP SW*/     00854000
         OI    CPWBLS(CUCPWPTR),B'00000001'                        0380 00855000
         NI    CPWSIG(CUCPWPTR),B'11111101'                        0380 00856000
*     END;                               /*END BLS ON*/            0381 00857000
*    ELSE;                               /*SIG SW NOT ON*/         0382 00858000
@RF00377 DS    0H                                                  0383 00859000
*   DO WHILE(CPWILN>CUZEROBT&(CURCHAR=CUBLNK|CURCHAR=CUHTAB));/*LOOP*/  00860000
         B     @DE00383                                            0383 00861000
@DL00383 DS    0H                                                  0384 00862000
*    CPWIPT = CPWIPT + CUONE;            /*INCREASE INPUT PTR*/    0384 00863000
         LA    @15,1                                               0384 00864000
         AL    @15,CPWIPT(,CUCPWPTR)                               0384 00865000
         ST    @15,CPWIPT(,CUCPWPTR)                               0384 00866000
*    CPWILN = CPWILN - CU1;              /*DECREMENT INPUT LENGTH*/     00867000
         LH    @15,CPWILN(,CUCPWPTR)                               0385 00868000
         BCTR  @15,0                                               0385 00869000
         STH   @15,CPWILN(,CUCPWPTR)                               0385 00870000
*   END;                                 /*END DO WHILE*/          0386 00871000
