SVTM     TITLE 'ISTINCUB-VTAM USS GET PARAMETER VALUE                  *00001000
                        '                                               00002000
ISTINCUB CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTINCUB  76.257'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
*                                      /* OF INPUT STRING            */ 00014000
*/********************************************************************/ 00015000
*/*                                                                  */ 00016000
*/* ISTINCUB MAINLINE PROCESSING -                                   */ 00017000
*/*                                                                  */ 00018000
*/********************************************************************/ 00019000
*CUBNCSPL = CUBINPUT;                  /* SET NCSPL BASE FROM INPUT  */ 00020000
         LR    CUBNCSPL,CUBINPUT                                02S0232 00021000
*RFY ISTNCSPL BASED(CUBNCSPL);                                  02S0233 00022000
*/********************************************************************/ 00023000
*/*                                                                  */ 00024000
*/* UPDATE COMPONENT RECOVERY RECORD AUDIT TRAIL TO INDICATE THAT    */ 00025000
*/* SUBROUTINE ISTINCUB HAS BEEN ENTERED.                            */ 00026000
*/*                                                                  */ 00027000
*/********************************************************************/ 00028000
*  DO;                                                          03S0234 00029000
*/***  CRRMAINT  PROCNM(ISTINCUB)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00030000
**/                                                             03S0235 00031000
*                                                               03S0235 00032000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */               03S0235 00033000
*  RFY ISTLCCW ;                                                03S0235 00034000
