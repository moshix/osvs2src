         ALR   @11,QSCAN                                           0089 00237000
         CLI   CUQSDCS-1(@11),X'04'                                0089 00238000
         BNE   @RF00089                                            0089 00239000
*     DO;                                                          0090 00240000
*       CPWSSR=CPWONE;              /* SET MAG CARD DATA FLAG ON     */ 00241000
         OI    CPWSSR(CPWPTR),B'01000000'                          0091 00242000
*       QSCAN=QSCAN+CUQTWO;         /* INCREMENT SCAN PAST MAG CHAR  */ 00243000
         AL    QSCAN,@CF00091                                      0092 00244000
*     END;                                                         0093 00245000
*   ELSE                                                           0094 00246000
*     IF CUQSDCS(QSCAN:QSCAN)=CUQUOTE THEN  /* LEAVE DO INFINIT LOOP */ 00247000
*                                                                  0094 00248000
         B     @RC00089                                            0094 00249000
@RF00089 LR    @11,IPTREG                                          0094 00250000
         ALR   @11,QSCAN                                           0094 00251000
         CLI   CUQSDCS-1(@11),C''''                                0094 00252000
         BE    @RT00094                                            0094 00253000
*/*      LEAVE(DO)  */                                             0095 00254000
*        GOTO #DC00012;                                            0095 00255000
*     ELSE                                                         0096 00256000
*       QSCAN=QSCAN+CUQONE;         /* INCREMENT TO SCAN NEXT CHAR   */ 00257000
*                                                                  0096 00258000
         AL    QSCAN,@CF00066                                      0096 00259000
*    END; /* END('DO INFINITE LOOP')  */                           0097 00260000
@RC00089 DS    0H                                                  0097 00261000
@DE00082 B     @DL00082                                            0097 00262000
*#DC00012:;                                                        0098 00263000
#DC00012 DS    0H                                                  0099 00264000
*    /*SEGMENT(FNDQUOT)FINISH*/DO;GOTO FNDQUOT#;FNDQUOT@:END;      0099 00265000
         B     FNDQUOT#                                            0100 00266000
FNDQUOT@ DS    0H                                                  0102 00267000
*                                                                  0102 00268000
*/********************************************************************/ 00269000
*/*                                                                  */ 00270000
*/* DELQUOTE: THE 'DELETES QUOTES' LOGIC IS ENTERED WITH THE SOURCE  */ 00271000
*/*        POINTER INDICATING THE START OF A SUBSTRING AND THE SCAN  */ 00272000
*/*        POINTER INDICATING THE FIRST QUOTE, OUTSIDE OF MCR DATA   */ 00273000
*/*        IN THE STRING.  DELQUOTE DETERMINES HOW MANY CHARACTERS,  */ 00274000
*/*        STARTING AT THE SOURCE POINTER, SHOULD APPEAR IN THE      */ 00275000
*/*        CHARACTER STRING AFTER QUOTE DELETION.                    */ 00276000
*/*                                                                  */ 00277000
*/********************************************************************/ 00278000
*                                                                  0102 00279000
*    /*SEGMENT(DELQUOT)START*/DO;GOTO DELQUOT@;DELQUOT:END;        0102 00280000
         B     DELQUOT@                                            0103 00281000
DELQUOT  DS    0H                                                  0105 00282000
*   RFY                                                            0105 00283000
*    (QUOTECNT,                                                    0105 00284000
*     MOVELEN)RSTD;                                                0105 00285000
*   QUOTECNT=0;                                                    0106 00286000
         SLR   QUOTECNT,QUOTECNT                                   0106 00287000
*   MOVELEN=QSCAN-QSOURCE;                                         0107 00288000
*                                                                  0107 00289000
         LR    MOVELEN,QSCAN                                       0107 00290000
         SLR   MOVELEN,QSOURCE                                     0107 00291000
*/*  DOINF  */                                                     0108 00292000
*                                                                  0108 00293000
*    DO  UNTIL(1=2);                                               0108 00294000
@DL00108 DS    0H                                                  0109 00295000
*     IF QSCAN=CUQLEN               /* LAST CHAR HAS BEEN REACHED    */ 00296000
*       THEN                                                       0109 00297000
         SLR   @15,@15                                             0109 00298000
         IC    @15,CUQLEN(,IPTREG)                                 0109 00299000
         CR    QSCAN,@15                                           0109 00300000
         BNE   @RF00109                                            0109 00301000
*       DO;                                                        0110 00302000
*         IF MOVELEN+CUQTWO=CUQLEN THEN                            0111 00303000
         LA    @14,2                                               0111 00304000
         ALR   @14,MOVELEN                                         0111 00305000
         CR    @14,@15                                             0111 00306000
         BNE   @RF00111                                            0111 00307000
*           MDTS=ON;                /* SET MOVE DATA THIS SCAN SWITCH*/ 00308000
         LA    MDTS,1                                              0112 00309000
*         ELSE                                                     0113 00310000
*           ;                                                      0113 00311000
*                                                                  0113 00312000
@RF00111 DS    0H                                                  0114 00313000
*/*        LEAVE(DO)  */                                           0114 00314000
*          GOTO #DC00018;                                          0114 00315000
         B     #DC00018                                            0114 00316000
*       END;                                                       0115 00317000
*     ELSE                                                         0116 00318000
*       DO;                                                        0116 00319000
@RF00109 DS    0H                                                  0117 00320000
*         QUOTECNT=QUOTECNT+CUQONE; /* INCREMENT NUMBER OF QUOTES    */ 00321000
         LA    @15,1                                               0117 00322000
         ALR   QUOTECNT,@15                                        0117 00323000
*         QSCAN=QSCAN+CUQONE;       /* INCREMENT CHAR POINTER        */ 00324000
         ALR   QSCAN,@15                                           0118 00325000
*         IF CUQSDCS(QSCAN:QSCAN)=CUQUOTE/* NEXT CHAR A QUOTE        */ 00326000
*           THEN                                                   0119 00327000
         LR    @15,IPTREG                                          0119 00328000
         ALR   @15,QSCAN                                           0119 00329000
         CLI   CUQSDCS-1(@15),C''''                                0119 00330000
         BNE   @RF00119                                            0119 00331000
*           ;                                                      0120 00332000
*         ELSE                                                     0121 00333000
*                                                                  0121 00334000
*/*          LEAVE(DO)  */                                         0121 00335000
*            GOTO #DC00018;                                        0121 00336000
*       END;                                                       0122 00337000
*                                                                  0122 00338000
*    END; /* END('DO INFINITE LOOP')  */                           0123 00339000
@DE00108 B     @DL00108                                            0123 00340000
*#DC00018:;                                                        0124 00341000
#DC00018 DS    0H                                                  0125 00342000
*   IF QUOTECNT//CUQTWO=CUQZERO     /* NUMBER OF QUOTES IS EVEN      */ 00343000
*     THEN                                                         0125 00344000
         LR    @02,QUOTECNT                                        0125 00345000
         SRDA  @02,32                                              0125 00346000
         D     @02,@CF00091                                        0125 00347000
         LTR   @02,@02                                             0125 00348000
         BNZ   @RF00125                                            0125 00349000
*     DO;                                                          0126 00350000
*                                                                  0126 00351000
*       /*************************************************************/ 00352000
*       /*                                                           */ 00353000
*       /* INCREMENT NUMBER OF CHARACTERS TO BE MOVED BY HALF THE    */ 00354000
*       /* NUMBER OF QUOTES AND SET CPWVLN FIELD IN ISTCPWA TO THAT  */ 00355000
*       /* NUMBER OF CHARACTERS                                      */ 00356000
*       /*                                                           */ 00357000
*       /*************************************************************/ 00358000
*                                                                  0127 00359000
*       MOVELEN=MOVELEN+QUOTECNT/CUQTWO;                           0127 00360000
         LR    @15,QUOTECNT                                        0127 00361000
         SRL   @15,1                                               0127 00362000
         ALR   MOVELEN,@15                                         0127 00363000
*       CPWVLN=CPWVLN+MOVELEN;                                     0128 00364000
         SLR   @15,@15                                             0128 00365000
         IC    @15,CPWVLN(,CPWPTR)                                 0128 00366000
         ALR   @15,MOVELEN                                         0128 00367000
         STC   @15,CPWVLN(,CPWPTR)                                 0128 00368000
*       RFY                                                        0129 00369000
*         QUOTECNT UNRSTD;                                         0129 00370000
*     END;                                                         0130 00371000
*   ELSE                            /* QUOTES ARE NOT PAIRED         */ 00372000
*     DO;                                                          0131 00373000
*                                                                  0131 00374000
         B     @RC00125                                            0131 00375000
@RF00125 DS    0H                                                  0132 00376000
*       /*************************************************************/ 00377000
*       /*                                                           */ 00378000
*       /* SET VALUE OF NUMBER OF CHARACTERS TO BE MOVED EQUAL TO    */ 00379000
*       /* LENGTH OF INPUT STRING AND SET CPWVLN FIELD IN ISTCPWA TO */ 00380000
*       /* THAT VALUE. SET FIRST AND LAST CHARACTER TO BE MOVED      */ 00381000
*       /* POINTERS EQUAL TO THE FIRST AND LAST CHARACTERS OF THE    */ 00382000
*       /* STRING, RESPECTIVELY, AND TURN ON MOVE CHARACTERS SWITCH. */ 00383000
*       /*                                                           */ 00384000
*       /*************************************************************/ 00385000
*                                                                  0132 00386000
*       MOVELEN=CUQLEN;                                            0132 00387000
         SLR   @11,@11                                             0132 00388000
         IC    @11,CUQLEN(,IPTREG)                                 0132 00389000
         LR    MOVELEN,@11                                         0132 00390000
*       CPWVLN=MOVELEN;                                            0133 00391000
         STC   MOVELEN,CPWVLN(,CPWPTR)                             0133 00392000
*       MDTS=ON;                                                   0134 00393000
         LA    @15,1                                               0134 00394000
         LR    MDTS,@15                                            0134 00395000
*       QSOURCE=CUQONE;                                            0135 00396000
         LR    QSOURCE,@15                                         0135 00397000
*       QSCAN=CUQLEN;                                              0136 00398000
         LR    QSCAN,@11                                           0136 00399000
*     END;                                                         0137 00400000
*    /*SEGMENT(DELQUOT)FINISH*/DO;GOTO DELQUOT#;DELQUOT@:END;      0138 00401000
@RC00125 DS    0H                                                  0139 00402000
         B     DELQUOT#                                            0139 00403000
DELQUOT@ DS    0H                                                  0141 00404000
*                                                                  0141 00405000
*/********************************************************************/ 00406000
*/*                                                                  */ 00407000
*/* MOVEDATA:  WHEN APPROPRIATE, MOVES SUBSTRINGS OF THE INPUT       */ 00408000
*/*        CHARACTER STRING, OR THE ENTIRE STRING, TO THE TARGET     */ 00409000
*/*        FIELD SPECIFIED BY THE CALLER. AFTER THE MOVE, THE TARGET */ 00410000
*/*        IS UPDATED TO POINT PASSED THE MOVED CHARACTERS.          */ 00411000
*/*        REGARDLESS OF WHETHER A MOVE IS DONE, THE SOURCE POINTER  */ 00412000
*/*        THE SOURCE POINTER IS INCREMENTED TO INDICATE THE START   */ 00413000
*/*        OF THE NEXT SUBSTRING TO BE SCANNED FOR QUOTES.           */ 00414000
*/*                                                                  */ 00415000
*/********************************************************************/ 00416000
*                                                                  0141 00417000
*    /*SEGMENT(MOVEDAT)START*/DO;GOTO MOVEDAT@;MOVEDAT:END;        0141 00418000
