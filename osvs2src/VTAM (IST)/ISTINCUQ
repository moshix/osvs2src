*          /*SEGMENT(DELQUOT)REQUEST*/DO;GOTO DELQUOT;DELQUOT#:END;     00145000
         B     DELQUOT                                             0063 00146000
DELQUOT# DS    0H                                                  0065 00147000
*          /*SEGMENT(MOVEDAT)REQUEST*/DO;GOTO MOVEDAT;MOVEDAT#:END;     00148000
         B     MOVEDAT                                             0066 00149000
MOVEDAT# DS    0H                                                  0068 00150000
*       END;                                                       0068 00151000
@DE00058 SLR   @11,@11                                             0068 00152000
         IC    @11,CUQLEN(,IPTREG)                                 0068 00153000
         CR    QSCAN,@11                                           0068 00154000
         BNE   @DL00058                                            0068 00155000
*       SECSCAN=SECSCAN+CUQONE;     /* INCREMENT '2ND-SCAN-NEEDED' 0069 00156000
*                                      SWITCH                        */ 00157000
         AL    SECSCAN,@CF00066                                    0069 00158000
*     END;                                                         0070 00159000
*                                                                  0070 00160000
@DE00054 C     SECSCAN,@CF00066                                    0070 00161000
         BNH   @DL00054                                            0070 00162000
*   /*****************************************************************/ 00163000
*   /*                                                               */ 00164000
*   /* NOT A QUOTED STRING. MOVE CHARACTER STRING IF REQUIRED AND    */ 00165000
*   /* LENGTH                                                        */ 00166000
*   /*                                                               */ 00167000
*   /*****************************************************************/ 00168000
*                                                                  0071 00169000
*   ELSE                                                           0071 00170000
*     DO;                                                          0071 00171000
         B     @RC00053                                            0071 00172000
@RF00053 DS    0H                                                  0072 00173000
*       IF TARGPTR0=CUQZERO         /* ONLY DATA LENGTH TO BE      0072 00174000
*                                      RETURNED                      */ 00175000
*         THEN                                                     0072 00176000
         LTR   TARGPTR0,TARGPTR0                                   0072 00177000
         BZ    @RT00072                                            0072 00178000
*         ;                                                        0073 00179000
