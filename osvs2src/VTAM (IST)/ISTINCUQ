         EX    @07,@SM02314                                        0074 00186000
*       CPWVLN=CUQLEN;              /* MOVE LENGTH TO ISTCPWA        */ 00187000
@RT00072 MVC   CPWVLN(1,CPWPTR),CUQLEN(IPTREG)                     0075 00188000
*     END;                                                         0076 00189000
*   WANXTAV=CWORKREG;               /* RESTORE PTR TO NEXT AVAILABLE */ 00190000
@RC00053 L     @11,NCSPLWKA(,NCSPLPTR)                             0077 00191000
         ST    CWORKREG,WANXTAV(,@11)                              0077 00192000
*                                                                  0078 00193000
*/********************************************************************/ 00194000
*/*                                                                  */ 00195000
*/* FNDQUOTE: SCANS THE INPUT CHARACTER STRING FOR A QUOTE.  IF      */ 00196000
*/*        MAGNETIC CARD READER DATA (BEGINNING WITH AN ESC          */ 00197000
*/*        CHARACTER AND ENDING WITH AN IRS CHARACTER) IS DETECTED,  */ 00198000
*/*        INTERVENING QUOTES ARE IGNORED.  SYNTAX ANALYSIS DONE     */ 00199000
*/*        EARLIER ENSURES THAT MCR DATA BEGINS WITH THE ESC         */ 00200000
*/*        CHARACTER FOLLOWED BY A X'50' AND ENDS WITH THE   @D32CKXK*/ 00201000
*/*        IRS CHARACTER.  FNDQUOTE RETURNS WITH THE SCAN POINTER    */ 00202000
*/*        LOCATING THE FIRST QUOTE FOUND OUTSIDE OF MCR DATA.       */ 00203000
*/*                                                                  */ 00204000
*/********************************************************************/ 00205000
*                                                                  0078 00206000
*    /*SEGMENT(FNDQUOT)START*/DO;GOTO FNDQUOT@;FNDQUOT:END;        0078 00207000
         B     FNDQUOT@                                            0079 00208000
FNDQUOT  DS    0H                                                  0081 00209000
*   CPWSSR=CPWZRO;                  /* SET MAG CARD DATA FLAG OFF    */ 00210000
*                                                                  0081 00211000
         NI    CPWSSR(CPWPTR),B'10111111'                          0081 00212000
*/*  DOINF  */                                                     0082 00213000
*                                                                  0082 00214000
*    DO  UNTIL(1=2);                                               0082 00215000
@DL00082 DS    0H                                                  0083 00216000
*   DO WHILE(CPWSSR=CPWONE);                                       0083 00217000
         B     @DE00083                                            0083 00218000
@DL00083 DS    0H                                                  0084 00219000
*     IF CUQSDCS(QSCAN:QSCAN)=CUQIRS THEN                          0084 00220000
         LR    @11,IPTREG                                          0084 00221000
         ALR   @11,QSCAN                                           0084 00222000
         CLI   CUQSDCS-1(@11),X'1E'                                0084 00223000
         BNE   @RF00084                                            0084 00224000
*         CPWSSR=CPWZRO;            /* SET MAG CARD DATA FLAG OFF    */ 00225000
         NI    CPWSSR(CPWPTR),B'10111111'                          0085 00226000
*     ELSE                                                         0086 00227000
*       ;                                                          0086 00228000
@RF00084 DS    0H                                                  0087 00229000
*     QSCAN=QSCAN+CUQONE;           /* INCREMENT SCAN TO NEXT CHAR   */ 00230000
         AL    QSCAN,@CF00066                                      0087 00231000
*   END;                            /* DO WHILE                      */ 00232000
@DE00083 TM    CPWSSR(CPWPTR),B'01000000'                          0088 00233000
         BO    @DL00083                                            0088 00234000
*   IF CUQSDCS(QSCAN:QSCAN)=CUQESC THEN                            0089 00235000
