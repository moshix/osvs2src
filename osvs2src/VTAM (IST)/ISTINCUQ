*   /*                                                               */ 00087000
*   /* ESTABLISH BASE FOR WORK AREA                                  */ 00088000
*   /*                                                               */ 00089000
*   /*****************************************************************/ 00090000
*                                                                  0048 00091000
*   GEN REFS(CWORKREG);                                            0048 00092000
*                                                                  0048 00093000
 USING @DATD,CWORKREG                                                   00094000
*   WANXTAV=CWORKREG+@SIZDATD;      /* UPDATE PTR TO NEXT AVAILABLE  */ 00095000
         L     @11,NCSPLWKA(,NCSPLPTR)                             0049 00096000
         LR    @07,CWORKREG                                        0049 00097000
         AL    @07,@SIZDATD                                        0049 00098000
         ST    @07,WANXTAV(,@11)                                   0049 00099000
*   CPWPTR=NCSCPWPT;                /* SET BASE FOR ISTCPWA          */ 00100000
         L     CPWPTR,NCSCPWPT(,NCSPLPTR)                          0050 00101000
*   SECSCAN=OFF;                    /* SET 'SCAN-NEEDED SWITCH' OFF  */ 00102000
         SLR   SECSCAN,SECSCAN                                     0051 00103000
*   IPTREG=CPWIPT;                  /* SET INPUT-SDCS-STRUCTURE BASE */ 00104000
*                                                                  0052 00105000
         L     IPTREG,CPWIPT(,CPWPTR)                              0052 00106000
*   /*****************************************************************/ 00107000
*   /*                                                               */ 00108000
*   /* CHECK FOR QUOTED STRING                                       */ 00109000
*   /*                                                               */ 00110000
*   /*****************************************************************/ 00111000
*                                                                  0053 00112000
*   IF(FIRSCHAR=CUQUOTE)&           /* FIRST CHAR OF STRING A QUOTE  */ 00113000
*       (CUQSDCS(CUQLEN:CUQLEN)=CUQUOTE)/* LAST CHAR A QUOTE         */ 00114000
*     THEN                                                         0053 00115000
         CLI   FIRSCHAR(IPTREG),C''''                              0053 00116000
         BNE   @RF00053                                            0053 00117000
         SLR   @11,@11                                             0053 00118000
         IC    @11,CUQLEN(,IPTREG)                                 0053 00119000
         ALR   @11,IPTREG                                          0053 00120000
         CLI   CUQSDCS-1(@11),C''''                                0053 00121000
         BNE   @RF00053                                            0053 00122000
*     DO UNTIL(SECSCAN>CUQONE);                                    0054 00123000
@DL00054 DS    0H                                                  0055 00124000
*       QSCAN=CUQTWO;               /* BEGIN SCAN AFTER FIRST QUOTE  */ 00125000
         LA    QSCAN,2                                             0055 00126000
*       QSOURCE=QSCAN;              /* SET MOVEMENT POINTERS         */ 00127000
         LR    QSOURCE,QSCAN                                       0056 00128000
*       CPWVLN=CUQZERO;                                            0057 00129000
         MVI   CPWVLN(CPWPTR),X'00'                                0057 00130000
*       DO UNTIL(QSCAN=CUQLEN);                                    0058 00131000
*                                                                  0058 00132000
@DL00058 DS    0H                                                  0059 00133000
*         /***********************************************************/ 00134000
*         /*                                                         */ 00135000
*         /* SCAN UNTIL NEXT QUOTE IS FOUND (FNDQUOTE), ADJUST MOVE  */ 00136000
*         /* LENGTH FOR PAIRED QUOTES (DELQUOTE), AND THEN MOVE DATA */ 00137000
*         /* AS REQUIRED (MOVEDATA)                                  */ 00138000
*         /*                                                         */ 00139000
