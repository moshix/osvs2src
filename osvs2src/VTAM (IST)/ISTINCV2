*                    IF (RLNEAM = ON   /*ENABLE ANS IN LINE*/      0132 00116000
*                        & NCSPLVBF = NCSVANON)  /*AND ANS=YES*/   0132 00117000
*                      | (RLNEAM = OFF  /*ABANDON ANS AND */       0132 00118000
