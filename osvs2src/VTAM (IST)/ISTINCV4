SVTM     TITLE ' SSCP -- ISTINCV4 -- ACTIVATE SWITCHED PU              *00001000
                        '                                               00002000
ISTINCV4 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCV4  77.299'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
*      /* MAINLINE CODE OF ISTINCV4 -- ACTIVATE SWITCHED PU*/      0075 00015000
*                                                                  0075 00016000
* DO;                                                              0075 00017000
*/***  CRRMAINT  PROCNM(ISTINCV4)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00018000
**/                                                                0076 00019000
*                                                                  0076 00020000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0076 00021000
* RFY ISTLCCW ;                                                    0076 00022000
* RFY ISTLCPB ;                                                    0077 00023000
* RFY ISTZSAVE;                                                    0078 00024000
* RFY ISTPFCRR;                                                    0079 00025000
* RFY ISTCFCRR;                                                    0080 00026000
* RFY ISTZCRR ;                                                    0081 00027000
* RFY ISTASCRR;                                                    0082 00028000
* RFY ISTAPCRR;                                                    0083 00029000
* RFY ISTCRA  ;                                                    0084 00030000
* RFY ISTCNSTA;                                                    0085 00031000
* RFY ISTVYCRR;                                                    0086 00032000
*/* END OF ATBUILD */                                              0087 00033000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0087 00034000
*NCSPLRPH->RPHCRR->CRACRR->NINCV4 = '1'B;                          0087 00035000
         L     @14,NCSPLRPH(,R1)                                   0087 00036000
         L     @14,RPHCRR(,@14)                                    0087 00037000
         L     @14,CRACRR(,@14)                                    0087 00038000
         OI    NINCV4(@14),B'01000000'                             0087 00039000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0088 00040000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0088 00041000
         XC    VYCCRT+1(11,@14),VYCCRT+1(@14)                      0088 00042000
         MVI   VYCCRT(@14),B'00000000'                             0088 00043000
*NCSPLRPH->RPHCRR->CRACRR->CINCV4 = '1'B;                          0089 00044000
         OI    CINCV4(@14),B'01000000'                             0089 00045000
* END;                                                             0090 00046000
*GEN REFS(CV4WAPTR) (   USING  @DATD,CV4WAPTR);                    0091 00047000
            USING  @DATD,CV4WAPTR                                       00048000
*R15=REGSAPTR;   /* SAVE THE CALLER'S REGISTER SAVEAREA POINTER */ 0092 00049000
         LR    R15,REGSAPTR                                        0092 00050000
*REGSAPTR=REGSAPTR-LENGTH(CV4REGSV); /* DECREMENT REG 13 BY 72 */  0093 00051000
         SL    REGSAPTR,@CF00918                                   0093 00052000
*CV4SVBKP=R15;   /* POINT THIS ROUTINE'S SAVEAREA TO THE CALLER'S */    00053000
         ST    R15,CV4SVBKP(,REGSAPTR)                             0094 00054000
*CV4SVFWP=ZERO;  /* ZERO THIS ROUTINE'S SAVEAREA FORWARD POINTER */     00055000
         SLR   @14,@14                                             0095 00056000
         ST    @14,CV4SVFWP(,REGSAPTR)                             0095 00057000
*R15->CV4SVFWP=REGSAPTR; /* POINT CALLER'S SAVEAREA TO THIS SAVEAREA */ 00058000
         ST    REGSAPTR,CV4SVFWP(,R15)                             0096 00059000
*CV4WAPTR=NXTAVAIL;/* GET THE NEXT AVAILABLE ADDRESS IN THE WORKAREA*/  00060000
         L     @14,NCSPLWKA(,R1)                                   0097 00061000
         L     CV4WAPTR,NXTAVAIL(,@14)                             0097 00062000
*CV4WAPTR=(CV4WAPTR+SEVEN)&MINUS8;  /*ALIGN ON DOUBLEWORD*/        0098 00063000
         LA    @00,7                                               0098 00064000
         ALR   @00,CV4WAPTR                                        0098 00065000
         N     @00,@CF00776                                        0098 00066000
         LR    CV4WAPTR,@00                                        0098 00067000
*NXTAVAIL=CV4WAPTR+@SIZDATD; /* RESET NEXT AVAILABLE BEYOND THIS   0099 00068000
*                              ROUTINE'S DYNAMIC AREA */           0099 00069000
         LR    @00,CV4WAPTR                                        0099 00070000
         AL    @00,@SIZDATD                                        0099 00071000
         ST    @00,NXTAVAIL(,@14)                                  0099 00072000
*NCSPLPTR=R1;                  /* GET THE NCSPL ADDRESS */         0100 00073000
         LR    NCSPLPTR,R1                                         0100 00074000
*RFY ISTNCSPL BASED(NCSPLPTR);                                     0101 00075000
*                                                                  0101 00076000
*CV4SWTCH = ZERO;                  /*INITIALIZE FLAGS OFF*/        0102 00077000
         MVI   CV4SWTCH,X'00'                                      0102 00078000
*PURDTE = NCSPLRAD;                /*GET PU RDTE ADDR*/            0103 00079000
*                                                                  0103 00080000
         L     PURDTE,NCSPLRAD(,NCSPLPTR)                          0103 00081000
*IF RPRONLIN = ON                                                  0104 00082000
*  THEN NOACTSW=ON;               /*IF PU ONLINE,DO NOT ACTIVATE*/ 0104 00083000
         TM    RPRONLIN(PURDTE),B'00001000'                        0104 00084000
         BNO   @RF00104                                            0104 00085000
         OI    NOACTSW,B'10000000'                                 0105 00086000
*  ELSE IF NCSPLRAD=NCSPLRPT                                       0106 00087000
*        THEN;                    /*COMMAND AGAINST PU*/           0106 00088000
         B     @RC00104                                            0106 00089000
@RF00104 CLC   NCSPLRAD(4,NCSPLPTR),NCSPLRPT(NCSPLPTR)             0106 00090000
         BE    @RT00106                                            0106 00091000
*        ELSE IF NCSPL2IN=ON      /*FIRST ACTIVATE OF SW SUBAREA*/ 0108 00092000
*              & RCCRBISA ^=ON    /*AND PU ISTATUS NOT ACTIVE*/    0108 00093000
*                THEN NOACTSW=ON;  /*DO NOT ACTIVATE*/             0108 00094000
         TM    NCSPL2IN(NCSPLPTR),B'10000000'                      0108 00095000
         BNO   @RF00108                                            0108 00096000
         TM    RCCRBISA(PURDTE),B'10000000'                        0108 00097000
         BO    @RF00108                                            0108 00098000
         OI    NOACTSW,B'10000000'                                 0109 00099000
*                ELSE;                                             0110 00100000
*                                                                  0110 00101000
@RF00108 DS    0H                                                  0111 00102000
*IF NOACTSW = ON                                                   0111 00103000
*  THEN R15 = FOUR;               /*INDICATE NO ACTIVATE REQUIRED */    00104000
@RT00106 DS    0H                                                  0111 00105000
@RC00104 TM    NOACTSW,B'10000000'                                 0111 00106000
         BNO   @RF00111                                            0111 00107000
         LA    R15,4                                               0112 00108000
*  ELSE DO;                       /*ACTIVATE PU*/                  0113 00109000
         B     @RC00111                                            0113 00110000
@RF00111 DS    0H                                                  0114 00111000
*       RPRONLIN=ON;              /*MARK PU RDTE ONLINE */         0114 00112000
         OI    RPRONLIN(PURDTE),B'00001000'                        0114 00113000
*       RPRLIVE=ON;               /*AND LIVE*/                     0115 00114000
         OI    RPRLIVE(PURDTE),B'00000001'                         0115 00115000
*       NCSACTV = ON;              /*MARK STATUS CHANGED     @X50BHKA*/ 00116000
         OI    NCSACTV(NCSPLPTR),B'10000000'                       0116 00117000
*       NCSAIMOD = ON;             /*MARK STATUS NOW ACTIVE  @X50BHKA*/ 00118000
         OI    NCSAIMOD(NCSPLPTR),B'10000000'                      0117 00119000
*       WRKRDTE = NCSPLRAD;       /*GET PU RDTE ADDR*/             0118 00120000
         L     WRKRDTE,NCSPLRAD(,NCSPLPTR)                         0118 00121000
*       RFY ISTRPRE BASED(WRKRDTE);                                0119 00122000
*                                 /*PROCESS LUS UNDER PU UNTIL END*/    00123000
*                                 /*OF SEGMENT OR NEXT PU REACHED*/     00124000
*       DO WHILE RPRENTED^= ON & EXITLOP1 = OFF;                   0120 00125000
         B     @DE00120                                            0120 00126000
@DL00120 DS    0H                                                  0121 00127000
*          WRKRDTE = WRKRDTE + RPRELEN;    /*GET ADDR NEXT ENTRY*/ 0121 00128000
         AH    WRKRDTE,RPRELEN(,WRKRDTE)                           0121 00129000
*          IF RPRENTRY > PURDTE->RPRENTRY                          0122 00130000
*             THEN                /* -YES-                   @ZA26931*/ 00131000
         CLC   RPRENTRY(1,WRKRDTE),RPRENTRY(PURDTE)                0122 00132000
         BNH   @RF00122                                            0122 00133000
*               IF RPRINV = OFF   /* IS LU VALID ?           @ZA26931*/ 00134000
*                 THEN DO;        /* -YES-                   @ZA26931*/ 00135000
         TM    RPRINV(WRKRDTE),B'01000000'                         0123 00136000
         BNZ   @RF00123                                            0123 00137000
*                  RPRPTHCT = ONE;          /*SET PATH COUNT TO 1*/     00138000
         MVI   RPRPTHCT(WRKRDTE),X'01'                             0125 00139000
*                  IF RLURBISA = ON                                0126 00140000
*                    THEN DO;               /*ISTATUS IS ACTIVE*/  0126 00141000
         TM    RLURBISA(WRKRDTE),B'10000000'                       0126 00142000
         BNO   @RF00126                                            0126 00143000
*                         RPRONLIN = ON;    /*MARK LU ONLINE*/     0128 00144000
         OI    RPRONLIN(WRKRDTE),B'00001000'                       0128 00145000
*                         RPRLIVE = ON;     /*AND LIVE*/           0129 00146000
         OI    RPRLIVE(WRKRDTE),B'00000001'                        0129 00147000
*                         END;                                     0130 00148000
*                    ELSE;                                         0131 00149000
@RF00126 DS    0H                                                  0132 00150000
*                  END;               /*END LU PROCESSING*/        0132 00151000
*               ELSE;             /* -NO- LU IS INVALID      @ZA26931*/ 00152000
@RF00123 DS    0H                                                  0134 00153000
*             ELSE EXITLOP1 = ON;           /*REACHED NEXT PU*/    0134 00154000
         B     @RC00122                                            0134 00155000
@RF00122 OI    EXITLOP1,B'01000000'                                0134 00156000
*        END;                     /*END OF DOWHILE*/               0135 00157000
*                                                                  0135 00158000
@RC00122 DS    0H                                                  0135 00159000
@DE00120 TM    RPRENTED(WRKRDTE),B'00000100'                       0135 00160000
         BO    @DC00120                                            0135 00161000
         TM    EXITLOP1,B'01000000'                                0135 00162000
         BZ    @DL00120                                            0135 00163000
@DC00120 DS    0H                                                  0136 00164000
*        R15 = ZERO;              /*INDICATE PU PROCESSED*/        0136 00165000
         SLR   R15,R15                                             0136 00166000
*        END;                     /*END ACTIVATE PU*/              0137 00167000
*                                                                  0137 00168000
*NXTAVAIL = CV4WAPTR;             /*RESTORE WORK AREA PTR*/        0138 00169000
@RC00111 L     @14,NCSPLWKA(,NCSPLPTR)                             0138 00170000
         ST    CV4WAPTR,NXTAVAIL(,@14)                             0138 00171000
*REGSAPTR = CV4SVBKP;             /*RESTORE ADDR CALLERS SAVE AREA*/    00172000
*                                                                  0139 00173000
         L     REGSAPTR,CV4SVBKP(,REGSAPTR)                        0139 00174000
*/* TPEPILOG   */                                                  0140 00175000
*  GEN (PRINT OFF);                                                0140 00176000
*                                                                  0140 00177000
         PRINT OFF                                                      00178000
*/*   PROGRAM OPERATOR MESSAGE HEADER                                */ 00179000
*DCL  1  ISTPOHD  BDY(WORD) BASED,                                 0141 00180000
*        2 POHRSVD1 CHAR(1),       /* RESERVED                       */ 00181000
*        2 POHSTAT,                /* HEADER STATUS BITS             */ 00182000
*          3 POHEND   BIT(1),      /* END LINE 1=YES                 */ 00183000
*          3 POHDATA  BIT(1),      /* DATA LINE 1=YES                */ 00184000
*          3 POHLBL   BIT(1),      /* LABEL LINE 1=YES               */ 00185000
*          3 POHCNTRL BIT(1),      /* CONTROL LINE 1=YES             */ 00186000
*          3 POHRSVD2 BIT(2),      /* RESERVED                       */ 00187000
*          3 POHRREQ  BIT(1),      /* REPLY REQUESTED 1=YES          */ 00188000
*          3 POHGEN   BIT(1),      /* ID ORIGIN  0=GENERATED BY VTAM */ 00189000
*                                  /*            1=GENERATED BY POA  */ 00190000
*        2 POHID    FIXED(16);     /* MESSAGE IDENTIFIER             */ 00191000
*/*            VTAM PREFIX FOR QUEUE ELEMENTS                         * 00192000
*/*  INITIAL LEVEL                                     001 03/04/75  */ 00193000
*DECLARE                                                           0142 00194000
*  1 ISTPFX  BDY(WORD)  BASED,                                     0142 00195000
*   2 PFXID        CHAR(4),             /* QUEUE ACRONYM (SAME AS  0142 00196000
*                                          QABID)                     * 00197000
*     3 PFXCBID    BIT(8),              /* CONTROL BLOCK ID           * 00198000
*   2 PFXPRTY      CHAR(1),             /* PRIORITY OF ELEMENT        * 00199000
*   2 PFXLNG       CHAR(3),             /* ELEMENT LENGTH,         0142 00200000
