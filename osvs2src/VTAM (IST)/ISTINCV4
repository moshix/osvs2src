*   2 PFXBCK       PTR(31) ;   /* POINTER TO PREVIOUS              0142 00205000
*                                          QUEUE ELEMENT (IF       0142 00206000
*                                          FIRST, POINTER TO QAB)     * 00207000
*DCL NCSPLTYP BIT(8) CONSTANT('60'X);   /* NCSPL CONTROL BLOCK ID     * 00208000
*   /* NETWORK CONFIGURATION SERVICES CONTROL BLOCK  */            0144 00209000
*/*  REVISIONS                                         14 07/01/75  */  00210000
*DECLARE                                                           0144 00211000
*  1 ISTNCSPL BASED BDY(DWORD),                                    0144 00212000
*   2 *      CHAR(16) BDY(WORD),                                   0144 00213000
*     3 NCSPFX CHAR(LENGTH(ISTPFX)), /* QUE ELEMENT PREFIX           */ 00214000
*       4 *        CHAR(12),                                       0144 00215000
*       4 NCSCRWKA PTR(31),       /* PTR TO WORK AREA FOR C/R I/O    */ 00216000
