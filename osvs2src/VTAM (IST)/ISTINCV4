*   2 NCSPLVCD    BIT(8),         /*VERB CODE                        */ 00218000
*   2 NCSPLVBF    CHAR(2),        /*VERB MODIFIER FLAGS              */ 00219000
*      3 NCSVBF01 BIT(1),         /* UNUSED                          */ 00220000
*      3 NCSVBF02 BIT(1),         /* UNUSED                          */ 00221000
*      3 NCSVBF03 BIT(1),         /* MODIFY MSG SUP                  */ 00222000
*      3 NCSVBF04 BIT(1),         /* TYPE=VTAM                       */ 00223000
*      3 NCSVBF05 BIT(1),         /* LINETRACE=1                     */ 00224000
*      3 NCSVBF06 BIT(1),         /* I/O TRACE=1                     */ 00225000
*      3 NCSVBF07 BIT(1),         /* BUFFER TRACE=1                  */ 00226000
*      3 NCSVBF08 BIT(1),         /* MODIFY TPRINT                   */ 00227000
*      3 NCSVBF09 BIT(1),         /* MODIFY TEST                     */ 00228000
*      3 NCSVBF10 BIT(1),         /* MODIFY TRACE=NO                 */ 00229000
*      3 NCSVBF11 BIT(1),         /* VARY INTERNAL ONLY & MODIFY     */ 00230000
*                                 /*  TRACE = YES                    */ 00231000
*      3 NCSVBF12 BIT(1),         /* VARY IMMEDIATE  & MODIFY        */ 00232000
*                                 /*  NETSOL=NO                      */ 00233000
*      3 NCSVBF13 BIT(1),         /* MODIFY NETSOL=YES               */ 00234000
*      3 NCSVBF14 BIT(1),         /* VARY LOGON & MODIFY             */ 00235000
*                                 /*  CHANGE                         */ 00236000
*      3 NCSVBF15 BIT(1),         /* VARY DEACTIVATE & VARY NORMAL   */ 00237000
*                                 /*  CLOSE(ERP)                     */ 00238000
*      3 NCSVBF16 BIT(1),         /* VARY ACTIVATE & ERP REQUEST     */ 00239000
*                                 /*   & MODIFY DUMP                 */ 00240000
*   2 NCSPLVFL    CHAR(1),        /*COMMAND FLAGS                    */ 00241000
*    3 NCSVNCSA   BIT(1),         /*NCSPL IS ACTIVE                  */ 00242000
*    3 NCSVDEL    BIT(1),         /*DELETE NCSPL                     */ 00243000
*    3 NCSVSTAT   BIT(1),         /*INDICATE NCSPL WAS NOT GETMAINED */ 00244000
*    3 NCSDCONT   BIT(1),         /* DO DISCONACT                    */ 00245000
*    3 NCSABCON   BIT(1),         /* DO ABANDON CONNECTION           */ 00246000
*    3 NCSDACLK   BIT(1),         /* DO ACTIVATE LINE                */ 00247000
*    3 NCSRSV02   BIT(2),         /* RESERVED                        */ 00248000
*   2 NCSPLVF1    CHAR(1),        /* SPECIAL PROC FLAGS              */ 00249000
*    3 NCSOVFMC   BIT(1),         /* PURGE ONLY VARY'S FMCB          */ 00250000
*   2 NCSPRMRC    CHAR(1),        /* PURGE MINOR RETURN CODE FIELD   */ 00251000
*   2 NCSIORC     CHAR(1),        /* I/O OR RESTART RETURN CODE      */ 00252000
*   2 NCSRRNPT    CHAR(4),        /* ADDR RN ENTRY FOR LINE TRACE    */ 00253000
*   2 NCSPLRID    CHAR(8),        /*ID = 'NODENAME'                  */ 00254000
*   2 NCSPLLID    CHAR(8),        /*LOGON = 'NODENAME'               */ 00255000
*   2 NCSDLID     CHAR(6),        /* OFFHOOK ID                      */ 00256000
*   2 NCSCRCMD    FIXED(8),       /* C/R I/O COMMAND CODE            */ 00257000
*   2 NCSCRPID    PTR(8),         /* C/R PATH ID (PID)               */ 00258000
*   2 NCSPLRPT    PTR(31),        /*ID='NODENAME' RDTE ADDRESS       */ 00259000
*   2 NCSPLLPT    PTR(31),        /*LOGON/LOGOFF='NODENAME' RDTE ADDR*/ 00260000
*   2 NCSPLCMD    CHAR(2),        /*3705 CONTROL COMMAND CODES       */ 00261000
*     3 NCSCMDA  BIT(8),          /*RH FLAGS                         */ 00262000
*       4 NCSPLSYS BIT(1),        /*ON-SYSTEM OFF-FM                 */ 00263000
*       4 NCSPLCTL BIT(1),        /*ON-CONTROL OFF-DATA              */ 00264000
*       4 NCSPLSYN BIT(1),        /*ON=SYNCHR OFF-ASYNCHR            */ 00265000
*       4 NCSPLFLO BIT(1),        /*ON-WITH FLOW  OFF-AGAINST FLOW   */ 00266000
*       4 NCSPLUFM BIT(1),        /*ON=UNFORMATTED RU,OFF=FORMATTED  */ 00267000
*       4 *        BIT(3),        /*RESERVED                         */ 00268000
*     3 NCSCMDB  BIT(8),          /*COMMAND FIELD                    */ 00269000
*   2 NCSNCPEP    BIT(8),         /* EP SUBCHANNEL ADDR              */ 00270000
*   2 NCSMSGSP    CHAR(1),        /* RESERVED FOR MESSAGE SURPRESSION*/ 00271000
*   2 NCSPLUAD    CHAR(3),        /*U = 'UNIT ADDRESS'               */ 00272000
*   2 NCSCRFLG    CHAR(1),        /* MISCELLANEOUS C/R FLAGS         */ 00273000
*     3 NCSPLWRM    BIT(1),       /* COMMAND SPECIFIED WARM PARAM,   */ 00274000
*                                 /* 1 = YES                         */ 00275000
*     3 NCSPLCLD    BIT(1),       /* COMMAND SPECIFIED COLD PARAM,   */ 00276000
*                                 /* 1 = YES                         */ 00277000
*     3 NCSFMCLD    BIT(1),       /* 370X RESPONDED COLD TO ACTPU    */ 00278000
*                                 /* (ERP), 1 = YES                  */ 00279000
*     3 NCSFMERP    BIT(1),       /* 370X RESPONDED ERP TO ACTPU     */ 00280000
*                                 /* (ERP), 1 = YES                  */ 00281000
*     3 NCSCRDSE    BIT(1),       /* 0=C/R DATA SET DOES NOT EXIST   */ 00282000
*                                 /* OR IS NULL                      */ 00283000
*                                 /* 1=C/R DATA SET EXISTS           */ 00284000
*                                 /* AND IS NOT NULL                 */ 00285000
*     3 NCSOPRQU    BIT(1),       /* OPERATOR REQUESTS REIPL, 1 = YES*/ 00286000
*     3 NCSUSRNF    BIT(1),       /* EARLY WARNING PURGE HAS ALREADY */ 00287000
*                                 /* OCCURED, 1 = YES                */ 00288000
*     3 NCSSTCMD    BIT(1),       /* ACTIVATE IS A RESULT OF         */ 00289000
*                                 /* START VTAM COMMAND, 1 = YES     */ 00290000
*   2 NCSPLWRE    BIT(32),        /* WTOR ECB                        */ 00291000
*   2 NCSPLWRF    CHAR(72),       /* WTOR REPLY AREA                 */ 00292000
*   2 NCSPLRAD    PTR(31),        /* RDTE PTR FOR ALLOCATE/DEALLOCATE*/ 00293000
*     3 NCSLSCMD BIT(8),          /* LINE SCHED COMMAND TYPE         */ 00294000
*     3 NCSPLRCD BIT(8),          /* HEX VALUE FOR LINE SCHED PARAM  */ 00295000
*   2 NCSIOCID    CHAR(4),        /* CURRENT CID FOR DAF LOCK HELD   */ 00296000
*     3  NCSPLCID CHAR(4),        /* ALIAS FOR ABOVE                 */ 00297000
*        4  NCSPLSAF  CHAR(2),    /* SOURCE ADDRESS                  */ 00298000
*        4  NCSPLDAF  CHAR(2),    /* DESTINATION ADDRESS             */ 00299000
*   2 NCSPLWKA    PTR(31),        /* NCS WORK AREA ADDRESS           */ 00300000
*   2 NCSPLCVT    PTR(31),        /* POINTER TO VTAM CVT             */ 00301000
*   2 NCSPLRPH    PTR(31),        /* POINTER TO RPH                  */ 00302000
*   2 NCSPLECB    BIT(32),        /* ECB FOR VARY PGS                */ 00303000
*   2 NCSPLFSB    PTR(31),        /* ADDRESS OF USS-FSS FSB OR RU    */ 00304000
*   2 NCSPLRCR    PTR(31),        /* ADDRESS RDTE FOR CR             */ 00305000
*   2 NCSPLFL1    CHAR(1),        /* VARY FLAGS                      */ 00306000
*     3 NCSPL2IN  BIT(1),         /* SECOND ENTRY                    */ 00307000
*     3 NCSPLSOM  BIT(1),         /* SUPRESS OP. MESSAGE             */ 00308000
*     3 NCSPLEBN  BIT(1),         /* ENTRY FROM BNN SSCP             */ 00309000
*     3 NCSPLREQ  BIT(1),         /* NCSPL FROM RESTORE              */ 00310000
*     3 NCSPLNON  BIT(1),         /* DO NOT ADDR RDT SEGMENT         */ 00311000
*     3 NCSPLAPU  BIT(1),         /* ACTIVATE PHYSICAL DONE BY LOAD  */ 00312000
*     3 NCSPL2CN  BIT(1),         /* SECOND CONTACT REQUEST          */ 00313000
*     3 NCSPLPOS  BIT(1),         /* BNN POSTED                      */ 00314000
*   2 NCSPLFL2    CHAR(1),        /* VARY FLAGS                      */ 00315000
*     3 NCSPLVDP  BIT(1),         /* NCSPL ON VARDEF PAB             */ 00316000
*     3 NCSPLRSF  BIT(1),         /* NO SICK CLEAR BEFORE I/O        */ 00317000
*     3 NCSPLRIO  BIT(1),         /* I/O = RESTART I/O               */ 00318000
*     3 NCSOUFMC  BIT(1),         /* PURGE ONLY USER FMCB            */ 00319000
*     3 NCSPLDS   BIT(1),         /* 1=CIO SAVED RESPONSE DATA       */ 00320000
*     3 NCSREMPO  BIT(1),         /* REMOTE POWER OFF SPEC BY OPER   */ 00321000
*     3 NCSPLVIS  BIT(1),         /* VARY IMM START CALLER OF CVP    */ 00322000
*     3 NCSPLVIT  BIT(1),         /* VARY IMM TERM CALLER OF CVP     */ 00323000
*   2 NCSPLFL3    CHAR(1),        /* FLAG BYTE                       */ 00324000
*     3 NCSPLMOM  BIT(1),         /* ERP CAUSED BY FAILURE OF      0144 00325000
*                                    HIGHER NODE                     */ 00326000
*     3 NCSEPP    BIT(1),         /* 1=EP SUBCHANNEL PRESENT         */ 00327000
*     3 NCSPLANS  BIT(1),         /* ON FOR ACT IN ANSWER MODE       */ 00328000
*                                 /* OFF FOR ACT IN NON-ANSWER MODE  */ 00329000
*     3 NCSGID    BIT(1),         /* ON FOR PATH WITH GID PARM       */ 00330000
*     3 NCSPID    BIT(1),         /* ON FOR PATH WITH PID PARM       */ 00331000
*     3 NCSANS    BIT(1),         /* ON FOR ACTIVATE WITH ANS PARM   */ 00332000
*     3 NCSFINAL  BIT(1),         /* FINAL PARM EXISTS               */ 00333000
*     3 NCSEND    BIT(1),         /* END PARM EXISTS                 */ 00334000
*   2 NCSPLFL4    CHAR(1),        /* FLAG BYTE                       */ 00335000
*     3 NCSDLNID  BIT(1),         /* ID PRESENT IN NCSPL - OFF       */ 00336000
*     3 NCSPLRSO  BIT(1),         /* RESET ONLY                      */ 00337000
