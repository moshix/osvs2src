*        END;                                                      0116 00110000
*/*** END -TPLOCK (VOCLOCK)TYPE(SHARE)- ***/                       0117 00111000
*                                                                  0117 00112000
*IF RPRNSNEA->RPRONLIN = OFF      /*IF PU CONTROLLING LU IS OFFLINE*/   00113000
*    THEN RETCODE = EIGHT;        /*INDICATE HIGHER NODE INACTIVE*/     00114000
         L     @14,RPRNSNEA(,LURDTE)                               0117 00115000
         TM    RPRONLIN(@14),B'00001000'                           0117 00116000
         BNZ   @RF00117                                            0117 00117000
         MVC   RETCODE(4),@CF00092                                 0118 00118000
*    ELSE IF RPRNSNEA->RPRVRYPD = ON     /*IF PU CONTROLLING LU  */     00119000
*          | RPRNSNEA->RCCCUDIE = ON     /* IS VARY PENDING*/      0119 00120000
*           THEN RETCODE = TWELVE; /*INDICATE HIGHER NODE DEACTIVATE*/  00121000
         B     @RC00117                                            0119 00122000
@RF00117 L     @14,RPRNSNEA(,LURDTE)                               0119 00123000
         TM    RPRVRYPD(@14),B'00000010'                           0119 00124000
         BO    @RT00119                                            0119 00125000
         TM    RCCCUDIE(@14),B'01000000'                           0119 00126000
         BNO   @RF00119                                            0119 00127000
@RT00119 DS    0H                                                  0120 00128000
         MVC   RETCODE(4),@CF00032                                 0120 00129000
*                                 /*IN PROGRESS*/                  0121 00130000
*           ELSE DO;              /*PROCESS THE LU*/               0121 00131000
         B     @RC00119                                            0121 00132000
@RF00119 DS    0H                                                  0122 00133000
