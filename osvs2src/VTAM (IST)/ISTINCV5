SVTM     TITLE ' SSCP -- ISTINCV5 -- ACTIVATE LU                       *00001000
                        '                                               00002000
ISTINCV5 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCV5  75.325'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
*      /* MAINLINE CODE OF ISTINCV5 -- ACTIVATE LU*/               0074 00015000
*                                                                  0074 00016000
*                                                                  0074 00017000
* DO;                                                              0074 00018000
*/***  CRRMAINT  PROCNM(ISTINCV5)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00019000
**/                                                                0075 00020000
*                                                                  0075 00021000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0075 00022000
* RFY ISTLCCW ;                                                    0075 00023000
* RFY ISTLCPB ;                                                    0076 00024000
* RFY ISTZSAVE;                                                    0077 00025000
* RFY ISTPFCRR;                                                    0078 00026000
* RFY ISTCFCRR;                                                    0079 00027000
* RFY ISTZCRR ;                                                    0080 00028000
* RFY ISTASCRR;                                                    0081 00029000
* RFY ISTAPCRR;                                                    0082 00030000
* RFY ISTCRA  ;                                                    0083 00031000
* RFY ISTCNSTA;                                                    0084 00032000
* RFY ISTVYCRR;                                                    0085 00033000
*/* END OF ATBUILD */                                              0086 00034000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0086 00035000
*NCSPLRPH->RPHCRR->CRACRR->NINCV5 = '1'B;                          0086 00036000
         L     @14,NCSPLRPH(,R1)                                   0086 00037000
         L     @14,RPHCRR(,@14)                                    0086 00038000
         L     @14,CRACRR(,@14)                                    0086 00039000
         OI    NINCV5(@14),B'00100000'                             0086 00040000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0087 00041000
