*/*******************************************************************/  01429000
*/*                                                                 */  01430000
*/*        CMRIO  THIS ROUTINE IS THE COMMON IO ROUTINE FOR         */  01431000
*/*               THE CV8 SUBROUTINES. IT ISSUES A CID FIND AND     */  01432000
*/*                CALLS ISTINCIO.  RETURN CODE CHECKING IS          */ 01433000
*/*                DONE IN THE SUBROUTINES                           */ 01434000
*/********************************************************************/ 01435000
*CMRIO: PROC OPTIONS(NOSAVE,NOSAVEAREA);                           0653 01436000
CMRIO    DS    0H                                                  0654 01437000
*   DCL IOR14SV REG(8) ;         /*SAVE AREA FOR RETURN REG*/      0654 01438000
*   RFY IOR14SV RSTD;                                              0655 01439000
*   DCL IOREG14 REG(14);             /*REG 14   */                 0656 01440000
*   RFY IOREG14 RSTD;                                              0657 01441000
*   IOR14SV=IOREG14;                  /*SAVE RETURN ADDR            */  01442000
         LR    IOR14SV,IOREG14                                     0658 01443000
*   RFY IOREG14 UNRSTD;                                            0659 01444000
*   RFY CPARMREG RSTD;                                             0660 01445000
*   RFY (CREG2,CMRRCODE) RSTD;                                     0661 01446000
*   CPARMREG=NCSPLRPH;                 /*GET RPH                     */ 01447000
         L     CPARMREG,NCSPLRPH(,CNCSPLBS)                        0662 01448000
*   CREG2=NCSPLCID;                    /*GET CID                     */ 01449000
*                                                                  0663 01450000
         L     CREG2,NCSPLCID(,CNCSPLBS)                           0663 01451000
*    DO; /* CIDCTL(FIND)  */                                       0664 01452000
*                                                                  0664 01453000
*    RFY (EMCR2,EMCR3) RSTD;                                       0665 01454000
*      GEN SETS(RPHWORK)                                           0666 01455000
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0666 01456000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              01457000
*      EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                         0667 01458000
         SLR   EMCR3,EMCR3                                         0667 01459000
*      EMCR3 = '01'X; /*INDICATE SHARED LOCK*/                     0668 01460000
         LA    EMCR3,1                                             0668 01461000
*      CALL ATCLCPTR->ATCCDFND->ISTSDCCF; /*PROCESS REQUEST*/      0669 01462000
*                                                                  0669 01463000
         L     @10,ATCLCPTR                                        0669 01464000
         L     @15,ATCCDFND(,@10)                                  0669 01465000
         BALR  @14,@15                                             0669 01466000
*    RFY (EMCR2,EMCR3) UNRSTD;                                     0670 01467000
*    END; /*CIDCTL*/         /*CALL CID FIND               */      0671 01468000
