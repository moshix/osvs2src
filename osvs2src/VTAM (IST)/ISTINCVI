@EF00005 DS    0H                                                  1047 00821000
@ER00005 BR    @14                                                 1047 00822000
@PB00005 DS    0H                                                  1048 00823000
* GEN(  EJECT);                                                    1048 00824000
           EJECT                                                        00825000
*CVICID: PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                   1049 00826000
         B     @PB00006                                            1049 00827000
CVICID   DS    0H                                                  1050 00828000
*/********************************************************************/ 00829000
*/* THIS ROUTINE IS CALLED TO DO A CIDCTL (FIND) LOCK(SHARED) AND    */ 00830000
*/* PUT THE NCB ADDRESS IN REGISTER 2.  THIS ROUTINE EXPECTS THE     */ 00831000
*/* NCSIOCID FIELD IN THE NCSPL TO ALREADY HAVE THE SAF/DAF.         */ 00832000
*/********************************************************************/ 00833000
*RFY (CVIR0,CVIR2) RSTD;                                           1050 00834000
*CVINSI19=CVIRTNAD;                                                1051 00835000
         ST    CVIRTNAD,CVINSI19                                   1051 00836000
*CVINPUT=NCSPLRPH;                                                 1052 00837000
         L     CVINPUT,NCSPLRPH(,CVINCSPL)                         1052 00838000
*CVIR2=NCSIOCID;                                                   1053 00839000
*                                                                  1053 00840000
         L     CVIR2,NCSIOCID(,CVINCSPL)                           1053 00841000
*  DO; /* CIDCTL(FIND) LOCK(SHARED) */                             1054 00842000
*                                                                  1054 00843000
*  RFY (EMCR2,EMCR3) RSTD;                                         1055 00844000
*    GEN SETS(RPHWORK)                                             1056 00845000
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      1056 00846000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00847000
*    EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                           1057 00848000
