         L     GPR15P,ATCLCPTR                                     1018 00758000
*    END; /*LISATCVT*/                                             1019 00759000
*        GPR15P=GPR15P->ATCAP33;                                   1020 00760000
         L     GPR15P,ATCAP33(,GPR15P)                             1020 00761000
