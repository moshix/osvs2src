*    CALL ATCLCPTR->ATCCDFND->ISTSDCCF; /*PROCESS REQUEST*/        1059 00852000
*                                                                  1059 00853000
         L     @09,ATCLCPTR                                        1059 00854000
         L     @15,ATCCDFND(,@09)                                  1059 00855000
         BALR  @14,@15                                             1059 00856000
*  RFY (EMCR2,EMCR3) UNRSTD;                                       1060 00857000
*  END; /*CIDCTL*/                                                 1061 00858000
*CVIR2=CVIR0;              /* NCB */                               1062 00859000
         LR    CVIR2,CVIR0                                         1062 00860000
*CVIRTNAD=CVINSI19;                                                1063 00861000
         L     CVIRTNAD,CVINSI19                                   1063 00862000
*END CVICID;                                                       1064 00863000
