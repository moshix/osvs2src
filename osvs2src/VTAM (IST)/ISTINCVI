*                  END;          /* END-VARY PENDING         @ZA27664*/ 00997000
*                ELSE                                      /*@ZA27664*/ 00998000
*                  CVIMSG#='104I';                         /*@ZA27664*/ 00999000
         B     @RC01095                                            1103 01000000
@RF01095 MVC   CVIMSG#(4),@CC05715                                 1103 01001000
*/********************************************************************/ 01002000
*/* MOD DUMP IS VALID ONLY FOR RN'S                                  */ 01003000
*/********************************************************************/ 01004000
*      ELSE IF RPRENTRY ^=RPRENTRN                                 1104 01005000
*             & NCSPLVCD=NCSPLVC2                                  1104 01006000
*          THEN DO;                                                1104 01007000
         B     @RC01076                                            1104 01008000
@RF01076 L     @09,CVIWKPT1                                        1104 01009000
         CLI   RPRENTRY(@09),X'01'                                 1104 01010000
         BE    @RF01104                                            1104 01011000
         CLI   NCSPLVCD(CVINCSPL),X'02'                            1104 01012000
         BNE   @RF01104                                            1104 01013000
*               CVIMSG# = CVI600I;                                 1106 01014000
         MVC   CVIMSG#(4),@CC05610                                 1106 01015000
*               CVIMSG1A = 'MODIFY';                       /*@DM12419*/ 01016000
         MVC   CVIMSG1A(6),@CC05716                                1107 01017000
*               CVIMSG2A = NCSPLRID;                       /*@DM12419*/ 01018000
         MVC   CVIMSG2A(8),NCSPLRID(CVINCSPL)                      1108 01019000
*               END;                                               1109 01020000
*/********************************************************************/ 01021000
*/* SET GO SWITCH IF VARY DEACTIVATE PROCESSING SHOULD CONTINUE      */ 01022000
*/********************************************************************/ 01023000
*        ELSE CVIGO='1'B;                                          1110 01024000
         B     @RC01104                                            1110 01025000
@RF01104 OI    CVIGO,B'10000000'                                   1110 01026000
*  /*SEGMENT(CVISYTX)FINISH*/DO;GOTO CVISYTX#;CVISYTX@:END;        1111 01027000
@RC01104 DS    0H                                                  1111 01028000
@RC01076 DS    0H                                                  1111 01029000
@RC01072 DS    0H                                                  1111 01030000
@RC01070 DS    0H                                                  1112 01031000
         B     CVISYTX#                                            1112 01032000
CVISYTX@ DS    0H                                                  1114 01033000
*  /*SEGMENT(CVIAPPL)START*/DO;GOTO CVIAPPL@;CVIAPPL:END;          1114 01034000
         B     CVIAPPL@                                            1115 01035000
CVIAPPL  DS    0H                                                  1117 01036000
*/********************************************************************/ 01037000
*/* DO DEACTIVATE PROCESSING FOR AN APPLICATION RDT SEGMENT, FIRST   */ 01038000
*/* OBTAINING THE OPEN/CLOSE/VARY LOCK.                              */ 01039000
*/********************************************************************/ 01040000
*RFY (ISTRDT,ISTRPRE) BASED(CVIWKPT1);                             1117 01041000
*CVINPUT=4;                                                        1118 01042000
         LA    CVINPUT,4                                           1118 01043000
*CALL CVILOC;                                                      1119 01044000
         BAL   @14,CVILOC                                          1119 01045000
*/********************************************************************/ 01046000
*/* USE CVIWKPT2 AS AN RDT ENTRY POINTER TO STEP THROUGH THE         */ 01047000
*/* APPLICATION RDT ENTRIES WITHIN THE SEGMENT. ALL THE              */ 01048000
*/* APPLICATIONS MUST BE MARKED 'INACTIVE' (RPRONLIN='0'B). THEN THE */ 01049000
*/* LOGON CHAIN WILL BE BROKEN, AND ALL NODES ON THAT CHAIN REMOVED  */ 01050000
*/* BEFORE THE APPLICATION SEGMENT CAN BE DEACTIVATED.               */ 01051000
*/********************************************************************/ 01052000
*IF RDTLGCT ^='0'B                                                 1120 01053000
*  THEN DO;                                                        1120 01054000
         L     @09,CVIWKPT1                                        1120 01055000
         CLI   RDTLGCT(@09),0                                      1120 01056000
         BE    @RF01120                                            1120 01057000
*       CVIMSG# = '103I';                                          1122 01058000
         MVC   CVIMSG#(4),@CC05722                                 1122 01059000
*       CVINPUT=4;                                                 1123 01060000
         LA    CVINPUT,4                                           1123 01061000
*       CALL CVIUNLOC;                                             1124 01062000
         BAL   @14,CVIUNLOC                                        1124 01063000
*       END;                                                       1125 01064000
*  ELSE DO;                                                        1126 01065000
         B     @RC01120                                            1126 01066000
@RF01120 DS    0H                                                  1127 01067000
*       CVIWKPT2=CVIWKPT1; /*BEGIN TO LOOP THROUGH APPLICATION'S   1127 01068000
*                            RDT SEGMENT                     @DM14933*/ 01069000
         L     CVIWKPT2,CVIWKPT1                                   1127 01070000
*       CVILOOP='0'B;     /*                                 @DM14933*/ 01071000
         NI    CVILOOP,B'11011111'                                 1128 01072000
*       DO WHILE (CVILOOP='0'B & CVIWKPT2->RPRENTED='0'B); /*@DM14933*/ 01073000
         B     @DE01129                                            1129 01074000
@DL01129 DS    0H                                                  1130 01075000
*       CVIWKPT2=CVIWKPT2+CVIWKPT2->RPRELEN; /*GET NEXT RDT  @DM14933*/ 01076000
         AH    CVIWKPT2,RPRELEN(,CVIWKPT2)                         1130 01077000
*       IF CVIWKPT2->RPRONLIN='1'B /*APPL WITH AN OPEN ACB   @DM14933*/ 01078000
*          THEN DO;  /* YES, SO FAIL THE COMMAND             @DM14933*/ 01079000
         TM    RPRONLIN(CVIWKPT2),B'00001000'                      1131 01080000
         BNO   @RF01131                                            1131 01081000
*               CVIMSG#='103I'; /*SET UP FOR MSG 103I        @DM14933*/ 01082000
         MVC   CVIMSG#(4),@CC05722                                 1133 01083000
*               CVINPUT=4; /*PREPARE TO RELEASE THE VOC LOCK @DM14933*/ 01084000
         LA    CVINPUT,4                                           1134 01085000
*               CALL CVIUNLOC; /*RELEASE THE VOC LOCK        @DM14933*/ 01086000
         BAL   @14,CVIUNLOC                                        1135 01087000
*               CVILOOP='1'B; /*EXIT THE LOOP                @DM14933*/ 01088000
         OI    CVILOOP,B'00100000'                                 1136 01089000
*               END;     /*                                  @DM14933*/ 01090000
*          ELSE;         /*                                  @DM14933*/ 01091000
@RF01131 DS    0H                                                  1139 01092000
*       END;    /* END DO WHILE                              @DM14933*/ 01093000
@DE01129 TM    CVILOOP,B'00100000'                                 1139 01094000
         BNZ   @DC01129                                            1139 01095000
         TM    RPRENTED(CVIWKPT2),B'00000100'                      1139 01096000
         BZ    @DL01129                                            1139 01097000
@DC01129 DS    0H                                                  1140 01098000
*/********************************************************************/ 01099000
*/* WHEN ALL THE APPLICATION ENTRIES HAVE BEEN FOUND TO BE INACTIVE, */ 01100000
*/* REMOVE ALL APPLICATION ENTRIES FROM THEIR LOGON QUEUES, AND      */ 01101000
*/* BREAK THE LOGON CHAIN, AND THEN CALL SYSDEF TO DELETE THE        */ 01102000
*/* APPLICATION SEGMENT, THEN SET UP FOR THE 'INACTIVE' MESSAGE.     */ 01103000
*/********************************************************************/ 01104000
*       CVIWKPT2=CVIWKPT1+RPRELEN; /*PREPARE TO SCAN THE APPLICATION    01105000
*                 SEGMENT TO BREAK ALL OF THE LOGON CHAINS   @DM14933*/ 01106000
         L     @07,CVIWKPT1                                        1140 01107000
         LH    CVIWKPT2,RPRELEN(,@07)                              1140 01108000
         ALR   CVIWKPT2,@07                                        1140 01109000
*       RFY CVIWORKR RSTD;  /*                               @DM14933*/ 01110000
*       DO WHILE CVILOOP='0'B; /*LOOP THROUGH SEGMENT        @DM14933*/ 01111000
         B     @DE01142                                            1142 01112000
@DL01142 DS    0H                                                  1143 01113000
*       CVIWORKR=CVIWKPT2; /*SAVE RDTE POINTER               @DM14933*/ 01114000
         LR    CVIWORKR,CVIWKPT2                                   1143 01115000
*       CVINSI6=CVIWKPT2->RPRLGONF; /*GET THE LOGON FORWARD POINTER     01116000
*                                     FOR THIS APPL RDTE     @DM14933*/ 01117000
         L     @09,RPRLGONF(,CVIWKPT2)                             1144 01118000
         ST    @09,CVINSI6                                         1144 01119000
*          DO WHILE CVINSI6^=CVIWKPT2; /*SCAN UNTIL THERE ARE NO MORE   01120000
*                                      ENTRIES ON THE CHAIN  @DM14933*/ 01121000
         B     @DE01145                                            1145 01122000
@DL01145 DS    0H                                                  1146 01123000
*          CVIWKPT2->RPRLGONF=CVIWKPT2; /*REINITIALIZE THE FORWARD 1146 01124000
*                                         POINTER            @DM14933*/ 01125000
         ST    CVIWKPT2,RPRLGONF(,CVIWKPT2)                        1146 01126000
*          CVIWKPT2->RPRLGONB=CVIWKPT2; /*REINITIALIZE THE BACKWARD     01127000
*                                         POINTER            @DM14933*/ 01128000
         ST    CVIWKPT2,RPRLGONB(,CVIWKPT2)                        1147 01129000
*          CVIWKPT2=CVINSI6; /*POINT TO THE NEXT ENTRY ON THIS APPL'S   01130000
*                              LOGON CHAIN                   @DM14933*/ 01131000
         L     @04,CVINSI6                                         1148 01132000
         LR    CVIWKPT2,@04                                        1148 01133000
*          CVINSI6=CVINSI6->RPRLGONF; /*POINT TO THE NEXT ENTRY ON THE  01134000
*                                       LOGON CHAIN          @DM14933*/ 01135000
         L     @09,RPRLGONF(,@04)                                  1149 01136000
         ST    @09,CVINSI6                                         1149 01137000
*          END; /* END DO WHILE                              @DM14933*/ 01138000
@DE01145 L     @09,CVINSI6                                         1150 01139000
         CR    @09,CVIWKPT2                                        1150 01140000
         BNE   @DL01145                                            1150 01141000
*       CVINSI6->RPRLGONB=CVINSI6; /*RESET THE LAST NODES BACKWARDS     01142000
*                                    POINTER                 @DM14933*/ 01143000
         ST    @09,RPRLGONB(,@09)                                  1151 01144000
*       CVIWKPT2=CVIWORKR; /*RESTORE THE RDTE SCAN ADDRESS   @DM14933*/ 01145000
         LR    CVIWKPT2,CVIWORKR                                   1152 01146000
*       IF CVIWKPT2->RPRENTED='0'B /*AT THE END OF THE SEGMENT?      */ 01147000
*          THEN /*NO, SO UPDATE POINTER TO THE NEXT APPL RDT @DM14933*/ 01148000
         TM    RPRENTED(CVIWKPT2),B'00000100'                      1153 01149000
         BNZ   @RF01153                                            1153 01150000
*             CVIWKPT2=CVIWKPT2+CVIWKPT2->RPRELEN; /*GET THE NEXT  1154 01151000
*                                          APPLICATION ENTRY @DM14933*/ 01152000
         AH    CVIWKPT2,RPRELEN(,CVIWKPT2)                         1154 01153000
*          ELSE DO; /*AT THE END OF THE SEGMENT, SO SET UP TO FREE 1155 01154000
*                     THE APPLICATION SEGMENT                @DM14933*/ 01155000
         B     @RC01153                                            1155 01156000
@RF01153 DS    0H                                                  1156 01157000
*               RPRVRYPD='1'B; /*SET VARY PENDING ON IN THE APPLICATION 01158000
*                                SEGMENT'S HEADER            @DM14933*/ 01159000
         L     @09,CVIWKPT1                                        1156 01160000
         OI    RPRVRYPD(@09),B'00000010'                           1156 01161000
*               CVINPUT=4; /*PREPARE TO RELEASE VOC LOCK     @DM14933*/ 01162000
         LA    CVINPUT,4                                           1157 01163000
*               CALL CVIUNLOC; /*RELEASE THE VOC LOCK        @DM14933*/ 01164000
         BAL   @14,CVIUNLOC                                        1158 01165000
*               CVINPUT=CVINCSPL; /*SET REG 1 = NCSPL ADDR   @DM14933*/ 01166000
         LR    CVINPUT,CVINCSPL                                    1159 01167000
*               CALL ISTINCFS; /*FREE APPL SEGMENT           @DM14933*/ 01168000
*                                                                  1160 01169000
         L     @15,@CV05723                                        1160 01170000
         BALR  @14,@15                                             1160 01171000
*                DO;                                               1161 01172000
*/***  CRRMAINT  PROCNM(ISTINCVI)COMP(VY)FUNC(RET)RPHADD((NCSPLRPH))  * 01173000
**/                                                                1162 01174000
*                /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/     1162 01175000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          1162 01176000
         L     @09,NCSPLRPH(,CVINCSPL)                             1162 01177000
         L     @09,RPHCRR(,@09)                                    1162 01178000
         L     @09,CRACRR(,@09)                                    1162 01179000
         XC    VYCCRT+1(11,@09),VYCCRT+1(@09)                      1162 01180000
         MVI   VYCCRT(@09),B'00000000'                             1162 01181000
*NCSPLRPH->RPHCRR->CRACRR->CINCVI = '1'B;                          1163 01182000
         OI    CINCVI(@09),B'00000100'                             1163 01183000
*                END; /*                       @DM14933*/          1164 01184000
*               CVIMSG#='105I'; /*SET UP FOR INACTIVE MSG    @DM14933*/ 01185000
         MVC   CVIMSG#(4),@CC05684                                 1165 01186000
*               CVILOOP='1'B; /*EXIT THE LOOP                @DM14933*/ 01187000
         OI    CVILOOP,B'00100000'                                 1166 01188000
*               END; /* END ELSE                             @DM14933*/ 01189000
*       END; /*END DO WHILE                                  @DM14933*/ 01190000
@RC01153 DS    0H                                                  1168 01191000
@DE01142 TM    CVILOOP,B'00100000'                                 1168 01192000
         BZ    @DL01142                                            1168 01193000
*       RFY CVIWORKR UNRSTD;  /*                             @DM14933*/ 01194000
*END;                                                              1170 01195000
*  /*SEGMENT(CVIAPPL)FINISH*/DO;GOTO CVIAPPL#;CVIAPPL@:END;        1171 01196000
@RC01120 DS    0H                                                  1172 01197000
         B     CVIAPPL#                                            1172 01198000
CVIAPPL@ DS    0H                                                  1174 01199000
*  /*SEGMENT(SETWKP2)START*/DO;GOTO SETWKP2@;SETWKP2:END;          1174 01200000
         B     SETWKP2@                                            1175 01201000
SETWKP2  DS    0H                                                  1177 01202000
*/********************************************************************/ 01203000
*/* INITIALIZE CVIWKPT2 TO THE ADDRESS OF THE FIRST RDT ENTRY TO BE  */ 01204000
*/* PROCESSED BY VARY DEACTIVATE NORMAL PROCESSING. INPUT TO THIS    */ 01205000
*/* SEGMENT IS CVIWKPT1=ADDRESS OF COMMAND RDT ENTRY AND CVINCSPL=   */ 01206000
*/* ADDRESS OF NCSPL.                                                */ 01207000
*/********************************************************************/ 01208000
*RFY (ISTRPRE,ISTRLN,ISTRPT) BASED(CVIWKPT1);                      1177 01209000
*/********************************************************************/ 01210000
*/* CVIWKPT2 WILL BE SAME AS CVIWKPT1 IF NONE OF FOLLOWING CHECKS    */ 01211000
*/* CAUSE A CHANGE TO CVIWKPT2.                                      */ 01212000
*/********************************************************************/ 01213000
*CVIWKPT2=CVIWKPT1;                                                1178 01214000
         L     @07,CVIWKPT1                                        1178 01215000
         LR    CVIWKPT2,@07                                        1178 01216000
*/********************************************************************/ 01217000
*/* IF COMMAND RDT ENTRY IS LCDO,THEN SET CVIWKPT2 TO ADDRESS OF RDT */ 01218000
*/* ENTRY IN FIRST SLOT OF ISTDOT TABLE (DOTADTRM) POINTED TO BY     */ 01219000
*/* RDSADDOT OF THE LCDO ENTRY.                                      */ 01220000
*/********************************************************************/ 01221000
*IF RPRENTRY=RPRENTDS                                              1179 01222000
* THEN DO;                                                         1179 01223000
         CLI   RPRENTRY(@07),X'20'                                 1179 01224000
         BNE   @RF01179                                            1179 01225000
*  RFY ISTRDS BASED(CVIWKPT1);                                     1181 01226000
*  CVIDOTCT=1;                                                     1182 01227000
         MVC   CVIDOTCT(2),@CH00096                                1182 01228000
*  CVINXDOT=RDSADDOT;                                              1183 01229000
         L     @09,RDSADDOT(,@07)                                  1183 01230000
         ST    @09,CVINXDOT                                        1183 01231000
*  CVIWKPT2=CVINXDOT->DOTADTRM ;                                   1184 01232000
         L     CVIWKPT2,DOTADTRM(,@09)                             1184 01233000
* END;                                                             1185 01234000
*/********************************************************************/ 01235000
*/* ELSE IF COMMAND RDT ENTRY IS FOR A PORT (RPRENTLN TYPE ENTRY     */ 01236000
*/* WITH RLNPORT='1'B) WHICH HAS AN ASSOCIATED TERMINAL (RPTATERM NOT*/ 01237000
*/* ZERO) THEN SET CVIWKPT2 TO ADDRESS OF RDT ENTRY FOR ASSOCIATED   */ 01238000
*/* TERMINAL. IF LINE ENTRY IS NOT A PORT BUT IS A PURE DIAL OUT     */ 01239000
*/* LINE (RLNRACAN='1'B AND RLNRACAT='0'B) THEN SET CVIWKPT2 TO      */ 01240000
*/* ADDRESS OF THE LINE ENTRY.                                       */ 01241000
*/********************************************************************/ 01242000
*ELSE                                                              1186 01243000
* IF RPRENTRY=RPRENTLN & RLNPORT='1'B & RPTATERM ^= 0              1186 01244000
*  THEN CVIWKPT2=RPTATERM;                                         1186 01245000
         B     @RC01179                                            1186 01246000
@RF01179 L     @09,CVIWKPT1                                        1186 01247000
         CLI   RPRENTRY(@09),X'50'                                 1186 01248000
         BNE   @RF01186                                            1186 01249000
         TM    RLNPORT(@09),B'00010000'                            1186 01250000
         BNO   @RF01186                                            1186 01251000
         L     @09,RPTATERM(,@09)                                  1186 01252000
         LTR   @09,@09                                             1186 01253000
         BZ    @RF01186                                            1186 01254000
         LR    CVIWKPT2,@09                                        1187 01255000
*  ELSE                                                            1188 01256000
*   IF RPRENTRY=RPRENTLN & RLNRACAN='1'B & RLNRACAT='0'B           1188 01257000
*    THEN;                                                         1188 01258000
         B     @RC01186                                            1188 01259000
@RF01186 L     @09,CVIWKPT1                                        1188 01260000
         CLI   RPRENTRY(@09),X'50'                                 1188 01261000
         BNE   @RF01188                                            1188 01262000
         TM    RLNRACAN(@09),B'01000000'                           1188 01263000
         BNO   @RF01188                                            1188 01264000
         TM    RLNRACAT(@09),B'10000000'                           1188 01265000
         BZ    @RT01188                                            1188 01266000
*    ELSE                                                          1190 01267000
*/********************************************************************/ 01268000
*/* ELSE IF COMMAND RDT ENTRY IS FOR 370X, CLUSTER, TERMINAL,        */ 01269000
*/* COMPONENT, LOCAL HEADER, OR LEASED LINE,THEN SET CVIWKPT2        */ 01270000
*/* TO THE ADDRESS OF THE LOWEST LEVEL RDT ENTRY TO WHICH THE VARY   */ 01271000
*/* REQUEST APPLIES.                                                 */ 01272000
*/* IN THIS CASE THE STRUCTURE OF THE RDT SEGMENT ALLOWS FOR FINDING */ 01273000
*/* THE LOWEST LEVEL RDT ENTRY TO WHICH THE VARY REQUEST APPLIES BY  */ 01274000
*/* SEARCHING ENTRIES BELOW CURRENT UNTIL EITHER THE LAST ENTRY      */ 01275000
*/* IN THE SEGMENT IS FOUND (RPRENTED='1'B) OR AN ENTRY TYPE CODE    */ 01276000
*/* LESS THAN OR EQUAL TO THAT OF THE COMMAND RDT ENTRY IS FOUND.    */ 01277000
*/********************************************************************/ 01278000
* IF RPRENTRY=RPRENTRN|RPRENTRY=RPRENTCL|RPRENTRY=RPRENTTR|        1190 01279000
*    RPRENTRY=RPRENTCM|RPRENTRY=RPRENTLH|RPRENTRY=RPRENTCC|        1190 01280000
*    RPRENTRY=RPRENTLU|                                            1190 01281000
*     (RPRENTRY=RPRENTLN & RLNRACAT='0'B & RLNRACAN='0'B & RLNPORT='0'B 01282000
*  THEN DO;                                                        1190 01283000
@RF01188 L     @09,CVIWKPT1                                        1190 01284000
         CLI   RPRENTRY(@09),X'01'                                 1190 01285000
         BE    @RT01190                                            1190 01286000
         CLI   RPRENTRY(@09),X'70'                                 1190 01287000
         BE    @RT01190                                            1190 01288000
         CLI   RPRENTRY(@09),X'80'                                 1190 01289000
         BE    @RT01190                                            1190 01290000
         CLI   RPRENTRY(@09),X'90'                                 1190 01291000
         BE    @RT01190                                            1190 01292000
         CLI   RPRENTRY(@09),X'03'                                 1190 01293000
         BE    @RT01190                                            1190 01294000
         CLI   RPRENTRY(@09),X'71'                                 1190 01295000
         BE    @RT01190                                            1190 01296000
         CLI   RPRENTRY(@09),X'81'                                 1190 01297000
         BE    @RT01190                                            1190 01298000
         CLI   RPRENTRY(@09),X'50'                                 1190 01299000
         BNE   @RF01190                                            1190 01300000
         TM    RLNRACAT(@09),B'11010000'                           1190 01301000
         BNZ   @RF01190                                            1190 01302000
@RT01190 DS    0H                                                  1191 01303000
*   CVILOOP='0'B;                                                  1192 01304000
         NI    CVILOOP,B'11011111'                                 1192 01305000
*   RFY ISTRPRE BASED(CVIWKPT2);                                   1193 01306000
*   IF RPRENTED='0'B                                               1194 01307000
*    THEN DO WHILE(CVILOOP='0'B);                                  1194 01308000
         TM    RPRENTED(CVIWKPT2),B'00000100'                      1194 01309000
         BNZ   @RF01194                                            1194 01310000
         B     @DE01195                                            1195 01311000
@DL01195 DS    0H                                                  1196 01312000
*     CVIWKPT2=CVIWKPT2+RPRELEN;                                   1196 01313000
         AH    CVIWKPT2,RPRELEN(,CVIWKPT2)                         1196 01314000
*     IF RPRENTRY<=CVIWKPT1->RPRENTRY                              1197 01315000
*      THEN DO;                                                    1197 01316000
         L     @09,CVIWKPT1                                        1197 01317000
         CLC   RPRENTRY(1,CVIWKPT2),RPRENTRY(@09)                  1197 01318000
         BH    @RF01197                                            1197 01319000
*       CVIWKPT2=CVIWKPT2-RPRPELEN;                                1199 01320000
         SH    CVIWKPT2,RPRPELEN(,CVIWKPT2)                        1199 01321000
*       CVILOOP='1'B;                                              1200 01322000
         OI    CVILOOP,B'00100000'                                 1200 01323000
*      END;                                                        1201 01324000
*     ELSE                                                         1202 01325000
*     IF RPRENTED='1'B                                             1202 01326000
*      THEN CVILOOP='1'B;                                          1202 01327000
         B     @RC01197                                            1202 01328000
@RF01197 TM    RPRENTED(CVIWKPT2),B'00000100'                      1202 01329000
         BNO   @RF01202                                            1202 01330000
         OI    CVILOOP,B'00100000'                                 1203 01331000
*     ELSE;                                                        1204 01332000
@RF01202 DS    0H                                                  1205 01333000
*    END;                                                          1205 01334000
@RC01197 DS    0H                                                  1205 01335000
@DE01195 TM    CVILOOP,B'00100000'                                 1205 01336000
         BZ    @DL01195                                            1205 01337000
*   ELSE;                                                          1206 01338000
@RF01194 DS    0H                                                  1207 01339000
*  END;                                                            1207 01340000
*  ELSE;                                                           1208 01341000
@RF01190 DS    0H                                                  1209 01342000
*  /*SEGMENT(SETWKP2)FINISH*/DO;GOTO SETWKP2#;SETWKP2@:END;        1209 01343000
@RT01188 DS    0H                                                  1209 01344000
@RC01186 DS    0H                                                  1209 01345000
@RC01179 DS    0H                                                  1210 01346000
         B     SETWKP2#                                            1210 01347000
SETWKP2@ DS    0H                                                  1212 01348000
* GEN(  EJECT);                                                    1212 01349000
           EJECT                                                        01350000
*  /*SEGMENT(NXTWKP2)START*/DO;GOTO NXTWKP2@;NXTWKP2:END;          1213 01351000
         B     NXTWKP2@                                            1214 01352000
NXTWKP2  DS    0H                                                  1216 01353000
*/********************************************************************/ 01354000
*/* UPDATE CVIWKPT2 TO THE ADDRESS OF THE NEXT RDT ENTRY TO BE       */ 01355000
*/* PROCESSED BY VARY DEACTIVATE NORMAL PROCESSING. INPUT TO THIS    */ 01356000
*/* SEGMENT IS CVIWKPT2=ADDRESS OF LAST RDT ENTRY PROCESSED AND      */ 01357000
*/* CVIWKPT1=ADDRESS OF COMMAND RDT ENTRY.                           */ 01358000
*/* SET LOOP ESCAPE TO TERMINATE VARY NORMAL PROCESSING IF ALREADY   */ 01359000
*/* PROCESSED RDT ENTRY COMMAND WAS AGAINST.                         */ 01360000
*/********************************************************************/ 01361000
* RFY ISTRPRE BASED(CVIWKPT1);                                     1216 01362000
*IF CVIWKPT2=CVIWKPT1                                              1217 01363000
* THEN CVILOOP='1'B;                                               1217 01364000
         C     CVIWKPT2,CVIWKPT1                                   1217 01365000
         BNE   @RF01217                                            1217 01366000
         OI    CVILOOP,B'00100000'                                 1218 01367000
*/********************************************************************/ 01368000
*/* IF VARY COMMAND AGAINST AN LCDO, THEN SET CVIWKPT2 TO ADDRESS    */ 01369000
*/* OF RDT ENTRY FROM NEXT ISTDOT TABLE SLOT, OR, IF ALL THE ISTDOT  */ 01370000
*/* ENTRIES HAVE BEEN PROCESSED (CVIDOTCT IS ACCUMULATOR FOR NUMBER  */ 01371000
*/* OF ISTDOT ENTRIES PROCESSED AND RDTSCTTRM IN THE LCDO RDT ENTRY  */ 01372000
*/* IS THE COUNT OF ISTDOT ENTRIES) THEN SET CVIWKPT2 TO ADDRESS OF  */ 01373000
*/* LCDO RDT ENTRY.                                                  */ 01374000
*/********************************************************************/ 01375000
*ELSE DO;                                                          1219 01376000
         B     @RC01217                                            1219 01377000
@RF01217 DS    0H                                                  1220 01378000
* IF RPRENTRY=RPRENTDS                                             1220 01379000
*  THEN DO;                                                        1220 01380000
         L     @09,CVIWKPT1                                        1220 01381000
         CLI   RPRENTRY(@09),X'20'                                 1220 01382000
         BNE   @RF01220                                            1220 01383000
*   RFY ISTRDS BASED(CVIWKPT1);                                    1222 01384000
*   CVIDOTCT=CVIDOTCT+1;                                           1223 01385000
         LA    @07,1                                               1223 01386000
         AH    @07,CVIDOTCT                                        1223 01387000
         STH   @07,CVIDOTCT                                        1223 01388000
*    CVINXDOT=CVINXDOT+LENGTH(ISTDOT);                             1224 01389000
         LA    @04,4                                               1224 01390000
         AL    @04,CVINXDOT                                        1224 01391000
         ST    @04,CVINXDOT                                        1224 01392000
*   IF CVIDOTCT>RDSCTTRM                                           1225 01393000
*    THEN CVIWKPT2=CVIWKPT1;                                       1225 01394000
         CH    @07,RDSCTTRM(,@09)                                  1225 01395000
         BNH   @RF01225                                            1225 01396000
         LR    CVIWKPT2,@09                                        1226 01397000
*   ELSE CVIWKPT2=CVINXDOT->DOTADTRM ;                             1227 01398000
         B     @RC01225                                            1227 01399000
@RF01225 L     @07,CVINXDOT                                        1227 01400000
         L     CVIWKPT2,DOTADTRM(,@07)                             1227 01401000
*  END;                                                            1228 01402000
*/********************************************************************/ 01403000
*/* ELSE IF VARY COMMAND AGAINST A PORT, THEN SET CVIWKPT2 TO ADDRESS * 01404000
*/* OF PORT ENTRY. OTHERWISE, BACK UP CVIWKPT2 BY THE LENGTH OF THE  */ 01405000
*/* PREVIOUS RDT ENTRY.                                              */ 01406000
*/********************************************************************/ 01407000
* ELSE                                                             1229 01408000
*   IF RPRENTRY=RPRENTLN & RLNPORT='1'B                            1229 01409000
*    THEN CVIWKPT2=CVIWKPT1;                                       1229 01410000
         B     @RC01220                                            1229 01411000
@RF01220 L     @09,CVIWKPT1                                        1229 01412000
         CLI   RPRENTRY(@09),X'50'                                 1229 01413000
         BNE   @RF01229                                            1229 01414000
         TM    RLNPORT(@09),B'00010000'                            1229 01415000
         BNO   @RF01229                                            1229 01416000
         LR    CVIWKPT2,@09                                        1230 01417000
*   ELSE CVIWKPT2=CVIWKPT2-CVIWKPT2->RPRPELEN;                     1231 01418000
         B     @RC01229                                            1231 01419000
@RF01229 SH    CVIWKPT2,RPRPELEN(,CVIWKPT2)                        1231 01420000
*END;                                                              1232 01421000
@RC01229 DS    0H                                                  1232 01422000
@RC01220 DS    0H                                                  1233 01423000
*  /*SEGMENT(NXTWKP2)FINISH*/DO;GOTO NXTWKP2#;NXTWKP2@:END;        1233 01424000
@RC01217 DS    0H                                                  1234 01425000
         B     NXTWKP2#                                            1234 01426000
NXTWKP2@ DS    0H                                                  1236 01427000
* GEN(  EJECT);                                                    1236 01428000
           EJECT                                                        01429000
*  /*SEGMENT(CVIQUIS)START*/DO;GOTO CVIQUIS@;CVIQUIS:END;          1237 01430000
         B     CVIQUIS@                                            1238 01431000
CVIQUIS  DS    0H                                                  1240 01432000
*/********************************************************************/ 01433000
*/* THIS ROUTINE IS INVOKED BY ISTINCVI MAINLINE IF A MODIFY COMMAND */ 01434000
*/* WAS ISSUED FOR THE RN.  THE DNCB ADDRESS IS OBTAINED VIA CIDCTL  */ 01435000
*/* FIND USING THE SAF/DAF (RPRCOMID) IN THE RN RDTE.  A CALL IS MADE*/ 01436000
*/* TO CVIDOIO TO ISSUE QUIESCE LCCW.  IF THE CID FIND OR THE        */ 01437000
*/* QUIESCE IS NOT SUCCESSFUL, AN ERROR MESSAGE IS ISSUED AND THE    */ 01438000
*/* INTERNAL 'QUIT' SWITCH IS SET ON.                                */ 01439000
*/********************************************************************/ 01440000
*NCSIOCID=CVISAF+NCSPLRPT->RPRDAF;                                 1240 01441000
         L     @09,NCSPLRPT(,CVINCSPL)                             1240 01442000
         LH    @09,RPRDAF(,@09)                                    1240 01443000
         N     @09,@CF06623                                        1240 01444000
         ST    @09,NCSIOCID(,CVINCSPL)                             1240 01445000
*NCSPLCMD=NCSCQUIS;                                                1241 01446000
         MVC   NCSPLCMD(2,CVINCSPL),@CB03396                       1241 01447000
*  CALL CVIDOIO;                                                   1242 01448000
         BAL   @14,CVIDOIO                                         1242 01449000
*  IF NCSIORC=NCSIOSUC                                             1243 01450000
*   THEN;                                                          1243 01451000
         CLI   NCSIORC(CVINCSPL),X'00'                             1243 01452000
         BE    @RT01243                                            1243 01453000
*   ELSE DO;                                                       1245 01454000
*    CVINPUT=0;                                            /*@ZA30798*/ 01455000
         SLR   CVINPUT,CVINPUT                                     1246 01456000
*    CALL CVILOC;                   /* GET RDT LOCK          @ZA30798*/ 01457000
         BAL   @14,CVILOC                                          1247 01458000
*    RRNLCDIP='0'B;                 /* RESET DEACT IN PROG   @ZA30798*/ 01459000
         L     @09,CVIWKPT1                                        1248 01460000
         NI    RRNLCDIP(@09),B'11110111'                           1248 01461000
*    CVINPUT=0;                                            /*@ZA30798*/ 01462000
         SLR   CVINPUT,CVINPUT                                     1249 01463000
*    CALL CVIUNLOC;                 /* RELEASE RDT LOCK      @ZA30798*/ 01464000
         BAL   @14,CVIUNLOC                                        1250 01465000
*    CVIQUIT='1'B;                                                 1251 01466000
         OI    CVIQUIT,B'00000100'                                 1251 01467000
*    CVIMSG# = '163I';                                             1252 01468000
