*     RFY                                                          0910 00487000
*      (EMCR2,                                                     0910 00488000
*       EMCR3) UNRSTD;                                             0910 00489000
*   END;                            /* CIDCTL GET DAF LOCK SHARED FOR   00490000
*                                      CIO                           */ 00491000
*   IF CVPRTNCD=0                   /*                       @ZA30081*/ 00492000
*     THEN                          /*                       @ZA30081*/ 00493000
         LTR   CVPRTNCD,CVPRTNCD                                   0912 00494000
         BNZ   @RF00912                                            0912 00495000
*     DO;                           /*                       @ZA30081*/ 00496000
