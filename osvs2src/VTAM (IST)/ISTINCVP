         LH    @03,RPRPEPCT(,@04)                                  1252 01601000
         LTR   @03,@03                                             1252 01602000
         BZ    @RF01252                                            1252 01603000
*         RPRPEPCT=RPRPEPCT-1;      /* DECREMENT PEP COUNT IF NOT  1253 01604000
*                                      ALREADY 0                     */ 01605000
         BCTR  @03,0                                               1253 01606000
         STH   @03,RPRPEPCT(,@04)                                  1253 01607000
*       IF RPRPEPCT=0 THEN          /* IF PENDING COUNT HAS REACHED 0*/ 01608000
@RF01252 L     @04,CVPWKPT1                                        1254 01609000
         LH    @03,RPRPEPCT(,@04)                                  1254 01610000
         LTR   @03,@03                                             1254 01611000
         BNZ   @RF01254                                            1254 01612000
*         DO;                                                      1255 01613000
*           RPRINIMM='0'B;          /* AS12052                       */ 01614000
*                                                                  1256 01615000
         NI    RPRINIMM(@04),B'11111110'                           1256 01616000
*           /*********************************************************/ 01617000
*           /*                                                       */ 01618000
*           /* SEGMENT(CVPSCAN)REQUEST                               */ 01619000
*           /*                                                       */ 01620000
*           /*********************************************************/ 01621000
*                                                                  1257 01622000
*           DO;                                                    1257 01623000
*             GOTO CVPSCAN;                                        1258 01624000
         B     CVPSCAN                                             1258 01625000
*CVPSCAN#:                                                         1259 01626000
*           END;                    /* PROCESS ENTRY                 */ 01627000
CVPSCAN# DS    0H                                                  1260 01628000
*           IF CVPSKPHD=CVPON THEN  /* IF SKIP HEAD SWITCH ON        */ 01629000
         TM    CVPSKPHD,B'00100000'                                1260 01630000
         BNO   @RF01260                                            1260 01631000
*             DO;                                                  1261 01632000
*               CVPMSG#='105I';     /* SET MSG # TO OFFLINE          */ 01633000
         MVC   CVPMSG#(4),@CC05831                                 1262 01634000
*               CVPRID=ADDR(NCSPLRID);/* SET UP RID FOR TPMSG        */ 01635000
         L     @04,CVPNCSPL                                        1263 01636000
         LA    @04,NCSPLRID(,@04)                                  1263 01637000
         ST    @04,CVPRID                                          1263 01638000
*               CALL CVPWTO;        /* ISSUE MSG                     */ 01639000
         BAL   @14,CVPWTO                                          1264 01640000
*             END;                                                 1265 01641000
*           ELSE                                                   1266 01642000
*             DO;                   /* ELSE SKIP HEAD SWITCH NOT ON  */ 01643000
         B     @RC01260                                            1266 01644000
@RF01260 DS    0H                                                  1267 01645000
*               IF RPRVRYPD=CVPON THEN/* ELSE IF VARY PENDING SET    */ 01646000
         L     @04,CVPWKPT1                                        1267 01647000
         TM    RPRVRYPD(@04),B'00000010'                           1267 01648000
         BNO   @RF01267                                            1267 01649000
*                 DO;                                              1268 01650000
*                   CVPMSG#='105I'; /* SET MSG # TO OFFLINE          */ 01651000
         MVC   CVPMSG#(4),@CC05831                                 1269 01652000
*                   CVPRID=ADDR(RPRNAME);/* SET UP NAME FOR TPMSG    */ 01653000
         ST    @04,CVPRID                                          1270 01654000
*                   CALL CVPWTO;    /* ISSUE MSG                     */ 01655000
         BAL   @14,CVPWTO                                          1271 01656000
*                   RPRVRYPD=CVPOFF;/* TURN OFF VARY PENDING         */ 01657000
         L     @04,CVPWKPT1                                        1272 01658000
         NI    RPRVRYPD(@04),B'11111101'                           1272 01659000
*                   RPRINIMM='0'B;  /* AS12052                       */ 01660000
         NI    RPRINIMM(@04),B'11111110'                           1273 01661000
*                 END;                                             1274 01662000
*               ELSE                                               1275 01663000
*                 ;                 /* NO VARY PENDING               */ 01664000
@RF01267 DS    0H                                                  1276 01665000
*               IF RPRENTRY=RPRENTLN&RLNPORT=CVPON THEN/* ENTRY LCDI?*/ 01666000
         L     @04,CVPWKPT1                                        1276 01667000
         CLI   RPRENTRY(@04),X'50'                                 1276 01668000
         BNE   @RF01276                                            1276 01669000
         TM    RLNPORT(@04),B'00010000'                            1276 01670000
         BNO   @RF01276                                            1276 01671000
*                 IF RPTVRYPD=CVPON THEN/* CTERM VQRY PD?            */ 01672000
         TM    RPTVRYPD(@04),B'01000000'                           1277 01673000
         BNO   @RF01277                                            1277 01674000
*                   DO;                                            1278 01675000
*                     CVPMSG#='105I';/* SET UP OFFLINE MSG           */ 01676000
