*   DO;                             /* CIDCTL(FIND)                  */ 00472000
*     RFY                                                          0905 00473000
*      (EMCR2,                                                     0905 00474000
*       EMCR3) RSTD;                                               0905 00475000
*     GEN SETS(RPHWORK)                                            0906 00476000
*         (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);     0906 00477000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00478000
*     EMCR3='00'X;                  /* CLEAR EXTRA BITS              */ 00479000
         SLR   EMCR3,EMCR3                                         0907 00480000
*     EMCR3='01'X;                  /* INDICATE SHARED LOCK          */ 00481000
         LA    EMCR3,1                                             0908 00482000
*     CALL ATCLCPTR->ATCCDFND->ISTSDCCF;/* PROCESS REQUEST           */ 00483000
         L     @04,ATCLCPTR                                        0909 00484000
         L     @15,ATCCDFND(,@04)                                  0909 00485000
