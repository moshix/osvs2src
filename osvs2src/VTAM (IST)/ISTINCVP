         ALR   @09,@06                                             0797 00084000
         ST    @09,NXTAVPTR(,@04)                                  0797 00085000
*   CVPSWICH=CVPSWICH&&CVPSWICH;    /* CLEAR ALL SWITCHES            */ 00086000
         XC    CVPSWICH(2),CVPSWICH                                0798 00087000
*   CVPSUBRN=NCSPLRPT;              /*                       @DC36143*/ 00088000
         ST    @03,CVPSUBRN                                        0799 00089000
*   IF NCSPLRPT->RPRVRYPD='1'B|NCSPLRPT->RPRHLVPD='1'B THEN        0800 00090000
         TM    RPRVRYPD(@03),B'00000010'                           0800 00091000
