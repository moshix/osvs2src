         ST    @03,NCSPLCID(,@04)                                  1120 01170000
*       NCSPLRAD=NCSPLRPT;                                         1121 01171000
         L     @03,NCSPLRPT(,@04)                                  1121 01172000
         ST    @03,NCSPLRAD(,@04)                                  1121 01173000
*       CVPINPUT=NCSPLRPH;                                         1122 01174000
         L     CVPINPUT,NCSPLRPH(,@04)                             1122 01175000
*       CVPINPUT->RPHWEA=CVPNCSPL;                                 1123 01176000
*                                                                  1123 01177000
         ST    @04,RPHWEA(,CVPINPUT)                               1123 01178000
*/* TPREDRVE                                                         */ 01179000
*                                                                  1124 01180000
*       DO;                         /* BEGIN TPREDRVE GENERATION     */ 01181000
*/* ATBUILD (ISTRPH)                                                 */ 01182000
*/* END OF ATBUILD                                                   */ 01183000
*/* ATBUILD (ISTCRA)                                                 */ 01184000
*/* END OF ATBUILD                                                   */ 01185000
*/* ATBUILD (ISTNCSPL)                                               */ 01186000
*/* END OF ATBUILD                                                   */ 01187000
