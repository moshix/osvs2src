*       NCSPLRUO=ADDR(CVPDCLRU);    /* PUT RU ADDR. IN NCSPL @ZM32959*/ 02017000
         LA    @04,CVPDCLRU                                        1379 02018000
         ST    @04,NCSPLRUO(,@03)                                  1379 02019000
*       CALL CVPDOIO;               /*                       @ZM32959*/ 02020000
         BAL   @14,CVPDOIO                                         1380 02021000
*       RPRLTASN='0'B;              /* TURN OFF LINE TRACE IN LINE 1381 02022000
