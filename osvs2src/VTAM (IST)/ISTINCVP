*   GEN(   EJECT);                                                 1327 01833000
*                                                                  1327 01834000
            EJECT                                                       01835000
*/* SEGMENT(CVPEPL3)START                                            */ 01836000
*                                                                  1328 01837000
*   DO;                                                            1328 01838000
*     GOTO CVPEPL3@;                                               1329 01839000
         B     CVPEPL3@                                            1329 01840000
*CVPEPL3:                                                          1330 01841000
*   END;                                                           1330 01842000
*                                                                  1330 01843000
CVPEPL3  DS    0H                                                  1331 01844000
*/********************************************************************/ 01845000
*/* THIS SEGMENT IS INVOKED BY CVPPEPLN TO SWITCH THE PEP LINE BEING */ 01846000
*/* DEACTIVATED TO EP MODE AND TO DEALLOCATE THE PEP LINE FROM VTAM. */ 01847000
*/* CVPWKPT1 IS THE ADDRESS OF THE RDT ENTRY FOR THE PEP LINE.       */ 01848000
*/********************************************************************/ 01849000
*                                                                  1331 01850000
*   RFY                                                            1331 01851000
*    (ISTRPRE,                                                     1331 01852000
*     ISTRLN,                                                      1331 01853000
*     ISTRPT) BASED(CVPWKPT1);      /*                       @DM12066*/ 01854000
*/********************************************************************/ 01855000
*/* IF THE QUIT SWITCH HAS NOT BEEN SET,THEN SET THE NCSPL COMMAND   */ 01856000
*/* CODE TO 'SWITCH TO EP', LOCATE THE 370X RDT ENTRY TO OBTAIN THE  */ 01857000
*/* DAF OF THE 370X, BUILD THE CID IN NCSIOCID USING THE 370X DAF,   */ 01858000
*/* OBTAIN THE DAF LOCK VIA CIDCTL, AND CALL ISTINCIO TO ISSUE THE   */ 01859000
*/* COMMAND TO THE 370X.                                             */ 01860000
*/********************************************************************/ 01861000
*                                                                  1332 01862000
*   IF CVPQUIT=CVPOFF THEN                                         1332 01863000
         TM    CVPQUIT,B'10000000'                                 1332 01864000
         BNZ   @RF01332                                            1332 01865000
*     DO;                                                          1333 01866000
*       NCSPLCMD=NCSCSEP;           /* SWITCH TO EMULATOR PGM        */ 01867000
         L     @04,CVPNCSPL                                        1334 01868000
         MVC   NCSPLCMD(2,@04),@CB03747                            1334 01869000
*       CVPP3OP='52'X;                                             1335 01870000
         MVI   CVPP3OP,X'52'                                       1335 01871000
*       IF RLNPORT=CVPON THEN                                      1336 01872000
         L     @04,CVPWKPT1                                        1336 01873000
         TM    RLNPORT(@04),B'00010000'                            1336 01874000
         BNO   @RF01336                                            1336 01875000
*         CVPP3DAF=RPTLNDAF;        /* USE LINE DAF IN PORT ENTRY  1337 01876000
*                                                            @DM12066*/ 01877000
         MVC   CVPP3DAF(2),RPTLNDAF(@04)                           1337 01878000
*       ELSE                                                       1338 01879000
*         CVPP3DAF=RPRDAF;          /* GET DAF FROM RDTE     @DM12066*/ 01880000
         B     @RC01336                                            1338 01881000
@RF01336 L     @04,CVPWKPT1                                        1338 01882000
         MVC   CVPP3DAF(2),RPRDAF(@04)                             1338 01883000
*       NCSPLRUO=ADDR(CVPP3RU);     /* SET OUTBOUND ADDRESS          */ 01884000
@RC01336 L     @04,CVPNCSPL                                        1339 01885000
         LA    @03,CVPP3RU                                         1339 01886000
         ST    @03,NCSPLRUO(,@04)                                  1339 01887000
*       NCSPLOLN=LENGTH(CVPP3RU);   /* SET BUFFER LENGTH             */ 01888000
         MVC   NCSPLOLN(2,@04),@CH00091                            1340 01889000
*       NCSPLRUI=0;                 /* INDICATE NO DATA SAVED        */ 01890000
         SLR   @03,@03                                             1341 01891000
         ST    @03,NCSPLRUI(,@04)                                  1341 01892000
*       CALL CVPDOIO;                                              1342 01893000
*                                                                  1342 01894000
         BAL   @14,CVPDOIO                                         1342 01895000
*/********************************************************************/ 01896000
*/* UPON RETURN, UPDATE THE CRR AUDIT TRAIL. IF NCSPL RETURN CODE    */ 01897000
*/* INDICATES THAT I/O FIALED, THEN, IF I/O WAS PURGED, SET THE QUIT */ 01898000
*/* SWITCH. CALL CVPWTO TO ISSUE ERROR MESSAGE FOR 'SWITCH TO EP'    */ 01899000
*/* FAILURE.                                                         */ 01900000
*/********************************************************************/ 01901000
*                                                                  1343 01902000
*       IF NCSIORC^=NCSIOSUC THEN                                  1343 01903000
         L     @04,CVPNCSPL                                        1343 01904000
         CLI   NCSIORC(@04),X'00'                                  1343 01905000
         BE    @RF01343                                            1343 01906000
*         DO;                                                      1344 01907000
*           CVPMSG#='158I';                                        1345 01908000
         MVC   CVPMSG#(4),@CC05936                                 1345 01909000
*           CVPRID=ADDR(RPRNAME);                                  1346 01910000
         L     @04,CVPWKPT1                                        1346 01911000
         ST    @04,CVPRID                                          1346 01912000
*           CALL CVPWTO;                                           1347 01913000
         BAL   @14,CVPWTO                                          1347 01914000
*         END;                                                     1348 01915000
*       ELSE                                                       1349 01916000
*         DO;                       /* INDICATE LINE ON EP SIDE      */ 01917000
         B     @RC01343                                            1349 01918000
@RF01343 DS    0H                                                  1350 01919000
*           RLNSWNCP=CVPOFF;        /* INDICATE LINE ON EP SIDE      */ 01920000
*           RLNSWEP=CVPON;                                         1351 01921000
         L     @04,CVPWKPT1                                        1351 01922000
         OI    RLNSWEP(@04),B'00000100'                            1351 01923000
         NI    RLNSWNCP(@04),B'10111111'                           1351 01924000
*         END;                                                     1352 01925000
*     END;                          /* END FOR QUIT SWITCH NOT SET   */ 01926000
*   ELSE                                                           1354 01927000
*     CVPQUIT=CVPOFF;                                              1354 01928000
*                                                                  1354 01929000
         B     @RC01332                                            1354 01930000
@RF01332 NI    CVPQUIT,B'01111111'                                 1354 01931000
*/* SEGMENT(CVPEPL3)FINISH                                           */ 01932000
*                                                                  1355 01933000
*   DO;                                                            1355 01934000
@RC01332 DS    0H                                                  1356 01935000
*     GOTO CVPEPL3#;                                               1356 01936000
         B     CVPEPL3#                                            1356 01937000
*CVPEPL3@:                                                         1357 01938000
*   END;                                                           1357 01939000
CVPEPL3@ DS    0H                                                  1358 01940000
*   GEN(   EJECT);                                                 1358 01941000
