*CVPDISC:                                                          2038 04136000
*   END;                                                           2038 04137000
*                                                                  2038 04138000
CVPDISC  DS    0H                                                  2039 04139000
*/********************************************************************  04140000
*                                                                    */ 04141000
*/*   THE FUNCTION OF CVPDISC IS TO SCAN THE RDT SEGMENT FOR ACTIVE  */ 04142000
*/* SUBNODES AT THE TIME THIS NODE WAS DEACTIVATED. FOR EACH ACTIVE  */ 04143000
*/* SUBNODE FOUND, A DISCONTACT RU MUST BE SENT & THEN THE SDLC LINK */ 04144000
*/* MUST BE DEACTIVATED VIA A DEACTIVATE LINK RU.                    */ 04145000
*/*   THE ROUTINE CONSISTS OF TWO DO WHILE LOOPS, A MAJOR LOOP WHICH */ 04146000
*/* HANDLES DEACTIVATE LINKS FOR EACH SDLC LINK. THE MINOR DO WHILE  */ 04147000
*/* LOOP SCANS EACH SUBENTRY UNDER THE PARTICULAR SDLC LINK FOR A NEW*/ 04148000
*/* PU, REMOTE 370X, OR LU. FOR NEW PUS & REMOTE 370XS, IF           */ 04149000
*/* DISCONTACT IS REQUIRED, THE DISCONTACT IS DONE. FOR REMOTE 370XS */ 04150000
*/* IF POWER OFF IS REQUIRED, THIS RU IS SENT ALSO.                  */ 04151000
*/********************************************************************  04152000
*                                                                    */ 04153000
*                                                                  2039 04154000
*   RFY                                                            2039 04155000
*    (ISTRPRE,                                                     2039 04156000
*     ISTRCPRE,                                                    2039 04157000
*     ISTRIN) BASED(CVPSUBRN);                                     2039 04158000
*   CVPDISOP='010202'X;             /* SET OP CODES IN 3 RU'S        */ 04159000
         MVC   CVPDISOP(3),@CB06034                                2040 04160000
*   CVPDLOP='01020B'X;                                             2041 04161000
         MVC   CVPDLOP(3),@CB05944                                 2041 04162000
*   CVPPOOP='010209'X;                                             2042 04163000
         MVC   CVPPOOP(3),@CB06035                                 2042 04164000
*   NCSPLOLN=CVPDLEN;               /* SET OUTBOUND LENGTH           */ 04165000
         L     @04,CVPNCSPL                                        2043 04166000
         MVC   NCSPLOLN(2,@04),@CH00133                            2043 04167000
*   NCSPLRUI=0;                     /* INDICATE NO DATA SAVED        */ 04168000
         SLR   @03,@03                                             2044 04169000
         ST    @03,NCSPLRUI(,@04)                                  2044 04170000
