         L     @03,CVPNCSPL                                        1105 01123000
         ST    @04,NCSPLRPT(,@03)                                  1105 01124000
*   IF RPRENTRY=RPRENTRN THEN                                      1106 01125000
         CLI   RPRENTRY(@04),X'01'                                 1106 01126000
         BNE   @RF01106                                            1106 01127000
*     DO;                                                          1107 01128000
*       IF NCSPL2IN='1'B THEN                                      1108 01129000
         TM    NCSPL2IN(@03),B'10000000'                           1108 01130000
         BNO   @RF01108                                            1108 01131000
*         DO;                       /*                       @DC36143*/ 01132000
*           CVPSUBRN=NCSPLRPT;      /*                       @DC36143*/ 01133000
         ST    @04,CVPSUBRN                                        1110 01134000
*           CALL CVPDEAC;           /*                       @DC36143*/ 01135000
         BAL   @14,CVPDEAC                                         1111 01136000
*         END;                                                     1112 01137000
*       ELSE                                                       1113 01138000
*         DO;                       /*                       @DC36143*/ 01139000
         B     @RC01108                                            1113 01140000
@RF01108 DS    0H                                                  1114 01141000
*           IF NCSPLVIT=CVPON       /* IF ENTERED FROM VAY IMM     1114 01142000
*                                      TERMINATE             @DC36143*/ 01143000
*             THEN                                                 1114 01144000
         L     @04,CVPNCSPL                                        1114 01145000
         TM    NCSPLVIT(@04),B'00000001'                           1114 01146000
         BO    @RT01114                                            1114 01147000
*             ;                     /* DON'T PROCESS CVPLIN  @DC36143*/ 01148000
*           ELSE                                                   1116 01149000
*             IF RPRVYIMM=CVPON     /* IF VARY DEACT IMM     @DC36143*/ 01150000
