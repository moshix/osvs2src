         B     @PB00003                                            0864 00353000
CVPWTO   DS    0H                                                  0865 00354000
*/********************************************************************/ 00355000
*/* THIS ROUTINE IS CALLED TO ISSUE THE MESSAGE WHOSE IDENTIFICATION */ 00356000
*/* IS SET IN CVPMSG# WITHIN THE WORK AREA. TPMSG IS INVOKED TO ISSUE*/ 00357000
*/* THE MESSAGE, WITH AN INDICATION THAT THE TPMSG PARAMETER LIST    */ 00358000
*/* IS TO BE BUILT IN THE NEXT AVAILABLE SPACE IN THE WORK AREA      */ 00359000
*/* (PLAREA PARAMETER).                                              */ 00360000
*/********************************************************************/ 00361000
*                                                                  0865 00362000
*   CVPWTOSV=CVPRTNAD;                                             0865 00363000
         ST    CVPRTNAD,CVPWTOSV                                   0865 00364000
*   CVPR2=ADDR(CVPMSG#);                                           0866 00365000
         LA    CVPR2,CVPMSG#                                       0866 00366000
*   CVPOPT=ADDR(NCSPLSID);                                         0867 00367000
         L     @04,CVPNCSPL                                        0867 00368000
         LA    @04,NCSPLSID(,@04)                                  0867 00369000
         ST    @04,CVPOPT                                          0867 00370000
*   CVPWORKR=ADDR(CVPMSGPL);                                       0868 00371000
*                                                                  0868 00372000
         LA    @03,CVPMSGPL                                        0868 00373000
         ST    @03,CVPWORKR                                        0868 00374000
*/*TPMSG MSGID((CVPR2))MSG((CVPRID))OPTIONS((CVPOPT))PLAREA((CVPWORKR)) 00375000
*                                                                    */ 00376000
*                                                                  0869 00377000
*   DO;                                                            0869 00378000
*     DCL                                                          0870 00379000
*       REG1025 REG(1) PTR;                                        0870 00380000
*     RFY                                                          0871 00381000
*      (REG1025) RSTD;                                             0871 00382000
*     RFY                                                          0872 00383000
*      (EMCR13) RSTD;                                              0872 00384000
*     DCL                                                          0873 00385000
*       1 * BASED(REG1025),                                        0873 00386000
*        2 CMZID025 PTR(31),                                       0873 00387000
*        2 *,                                                      0873 00388000
*         3 CMZRL025 PTR(8),                                       0873 00389000
*         3 CMZRA025 PTR(24) BDY(BYTE),                            0873 00390000
*        2 CMZEA025 PTR(31),                                       0873 00391000
*        2 CMZVP025 PTR(31);                                       0873 00392000
