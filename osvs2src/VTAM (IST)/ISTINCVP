         CLI   RPRENTRY(@04),X'75'                                 0988 00784000
         BE    @RT00988                                            0988 00785000
         CLI   RPRENTRY(@04),X'90'                                 0988 00786000
         BNE   @RF00988                                            0988 00787000
@RT00988 DS    0H                                                  0989 00788000
*             DO;                                                  0989 00789000
*               IF RPRVRYPD=CVPON THEN/* IF VARY PENDING SET         */ 00790000
         L     @04,CVPWKPT1                                        0990 00791000
         TM    RPRVRYPD(@04),B'00000010'                           0990 00792000
         BNO   @RF00990                                            0990 00793000
*                 DO;                                              0991 00794000
*                   RPRONLIN=CVPOFF;/* TURN OFF ONLINE & LIVE        */ 00795000
         NI    RPRONLIN(@04),B'11110111'                           0992 00796000
*                   RPRLIVE=CVPOFF;                                0993 00797000
         NI    RPRLIVE(@04),B'11111110'                            0993 00798000
*                   NCSACTV='1'B;   /*                       @X50BHLV*/ 00799000
         L     @04,CVPNCSPL                                        0994 00800000
         OI    NCSACTV(@04),B'10000000'                            0994 00801000
*                   NCSAIMOD='0'B;  /*                       @X50BHLV*/ 00802000
