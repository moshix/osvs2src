*                     CVPRID=ADDR(RPTCTNM);/* SET UP NAME FOR TPMSG  */ 01678000
         LA    @03,RPTCTNM(,@04)                                   1280 01679000
         ST    @03,CVPRID                                          1280 01680000
*                     RPTVRYPD=CVPOFF;/* TURN OFF VARY PENDING       */ 01681000
         NI    RPTVRYPD(@04),B'10111111'                           1281 01682000
*                     CALL CVPWTO;  /* ISSUE MSG                     */ 01683000
         BAL   @14,CVPWTO                                          1282 01684000
*                   END;            /* END FOR CTERM VARY PENDING    */ 01685000
*                 ELSE                                             1284 01686000
*                   ;               /* ELSE CTERM NOT VARY PENDING   */ 01687000
@RF01277 DS    0H                                                  1285 01688000
*               ELSE                                               1285 01689000
*                 ;                 /* ELSE NOT AN LCDI              */ 01690000
@RF01276 DS    0H                                                  1286 01691000
*               RPRVYIMM=CVPOFF;    /* TURN OFF VARY IMMEDIATE FLAG  */ 01692000
         L     @04,CVPWKPT1                                        1286 01693000
         NI    RPRVYIMM(@04),B'11111101'                           1286 01694000
*               IF RPRHLVPD=CVPON THEN/* IS HIGHER LEVEL VARY      1287 01695000
*                                      PENDING?                      */ 01696000
         TM    RPRHLVPD(@04),B'00000100'                           1287 01697000
         BNO   @RF01287                                            1287 01698000
*                 DO;                                              1288 01699000
*                   CVPPR1LP=CVPOFF;/* STAY IN LOOP                  */ 01700000
         NI    CVPPR1LP,B'11111011'                                1289 01701000
*                   RPRHLVPD=CVPOFF;/* CLEAR HIGHER LEVEL VARY PEND. */ 01702000
         NI    RPRHLVPD(@04),B'11111011'                           1290 01703000
*                   IF CVPWKPT1=NCSPLRPT THEN/* IS THIS COMMAND    1291 01704000
*                                      ENTRY?                        */ 01705000
         L     @03,CVPNCSPL                                        1291 01706000
         C     @04,NCSPLRPT(,@03)                                  1291 01707000
         BNE   @RF01291                                            1291 01708000
*                     CVPWKPT1=NCSPLNSN;/* GET NSNEA FROM NCSPL      */ 01709000
         L     @04,NCSPLNSN(,@03)                                  1292 01710000
         ST    @04,CVPWKPT1                                        1292 01711000
*                   ELSE                                           1293 01712000
*                     CVPWKPT1=RPRNSNEA;/* ELSE SET WKPT1 TO NSNEA OF   01713000
*                                      CURRENT ENTRY                 */ 01714000
         B     @RC01291                                            1293 01715000
@RF01291 L     @04,CVPWKPT1                                        1293 01716000
         L     @04,RPRNSNEA(,@04)                                  1293 01717000
         ST    @04,CVPWKPT1                                        1293 01718000
*                 END;              /* END FOR HIGHER LEVEL VARY   1294 01719000
*                                      PENDING                       */ 01720000
*               ELSE                                               1295 01721000
*                 ;                                                1295 01722000
@RF01287 DS    0H                                                  1296 01723000
*             END;                  /* END FOR SKIP HEADER NOT ON    */ 01724000
*         END;                      /* END FOR PEP COUNT = 0         */ 01725000
*       ELSE                                                       1298 01726000
*         ;                         /* ELSE PEP COUNT ^=0            */ 01727000
@RF01254 DS    0H                                                  1299 01728000
*     END;                          /* END FOR DO WHILE LOOP TO    1299 01729000
*                                      PROCESS HIGHER LEVEL VARY P   */ 01730000
@RC01254 DS    0H                                                  1299 01731000
@DE01245 TM    CVPPR1LP,B'00000100'                                1299 01732000
         BZ    @DL01245                                            1299 01733000
*/* SEGMENT(CVPPR01)FINISH                                           */ 01734000
*                                                                  1300 01735000
*   DO;                                                            1300 01736000
@RC01243 DS    0H                                                  1301 01737000
*     GOTO CVPPR01#;                                               1301 01738000
