         B     @PB00004                                            0900 00454000
CVPDOIO  DS    0H                                                  0901 00455000
*/********************************************************************  00456000
*                                                                    */ 00457000
*/*   CVPDOIO IS ISTINCVP'S INTERFACE SUBROUTINE TO ISTINCIO, THE    */ 00458000
*/* SSCP I/O ROUTINE. ISTINCIO IS CALLED WITH THE NCSPL IN REG 1, THE*/ 00459000
*/* NCB PTR IN REG 2, SAVE AREA IN REG 13 & THE DAF LOCK HELD.       */ 00460000
*/* ISTINCIO WILL RELEASE THE DAF LOCK.                              */ 00461000
*/********************************************************************  00462000
*                                                                    */ 00463000
*                                                                  0901 00464000
*   CVPIOSAV=CVPRTNAD;              /* SAVE RETURN @                 */ 00465000
         ST    CVPRTNAD,CVPIOSAV                                   0901 00466000
*   CVPINPUT=NCSPLRPH;              /* SET UP RPH TO REG 1           */ 00467000
         L     @04,CVPNCSPL                                        0902 00468000
         L     CVPINPUT,NCSPLRPH(,@04)                             0902 00469000
*   CVPR2=NCSPLCID;                 /* SET UP CID IN REG 2           */ 00470000
