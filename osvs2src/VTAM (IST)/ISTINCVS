*       CVSR0=CVSNCSPL->NCSPLWKA;  /* SAVE FIELDS SET UP BY CW3*/  0848 00741000
         L     CVSR0,NCSPLWKA(,@11)                                0848 00742000
*       CVSR1=CVSNCSPL->NCSPLCVT;  /*SAME                    @D32CKDQ*/ 00743000
         L     CVSR1,NCSPLCVT(,@11)                                0849 00744000
*       IF CVSNCSPL->NCSPLREQ=CVSON                                0850 00745000
