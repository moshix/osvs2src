*       IF CVSR15=0                 /* WAS BUILD SUCCESSFUL?         */ 01112000
*         THEN                      /* YES - CONTINUE                */ 01113000
         SLR   @11,@11                                             0970 01114000
         CR    CVSR15,@11                                          0970 01115000
         BNE   @RF00970                                            0970 01116000
*         DO;                                                      0971 01117000
*           RDTEPTR=CVSR1;  /* PUT ADDR OF NEW SEGMENT HDR IN      0972 01118000
