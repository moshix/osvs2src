*         3  RPHSMQ BIT(1),            /* INDICATOR IF RPH IS TO BE   * 00334000
*                                      /* QUEUED                      * 00335000
*         3  RPHSMTYP BIT(1),          /* REQUEST TYPE IS BUFFER OR   * 00336000
*                                      /* DOUBLE-WORD                 * 00337000
*         3  RPHSMCLR BIT(1),          /* INDICATOR IF BUFFER TO BE   * 00338000
*                                      /* CLEARED                     * 00339000
*           3  RPHMLTCP BIT(1),        /* USED BY LOCAL 3270  SUPPORT * 00340000
*                                      /* WHEN BUILDING MULTIPLE CHAN * 00341000
*                                      /* PROGRAM                     * 00342000
*           3  RPHFSTLC BIT(1),        /* USED BY LOCAL 3270 SUPPORT  * 00343000
*                                      /* TO INDICATE FIRST LCCW      * 00344000
*              4  RPHFNFLG BIT(1),     /* LCCW SET FUNCTION FLAGS     * 00345000
*        2  RPHFLGB BIT(8),            /* SECOND FLAG BYTE            * 00346000
*           3  RPHLOCK BIT(1),         /* LOCKING OPTION              * 00347000
*           3  RPHBSSP BIT(1),         /* SYSTEM SERVICES PROGRESS    * 00348000
*           3  RPHFBAPS BIT(1),        /* STG. OBTAINED BY APS        * 00349000
*           3  RPHPGCMP BIT(1),        /* BIT ON INDICATES RESOURCE   * 00350000
*                                      /* MGR HAS COMPLETED UNLOCK    * 00351000
*           3  RPHNRSAV BIT(1),        /* DO NOT SAVE ON ACTIVATE     * 00352000
*           3  RPHRLCRA BIT(1),        /* RPH CAN BE RELEASED         * 00353000
