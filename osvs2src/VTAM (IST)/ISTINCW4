*   DCL IOSGDPWE BIT(8) CONSTANT('6D'X); /* IECVPST GOTO ABE & ERP 0085 02476000
*                                                            @DCRR054*/ 02477000
*   DCL IOSERRC BIT(8) CONSTANT('41'X); /*                         0086 02478000
*                                     PERMANENT I/O ERROR     */   0086 02479000
*   DCL IOSEXTC BIT(8) CONSTANT('42'X); /*                         0087 02480000
*                                     EXTENT ERROR            */   0087 02481000
*   DCL IOSDPXC BIT(8) CONSTANT('43'X); /*                         0088 02482000
*                                     DUPLEXED I/O REQUEST WAS     0088 02483000
*                                     NOT STARTED BECAUSE OF       0088 02484000
*                                     A QUIESCED OR NOT READY      0088 02485000
*                                     DEVICE.                 */   0088 02486000
*   DCL IOSINTC BIT(8) CONSTANT('44'X); /*                         0089 02487000
*                                     REQUEST INTERCEPTED BE-      0089 02488000
*                                     CAUSE A PERMANENT ERROR      0089 02489000
*                                     OCCURRED THE LAST TIME       0089 02490000
*                                     THE DEVICE WAS USED.    */   0089 02491000
*   DCL IOSABNC BIT(8) CONSTANT('45'X); /*                         0090 02492000
*                                     I/O REQUEST ABNORMALLY       0090 02493000
*                                     TERMINATED BECAUSE OF PRO-   0090 02494000
