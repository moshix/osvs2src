*DCL ZCTYPE BIT(8) CONSTANT('00'X);   /* ZCR TYPE                   */  00884000
*DCL ZCLNG FIXED(15) CONSTANT(172);   /* ZCRR LENGTH                */  00885000
*DCL ZCRSAVNO BIT(8) CONSTANT('06'X); /* NUMBER OF SAVEAREAS IN ZCRR */ 00886000
*DCL ZCRLSVNO BIT(8) CONSTANT('04'X); /* NUMBER OF SAVE AREAS IN   0492 00887000
*                                         LOCAL SUPPORT              */ 00888000
*/* *** EQUATES FOR ZCRRCDSV *** */                                0493 00889000
*                                                                  0493 00890000
*DCL ZCRMRFSB BIT(8) CONSTANT('0C'X);  /* REQUEST COMPLETION MOVE FSB   00891000
*                                         TO PROCESS                 */ 00892000
*DCL ZCRSRCHQ BIT(8) CONSTANT('10'X);  /* REQUEST COMPLETION SEARCH     00893000
*                                         REQUEST QUEUE              */ 00894000
*DCL ZCREXIT  BIT(8) CONSTANT('14'X);  /* REQUEST COMPLETION EXIT    */ 00895000
*DCL ZCRSFSB  BIT(8) CONSTANT('18'X);  /* REQUEST COMPLETION SINGLE     00896000
*                                         FSB                        */ 00897000
*DCL ZCRDLFSB BIT(8) CONSTANT('1C'X);  /* REQUEST COMPLETION DUAL FSB * 00898000
*DCL ZCRNOSCH BIT (8) CONSTANT('20'X); /* REQUEST COMPLETION, EXIT WITH 00899000
*                                         SCHED NONE                 */ 00900000
*DCL ZCRNRSCH BIT (8) CONSTANT('24'X); /* REQUEST COMPLETION, EXIT WITH 00901000
*                                         SCHED NORM                 */ 00902000
