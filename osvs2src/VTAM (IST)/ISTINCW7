SVTM     TITLE 'SSCP -- ISTINCW7 -- VARY PAB OBTAIN                    *00001000
                        '                                               00002000
ISTINCW7 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCW7  75.129'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @11,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@11                                         0001 00014000
* DO;                                                              0344 00015000
*/***  CRRMAINT  PROCNM(ISTINCW7)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00016000
**/                                                                0345 00017000
*                                                                  0345 00018000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0345 00019000
* RFY ISTLCCW ;                                                    0345 00020000
* RFY ISTLCPB ;                                                    0346 00021000
* RFY ISTZSAVE;                                                    0347 00022000
* RFY ISTPFCRR;                                                    0348 00023000
* RFY ISTCFCRR;                                                    0349 00024000
* RFY ISTZCRR ;                                                    0350 00025000
* RFY ISTASCRR;                                                    0351 00026000
* RFY ISTAPCRR;                                                    0352 00027000
* RFY ISTCRA  ;                                                    0353 00028000
* RFY ISTCNSTA;                                                    0354 00029000
* RFY ISTVYCRR;                                                    0355 00030000
*/* END OF ATBUILD */                                              0356 00031000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0356 00032000
*NCSPLRPH->RPHCRR->CRACRR->NINCW7 = '1'B;                          0356 00033000
         L     @12,NCSPLRPH(,CW7R1)                                0356 00034000
         L     @12,RPHCRR(,@12)                                    0356 00035000
         L     @12,CRACRR(,@12)                                    0356 00036000
         OI    NINCW7(@12),B'00001000'                             0356 00037000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0357 00038000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0357 00039000
         XC    VYCCRT+1(11,@12),VYCCRT+1(@12)                      0357 00040000
         MVI   VYCCRT(@12),B'00000000'                             0357 00041000
*NCSPLRPH->RPHCRR->CRACRR->CINCW7 = '1'B;                          0358 00042000
         OI    CINCW7(@12),B'00001000'                             0358 00043000
* END;                                                             0359 00044000
*   RFY  CW7R7 RSTD;                                               0360 00045000
*   CW7R6 = CW7R1;                                                 0361 00046000
         LR    CW7R6,CW7R1                                         0361 00047000
*   RFY  ISTNCSPL BASED(CW7R6);                                    0362 00048000
*   CW7R1 = NCSPLRPH;                                              0363 00049000
         L     CW7R1,NCSPLRPH(,CW7R6)                              0363 00050000
*   RFY  ISTRPH   BASED(CW7R1);                                    0364 00051000
*   RFY  ISTATCVT BASED(NCSPLCVT);                                 0365 00052000
*   RFY  ISTRDT   BASED(NCSPLRAD);                                 0366 00053000
*   RFY  ISTRPRE  BASED(NCSPLRAD);                                 0367 00054000
*   RFY  ISTRCC   BASED(NCSPLRAD);                                 0368 00055000
*/********************************************************************/ 00056000
*/*                                                                  */ 00057000
*/* DETERMINE IF THE RDTE IS A LOCAL HEADER OR AN RN.  IF IT IS,     */ 00058000
*/* OBTAIN SPACE FOR 4 PABS; OTHERWISE, OBTAIN SPACE FOR 3 PABS.     */ 00059000
*/* THE 'GETSTOR' MACRO IS USED FOR OBTAINING THE SPACE.             */ 00060000
*/*                                                                  */ 00061000
*/********************************************************************/ 00062000
*   IF RPRENTRY = RPRENTLH |                                       0369 00063000
*      RPRENTRY = RPRENTRN                                         0369 00064000
*     THEN                                                         0369 00065000
         L     @12,NCSPLRAD(,CW7R6)                                0369 00066000
         CLI   RPRENTRY(@12),X'03'                                 0369 00067000
         BE    @RT00369                                            0369 00068000
         CLI   RPRENTRY(@12),X'01'                                 0369 00069000
         BNE   @RF00369                                            0369 00070000
@RT00369 DS    0H                                                  0370 00071000
*       CW7R7 = 4 * LENGTH(ISTDYPAB);                              0370 00072000
         LA    CW7R7,128                                           0370 00073000
*   ELSE                                                           0371 00074000
*     CW7R7 = 3 * LENGTH(ISTDYPAB);                                0371 00075000
         B     @RC00369                                            0371 00076000
@RF00369 LA    CW7R7,96                                            0371 00077000
*    /*GETSTOR SIZE(CW7R7)CLEAR(NO)FETCHP(YES)SAVE(YES)TYPE(PAGEABLE)*/ 00078000
*     DO;                                                          0372 00079000
@RC00369 DS    0H                                                  0373 00080000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0373 00081000
*/* END OF ATBUILD */                                              0373 00082000
*    DCL GPR00F FIXED(31) REG(00);                                 0373 00083000
*    DCL GPR00P PTR(31)   REG(00);                                 0374 00084000
*    DCL GPR01F FIXED(31) REG(01);                                 0375 00085000
*    DCL GPR01P PTR(31)   REG(01);                                 0376 00086000
*    DCL GPR02F FIXED(31) REG(02);                                 0377 00087000
*    DCL GPR02P PTR(31)   REG(02);                                 0378 00088000
*    DCL GPR03F FIXED(31) REG(03);                                 0379 00089000
*    DCL GPR03P PTR(31)   REG(03);                                 0380 00090000
*    DCL GPR04F FIXED(31) REG(04);                                 0381 00091000
*    DCL GPR04P PTR(31)   REG(04);                                 0382 00092000
*    DCL GPR05F FIXED(31) REG(05);                                 0383 00093000
*    DCL GPR05P PTR(31)   REG(05);                                 0384 00094000
*    DCL GPR06F FIXED(31) REG(06);                                 0385 00095000
*    DCL GPR06P PTR(31)   REG(06);                                 0386 00096000
*    DCL GPR07F FIXED(31) REG(07);                                 0387 00097000
*    DCL GPR07P PTR(31)   REG(07);                                 0388 00098000
*    DCL GPR08F FIXED(31) REG(08);                                 0389 00099000
*    DCL GPR08P PTR(31)   REG(08);                                 0390 00100000
*    DCL GPR09F FIXED(31) REG(09);                                 0391 00101000
*    DCL GPR09P PTR(31)   REG(09);                                 0392 00102000
*    DCL GPR10F FIXED(31) REG(10);                                 0393 00103000
*    DCL GPR10P PTR(31)   REG(10);                                 0394 00104000
*    DCL GPR11F FIXED(31) REG(11);                                 0395 00105000
*    DCL GPR11P PTR(31)   REG(11);                                 0396 00106000
*    DCL GPR12F FIXED(31) REG(12);                                 0397 00107000
*    DCL GPR12P PTR(31)   REG(12);                                 0398 00108000
*    DCL GPR13F FIXED(31) REG(13);                                 0399 00109000
*    DCL GPR13P PTR(31)   REG(13);                                 0400 00110000
*    DCL GPR14F FIXED(31) REG(14);                                 0401 00111000
*    DCL GPR14P PTR(31)   REG(14);                                 0402 00112000
*    DCL GPR15F FIXED(31) REG(15);                                 0403 00113000
*    DCL GPR15P PTR(31)   REG(15);                                 0404 00114000
*    RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                       0405 00115000
*    RFY ISTRPH BASED(GPR01P);                                     0406 00116000
*    GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,                         0407 00117000
*             GPR06P,GPR07P,GPR08P,GPR09P,                         0407 00118000
*             GPR10P,GPR11P,GPR12P,GPR13P)                         0407 00119000
*        SETS(RPHWORK);                                            0407 00120000
*                                                                  0407 00121000
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS      00122000
*    GPR00F = CW7R7;                                   /*SIZE IN REG0*/ 00123000
         LR    GPR00F,CW7R7                                        0408 00124000
*    DCL 1 CMZA0009 FIXED(31) STATIC LOCAL,                        0409 00125000
*          2 * FIXED(8) INIT(64),                    /* OPTION FLAGS */ 00126000
*          2 * FIXED(8) INIT(96),                     /* STORAGE KEY */ 00127000
*          2 * FIXED(8) INIT(231),                 /* SUBPOOL NUMBER */ 00128000
*          2 * FIXED(8) INIT('00'X);                 /* REQUEST TYPE */ 00129000
*    GPR03F = CMZA0009;                                            0410 00130000
*                                                                  0410 00131000
         L     GPR03F,CMZA0009                                     0410 00132000
*    DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */                 0411 00133000
*      GPR02P = ATCLCPTR; /*SET UP ATCVT POINTER*/                 0412 00134000
         L     GPR02P,ATCLCPTR                                     0412 00135000
*    END; /*LISATCVT*/                                             0413 00136000
*    CALL GPR02P -> ATCGTSTR;                         /*CALL ISTORFSR*/ 00137000
         L     @15,ATCGTSTR(,GPR02P)                               0414 00138000
         BALR  @14,@15                                             0414 00139000
*    RFY (GPR00P,GPR01P,GPR02P,GPR03P) UNRSTD;                     0415 00140000
*    END ;                                           /*END OF GETSTOR*/ 00141000
*                              /*@XM10117*/                        0417 00142000
*   RFY  CW7PABS BASED(RPHSRPRM);                                  0417 00143000
*/********************************************************************/ 00144000
*/*                                                                  */ 00145000
*/* IF THE 'GETSTOR' WAS NOT SUCCESSFUL, SET REG 15 TO 4 AND RETURN  */ 00146000
*/* TO THE CALLING MODULE.  IF IT WAS SUCCESSFUL, INITIALIZE 3 PABS  */ 00147000
*/* AND SAVE THEIR ADDRESSES IN THE APPROPRIATE RDTE FIELDS (BASED   */ 00148000
*/* ON WHETHER THE RDTE IS A PU OR NOT).  IF 4 PABS WERE OBTAINED,   */ 00149000
*/* INITIALIZE THE 4TH ONE AND STORE ITS ADDRESS IN THE RDTE FIELD.  */ 00150000
*/* RETURN TO THE CALLING MODULE.                                    */ 00151000
*/*     THE PABS OBTAINED ARE THE:                                   */ 00152000
*/*         1. VARY PAB                                              */ 00153000
*/*         2. ERP  PAB                                              */ 00154000
*/*         3. LOAD/DUMP PAB                                         */ 00155000
*/*         4. TOLTEP PAB                                            */ 00156000
*/*                                                                  */ 00157000
*/********************************************************************/ 00158000
*   IF CW7R15 ^= CW7C0                                             0418 00159000
*     THEN                                                         0418 00160000
         LTR   CW7R15,CW7R15                                       0418 00161000
         BZ    @RF00418                                            0418 00162000
*       CW7R15 = CW7C4;                                            0419 00163000
         LA    CW7R15,4                                            0419 00164000
*   ELSE                                                           0420 00165000
*     DO;                                                          0420 00166000
         B     @RC00418                                            0420 00167000
@RF00418 DS    0H                                                  0421 00168000
*       CW7PABS(1:3*LENGTH(ISTDYPAB)) =                            0421 00169000
*                          ATCVPAB1(1:3*LENGTH(ISTDYPAB));         0421 00170000
         L     @12,RPHSRPRM(,GPR01P)                               0421 00171000
         L     @10,NCSPLCVT(,CW7R6)                                0421 00172000
         MVC   CW7PABS(96,@12),ATCVPAB1(@10)                       0421 00173000
*       IF RPRENTRY = RPRENTCC                                     0422 00174000
*         THEN                                                     0422 00175000
         L     @10,NCSPLRAD(,CW7R6)                                0422 00176000
         CLI   RPRENTRY(@10),X'71'                                 0422 00177000
         BNE   @RF00422                                            0422 00178000
*           DO;                                                    0423 00179000
*             RCCVPAB = RPHSRPRM;                                  0424 00180000
         ST    @12,RCCVPAB(,@10)                                   0424 00181000
*             RCCEPAB = RCCVPAB + LENGTH(ISTDYPAB);                0425 00182000
         LA    @09,32                                              0425 00183000
         ALR   @12,@09                                             0425 00184000
         ST    @12,RCCEPAB(,@10)                                   0425 00185000
*             RCCDPAB = RCCEPAB + LENGTH(ISTDYPAB);                0426 00186000
         ALR   @12,@09                                             0426 00187000
         ST    @12,RCCDPAB(,@10)                                   0426 00188000
*           END;                                                   0427 00189000
*       ELSE                                                       0428 00190000
*         DO;                                                      0428 00191000
         B     @RC00422                                            0428 00192000
@RF00422 DS    0H                                                  0429 00193000
*           RDTVPAB = RPHSRPRM;                                    0429 00194000
         L     @12,NCSPLRAD(,CW7R6)                                0429 00195000
         L     @10,RPHSRPRM(,GPR01P)                               0429 00196000
         ST    @10,RDTVPAB(,@12)                                   0429 00197000
*           RDTEPAB = RDTVPAB + LENGTH(ISTDYPAB);                  0430 00198000
         LA    @09,32                                              0430 00199000
         ALR   @10,@09                                             0430 00200000
         ST    @10,RDTEPAB(,@12)                                   0430 00201000
*           RDTDPAB = RDTEPAB + LENGTH(ISTDYPAB);                  0431 00202000
         ALR   @10,@09                                             0431 00203000
         ST    @10,RDTDPAB(,@12)                                   0431 00204000
*         END;                                                     0432 00205000
*       IF RPRENTRY = RPRENTLH |                                   0433 00206000
*          RPRENTRY = RPRENTRN                                     0433 00207000
*         THEN                                                     0433 00208000
@RC00422 L     @12,NCSPLRAD(,CW7R6)                                0433 00209000
         CLI   RPRENTRY(@12),X'03'                                 0433 00210000
         BE    @RT00433                                            0433 00211000
         CLI   RPRENTRY(@12),X'01'                                 0433 00212000
         BNE   @RF00433                                            0433 00213000
@RT00433 DS    0H                                                  0434 00214000
*           DO;                                                    0434 00215000
*             RDTTPAB = RPHSRPRM + (3*LENGTH(ISTDYPAB));           0435 00216000
         L     @12,NCSPLRAD(,CW7R6)                                0435 00217000
         LA    @10,96                                              0435 00218000
         AL    @10,RPHSRPRM(,GPR01P)                               0435 00219000
         ST    @10,RDTTPAB(,@12)                                   0435 00220000
*             RFY CW7PABS BASED(RDTTPAB);                          0436 00221000
*             CW7PABS(1:LENGTH(ISTDYPAB)) =                        0437 00222000
*                              ATCVPAB1(1:LENGTH(ISTDYPAB));       0437 00223000
         L     @12,NCSPLCVT(,CW7R6)                                0437 00224000
         MVC   CW7PABS(32,@10),ATCVPAB1(@12)                       0437 00225000
*           END;                                                   0438 00226000
*       ELSE;                                                      0439 00227000
@RF00433 DS    0H                                                  0440 00228000
*       CW7R15 = CW7C0;                                            0440 00229000
         SLR   CW7R15,CW7R15                                       0440 00230000
*     END;                                                         0441 00231000
*   RFY  CW7R7 UNRSTD;                                             0442 00232000
*   RETURN;                                                        0443 00233000
*                                                                  0443 00234000
@EL00001 DS    0H                                                  0443 00235000
@EF00001 DS    0H                                                  0443 00236000
@ER00001 L     @14,12(,@13)                                        0443 00237000
         LM    @00,@12,20(@13)                                     0443 00238000
         BR    @14                                                 0443 00239000
*/* TPEPILOG   */                                                  0444 00240000
* GEN (PRINT OFF);                                                 0444 00241000
*                                                                  0444 00242000
         PRINT OFF                                                      00243000
*/* LOGICAL CHANNEL COMMAND WORD                                      * 00244000
*/* ADD A DECLARE                                      003 10/18/74   * 00245000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0445 00246000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 00247000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 00248000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 00249000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 00250000
*              4  * BIT(1),            /* RESERVED                    * 00251000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0445 00252000
*                                         ERASE OR READ CONT          * 00253000
*              4  LCCWCTL BIT(4),      /* CONTROL TYPE                * 00254000
*                 5  LCCWMODE BIT(2),  /* MODE TYPE                   * 00255000
*                 5  LCCWTYPE BIT(2),  /* OPERATION TYPE              * 00256000
*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00257000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00258000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00259000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00260000
*                                         RECORD                      * 00261000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00262000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00263000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00264000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00265000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00266000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00267000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00268000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00269000
*/* LOGICAL COMMAND CODES */                                       0446 00270000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00271000
*     LXEC     BIT(8) CONSTANT('4B'X), /* RESET AT END OF COMMAND     * 00272000
*     LXO      BIT(8) CONSTANT('5B'X), /* RESET ORDERLY               * 00273000
*     LCCWPRGE BIT(8) CONSTANT('6B'X), /* PURGE                       * 00274000
*     LWH      BIT(8) CONSTANT('0F'X), /* WRITE HEADER INDI. 1ST BLOCK* 00275000
*     LWRVI    BIT(8) CONSTANT('1F'X), /* WRITE RVI                   * 00276000
*     LWNA     BIT(8) CONSTANT('2F'X), /* WRITE NEGATIVE ACK          * 00277000
*     LWAA     BIT(8) CONSTANT('3F'X), /* WRITE ALTERNATE ACK         * 00278000
*     LWPLG    BIT(8) CONSTANT('5F'X), /* WRITE ACK LEADING GRAPHICS  * 00279000
*     LWNLG    BIT(8) CONSTANT('6F'X), /* WRITE NACK LEADING GRAPHICS * 00280000
*     LSMDE    BIT(8) CONSTANT('4F'X), /* SET MODE                    * 00281000
*     LIFBM    BIT(8) CONSTANT('7F'X), /* INDIC 1ST BLOCK OF MESSAGE  * 00282000
*     LTEST    BIT(8) CONSTANT('FE'X), /* TEST LCCW                   * 00283000
*     LPRMPT   BIT(8) CONSTANT('13'X), /* PREEMPT LCCW                * 00284000
*     LRSTR    BIT(8) CONSTANT('43'X), /* RESTORE LCCW                * 00285000
*     LEXEC    BIT(8) CONSTANT('FF'X), /* SPECIAL CONTROL OPERATION   * 00286000
*     LREAD    BIT(2) CONSTANT('10'B),  /* READ TYPE LCCW             * 00287000
*     LWRITE   BIT(2) CONSTANT('01'B),  /* WRITE TYPE LCCW            * 00288000
*     LCNTRL   BIT(2) CONSTANT('11'B),  /* CONTROL TYPE LCCW          * 00289000
*     LRESET   BIT(4) CONSTANT('1011'B),/* RESET COMMANDS             * 00290000
*     LTIC     BIT(8) CONSTANT('08'X),  /* LOGICAL TIC                * 00291000
*     LWR      BIT(6) CONSTANT('000001'B),  /* WRITE CONVERSATIONAL   * 00292000
*     LWB      BIT(6) CONSTANT('000101'B),  /* WRITE BLOCK            * 00293000
*     LWM      BIT(6) CONSTANT('001001'B),  /* WRITE MESSAGE          * 00294000
*     LWT      BIT(6) CONSTANT('001101'B),  /* WRITE TRANSMISSION     * 00295000
*     LEWM     BIT(6) CONSTANT('011001'B),  /* ERASE/WRITE MESSAGE    * 00296000
*     LEWT     BIT(6) CONSTANT('011101'B),  /* ERASE/WRITE TRNSMSSN   * 00297000
*     LEAU     BIT(6) CONSTANT('010001'B),  /* ERASE ALL UNPROTECTED  * 00298000
*     LRB      BIT(6) CONSTANT('000110'B),  /* READ BLOCK             * 00299000
*     LRM      BIT(6) CONSTANT('001010'B),  /* READ MESSAGE           * 00300000
*     LRT      BIT(6) CONSTANT('001110'B),  /* READ TRANSMISSION      * 00301000
*     LRMOD    BIT(6) CONSTANT('010010'B),  /* READ MODIFIED          * 00302000
*     LRBUF    BIT(6) CONSTANT('010110'B),  /* READ BUFFER            * 00303000
*     LRCONT   BIT(6) CONSTANT('011010'B),  /* READ CONTINUOUS        * 00304000
*     LRPEP    BIT(6) CONSTANT('011110'B),  /* READ PERPETUAL         * 00305000
*     LCPYM    BIT(6) CONSTANT('101001'B),  /* COPY MESSAGE           * 00306000
*     LCPYT    BIT(6) CONSTANT('101101'B),  /* COPY TRANSMISSION      * 00307000
*     LD       BIT(8) CONSTANT('23'X),  /* DISCONNECT                 * 00308000
*     LDEC     BIT(8) CONSTANT('33'X),  /* DISCONNECT WITH END OF CALL* 00309000
*     LEOC     BIT(8) CONSTANT('53'X),  /* END OF CALL COMMAND        * 00310000
*     LXEL     BIT(8) CONSTANT('0B'X),  /* RESET ERROR LOCK           * 00311000
*     LXC      BIT(8) CONSTANT('1B'X),  /* RESET CONDITIONAL          * 00312000
*     LXI      BIT(8) CONSTANT('2B'X),  /* RESET IMMEDIATE            * 00313000
*     LC       BIT(8) CONSTANT('03'X),  /* CONTACT                    * 00314000
*     LWPA     BIT(8) CONSTANT('FD'X); /* WRITE POSITIVE ACK          * 00315000
*DCL  LMT      BIT(2) CONSTANT('11'B), /* TRANSMISSION MODE           * 00316000
*     LMS      BIT(2) CONSTANT('00'B), /* SPECIAL MODE LIKE CONV, EAU * 00317000
*     LMB      BIT(2) CONSTANT('01'B), /* BLOCK MODE                  * 00318000
*     LMM      BIT(2) CONSTANT('10'B); /* MSG OR CONTINUOUS MODE      * 00319000
