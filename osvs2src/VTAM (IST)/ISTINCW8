*DCL ZCTYPE BIT(8) CONSTANT('00'X);   /* ZCR TYPE                   */  00890000
*DCL ZCLNG FIXED(15) CONSTANT(172);   /* ZCRR LENGTH                */  00891000
*DCL ZCRSAVNO BIT(8) CONSTANT('06'X); /* NUMBER OF SAVEAREAS IN ZCRR */ 00892000
*DCL ZCRLSVNO BIT(8) CONSTANT('04'X); /* NUMBER OF SAVE AREAS IN   0621 00893000
*                                         LOCAL SUPPORT              */ 00894000
*/* *** EQUATES FOR ZCRRCDSV *** */                                0622 00895000
*                                                                  0622 00896000
*DCL ZCRMRFSB BIT(8) CONSTANT('0C'X);  /* REQUEST COMPLETION MOVE FSB   00897000
*                                         TO PROCESS                 */ 00898000
