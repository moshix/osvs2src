SVTM     TITLE 'SSCP -- ISTINCW8 -- VARY ACT PU/LU INTERFACE           *00001000
                        '                                               00002000
ISTINCW8 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCW8  75.129'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@11                                         0001 00013000
      USING  @DATD,CW8WKPTR                                             00014000
* DO;                                                              0483 00015000
*/***  CRRMAINT  PROCNM(ISTINCW8)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00016000
**/                                                                0484 00017000
*                                                                  0484 00018000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0484 00019000
* RFY ISTLCCW ;                                                    0484 00020000
* RFY ISTLCPB ;                                                    0485 00021000
* RFY ISTZSAVE;                                                    0486 00022000
* RFY ISTPFCRR;                                                    0487 00023000
* RFY ISTCFCRR;                                                    0488 00024000
* RFY ISTZCRR ;                                                    0489 00025000
* RFY ISTASCRR;                                                    0490 00026000
* RFY ISTAPCRR;                                                    0491 00027000
* RFY ISTCRA  ;                                                    0492 00028000
* RFY ISTCNSTA;                                                    0493 00029000
* RFY ISTVYCRR;                                                    0494 00030000
*/* END OF ATBUILD */                                              0495 00031000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0495 00032000
*NCSPLRPH->RPHCRR->CRACRR->NINCW8 = '1'B;                          0495 00033000
         L     @12,NCSPLRPH(,CW8R1)                                0495 00034000
         L     @12,RPHCRR(,@12)                                    0495 00035000
         L     @12,CRACRR(,@12)                                    0495 00036000
         OI    NINCW8(@12),B'00000100'                             0495 00037000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0496 00038000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0496 00039000
         XC    VYCCRT+1(11,@12),VYCCRT+1(@12)                      0496 00040000
         MVI   VYCCRT(@12),B'00000000'                             0496 00041000
*NCSPLRPH->RPHCRR->CRACRR->CINCW8 = '1'B;                          0497 00042000
         OI    CINCW8(@12),B'00000100'                             0497 00043000
* END;                                                             0498 00044000
*   RFY  CW8R7 RSTD;                                               0499 00045000
*   CW8R7 = CW8R13;                                                0500 00046000
         LR    CW8R7,CW8R13                                        0500 00047000
*   CW8R13 = CW8R13 - CW8C72;           /* ESTABLISH SAVEAREA        */ 00048000
         SL    CW8R13,@CF00772                                     0501 00049000
*   CW8SVBKP = CW8R7;                   /* CHAIN BACKWARDS TO CALLER */ 00050000
         ST    CW8R7,CW8SVBKP(,CW8R13)                             0502 00051000
*   CW8R7->CW8SVFWP = CW8R13;           /* CHAIN CALLER FORWARD      */ 00052000
         ST    CW8R13,CW8SVFWP(,CW8R7)                             0503 00053000
*   CW8WKPTR = CW8WKA;                  /* FIND BEGINNING OF WORKAREA*/ 00054000
         L     @12,NCSPLWKA(,CW8R1)                                0504 00055000
         L     @10,CW8WKA(,@12)                                    0504 00056000
         LR    CW8WKPTR,@10                                        0504 00057000
*   CW8WKA = CW8WKA + @SIZDATD;         /* UPDATE NEXT AVAILABLE ADDR*/ 00058000
         AL    @10,@SIZDATD                                        0505 00059000
         ST    @10,CW8WKA(,@12)                                    0505 00060000
*   NCSPLPTR = CW8R1;                                              0506 00061000
         LR    NCSPLPTR,CW8R1                                      0506 00062000
*   RFY  ISTNCSPL BASED(NCSPLPTR);                                 0507 00063000
*   RFY  CW8R7 UNRSTD;                                             0508 00064000
*/********************************************************************/ 00065000
*/*                                                                  */ 00066000
*/* PREPARE SPACE FOR AN OUTBOUND RU AND THEN DETERMINE IF AN ACTPU  */ 00067000
*/* OR AN ACTLU IS NEEDED.  BASED ON THIS DETERMINATION, BUILD THE   */ 00068000
*/* OUTBOUND RU APPROPRIATELY.  UPDATE THE NCSPL AND BUILD A CID TO  */ 00069000
*/* HELP FIND THE NCB ADDRESS.                                       */ 00070000
*/*                                                                  */ 00071000
*/********************************************************************/ 00072000
*   CW8RUPTR = CW8WKA;                  /* ALLOCATE AND ZERO SPACE   */ 00073000
         L     @12,NCSPLWKA(,NCSPLPTR)                             0509 00074000
         L     @10,CW8WKA(,@12)                                    0509 00075000
         LR    CW8RUPTR,@10                                        0509 00076000
*   CW8WKA = CW8WKA + LENGTH(ISTSRU);   /* FROM NCSPL WORKAREA       */ 00077000
         AL    @10,@CF04130                                        0510 00078000
         ST    @10,CW8WKA(,@12)                                    0510 00079000
*   RFY  ISTRUB BASED(CW8RUPTR);                                   0511 00080000
*   RUBDATA(1:LENGTH(ISTSRU)) = CW8ZERO;                           0512 00081000
         XC    RUBDATA+1(9,CW8RUPTR),RUBDATA+1(CW8RUPTR)           0512 00082000
         MVI   RUBDATA(CW8RUPTR),X'00'                             0512 00083000
*   IF RPRENTRY = RPRENTLU              /* RDTE A LU?                */ 00084000
*     THEN                              /*    YES, IT IS.            */ 00085000
         L     @12,NCSPLRAD(,NCSPLPTR)                             0513 00086000
         CLI   RPRENTRY(@12),X'81'                                 0513 00087000
         BNE   @RF00513                                            0513 00088000
*       DO;                                                        0514 00089000
*         NCSPLCMD = NCSCALU;           /* NCSPLCMD = ACTIVATE LU    */ 00090000
         MVC   NCSPLCMD(2,NCSPLPTR),@CB01060                       0515 00091000
*         SRUQSREQ = SRACTLU;           /* BUILD OUTBOUND ACTLU RU   */ 00092000
         MVI   SRUQSREQ(CW8RUPTR),X'0D'                            0516 00093000
*         SRUDAT1T = SRUCOLD;                                      0517 00094000
         MVI   SRUDAT1T(CW8RUPTR),X'01'                            0517 00095000
*         SRUFMPRO = SRUFMP0;                              /*@D32CKYJ*/ 00096000
         NI    SRUFMPRO(CW8RUPTR),B'00001111'                      0518 00097000
*         SRUTSPRO = SRUTSP1;                              /*@D32CKYJ*/ 00098000
         NI    SRUTSPRO(CW8RUPTR),B'11110001'                      0519 00099000
         OI    SRUTSPRO(CW8RUPTR),B'00000001'                      0519 00100000
*         NCSPLRUO = CW8RUPTR;          /* SET ADDRESS AND LENGTH OF */ 00101000
         ST    CW8RUPTR,NCSPLRUO(,NCSPLPTR)                        0520 00102000
*         NCSPLOLN = CW8C3;             /* RU IN NCSPL               */ 00103000
         MVC   NCSPLOLN(2,NCSPLPTR),@CH00279                       0521 00104000
*       END;                                                       0522 00105000
*    ELSE                               /*    NO, IT'S A PU.         */ 00106000
*     DO;                                                          0523 00107000
         B     @RC00513                                            0523 00108000
@RF00513 DS    0H                                                  0524 00109000
*       NCSPLCMD = NCSCAPU;             /* NCSPLCMD = ACTIVATE PU    */ 00110000
         MVC   NCSPLCMD(2,NCSPLPTR),@CB01056                       0524 00111000
*       SRUQSREQ = SRACTPU;             /* BUILD OUTBOUND ACTPU RU   */ 00112000
         MVI   SRUQSREQ(CW8RUPTR),X'11'                            0525 00113000
*       SRUDAT1T = SRUCOLD;                                        0526 00114000
         MVI   SRUDAT1T(CW8RUPTR),X'01'                            0526 00115000
*       IF RPRENTRY = RPRENTRN                                     0527 00116000
*         THEN                                                     0527 00117000
         L     @12,NCSPLRAD(,NCSPLPTR)                             0527 00118000
         CLI   RPRENTRY(@12),X'01'                                 0527 00119000
         BNE   @RF00527                                            0527 00120000
*           DO;                                            /*@D32CKYJ*/ 00121000
*             SRUFMPRO = SRUFMP5;                          /*@D32CKYJ*/ 00122000
         NI    SRUFMPRO(CW8RUPTR),B'01011111'                      0529 00123000
         OI    SRUFMPRO(CW8RUPTR),B'01010000'                      0529 00124000
*             SRUTSPRO = SRUTSP5;                          /*@D32CKYJ*/ 00125000
         NI    SRUTSPRO(CW8RUPTR),B'11110101'                      0530 00126000
         OI    SRUTSPRO(CW8RUPTR),B'00000101'                      0530 00127000
*           END;                                           /*@D32CKYJ*/ 00128000
*       ELSE                                                       0532 00129000
*         DO;                                              /*@D32CKYJ*/ 00130000
         B     @RC00527                                            0532 00131000
@RF00527 DS    0H                                                  0533 00132000
*           SRUFMPRO = SRUFMP0;                            /*@D32CKYJ*/ 00133000
         NI    SRUFMPRO(CW8RUPTR),B'00001111'                      0533 00134000
*           SRUTSPRO = SRUTSP1;                            /*@D32CKYJ*/ 00135000
         NI    SRUTSPRO(CW8RUPTR),B'11110001'                      0534 00136000
         OI    SRUTSPRO(CW8RUPTR),B'00000001'                      0534 00137000
*         END;                                             /*@D32CKYJ*/ 00138000
*       SRUDAT1I = CONSCPID;                                       0536 00139000
@RC00527 L     @12,NCSPLCVT(,NCSPLPTR)                             0536 00140000
         L     @12,ATCCONFT(,@12)                                  0536 00141000
         MVC   SRUDAT1I(6,CW8RUPTR),CONSCPID(@12)                  0536 00142000
*       NCSPLRUO = CW8RUPTR;            /* SET ADDRESS AND LENGTH OF */ 00143000
         ST    CW8RUPTR,NCSPLRUO(,NCSPLPTR)                        0537 00144000
*       NCSPLOLN = CW8C9;               /* RU IN NCSPL               */ 00145000
         MVC   NCSPLOLN(2,NCSPLPTR),@CH01368                       0538 00146000
*       NCSPLRIO = CW8ON;                                  /*@DM13303*/ 00147000
         OI    NCSPLRIO(NCSPLPTR),B'00100000'                      0539 00148000
*     END;                                                         0540 00149000
*   NCSPLRUI = CW8C0;                   /* SET INBOUND RU ADDRESS =0 */ 00150000
@RC00513 SLR   @12,@12                                             0541 00151000
         ST    @12,NCSPLRUI(,NCSPLPTR)                             0541 00152000
*   NCSIOCID = CW8C0 | RPRDAF;          /* BUILD CID                 */ 00153000
         L     @12,NCSPLRAD(,NCSPLPTR)                             0542 00154000
         LH    @12,RPRDAF(,@12)                                    0542 00155000
         N     @12,@CF04628                                        0542 00156000
         ST    @12,NCSIOCID(,NCSPLPTR)                             0542 00157000
*/********************************************************************/ 00158000
*/*                                                                  */ 00159000
*/* FIND THE NCB ADDRESS VIA CIDCTL MACRO                            */ 00160000
*/*                                                                  */ 00161000
*/********************************************************************/ 00162000
*   RFY  (CW8R0,CW8R2) RSTD;                                       0543 00163000
*   CW8R1 = NCSPLRPH;                                              0544 00164000
         L     CW8R1,NCSPLRPH(,NCSPLPTR)                           0544 00165000
*   CW8R2 = NCSIOCID;                                              0545 00166000
*                                                                  0545 00167000
         LR    CW8R2,@12                                           0545 00168000
*    DO; /* CIDCTL(FIND)  */                                       0546 00169000
*                                                                  0546 00170000
*    RFY (EMCR2,EMCR3) RSTD;                                       0547 00171000
*      GEN SETS(RPHWORK)                                           0548 00172000
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0548 00173000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00174000
*      EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                         0549 00175000
         SLR   EMCR3,EMCR3                                         0549 00176000
*      EMCR3 = '01'X; /*INDICATE SHARED LOCK*/                     0550 00177000
         LA    EMCR3,1                                             0550 00178000
*      CALL ATCLCPTR->ATCCDFND->ISTSDCCF; /*PROCESS REQUEST*/      0551 00179000
*                                                                  0551 00180000
         L     @12,ATCLCPTR                                        0551 00181000
         L     @15,ATCCDFND(,@12)                                  0551 00182000
         BALR  @14,@15                                             0551 00183000
*    RFY (EMCR2,EMCR3) UNRSTD;                                     0552 00184000
*    END; /*CIDCTL*/                                               0553 00185000
*/********************************************************************/ 00186000
*/*                                                                  */ 00187000
*/* CALL ISTINCIO TO DO THE ACTUAL ACTIVATE OPERATION.  WHEN CONTROL */ 00188000
*/* RETURNS, SET SWITCHES BASED ON WHETHER I/O WAS PURGED AND WHETHER*/ 00189000
*/* A LU WAS INVOLVED.  RELEASE STORAGE AND RETURN TO THE CALLING    */ 00190000
*/* ROUTINE.                                                         */ 00191000
*/*                                                                  */ 00192000
*/********************************************************************/ 00193000
*   CW8R2 = CW8R0;                                                 0554 00194000
         LR    CW8R2,CW8R0                                         0554 00195000
*   CW8R1 = NCSPLPTR;                                              0555 00196000
         LR    CW8R1,NCSPLPTR                                      0555 00197000
*   CALL ISTINCIO;                                                 0556 00198000
         L     @15,@CV03489                                        0556 00199000
         BALR  @14,@15                                             0556 00200000
* DO;                                                              0557 00201000
*/***  CRRMAINT  PROCNM(ISTINCW8)COMP(VY)FUNC(RET)RPHADD((NCSPLRPH))  * 00202000
**/                                                                0558 00203000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0558 00204000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0558 00205000
         L     @12,NCSPLRPH(,NCSPLPTR)                             0558 00206000
         L     @12,RPHCRR(,@12)                                    0558 00207000
         L     @12,CRACRR(,@12)                                    0558 00208000
         XC    VYCCRT+1(11,@12),VYCCRT+1(@12)                      0558 00209000
         MVI   VYCCRT(@12),B'00000000'                             0558 00210000
*NCSPLRPH->RPHCRR->CRACRR->CINCW8 = '1'B;                          0559 00211000
         OI    CINCW8(@12),B'00000100'                             0559 00212000
* END;                                                             0560 00213000
*   RFY  (CW8R0,CW8R2) UNRSTD;                                     0561 00214000
*   IF NCSIORC = NCSIOPRG               /* I/O PURGED?               */ 00215000
*     THEN;                             /* YES                       */ 00216000
         CLI   NCSIORC(NCSPLPTR),X'08'                             0562 00217000
         BE    @RT00562                                            0562 00218000
*   ELSE                                /* NO, TURN OFF I/O DONE FLAG*/ 00219000
*     DO;                                                          0564 00220000
*       RPRVIODN = CW8OFF;                                         0565 00221000
         L     @12,NCSPLRAD(,NCSPLPTR)                             0565 00222000
         NI    RPRVIODN(@12),B'11101111'                           0565 00223000
*       IF NCSIORC = NCSIOSUC &         /* I/O SUCCESSFUL FOR AN LU? */ 00224000
*          RPRENTRY = RPRENTLU                                     0566 00225000
*         THEN RLUACTLU = CW8ON;        /* YES, FLAG RDTE ACTLU DONE */ 00226000
         CLI   NCSIORC(NCSPLPTR),X'00'                             0566 00227000
         BNE   @RF00566                                            0566 00228000
         CLI   RPRENTRY(@12),X'81'                                 0566 00229000
         BNE   @RF00566                                            0566 00230000
         OI    RLUACTLU(@12),B'10000000'                           0567 00231000
*       ELSE;                                                      0568 00232000
@RF00566 DS    0H                                                  0569 00233000
*     END;                                                         0569 00234000
*   CW8WKA = CW8WKPTR;                  /* RELEASE DYNAMIC STORAGE   */ 00235000
@RT00562 L     @12,NCSPLWKA(,NCSPLPTR)                             0570 00236000
         ST    CW8WKPTR,CW8WKA(,@12)                               0570 00237000
*   CW8R13 = CW8SVBKP;                                             0571 00238000
         L     CW8R13,CW8SVBKP(,CW8R13)                            0571 00239000
*   RETURN;                                                        0572 00240000
*                                                                  0572 00241000
@EL00001 DS    0H                                                  0572 00242000
@EF00001 DS    0H                                                  0572 00243000
@ER00001 LM    @14,@12,12(@13)                                     0572 00244000
         BR    @14                                                 0572 00245000
*/* TPEPILOG   */                                                  0573 00246000
*    GEN (PRINT OFF);                                              0573 00247000
*                                                                  0573 00248000
         PRINT OFF                                                      00249000
*/* LOGICAL CHANNEL COMMAND WORD                                      * 00250000
*/* ADD A DECLARE                                      003 10/18/74   * 00251000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0574 00252000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 00253000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 00254000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 00255000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 00256000
*              4  * BIT(1),            /* RESERVED                    * 00257000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0574 00258000
*                                         ERASE OR READ CONT          * 00259000
*              4  LCCWCTL BIT(4),      /* CONTROL TYPE                * 00260000
*                 5  LCCWMODE BIT(2),  /* MODE TYPE                   * 00261000
*                 5  LCCWTYPE BIT(2),  /* OPERATION TYPE              * 00262000
*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00263000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00264000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00265000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00266000
*                                         RECORD                      * 00267000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00268000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00269000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00270000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00271000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00272000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00273000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00274000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00275000
*/* LOGICAL COMMAND CODES */                                       0575 00276000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00277000
*     LXEC     BIT(8) CONSTANT('4B'X), /* RESET AT END OF COMMAND     * 00278000
*     LXO      BIT(8) CONSTANT('5B'X), /* RESET ORDERLY               * 00279000
*     LCCWPRGE BIT(8) CONSTANT('6B'X), /* PURGE                       * 00280000
*     LWH      BIT(8) CONSTANT('0F'X), /* WRITE HEADER INDI. 1ST BLOCK* 00281000
*     LWRVI    BIT(8) CONSTANT('1F'X), /* WRITE RVI                   * 00282000
*     LWNA     BIT(8) CONSTANT('2F'X), /* WRITE NEGATIVE ACK          * 00283000
*     LWAA     BIT(8) CONSTANT('3F'X), /* WRITE ALTERNATE ACK         * 00284000
*     LWPLG    BIT(8) CONSTANT('5F'X), /* WRITE ACK LEADING GRAPHICS  * 00285000
*     LWNLG    BIT(8) CONSTANT('6F'X), /* WRITE NACK LEADING GRAPHICS * 00286000
*     LSMDE    BIT(8) CONSTANT('4F'X), /* SET MODE                    * 00287000
*     LIFBM    BIT(8) CONSTANT('7F'X), /* INDIC 1ST BLOCK OF MESSAGE  * 00288000
*     LTEST    BIT(8) CONSTANT('FE'X), /* TEST LCCW                   * 00289000
*     LPRMPT   BIT(8) CONSTANT('13'X), /* PREEMPT LCCW                * 00290000
*     LRSTR    BIT(8) CONSTANT('43'X), /* RESTORE LCCW                * 00291000
*     LEXEC    BIT(8) CONSTANT('FF'X), /* SPECIAL CONTROL OPERATION   * 00292000
*     LREAD    BIT(2) CONSTANT('10'B),  /* READ TYPE LCCW             * 00293000
*     LWRITE   BIT(2) CONSTANT('01'B),  /* WRITE TYPE LCCW            * 00294000
*     LCNTRL   BIT(2) CONSTANT('11'B),  /* CONTROL TYPE LCCW          * 00295000
*     LRESET   BIT(4) CONSTANT('1011'B),/* RESET COMMANDS             * 00296000
*     LTIC     BIT(8) CONSTANT('08'X),  /* LOGICAL TIC                * 00297000
*     LWR      BIT(6) CONSTANT('000001'B),  /* WRITE CONVERSATIONAL   * 00298000
*     LWB      BIT(6) CONSTANT('000101'B),  /* WRITE BLOCK            * 00299000
*     LWM      BIT(6) CONSTANT('001001'B),  /* WRITE MESSAGE          * 00300000
*     LWT      BIT(6) CONSTANT('001101'B),  /* WRITE TRANSMISSION     * 00301000
*     LEWM     BIT(6) CONSTANT('011001'B),  /* ERASE/WRITE MESSAGE    * 00302000
*     LEWT     BIT(6) CONSTANT('011101'B),  /* ERASE/WRITE TRNSMSSN   * 00303000
*     LEAU     BIT(6) CONSTANT('010001'B),  /* ERASE ALL UNPROTECTED  * 00304000
*     LRB      BIT(6) CONSTANT('000110'B),  /* READ BLOCK             * 00305000
*     LRM      BIT(6) CONSTANT('001010'B),  /* READ MESSAGE           * 00306000
*     LRT      BIT(6) CONSTANT('001110'B),  /* READ TRANSMISSION      * 00307000
*     LRMOD    BIT(6) CONSTANT('010010'B),  /* READ MODIFIED          * 00308000
*     LRBUF    BIT(6) CONSTANT('010110'B),  /* READ BUFFER            * 00309000
*     LRCONT   BIT(6) CONSTANT('011010'B),  /* READ CONTINUOUS        * 00310000
*     LRPEP    BIT(6) CONSTANT('011110'B),  /* READ PERPETUAL         * 00311000
*     LCPYM    BIT(6) CONSTANT('101001'B),  /* COPY MESSAGE           * 00312000
*     LCPYT    BIT(6) CONSTANT('101101'B),  /* COPY TRANSMISSION      * 00313000
*     LD       BIT(8) CONSTANT('23'X),  /* DISCONNECT                 * 00314000
*     LDEC     BIT(8) CONSTANT('33'X),  /* DISCONNECT WITH END OF CALL* 00315000
*     LEOC     BIT(8) CONSTANT('53'X),  /* END OF CALL COMMAND        * 00316000
*     LXEL     BIT(8) CONSTANT('0B'X),  /* RESET ERROR LOCK           * 00317000
*     LXC      BIT(8) CONSTANT('1B'X),  /* RESET CONDITIONAL          * 00318000
*     LXI      BIT(8) CONSTANT('2B'X),  /* RESET IMMEDIATE            * 00319000
*     LC       BIT(8) CONSTANT('03'X),  /* CONTACT                    * 00320000
*     LWPA     BIT(8) CONSTANT('FD'X); /* WRITE POSITIVE ACK          * 00321000
*DCL  LMT      BIT(2) CONSTANT('11'B), /* TRANSMISSION MODE           * 00322000
*     LMS      BIT(2) CONSTANT('00'B), /* SPECIAL MODE LIKE CONV, EAU * 00323000
*     LMB      BIT(2) CONSTANT('01'B), /* BLOCK MODE                  * 00324000
*     LMM      BIT(2) CONSTANT('10'B); /* MSG OR CONTINUOUS MODE      * 00325000
*/* FOLLOWING EQUATES FOR  - PORT SOLICITOR (DIAL)                    * 00326000
*/* PORT SOLICITOR COMMAND CODES                                      * 00327000
*DCL PSOL    BIT(8) CONSTANT('FE'X), /* SPECIAL PORT SOLICITOR     0577 00328000
*                                       COMMAND                       * 00329000
*    STCTL    BIT(8) CONSTANT('01'X), /* START CONTROL                * 00330000
*    RESTCTL  BIT(8) CONSTANT('02'X), /* RESET & CONTROL - REDRIVE    * 00331000
*    STIPUT   BIT(8) CONSTANT('03'X); /* START INPUT                  * 00332000
*/*   FOLLOWING EQUATES FOR SDLC SUPPORT                              * 00333000
*DCL  LCCHOLD  BIT(8) CONSTANT('01'X),  /* HOLD                       * 00334000
