*SAV14A = R14;                     /*SAVE RETURN ADDR*/            0135 00162000
         ST    R14,SAV14A                                          0135 00163000
*IF RPRENTED = ON THEN;            /*END OF SEG-NO PU IN SW SUBAREA*/   00164000
         TM    RPRENTED(R9RDT),B'00000100'                         0136 00165000
         BO    @RT00136                                            0136 00166000
* ELSE DO;                         /*SEARCH EVERY PU IN THE SEGMENT*/   00167000
*  DO UNTIL RPRENTED = ON;                                         0139 00168000
@DL00139 DS    0H                                                  0140 00169000
*    R9RDT = R9RDT + RPRELEN;      /* GET NEXT ENTRY*/             0140 00170000
         AH    R9RDT,RPRELEN(,R9RDT)                               0140 00171000
*    IF RPRENTRY = RPRENTCC                                        0141 00172000
*       THEN DO;                   /*HAVE A PU */                  0141 00173000
         CLI   RPRENTRY(R9RDT),X'71'                               0141 00174000
         BNE   @RF00141                                            0141 00175000
*           R8COUNT = RCCPATHZ;    /*COUNT OF ENTRIES IN ITS PATH TAB*/ 00176000
