* R8COUNT = RCCPATHZ;              /*COUNT OF ENTRIES IN PATH TABLE*/   00314000
         LH    R8COUNT,RCCPATHZ(,R9RDT)                            0196 00315000
*                                  /*OF THIS PU*/                  0197 00316000
* R7PATENT = RCCPATHT;             /*ADDR FIRST ENTRY*/            0197 00317000
