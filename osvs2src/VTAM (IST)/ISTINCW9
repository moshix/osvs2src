*                                  /*IS ZERO IF NO TABLE EXISTS*/  0144 00178000
*           R7PATENT = RCCPATHT;   /*ADDR PATH TABLE(FIRST ENTRY)*/     00179000
         L     R7PATENT,RCCPATHT(,R9RDT)                           0144 00180000
*                                  /*SEARCH EVERY ENTRY IN THIS TABLE*/ 00181000
*           DO WHILE R8COUNT > ZERO;                               0145 00182000
         B     @DE00145                                            0145 00183000
@DL00145 DS    0H                                                  0146 00184000
*              IF NCSPLGID = PATGRPID                              0146 00185000
*                 THEN DO;         /*GID THIS ENTRY MATCHES ONE GIVEN*/ 00186000
