*                     MATCH = ON;  /*INDICATE MATCH FOUND*/        0148 00189000
         OI    MATCH,B'10000000'                                   0148 00190000
*                     IF NCSPLVBF = NCSVPUSE  /*IF PATH=USE SPECIFIED*/ 00191000
*                        THEN PATUSEFG = ON;  /*MARK PATH USEABLE*/     00192000
