* IF NCSIORC = NCSIOSUC            /*IF I/O WAS SUCCESSFUL */      0228 00358000
*    | NCSIORC = NCSIOPRG          /*OR PURGED*/                   0228 00359000
*    THEN;                         /*THEN RETURN*/                 0228 00360000
         CLI   NCSIORC(NCSPLPTR),X'00'                             0228 00361000
         BE    @RT00228                                            0228 00362000
         CLI   NCSIORC(NCSPLPTR),X'08'                             0228 00363000
         BE    @RT00228                                            0228 00364000
*    ELSE IF NCSPLDS = ON          /*IF DATA WAS SAVED*/           0230 00365000
*          THEN IF SRUSENS1 = OPENCON   /*SENSE DATA SAYS FAILED DUE*/  00366000
