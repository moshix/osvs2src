SVTM     TITLE ' SSCP -- ISTINCX1 -- DEACTIVATE SWITCHED OR LOCAL PU   *00001000
                        '                                               00002000
ISTINCX1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCX1  78.031'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*      /* MAINLINE CODE OF ISTINCX1 -- DEACTIVATE SWITCHED OR LOCAL PU  00014000
*             */                                                   0076 00015000
* DO;                                                              0076 00016000
*/***  CRRMAINT  PROCNM(ISTINCX1)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00017000
**/                                                                0077 00018000
*                                                                  0077 00019000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0077 00020000
* RFY ISTLCCW ;                                                    0077 00021000
* RFY ISTLCPB ;                                                    0078 00022000
* RFY ISTZSAVE;                                                    0079 00023000
* RFY ISTPFCRR;                                                    0080 00024000
* RFY ISTCFCRR;                                                    0081 00025000
* RFY ISTZCRR ;                                                    0082 00026000
* RFY ISTASCRR;                                                    0083 00027000
* RFY ISTAPCRR;                                                    0084 00028000
* RFY ISTCRA  ;                                                    0085 00029000
* RFY ISTCNSTA;                                                    0086 00030000
* RFY ISTVYCRR;                                                    0087 00031000
*/* END OF ATBUILD */                                              0088 00032000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0088 00033000
*NCSPLRPH->RPHCRR->CRACRR->NINCX1 = '1'B;                          0088 00034000
         L     @08,NCSPLRPH(,R1)                                   0088 00035000
         L     @08,RPHCRR(,@08)                                    0088 00036000
         L     @08,CRACRR(,@08)                                    0088 00037000
         OI    NINCX1(@08),B'10000000'                             0088 00038000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0089 00039000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0089 00040000
         XC    VYCCRT+1(11,@08),VYCCRT+1(@08)                      0089 00041000
         MVI   VYCCRT(@08),B'00000000'                             0089 00042000
*NCSPLRPH->RPHCRR->CRACRR->CINCX1 = '1'B;                          0090 00043000
         OI    CINCX1(@08),B'10000000'                             0090 00044000
* END;                                                             0091 00045000
*RFY R15 RSTD;                                                     0092 00046000
*R15=REGSAPTR;   /* SAVE THE CALLER'S REGISTER SAVEAREA POINTER */ 0093 00047000
         LR    R15,REGSAPTR                                        0093 00048000
*REGSAPTR=REGSAPTR-LENGTH(CX1REGSV); /* DECREMENT REG 13 BY 72 */  0094 00049000
         SL    REGSAPTR,@CF00994                                   0094 00050000
*CX1SVBKP=R15;   /* POINT THIS ROUTINE'S SAVEAREA TO THE CALLER'S */    00051000
