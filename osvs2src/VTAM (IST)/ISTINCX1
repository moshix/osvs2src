*                                       THE PU VARY PAB */         0128 00135000
         L     @08,RCCVPAB(,CX1RDT@)                               0128 00136000
         LTR   @08,@08                                             0128 00137000
         BZ    @RF00128                                            0128 00138000
*                  NCSPLEBN=ON;     /* MARK NCSPL AS QUEUED TO THE PU * 00139000
*                                      VARY PAB */                 0130 00140000
         OI    NCSPLEBN(NCSPLPTR),B'00100000'                      0130 00141000
*                  CALL CX1UNLOK;  /* RELEASE THE RDT, VD, AND THE VOC  00142000
*                                     LOCKS */                     0131 00143000
         BAL   @14,CX1UNLOK                                        0131 00144000
*                  NXTAVAIL=NCSPLWKA+EIGHT;  /* FREE ALL WORKING   0132 00145000
*                                               STORAGE */         0132 00146000
*                                                                  0132 00147000
