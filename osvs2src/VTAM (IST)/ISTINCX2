SVTM     TITLE ' SSCP -- ISTINCX2 -- BREAK CONNECTION                  *00001000
                        '                                               00002000
ISTINCX2 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCX2  75.129'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0069 00014000
*CX2NCSPL=CX2INPUT;                 /* COPY INPUT-NCSPL PTR */     0070 00015000
         LR    CX2NCSPL,CX2INPUT                                   0070 00016000
* DO;                                                              0071 00017000
*/***  CRRMAINT  PROCNM(ISTINCX2)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00018000
**/                                                                0072 00019000
*                                                                  0072 00020000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0072 00021000
* RFY ISTLCCW ;                                                    0072 00022000
* RFY ISTLCPB ;                                                    0073 00023000
* RFY ISTZSAVE;                                                    0074 00024000
* RFY ISTPFCRR;                                                    0075 00025000
* RFY ISTCFCRR;                                                    0076 00026000
* RFY ISTZCRR ;                                                    0077 00027000
* RFY ISTASCRR;                                                    0078 00028000
* RFY ISTAPCRR;                                                    0079 00029000
* RFY ISTCRA  ;                                                    0080 00030000
* RFY ISTCNSTA;                                                    0081 00031000
* RFY ISTVYCRR;                                                    0082 00032000
*/* END OF ATBUILD */                                              0083 00033000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0083 00034000
*NCSPLRPH->RPHCRR->CRACRR->NINCX2 = '1'B;                          0083 00035000
         L     @11,NCSPLRPH(,CX2NCSPL)                             0083 00036000
         L     @11,RPHCRR(,@11)                                    0083 00037000
         L     @11,CRACRR(,@11)                                    0083 00038000
         OI    NINCX2(@11),B'01000000'                             0083 00039000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0084 00040000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0084 00041000
         XC    VYCCRT+1(11,@11),VYCCRT+1(@11)                      0084 00042000
         MVI   VYCCRT(@11),B'00000000'                             0084 00043000
*NCSPLRPH->RPHCRR->CRACRR->CINCX2 = '1'B;                          0085 00044000
         OI    CINCX2(@11),B'01000000'                             0085 00045000
* END;                                                             0086 00046000
*/*------------------------------------------------------------------*/ 00047000
*/* HOUSE KEEKING - SET UP SAVE AREA LINKAGE ETC                     */ 00048000
*/*------------------------------------------------------------------*/ 00049000
*CX2WORKR=CX2SAVE;                                                 0087 00050000
         LR    CX2WORKR,CX2SAVE                                    0087 00051000
*CX2SAVE=CX2SAVE-LENGTH(CX2SVMAP);                                 0088 00052000
         SL    CX2SAVE,@CF00902                                    0088 00053000
*CX2WORKR->CX2FWPTR=CX2SAVE;                                       0089 00054000
         ST    CX2SAVE,CX2FWPTR(,CX2WORKR)                         0089 00055000
*CX2SAVE->CX2BKPTR=CX2WORKR;                                       0090 00056000
         ST    CX2WORKR,CX2BKPTR(,CX2SAVE)                         0090 00057000
*CX2SAVE->CX2FWPTR=0;                                              0091 00058000
         SLR   @11,@11                                             0091 00059000
         ST    @11,CX2FWPTR(,CX2SAVE)                              0091 00060000
*CX2WKA=NCSPLWKA->CX2NXTAV;                                        0092 00061000
         L     @11,NCSPLWKA(,CX2NCSPL)                             0092 00062000
         L     CX2WKA,CX2NXTAV(,@11)                               0092 00063000
*CX2WKL='00'X;                                                     0093 00064000
         XC    CX2WKL+1(67,CX2WKA),CX2WKL+1(CX2WKA)                0093 00065000
         MVI   CX2WKL(CX2WKA),X'00'                                0093 00066000
*CX2WKASV=CX2WKA;                   /* SAVE NEXT AVAILABLE ADDR */ 0094 00067000
         ST    CX2WKA,CX2WKASV(,CX2WKA)                            0094 00068000
*CX2NXTAV=CX2WKA+LENGTH(CX2WKMAP);                                 0095 00069000
         LA    @10,68                                              0095 00070000
         ALR   @10,CX2WKA                                          0095 00071000
         ST    @10,CX2NXTAV(,@11)                                  0095 00072000
*CX2NXTAV=CX2NXTAV+(8-(CX2NXTAV//8));                              0096 00073000
         LR    @06,@10                                             0096 00074000
         SRDA  @06,32                                              0096 00075000
         D     @06,@CF00092                                        0096 00076000
         LCR   @06,@06                                             0096 00077000
         AL    @06,@CF00092                                        0096 00078000
         ALR   @10,@06                                             0096 00079000
         ST    @10,CX2NXTAV(,@11)                                  0096 00080000
*/*------------------------------------------------------------------*/ 00081000
*/* THE FIRST FUNCTION OF THE SEGMET IS TO DETERMINE WHETHER OR NOT  */ 00082000
*/* THERE ARE ANY LOGON OR ACQUIRE ICES FOR THIS NODE AND IF THERE   */ 00083000
*/* ARE, BUILD A PARAMETER LIST FOR CONNECTION SERVICES TO CANCEL    */ 00084000
*/* ICES.  IF THERE ARE NO LOGON OR ACQUIRE ICES, THE FIELDS RCPCELN */ 00085000
*/* AND RCPCEAQ WILL EACH POINT TO THEMSELVES.                       */ 00086000
*/* THE SECOND FUNCTION OF THIS SEGMENT IS TO DECHAIN THE NODE FROM  */ 00087000
*/* FROM THE LOGON CHAIN.  IF IT IS NOT ON A LOGON CHAIN, THE FIELD  */ 00088000
*/* RPRLGONB WILL POINT TO THE INPUT RDTE, IN WHICH CASE PROCESSING  */ 00089000
*/* IS COMPLETE.  OTHERWISE, RPRLGONB POINTS TO THE APPLICATION RDTE */ 00090000
*/* WHOSE LOGON FORWARD CHAIN IS FOLLOWED UNTIL THE RDTE IS FOUND    */ 00091000
*/* WHOSE LOGON FORWARD CHAIN POINTS TO THIS RDTE.  ONCE THIS RDTE   */ 00092000
*/* IS FOUND, THE CONTENT OF ITS LOGON FORWORD FIELD IS CHANGED TO   */ 00093000
*/* THE VALUE OF THE INPUT RDTE'S LOGON FORWARD FIELD.               */ 00094000
*/*------------------------------------------------------------------*/ 00095000
*                                                                  0097 00096000
*CX2RDTE=NCSPLRAD;                                                 0097 00097000
         L     CX2RDTE,NCSPLRAD(,CX2NCSPL)                         0097 00098000
*IF RCPICEAQ = ADDR(RCPICEAQ) &                                    0098 00099000
*   RCPICELN = ADDR(RCPICELN)                                      0098 00100000
* THEN;                                                            0098 00101000
         LA    @11,RCPICEAQ(,CX2RDTE)                              0098 00102000
         C     @11,RCPICEAQ(,CX2RDTE)                              0098 00103000
         BNE   @RF00098                                            0098 00104000
         LA    @11,RCPICELN(,CX2RDTE)                              0098 00105000
         C     @11,RCPICELN(,CX2RDTE)                              0098 00106000
         BE    @RT00098                                            0098 00107000
* ELSE DO;                                                         0100 00108000
@RF00098 DS    0H                                                  0101 00109000
*  CX2CSPPT=ADDR(CX2CSP);                                          0101 00110000
         ST    CX2WKA,CX2CSPPT(,CX2WKA)                            0101 00111000
*  CX2CSLPT=ADDR(CX2CSL);                                          0102 00112000
         LA    @11,CX2CSL(,CX2WKA)                                 0102 00113000
         ST    @11,CX2CSLPT(,CX2WKA)                               0102 00114000
*  CSPIDX=CSPICNCL;                   /* CANCEL */                 0103 00115000
         L     @10,CX2CSPPT(,CX2WKA)                               0103 00116000
         MVC   CSPIDX(2,@10),@CH01002                              0103 00117000
*  CSPACQS='1'B;                                                   0104 00118000
         OI    CSPACQS(@10),B'00001000'                            0104 00119000
*  CSPNO=1;                                                        0105 00120000
         MVC   CSPNO(2,@10),@CH00094                               0105 00121000
*  CSPRSCE=ADDR(CX2CSL);                                           0106 00122000
         ST    @11,CSPRSCE(,@10)                                   0106 00123000
*  CSLRDTE=NCSPLRAD;                                               0107 00124000
         L     @09,NCSPLRAD(,CX2NCSPL)                             0107 00125000
         ST    @09,CSLRDTE(,@11)                                   0107 00126000
*  CX2CSLW=NCSPLRAD;                                               0108 00127000
         ST    @09,CX2CSLW(,CX2WKA)                                0108 00128000
*  CX2INPUT=NCSPLRPH;                                              0109 00129000
         L     CX2INPUT,NCSPLRPH(,CX2NCSPL)                        0109 00130000
*  CX2INPUT->RPHWEA=CX2CSPPT;                                      0110 00131000
         ST    @10,RPHWEA(,CX2INPUT)                               0110 00132000
*  CX2SAVE=CX2SAVE-2*LENGTH(CX2SVMAP);                             0111 00133000
         SL    CX2SAVE,@CF03791                                    0111 00134000
*  CALL NCSPLCVT->ATCCSMR;                                         0112 00135000
         L     @11,NCSPLCVT(,CX2NCSPL)                             0112 00136000
         L     @15,ATCCSMR(,@11)                                   0112 00137000
         BALR  @14,@15                                             0112 00138000
*  CX2INPUT->RPHWEA=CX2NCSPL;                                      0113 00139000
         ST    CX2NCSPL,RPHWEA(,CX2INPUT)                          0113 00140000
*  CX2SAVE=CX2SAVE+2*LENGTH(CX2SVMAP);                             0114 00141000
*                                                                  0114 00142000
         AL    CX2SAVE,@CF03791                                    0114 00143000
*    DO;                                                           0115 00144000
*/***  CRRMAINT  PROCNM(ISTINCX2)COMP(VY)FUNC(RET)  ***/           0116 00145000
*    DCL CMZK0010 PTR(31) REG(1);                                  0116 00146000
*    RFY (CMZK0010) RSTD;                                          0117 00147000
*    /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                 0118 00148000
*CMZK0010->RPHCRR->CRACRR->VYCCRT = '0'B;                          0118 00149000
         L     @11,RPHCRR(,CMZK0010)                               0118 00150000
         L     @11,CRACRR(,@11)                                    0118 00151000
         XC    VYCCRT+1(11,@11),VYCCRT+1(@11)                      0118 00152000
         MVI   VYCCRT(@11),B'00000000'                             0118 00153000
*CMZK0010->RPHCRR->CRACRR->CINCX2 = '1'B;                          0119 00154000
         OI    CINCX2(@11),B'01000000'                             0119 00155000
*    END;                                                          0120 00156000
* END;                                                             0121 00157000
*                                                                  0121 00158000
*IF CX2RDTE->RPRLGONB ^= CX2RDTE                                   0122 00159000
* THEN DO;                                                         0122 00160000
@RT00098 L     @11,RPRLGONB(,CX2RDTE)                              0122 00161000
         CR    @11,CX2RDTE                                         0122 00162000
         BE    @RF00122                                            0122 00163000
*  CX2WORKR=CX2RDTE->RPRLGONB;                                     0124 00164000
         LR    CX2WORKR,@11                                        0124 00165000
*  DO WHILE(CX2WORKR->RPRLGONF^=CX2RDTE);                          0125 00166000
         B     @DE00125                                            0125 00167000
@DL00125 DS    0H                                                  0126 00168000
*   CX2WORKR=CX2WORKR->RPRLGONF;                                   0126 00169000
         L     CX2WORKR,RPRLGONF(,CX2WORKR)                        0126 00170000
*  END;                                                            0127 00171000
@DE00125 C     CX2RDTE,RPRLGONF(,CX2WORKR)                         0127 00172000
         BNE   @DL00125                                            0127 00173000
*  CX2WORKR->RPRLGONF=CX2RDTE->RPRLGONF;                           0128 00174000
         L     @11,RPRLGONF(,CX2RDTE)                              0128 00175000
         ST    @11,RPRLGONF(,CX2WORKR)                             0128 00176000
*  CX2RDTE->RPRLGONF=CX2RDTE;                                      0129 00177000
         ST    CX2RDTE,RPRLGONF(,CX2RDTE)                          0129 00178000
*  CX2RDTE->RPRLGONB=CX2RDTE;                                      0130 00179000
         ST    CX2RDTE,RPRLGONB(,CX2RDTE)                          0130 00180000
* END;                                                             0131 00181000
*ELSE;                                                             0132 00182000
@RF00122 DS    0H                                                  0133 00183000
*CX2SAVE=CX2SAVE->CX2BKPTR;         /* RESTORE CALLER R13 */       0133 00184000
         L     CX2SAVE,CX2BKPTR(,CX2SAVE)                          0133 00185000
*CX2NXTAV=CX2WKASV;                                                0134 00186000
         L     @11,NCSPLWKA(,CX2NCSPL)                             0134 00187000
         L     @10,CX2WKASV(,CX2WKA)                               0134 00188000
         ST    @10,CX2NXTAV(,@11)                                  0134 00189000
*RETURN;                                                           0135 00190000
@EL00001 DS    0H                                                  0135 00191000
@EF00001 DS    0H                                                  0135 00192000
@ER00001 LM    @14,@12,12(@13)                                     0135 00193000
         BR    @14                                                 0135 00194000
*/*------------------------------------------------------------------*/ 00195000
*/* END OF ISTINCX2, SSCP  BREAK CONNECTION                          */ 00196000
