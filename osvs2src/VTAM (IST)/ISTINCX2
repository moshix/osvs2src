         PRINT OFF                                                      00202000
*/*   PROGRAM OPERATOR MESSAGE HEADER                                */ 00203000
*DCL  1  ISTPOHD  BDY(WORD) BASED,                                 0137 00204000
*        2 POHRSVD1 CHAR(1),       /* RESERVED                       */ 00205000
*        2 POHSTAT,                /* HEADER STATUS BITS             */ 00206000
*          3 POHEND   BIT(1),      /* END LINE 1=YES                 */ 00207000
*          3 POHDATA  BIT(1),      /* DATA LINE 1=YES                */ 00208000
*          3 POHLBL   BIT(1),      /* LABEL LINE 1=YES               */ 00209000
*          3 POHCNTRL BIT(1),      /* CONTROL LINE 1=YES             */ 00210000
*          3 POHRSVD2 BIT(2),      /* RESERVED                       */ 00211000
*          3 POHRREQ  BIT(1),      /* REPLY REQUESTED 1=YES          */ 00212000
*          3 POHGEN   BIT(1),      /* ID ORIGIN  0=GENERATED BY VTAM */ 00213000
*                                  /*            1=GENERATED BY POA  */ 00214000
*        2 POHID    FIXED(16);     /* MESSAGE IDENTIFIER             */ 00215000
*/*            VTAM PREFIX FOR QUEUE ELEMENTS                         * 00216000
*/*  INITIAL LEVEL                                     001 03/04/75  */ 00217000
*DECLARE                                                           0138 00218000
*  1 ISTPFX  BDY(WORD)  BASED,                                     0138 00219000
*   2 PFXID        CHAR(4),             /* QUEUE ACRONYM (SAME AS  0138 00220000
*                                          QABID)                     * 00221000
*     3 PFXCBID    BIT(8),              /* CONTROL BLOCK ID           * 00222000
*   2 PFXPRTY      CHAR(1),             /* PRIORITY OF ELEMENT        * 00223000
*   2 PFXLNG       CHAR(3),             /* ELEMENT LENGTH,         0138 00224000
*                                          INCLUDING PREFIX           * 00225000
*   2 PFXFWD       PTR(31),   /* POINTER TO NEXT QUEUE             0138 00226000
*                                          ELEMENT (IF LAST        0138 00227000
*                                          POINTER TO QAB)            * 00228000
*   2 PFXBCK       PTR(31) ;   /* POINTER TO PREVIOUS              0138 00229000
*                                          QUEUE ELEMENT (IF       0138 00230000
*                                          FIRST, POINTER TO QAB)     * 00231000
*DCL NCSPLTYP BIT(8) CONSTANT('60'X);   /* NCSPL CONTROL BLOCK ID     * 00232000
*   /* NETWORK CONFIGURATION SERVICES CONTROL BLOCK  */            0140 00233000
*/*  DELTA                                             13 02/08/75  */  00234000
*DECLARE                                                           0140 00235000
*  1 ISTNCSPL BASED BDY(DWORD),                                    0140 00236000
*   2 *      CHAR(16) BDY(WORD),                                   0140 00237000
*     3 NCSPFX CHAR(LENGTH(ISTPFX)), /* QUE ELEMENT PREFIX           */ 00238000
*       4 *        CHAR(12),                                       0140 00239000
*       4 NCSCRWKA PTR(31),       /* PTR TO WORK AREA FOR C/R I/O    */ 00240000
*   2 *           BIT(8),         /*RESERVED                         */ 00241000
*   2 NCSPLVCD    BIT(8),         /*VERB CODE                        */ 00242000
*   2 NCSPLVBF    CHAR(2),        /*VERB MODIFIER FLAGS              */ 00243000
*      3 NCSVBF01 BIT(1),         /* UNUSED                          */ 00244000
*      3 NCSVBF02 BIT(1),         /* UNUSED                          */ 00245000
*      3 NCSVBF03 BIT(1),         /* MODIFY MSG SUP                  */ 00246000
*      3 NCSVBF04 BIT(1),         /* TYPE=VTAM                       */ 00247000
*      3 NCSVBF05 BIT(1),         /* LINETRACE=1                     */ 00248000
*      3 NCSVBF06 BIT(1),         /* I/O TRACE=1                     */ 00249000
*      3 NCSVBF07 BIT(1),         /* BUFFER TRACE=1                  */ 00250000
*      3 NCSVBF08 BIT(1),         /* MODIFY TPRINT                   */ 00251000
*      3 NCSVBF09 BIT(1),         /* MODIFY TEST                     */ 00252000
*      3 NCSVBF10 BIT(1),         /* MODIFY TRACE=NO                 */ 00253000
*      3 NCSVBF11 BIT(1),         /* VARY INTERNAL ONLY & MODIFY     */ 00254000
*                                 /*  TRACE = YES                    */ 00255000
*      3 NCSVBF12 BIT(1),         /* VARY IMMEDIATE  & MODIFY        */ 00256000
*                                 /*  NETSOL=NO                      */ 00257000
*      3 NCSVBF13 BIT(1),         /* MODIFY NETSOL=YES               */ 00258000
*      3 NCSVBF14 BIT(1),         /* VARY LOGON & MODIFY             */ 00259000
*                                 /*  CHANGE                         */ 00260000
*      3 NCSVBF15 BIT(1),         /* VARY DEACTIVATE & VARY NORMAL   */ 00261000
*                                 /*  CLOSE(ERP)                     */ 00262000
*      3 NCSVBF16 BIT(1),         /* VARY ACTIVATE & ERP REQUEST     */ 00263000
*                                 /*   & MODIFY DUMP                 */ 00264000
*   2 NCSPLVFL    CHAR(1),        /*COMMAND FLAGS                    */ 00265000
*    3 NCSVNCSA   BIT(1),         /*NCSPL IS ACTIVE                  */ 00266000
*    3 NCSVDEL    BIT(1),         /*DELETE NCSPL                     */ 00267000
*    3 NCSVSTAT   BIT(1),         /*INDICATE NCSPL WAS NOT GETMAINED */ 00268000
*    3 NCSDCONT   BIT(1),         /* DO DISCONACT                    */ 00269000
*    3 NCSABCON   BIT(1),         /* DO ABANDON CONNECTION           */ 00270000
*    3 NCSDACLK   BIT(1),         /* DO ACTIVATE LINE                */ 00271000
*    3 NCSRSV02   BIT(2),         /* RESERVED                        */ 00272000
*   2 NCSPLVF1    CHAR(1),        /* SPECIAL PROC FLAGS              */ 00273000
*    3 NCSOVFMC   BIT(1),         /* PURGE ONLY VARY'S FMCB          */ 00274000
*   2 NCSPRMRC    CHAR(1),        /* PURGE MINOR RETURN CODE FIELD   */ 00275000
*   2 NCSIORC     CHAR(1),        /* I/O OR RESTART RETURN CODE      */ 00276000
*   2 NCSRRNPT    CHAR(4),        /* ADDR RN ENTRY FOR LINE TRACE    */ 00277000
*   2 NCSPLRID    CHAR(8),        /*ID = 'NODENAME'                  */ 00278000
*   2 NCSPLLID    CHAR(8),        /*LOGON = 'NODENAME'               */ 00279000
*   2 NCSDLID     CHAR(6),        /* OFFHOOK ID                      */ 00280000
*   2 NCSCRCMD    FIXED(8),       /* C/R I/O COMMAND CODE            */ 00281000
*   2 NCSCRPID    PTR(8),         /* C/R PATH ID (PID)               */ 00282000
*   2 NCSPLRPT    PTR(31),        /*ID='NODENAME' RDTE ADDRESS       */ 00283000
*   2 NCSPLLPT    PTR(31),        /*LOGON/LOGOFF='NODENAME' RDTE ADDR*/ 00284000
*   2 NCSPLCMD    CHAR(2),        /*3705 CONTROL COMMAND CODES       */ 00285000
*     3 NCSCMDA  BIT(8),          /*RH FLAGS                         */ 00286000
*       4 NCSPLSYS BIT(1),        /*ON-SYSTEM OFF-FM                 */ 00287000
*       4 NCSPLCTL BIT(1),        /*ON-CONTROL OFF-DATA              */ 00288000
*       4 NCSPLSYN BIT(1),        /*ON=SYNCHR OFF-ASYNCHR            */ 00289000
*       4 NCSPLFLO BIT(1),        /*ON-WITH FLOW  OFF-AGAINST FLOW   */ 00290000
*       4 NCSPLUFM BIT(1),        /*ON=UNFORMATTED RU,OFF=FORMATTED  */ 00291000
*       4 *        BIT(3),        /*RESERVED                         */ 00292000
*     3 NCSCMDB  BIT(8),          /*COMMAND FIELD                    */ 00293000
*   2 NCSNCPEP    BIT(8),         /* EP SUBCHANNEL ADDR              */ 00294000
*   2 NCSMSGSP    CHAR(1),        /* RESERVED FOR MESSAGE SURPRESSION*/ 00295000
*   2 NCSPLUAD    CHAR(3),        /*U = 'UNIT ADDRESS'               */ 00296000
*   2 NCSCRFLG    CHAR(1),        /* MISCELLANEOUS C/R FLAGS         */ 00297000
*     3 NCSPLWRM    BIT(1),       /* COMMAND SPECIFIED WARM PARAM,   */ 00298000
*                                 /* 1 = YES                         */ 00299000
*     3 NCSPLCLD    BIT(1),       /* COMMAND SPECIFIED COLD PARAM,   */ 00300000
*                                 /* 1 = YES                         */ 00301000
*     3 NCSFMCLD    BIT(1),       /* 370X RESPONDED COLD TO ACTPU    */ 00302000
*                                 /* (ERP), 1 = YES                  */ 00303000
*     3 NCSFMERP    BIT(1),       /* 370X RESPONDED ERP TO ACTPU     */ 00304000
*                                 /* (ERP), 1 = YES                  */ 00305000
*     3 NCSCRDSE    BIT(1),       /* 0=C/R DATA SET DOES NOT EXIST   */ 00306000
*                                 /* OR IS NULL                      */ 00307000
*                                 /* 1=C/R DATA SET EXISTS           */ 00308000
*                                 /* AND IS NOT NULL                 */ 00309000
*     3 NCSOPRQU    BIT(1),       /* OPERATOR REQUESTS REIPL, 1 = YES*/ 00310000
*     3 NCSUSRNF    BIT(1),       /* EARLY WARNING PURGE HAS ALREADY */ 00311000
*                                 /* OCCURED, 1 = YES                */ 00312000
*     3 NCSSTCMD    BIT(1),       /* ACTIVATE IS A RESULT OF         */ 00313000
*                                 /* START VTAM COMMAND, 1 = YES     */ 00314000
*   2 NCSPLWRE    BIT(32),        /* WTOR ECB                        */ 00315000
*   2 NCSPLWRF    CHAR(72),       /* WTOR REPLY AREA                 */ 00316000
*   2 NCSPLRAD    PTR(31),        /* RDTE PTR FOR ALLOCATE/DEALLOCATE*/ 00317000
*     3 NCSLSCMD BIT(8),          /* LINE SCHED COMMAND TYPE         */ 00318000
*     3 NCSPLRCD BIT(8),          /* HEX VALUE FOR LINE SCHED PARAM  */ 00319000
*   2 NCSIOCID    CHAR(4),        /* CURRENT CID FOR DAF LOCK HELD   */ 00320000
*     3  NCSPLCID CHAR(4),        /* ALIAS FOR ABOVE                 */ 00321000
*        4  NCSPLSAF  CHAR(2),    /* SOURCE ADDRESS                  */ 00322000
*        4  NCSPLDAF  CHAR(2),    /* DESTINATION ADDRESS             */ 00323000
*   2 NCSPLWKA    PTR(31),        /* NCS WORK AREA ADDRESS           */ 00324000
*   2 NCSPLCVT    PTR(31),        /* POINTER TO VTAM CVT             */ 00325000
*   2 NCSPLRPH    PTR(31),        /* POINTER TO RPH                  */ 00326000
*   2 NCSPLECB    BIT(32),        /* ECB FOR VARY PGS                */ 00327000
*   2 NCSPLFSB    PTR(31),        /* ADDRESS OF USS-FSS FSB OR RU    */ 00328000
*   2 NCSPLRCR    PTR(31),        /* ADDRESS RDTE FOR CR             */ 00329000
*   2 NCSPLFL1    CHAR(1),        /* VARY FLAGS                      */ 00330000
*     3 NCSPL2IN  BIT(1),         /* SECOND ENTRY                    */ 00331000
*     3 NCSPLSOM  BIT(1),         /* SUPRESS OP. MESSAGE             */ 00332000
*     3 NCSPLEBN  BIT(1),         /* ENTRY FROM BNN SSCP             */ 00333000
*     3 NCSPLREQ  BIT(1),         /* NCSPL FROM RESTORE              */ 00334000
*     3 NCSPLNON  BIT(1),         /* DO NOT ADDR RDT SEGMENT         */ 00335000
*     3 NCSPLAPU  BIT(1),         /* ACTIVATE PHYSICAL DONE BY LOAD  */ 00336000
*     3 NCSPL2CN  BIT(1),         /* SECOND CONTACT REQUEST          */ 00337000
*     3 NCSPLPOS  BIT(1),         /* BNN POSTED                      */ 00338000
*   2 NCSPLFL2    CHAR(1),        /* VARY FLAGS                      */ 00339000
*     3 NCSPLVDP  BIT(1),         /* NCSPL ON VARDEF PAB             */ 00340000
*     3 NCSPLRSF  BIT(1),         /* NO SICK CLEAR BEFORE I/O        */ 00341000
