         L     @15,CRACRR(,@15)                                    0107 00090000
         XC    VYCCRT+1(11,@15),VYCCRT+1(@15)                      0107 00091000
         MVI   VYCCRT(@15),B'00000000'                             0107 00092000
*NCSPLRPH->RPHCRR->CRACRR->CINCX6 = '1'B;                          0108 00093000
         OI    CINCX6(@15),B'00010000'                             0108 00094000
*  END;                                                            0109 00095000
*                                                                  0109 00096000
*DFNCSPL = RLNX2SW;               /*LINK CONTAINS ADDR 370X TO SW */    00097000
         L     @00,RLNX2SW(,LINKRDTE)                              0110 00098000
         ST    @00,DFNCSPL                                         0110 00099000
*                                 /*NCSPL-FORMAT IT INTO DIAL FAIL */   00100000
*                                 /*NCSPL */                       0111 00101000
*DFNCSPL -> NCSPLVCD = NCSPLVC8;  /*VERB CODE IS DIAL */           0111 00102000
         LR    @15,@00                                             0111 00103000
         MVI   NCSPLVCD(@15),X'08'                                 0111 00104000
*DFNCSPL -> NCSPLVBF = NCSDDF;    /*MODIFIER IS DIAL FAIL */       0112 00105000
         LR    @15,@00                                             0112 00106000
         MVC   NCSPLVBF(2,@15),@CB01062                            0112 00107000
*DFNCSPL -> NCSDLRSN = NCSDLRSN;  /*REASON CODES SAME AS ON INPUT*/     00108000
         LR    @15,@00                                             0113 00109000
         MVC   NCSDLRSN(1,@15),NCSDLRSN(NCSPLPTR)                  0113 00110000
*DFNCSPL -> NCSPLRPT = WRKRDTE;   /*ADDR PU RDTE*/                 0114 00111000
         LR    @15,@00                                             0114 00112000
         ST    WRKRDTE,NCSPLRPT(,@15)                              0114 00113000
*RLNX2SW = ZERO;                  /*CLEAR PTR-NO DIAL CONNECTION */     00114000
         SLR   @00,@00                                             0115 00115000
         ST    @00,RLNX2SW(,LINKRDTE)                              0115 00116000
*                                 /* OVER THIS LINK */             0116 00117000
*                                 /*SEARCH UP CHAIN TO GET SW SUBAREA*/ 00118000
*                                 /* HDR OF DIALING PU */          0116 00119000
*DO WHILE WRKRDTE -> RPRENTRY^= RPRENTSW;                          0116 00120000
         B     @DE00116                                            0116 00121000
@DL00116 DS    0H                                                  0117 00122000
*    WRKRDTE = WRKRDTE -> RPRNSNEA;   /*GET NEXT HIGHER RDTE*/     0117 00123000
