*END;                                                              0118 00125000
*                                                                  0118 00126000
@DE00116 CLI   RPRENTRY(WRKRDTE),X'04'                             0118 00127000
         BNE   @DL00116                                            0118 00128000
*R1 = NCSPLRPH;                   /* GET RPH ADDR */               0119 00129000
         L     R1,NCSPLRPH(,NCSPLPTR)                              0119 00130000
*                                  /*QUEUE DIAL FAIL NCSPL TO SW SUB-*/ 00131000
*                                  /*AREA HDR TO NOTIFY IT OF FAILURE*/ 00132000
*/*  TPQUE (DFNCSPL,DYPPAB) NORM */                                0120 00133000
*                                                                  0120 00134000
*  DO;                                                             0120 00135000
*  RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;                  0121 00136000
*  GEN REFS(GPR01P) SETS(RPHWORK)                                  0122 00137000
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0122 00138000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00139000
*  GPR00P=0;                                                       0123 00140000
         SLR   GPR00P,GPR00P                                       0123 00141000
*  GPR15P=ADDR(DYPPAB); /*HOLD PAB PTR*/                           0124 00142000
