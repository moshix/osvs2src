SVTM     TITLE ' SSCP -- ISTINCX6 -- DIAL FAIL NOTIFY                  *00001000
                        '                                               00002000
ISTINCX6 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCX6  75.129'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*      /* MAINLINE CODE OF ISTINCX6 -- DIAL FAIL NOTIFY */         0075 00014000
*                                                                  0075 00015000
* DO;                                                              0075 00016000
*/***  CRRMAINT  PROCNM(ISTINCX6)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00017000
**/                                                                0076 00018000
*                                                                  0076 00019000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0076 00020000
* RFY ISTLCCW ;                                                    0076 00021000
* RFY ISTLCPB ;                                                    0077 00022000
* RFY ISTZSAVE;                                                    0078 00023000
* RFY ISTPFCRR;                                                    0079 00024000
* RFY ISTCFCRR;                                                    0080 00025000
* RFY ISTZCRR ;                                                    0081 00026000
* RFY ISTASCRR;                                                    0082 00027000
* RFY ISTAPCRR;                                                    0083 00028000
* RFY ISTCRA  ;                                                    0084 00029000
* RFY ISTCNSTA;                                                    0085 00030000
* RFY ISTVYCRR;                                                    0086 00031000
*/* END OF ATBUILD */                                              0087 00032000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0087 00033000
*NCSPLRPH->RPHCRR->CRACRR->NINCX6 = '1'B;                          0087 00034000
         L     @15,NCSPLRPH(,R1)                                   0087 00035000
         L     @15,RPHCRR(,@15)                                    0087 00036000
         L     @15,CRACRR(,@15)                                    0087 00037000
         OI    NINCX6(@15),B'00010000'                             0087 00038000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0088 00039000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0088 00040000
         XC    VYCCRT+1(11,@15),VYCCRT+1(@15)                      0088 00041000
         MVI   VYCCRT(@15),B'00000000'                             0088 00042000
*NCSPLRPH->RPHCRR->CRACRR->CINCX6 = '1'B;                          0089 00043000
         OI    CINCX6(@15),B'00010000'                             0089 00044000
* END;                                                             0090 00045000
*GEN REFS(CX6WAPTR) (   USING  @DATD,CX6WAPTR);                    0091 00046000
            USING  @DATD,CX6WAPTR                                       00047000
*RFY R15 RSTD;                                                     0092 00048000
*R15=REGSAPTR;   /* SAVE THE CALLER'S REGISTER SAVEAREA POINTER */ 0093 00049000
         LR    R15,REGSAPTR                                        0093 00050000
*REGSAPTR=REGSAPTR-LENGTH(CX6REGSV); /* DECREMENT REG 13 BY 72 */  0094 00051000
         SL    REGSAPTR,@CF00904                                   0094 00052000
*CX6SVBKP=R15;   /* POINT THIS ROUTINE'S SAVEAREA TO THE CALLER'S */    00053000
         ST    R15,CX6SVBKP(,REGSAPTR)                             0095 00054000
*CX6SVFWP=ZERO;  /* ZERO THIS ROUTINE'S SAVEAREA FORWARD POINTER */     00055000
         SLR   @14,@14                                             0096 00056000
         ST    @14,CX6SVFWP(,REGSAPTR)                             0096 00057000
*R15->CX6SVFWP=REGSAPTR; /* POINT CALLER'S SAVEAREA TO THIS SAVEAREA */ 00058000
         ST    REGSAPTR,CX6SVFWP(,R15)                             0097 00059000
*CX6WAPTR=NXTAVAIL;/* GET THE NEXT AVAILABLE ADDRESS IN THE WORKAREA*/  00060000
         L     @14,NCSPLWKA(,R1)                                   0098 00061000
         L     CX6WAPTR,NXTAVAIL(,@14)                             0098 00062000
*RFY R15 UNRSTD;                                                   0099 00063000
*NXTAVAIL=CX6WAPTR+@SIZDATD; /* RESET NEXT AVAILABLE BEYOND THIS   0100 00064000
*                           ROUTINE'S DYNAMIC AREA */              0100 00065000
         LR    @00,CX6WAPTR                                        0100 00066000
         AL    @00,@SIZDATD                                        0100 00067000
         ST    @00,NXTAVAIL(,@14)                                  0100 00068000
*NCSPLPTR=R1;             /* GET THE NCSPL ADDRESS */              0101 00069000
         LR    NCSPLPTR,R1                                         0101 00070000
*RFY ISTNCSPL BASED(NCSPLPTR);                                     0102 00071000
*                                                                  0102 00072000
*WRKRDTE = NCSPLRAD;              /*ADDR OF DIALING OUT PU RDTE*/  0103 00073000
         L     WRKRDTE,NCSPLRAD(,NCSPLPTR)                         0103 00074000
*LINKRDTE = WRKRDTE->RPRNSNEA;    /*ON ENTRY, NSNEA IS LINK RDTE-SAVE*/ 00075000
         L     LINKRDTE,RPRNSNEA(,WRKRDTE)                         0104 00076000
*                                 /*CALL RTN TO DO ACTUAL DEALLOCATE*/  00077000
*                                 /*PARMS REMAIN SAME AS INPUT */  0105 00078000
*CALL ISTINCD7;                                                    0105 00079000
         L     @15,@CV00783                                        0105 00080000
         BALR  @14,@15                                             0105 00081000
*                                 /*NOTE:NSNEA OF PU NOW IS SW SUBAR */ 00082000
*  DO;                                                             0106 00083000
*/***  CRRMAINT  PROCNM(ISTINCX6)COMP(VY)FUNC(RET)RPHADD((NCSPLRPH))  * 00084000
**/                                                                0107 00085000
*  /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                   0107 00086000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0107 00087000
