         MVI   VYCCRT(@11),B'00000000'                             0191 00169000
*NCSPLRPH->RPHCRR->CRACRR->CINCX7 = '1'B;                          0192 00170000
         OI    CINCX7(@11),B'00001000'                             0192 00171000
*  END;                                                            0193 00172000
*CX7NXTAV = CX7WKASV;                /* RESET NCSPL WORKAREA */    0194 00173000
         L     @11,NCSPLWKA(,CX7NCSPL)                             0194 00174000
         L     @10,CX7WKASV(,CX7WKA)                               0194 00175000
         ST    @10,CX7NXTAV(,@11)                                  0194 00176000
*CX7SAVE=CX7SAVE->CX7BKPTR;         /* RESTORE CALLER R13 */       0195 00177000
         L     CX7SAVE,CX7BKPTR(,CX7SAVE)                          0195 00178000
