SVTM     TITLE ' SSCP -- ISTINCX7 -- DACTPU HANDLER                    *00001000
                        '                                               00002000
ISTINCX7 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCX7  75.129'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0133 00014000
*CX7NCSPL=CX7INPUT;                 /* COPY INPUT-NCSPL PTR */     0134 00015000
         LR    CX7NCSPL,CX7INPUT                                   0134 00016000
* DO;                                                              0135 00017000
*/***  CRRMAINT  PROCNM(ISTINCX7)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00018000
**/                                                                0136 00019000
*                                                                  0136 00020000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0136 00021000
* RFY ISTLCCW ;                                                    0136 00022000
* RFY ISTLCPB ;                                                    0137 00023000
* RFY ISTZSAVE;                                                    0138 00024000
* RFY ISTPFCRR;                                                    0139 00025000
* RFY ISTCFCRR;                                                    0140 00026000
* RFY ISTZCRR ;                                                    0141 00027000
* RFY ISTASCRR;                                                    0142 00028000
* RFY ISTAPCRR;                                                    0143 00029000
* RFY ISTCRA  ;                                                    0144 00030000
* RFY ISTCNSTA;                                                    0145 00031000
* RFY ISTVYCRR;                                                    0146 00032000
*/* END OF ATBUILD */                                              0147 00033000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0147 00034000
*NCSPLRPH->RPHCRR->CRACRR->NINCX7 = '1'B;                          0147 00035000
         L     @11,NCSPLRPH(,CX7NCSPL)                             0147 00036000
         L     @11,RPHCRR(,@11)                                    0147 00037000
         L     @11,CRACRR(,@11)                                    0147 00038000
         OI    NINCX7(@11),B'00001000'                             0147 00039000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0148 00040000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0148 00041000
         XC    VYCCRT+1(11,@11),VYCCRT+1(@11)                      0148 00042000
         MVI   VYCCRT(@11),B'00000000'                             0148 00043000
*NCSPLRPH->RPHCRR->CRACRR->CINCX7 = '1'B;                          0149 00044000
         OI    CINCX7(@11),B'00001000'                             0149 00045000
* END;                                                             0150 00046000
*/*------------------------------------------------------------------*/ 00047000
*/* HOUSE KEEKING - SET UP SAVE AREA LINKAGE ETC                     */ 00048000
*/*------------------------------------------------------------------*/ 00049000
*CX7WORKR=CX7SAVE;                                                 0151 00050000
         LR    CX7WORKR,CX7SAVE                                    0151 00051000
*CX7SAVE=CX7SAVE-LENGTH(CX7SVMAP);                                 0152 00052000
         SL    CX7SAVE,@CF01443                                    0152 00053000
*CX7WORKR->CX7FWPTR=CX7SAVE;                                       0153 00054000
         ST    CX7SAVE,CX7FWPTR(,CX7WORKR)                         0153 00055000
*CX7SAVE->CX7BKPTR=CX7WORKR;                                       0154 00056000
         ST    CX7WORKR,CX7BKPTR(,CX7SAVE)                         0154 00057000
*CX7SAVE->CX7FWPTR=0;                                              0155 00058000
         SLR   @11,@11                                             0155 00059000
         ST    @11,CX7FWPTR(,CX7SAVE)                              0155 00060000
*CX7WKA=NCSPLWKA->CX7NXTAV;                                        0156 00061000
         L     @10,NCSPLWKA(,CX7NCSPL)                             0156 00062000
         L     CX7WKA,CX7NXTAV(,@10)                               0156 00063000
*CX7WKL='00'X;                                                     0157 00064000
         XC    CX7WKL+1(11,CX7WKA),CX7WKL+1(CX7WKA)                0157 00065000
         MVI   CX7WKL(CX7WKA),X'00'                                0157 00066000
*CX7WKASV=CX7WKA;                   /* SAVE NEXT AVAILABLE ADDR */ 0158 00067000
         ST    CX7WKA,CX7WKASV(,CX7WKA)                            0158 00068000
*CX7NXTAV=CX7WKA+LENGTH(CX7WKMAP);                                 0159 00069000
         LA    @09,12                                              0159 00070000
         ALR   @09,CX7WKA                                          0159 00071000
         ST    @09,CX7NXTAV(,@10)                                  0159 00072000
*CX7NXTAV=CX7NXTAV+(8-(CX7NXTAV//8));                              0160 00073000
         LR    @06,@09                                             0160 00074000
         SRDA  @06,32                                              0160 00075000
         D     @06,@CF00092                                        0160 00076000
         LCR   @06,@06                                             0160 00077000
         AL    @06,@CF00092                                        0160 00078000
         ALR   @09,@06                                             0160 00079000
         ST    @09,CX7NXTAV(,@10)                                  0160 00080000
*/*-------------------------------------------------------------------* 00081000
*/* THIS IS TO BUILD A DACTPU RU INDICATING "FINAL" OR "NOT FINAL"   */ 00082000
*/* AS REQUIRED.  THE PU DAF LOCK IS THEN OBTAINED AND ISTINCIO IS   */ 00083000
*/* CALLED TO ISSUE THE I/O REQUEST TO THE PU.  ON RETURN FROM       */ 00084000
*/* ISTINCIO THIS ROUNTINE RETURNS TO ITS CALLER                     */ 00085000
*/*------------------------------------------------------------------*/ 00086000
*CX7RDTE = NCSPLRAD;                /* ADDR OF NON-370X PU RDTE */ 0161 00087000
         L     @10,NCSPLRAD(,CX7NCSPL)                             0161 00088000
         ST    @10,CX7RDTE(,CX7WKA)                                0161 00089000
*NCSPLOLN=2;                        /* RU LENGTH */                0162 00090000
         MVC   NCSPLOLN(2,CX7NCSPL),@CH00106                       0162 00091000
*NCSPLRUI=0;                                                       0163 00092000
         ST    @11,NCSPLRUI(,CX7NCSPL)                             0163 00093000
*NCSPLRUO=ADDR(CX7RU);              /* RU ADDR */                  0164 00094000
         ST    CX7WKA,NCSPLRUO(,CX7NCSPL)                          0164 00095000
*CX7RUB1='12'X;                                                    0165 00096000
         MVI   CX7RUB1(CX7WKA),X'12'                               0165 00097000
*IF RCCFINAL = CX7ON                /* REQUEST TO BUILD FINAL RU ? */   00098000
*  THEN CX7RUB2 = '01'X;            /* FINAL */                    0166 00099000
         TM    RCCFINAL(@10),B'00001000'                           0166 00100000
         BNO   @RF00166                                            0166 00101000
         MVI   CX7RUB2(CX7WKA),X'01'                               0167 00102000
*  ELSE CX7RUB2 = '02'X;            /* NOT FINAL */                0168 00103000
         B     @RC00166                                            0168 00104000
@RF00166 MVI   CX7RUB2(CX7WKA),X'02'                               0168 00105000
*NCSPLCMD = NCSCDPU;                                               0169 00106000
@RC00166 MVC   NCSPLCMD(2,CX7NCSPL),@CB01702                       0169 00107000
*NCSPLSAF = 0;                                                     0170 00108000
         SLR   @11,@11                                             0170 00109000
         STH   @11,NCSPLSAF(,CX7NCSPL)                             0170 00110000
*NCSPLDAF = RPRDAF;                                                0171 00111000
         L     @11,CX7RDTE(,CX7WKA)                                0171 00112000
         MVC   NCSPLDAF(2,CX7NCSPL),RPRDAF(@11)                    0171 00113000
*CX7INPUT = NCSPLRPH;                                              0172 00114000
         L     CX7INPUT,NCSPLRPH(,CX7NCSPL)                        0172 00115000
*CX7R2 = NCSIOCID;                  /* R1,R2 INPUT FOR CIDCTL */   0173 00116000
*                                                                  0173 00117000
         L     CX7R2,NCSIOCID(,CX7NCSPL)                           0173 00118000
*  DO; /* CIDCTL(FIND) LOCK(SHARED) */                             0174 00119000
*                                                                  0174 00120000
*  RFY (EMCR2,EMCR3) RSTD;                                         0175 00121000
*                                                                  0175 00122000
*  RFY ISTDYPAB;                                                   0176 00123000
*  RFY ISTATCVT;                                                   0177 00124000
*    GEN SETS(RPHWORK)                                             0178 00125000
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0178 00126000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00127000
*    EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                           0179 00128000
         SLR   EMCR3,EMCR3                                         0179 00129000
*    EMCR3 = '01'X; /*INDICATE SHARED LOCK*/                       0180 00130000
         LA    EMCR3,1                                             0180 00131000
*    CALL ATCLCPTR->ATCCDFND->ISTSDCCF; /*PROCESS REQUEST*/        0181 00132000
*                                                                  0181 00133000
         L     @11,ATCLCPTR                                        0181 00134000
         L     @15,ATCCDFND(,@11)                                  0181 00135000
         BALR  @14,@15                                             0181 00136000
*  RFY (EMCR2,EMCR3) UNRSTD;                                       0182 00137000
*  END; /*CIDCTL*/                                                 0183 00138000
*IF RPRHDTYP=RPRENTLS /* IF PU IN LOCAL SUBAREA              @DCR4036*/ 00139000
*  THEN RPRNSNEA->RPRNCBA->ICNCLOSE='1'B;                  /*@DCR4036*/ 00140000
         L     @11,CX7RDTE(,CX7WKA)                                0184 00141000
         CLI   RPRHDTYP(@11),X'05'                                 0184 00142000
         BNE   @RF00184                                            0184 00143000
         L     @11,RPRNSNEA(,@11)                                  0185 00144000
         L     @11,RPRNCBA(,@11)                                   0185 00145000
         L     @10,ICNCLOSE-1(,@11)                                0185 00146000
@GL00001 LR    @09,@10                                             0185 00147000
         O     @09,@CF04497                                        0185 00148000
         CS    @10,@09,ICNCLOSE-1(@11)                             0185 00149000
         BNE   @GL00001                                            0185 00150000
*  ELSE;                                                   /*@DCR4036*/ 00151000
@RF00184 DS    0H                                                  0187 00152000
*CX7R2 = CX7R0;                     /* GET NCB ADDR */             0187 00153000
         LR    CX7R2,CX7R0                                         0187 00154000
*CX7INPUT = CX7NCSPL;               /* INPUT FOR CIO */            0188 00155000
         LR    CX7INPUT,CX7NCSPL                                   0188 00156000
*CALL ISTINCIO;                     /* DO IO */                    0189 00157000
         L     @15,@CV01358                                        0189 00158000
         BALR  @14,@15                                             0189 00159000
*  DO;                                                             0190 00160000
*/***  CRRMAINT  PROCNM(ISTINCX7)COMP(VY)FUNC(RET)RPHADD((NCSPLRPH))  * 00161000
**/                                                                0191 00162000
*  /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                   0191 00163000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0191 00164000
         L     @11,NCSPLRPH(,CX7NCSPL)                             0191 00165000
         L     @11,RPHCRR(,@11)                                    0191 00166000
