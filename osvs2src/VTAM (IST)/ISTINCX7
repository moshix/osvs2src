         PRINT OFF                                                      00186000
*/*   PROGRAM OPERATOR MESSAGE HEADER                                */ 00187000
*DCL  1  ISTPOHD  BDY(WORD) BASED,                                 0197 00188000
*        2 POHRSVD1 CHAR(1),       /* RESERVED                       */ 00189000
*        2 POHSTAT,                /* HEADER STATUS BITS             */ 00190000
*          3 POHEND   BIT(1),      /* END LINE 1=YES                 */ 00191000
*          3 POHDATA  BIT(1),      /* DATA LINE 1=YES                */ 00192000
*          3 POHLBL   BIT(1),      /* LABEL LINE 1=YES               */ 00193000
*          3 POHCNTRL BIT(1),      /* CONTROL LINE 1=YES             */ 00194000
*          3 POHRSVD2 BIT(2),      /* RESERVED                       */ 00195000
*          3 POHRREQ  BIT(1),      /* REPLY REQUESTED 1=YES          */ 00196000
*          3 POHGEN   BIT(1),      /* ID ORIGIN  0=GENERATED BY VTAM */ 00197000
*                                  /*            1=GENERATED BY POA  */ 00198000
*        2 POHID    FIXED(16);     /* MESSAGE IDENTIFIER             */ 00199000
*/*            VTAM PREFIX FOR QUEUE ELEMENTS                         * 00200000
*/*  INITIAL LEVEL                                     001 03/04/75  */ 00201000
*DECLARE                                                           0198 00202000
*  1 ISTPFX  BDY(WORD)  BASED,                                     0198 00203000
*   2 PFXID        CHAR(4),             /* QUEUE ACRONYM (SAME AS  0198 00204000
*                                          QABID)                     * 00205000
*     3 PFXCBID    BIT(8),              /* CONTROL BLOCK ID           * 00206000
*   2 PFXPRTY      CHAR(1),             /* PRIORITY OF ELEMENT        * 00207000
*   2 PFXLNG       CHAR(3),             /* ELEMENT LENGTH,         0198 00208000
*                                          INCLUDING PREFIX           * 00209000
*   2 PFXFWD       PTR(31),   /* POINTER TO NEXT QUEUE             0198 00210000
*                                          ELEMENT (IF LAST        0198 00211000
*                                          POINTER TO QAB)            * 00212000
*   2 PFXBCK       PTR(31) ;   /* POINTER TO PREVIOUS              0198 00213000
*                                          QUEUE ELEMENT (IF       0198 00214000
*                                          FIRST, POINTER TO QAB)     * 00215000
*DCL NCSPLTYP BIT(8) CONSTANT('60'X);   /* NCSPL CONTROL BLOCK ID     * 00216000
*   /* NETWORK CONFIGURATION SERVICES CONTROL BLOCK  */            0200 00217000
*/*  DELTA                                             13 02/08/75  */  00218000
*DECLARE                                                           0200 00219000
*  1 ISTNCSPL BASED BDY(DWORD),                                    0200 00220000
*   2 *      CHAR(16) BDY(WORD),                                   0200 00221000
*     3 NCSPFX CHAR(LENGTH(ISTPFX)), /* QUE ELEMENT PREFIX           */ 00222000
*       4 *        CHAR(12),                                       0200 00223000
*       4 NCSCRWKA PTR(31),       /* PTR TO WORK AREA FOR C/R I/O    */ 00224000
*   2 *           BIT(8),         /*RESERVED                         */ 00225000
*   2 NCSPLVCD    BIT(8),         /*VERB CODE                        */ 00226000
*   2 NCSPLVBF    CHAR(2),        /*VERB MODIFIER FLAGS              */ 00227000
*      3 NCSVBF01 BIT(1),         /* UNUSED                          */ 00228000
*      3 NCSVBF02 BIT(1),         /* UNUSED                          */ 00229000
*      3 NCSVBF03 BIT(1),         /* MODIFY MSG SUP                  */ 00230000
*      3 NCSVBF04 BIT(1),         /* TYPE=VTAM                       */ 00231000
*      3 NCSVBF05 BIT(1),         /* LINETRACE=1                     */ 00232000
*      3 NCSVBF06 BIT(1),         /* I/O TRACE=1                     */ 00233000
*      3 NCSVBF07 BIT(1),         /* BUFFER TRACE=1                  */ 00234000
*      3 NCSVBF08 BIT(1),         /* MODIFY TPRINT                   */ 00235000
*      3 NCSVBF09 BIT(1),         /* MODIFY TEST                     */ 00236000
*      3 NCSVBF10 BIT(1),         /* MODIFY TRACE=NO                 */ 00237000
*      3 NCSVBF11 BIT(1),         /* VARY INTERNAL ONLY & MODIFY     */ 00238000
*                                 /*  TRACE = YES                    */ 00239000
*      3 NCSVBF12 BIT(1),         /* VARY IMMEDIATE  & MODIFY        */ 00240000
*                                 /*  NETSOL=NO                      */ 00241000
*      3 NCSVBF13 BIT(1),         /* MODIFY NETSOL=YES               */ 00242000
*      3 NCSVBF14 BIT(1),         /* VARY LOGON & MODIFY             */ 00243000
*                                 /*  CHANGE                         */ 00244000
*      3 NCSVBF15 BIT(1),         /* VARY DEACTIVATE & VARY NORMAL   */ 00245000
*                                 /*  CLOSE(ERP)                     */ 00246000
*      3 NCSVBF16 BIT(1),         /* VARY ACTIVATE & ERP REQUEST     */ 00247000
*                                 /*   & MODIFY DUMP                 */ 00248000
*   2 NCSPLVFL    CHAR(1),        /*COMMAND FLAGS                    */ 00249000
*    3 NCSVNCSA   BIT(1),         /*NCSPL IS ACTIVE                  */ 00250000
*    3 NCSVDEL    BIT(1),         /*DELETE NCSPL                     */ 00251000
*    3 NCSVSTAT   BIT(1),         /*INDICATE NCSPL WAS NOT GETMAINED */ 00252000
*    3 NCSDCONT   BIT(1),         /* DO DISCONACT                    */ 00253000
*    3 NCSABCON   BIT(1),         /* DO ABANDON CONNECTION           */ 00254000
*    3 NCSDACLK   BIT(1),         /* DO ACTIVATE LINE                */ 00255000
*    3 NCSRSV02   BIT(2),         /* RESERVED                        */ 00256000
*   2 NCSPLVF1    CHAR(1),        /* SPECIAL PROC FLAGS              */ 00257000
*    3 NCSOVFMC   BIT(1),         /* PURGE ONLY VARY'S FMCB          */ 00258000
*   2 NCSPRMRC    CHAR(1),        /* PURGE MINOR RETURN CODE FIELD   */ 00259000
*   2 NCSIORC     CHAR(1),        /* I/O OR RESTART RETURN CODE      */ 00260000
*   2 NCSRRNPT    CHAR(4),        /* ADDR RN ENTRY FOR LINE TRACE    */ 00261000
*   2 NCSPLRID    CHAR(8),        /*ID = 'NODENAME'                  */ 00262000
*   2 NCSPLLID    CHAR(8),        /*LOGON = 'NODENAME'               */ 00263000
*   2 NCSDLID     CHAR(6),        /* OFFHOOK ID                      */ 00264000
*   2 NCSCRCMD    FIXED(8),       /* C/R I/O COMMAND CODE            */ 00265000
*   2 NCSCRPID    PTR(8),         /* C/R PATH ID (PID)               */ 00266000
*   2 NCSPLRPT    PTR(31),        /*ID='NODENAME' RDTE ADDRESS       */ 00267000
*   2 NCSPLLPT    PTR(31),        /*LOGON/LOGOFF='NODENAME' RDTE ADDR*/ 00268000
*   2 NCSPLCMD    CHAR(2),        /*3705 CONTROL COMMAND CODES       */ 00269000
*     3 NCSCMDA  BIT(8),          /*RH FLAGS                         */ 00270000
*       4 NCSPLSYS BIT(1),        /*ON-SYSTEM OFF-FM                 */ 00271000
*       4 NCSPLCTL BIT(1),        /*ON-CONTROL OFF-DATA              */ 00272000
*       4 NCSPLSYN BIT(1),        /*ON=SYNCHR OFF-ASYNCHR            */ 00273000
*       4 NCSPLFLO BIT(1),        /*ON-WITH FLOW  OFF-AGAINST FLOW   */ 00274000
*       4 NCSPLUFM BIT(1),        /*ON=UNFORMATTED RU,OFF=FORMATTED  */ 00275000
*       4 *        BIT(3),        /*RESERVED                         */ 00276000
*     3 NCSCMDB  BIT(8),          /*COMMAND FIELD                    */ 00277000
*   2 NCSNCPEP    BIT(8),         /* EP SUBCHANNEL ADDR              */ 00278000
*   2 NCSMSGSP    CHAR(1),        /* RESERVED FOR MESSAGE SURPRESSION*/ 00279000
*   2 NCSPLUAD    CHAR(3),        /*U = 'UNIT ADDRESS'               */ 00280000
*   2 NCSCRFLG    CHAR(1),        /* MISCELLANEOUS C/R FLAGS         */ 00281000
*     3 NCSPLWRM    BIT(1),       /* COMMAND SPECIFIED WARM PARAM,   */ 00282000
*                                 /* 1 = YES                         */ 00283000
*     3 NCSPLCLD    BIT(1),       /* COMMAND SPECIFIED COLD PARAM,   */ 00284000
*                                 /* 1 = YES                         */ 00285000
*     3 NCSFMCLD    BIT(1),       /* 370X RESPONDED COLD TO ACTPU    */ 00286000
*                                 /* (ERP), 1 = YES                  */ 00287000
*     3 NCSFMERP    BIT(1),       /* 370X RESPONDED ERP TO ACTPU     */ 00288000
*                                 /* (ERP), 1 = YES                  */ 00289000
*     3 NCSCRDSE    BIT(1),       /* 0=C/R DATA SET DOES NOT EXIST   */ 00290000
*                                 /* OR IS NULL                      */ 00291000
*                                 /* 1=C/R DATA SET EXISTS           */ 00292000
*                                 /* AND IS NOT NULL                 */ 00293000
*     3 NCSOPRQU    BIT(1),       /* OPERATOR REQUESTS REIPL, 1 = YES*/ 00294000
*     3 NCSUSRNF    BIT(1),       /* EARLY WARNING PURGE HAS ALREADY */ 00295000
*                                 /* OCCURED, 1 = YES                */ 00296000
*     3 NCSSTCMD    BIT(1),       /* ACTIVATE IS A RESULT OF         */ 00297000
*                                 /* START VTAM COMMAND, 1 = YES     */ 00298000
*   2 NCSPLWRE    BIT(32),        /* WTOR ECB                        */ 00299000
*   2 NCSPLWRF    CHAR(72),       /* WTOR REPLY AREA                 */ 00300000
*   2 NCSPLRAD    PTR(31),        /* RDTE PTR FOR ALLOCATE/DEALLOCATE*/ 00301000
*     3 NCSLSCMD BIT(8),          /* LINE SCHED COMMAND TYPE         */ 00302000
*     3 NCSPLRCD BIT(8),          /* HEX VALUE FOR LINE SCHED PARAM  */ 00303000
*   2 NCSIOCID    CHAR(4),        /* CURRENT CID FOR DAF LOCK HELD   */ 00304000
*     3  NCSPLCID CHAR(4),        /* ALIAS FOR ABOVE                 */ 00305000
*        4  NCSPLSAF  CHAR(2),    /* SOURCE ADDRESS                  */ 00306000
*        4  NCSPLDAF  CHAR(2),    /* DESTINATION ADDRESS             */ 00307000
*   2 NCSPLWKA    PTR(31),        /* NCS WORK AREA ADDRESS           */ 00308000
*   2 NCSPLCVT    PTR(31),        /* POINTER TO VTAM CVT             */ 00309000
*   2 NCSPLRPH    PTR(31),        /* POINTER TO RPH                  */ 00310000
*   2 NCSPLECB    BIT(32),        /* ECB FOR VARY PGS                */ 00311000
*   2 NCSPLFSB    PTR(31),        /* ADDRESS OF USS-FSS FSB OR RU    */ 00312000
*   2 NCSPLRCR    PTR(31),        /* ADDRESS RDTE FOR CR             */ 00313000
*   2 NCSPLFL1    CHAR(1),        /* VARY FLAGS                      */ 00314000
*     3 NCSPL2IN  BIT(1),         /* SECOND ENTRY                    */ 00315000
*     3 NCSPLSOM  BIT(1),         /* SUPRESS OP. MESSAGE             */ 00316000
*     3 NCSPLEBN  BIT(1),         /* ENTRY FROM BNN SSCP             */ 00317000
*     3 NCSPLREQ  BIT(1),         /* NCSPL FROM RESTORE              */ 00318000
*     3 NCSPLNON  BIT(1),         /* DO NOT ADDR RDT SEGMENT         */ 00319000
*     3 NCSPLAPU  BIT(1),         /* ACTIVATE PHYSICAL DONE BY LOAD  */ 00320000
*     3 NCSPL2CN  BIT(1),         /* SECOND CONTACT REQUEST          */ 00321000
*     3 NCSPLPOS  BIT(1),         /* BNN POSTED                      */ 00322000
*   2 NCSPLFL2    CHAR(1),        /* VARY FLAGS                      */ 00323000
*     3 NCSPLVDP  BIT(1),         /* NCSPL ON VARDEF PAB             */ 00324000
*     3 NCSPLRSF  BIT(1),         /* NO SICK CLEAR BEFORE I/O        */ 00325000
*     3 NCSPLRIO  BIT(1),         /* I/O = RESTART I/O               */ 00326000
*     3 NCSOUFMC  BIT(1),         /* PURGE ONLY USER FMCB            */ 00327000
*     3 NCSPLDS   BIT(1),         /* 1=CIO SAVED RESPONSE DATA       */ 00328000
*     3 NCSREMPO  BIT(1),         /* REMOTE POWER OFF SPEC BY OPER   */ 00329000
*     3 NCSPLVIS  BIT(1),         /* VARY IMM START CALLER OF CVP    */ 00330000
*     3 NCSPLVIT  BIT(1),         /* VARY IMM TERM CALLER OF CVP     */ 00331000
*   2 NCSPLFL3    CHAR(1),        /* FLAG BYTE                       */ 00332000
*     3 NCSPLMOM  BIT(1),         /* ERP CAUSED BY FAILURE OF      0200 00333000
*                                    HIGHER NODE                     */ 00334000
*     3 NCSEPP    BIT(1),         /* 1=EP SUBCHANNEL PRESENT         */ 00335000
*     3 NCSPLANS  BIT(1),         /* ON FOR ACT IN ANSWER MODE       */ 00336000
