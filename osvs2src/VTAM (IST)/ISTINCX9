*RFY ISTNIB BASED(RPLARG);                                         0459 00117000
*NIBSYM=RPRNAME;                                                   0460 00118000
         LR    @06,@00                                             0460 00119000
         L     @05,NCSPLRAD(,CX9NCSPL)                             0460 00120000
         MVC   NIBSYM(8,@06),RPRNAME(@05)                          0460 00121000
*RFY ISTCONFT BASED(NCSPLCVT->ATCCONFT) ;                          0461 00122000
*RFY ISTACDEB BASED(ADDR(CONVVTAM) -> RAPACDEB);                   0462 00123000
*CX9INPUT=NCSPLRPH;                                                0463 00124000
         LR    CX9INPUT,@07                                        0463 00125000
*/*  TPQUE (CX9RPLPT,ACDSSPAB) NORM */                             0464 00126000
*                                                                  0464 00127000
*  DO;                                                             0464 00128000
*  RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;                  0465 00129000
*  GEN REFS(GPR01P) SETS(RPHWORK)                                  0466 00130000
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0466 00131000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00132000
*  GPR00P=0;                                                       0467 00133000
         SLR   GPR00P,GPR00P                                       0467 00134000
*  GPR15P=ADDR(ACDSSPAB); /*HOLD PAB PTR*/                         0468 00135000
         L     @07,NCSPLCVT(,CX9NCSPL)                             0468 00136000
         L     @07,ATCCONFT(,@07)                                  0468 00137000
         L     GPR15P,RAPACDEB+556(,@07)                           0468 00138000
         LA    GPR15P,ACDSSPAB(,GPR15P)                            0468 00139000
*  GPR04P=CX9RPLPT; /*WORK ELE PTR*/                               0469 00140000
         LR    GPR04P,CX9RPLPT                                     0469 00141000
*  GPR05P=GPR15P; /*PASS PAB PTR*/                                 0470 00142000
         LR    GPR05P,GPR15P                                       0470 00143000
*  CALL ATCLCPTR->ATCAP37->ISTAPC37;                               0471 00144000
         L     @07,ATCLCPTR                                        0471 00145000
         L     @15,ATCAP37(,@07)                                   0471 00146000
         BALR  @14,@15                                             0471 00147000
*  GEN(NOPR  0)NOSETS NOREFS;                                      0472 00148000
         NOPR  0                                                        00149000
*  RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;                0473 00150000
*  END; /**** END OF TPQUE GENERATION ****/                        0474 00151000
*/********************************************************************/ 00152000
*/* WAIT FOR CLSDST TO POST THE CLOSE DESTINATION PROCESSING COMPLETE*/ 00153000
*/********************************************************************/ 00154000
*/*  TPWAIT   */                                                   0475 00155000
*  DO; /* BEGIN TPWAIT GENERATION*/                                0475 00156000
*  RFY (GPR01P,GPR14P,GPR15P) RSTD;                                0476 00157000
*  RFY (GPR00P) RSTD;                                              0477 00158000
*  GEN (STM   GPR00P,GPR15P,RPHWORK(GPR01P))                       0478 00159000
*  REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);                       0478 00160000
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)                            00161000
*  RFY (GPR00P) UNRSTD;                                            0479 00162000
*  GPR15P=ADDR(LHAA0016);                                          0480 00163000
         LA    GPR15P,LHAA0016                                     0480 00164000
*  GPR14P=0;                                                       0481 00165000
         SLR   GPR14P,GPR14P                                       0481 00166000
*  GPR15P=GPR15P & '00FFFFFF'X;                                    0482 00167000
         N     GPR15P,@CF03537                                     0482 00168000
*  GPR15P=GPR15P | RPHWT1;                                         0483 00169000
         O     GPR15P,@CF01890                                     0483 00170000
*  CS (GPR14P,GPR15P,GPR01P->RPHRESMA);                            0484 00171000
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         0484 00172000
*  BC (7,LHAB0016);                                                0485 00173000
*                                                                  0485 00174000
         BC    7,LHAB0016                                          0485 00175000
*/* TPEXIT  NONE */                                                0486 00176000
*  DO;                                                             0486 00177000
*    RESPECIFY GPR15P RSTD;                                        0487 00178000
*                                                                  0487 00179000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)  */                         0488 00180000
*/* END OF ATBUILD */                                              0488 00181000
*                                                                  0488 00182000
*  DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */                   0488 00183000
*    GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/                   0489 00184000
         L     GPR15P,ATCLCPTR                                     0489 00185000
*  END; /*LISATCVT*/                                               0490 00186000
*    RESPECIFY GPR00F RSTD;                                        0491 00187000
*    GPR00F=0;     /*SET PARAMETER REGISTER TO 0*/                 0492 00188000
         SLR   GPR00F,GPR00F                                       0492 00189000
*    GPR15P=GPR15P->ATCAP31;                                       0493 00190000
         L     GPR15P,ATCAP31(,GPR15P)                             0493 00191000
*    DCL FMZL0021 BASED(GPR15P) LABEL VALUERANGE(*);               0494 00192000
*    CALL FMZL0021;                                                0495 00193000
         BALR  @14,@15                                             0495 00194000
*    RESPECIFY (GPR00F,GPR15P) UNRSTD;                             0496 00195000
*  END; /***** END OF TPEXIT  *****/                               0497 00196000
*LHAB0016:;                                                        0498 00197000
LHAB0016 DS    0H                                                  0499 00198000
*  GPR01P->RPHRESMA = 0;                                           0499 00199000
         SLR   @00,@00                                             0499 00200000
         ST    @00,RPHRESMA(,GPR01P)                               0499 00201000
*  RFY (GPR00P) RSTD;                                              0500 00202000
*LHAA0016:;                                                        0501 00203000
LHAA0016 DS    0H                                                  0502 00204000
*  GEN (LM    GPR00P,GPR15P,RPHWORK(GPR01P))                       0502 00205000
