*/* LOGICAL CHANNEL COMMAND WORD                                      * 00218000
*/* ADD A DECLARE                                      003 10/18/74   * 00219000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0507 00220000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 00221000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 00222000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 00223000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 00224000
*              4  * BIT(1),            /* RESERVED                    * 00225000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0507 00226000
*                                         ERASE OR READ CONT          * 00227000
*              4  LCCWCTL BIT(4),      /* CONTROL TYPE                * 00228000
*                 5  LCCWMODE BIT(2),  /* MODE TYPE                   * 00229000
*                 5  LCCWTYPE BIT(2),  /* OPERATION TYPE              * 00230000
*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00231000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00232000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00233000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00234000
*                                         RECORD                      * 00235000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00236000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00237000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00238000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00239000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00240000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00241000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00242000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00243000
*/* LOGICAL COMMAND CODES */                                       0508 00244000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00245000
*     LXEC     BIT(8) CONSTANT('4B'X), /* RESET AT END OF COMMAND     * 00246000
*     LXO      BIT(8) CONSTANT('5B'X), /* RESET ORDERLY               * 00247000
*     LCCWPRGE BIT(8) CONSTANT('6B'X), /* PURGE                       * 00248000
*     LWH      BIT(8) CONSTANT('0F'X), /* WRITE HEADER INDI. 1ST BLOCK* 00249000
*     LWRVI    BIT(8) CONSTANT('1F'X), /* WRITE RVI                   * 00250000
*     LWNA     BIT(8) CONSTANT('2F'X), /* WRITE NEGATIVE ACK          * 00251000
*     LWAA     BIT(8) CONSTANT('3F'X), /* WRITE ALTERNATE ACK         * 00252000
*     LWPLG    BIT(8) CONSTANT('5F'X), /* WRITE ACK LEADING GRAPHICS  * 00253000
*     LWNLG    BIT(8) CONSTANT('6F'X), /* WRITE NACK LEADING GRAPHICS * 00254000
*     LSMDE    BIT(8) CONSTANT('4F'X), /* SET MODE                    * 00255000
*     LIFBM    BIT(8) CONSTANT('7F'X), /* INDIC 1ST BLOCK OF MESSAGE  * 00256000
*     LTEST    BIT(8) CONSTANT('FE'X), /* TEST LCCW                   * 00257000
*     LPRMPT   BIT(8) CONSTANT('13'X), /* PREEMPT LCCW                * 00258000
*     LRSTR    BIT(8) CONSTANT('43'X), /* RESTORE LCCW                * 00259000
*     LEXEC    BIT(8) CONSTANT('FF'X), /* SPECIAL CONTROL OPERATION   * 00260000
*     LREAD    BIT(2) CONSTANT('10'B),  /* READ TYPE LCCW             * 00261000
*     LWRITE   BIT(2) CONSTANT('01'B),  /* WRITE TYPE LCCW            * 00262000
*     LCNTRL   BIT(2) CONSTANT('11'B),  /* CONTROL TYPE LCCW          * 00263000
*     LRESET   BIT(4) CONSTANT('1011'B),/* RESET COMMANDS             * 00264000
*     LTIC     BIT(8) CONSTANT('08'X),  /* LOGICAL TIC                * 00265000
*     LWR      BIT(6) CONSTANT('000001'B),  /* WRITE CONVERSATIONAL   * 00266000
*     LWB      BIT(6) CONSTANT('000101'B),  /* WRITE BLOCK            * 00267000
*     LWM      BIT(6) CONSTANT('001001'B),  /* WRITE MESSAGE          * 00268000
*     LWT      BIT(6) CONSTANT('001101'B),  /* WRITE TRANSMISSION     * 00269000
*     LEWM     BIT(6) CONSTANT('011001'B),  /* ERASE/WRITE MESSAGE    * 00270000
*     LEWT     BIT(6) CONSTANT('011101'B),  /* ERASE/WRITE TRNSMSSN   * 00271000
*     LEAU     BIT(6) CONSTANT('010001'B),  /* ERASE ALL UNPROTECTED  * 00272000
*     LRB      BIT(6) CONSTANT('000110'B),  /* READ BLOCK             * 00273000
*     LRM      BIT(6) CONSTANT('001010'B),  /* READ MESSAGE           * 00274000
*     LRT      BIT(6) CONSTANT('001110'B),  /* READ TRANSMISSION      * 00275000
*     LRMOD    BIT(6) CONSTANT('010010'B),  /* READ MODIFIED          * 00276000
*     LRBUF    BIT(6) CONSTANT('010110'B),  /* READ BUFFER            * 00277000
*     LRCONT   BIT(6) CONSTANT('011010'B),  /* READ CONTINUOUS        * 00278000
*     LRPEP    BIT(6) CONSTANT('011110'B),  /* READ PERPETUAL         * 00279000
*     LCPYM    BIT(6) CONSTANT('101001'B),  /* COPY MESSAGE           * 00280000
*     LCPYT    BIT(6) CONSTANT('101101'B),  /* COPY TRANSMISSION      * 00281000
*     LD       BIT(8) CONSTANT('23'X),  /* DISCONNECT                 * 00282000
*     LDEC     BIT(8) CONSTANT('33'X),  /* DISCONNECT WITH END OF CALL* 00283000
*     LEOC     BIT(8) CONSTANT('53'X),  /* END OF CALL COMMAND        * 00284000
*     LXEL     BIT(8) CONSTANT('0B'X),  /* RESET ERROR LOCK           * 00285000
*     LXC      BIT(8) CONSTANT('1B'X),  /* RESET CONDITIONAL          * 00286000
*     LXI      BIT(8) CONSTANT('2B'X),  /* RESET IMMEDIATE            * 00287000
*     LC       BIT(8) CONSTANT('03'X),  /* CONTACT                    * 00288000
*     LWPA     BIT(8) CONSTANT('FD'X); /* WRITE POSITIVE ACK          * 00289000
*DCL  LMT      BIT(2) CONSTANT('11'B), /* TRANSMISSION MODE           * 00290000
*     LMS      BIT(2) CONSTANT('00'B), /* SPECIAL MODE LIKE CONV, EAU * 00291000
*     LMB      BIT(2) CONSTANT('01'B), /* BLOCK MODE                  * 00292000
*     LMM      BIT(2) CONSTANT('10'B); /* MSG OR CONTINUOUS MODE      * 00293000
*/* FOLLOWING EQUATES FOR  - PORT SOLICITOR (DIAL)                    * 00294000
*/* PORT SOLICITOR COMMAND CODES                                      * 00295000
*DCL PSOL    BIT(8) CONSTANT('FE'X), /* SPECIAL PORT SOLICITOR     0510 00296000
*                                       COMMAND                       * 00297000
*    STCTL    BIT(8) CONSTANT('01'X), /* START CONTROL                * 00298000
*    RESTCTL  BIT(8) CONSTANT('02'X), /* RESET & CONTROL - REDRIVE    * 00299000
*    STIPUT   BIT(8) CONSTANT('03'X); /* START INPUT                  * 00300000
*/*   FOLLOWING EQUATES FOR SDLC SUPPORT                              * 00301000
*DCL  LCCHOLD  BIT(8) CONSTANT('01'X),  /* HOLD                       * 00302000
*     LCCRHOLD BIT(8) CONSTANT('02'X),  /* RELEASE HOLD               * 00303000
*     LSEND    BIT(8) CONSTANT ('F1'X), /* SEND                       * 00304000
*     LCLEAR   BIT(8) CONSTANT ('F2'X), /* CLEAR                      * 00305000
*     LCRUD    BIT(8) CONSTANT ('F3'X), /* CLEAR, UNBIND              * 00306000
*     LCRUDNFY BIT(8) CONSTANT ('F4'X), /* CLEAR, UNBIND, NOTIFY      * 00307000
*     LNFY     BIT(8) CONSTANT ('F5'X), /* NOTIFY                     * 00308000
*     LRELCB   BIT(8) CONSTANT ('F6'X), /* RELEASE CB                 * 00309000
*     LRESTSR  BIT(8) CONSTANT ('F7'X), /* RESETSR                    * 00310000
*     LCLRNFY  BIT(8) CONSTANT('F8'X),  /* CLEAR/NOTIFY               * 00311000
*     LCHPUR   BIT(8) CONSTANT('F9'X);  /* PURGE                      * 00312000
*DCL  LCCWSIZE FIXED(8) CONSTANT(8);    /* LCCW SIZE                  * 00313000
*DCL  LRDS     BIT(8) CONSTANT('FF'X);  /* RECORD DEV STATS COMMAND   * 00314000
*DCL  LQUIS    BIT(8) CONSTANT('FE'X);  /* QUIESCE COMMAND            * 00315000
*/*              LOGICAL CHANNEL PROGRAM BLOCK                       */ 00316000
*/*                                                                  */ 00317000
*/*  REVISIONS                                         006 03/24/75  */ 00318000
*/*                                                                  */ 00319000
*DCL  1  ISTLCPB BDY(WORD) BASED(LCPBPTR),                         0515 00320000
*        2 LCPTYPE CHAR(1),            /* TYPE CODE                  */ 00321000
*        2 LCPLNGTH  PTR(8),           /* LENGTH IN BYTES            */ 00322000
*        2 LCPREQTG CHAR(2),           /* LCPB REQ. TAG FIELD        */ 00323000
*          3 LCPFLAGS BIT(8),          /* FLAG BYTE                  */ 00324000
*            4 LCPZRT00 BIT(1),        /* LOGICAL ERROR FLAG         */ 00325000
*            4 LCPZRT01 BIT(1),        /* CONTROL BLOCK ERROR        */ 00326000
*            4 LCPZF001 BIT(1),        /* LCP START PROCESS FLAG     */ 00327000
*              5 LCPZRT02 BIT(1),      /* I/O INITIATION DISABLED    */ 00328000
*                6 LCPZRTL2 BIT(1),    /* END OF SCHEDULABLE BLOCK   */ 00329000
*            4 LCPZF002 BIT(1),        /* LCP IN PROCESSING FLAG     */ 00330000
