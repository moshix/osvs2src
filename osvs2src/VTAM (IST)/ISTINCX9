SVTM     TITLE ' SSCP -- ISTINCX9 -- CLSDST HANDLER                    *00001000
                        '                                               00002000
ISTINCX9 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINCX9  75.129'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
*/*-----------------------------------------------------------------*/  00015000
*/* HOUSE HEEPING                                                   */  00016000
*/*-----------------------------------------------------------------*/  00017000
*                                                                  0421 00018000
*CX9NCSPL=CX9INPUT;                /* COPY INPUT-NCSPL */          0421 00019000
         LR    CX9NCSPL,CX9INPUT                                   0421 00020000
* DO;                                                              0422 00021000
*/***  CRRMAINT  PROCNM(ISTINCX9)COMP(VY)FUNC(SUB)RPHADD((NCSPLRPH))  * 00022000
**/                                                                0423 00023000
*                                                                  0423 00024000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTVYCRR)  */                  0423 00025000
* RFY ISTLCCW ;                                                    0423 00026000
* RFY ISTLCPB ;                                                    0424 00027000
* RFY ISTZSAVE;                                                    0425 00028000
* RFY ISTPFCRR;                                                    0426 00029000
* RFY ISTCFCRR;                                                    0427 00030000
* RFY ISTZCRR ;                                                    0428 00031000
* RFY ISTASCRR;                                                    0429 00032000
* RFY ISTAPCRR;                                                    0430 00033000
* RFY ISTCRA  ;                                                    0431 00034000
* RFY ISTCNSTA;                                                    0432 00035000
* RFY ISTVYCRR;                                                    0433 00036000
*/* END OF ATBUILD */                                              0434 00037000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0434 00038000
*NCSPLRPH->RPHCRR->CRACRR->NINCX9 = '1'B;                          0434 00039000
         L     @07,NCSPLRPH(,CX9NCSPL)                             0434 00040000
         L     @06,RPHCRR(,@07)                                    0434 00041000
         L     @06,CRACRR(,@06)                                    0434 00042000
         OI    NINCX9(@06),B'00000010'                             0434 00043000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0435 00044000
*NCSPLRPH->RPHCRR->CRACRR->VYCCRT = '0'B;                          0435 00045000
         XC    VYCCRT+1(11,@06),VYCCRT+1(@06)                      0435 00046000
         MVI   VYCCRT(@06),B'00000000'                             0435 00047000
*NCSPLRPH->RPHCRR->CRACRR->CINCX9 = '1'B;                          0436 00048000
         OI    CINCX9(@06),B'00000010'                             0436 00049000
* END;                                                             0437 00050000
*CX9WORKR=CX9SAVE;                                                 0438 00051000
         LR    CX9WORKR,CX9SAVE                                    0438 00052000
*CX9SAVE=CX9SAVE-LENGTH(CX9SVMAP); /* OUR SAVE ADDR */             0439 00053000
         SL    CX9SAVE,@CF01429                                    0439 00054000
*CX9WORKR->CX9FWPTR=CX9SAVE;       /* OUR SAVE ADDR IN CALLER SAVE */   00055000
         ST    CX9SAVE,CX9FWPTR(,CX9WORKR)                         0440 00056000
*CX9SAVE->CX9BKPTR=CX9WORKR;       /* CALLER SAVE IN OUR SAVEAREA */    00057000
         ST    CX9WORKR,CX9BKPTR(,CX9SAVE)                         0441 00058000
*CX9SAVE->CX9FWPTR=0;                                              0442 00059000
         SLR   @00,@00                                             0442 00060000
         ST    @00,CX9FWPTR(,CX9SAVE)                              0442 00061000
*CX9WKA=NCSPLWKA->CX9NXTAV;        /* ESTABLISH OUR WORK AREA BASE */   00062000
         L     @06,NCSPLWKA(,CX9NCSPL)                             0443 00063000
         L     CX9WKA,CX9NXTAV(,@06)                               0443 00064000
*CX9WKA=CX9WKA+(8-(CX9WKA//8));                                    0444 00065000
         LR    @02,CX9WKA                                          0444 00066000
         SRDA  @02,32                                              0444 00067000
         D     @02,@CF00092                                        0444 00068000
         LCR   @02,@02                                             0444 00069000
         AL    @02,@CF00092                                        0444 00070000
         ALR   CX9WKA,@02                                          0444 00071000
*/*------------------------------------------------------------------*/ 00072000
*/* THE FOLLOWING CREATES THE RPL AND NIB REQUIRED BY CLSDST,TPQUES  */ 00073000
*/* THE RPL TO THE SYSTEM SERVICE PAB IN THE VTAM ACDEB, TPWITS FOR  */ 00074000
*/* CLSDST TO COMPLETE, AND THEN FEEDS BACK TO ITS CALLER CLSDST'S   */ 00075000
*/* MAJOR RETURN CODE.                                               */ 00076000
*/*------------------------------------------------------------------*/ 00077000
*CX9RPLPT = ADDR(CX9RPL);                                          0445 00078000
         LR    CX9RPLPT,CX9WKA                                     0445 00079000
*RFY IFGRPL BASED(ADDR(CX9RPLS));                                  0446 00080000
*CX9RPL(1:RPLLEN) = CX9RPLS(1:RPLLEN);                             0447 00081000
         LA    @06,CX9RPLS                                         0447 00082000
         SLR   @05,@05                                             0447 00083000
         IC    @05,RPLLEN(,@06)                                    0447 00084000
         BCTR  @05,0                                               0447 00085000
         EX    @05,@SM04678                                        0447 00086000
*RFY IFGRPL BASED(CX9RPLPT);                                       0448 00087000
*RPLDACB=NCSPLCVT->ATCCONFT->CONACB;                               0449 00088000
         L     @06,NCSPLCVT(,CX9NCSPL)                             0449 00089000
         L     @06,ATCCONFT(,@06)                                  0449 00090000
         L     @00,CONACB(,@06)                                    0449 00091000
         ST    @00,RPLDACB(,CX9RPLPT)                              0449 00092000
*RPLNIB='1'B;                                                      0450 00093000
         OI    RPLNIB(CX9RPLPT),B'00000100'                        0450 00094000
*RPLREQ='1F'X;                                                     0451 00095000
         MVI   RPLREQ(CX9RPLPT),X'1F'                              0451 00096000
*RPLTPOST='1'B;                                                    0452 00097000
         OI    RPLTPOST(CX9RPLPT),B'00001000'                      0452 00098000
*RPLVTUSE='1'B;                                                    0453 00099000
         OI    RPLVTUSE+16(CX9RPLPT),B'01000000'                   0453 00100000
*RPLECB=NCSPLRPH;                                                  0454 00101000
         ST    @07,RPLECB(,CX9RPLPT)                               0454 00102000
*/********************************************************************/ 00103000
*/* SET UP NIB FOLLOWING RPL IN THE WORK AREA.                       */ 00104000
*/********************************************************************/ 00105000
*RPLARG=ADDR(CX9NIB);                                              0455 00106000
         LA    @00,CX9NIB(,CX9WKA)                                 0455 00107000
