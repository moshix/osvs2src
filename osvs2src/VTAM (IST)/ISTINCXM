SVTM     TITLE 'ISTINCXM - 370X DUMP REMOTE I/O ROUTINE                *00001000
                        '                                               00002000
         ISEQ  73,80                                                    00003000
ISTINCXM CSECT ,                                                   0002 00004000
@MAINENT DS    0H                                                  0002 00005000
         USING *,@15                                               0002 00006000
         B     @PROLOG                                             0002 00007000
         DC    AL1(16)                                             0002 00008000
         DC    C'ISTINCXM  77.311'                                 0002 00009000
         DROP  @15                                                      00010000
@PROLOG  STM   @14,@12,12(@13)                                     0002 00011000
         BALR  @12,0                                               0002 00012000
@PSTART  DS    0H                                                  0002 00013000
         USING @PSTART,@12                                         0002 00014000
*   CXMNCSPL=GPR00P;                  /* SAVE INPUT NCSPL PTR        */ 00015000
         LR    CXMNCSPL,GPR00P                                     0198 00016000
*   RFY                                                            0199 00017000
*     GPR00P UNRSTD;                                               0199 00018000
*   CXMNCSAP=NCSPLAPP;                /* GET NCSPL APPNDG PTR        */ 00019000
         L     CXMNCSAP,NCSPLAPP(,CXMNCSPL)                        0200 00020000
*   CXMSVBCK=CXMSVPTR;                /* SAVE CALLERS R13            */ 00021000
         ST    CXMSVPTR,CXMSVBCK(,CXMNCSAP)                        0201 00022000
*   CXMSAPTR=ADDR(NCSAPSA);           /* GET SAVE AREA PTR           */ 00023000
*                                                                  0202 00024000
         LR    CXMSAPTR,CXMNCSAP                                   0202 00025000
*   IF CXMBFPTR=TRMNATN               /* IF TERMINATION              */ 00026000
*     THEN                                                         0203 00027000
         LTR   CXMBFPTR,CXMBFPTR                                   0203 00028000
         BNZ   @RF00203                                            0203 00029000
*DVLDTERM:                                                         0204 00030000
*/********************************************************************/ 00031000
*/*                                                                  */ 00032000
*/* SEGMENT-NAME      = DVLDTERM                                     */ 00033000
*/*                                                                  */ 00034000
*/* SEGMENT-TYPE      = BLOCK                                        */ 00035000
*/*                                                                  */ 00036000
*/* SEGMENT-FUNCTION  = TO DETERMINE WHETHER OR NOT A DUMP           */ 00037000
*/*                     FINAL SIGNAL NEEDS TO BE SENT TO             */ 00038000
*/*                     THE REMOTE 370X AND, IF SO, TO               */ 00039000
*/*                     SEND IT                                      */ 00040000
*/*                                                                  */ 00041000
*/* SEGMENT-INVOKERS  = ISTINCXM                                     */ 00042000
*/*                                                                  */ 00043000
*/* SEGMENTS-INCLUDED = CXMVWAIT                                     */ 00044000
*/*                                                                  */ 00045000
*/* SEGMENTS-CALLED   = DVLDEXCP                                     */ 00046000
*/*                                                                  */ 00047000
*/* ROUTINES-CALLED   = NONE                                         */ 00048000
*/*                                                                  */ 00049000
*/* MACROS-INVOKED    = VWAIT IN CXMVWAIT                            */ 00050000
*/*                                                                  */ 00051000
*/********************************************************************/ 00052000
*       DO;                                                        0204 00053000
DVLDTERM DS    0H                                                  0205 00054000
*         IF RIBDAD^=DMPINPRG         /* IF DUMP IN PROGRESS         */ 00055000
*           THEN                                                   0205 00056000
         L     @15,RIBDAD+76(,CXMNCSAP)                            0205 00057000
         LTR   @15,@15                                             0205 00058000
         BZ    @RF00205                                            0205 00059000
*           DO;                                                    0206 00060000
*             IF RIBDAD<NCSAPEND      /* IF STILL DUMPING            */ 00061000
*               THEN                                               0207 00062000
         C     @15,NCSAPEND(,CXMNCSAP)                             0207 00063000
         BNL   @RF00207                                            0207 00064000
*               DO;                                                0208 00065000
*                 NCSPLVBF=NCSDF;     /* SET DUMP FINAL OPCD         */ 00066000
         MVC   NCSPLVBF(2,CXMNCSPL),@CB00977                       0209 00067000
*                 RFY                                              0210 00068000
*                   CXMRTPTR UNRSTD;                               0210 00069000
*                 CALL DVLDEXCP;      /* INIT I/O OP TO 370X         */ 00070000
         BAL   @14,DVLDEXCP                                        0211 00071000
*                 RFY                                              0212 00072000
*                   CXMRTPTR RSTD;                                 0212 00073000
*               END;                                               0213 00074000
*               ELSE;                 /* DUMP COMPLETE               */ 00075000
@RF00207 DS    0H                                                  0215 00076000
*/********************************************************************/ 00077000
*/*                                                                  */ 00078000
*/* SEGMENT-NAME      = CXMVWAIT                                     */ 00079000
*/*                                                                  */ 00080000
*/* SEGMENT-TYPE      = BLOCK                                        */ 00081000
*/*                                                                  */ 00082000
*/* SEGMENT-FUNCTION  = TO ISSUE THE VWAIT MACRO TO WAIT             */ 00083000
*/*                     FOR THE LAST I/O OPERATION TO                */ 00084000
*/*                     COMPLETE                                     */ 00085000
*/*                                                                  */ 00086000
*/* SEGMENT-INVOKERS  = ISTINCXM                                     */ 00087000
*/*                     DVLDTERM                                     */ 00088000
*/*                     DVLDCHEK                                     */ 00089000
*/*                                                                  */ 00090000
*/* SEGMENTS-INCLUDED = NONE                                         */ 00091000
*/*                                                                  */ 00092000
*/* SEGMENTS-CALLED   = NONE                                         */ 00093000
*/*                                                                  */ 00094000
*/* ROUTINES-CALLED   = NONE                                         */ 00095000
*/*                                                                  */ 00096000
*/* MACROS-INVOKED    = VWAIT TO WAIT FOR THE LAST I/O               */ 00097000
*/*                     OPERATION INITIATED TO COMPLETE              */ 00098000
*/*                                                                  */ 00099000
*/********************************************************************/ 00100000
*          DO;                                                     0215 00101000
*      CXMSVBRP=CXMBFPTR;                                 /*    9530 */ 00102000
         ST    CXMBFPTR,CXMSVBRP(,CXMNCSAP)                        0216 00103000
*/*VWAIT  ECB(NCSAPECB)TASK(VTAM)*/                                0217 00104000
*             DO;                                                  0217 00105000
*                                        /*DECLARE AND RESTRICT REG 1*/ 00106000
*               RESPECIFY GPR01F RSTD;   /*INVOKE XREGSET MACRO TO*/    00107000
*                                        /*TEST ECB PARM VALIDITY */    00108000
*                                                                  0219 00109000
*/*XREGSET  PPARM(NCSAPECB)RREG(1,GPR01F)PID('ECB')*/              0219 00110000
*             DO;                                                  0219 00111000
*               GPR01F=ADDR(NCSAPECB);   /*SET RREG SYMBOL TO ADDRESS*/ 00112000
         LA    GPR01F,NCSAPECB(,CXMNCSAP)                          0220 00113000
*                                        /*OF PPARM OPERAND          */ 00114000
*             END;  /* XREGSET */                                  0221 00115000
*               GEN;                                               0222 00116000
         WAIT 1,ECB=(1)                                                 00117000
*               RESPECIFY GPR01F UNRSTD; /*UNRESTRICT REG 1          */ 00118000
*             END;  /* VWAIT */    /* WAIT             */          0224 00119000
*      CXMBFPTR=CXMSVBRP;                                 /*@xm9530k*/  00120000
         L     CXMBFPTR,CXMSVBRP(,CXMNCSAP)                        0225 00121000
*          END;                                                    0226 00122000
*           END;                                                   0227 00123000
*           ELSE;                                                  0228 00124000
@RF00205 DS    0H                                                  0229 00125000
*         CXMSVRTV=ENDOFILE;          /* SET RETURN CODE             */ 00126000
         MVC   CXMSVRTV(4,CXMNCSAP),@CF00069                       0229 00127000
*       END;                                                       0230 00128000
*     ELSE                            /* NOT TRMNATN, SO:            */ 00129000
*       IF RIBDAD=FRSTCALL            /* IF FIRST CALL               */ 00130000
*         THEN                                                     0231 00131000
         B     @RC00203                                            0231 00132000
@RF00203 ICM   @15,15,RIBDAD+76(CXMNCSAP)                          0231 00133000
         BNZ   @RF00231                                            0231 00134000
*DVLDINIT:                                                         0232 00135000
*/********************************************************************/ 00136000
*/*                                                                  */ 00137000
*/* SEGMENT-NAME      = DVLDINIT                                     */ 00138000
*/*                                                                  */ 00139000
*/* SEGMENT-TYPE      = BLOCK                                        */ 00140000
*/*                                                                  */ 00141000
*/* SEGMENT-FUNCTION  = TO INITIALIZE THE REMOTE 370X AND            */ 00142000
*/*                     THE NETWORK CONFIGURATION SERVICES           */ 00143000
*/*                     PARAMETER LIST (NCSPL) FOR A 370X            */ 00144000
*/*                     DUMP OPERATION                               */ 00145000
*/*                                                                  */ 00146000
*/* SEGMENT-INVOKERS  = ISTINCXM                                     */ 00147000
*/*                                                                  */ 00148000
*/* SEGMENTS-INCLUDED = DVLDISSZ                                     */ 00149000
*/*                                                                  */ 00150000
*/* SEGMENTS-CALLED   = DVLDEXCP                                     */ 00151000
*/*                     DVLDCHEK                                     */ 00152000
*/*                                                                  */ 00153000
*/* ROUTINES-CALLED   = NONE                                         */ 00154000
*/*                                                                  */ 00155000
*/* MACROS-INVOKED    = NONE                                         */ 00156000
*/*                                                                  */ 00157000
*/********************************************************************/ 00158000
*           DO;                                                    0232 00159000
DVLDINIT DS    0H                                                  0233 00160000
*             NCSPLVCD=NCSPLVC6;      /* SET REMOTE LOAD/DUMP      0233 00161000
*                                        I/O VERB CODE               */ 00162000
         MVI   NCSPLVCD(CXMNCSPL),X'06'                            0233 00163000
*             NCSPLVBF=NCSDI;         /* SET DUMP INITL OPCD         */ 00164000
         MVC   NCSPLVBF(2,CXMNCSPL),@CB00963                       0234 00165000
*             RFY                                                  0235 00166000
*               CXMRTPTR UNRSTD;                                   0235 00167000
*             CALL DVLDEXCP;          /* INIT I/O OP TO 370X         */ 00168000
         BAL   @14,DVLDEXCP                                        0236 00169000
*             CALL DVLDCHEK;          /* WAIT ON LAST I/O          0237 00170000
*                                        OPERATION AND             0237 00171000
*                                        TEST SUCCESS                */ 00172000
         BAL   @14,DVLDCHEK                                        0237 00173000
*             RFY                                                  0238 00174000
*               CXMRTPTR RSTD;                                     0238 00175000
*             IF CXMSVRTV=SUCCESS     /* IF SUCCESS RETURN           */ 00176000
*               THEN                                               0239 00177000
         ICM   @15,15,CXMSVRTV(CXMNCSAP)                           0239 00178000
         BNZ   @RF00239                                            0239 00179000
*DVLDISSZ:                                                         0240 00180000
*/********************************************************************/ 00181000
*/*                                                                  */ 00182000
*/* SEGMENT-NAME      = DVLDISSZ                                     */ 00183000
*/*                                                                  */ 00184000
*/* SEGMENT-TYPE      = BLOCK                                        */ 00185000
*/*                                                                  */ 00186000
*/* SEGMENT-FUNCTION  = TO PROCESS THE FIRST RECORD OF A             */ 00187000
*/*                     370X DUMP, DETERMINING AND SAVING            */ 00188000
*/*                     THE STORAGE SIZE OF THE 370X                 */ 00189000
*/*                                                                  */ 00190000
*/* SEGMENT-INVOKERS  = DVLDINIT                                     */ 00191000
*/*                                                                  */ 00192000
*/* SEGMENTS-INCLUDED = NONE                                         */ 00193000
*/*                                                                  */ 00194000
*/* SEGMENTS-CALLED   = NONE                                         */ 00195000
*/*                                                                  */ 00196000
*/* ROUTINES-CALLED   = NONE                                         */ 00197000
*/*                                                                  */ 00198000
*/* MACROS-INVOKED    = NONE                                         */ 00199000
*/*                                                                  */ 00200000
*/********************************************************************/ 00201000
*                 DO;                                              0240 00202000
DVLDISSZ DS    0H                                                  0241 00203000
*                   IF RIBHAD=MACH64K /* IF 64K MACHINE              */ 00204000
*                     THEN                                         0241 00205000
         LA    @15,96                                              0241 00206000
         ALR   @15,CXMNCSAP                                        0241 00207000
         ICM   @08,15,RIBHAD(@15)                                  0241 00208000
         BNZ   @RF00241                                            0241 00209000
*                       RIBHAD=SIXTY4K; /* SET HIGH ADDR 64K         */ 00210000
         MVC   RIBHAD(4,@15),@CF01256                              0242 00211000
*                     ELSE            /* NOT 64K MACHINE, SO:        */ 00212000
*                       IF RIBHAD=FOURTY8K /* IF 48K MACH            */ 00213000
*                         THEN                                     0243 00214000
         B     @RC00241                                            0243 00215000
@RF00241 CLC   RIBHAD+96(4,CXMNCSAP),@CF01254                      0243 00216000
         BNE   @RF00243                                            0243 00217000
*                         DO;                                      0244 00218000
*                           /*************************************/     00219000
*                           /* ZERO THE GARBAGE BITS IN THE 370X */     00220000
*                           /* GROUP 0 REGISTERS                 */     00221000
*                           /*************************************/     00222000
*                           RFY                                    0245 00223000
*                             (CXMNCSDT                            0245 00224000
*                             ,CXMLPCTR) RSTD;                     0245 00225000
*                           CXMNCSDT=ADDR(NCSAPDAT)+C370XRST;      0246 00226000
         LA    CXMNCSDT,NCSAPDAT(,CXMNCSAP)                        0246 00227000
         AH    CXMNCSDT,@CH00853                                   0246 00228000
*                           DO CXMLPCTR=CNSTNT1 TO CT370XRG;       0247 00229000
         LA    CXMLPCTR,1                                          0247 00230000
@DL00247 DS    0H                                                  0248 00231000
*                              CXMGBITS=CNSTNT0;                   0248 00232000
         MVI   CXMGBITS(CXMNCSDT),X'00'                            0248 00233000
*                              CXMNCSDT=CXMNCSDT+LENGTH(CXMGRBRG); 0249 00234000
         AH    CXMNCSDT,@CH00069                                   0249 00235000
*                           END;                                   0250 00236000
         AH    CXMLPCTR,@CH00077                                   0250 00237000
         CH    CXMLPCTR,@CH00075                                   0250 00238000
         BNH   @DL00247                                            0250 00239000
*                           RFY                                    0251 00240000
*                             (CXMNCSDT                            0251 00241000
*                             ,CXMLPCTR) UNRSTD;                   0251 00242000
*                         END;                                     0252 00243000
*                         ELSE;                                    0253 00244000
@RF00243 DS    0H                                                  0254 00245000
*                   NCSAPEND=RIBHAD;  /* SET END = HIGH ADDR         */ 00246000
@RC00241 MVC   NCSAPEND(4,CXMNCSAP),RIBHAD+96(CXMNCSAP)            0254 00247000
*                   NCSPLVBF=NCSDT;   /* SET DUMP TEXT OPCD          */ 00248000
         MVC   NCSPLVBF(2,CXMNCSPL),@CB00965                       0255 00249000
*                 END;                                             0256 00250000
*               ELSE;                                              0257 00251000
@RF00239 DS    0H                                                  0258 00252000
*           END;                                                   0258 00253000
*         ELSE                        /* NOT FIRST CALL, SO:         */ 00254000
*           IF RIBDAD>=NCSAPEND       /* IF END OF FILE              */ 00255000
*             THEN                                                 0259 00256000
         B     @RC00231                                            0259 00257000
@RF00231 CLC   RIBDAD+76(4,CXMNCSAP),NCSAPEND(CXMNCSAP)            0259 00258000
         BL    @RF00259                                            0259 00259000
*             DO;                                                  0260 00260000
*/********************************************************************/ 00261000
*/*                                                                  */ 00262000
*/* SEGMENT-NAME      = CXMVWAIT                                     */ 00263000
*/*                                                                  */ 00264000
*/* SEGMENT-TYPE      = BLOCK                                        */ 00265000
*/*                                                                  */ 00266000
*/* SEGMENT-FUNCTION  = TO ISSUE THE VWAIT MACRO TO WAIT             */ 00267000
*/*                     FOR THE LAST I/O OPERATION TO                */ 00268000
*/*                     COMPLETE                                     */ 00269000
*/*                                                                  */ 00270000
*/* SEGMENT-INVOKERS  = ISTINCXM                                     */ 00271000
*/*                     DVLDTERM                                     */ 00272000
*/*                     DVLDCHEK                                     */ 00273000
*/*                                                                  */ 00274000
*/* SEGMENTS-INCLUDED = NONE                                         */ 00275000
*/*                                                                  */ 00276000
*/* SEGMENTS-CALLED   = NONE                                         */ 00277000
*/*                                                                  */ 00278000
*/* ROUTINES-CALLED   = NONE                                         */ 00279000
*/*                                                                  */ 00280000
*/* MACROS-INVOKED    = VWAIT TO WAIT FOR THE LAST I/O               */ 00281000
*/*                     OPERATION INITIATED TO COMPLETE              */ 00282000
*/*                                                                  */ 00283000
*/********************************************************************/ 00284000
*          DO;                                                     0261 00285000
*      CXMSVBRP=CXMBFPTR;                                 /*    9530 */ 00286000
*                                                                  0262 00287000
         ST    CXMBFPTR,CXMSVBRP(,CXMNCSAP)                        0262 00288000
*/*VWAIT  ECB(NCSAPECB)TASK(VTAM)*/                                0263 00289000
*             DO;                                                  0263 00290000
*                                        /*DECLARE AND RESTRICT REG 1*/ 00291000
*               RESPECIFY GPR01F RSTD;   /*INVOKE XREGSET MACRO TO*/    00292000
*                                        /*TEST ECB PARM VALIDITY */    00293000
*                                                                  0265 00294000
*/*XREGSET  PPARM(NCSAPECB)RREG(1,GPR01F)PID('ECB')*/              0265 00295000
*             DO;                                                  0265 00296000
*               GPR01F=ADDR(NCSAPECB);   /*SET RREG SYMBOL TO ADDRESS*/ 00297000
         LA    GPR01F,NCSAPECB(,CXMNCSAP)                          0266 00298000
*                                        /*OF PPARM OPERAND          */ 00299000
*             END;  /* XREGSET */                                  0267 00300000
*               GEN;                                               0268 00301000
         WAIT 1,ECB=(1)                                                 00302000
*               RESPECIFY GPR01F UNRSTD; /*UNRESTRICT REG 1          */ 00303000
*             END;  /* VWAIT */    /* WAIT             */          0270 00304000
*      CXMBFPTR=CXMSVBRP;                                 /*@xm9530k*/  00305000
         L     CXMBFPTR,CXMSVBRP(,CXMNCSAP)                        0271 00306000
*          END;                                                    0272 00307000
*               CXMSVRTV=ENDOFILE;    /* SET RETURN CODE             */ 00308000
         MVC   CXMSVRTV(4,CXMNCSAP),@CF00069                       0273 00309000
*             END;                                                 0274 00310000
*             ELSE                    /* NOT END OF FILE, SO:        */ 00311000
*             DO;                                                  0275 00312000
         B     @RC00259                                            0275 00313000
@RF00259 DS    0H                                                  0276 00314000
*               RFY                                                0276 00315000
*                 CXMRTPTR UNRSTD;                                 0276 00316000
*               CALL DVLDCHEK;        /* WAIT ON LAST I/O          0277 00317000
*                                        OPERATION AND             0277 00318000
*                                        TEST SUCCESS                */ 00319000
         BAL   @14,DVLDCHEK                                        0277 00320000
*               RFY                                                0278 00321000
*                 CXMRTPTR RSTD;                                   0278 00322000
*             END;                                                 0279 00323000
*   IF CXMSVRTV=SUCCESS               /* IF SUCCESS TO NOW           */ 00324000
*     THEN                                                         0280 00325000
@RC00259 DS    0H                                                  0280 00326000
@RC00231 DS    0H                                                  0280 00327000
@RC00203 ICM   @15,15,CXMSVRTV(CXMNCSAP)                           0280 00328000
         BNZ   @RF00280                                            0280 00329000
*     DO;                                                          0281 00330000
*       RFY                                                        0282 00331000
*         (CXMXBFRP                                                0282 00332000
*         ,CXMXBFRL                                                0282 00333000
*         ,CXMNCSDT                                                0282 00334000
*         ,CXMNCSDL) RSTD;                                         0282 00335000
*       CXMXBFRP=ADDR(CXMXBUFR);      /* SET MVCL TO PTR             */ 00336000
         LR    CXMXBFRP,CXMBFPTR                                   0283 00337000
*       CXMXBFRL=LENGTH(CXMXBUFR);    /* SET MVCL TO LEN             */ 00338000
         LA    @15,512                                             0284 00339000
         LR    CXMXBFRL,@15                                        0284 00340000
*       CXMNCSDT=ADDR(NCSAPDAT);      /* SET MVCL FROM PTR           */ 00341000
         LA    CXMNCSDT,NCSAPDAT(,CXMNCSAP)                        0285 00342000
*       CXMNCSDL=LENGTH(NCSAPDAT);    /* SET MVCL FROM LEN           */ 00343000
         LR    CXMNCSDL,@15                                        0286 00344000
*       GEN                                                        0287 00345000
*         SETS                                                     0287 00346000
*            (CXMXBUFR)                                            0287 00347000
*         REFS                                                     0287 00348000
*            (CXMXBFRP                                             0287 00349000
*            ,CXMXBFRL                                             0287 00350000
*            ,CXMNCSDT                                             0287 00351000
*            ,CXMNCSDL);                                           0287 00352000
         MVCL  CXMXBFRP,CXMNCSDT         MOVE DATA TO BUFFER            00353000
*       RFY                                                        0288 00354000
*         (CXMXBFRP                                                0288 00355000
*         ,CXMXBFRL                                                0288 00356000
*         ,CXMNCSDT                                                0288 00357000
*         ,CXMNCSDL) UNRSTD;                                       0288 00358000
*       RIBDAD=RIBDAD+LENGTH(NCSAPDAT); /* BUMP DUMP ADDRESS         */ 00359000
         LA    @15,76                                              0289 00360000
         ALR   @15,CXMNCSAP                                        0289 00361000
         LA    @08,512                                             0289 00362000
         AL    @08,RIBDAD(,@15)                                    0289 00363000
         ST    @08,RIBDAD(,@15)                                    0289 00364000
*       IF RIBDAD>=NCSAPEND           /* IF LAST DATA READ           */ 00365000
*         THEN                                                     0290 00366000
         C     @08,NCSAPEND(,CXMNCSAP)                             0290 00367000
         BL    @RF00290                                            0290 00368000
*           NCSPLVBF=NCSDF;           /* SET DUMP FINAL OPCD         */ 00369000
         MVC   NCSPLVBF(2,CXMNCSPL),@CB00977                       0291 00370000
*         ELSE;                                                    0292 00371000
@RF00290 DS    0H                                                  0293 00372000
*       RFY                                                        0293 00373000
*         CXMRTPTR UNRSTD;                                         0293 00374000
*       CALL DVLDEXCP;                /* INIT I/O OP TO 370X         */ 00375000
         BAL   @14,DVLDEXCP                                        0294 00376000
*       RFY                                                        0295 00377000
*         CXMRTPTR RSTD;                                           0295 00378000
*       CXMSVRTV=SUCCESS;             /* SET RETURN CODE             */ 00379000
         SLR   @15,@15                                             0296 00380000
         ST    @15,CXMSVRTV(,CXMNCSAP)                             0296 00381000
*     END;                                                         0297 00382000
*     ELSE                            /* FAILED SO:                  */ 00383000
*       RIBDAD=CNSTNT0;               /* CLEAR DUMP ADDRESS          */ 00384000
         B     @RC00280                                            0298 00385000
@RF00280 SLR   @15,@15                                             0298 00386000
         ST    @15,RIBDAD+76(,CXMNCSAP)                            0298 00387000
*   CXMSVPTR=CXMSVBCK;                /* RSTR CALLERS R13      9530  */ 00388000
@RC00280 L     CXMSVPTR,CXMSVBCK(,CXMNCSAP)                        0299 00389000
*   RETURN CODE(CXMSVRTV);            /* RETURN TO CALLER          0300 00390000
*                                        WITH RETVALUE               */ 00391000
         L     @15,CXMSVRTV(,CXMNCSAP)                             0300 00392000
         L     @14,12(,@13)                                        0300 00393000
         LM    @00,@12,20(@13)                                     0300 00394000
         BR    @14                                                 0300 00395000
*DVLDCHEK:                                                         0301 00396000
*   PROCEDURE OPTIONS(NOSAVE);                                     0301 00397000
DVLDCHEK DS    0H                                                  0302 00398000
*/********************************************************************/ 00399000
