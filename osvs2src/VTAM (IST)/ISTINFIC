         TITLE 'ISTINFIC/AIC-VS1/FIC-VS2 VTAM CVT AND CONFIG INITIALIZA*00001000
               ATION    '                                               00002000
ISTINFIC CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINFIC  75.231'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
