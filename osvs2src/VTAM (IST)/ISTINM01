         TITLE 'ISTINM01 - HOST INTERFACE ROUTINE                      *00001000
                        '                                               00002000
ISTINM01 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTINM01  76.018'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @11,@SA00001                                        0001 00015000
         ST    @11,8(,@13)                                         0001 00016000
         LR    @13,@11                                             0001 00017000
JSCBDISP EQU 188                                                        00018000
JSCBAOS1 EQU  72                                                        00019000
JSCBAOS2 EQU 192                                                        00020000
         MODESET MODE=SUP                                               00021000
         ENQ (M01MAJ,M01MIN,E,5,SYSTEM)                                 00022000
*   SKELPSA=0;                      /* BASE PSA ON LOCATION 0        */ 00023000
         SLR   SKELPSA,SKELPSA                                     0237 00024000
*   GEN(MODESET EXTKEY=ZERO);       /* MODESET TO REF JSCB AND CSCB  */ 00025000
         MODESET EXTKEY=ZERO                                            00026000
*   CSCBPTR=JSCBCSCB;               /* INIT CSCB POINTER             */ 00027000
         L     @09,CVTPTR                                          0239 00028000
         L     @09,CVTTCBP(,@09)                                   0239 00029000
         L     @09,TCBPTR(,@09)                                    0239 00030000
         L     @09,TCBJSCB(,@09)                                   0239 00031000
         L     CSCBPTR,JSCBCSCB(,@09)                              0239 00032000
*   WTOCONID=CHUCMP;                /* ID OF CONSOLE THAT ISSUED   0240 00033000
*                                      START VTAM - USED IN WTO      */ 00034000
         MVC   WTOCONID(1),CHUCMP(CSCBPTR)                         0240 00035000
*   GEN(MODESET EXTKEY=VTAM);       /* MODSET BACK FROM KEY ZERO     */ 00036000
         MODESET EXTKEY=VTAM                                            00037000
*   IF CVTATCVT=0 THEN              /* IS AVT PTR IN CVT             */ 00038000
         L     @09,CVTPTR                                          0242 00039000
         L     @09,CVTEXT2(,@09)                                   0242 00040000
         L     @09,CVTATCVT(,@09)                                  0242 00041000
         LTR   @09,@09                                             0242 00042000
         BNZ   @RF00242                                            0242 00043000
*     DO;                           /* NO - CONTINUE INIT            */ 00044000
*       CALL LOADRTM;               /* LOAD TRAMA MODS               */ 00045000
         BAL   @14,LOADRTM                                         0244 00046000
*       IF M01R15=0 THEN            /* WERE LOADS SUCCESSFUL         */ 00047000
         LTR   M01R15,M01R15                                       0245 00048000
         BNZ   @RF00245                                            0245 00049000
*         DO;                       /* YES                           */ 00050000
*           CALL INITVTAM;          /* GET STORAGE FOR             0247 00051000
*                                      AVT,ATCVT,CONFIGT             */ 00052000
         BAL   @14,INITVTAM                                        0247 00053000
*           IF M01R15^=0 THEN       /* WERE GETMAINS SUCCESSFUL      */ 00054000
         LTR   M01R15,M01R15                                       0248 00055000
         BNZ   @RT00248                                            0248 00056000
*             GOTO PREEXIT;         /* NO - PREPARE TO RETURN TO   0249 00057000
*                                      SYSTEM                        */ 00058000
*           CALL LOADMODS;          /* LOAD OTHER MODULES            */ 00059000
         BAL   @14,LOADMODS                                        0250 00060000
*           IF M01R15^=0 THEN       /* WERE LOADS SUCCESSFUL         */ 00061000
         LTR   M01R15,M01R15                                       0251 00062000
         BNZ   @RT00251                                            0251 00063000
*             GOTO LOADFAIL;        /* NO - ISSUE MESSAGE            */ 00064000
*           CALL GETPIOSV;          /* GETMAIN AND INIT TWO SAVE   0253 00065000
*                                      AREAS FOR TPIOS               */ 00066000
         BAL   @14,GETPIOSV                                        0253 00067000
*           IF M01R15^=0 THEN       /* WERE GETMAINS SUCCESSFUL      */ 00068000
         LTR   M01R15,M01R15                                       0254 00069000
         BNZ   @RT00254                                            0254 00070000
*             GOTO PREEXIT;         /* NO - TERMINATE                */ 00071000
*           CALL SETPSA;            /* INIT ATCVT ADDR IN ALL PSA'S  */ 00072000
         BAL   @14,SETPSA                                          0256 00073000
*           IF M01R15^=0 THEN       /* WAS INIT SUCCESSFUL           */ 00074000
         LTR   M01R15,M01R15                                       0257 00075000
         BNZ   @RT00257                                            0257 00076000
*             GOTO PREEXIT;         /* NO - TERMINATE                */ 00077000
*           CALL ISTINFIC;          /* INIT ATCVT AND CONFT          */ 00078000
         L     @15,@CV03666                                        0259 00079000
         BALR  @14,@15                                             0259 00080000
*           M01R15=0;               /* INDICATE INIT SUCCESSFUL      */ 00081000
         SLR   M01R15,M01R15                                       0260 00082000
*         END;                                                     0261 00083000
*       ELSE                                                       0262 00084000
*         DO;                       /* NO - ISSUE MSG AND TERMINATE  */ 00085000
         B     @RC00245                                            0262 00086000
@RF00245 DS    0H                                                  0263 00087000
*           GEN(DEQ (M01MAJ,M01MIN,5,SYSTEM));/* DEQ TO ALLOW RESTART*/ 00088000
         DEQ (M01MAJ,M01MIN,5,SYSTEM)                                   00089000
*           LDPRMPTR=ADDR(ALOADLST);/* ADDR OF LOAD LIST FOR MSG     */ 00090000
         LA    LDPRMPTR,ALOADLST                                   0264 00091000
*LOADFAIL:                                                         0265 00092000
*           MODNAME=LDPMBR;         /* NAME OF MODULE THAT FAILED TO    00093000
*                                      LOAD                          */ 00094000
LOADFAIL LA    @10,MSG012I                                         0265 00095000
         MVC   MODNAME+51(8,@10),LDPMBR(LDPRMPTR)                  0265 00096000
*           M01R0=WTOCONID;         /* CONSOLE ID FOR WTO            */ 00097000
         SLR   M01R0,M01R0                                         0266 00098000
         IC    M01R0,WTOCONID                                      0266 00099000
*           GEN REFS(MSG012I);                                     0267 00100000
MSG012I  WTO   'IST012I VTAM START REJECTED--CANNOT LOCATE XXXXXXXX',  X00101000
               MCSFLAG=(REG0,RESP),ROUTCDE=(10),DESC=4                  00102000
*           GO TO PREEXIT;                                         0268 00103000
         B     PREEXIT                                             0268 00104000
*         END;                                                     0269 00105000
*     END;                                                         0270 00106000
*   ELSE                                                           0271 00107000
*     DO;                           /* YES - AVT PTR IS IN CVT       */ 00108000
         B     @RC00242                                            0271 00109000
@RF00242 DS    0H                                                  0272 00110000
*       M01R0=WTOCONID;             /* CONSOLE ID FOR WTO            */ 00111000
         SLR   M01R0,M01R0                                         0272 00112000
         IC    M01R0,WTOCONID                                      0272 00113000
*       GEN;                                                       0273 00114000
         WTO   'IST009I VTAM IS ALREADY ACTIVE--START REJECTED',       X00115000
               MCSFLAG=(REG0,RESP),ROUTCDE=(10),DESC=4                  00116000
*       GEN(DEQ (M01MAJ,M01MIN,5,SYSTEM));/* DEQ TO ALLOW RESTART    */ 00117000
         DEQ (M01MAJ,M01MIN,5,SYSTEM)                                   00118000
*       M01R15=4;                   /* INDICATE INIT FAILED          */ 00119000
         LA    M01R15,4                                            0275 00120000
*     END;                                                         0276 00121000
*   IF M01R15^=0 THEN               /* HAS INIT BEEN SUCCESSFUL      */ 00122000
@RC00242 LTR   M01R15,M01R15                                       0277 00123000
         BNZ   @RT00277                                            0277 00124000
*     GOTO EXIT;                    /* NO-RETURN TO SYSTEM VS33607   */ 00125000
*   CONCSCB=CSCBPTR;                /* SET CSCB PTR IN CONFT         */ 00126000
         L     @09,M01ATCAD                                        0279 00127000
         L     @09,ATCCONFT(,@09)                                  0279 00128000
         ST    CSCBPTR,CONCSCB(,@09)                               0279 00129000
*   GEN(MODESET EXTKEY=ZERO);       /* MODESET TO MODIFY CSCB        */ 00130000
         MODESET EXTKEY=ZERO                                            00131000
*   CHECBP=ADDR(ATCECMOD);          /* ECB ADDR FOR MODIFY PROC      */ 00132000
         L     @09,M01ATCAD                                        0281 00133000
         LA    @08,ATCECMOD(,@09)                                  0281 00134000
         ST    @08,CHECBP(,CSCBPTR)                                0281 00135000
*   CONSTCIB=CHCIBP;                /* PTR TO CIB                    */ 00136000
         L     @09,ATCCONFT(,@09)                                  0282 00137000
         L     @08,CHCIBP(,CSCBPTR)                                0282 00138000
         ST    @08,CONSTCIB(,@09)                                  0282 00139000
*   RESPECIFY                                                      0283 00140000
*     M01R2 RSTD;                                                  0283 00141000
*   M01R2=ADDR(CHCIBP);             /* CIB PTR FOR QEDIT             */ 00142000
         LA    M01R2,CHCIBP(,CSCBPTR)                              0284 00143000
*   GEN(QEDIT ORIGIN=(2),CIBCTR=4); /* MODIFY COUNT                  */ 00144000
         QEDIT ORIGIN=(2),CIBCTR=4                                      00145000
*   RESPECIFY                                                      0286 00146000
*     M01R2 UNRSTD;                                                0286 00147000
*   ATCASCB=PSAAOLD;                /* VTAM ASCB ADDR                */ 00148000
         L     @09,PSAAOLD(,SKELPSA)                               0287 00149000
         L     @08,M01ATCAD                                        0287 00150000
         ST    @09,ATCASCB(,@08)                                   0287 00151000
*   GEN(MODESET EXTKEY=VTAM);       /* MODSET BACK FROM KEY ZERO     */ 00152000
*                                                                  0288 00153000
         MODESET EXTKEY=VTAM                                            00154000
*   /*****************************************************************/ 00155000
*   /*                                                               */ 00156000
*   /* INIT PAB FOR CFMTM                                            */ 00157000
*   /*                                                               */ 00158000
*   /*****************************************************************/ 00159000
*                                                                  0289 00160000
*   ATCTPMMI=ADDR(ISTCFMMI);        /* INIT ATCVT WITH CFMMI ADDR    */ 00161000
         L     @10,M01ATCAD                                        0289 00162000
         L     @09,@CV03667                                        0289 00163000
         ST    @09,ATCTPMMI(,@10)                                  0289 00164000
*   RESPECIFY                                                      0290 00165000
*    (M01R2,                                                       0290 00166000
*     M01R3) RESTRICTED;                                           0290 00167000
*   M01R2=ADDR(ATCTPMPB);           /* ADDR OF TPMSG PAB             */ 00168000
         LA    M01R2,ATCTPMPB(,@10)                                0291 00169000
*   RESPECIFY                                                      0292 00170000
*     ISTDYPAB BASED(M01R2);                                       0292 00171000
*   M01R3=ADDR(DYPPAB);                                            0293 00172000
         LA    @09,DYPPAB(,M01R2)                                  0293 00173000
         LR    M01R3,@09                                           0293 00174000
*   RESPECIFY                                                      0294 00175000
*     ISTPAB BASED(M01R3);                                         0294 00176000
*   DYPTYPE=DYTYP;                                                 0295 00177000
         MVI   DYPTYPE(M01R2),X'11'                                0295 00178000
*   PABDVTA=ADDR(ATCTPMMI);                                        0296 00179000
         LA    @10,ATCTPMMI(,@10)                                  0296 00180000
         STCM  @10,7,PABDVTA(M01R3)                                0296 00181000
*   PABDYNAM='1'B;                                                 0297 00182000
*   PABSSN='1'B;                                                   0298 00183000
         OI    PABDYNAM(M01R3),B'00011000'                         0298 00184000
*   PABOFFST=ADDR(DYPPAB)-ADDR(ISTDYPAB);                          0299 00185000
         SLR   @09,M01R2                                           0299 00186000
         STC   @09,PABOFFST(,M01R3)                                0299 00187000
*   RESPECIFY                                                      0300 00188000
*    (M01R2,                                                       0300 00189000
*     M01R3) UNRESTRICTED;                                         0300 00190000
*   CALL ISTINCIT;                  /* INCIT COMPLETES INITIALIZATION*/ 00191000
         L     @15,@CV03665                                        0301 00192000
         BALR  @14,@15                                             0301 00193000
*PREEXIT:                                                          0302 00194000
*   CALL CLEANUP;                   /* FREE STORAGE THAT WAS       0302 00195000
*                                      GETMAIN'D                     */ 00196000
PREEXIT  BAL   @14,CLEANUP                                         0302 00197000
*   CALL UNLOADER;                  /* UNLOAD MODULES PREVIOUSLY   0303 00198000
*                                      LOADED                        */ 00199000
         BAL   @14,UNLOADER                                        0303 00200000
*EXIT:                              /* RETURN TO SYSTEM VS33607      */ 00201000
*   RETURN CODE(0);                                                0304 00202000
EXIT     SLR   @15,@15                                             0304 00203000
         L     @13,4(,@13)                                         0304 00204000
         L     @14,12(,@13)                                        0304 00205000
         LM    @00,@12,20(@13)                                     0304 00206000
         BR    @14                                                 0304 00207000
*LOADRTM:                                                          0305 00208000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0305 00209000
*                                                                  0305 00210000
LOADRTM  DS    0H                                                  0306 00211000
*/********************************************************************/ 00212000
*/*                                                                  */ 00213000
*/*     THIS ROUTINE GETS THE ADDRESSES IF THE RECOVERY/TERMINATION  */ 00214000
*/*  MODULES AND PUTS THEM IN THE SYSTEM CVT.                        */ 00215000
*/*                                                                  */ 00216000
*/********************************************************************/ 00217000
*                                                                  0306 00218000
*   M01SAVE=M01R14;                 /* SAVE CALLER'S RETURN ADDR     */ 00219000
         ST    M01R14,M01SAVE                                      0306 00220000
*   ALOADLST=ALOADLST&&ALOADLST;    /* ZERO OUT ALOAD LIST - THIS  0307 00221000
*                                      WILL SET THE DCB ADDR TO 0  0307 00222000
*                                      CAUSING BLDL TO SEARCH LPALIB */ 00223000
         XC    ALOADLST(28),ALOADLST                               0307 00224000
*   LDPRMPTR=ADDR(ALOADLST);        /* ADDR OF ALOAD LIST FOR INMAL  */ 00225000
         LA    LDPRMPTR,ALOADLST                                   0308 00226000
*   LDPPDS='LPALIB  ';              /* LIBRARY MOD TO BE LOADED FROM */ 00227000
         MVC   LDPPDS(8,LDPRMPTR),@CC03737                         0309 00228000
*   LDPMBR='ISTRAMA1';              /* NAME OF MODULE TO BE LOADED   */ 00229000
         MVC   LDPMBR(8,LDPRMPTR),@CC03738                         0310 00230000
*   CALL ISTINMAL;                  /* LOAD MODULE                   */ 00231000
         L     @15,@CV03664                                        0311 00232000
         BALR  @14,@15                                             0311 00233000
*   IF M01R15=0 THEN                /* WAS LOAD SUCCESSFUL           */ 00234000
         LTR   M01R15,M01R15                                       0312 00235000
         BNZ   @RF00312                                            0312 00236000
*     DO;                           /* YES                           */ 00237000
*       GEN(MODESET EXTKEY=ZERO);   /* MODESET TO MODIFY CVT         */ 00238000
         MODESET EXTKEY=ZERO                                            00239000
*       CVTRMPTT=M01R0;             /* INIT ADDR OF RAMA1 IN CVT     */ 00240000
         L     @09,CVTPTR                                          0315 00241000
         ST    M01R0,CVTRMPTT(,@09)                                0315 00242000
*       LDPMBR='ISTRAMA2';          /* NAME OF MODULE TO BE LOADED   */ 00243000
         MVC   LDPMBR(8,LDPRMPTR),@CC03739                         0316 00244000
*       CALL ISTINMAL;              /* LOAD MODULE                   */ 00245000
         L     @15,@CV03664                                        0317 00246000
         BALR  @14,@15                                             0317 00247000
*       IF M01R15=0 THEN            /* WAS LOAD SUCCESSFUL           */ 00248000
         LTR   M01R15,M01R15                                       0318 00249000
         BNZ   @RF00318                                            0318 00250000
*         CVTRMPMT=M01R0;           /* YES-INIT ADDR OF RAMA2 IN CVT */ 00251000
         L     @09,CVTPTR                                          0319 00252000
         ST    M01R0,CVTRMPMT(,@09)                                0319 00253000
*       GEN(MODESET EXTKEY=VTAM);   /* MODESET BACK FROM KEY ZERO    */ 00254000
@RF00318 DS    0H                                                  0320 00255000
         MODESET EXTKEY=VTAM                                            00256000
*     END;                                                         0321 00257000
*   M01R14=M01SAVE;                 /* RESTORE CALLER'S RETURN ADDR  */ 00258000
@RF00312 L     M01R14,M01SAVE                                      0322 00259000
*   END LOADRTM;                                                   0323 00260000
@EL00002 DS    0H                                                  0323 00261000
@EF00002 DS    0H                                                  0323 00262000
@ER00002 BR    @14                                                 0323 00263000
*LOADMODS:                                                         0324 00264000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0324 00265000
*                                                                  0324 00266000
LOADMODS DS    0H                                                  0325 00267000
*/********************************************************************/ 00268000
*/*                                                                  */ 00269000
*/*     THIS ROUTINE LOADS THE TP MESSAGE MODULES AND THE STORAGE    */ 00270000
*/*  MANAGEMENT MODULE AND INITIALIZES THEIR ADDRESSES IN THE ATCVT. */ 00271000
*/*                                                                  */ 00272000
*/********************************************************************/ 00273000
*                                                                  0325 00274000
*   M01SAVE=M01R14;                 /* SAVE CALLER'S RETURN ADDR     */ 00275000
         ST    M01R14,M01SAVE                                      0325 00276000
*   ALOADLST=ALOADLST&&ALOADLST;    /* ZERO OUT ALOAD LIST - THIS  0326 00277000
*                                      WILL SET THE DCB ADDR TO 0  0326 00278000
*                                      CAUSING BLDL TO SEARCH LPALIB */ 00279000
         XC    ALOADLST(28),ALOADLST                               0326 00280000
*   LDPRMPTR=ADDR(ALOADLST);        /* ADDR OF ALOAD LIST FOR INMAL  */ 00281000
