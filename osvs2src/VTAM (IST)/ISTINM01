*SETPSA:                                                           0466 00673000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0466 00674000
*                                                                  0466 00675000
SETPSA   DS    0H                                                  0467 00676000
*/********************************************************************/ 00677000
*/*                                                                  */ 00678000
*/*    THIS ROUTINE INITIALIZES THE PSAATCVT FIELD TO THE ADDRESS OF */ 00679000
*/*  THE ATCVT. IF THE SYSTEM IS A U.P. ONLY ONE PSA MUST BE MODIFIED*/ 00680000
*/*  IF THE SYSTEM IS AN M.P. THE FOLLOWING ARE DONE:                */ 00681000
*/*      1) ADDITIONAL CPUS ARE PREVENTED FROM BEING VARIED ON BY    */ 00682000
*/*          ENQUEING ON THE 'VARY CPU' RESOURCE.                    */ 00683000
*/*      2) THE SKELETON PSA COPY IS MODIFIED.                       */ 00684000
*/*      3) THE GLOBAL DISPATCHER LOCK IS OBTAINED TO PREVENT M01    */ 00685000
*/*          FROM BEING DISPATCHED ON ANOTHER CPU.                   */ 00686000
*/*        NOTE: THE SKELETON PSA MUST BE MODIFIED PRIOR TO GETTING  */ 00687000
*/*          THE LOCK SINCE THE SKELETON PSA IS NOT PAGED FIXED.     */ 00688000
*/*      4) M01'S PSA IS MODIFIED                                    */ 00689000
*/*      5) THE CPU ALIVE BITS IN THE CSD ARE CHECKED AND THE PSA FOR*/ 00690000
*/*          EACH ALIVE CPU IS MODIFIED.                             */ 00691000
*/*      6) THE GLOBAL LOCK IS THEN FREED AND THE 'VARY CPU' RESOURCE*/ 00692000
*/*          IS DEQUEUED.                                            */ 00693000
*/*                                                                  */ 00694000
*/********************************************************************/ 00695000
*                                                                  0467 00696000
*   M01SAVE=M01R14;                 /* SAVE CALLER'S RETURN ADDR     */ 00697000
         ST    M01R14,M01SAVE                                      0467 00698000
*   GEN(MODESET EXTKEY=ZERO);       /* MODESET TO MODIFY PSA         */ 00699000
         MODESET EXTKEY=ZERO                                            00700000
*   IF CSDMP='0'B THEN              /* IS THIS A UP MACHINE          */ 00701000
         L     @09,CVTPTR                                          0469 00702000
         L     @09,CVTCSD(,@09)                                    0469 00703000
         TM    CSDMP(@09),B'10000000'                              0469 00704000
         BNZ   @RF00469                                            0469 00705000
*     DO;                           /* YES - ONLY ONE PSA TO MODIFY  */ 00706000
*       PSAATCVT=M01ATCAD;          /* INIT ADDR OF ATCVT            */ 00707000
         L     @09,M01ATCAD                                        0471 00708000
         ST    @09,PSAATCVT(,SKELPSA)                              0471 00709000
*       RTNCDE=0;                   /* INDICATE PSA INIT SUCCESSFUL  */ 00710000
         SLR   RTNCDE,RTNCDE                                       0472 00711000
*     END;                                                         0473 00712000
*   ELSE                                                           0474 00713000
*     DO;                           /* NO - ITS MP                   */ 00714000
         B     @RC00469                                            0474 00715000
@RF00469 DS    0H                                                  0475 00716000
*       GEN(ENQ (SYSMAJ,SYSMIN,E,3,SYSTEM));/* LOCK OUT VARY CPU     */ 00717000
         ENQ (SYSMAJ,SYSMIN,E,3,SYSTEM)                                 00718000
*       M01CB1=0;                   /* CLEAR ECB FOR PGFIX           */ 00719000
         SLR   @09,@09                                             0476 00720000
         ST    @09,M01CB1                                          0476 00721000
*       RESPECIFY                                                  0477 00722000
*        (M01R2,                                                   0477 00723000
*         M01R3) RESTRICTED;                                       0477 00724000
*       M01R2=ADDR(ISTINM01);       /* M01 CSECT WILL BE FIXED       */ 00725000
         L     M01R2,@CA00028                                      0478 00726000
*       M01R3=M01R2+M01END;         /* LENGTH OF M01 FOR PGFIX       */ 00727000
         LR    M01R3,M01R2                                         0479 00728000
         AL    M01R3,M01END                                        0479 00729000
*       GEN REFS(M01R2,M01R3,M01CB1)                               0480 00730000
*           (PGFIX R,A=(M01R2),EA=(M01R3),ECB=M01CB1);             0480 00731000
         PGFIX R,A=(M01R2),EA=(M01R3),ECB=M01CB1                        00732000
*       RESPECIFY                                                  0481 00733000
*        (M01R2,                                                   0481 00734000
*         M01R3) UNRESTRICTED;                                     0481 00735000
*       GEN SETS(M01CB1)(WAIT 1,ECB=M01CB1);                       0482 00736000
         WAIT 1,ECB=M01CB1                                              00737000
*       IF M01ECBCD^='0'B THEN      /* WAS PGFIX SUCCESSFUL          */ 00738000
         TM    M01ECBCD,B'00000100'                                0483 00739000
         BZ    @RF00483                                            0483 00740000
*         DO;                       /* NO - PGFIX FAILED             */ 00741000
*           M01R0=WTOCONID;         /* CONSOLE ID FOR WTO            */ 00742000
         SLR   M01R0,M01R0                                         0485 00743000
         IC    M01R0,WTOCONID                                      0485 00744000
*           M01R1=ADDR(MSG001I);    /* ADDR OF LIST FORM OF WTO      */ 00745000
         LA    M01R1,MSG001I                                       0486 00746000
*           GEN REFS(MSG001I)(WTO   MF=(E,(1)));                   0487 00747000
         WTO   MF=(E,(1))                                               00748000
*           RTNCDE=4;                                              0488 00749000
         LA    RTNCDE,4                                            0488 00750000
*         END;                                                     0489 00751000
*       ELSE                                                       0490 00752000
*         DO;                       /* YES - SUCCESSFUL              */ 00753000
*                                                                  0490 00754000
         B     @RC00483                                            0490 00755000
@RF00483 DS    0H                                                  0491 00756000
*           /*********************************************************/ 00757000
*           /*                                                       */ 00758000
*           /* MODIFY SKELETON PSA BEFORE DISABLING                  */ 00759000
*           /*                                                       */ 00760000
*           /*********************************************************/ 00761000
*                                                                  0491 00762000
*           SKELPSA=CVTVPSA;        /* SKEL COPY OF PSA              */ 00763000
         L     @02,CVTPTR                                          0491 00764000
         L     SKELPSA,CVTVPSA(,@02)                               0491 00765000
*           PSAATCVT=M01ATCAD;      /* ADDR OF ATCVT                 */ 00766000
*                                                                  0492 00767000
         L     @02,M01ATCAD                                        0492 00768000
         ST    @02,PSAATCVT(,SKELPSA)                              0492 00769000
*           /*********************************************************/ 00770000
*           /*                                                       */ 00771000
*           /* DISABLE TO PREVENT BEING DISPATCHED ON ANOTHER CPU    */ 00772000
*           /*                                                       */ 00773000
*           /*********************************************************/ 00774000
*                                                                  0493 00775000
*           GEN REFS(PSALITA,FLC) SETS(M01R0,M01R1,M01R2,M01R3,M01R4);  00776000
*                                                                  0493 00777000
        SETLOCK OBTAIN,TYPE=DISP,MODE=UNCOND,REGS=USE,                 X00778000
               RELATED=('DISABLE')                                      00779000
*           /*********************************************************/ 00780000
*           /*                                                       */ 00781000
*           /* MODIFY VTAM'S PSA                                     */ 00782000
*           /*                                                       */ 00783000
*           /*********************************************************/ 00784000
*                                                                  0494 00785000
*           SKELPSA=0;              /* PREFIXING WILL RESULT         */ 00786000
         SLR   SKELPSA,SKELPSA                                     0494 00787000
*           PSAATCVT=M01ATCAD;                                     0495 00788000
*                                                                  0495 00789000
         L     @02,M01ATCAD                                        0495 00790000
         ST    @02,PSAATCVT(,SKELPSA)                              0495 00791000
*           /*********************************************************/ 00792000
*           /*                                                       */ 00793000
*           /* SEARCH FOR OTHER PSA'S - CHECK THE CSD TO SEE IF THE  */ 00794000
*           /* CPU IS ALIVE BEFORE UPDATING PSA                      */ 00795000
*           /*                                                       */ 00796000
*           /*********************************************************/ 00797000
*                                                                  0496 00798000
*           RESPECIFY                                              0496 00799000
*             CPUMSK RESTRICTED;                                   0496 00800000
*           RESPECIFY                                              0497 00801000
*             PSA BASED(PCCAPSAV);  /* PSA ADDR IN PCCA              */ 00802000
*           CPUMSK=CSDCPUAL;        /* LOAD ALIVE CPU MASK INTO REG  */ 00803000
         L     @09,CVTPTR                                          0498 00804000
         L     @08,CVTCSD(,@09)                                    0498 00805000
         LH    CPUMSK,CSDCPUAL(,@08)                               0498 00806000
         N     CPUMSK,@CF03784                                     0498 00807000
*           PAVTPTR=CVTPCCAT;       /* ADDR OF PCCAVT                */ 00808000
         L     PAVTPTR,CVTPCCAT(,@09)                              0499 00809000
*           GEN(SLL CPUMSK,16);     /* SHIFT TO CORRECT PLACE        */ 00810000
         SLL CPUMSK,16                                                  00811000
*           DO WHILE CPUMSK^=0;     /* PROCESS WHILE THERE ARE BITS     00812000
*                                      REMAINING IN THE REG,       0501 00813000
*                                      INDICATING ALIVE CPUS         */ 00814000
         B     @DE00501                                            0501 00815000
@DL00501 DS    0H                                                  0502 00816000
*             GEN(ALR CPUMSK,CPUMSK);/* THIS WILL SHIFT REG ONE BIT     00817000
*                                      AND CAUSE OVERFLOW IF THE   0502 00818000
*                                      HI-ORDER BIT WAS ON           */ 00819000
         ALR CPUMSK,CPUMSK                                              00820000
*             BC(12,UPDATPTR);      /* BRANCH IF CPU ISN'T ALIVE     */ 00821000
         BC    12,UPDATPTR                                         0503 00822000
*             PSAATCVT=M01ATCAD;    /* YES - SET ADDR OF ATCVT       */ 00823000
         L     @09,PCCAPTR(,PAVTPTR)                               0504 00824000
         L     @09,PCCAPSAV(,@09)                                  0504 00825000
         L     @08,M01ATCAD                                        0504 00826000
         ST    @08,PSAATCVT(,@09)                                  0504 00827000
*UPDATPTR:                                                         0505 00828000
*             PAVTPTR=PAVTPTR+4;    /* NEXT ENTRY IN PCCAVT          */ 00829000
UPDATPTR AL    PAVTPTR,@CF00053                                    0505 00830000
*           END;                                                   0506 00831000
@DE00501 LTR   CPUMSK,CPUMSK                                       0506 00832000
         BNZ   @DL00501                                            0506 00833000
*           GEN REFS(PSALITA,FLC) SETS(M01R1,M01R2,M01R3,M01R4)    0507 00834000
*               (SETLOCK RELEASE,TYPE=DISP,REGS=USE,RELATED=('ENABLE')) 00835000
*               ;                                                  0507 00836000
         SETLOCK RELEASE,TYPE=DISP,REGS=USE,RELATED=('ENABLE')          00837000
*           RTNCDE=0;               /* INDICATE PSA INIT SUCCESS     */ 00838000
         SLR   RTNCDE,RTNCDE                                       0508 00839000
*         END;                                                     0509 00840000
