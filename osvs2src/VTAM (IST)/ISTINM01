*UNLOADER:                                                         0347 00331000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0347 00332000
*                                                                  0347 00333000
UNLOADER DS    0H                                                  0348 00334000
*/********************************************************************/ 00335000
*/*                                                                  */ 00336000
*/*     THIS ROUTINE RESETS THE CIB COUNT, CSCBPTR AND UNLOADS THE   */ 00337000
*/*  TP MESSAGE MODULES AND STORAGE MANAGEMENT MODULE.               */ 00338000
*/*                                                                  */ 00339000
*/********************************************************************/ 00340000
*                                                                  0348 00341000
*   M01SAVE=M01R14;                 /* SAVE CALLER'S RETURN ADDR     */ 00342000
         ST    M01R14,M01SAVE                                      0348 00343000
*   GEN(MODESET EXTKEY=ZERO);       /* MODESET TO MODIFY CVT/CSCB    */ 00344000
         MODESET EXTKEY=ZERO                                            00345000
*   RESPECIFY                                                      0350 00346000
*     M01R2 RSTD;                                                  0350 00347000
*   M01R2=ADDR(CHCIBP);             /* CIB PTR FOR QEDIT             */ 00348000
         LA    M01R2,CHCIBP(,CSCBPTR)                              0351 00349000
*   GEN(QEDIT ORIGIN=(2),CIBCTR=0); /* RESET MODIFY COUNT            */ 00350000
         QEDIT ORIGIN=(2),CIBCTR=0                                      00351000
*   RESPECIFY                                                      0353 00352000
*     M01R2 UNRSTD;                                                0353 00353000
*   CHECBP=0;                       /* RESET ADDR OF ECB FOR MODIFY     00354000
*                                      PROC                          */ 00355000
         SLR   @10,@10                                             0354 00356000
         ST    @10,CHECBP(,CSCBPTR)                                0354 00357000
*   GEN(MODESET EXTKEY=VTAM);       /* MODESET BACK FROM KEY ZERO    */ 00358000
         MODESET EXTKEY=VTAM                                            00359000
*   LDPRMPTR=ADDR(ALOADLST);        /* ADDR OF ALOAD LIST FOR INMAD  */ 00360000
         LA    LDPRMPTR,ALOADLST                                   0356 00361000
*   LDPMBR='ISTORFSR';              /* NAME OF MOD TO BE DELETED     */ 00362000
         MVC   LDPMBR(8,LDPRMPTR),@CC03745                         0357 00363000
*   CALL ISTINMAD;                  /* DELETE MODULE                 */ 00364000
         L     @15,@CV03663                                        0358 00365000
         BALR  @14,@15                                             0358 00366000
*   LDPMBR='ISTCFMTM';              /* NAME OF MOD TO BE DELETED     */ 00367000
         MVC   LDPMBR(8,LDPRMPTR),@CC03743                         0359 00368000
*   CALL ISTINMAD;                  /* DELETE MODULE                 */ 00369000
         L     @15,@CV03663                                        0360 00370000
         BALR  @14,@15                                             0360 00371000
*   LDPMBR='ISTCFCMM';              /* NAME OF MOD TO BE DELETED     */ 00372000
         MVC   LDPMBR(8,LDPRMPTR),@CC03744                         0361 00373000
*   CALL ISTINMAD;                  /* DELETE MODULE                 */ 00374000
