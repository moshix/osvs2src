*       RESPECIFY                                                  0515 00854000
*        (M01R2,                                                   0515 00855000
*         M01R3) UNRESTRICTED;                                     0515 00856000
*     END;                                                         0516 00857000
*   GEN(MODESET EXTKEY=VTAM);       /* MODESET BACK FROM KEY ZERO    */ 00858000
@RC00469 DS    0H                                                  0517 00859000
         MODESET EXTKEY=VTAM                                            00860000
*   M01R14=M01SAVE;                 /* RESTORE CALLER'S RETURN ADDR  */ 00861000
         L     M01R14,M01SAVE                                      0518 00862000
*   M01R15=RTNCDE;                  /* RETURN CODE FOR MAIN LINE   0519 00863000
*                                      CHECK                         */ 00864000
         LR    M01R15,RTNCDE                                       0519 00865000
*   END SETPSA;                                                    0520 00866000
@EL00007 DS    0H                                                  0520 00867000
@EF00007 DS    0H                                                  0520 00868000
@ER00007 BR    @14                                                 0520 00869000
*GETPIOSV:                                                         0521 00870000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0521 00871000
*                                                                  0521 00872000
GETPIOSV DS    0H                                                  0522 00873000
*/********************************************************************/ 00874000
*/*                                                                  */ 00875000
*/*      THIS ROUTINE GETMAINS, PGFIXES, AND INITIALIZES TWO WORK    */ 00876000
*/*  AREAS FOR TPIOS. IT PUTS THE ADDRESS OF EACH AREA IN THE ATCVT. */ 00877000
*/*                                                                  */ 00878000
*/********************************************************************/ 00879000
*                                                                  0522 00880000
*   M01SAVE=M01R14;                 /* SAVE CALLER'S RETURN ADDR     */ 00881000
         ST    M01R14,M01SAVE                                      0522 00882000
*   SVAREASZ=(5*ZSAFSVSZ)+LENGTH(ISTZ19SV);/* SIZE FOR GETMAIN       */ 00883000
         LA    @03,296                                             0523 00884000
         ST    @03,SVAREASZ                                        0523 00885000
*   M01R0=SVAREASZ;                                                0524 00886000
