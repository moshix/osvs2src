         BNZ   @RF00336                                            0336 00304000
*         DO;                       /* YES                           */ 00305000
*           ATCMSGM=M01R0;          /* INIT ADDR OF CFCMM IN ATCVT   */ 00306000
         L     @09,M01ATCAD                                        0338 00307000
         ST    M01R0,ATCMSGM(,@09)                                 0338 00308000
*           LDPMBR='ISTORFSR';      /* GETSTORE MODULE               */ 00309000
         MVC   LDPMBR(8,LDPRMPTR),@CC03745                         0339 00310000
*           CALL ISTINMAL;          /* LOAD MODULE                   */ 00311000
         L     @15,@CV03664                                        0340 00312000
