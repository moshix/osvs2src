*   LDPPDS='LPALIB  ';              /* LIBRARY MOD TO BE LOADED FROM */ 00283000
         MVC   LDPPDS(8,LDPRMPTR),@CC03737                         0328 00284000
*   LDPMBR='ISTCFMTM';              /* NAME OF MODULE TO BE LOADED   */ 00285000
         MVC   LDPMBR(8,LDPRMPTR),@CC03743                         0329 00286000
*   CALL ISTINMAL;                  /* LOAD MODULE                   */ 00287000
         L     @15,@CV03664                                        0330 00288000
         BALR  @14,@15                                             0330 00289000
*   IF M01R15=0 THEN                /* WAS LOAD SUCCESSFUL           */ 00290000
         LTR   M01R15,M01R15                                       0331 00291000
         BNZ   @RF00331                                            0331 00292000
*     DO;                           /* YES                           */ 00293000
*       ATCMSGP=M01R0;              /* ADDR OF ISTCFMTM FROM LOAD    */ 00294000
         L     @09,M01ATCAD                                        0333 00295000
         ST    M01R0,ATCMSGP(,@09)                                 0333 00296000
*       LDPMBR='ISTCFCMM';          /* NAME OF MODULE TO BE LOADED   */ 00297000
         MVC   LDPMBR(8,LDPRMPTR),@CC03744                         0334 00298000
*       CALL ISTINMAL;              /* LOAD MODULE                   */ 00299000
         L     @15,@CV03664                                        0335 00300000
         BALR  @14,@15                                             0335 00301000
*       IF M01R15=0 THEN            /* WAS LOAD SUCCESSFUL           */ 00302000
