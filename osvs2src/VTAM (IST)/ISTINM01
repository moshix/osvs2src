*   M01R3=TPIOSSV2+SVAREASZ;        /* END ADDR FOR PGFIX            */ 00930000
         AL    @09,SVAREASZ                                        0542 00931000
         LR    M01R3,@09                                           0542 00932000
*   GEN REFS(M01R2,M01R3,M01CB1)                                   0543 00933000
*       (PGFIX R,A=(M01R2),EA=(M01R3),ECB=M01CB1);                 0543 00934000
         PGFIX R,A=(M01R2),EA=(M01R3),ECB=M01CB1                        00935000
*   GEN SETS(M01CB1)(WAIT 1,ECB=M01CB1);                           0544 00936000
