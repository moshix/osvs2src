*         RFY                                                      0712 00112000
*          (EMCR2,                                                 0712 00113000
*           EMCR3) UNRSTD;                                         0712 00114000
*       END;                        /* CIDCTL                        */ 00115000
*       RFY                                                        0714 00116000
*         R1 UNRSTD;                                               0714 00117000
*       RFY                                                        0715 00118000
*         R2 UNRSTD;                                               0715 00119000
*       IF R15=0 THEN               /* TEST IF CIDCTL SUCCESSFUL     */ 00120000
         LTR   R15,R15                                             0716 00121000
         BNZ   @RF00716                                            0716 00122000
*         DO;                                                      0717 00123000
*           RFY                                                    0718 00124000
*             R15 UNRSTD;                                          0718 00125000
*           NCBPTR=R0;              /* SAVE NODE PTR IN WORKAREA     */ 00126000
