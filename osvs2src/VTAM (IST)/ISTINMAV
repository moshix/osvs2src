         BO    @RT00729                                            0729 00156000
*                     ;                                            0730 00157000
*                                                                  0730 00158000
*                   /*************************************************/ 00159000
*                   /*                                               */ 00160000
*                   /* TEST FOR REMOTE 370X                  @Z40GKD1*/ 00161000
*                   /*                                               */ 00162000
*                   /*************************************************/ 00163000
*                                                                  0731 00164000
*                   ELSE                                           0731 00165000
*                     DO;           /* TEST FOR LOCAL 370X OR 3791 0731 00166000
*                                                            @Z40GKD1*/ 00167000
@RF00729 DS    0H                                                  0732 00168000
*                       IF(NCSPLUAD=0)|/* TEST FOR OVERRIDE ADDR     */ 00169000
*                           (NCSPLVBF^=NCSVACT&NCSPLVBF^=NCSVALO)  0732 00170000
*                                                                  0732 00171000
*                       /*********************************************/ 00172000
*                       /*                                           */ 00173000
*                       /* OR DEALLOCATE @ZM33576,@ZM31576           */ 00174000
*                       /*                                           */ 00175000
*                       /*********************************************/ 00176000
*                                                                  0732 00177000
*                         THEN                                     0732 00178000
         L     @08,NCSPLPTR                                        0732 00179000
