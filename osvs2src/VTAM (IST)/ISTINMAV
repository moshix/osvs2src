*             DO;                                                  0761 00259000
*               CALL ICINIT;        /* INITIALIZE ICNCB              */ 00260000
         BAL   @14,ICINIT                                          0762 00261000
*             END;                                                 0763 00262000
*           ELSE                                                   0764 00263000
*             DO;                                                  0764 00264000
*                                                                  0764 00265000
         B     @RC00760                                            0764 00266000
@RF00760 DS    0H                                                  0765 00267000
*               /*****************************************************/ 00268000
*               /*                                                   */ 00269000
*               /* THIS SEGMENT PERFORMS THE FOLLOWING FUNCTIONS: 1) */ 00270000
*               /* CHECKS THE UNIT ADDRESS FOR VALID EBCDIC NUMBERS: */ 00271000
*               /* 0-9 (F0-F9), A-F (C1-C6). 2) CONVERTS AND PACKS   */ 00272000
*               /* THE VALIDATED EBCDIC UNIT ADDRESS INTO            */ 00273000
*               /* HEXADECIMAL. 3) FINDS THE ADDRESS OF THE UCB      */ 00274000
*               /* ASSOCIATED WITH THE UNIT ADDRESS, IF ANY, BY USING*/ 00275000
*               /* THE IOSGEN UCBLOOKUP MACRO. 4) CHECKS THE DEVICE  */ 00276000
*               /* TYPE OF THE UCB TO ENSURE THAT IT CORRESPONDS TO  */ 00277000
*               /* THE ENTRY TYPE IN THE RDT ENTRY.                  */ 00278000
*               /*                                                   */ 00279000
*               /*****************************************************/ 00280000
*                                                                  0765 00281000
*               RFY                                                0765 00282000
*                 R14 RSTD;         /* IOSGEN MACRO DESTROYS R14     */ 00283000
*               SAV14=R14;          /* SO SAVE R14                   */ 00284000
         ST    R14,SAV14(,WORKAPTR)                                0766 00285000
*               R13=R13+12;         /* IOSGEN WILL DESTROY PTRS SO 0767 00286000
*                                      ADD                           */ 00287000
*                                                                  0767 00288000
         AL    R13,@CF02186                                        0767 00289000
*               /*****************************************************/ 00290000
*               /*                                                   */ 00291000
*               /* VALIDATE UNIT ADDRESS                             */ 00292000
*               /*                                                   */ 00293000
*               /*****************************************************/ 00294000
*                                                                  0768 00295000
*               WKNCSUAD=NCSPLUAD;  /* MOVE DEVICE ADDR INTO WORKAREA*/ 00296000
         L     @15,NCSPLPTR                                        0768 00297000
         MVC   WKNCSUAD(3,WORKAPTR),NCSPLUAD(@15)                  0768 00298000
*               DO K=1 TO 3;        /* 3 CHARACTER UNIT ADDRESS      */ 00299000
         LA    K,1                                                 0769 00300000
@DL00769 DS    0H                                                  0770 00301000
*                 IF(UAD(K)>239)&   /* F0-F9 VALID                   */ 00302000
*                     (UAD(K)<250) THEN                            0770 00303000
         LR    @15,WORKAPTR                                        0770 00304000
         BCTR  @15,0                                               0770 00305000
         SLR   @08,@08                                             0770 00306000
         IC    @08,UAD(K,@15)                                      0770 00307000
         C     @08,@CF05541                                        0770 00308000
         BNH   @RF00770                                            0770 00309000
         C     @08,@CF05542                                        0770 00310000
         BNL   @RF00770                                            0770 00311000
*                   DO;                                            0771 00312000
*                     UAD(K)=UAD(K)-240;/* SUBTRACT F0               */ 00313000
         SL    @08,@CF05543                                        0772 00314000
         LR    @15,WORKAPTR                                        0772 00315000
         BCTR  @15,0                                               0772 00316000
         STC   @08,UAD(K,@15)                                      0772 00317000
*                   END;                                           0773 00318000
*                 ELSE                                             0774 00319000
*                   DO;                                            0774 00320000
         B     @RC00770                                            0774 00321000
@RF00770 DS    0H                                                  0775 00322000
*                     IF(UAD(K)>192)&/* C1-C6 VALID                  */ 00323000
*                         (UAD(K)<199) THEN                        0775 00324000
         LR    @15,WORKAPTR                                        0775 00325000
         BCTR  @15,0                                               0775 00326000
         SLR   @08,@08                                             0775 00327000
         IC    @08,UAD(K,@15)                                      0775 00328000
         C     @08,@CF05544                                        0775 00329000
         BNH   @RF00775                                            0775 00330000
         C     @08,@CF05545                                        0775 00331000
         BNL   @RF00775                                            0775 00332000
*                       DO;                                        0776 00333000
*                         UAD(K)=UAD(K)-183;/* SUBTRACT B7           */ 00334000
         SL    @08,@CF05546                                        0777 00335000
         LR    @15,WORKAPTR                                        0777 00336000
         BCTR  @15,0                                               0777 00337000
         STC   @08,UAD(K,@15)                                      0777 00338000
*                       END;                                       0778 00339000
*                     ELSE                                         0779 00340000
*                       DO;                                        0779 00341000
         B     @RC00775                                            0779 00342000
@RF00775 DS    0H                                                  0780 00343000
*                         RC=RCINVCUA;/* INVALID DEVICE ADDR @Z40GKD1*/ 00344000
         MVI   RC(WORKAPTR),X'04'                                  0780 00345000
*                       END;                                       0781 00346000
*                   END;                                           0782 00347000
@RC00775 DS    0H                                                  0783 00348000
*               END;                                               0783 00349000
@RC00770 AL    K,@CF00071                                          0783 00350000
         C     K,@CF00076                                          0783 00351000
         BNH   @DL00769                                            0783 00352000
*               IF RC=0 THEN        /* TEST FOR VALID DEVICE ADDRESS */ 00353000
         CLI   RC(WORKAPTR),0                                      0784 00354000
         BNE   @RF00784                                            0784 00355000
*                 DO;                                              0785 00356000
*                   PACK(DEV16,WKNCSUAD);/* PACK HEX ADDRESS         */ 00357000
         PACK  DEV16(2,WORKAPTR),WKNCSUAD(3,WORKAPTR)              0786 00358000
*                   RFY                                            0787 00359000
*                    (R6,                                          0787 00360000
*                     R7,                                          0787 00361000
*                     R15) RSTD;                                   0787 00362000
*                   R6=DEV16;       /* PUT IN REG 6 FOR IOSGEN MACRO */ 00363000
         LH    R6,DEV16(,WORKAPTR)                                 0788 00364000
         N     R6,@CF05606                                         0788 00365000
*                   R6=R6/16;       /* SHIFT OUT SIGN IN LOW ORDER 4    00366000
*                                      BITS                          */ 00367000
         SRL   R6,4                                                0789 00368000
*                   GEN REFS(CVTILK1,CVTILK2,CVTIXAVL,CVTMAP,IOCOMEX,   00369000
*                       CVTPTR,IOCOM,IOX,IOCUCBBM,IOXHICH);        0790 00370000
              IOSGEN UCBLOOK                                            00371000
*                   IF R15=0 THEN   /* TEST FOR UCB FOUND            */ 00372000
         LTR   R15,R15                                             0791 00373000
         BNZ   @RF00791                                            0791 00374000
*                     DO;                                          0792 00375000
*                       RFY                                        0793 00376000
*                         R6 UNRSTD;                               0793 00377000
*                       RFY                                        0794 00378000
*                         R15 UNRSTD;                              0794 00379000
*                       UCBPTR=R7;  /* IOSGEN MACRO RETURNS UCB ADDR    00380000
*                                      IN REG 7                      */ 00381000
         ST    R7,UCBPTR(,WORKAPTR)                                0795 00382000
*                       RFY                                        0796 00383000
*                         R7 UNRSTD;                               0796 00384000
*                                                                  0796 00385000
*                       /*********************************************/ 00386000
*                       /*                                           */ 00387000
*                       /* LINE ENTRY SHOULD HAVE UCB WITH           */ 00388000
*                       /* COMMUNICATIONS FLAG ON LOCAL DEVICE ENTRY */ 00389000
*                       /* SHOULD HAVE UCB WITH DISPLAY FLAG ON      */ 00390000
*                       /*                                           */ 00391000
*                       /*********************************************/ 00392000
*                                                                  0797 00393000
*                       IF(RPRENTRY=RPRENTLC)&(UCB3DISP='1'B) THEN 0797 00394000
         CLI   RPRENTRY(RDTEPTR),X'45'                             0797 00395000
         BNE   @RF00797                                            0797 00396000
         L     @15,UCBPTR(,WORKAPTR)                               0797 00397000
         TM    UCB3DISP(@15),B'00010000'                           0797 00398000
         BNO   @RF00797                                            0797 00399000
*                         DO;                                      0798 00400000
*                                                                  0798 00401000
*                           /*****************************************/ 00402000
*                           /*                                       */ 00403000
*                           /* UCB WITH DEVICE FLAG ON SHOULD ALSO   */ 00404000
*                           /* HAVE THE FOLLOWING VALUES IN UCBTYP   */ 00405000
*                           /* BYTE 4: 09 - 3277 0A - 3284 0B - 3286 */ 00406000
*                           /* 0C - 3158                             */ 00407000
*                           /*                                       */ 00408000
*                           /*****************************************/ 00409000
*                                                                  0799 00410000
*                           IF(UCBTBYT4>'08'X)&(UCBTBYT4<'0D'X) THEN    00411000
         CLI   UCBTBYT4(@15),X'08'                                 0799 00412000
         BNH   @RF00799                                            0799 00413000
         CLI   UCBTBYT4(@15),X'0D'                                 0799 00414000
         BL    @RT00799                                            0799 00415000
*                             ;                                    0800 00416000
*                           ELSE                                   0801 00417000
*                             DO;                                  0801 00418000
@RF00799 DS    0H                                                  0802 00419000
*                               RC=RCWDTYPE;/* INVALID DEV TYPE    0802 00420000
*                                                            @Z40GKD1*/ 00421000
         MVI   RC(WORKAPTR),X'0C'                                  0802 00422000
*                             END;                                 0803 00423000
*                         END;                                     0804 00424000
*                       ELSE                                       0805 00425000
