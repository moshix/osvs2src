*                                                                  0702 00078000
*       /*************************************************************/ 00079000
*       /*                                                           */ 00080000
*       /* INITIALIZE POINTERS NECESSAY FOR ALLOCATION OR            */ 00081000
*       /* DEALLOCATION USE CIDCTL TO GET NODE OF 370X OR LOCAL      */ 00082000
*       /* DEVICE                                                    */ 00083000
*       /*                                                           */ 00084000
*       /*************************************************************/ 00085000
*                                                                  0703 00086000
*       RFY                                                        0703 00087000
*        (R0,                                                      0703 00088000
*         R1,                                                      0703 00089000
*         R2,                                                      0703 00090000
*         R15) RSTD;                                               0703 00091000
*       R1=NCSPLRPH;                /* RPH FOR CIDCTL                */ 00092000
         L     @14,NCSPLPTR                                        0704 00093000
         L     R1,NCSPLRPH(,@14)                                   0704 00094000
*       R2=RPRCOMID;                /* COMMUNICATION ID FOR CIDCTL   */ 00095000
         L     R2,RPRCOMID(,RDTEPTR)                               0705 00096000
*       DO;                         /* CIDCTL(FIND) LOCK(XCLUSIVE)   */ 00097000
*         RFY                                                      0707 00098000
*          (EMCR2,                                                 0707 00099000
*           EMCR3) RSTD;                                           0707 00100000
*         GEN SETS(RPHWORK)                                        0708 00101000
*             (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH); 0708 00102000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00103000
*         EMCR3='00'X;              /* CLEAR EXTRA BITS              */ 00104000
