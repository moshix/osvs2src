*                     RFY                                          0748 00231000
*                      (EMCR2,                                     0748 00232000
*                       EMCR3) UNRSTD;                             0748 00233000
*                   END;            /* CIDCTL                        */ 00234000
*                   RFY                                            0750 00235000
*                     R2 UNRSTD;                                   0750 00236000
*                   RC=RCWCNTLB;    /* INVALID ENTRY TYPE    @Z40GKD1*/ 00237000
         MVI   RC(WORKAPTR),X'1C'                                  0751 00238000
*                 END;                                             0752 00239000
*             END;                                                 0753 00240000
*         END;                                                     0754 00241000
*       ELSE                                                       0755 00242000
*         DO;                                                      0755 00243000
         B     @RC00716                                            0755 00244000
@RF00716 DS    0H                                                  0756 00245000
*           RC=RCNONCB;             /* NODE NOT OPENED       @Z40GKD1*/ 00246000
         MVI   RC(WORKAPTR),X'2C'                                  0756 00247000
*         END;                                                     0757 00248000
*       IF RC=0 THEN                /* TEST FOR ERROR FROM INIT      */ 00249000
@RC00716 CLI   RC(WORKAPTR),0                                      0758 00250000
         BNE   @RF00758                                            0758 00251000
*         DO;                                                      0759 00252000
*           IF RPRENTRY=RPRENTRN&RRNRBREM='1'B THEN/* FOR REMOTE RN     00253000
*                                      JUST INIT ICNCB       @ZM31611*/ 00254000
