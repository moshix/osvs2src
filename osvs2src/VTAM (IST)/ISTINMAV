*                         END;                                     0735 00193000
*                       ELSE                                       0736 00194000
*                         ;         /* NULL                          */ 00195000
@RF00732 DS    0H                                                  0737 00196000
*                     END;                                         0737 00197000
*                 END;                                             0738 00198000
*               ELSE                                               0739 00199000
*                 DO;               /* NOT 3270 OR 370X RELEASE CID  */ 00200000
*                                                                  0739 00201000
         B     @RC00727                                            0739 00202000
@RF00727 DS    0H                                                  0740 00203000
*                   /*************************************************/ 00204000
*                   /*                                               */ 00205000
*                   /* CIDCTL (FINISH) LOCK(RELEASE)                 */ 00206000
*                   /*                                               */ 00207000
*                   /*************************************************/ 00208000
*                                                                  0740 00209000
*                   R1=NCSPLRPH;    /* SET UP PARAMETERS FOR CIDCTL  */ 00210000
         L     @08,NCSPLPTR                                        0740 00211000
         L     R1,NCSPLRPH(,@08)                                   0740 00212000
*                   RFY                                            0741 00213000
*                     R2 RSTD;                                     0741 00214000
*                   R2=RPRCOMID;                                   0742 00215000
         L     R2,RPRCOMID(,RDTEPTR)                               0742 00216000
*                   DO;             /* CIDCTL(FINISH) LOCK(RELEASE)  */ 00217000
*                     RFY                                          0744 00218000
*                      (EMCR2,                                     0744 00219000
*                       EMCR3) RSTD;                               0744 00220000
*                     GEN SETS(RPHWORK)                            0745 00221000
*(STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);              0745 00222000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00223000
*                     EMCR3='00'X;  /* CLEAR EXTRA BITS              */ 00224000
         SLR   EMCR3,EMCR3                                         0746 00225000
*                     CALL ATCLCPTR->ATCCDFIN->ISTSDCCR;/* PROCESS 0747 00226000
*                                      REQUEST                       */ 00227000
         L     @08,ATCLCPTR                                        0747 00228000
         L     @15,ATCCDFIN(,@08)                                  0747 00229000
