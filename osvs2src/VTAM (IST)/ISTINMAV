*   LDPFX='1'B;                     /* INITIALIZE FIX FLAG           */ 00063000
         OI    LDPFX(@08),B'10000000'                              0697 00064000
*   LDPPDS='VTAMLIB';                                              0698 00065000
         MVI   LDPPDS+7(@08),C' '                                  0698 00066000
         MVC   LDPPDS(7,@08),@CC05532                              0698 00067000
*   RC=0;                           /* INITIALIZE RETURN CODE SWITCH */ 00068000
         MVI   RC(WORKAPTR),X'00'                                  0699 00069000
*   IF RPRENTRY=RPRENTLN THEN                                      0700 00070000
         CLI   RPRENTRY(RDTEPTR),X'50'                             0700 00071000
         BE    @RT00700                                            0700 00072000
*     ;                             /* DON'T ALLOCATE PEP LINES    0701 00073000
*                                                            @ZM33418*/ 00074000
