         TITLE 'ISTINMAV - VS2 ALLOCATION/DEALLOCATION ROUTINE         *00001000
                        '                                               00002000
ISTINMAV CSECT ,                                                   0001 00003000
ISTINCAV DS    0H                                                  0001 00004000
         ENTRY ISTINCAV                                                 00005000
@MAINENT DS    0H                                                  0001 00006000
         USING *,@15                                               0001 00007000
         B     @PROLOG                                             0001 00008000
         DC    AL2(@EP00001-@MAINENT)                                   00009000
         DC    AL1(16)                                             0001 00010000
         DC    C'ISTINMAV  77.069'                                 0001 00011000
         DROP  @15                                                      00012000
@PROLOG  ST    @14,12(,@13)                                        0001 00013000
         STM   @00,@12,20(@13)                                     0001 00014000
         BALR  @12,0                                               0001 00015000
@PSTART  DS    0H                                                  0001 00016000
         USING @PSTART,@12                                         0001 00017000
         AH    @15,4(,@15)                                         0001 00018000
         BR    @15                                                 0001 00019000
@EP00001 DS    0H                                                  0002 00020000
*   REGSAVE=R13;                    /* SAVE PRT TO MY SAVEAREA       */ 00021000
         LR    REGSAVE,R13                                         0681 00022000
*   R13=R13-(LENGTH(SAVAREA));      /* CONSTRUCT SAVEAREA FOR NEXT   */ 00023000
         SL    R13,@CF04038                                        0682 00024000
*   BKPTR=REGSAVE;                  /* POINT SAVEAREA BACK           */ 00025000
         ST    REGSAVE,BKPTR(,R13)                                 0683 00026000
*   FWDPTR=0;                       /* ZERO FORWARD PTR              */ 00027000
         SLR   @11,@11                                             0684 00028000
         ST    @11,FWDPTR(,R13)                                    0684 00029000
*   REGSAVE->FWDPTR=R13;                                           0685 00030000
         ST    R13,FWDPTR(,REGSAVE)                                0685 00031000
*   NCSPLPTR=R1;                    /* SAVE PTR TO NCSPL             */ 00032000
         ST    R1,NCSPLPTR                                         0686 00033000
*   RFY                                                            0687 00034000
*     R1 UNRSTD;                                                   0687 00035000
*   WORKAPTR=WORKA;                 /* SAVE PTR TO WORKAREA          */ 00036000
         L     @11,NCSPLPTR                                        0688 00037000
         L     @15,NCSPLWKA(,@11)                                  0688 00038000
         L     WORKAPTR,WORKA(,@15)                                0688 00039000
*   WORKA=WORKAPTR+(LENGTH(WORKAREA));/* ADJUST PTR FOR MY WORKAREA  */ 00040000
         LA    @08,120                                             0689 00041000
         ALR   @08,WORKAPTR                                        0689 00042000
         ST    @08,WORKA(,@15)                                     0689 00043000
*   RDTEPTR=NCSPLRAD;               /* SAVE PTR TO RDTE IN         0690 00044000
*                                      NON-VOLATILE REG              */ 00045000
         L     RDTEPTR,NCSPLRAD(,@11)                              0690 00046000
*   RFY                                                            0691 00047000
*     ISTATCVT BASED(ATCVTPTR);     /* SET UP ADDRESSABILITY FOR   0691 00048000
*                                      ATCVT                         */ 00049000
*   DO;                             /* LISATCVT                    0692 00050000
*                                      CVTBASE(PAGEABLE,(ATCVTPTR))  */ 00051000
*     ATCVTPTR=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00052000
         L     ATCVTPTR,ATCLCPTR                                   0693 00053000
*   END;                            /* LISATCVT                      */ 00054000
*   LDPRMLST=LDPRMLST&&LDPRMLST;    /* ZERO LDPRMLST FOR LOAD DELETE */ 00055000
         XC    LDPRMLST(28,WORKAPTR),LDPRMLST(WORKAPTR)            0695 00056000
*   LDPDCBA=ATCCONFT->CONDCBBA;     /* INITIALIZE DCB ADDRESS        */ 00057000
         LA    @08,36                                              0696 00058000
         ALR   @08,WORKAPTR                                        0696 00059000
