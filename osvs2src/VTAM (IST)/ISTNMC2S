*              4  NMRESTI BIT(1),      /* RESET IMMEDIATE          0068 00887000
*                                        COMMAND OUTSTANDING         */ 00888000
*              4  NMPURGE BIT(1),      /* PURGE (RESET)              */ 00889000
*              4  NMDSEOC BIT(1),      /* DISCONNECT WITH END OF CALL*/ 00890000
*              4  NMRDBLK BIT(1),      /* READ BLOCK                 */ 00891000
*              4  NMFBDTA BIT(1),      /* LOAD/DUMP I/O REQUEST    0068 00892000
*                                         EXPECTS DATA RETURNED      */ 00893000
*              4  NMSTCTLR BIT(1),     /* START CONTROL RESTART    0068 00894000
*                                         (FUNCTION CODE MUST BE   0068 00895000
*                                         NMFCD01)                   */ 00896000
*              4  NMSTCTLI BIT(1),     /* START CONTROL INITIAL    0068 00897000
*                                         (FUNCTION CODE MUST BE   0068 00898000
*                                         NMFCD01)                   */ 00899000
*              4  NMSTCTLF BIT(1),     /* START CONTROL FINAL      0068 00900000
*                                         (FUNCTION CODE MUST BE   0068 00901000
*                                         NMFCD01)                   */ 00902000
*              4  NMVRYPRG BIT(1),     /* VARY DEACT IMMED ISS PURGE */ 00903000
*              4  NMRSV01 BIT(5),      /* NOT ASSIGNED               */ 00904000
*           3  NMRDTEA PTR(31),        /* END CONTROL TERM RDRE ADDR */ 00905000
*              4  NMNCSPL PTR(31),     /* ADDR OF NCSPL FOR REMOTE 0068 00906000
*                                         LOAD/DUMP SUBTASK          */ 00907000
*        2  NMIPSEQ FIXED(8),          /* INVITE PERPETUAL COUNT     */ 00908000
*        2  NMLRSV06 BIT(8),           /* RESERVED                   */ 00909000
