*   C3SR2=THREE;                    /* GET CSP FOR CON SER           */ 01562000
         LA    C3SR2,3                                             0852 01563000
*   CALL GTSTOR;                    /* RTN TO GET STORAGE            */ 01564000
         BAL   @14,GTSTOR                                          0853 01565000
*   IF C3SR15<EIGHT THEN                                           0854 01566000
         C     C3SR15,@CF00043                                     0854 01567000
         BNL   @RF00854                                            0854 01568000
*     DO;                           /* GOT STORAGE FOR CSP           */ 01569000
*       RPHCSPA=RPHSRPRM;           /* PUT ADDR INTO RPH-NOTE-     0856 01570000
*                                      OVERLAYS RPHWEA FIELD CON SER    01571000
