*                                                                  0576 01917000
*                                                                  0576 01918000
*/***************************************************************    */ 01919000
*/* COMMAND RESET - TEST IF COMMAND WAS RESET IF SO QUEUE            */ 01920000
*/*                 'START CONTROL' OTHERWISE TAKE ACTION            */ 01921000
*/*                 DEPENDING ON STATUS OF PORT                      */ 01922000
*/*   INPUT:  FSBLCPB HAS ADDR OF NMLPB WITH LC (CONTACT) OR         */ 01923000
*/*           LRB (INVITE)                                           */ 01924000
*/*           NCBRDTE HAS ADDR OF PORT RDT                           */ 01925000
*/*                                                                  */ 01926000
*/*   OUTPUT: UNUSUAL CONDITION - FREED NMLPB                        */ 01927000
*/*           IF COMMAND WAS RESET - 'START CONTROL' QUEUED          */ 01928000
*/*           IF PORT IN DIAL-OUT SESSION - FREED NMLPB              */ 01929000
*/*           OTHER - REDRIVE = 2  (IND. TO REDRIVE PORT VIA         */ 01930000
*/*                                            ISTNMC2S)             */ 01931000
*/***************************************************************    */ 01932000
*                                                                  0576 01933000
*CMDRST:                                                           0576 01934000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0576 01935000
         B     @PB00002                                            0576 01936000
CMDRST   DS    0H                                                  0577 01937000
*   C5SR7=C5SR14;                   /* SAVE RETURN ADDR              */ 01938000
         LR    C5SR7,C5SR14                                        0577 01939000
*   RFY                                                            0578 01940000
*     ISTLCPB BASED(FSBLCPB);                                      0578 01941000
*   RFY                                                            0579 01942000
*     ISTNMLPB BASED(FSBLCPB);                                     0579 01943000
*   RFY                                                            0580 01944000
*     ISTFSB BASED(RPHWEA);                                        0580 01945000
*   IF RPTVRYPD=ON                  /* IF VARY PENDING               */ 01946000
*       |FMCDEVOF=ON                /* OR DEVICE GOING OFFLINE       */ 01947000
*       |RPTHLVPD=ON                /* OR HI- ER LEVEL GOING OFF     */ 01948000
*     THEN                                                         0581 01949000
         L     @06,RPHMAJCB-1(,C5SR1)                              0581 01950000
         LA    @06,0(,@06)                                         0581 01951000
         L     @05,FMCDNCBA(,@06)                                  0581 01952000
         L     @05,NCBRDTE(,@05)                                   0581 01953000
         TM    RPTVRYPD(@05),B'01000000'                           0581 01954000
         BO    @RT00581                                            0581 01955000
         TM    FMCDEVOF(@06),B'00010000'                           0581 01956000
         BO    @RT00581                                            0581 01957000
         TM    RPTHLVPD(@05),B'00100000'                           0581 01958000
         BNO   @RF00581                                            0581 01959000
@RT00581 DS    0H                                                  0582 01960000
*     DO;                                                          0582 01961000
*       REDRIVE=C5SONE;             /* FREE NMLPB AND NO REDRIVE     */ 01962000
         LA    REDRIVE,1                                           0583 01963000
*     END;                                                         0584 01964000
*   ELSE                                                           0585 01965000
*     IF FSBMJRCD=FRCMDRST          /* IF COMMAND WAS RESET          */ 01966000
*       THEN                                                       0585 01967000
         B     @RC00581                                            0585 01968000
@RF00581 L     @06,RPHWEA(,C5SR1)                                  0585 01969000
         CLI   FSBMJRCD(@06),X'18'                                 0585 01970000
         BNE   @RF00585                                            0585 01971000
*       DO;                         /*                               */ 01972000
*         NMFNCTCD=NMFCD01;         /* THEN BUILD 'START CONTROL' AND   01973000
*                                      QUEUE CMD TO PSOP             */ 01974000
         L     @06,FSBLCPB(,@06)                                   0587 01975000
         MVI   NMFNCTCD(@06),X'01'                                 0587 01976000
*/*  TPQUE (FSBLCPB,FMCPAB1) NORM                                    */ 01977000
*                                                                  0588 01978000
*         DO;                                                      0588 01979000
*           RFY                                                    0589 01980000
*            (GPR00P,                                              0589 01981000
*             GPR01P,                                              0589 01982000
*             GPR04P,                                              0589 01983000
*             GPR05P,                                              0589 01984000
*             GPR15P) RSTD;                                        0589 01985000
*           GEN REFS(GPR01P) SETS(RPHWORK)                         0590 01986000
*               (STM GPR02P,GPR08P,RPHWORK(GPR01P));               0590 01987000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              01988000
*           GPR00P=0;                                              0591 01989000
         SLR   GPR00P,GPR00P                                       0591 01990000
*           GPR15P=ADDR(FMCPAB1);   /* HOLD PAB PTR                  */ 01991000
         L     GPR15P,RPHMAJCB-1(,C5SR1)                           0592 01992000
         LA    GPR15P,0(,GPR15P)                                   0592 01993000
         LA    GPR15P,FMCPAB1(,GPR15P)                             0592 01994000
*           GPR04P=FSBLCPB;         /* WORK ELE PTR                  */ 01995000
         L     @06,RPHWEA(,C5SR1)                                  0593 01996000
         L     GPR04P,FSBLCPB(,@06)                                0593 01997000
*           GPR05P=GPR15P;          /* PASS PAB PTR                  */ 01998000
         LR    GPR05P,GPR15P                                       0594 01999000
*           CALL ATCLCPTR->ATCAP37->ISTAPC37;                      0595 02000000
         L     @06,ATCLCPTR                                        0595 02001000
         L     @15,ATCAP37(,@06)                                   0595 02002000
         BALR  @14,@15                                             0595 02003000
*           GEN(NOPR  0)                                           0596 02004000
*           NOSETS NOREFS;                                         0596 02005000
         NOPR  0                                                        02006000
*           RFY                                                    0597 02007000
*            (GPR00P,                                              0597 02008000
*             GPR01P,                                              0597 02009000
*             GPR04P,                                              0597 02010000
*             GPR05P,                                              0597 02011000
*             GPR15P) UNRSTD;                                      0597 02012000
*         END;                      /* *** END OF TPQUE GENERATION   */ 02013000
*       END;                        /* TEST FOR DIAL-OUT SESSION     */ 02014000
*     ELSE                                                         0600 02015000
*       IF FSBMJRCD=FRLOGICE        /* LOGIC ERROR IN ACCESS         */ 02016000
*           &FSBMNRCD=FRMVTAME      /* METHOD AND VTAM ERROR         */ 02017000
*         THEN                                                     0600 02018000
         B     @RC00585                                            0600 02019000
@RF00585 L     @06,RPHWEA(,C5SR1)                                  0600 02020000
         CLI   FSBMJRCD(@06),X'08'                                 0600 02021000
         BNE   @RF00600                                            0600 02022000
         CLI   FSBMNRCD(@06),X'A0'                                 0600 02023000
         BNE   @RF00600                                            0600 02024000
*         DO;                                                      0601 02025000
*           RPTASLEP=ON;            /* TURN SLEEP BIT ON IN PORT     */ 02026000
         L     @06,RPHMAJCB-1(,C5SR1)                              0602 02027000
         LA    @06,0(,@06)                                         0602 02028000
         L     @06,FMCDNCBA(,@06)                                  0602 02029000
         L     @06,NCBRDTE(,@06)                                   0602 02030000
         OI    RPTASLEP(@06),B'10000000'                           0602 02031000
*           RPRNSNEA->RPRNSNEA      /* (PORT->GROUP->RN)             */ 02032000
*               ->RRNPTASL=ON;      /* AND IN RN                     */ 02033000
         L     @06,RPRNSNEA(,@06)                                  0603 02034000
         L     @06,RPRNSNEA(,@06)                                  0603 02035000
         OI    RRNPTASL(@06),B'00000001'                           0603 02036000
*           REDRIVE=C5SONE;         /* FREE NMLPB, NO REDRIVE        */ 02037000
         LA    REDRIVE,1                                           0604 02038000
*         END;                                                     0605 02039000
*       ELSE                                                       0606 02040000
*         REDRIVE=C5STWO;           /* SET REDRIVE SW TO 2           */ 02041000
         B     @RC00600                                            0606 02042000
@RF00600 LA    REDRIVE,2                                           0606 02043000
*   C5SR14=C5SR7;                   /* RESTORE RETURN ADDR           */ 02044000
@RC00600 DS    0H                                                  0607 02045000
@RC00585 DS    0H                                                  0607 02046000
@RC00581 LR    C5SR14,C5SR7                                        0607 02047000
*   END CMDRST;                                                    0608 02048000
*                                                                  0608 02049000
