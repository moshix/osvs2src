@MAINENT DS    0H                                                  0001 00005000
         USING *,@15                                               0001 00006000
         B     @PROLOG                                             0001 00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ISTNMC6S  78.158'                                 0001 00009000
         DROP  @15                                                      00010000
@PROLOG  BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
