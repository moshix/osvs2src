*DCL ZCLNG FIXED(15) CONSTANT(172);   /* ZCRR LENGTH                */  02190000
*DCL ZCRSAVNO BIT(8) CONSTANT('06'X); /* NUMBER OF SAVEAREAS IN ZCRR */ 02191000
