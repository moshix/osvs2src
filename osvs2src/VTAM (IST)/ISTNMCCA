SVTM     TITLE 'ISTNMCCA-CLUSTER NETWORK MGR-START CTL OUTBOUND        *00001000
                        '                                               00002000
ISTNMCCA CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTNMCCA  75.130'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*/********************************************************************* 00013000
*/*   ISTNMCCA IS THE START CONTROL OUTBOUND PROCESSOR FOR THE CLUSTER* 00014000
*/* CONTRO LAYER. IT FORMATS WITHIN THE 1ST LCCW SLOT AN INVITE PER-  * 00015000
*/* PETUAL LCCW TO BEGIN GENERAL POLLING OF THE CLUSTER CONTROL UNIT  * 00016000
*/********************************************************************* 00017000
*/* CODE FOR ISTNMCCA BEGINS HERE */                               0303 00018000
* DO;                                                              0303 00019000
*/***  CRRMAINT  PROCNM(ISTNMCCA)COMP(CS)FUNC(SUB)  ***/           0304 00020000
* DCL CMZK0005 PTR(31) REG(1);                                     0304 00021000
* RFY (CMZK0005) RSTD;                                             0305 00022000
*                                                                  0305 00023000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTCSCRR)  */                  0306 00024000
*/* END OF ATBUILD */                                              0306 00025000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0306 00026000
*CMZK0005->RPHCRR->CRACRR->NNMCCA = '1'B;                          0306 00027000
         L     @08,RPHCRR(,CMZK0005)                               0306 00028000
         L     @08,CRACRR(,@08)                                    0306 00029000
         OI    NNMCCA(@08),B'00000010'                             0306 00030000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0307 00031000
*CMZK0005->RPHCRR->CRACRR->CSCCRT = '0'B;                          0307 00032000
         MVI   CSCCRT+1(@08),X'00'                                 0307 00033000
         MVI   CSCCRT(@08),B'00000000'                             0307 00034000
*CMZK0005->RPHCRR->CRACRR->CNMCCA = '1'B;                          0308 00035000
         OI    CNMCCA(@08),B'00000010'                             0308 00036000
* END;                                                             0309 00037000
*RPHSVPTR=RPHPTR; /* SAVE PTR TO RPH */                            0310 00038000
