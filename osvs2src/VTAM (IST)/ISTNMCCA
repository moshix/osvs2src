         OI    LCCWRWOP(@15),B'00011110'                           0317 00067000
*   LCCWSI=CCAON; /* TURN ON SESSIONS INITIATED FLAG */            0318 00068000
*   LCCWSD=CCAON; /* TURN ON SESSIONS DISCONNECT FLAG */           0319 00069000
         OI    LCCWSI(@15),B'11000000'                             0319 00070000
*   LCPNXLCW=ADDR(LCPLCCW); /* NEXT CCW = SLOT 1 */                0320 00071000
