* /*** SET OUR COMPONENT PROCESS ID IN CRR ***/                    0454 00049000
*CMZK0005->RPHCRR->CRACRR->PFCFTA = TIDNMCCI;                      0454 00050000
         MVI   PFCFTA(@08),X'00'                                   0454 00051000
* /*** CLEAR COMPONENT ENTERED INDICATORS ***/                     0455 00052000
*CMZK0005->RPHCRR->CRACRR->CSCNTRD = '0'B;                         0455 00053000
         MVI   CSCNTRD+1(@08),X'00'                                0455 00054000
         MVI   CSCNTRD(@08),B'00000000'                            0455 00055000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0456 00056000
*CMZK0005->RPHCRR->CRACRR->NNMCCI = '1'B;                          0456 00057000
         OI    NNMCCI(@08),B'10000000'                             0456 00058000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0457 00059000
*CMZK0005->RPHCRR->CRACRR->CSCCRT = '0'B;                          0457 00060000
         MVI   CSCCRT+1(@08),X'00'                                 0457 00061000
         MVI   CSCCRT(@08),B'00000000'                             0457 00062000
*CMZK0005->RPHCRR->CRACRR->CNMCCI = '1'B;                          0458 00063000
         OI    CNMCCI(@08),B'10000000'                             0458 00064000
* END;                                                             0459 00065000
*CCISVPTR=ADDR(CSCREGSV); /* GET @ TPMSG SAVE AREA */              0460 00066000
         AL    @15,RPHCRR(,RPHSVPTR)                               0460 00067000
         LA    CCISVPTR,CSCREGSV(,@15)                             0460 00068000
*GEN(USING @DATD,CCISVPTR); /* SET UP ADDR FOR @DATD DSECT */      0461 00069000
         USING @DATD,CCISVPTR                                           00070000
*CCIEXIT=CCICLEAR; /* CLEAR CCIEXIT FLAG */                        0462 00071000
         SLR   CCIEXIT,CCIEXIT                                     0462 00072000
*IF FSBLCPB ^= CCICLEAR THEN  /* IF FSB PTS TO LCPB THEN GO AHEAD */    00073000
         L     @15,RPHWEA(,RPHSVPTR)                               0463 00074000
         L     @15,FSBLCPB(,@15)                                   0463 00075000
         LTR   @15,@15                                             0463 00076000
         BZ    @RF00463                                            0463 00077000
*   DO;                                                            0464 00078000
*   IF LCPTYPE=DCLTYPE THEN /* CHECK IF LCPB IS DCLCPB */          0465 00079000
         CLI   LCPTYPE(@15),X'12'                                  0465 00080000
         BNE   @RF00465                                            0465 00081000
*      DO;                                                         0466 00082000
*      DODCLCPB: ;                                                 0467 00083000
DODCLCPB DS    0H                                                  0468 00084000
*      IF LCCWOP=LXI THEN DVTPTR=ADDR(INIPURGE); /* IF LCCW IS A */     00085000
         L     @15,RPHWEA(,RPHSVPTR)                               0468 00086000
         L     @15,FSBLCCWA(,@15)                                  0468 00087000
         CLI   LCCWOP(@15),X'2B'                                   0468 00088000
         BNE   @RF00468                                            0468 00089000
         LA    DVTPTR,INIPURGE                                     0469 00090000
*                                                /* RESET IMM. */  0470 00091000
*      ELSE DO;                                                    0470 00092000
         B     @RC00468                                            0470 00093000
@RF00468 DS    0H                                                  0471 00094000
*           CALL DOERMSG; /* ELSE ISSUE ERROR MSG */               0471 00095000
         BAL   @14,DOERMSG                                         0471 00096000
*           CCIEXIT=CCIINC; /* INDICATE SHOULD TPEXIT */           0472 00097000
         LA    CCIEXIT,1                                           0472 00098000
*           END;                                                   0473 00099000
*      END;                                                        0474 00100000
*   ELSE DO; /* IF NOT DCLCPB, CHECK FOR NMLPB */                  0475 00101000
         B     @RC00465                                            0475 00102000
@RF00465 DS    0H                                                  0476 00103000
*        IF LCPTYPE=NMLPBTYP THEN /* IF LCPB IS NMLPB THE */       0476 00104000
         L     @15,RPHWEA(,RPHSVPTR)                               0476 00105000
         L     @15,FSBLCPB(,@15)                                   0476 00106000
         CLI   LCPTYPE(@15),X'27'                                  0476 00107000
         BNE   @RF00476                                            0476 00108000
*             /*SEGMENT(DONMLPB)REQUEST*/DO;GOTO DONMLPB;DONMLPB#:END;  00109000
         B     DONMLPB                                             0478 00110000
*        ELSE DO;                                                  0480 00111000
         B     @RC00476                                            0480 00112000
@RF00476 DS    0H                                                  0481 00113000
*             CALL DOERMSG; /* GO ISSUE ERROR MSG */               0481 00114000
         BAL   @14,DOERMSG                                         0481 00115000
*             CCIEXIT=CCIINC; /* INDICATE SHOULD TPEXIT */         0482 00116000
         LA    CCIEXIT,1                                           0482 00117000
*             END;                                                 0483 00118000
*        END; /* END FOR LCPB NOT DCLCPB */                        0484 00119000
*   END; /* END FOR FSBLCPB NOT = 0 */                             0485 00120000
*ELSE DO; /* IF FSBLCPB=0 THEN ERROR */                            0486 00121000
         B     @RC00463                                            0486 00122000
@RF00463 DS    0H                                                  0487 00123000
*     CALL DOERMSG;                                                0487 00124000
         BAL   @14,DOERMSG                                         0487 00125000
*     CCIEXIT=CCIINC; /* INDICATE MUST TPEXIT */                   0488 00126000
         LA    CCIEXIT,1                                           0488 00127000
*     END;                                                         0489 00128000
*IF CCIEXIT=CCICLEAR THEN /* IF MUST TPEXIT FLAG NOT ON, PROCESS */     00129000
@RC00463 LTR   CCIEXIT,CCIEXIT                                     0490 00130000
         BNZ   @RF00490                                            0490 00131000
*   DO;                                                            0491 00132000
*   RPHPTR=RPHSVPTR;                                               0492 00133000
*                                                                  0492 00134000
         LR    RPHPTR,RPHSVPTR                                     0492 00135000
