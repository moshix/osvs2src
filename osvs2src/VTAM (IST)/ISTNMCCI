SVTM     TITLE 'ISTNMCCI-CLUSTER NETWORK MANAGER INBOUND PROCESSOR     *00001000
                        '                                               00002000
ISTNMCCI CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTNMCCI  77.085'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*/********************************************************************* 00013000
*/*   ISTNMCCI IS THE INBOUND ROUTER FOR THE CLUSTER CONTROL LAYER.   * 00014000
*/* IT ROUTES FEEDBACK FROM TPIOS TO THE APPROPRIATE PROCESSING RTN.  * 00015000
*/*   IF THE INPUT LCPB IS A DCLCP & THE LCCW OP CODE INDICATES       * 00016000
*/* RESET IMMEDIATE, ISTNMCCI WILL TPDVTS TO CONTROL LAYER INBOUND    * 00017000
*/* PURGE.                                                            * 00018000
*/*   IF THE LCPB IS AN NMLPB, DOMANAL IS CALLED TO PROCESS THE NMLPB.* 00019000
*/********************************************************************* 00020000
*RPHSVPTR=RPHPTR; /* SAVE PTR TO RPH */                            0446 00021000
         LR    RPHSVPTR,RPHPTR                                     0446 00022000
*/* UPDATE CRA/CRR PTRS */                                         0447 00023000
* DO;                                                              0447 00024000
*/***  CRRMAINT  PROCNM(ISTNMCCI)COMP(CS)  ***/                    0448 00025000
* DCL CMZK0005 PTR(31) REG(1);                                     0448 00026000
* RFY (CMZK0005) RSTD;                                             0449 00027000
*                                                                  0449 00028000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTCSCRR)  */                  0450 00029000
*/* END OF ATBUILD */                                              0450 00030000
* /*** SAVE CRR AND FRR POINTERS ***/                              0450 00031000
*ADDR(CMZK0005->RPHCRR->CRAPROCR)->PFCCRRO = CMZK0005->RPHCRR->CRACRR;  00032000
         LA    @15,312                                             0450 00033000
         L     @11,RPHCRR(,CMZK0005)                               0450 00034000
         LR    @08,@11                                             0450 00035000
         ALR   @08,@15                                             0450 00036000
         L     @06,CRACRR(,@11)                                    0450 00037000
         ST    @06,PFCCRRO(,@08)                                   0450 00038000
*ADDR(CMZK0005->RPHCRR->CRAPROCR)->PFCFRRO = CMZK0005->RPHCRR->CRAFRR;  00039000
         L     @06,CRAFRR(,@11)                                    0451 00040000
         ST    @06,PFCFRRO(,@08)                                   0451 00041000
* /*** SET UP POINTERS TO OUR CRR, FRR ***/                        0452 00042000
*CMZK0005->RPHCRR->CRACRR = ADDR(CMZK0005->RPHCRR->CRAPROCR);      0452 00043000
         LA    @08,CRAPROCR(,@11)                                  0452 00044000
         ST    @08,CRACRR(,@11)                                    0452 00045000
*CMZK0005->RPHCRR->CRAFRR = 0;                                     0453 00046000
