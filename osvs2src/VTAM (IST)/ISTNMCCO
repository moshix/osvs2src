         CLI   NMFNCTCD(@15),X'02'                                 0425 00109000
         BNE   @RF00425                                            0425 00110000
*                       DVTPTR=ADDR(INOREDRV); /* GET @REDRV NAME */    00111000
         LA    DVTPTR,INOREDRV                                     0426 00112000
*                    ELSE DVTPTR=ADDR(INOENCTL); /* MUST BE END CTL */  00113000
         B     @RC00425                                            0427 00114000
@RF00425 LA    DVTPTR,INOENCTL                                     0427 00115000
*                    END; /* END FOR FUNC NOT START CTL */         0428 00116000
*               END; /* END IF CLAUSE FOR DEAD CONNECTION */       0429 00117000
*             ELSE DO;               /* CONNECTION DEAD       @ZA10431* 00118000
         B     @RC00420                                            0430 00119000
@RF00420 DS    0H                                                  0431 00120000
*             CCOEXIT=CCOSETON;      /* INDICATE MUST TPEXIT  @ZA10431* 00121000
         LA    CCOEXIT,1                                           0431 00122000
*             FMCLUSTR=CCOOFF;       /* START GEN POLL WHEN   @ZA10431* 00123000
