         LR    @00,@08                                             0331 00068000
         XR    @00,@08                                             0331 00069000
         ST    @00,LCPFDBA(,@11)                                   0331 00070000
*     LCPTLBAD=LCPTLBAD&&LCPTLBAD;                         /*@ZA18452*/ 00071000
         L     @08,LCPTLBAD(,@11)                                  0332 00072000
         LR    @00,@08                                             0332 00073000
         XR    @00,@08                                             0332 00074000
         ST    @00,LCPTLBAD(,@11)                                  0332 00075000
*     LCPFOSN=LCPFOSN&&LCPFOSN;                            /*@ZA18452*/ 00076000
         XC    LCPFOSN(2,@11),LCPFOSN(@11)                         0333 00077000
*     LCPFSNCH=LCPFSNCH&&LCPFSNCH;                         /*@ZA18452*/ 00078000
         LH    @08,LCPFSNCH(,@11)                                  0334 00079000
         N     @08,@CF03185                                        0334 00080000
         LR    @00,@08                                             0334 00081000
         XR    @00,@08                                             0334 00082000
         STH   @00,LCPFSNCH(,@11)                                  0334 00083000
*     LCPSTYPE=0;                                          /*@ZA18452*/ 00084000
         MVI   LCPSTYPE(@11),X'00'                                 0335 00085000
*     LCPFLG1=0;                                           /*@ZA18452*/ 00086000
         MVI   LCPFLG1(@11),X'00'                                  0336 00087000
*     LCPLCCW=LCPLCCW&&LCPLCCW;                                    0337 00088000
         XC    LCPLCCW(8,@11),LCPLCCW(@11)                         0337 00089000
*     NMLCW2=NMLCW2&&NMLCW2;                                       0338 00090000
         XC    NMLCW2(8,@11),NMLCW2(@11)                           0338 00091000
*     NMLCW3=NMLCW3&&NMLCW3;                                       0339 00092000
         XC    NMLCW3(8,@11),NMLCW3(@11)                           0339 00093000
*     IF PABERLCK=CCRON THEN /* CHECK IF PAB ERR LOCK ON */        0340 00094000
         L     @11,RPHMAJCB-1(,RPHSVPTR)                           0340 00095000
         TM    PABERLCK+96(@11),B'00000100'                        0340 00096000
         BNO   @RF00340                                            0340 00097000
*        DO;                                                       0341 00098000
*        ADDR(NMLCW2)->LCCWOP=LXEL; /* SET UP RESET ERROR LOCK */  0342 00099000
         L     @11,RPHWEA(,RPHSVPTR)                               0342 00100000
         LA    @08,56                                              0342 00101000
         ALR   @08,@11                                             0342 00102000
         MVI   LCCWOP(@08),X'0B'                                   0342 00103000
*                                   /* LCCW IN NMLPB */            0343 00104000
*        ADDR(NMLCW2)->LCCWCC=CCRON; /* SET CHAIN CMD BIT ON */    0343 00105000
         OI    LCCWCC(@08),B'01000000'                             0343 00106000
*        ADDR(NMLCW3)->LCCWRWOP=LRPEP; /* INDICATE READ PERPETUAL */    00107000
         LA    @08,64                                              0344 00108000
         ALR   @08,@11                                             0344 00109000
         NI    LCCWRWOP(@08),B'11011110'                           0344 00110000
         OI    LCCWRWOP(@08),B'00011110'                           0344 00111000
*                                    /* IN 3RD SLOT */             0345 00112000
*        ADDR(NMLCW3)->LCCWSI=CCRON; /* TURN ON SEESIONS INITIATE  0345 00113000
*                                       FLAG */                    0345 00114000
*        ADDR(NMLCW3)->LCCWSD=CCRON; /* TURN ON SESSIONS DISCONN */     00115000
         OI    LCCWSI(@08),B'11000000'                             0346 00116000
*        LCPNXLCW=ADDR(NMLCW2); /* PT NEXT LCCW AD TO SLOT 2 */    0347 00117000
         LA    @08,NMLCW2(,@11)                                    0347 00118000
         ST    @08,LCPNXLCW(,@11)                                  0347 00119000
*        RPHPTR=RPHSVPTR; /* PUT RPH @ IN REG 1 */                 0348 00120000
         LR    RPHPTR,RPHSVPTR                                     0348 00121000
*        /* TPQUE THE NMLPB TO TPIOS OUTBOUND PAB */               0349 00122000
*                                                                  0349 00123000
*/*  TPQUE (RPHWEA,FMCPAB3) RESET */                               0349 00124000
*                                                                  0349 00125000
*         DO;                                                      0349 00126000
*                                                                  0349 00127000
*         RFY ISTDYPAB;                                            0350 00128000
*         RFY ISTATCVT;                                            0351 00129000
*         DCL GPR00F FIXED(31) REG(00);                            0352 00130000
*         DCL GPR00P PTR(31)   REG(00);                            0353 00131000
*         DCL GPR01F FIXED(31) REG(01);                            0354 00132000
*         DCL GPR01P PTR(31)   REG(01);                            0355 00133000
*         DCL GPR02F FIXED(31) REG(02);                            0356 00134000
*         DCL GPR02P PTR(31)   REG(02);                            0357 00135000
*         DCL GPR15F FIXED(31) REG(15);                            0358 00136000
*         DCL GPR15P PTR(31)   REG(15);                            0359 00137000
*         RFY (GPR00P,GPR01P,GPR02P,GPR15P) RSTD;                  0360 00138000
*         DCL GPR09F FIXED(31) REG(09);                            0361 00139000
*         DCL GPR09P PTR(31)   REG(09);                            0362 00140000
*         DCL GPR14F FIXED(31) REG(14);                            0363 00141000
*         DCL GPR14P PTR(31)   REG(14);                            0364 00142000
*         GEN REFS(GPR01P) SETS(RPHWORK)                           0365 00143000
*        (STM   GPR14P,GPR09P,RPHWORK(GPR01P));                    0365 00144000
         STM   GPR14P,GPR09P,RPHWORK(GPR01P)                            00145000
*         GPR00P=ADDR(FMCPAB3);                                    0366 00146000
         L     @14,RPHMAJCB-1(,RPHSVPTR)                           0366 00147000
         LA    @14,0(,@14)                                         0366 00148000
         LA    GPR00P,FMCPAB3(,@14)                                0366 00149000
*         GPR02P=RPHWEA;                                           0367 00150000
         L     GPR02P,RPHWEA(,RPHSVPTR)                            0367 00151000
*         CALL ATCLCPTR->ATCAP34->ISTAPC34;                        0368 00152000
         L     @11,ATCLCPTR                                        0368 00153000
         L     @15,ATCAP34(,@11)                                   0368 00154000
         BALR  @14,@15                                             0368 00155000
*         GEN REFS(RPHWORK,GPR01P) NOSETS                          0369 00156000
*        (LM    GPR14P,GPR09P,RPHWORK(GPR01P));                    0369 00157000
         LM    GPR14P,GPR09P,RPHWORK(GPR01P)                            00158000
*         RFY (GPR00P,GPR01P,GPR02P,GPR15P) UNRSTD;                0370 00159000
*         END; /**** END OF TPQUE GENERATION ****/                 0371 00160000
*        END; /* END FOR PABERLCK =1 */                            0372 00161000
*      ELSE DO;                                                    0373 00162000
         B     @RC00340                                            0373 00163000
@RF00340 DS    0H                                                  0374 00164000
*           LCCWRWOP=LRPEP; /* INDICATE READ PERPETUAL IN SLOT 1 */     00165000
         L     @11,RPHWEA(,RPHSVPTR)                               0374 00166000
