SVTM     TITLE 'ISTNMCCR - CLUSTER REDRIVE OUTBOUND PROCESSOR          *00001000
                        '                                               00002000
ISTNMCCR CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTNMCCR  77.087'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*/********************************************************************* 00013000
*/*   ISTNMCCR IS THE REDRIVE OUTBOUND PROCESSOR FOR THE CLUSTER      * 00014000
*/* CONTRO LAYER. IT DETERMINES IF THE CLUSTER CONTROL LAYER SHOULD   * 00015000
*/* ATTEMPT GENERAL POLLING OF THE CLUSTER CONTROL UNIT.              * 00016000
*/*    IF GENERAL POLLING IS TO BE RETRIED, IT DOES THE FOLLOWING:    * 00017000
*/* IF THE PABERLCK BIT IS SET, ISTNMCCR FORMATS A RESET ERROR LOCK   * 00018000
*/* LCCW IN THE 2ND LCCW SLOT OF THE NMLPB AND CHAINS A READ PERPET-  * 00019000
*/* UAL LCCW TO IT. OTHERWISE, IT JUST FORMATS A READ PERPETUAL LCCW  * 00020000
*/* IN THE 1ST LCCW SLOT.                                             * 00021000
*/********************************************************************* 00022000
*/* CODE FOR ISTNMCCR BEGINS HERE */                               0315 00023000
* DO;                                                              0315 00024000
*/***  CRRMAINT  PROCNM(ISTNMCCR)COMP(CS)FUNC(SUB)  ***/           0316 00025000
* DCL CMZK0005 PTR(31) REG(1);                                     0316 00026000
* RFY (CMZK0005) RSTD;                                             0317 00027000
*                                                                  0317 00028000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTCSCRR)  */                  0318 00029000
*/* END OF ATBUILD */                                              0318 00030000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0318 00031000
*CMZK0005->RPHCRR->CRACRR->NNMCCR = '1'B;                          0318 00032000
         L     @11,RPHCRR(,CMZK0005)                               0318 00033000
         L     @11,CRACRR(,@11)                                    0318 00034000
         OI    NNMCCR(@11),B'00000001'                             0318 00035000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0319 00036000
*CMZK0005->RPHCRR->CRACRR->CSCCRT = '0'B;                          0319 00037000
         MVI   CSCCRT+1(@11),X'00'                                 0319 00038000
         MVI   CSCCRT(@11),B'00000000'                             0319 00039000
*CMZK0005->RPHCRR->CRACRR->CNMCCR = '1'B;                          0320 00040000
         OI    CNMCCR(@11),B'00000001'                             0320 00041000
* END;                                                             0321 00042000
*RPHSVPTR=RPHPTR; /* SAVE PTR TO RPH */                            0322 00043000
         LR    RPHSVPTR,RPHPTR                                     0322 00044000
*FREEREG=CCRINC; /* INDICATE NMLPB SHOULD BE FREED */              0323 00045000
         LA    FREEREG,1                                           0323 00046000
*IF FMCLUSTR>CCRCLEAR THEN /* IS ST CTL COUNT > 0 ? */             0324 00047000
         L     @11,RPHMAJCB-1(,RPHSVPTR)                           0324 00048000
         LA    @11,0(,@11)                                         0324 00049000
         CLI   FMCLUSTR(@11),0                                     0324 00050000
         BNH   @RF00324                                            0324 00051000
*  IF NMIPSEQ=FMCIPSEQ THEN /* IF ID OF NMLPB MATCHES ID OF FMCB */     00052000
         L     @08,RPHWEA(,RPHSVPTR)                               0325 00053000
         CLC   NMIPSEQ(1,@08),FMCIPSEQ(@11)                        0325 00054000
         BNE   @RF00325                                            0325 00055000
*     DO;                                                          0326 00056000
*     FREEREG=FREEREG&&FREEREG; /* INDICATE SHOULD NOT FREE NMLPB */    00057000
         XR    FREEREG,FREEREG                                     0327 00058000
*     DOREDRIV: ;                                                  0328 00059000
DOREDRIV DS    0H                                                  0329 00060000
*     LCPFLAGS=LCPFLAGS&&LCPFLAGS; /* CLEAR LCPB FIELDS */         0329 00061000
         L     @11,RPHWEA(,RPHSVPTR)                               0329 00062000
         XC    LCPFLAGS(1,@11),LCPFLAGS(@11)                       0329 00063000
*     LCPMNRCD=LCPMNRCD&&LCPMNRCD;                                 0330 00064000
         XC    LCPMNRCD(1,@11),LCPMNRCD(@11)                       0330 00065000
*     LCPFDBA=LCPFDBA&&LCPFDBA;                                    0331 00066000
