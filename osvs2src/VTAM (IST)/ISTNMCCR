         OI    LCCWSI(@08),B'11000000'                             0376 00173000
*           LCPNXLCW=ADDR(LCPLCCW); /* NEXT LCCW ADD=SLOT 1 */     0377 00174000
         LA    @08,LCPLCCW(,@11)                                   0377 00175000
         ST    @08,LCPNXLCW(,@11)                                  0377 00176000
*           RPHPTR=RPHSVPTR; /* PUT RPH @ REG 1 */                 0378 00177000
         LR    RPHPTR,RPHSVPTR                                     0378 00178000
*           /* TPQUE NMLPB TO TPIOS OUTBOUND PAB */                0379 00179000
*                                                                  0379 00180000
*/*  TPQUE (RPHWEA,FMCPAB3) NORM */                                0379 00181000
*                                                                  0379 00182000
*            DO;                                                   0379 00183000
