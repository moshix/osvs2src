         LA    @14,0(,@14)                                         0405 00061000
         LH    CIDREG,FMCSAF(,@14)                                 0405 00062000
         N     CIDREG,@CF03934                                     0405 00063000
*SLL(CIDREG,CCTSHIFT); /* SHIFT SAF TO HIGH BYTES */               0406 00064000
         SLR   @06,@06                                             0406 00065000
         IC    @06,CCTSHIFT                                        0406 00066000
         SLL   CIDREG,0(@06)                                       0406 00067000
*CIDREG=CIDREG | NCBDAF; /* GET DAF IN LOW BYTES */                0407 00068000
         L     @14,FMCDNCBA(,@14)                                  0407 00069000
         LH    @14,NCBDAF(,@14)                                    0407 00070000
         N     @14,@CF03934                                        0407 00071000
         OR    CIDREG,@14                                          0407 00072000
*CIDSAVE=CIDREG; /* SAVE CID FOR CID FINISH */                     0408 00073000
*                                                                  0408 00074000
         LR    CIDSAVE,CIDREG                                      0408 00075000
* DO; /* CIDCTL(FIND)  */                                          0409 00076000
*                                                                  0409 00077000
* RFY (EMCR2,EMCR3) RSTD;                                          0410 00078000
*                                                                  0410 00079000
* RFY ISTDYPAB;                                                    0411 00080000
* RFY ISTATCVT;                                                    0412 00081000
*   GEN SETS(RPHWORK)                                              0413 00082000
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0413 00083000
