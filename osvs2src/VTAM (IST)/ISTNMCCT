SVTM     TITLE 'ISTNMCCT-CLUSTER START CONTROL REDRIVE PROCESSOR       *00001000
                        '                                               00002000
ISTNMCCT CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTNMCCT  75.130'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*/* CODE FOR ISTNMCCT BEGINS HERE */                               0392 00013000
*/********************************************************************* 00014000
*/*   ISTNMCCT IS THE INBOUND START CONTROL REDRIVE PROCESSOR. IT     * 00015000
*/* FIRST DOES A CID FIND FOR THE DAF LOCK & IF CID FIND FAILS IT     * 00016000
*/* ISSUES AN ERROR MESSAGE. IF NOT, IT CHECKS FOR CLOSE DST IN PROG- * 00017000
*/* RESS & CONNECTION DEAD. IF NEITHER CONDITION IS THE CASE,         * 00018000
*/* ISTNMCCT WILL TPQUE THE NMLPB TO THE CCLOP CAUSING ISTNMCCO TO    * 00019000
*/* ROUTE CONTROL TO ISTNMCCR, CLUSTER CONTROL LAYER OUTBOUND REDRIVE * 00020000
*/* PROCESSOR. IF ONE OF THE ABOVE TWO CONDITIONS OBTAINS, ISTNMCCT   * 00021000
*/* WILL RELEASE THE NMLPB AND TPEXIT.                                * 00022000
*/********************************************************************* 00023000
* DO;                                                              0392 00024000
*/***  CRRMAINT  PROCNM(ISTNMCCT)COMP(CS)FUNC(SUB)  ***/           0393 00025000
* DCL CMZK0005 PTR(31) REG(1);                                     0393 00026000
* RFY (CMZK0005) RSTD;                                             0394 00027000
*                                                                  0394 00028000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTCSCRR)  */                  0395 00029000
*/* END OF ATBUILD */                                              0395 00030000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0395 00031000
*CMZK0005->RPHCRR->CRACRR->NNMCCT = '1'B;                          0395 00032000
         L     @14,RPHCRR(,CMZK0005)                               0395 00033000
         L     @14,CRACRR(,@14)                                    0395 00034000
         OI    NNMCCT(@14),B'00100000'                             0395 00035000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0396 00036000
*CMZK0005->RPHCRR->CRACRR->CSCCRT = '0'B;                          0396 00037000
         MVI   CSCCRT+1(@14),X'00'                                 0396 00038000
         MVI   CSCCRT(@14),B'00000000'                             0396 00039000
*CMZK0005->RPHCRR->CRACRR->CNMCCT = '1'B;                          0397 00040000
         OI    CNMCCT(@14),B'00100000'                             0397 00041000
* END;                                                             0398 00042000
*RPHSVPTR=RPHPTR; /* SAVE PTR TO RPH */                            0399 00043000
         LR    RPHSVPTR,RPHPTR                                     0399 00044000
*CCTSVPTR=ADDR(CSCREGSV); /* SET UP SAVE AREA PTR FOR TPMSG */     0400 00045000
         L     CCTSVPTR,RPHCRR(,RPHSVPTR)                          0400 00046000
         LA    CCTSVPTR,CSCREGSV+312(,CCTSVPTR)                    0400 00047000
*/* CLEAR FIELDS IN NMLPB */                                       0401 00048000
*LCPLCCW=LCPLCCW&&LCPLCCW;                                         0401 00049000
         L     @14,RPHWEA(,RPHSVPTR)                               0401 00050000
         XC    LCPLCCW(8,@14),LCPLCCW(@14)                         0401 00051000
*NMLCW2=NMLCW2&&NMLCW2;                                            0402 00052000
