*CCBASYN@:                                                         0378 00398000
*   END;                                                           0378 00399000
CCBASYN@ DS    0H                                                  0379 00400000
*                                                                  0379 00401000
*/********************************************************************/ 00402000
*/*                                                                  */ 00403000
*/*   ROUTINE NAME : CCBDEBC                                         */ 00404000
*/*                                                                  */ 00405000
*/*   FUNCTION : THIS SUBROUTINE TERMINATES PSS VIA THE APSTERM MACRO*/ 00406000
*/*              AND FREES THE DEB VIA THE FREESTOR MACRO.           */ 00407000
*/*                                                                  */ 00408000
*/*   ROUTINES CALLED : NONE                                         */ 00409000
*/*                                                                  */ 00410000
*/*   MACROS USED : APSTERM - TERMINATE PSS                          */ 00411000
*/*                 FREESTOR - FREE THE DEB                          */ 00412000
*/*                                                                  */ 00413000
*/********************************************************************/ 00414000
*                                                                  0379 00415000
*   /*****************************************************************/ 00416000
*   /*                                                               */ 00417000
*   /* SEGMENT(CCBDEBC)START                                         */ 00418000
*   /*                                                               */ 00419000
*   /*****************************************************************/ 00420000
*                                                                  0379 00421000
*   DO;                                                            0379 00422000
*     GOTO CCBDEBC@;                                               0380 00423000
         B     CCBDEBC@                                            0380 00424000
*CCBDEBC:                                                          0381 00425000
*   END;                                                           0381 00426000
CCBDEBC  DS    0H                                                  0382 00427000
*   REG1=DEBREG;                    /* INPUT TO APSTERM              */ 00428000
         LR    REG1,DEBREG                                         0382 00429000
*/*** start -apsterm- macro call **                                  */ 00430000
*                                                                  0383 00431000
*   DO;                                                            0383 00432000
*     RFY                                                          0384 00433000
*      (GPR01P,                                                    0384 00434000
*       GPR15P) RSTD;                                              0384 00435000
*                                                                  0384 00436000
*/* ATBUILD (ISTDYPAB,ISTATCVT)                                      */ 00437000
*/* END OF ATBUILD                                                   */ 00438000
*                                                                  0385 00439000
*     DO;                           /* LISATCVT                    0385 00440000
