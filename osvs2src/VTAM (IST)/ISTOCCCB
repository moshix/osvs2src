         LA 13,72(13)      REG 13 EQUALS ADDR OCASAV3                   00015000
         ST 13,8(9)        SAVE AREA FWD PTR                            00016000
         ST 9,4(13)        SAVE AREA BKWD PTR                           00017000
*   RWORK=REG1;                     /* SET UP WA BASE REG            */ 00018000
         LR    RWORK,REG1                                          0252 00019000
*   DEBREG=OCADEBAD;                /* DEB ADDR                      */ 00020000
         L     DEBREG,OCADEBAD(,RWORK)                             0253 00021000
*                                                                  0254 00022000
*/*VMDESET  ((15),ZERO)                                              */ 00023000
