*/*                                                                  */ 00195000
*/*   ROUTINE NAME : CCBASYN                                         */ 00196000
*/*                                                                  */ 00197000
*/*   FUNCTION : THIS SUBROUTINE INITIALIZES A WORK ELEMENT USING THE*/ 00198000
*/*              RPL DSECT.  IT THEN SCHEDULES ISTOCCCC (ASYNCHRONOUS*/ 00199000
*/*              CLOSE ACB) AND WAITS FOR ISTOCCCC TO COMPLETE       */ 00200000
*/*              PROCESSING.                                         */ 00201000
*/*                                                                  */ 00202000
*/*   ROUTINES CALLED : NONE                                         */ 00203000
*/*                                                                  */ 00204000
*/*   MACROS USED : TPQUE - SCHEDULE ISTOCCCC                        */ 00205000
*/*                 VWAIT - WAIT TO BE POSTED BY ISTOCCCC            */ 00206000
*/*                                                                  */ 00207000
*/********************************************************************/ 00208000
*                                                                  0314 00209000
*   /*****************************************************************/ 00210000
*   /*                                                               */ 00211000
*   /* SEGMENT(CCBASYN)START                                         */ 00212000
*   /*                                                               */ 00213000
*   /*****************************************************************/ 00214000
*                                                                  0314 00215000
*   DO;                                                            0314 00216000
*     GOTO CCBASYN@;                                               0315 00217000
*CCBASYN:                                                          0316 00218000
*   END;                                                           0316 00219000
CCBASYN  DS    0H                                                  0317 00220000
*   IF ATCLCPTR->ATCABEN=ON         /* IF VTAM SHUTTING DOWN @X60CVDU*/ 00221000
*     THEN                                                         0317 00222000
         L     @08,ATCLCPTR                                        0317 00223000
         TM    ATCABEN(@08),B'10000000'                            0317 00224000
         BNO   @RF00317                                            0317 00225000
*     OCARC='50'X;                  /* BYPASS PROCESSING     @X60CVDU*/ 00226000
         MVI   OCARC(RWORK),X'50'                                  0318 00227000
*   ELSE                                                           0319 00228000
*     DO;                           /*                       @X60CVDU*/ 00229000
         B     @RC00317                                            0319 00230000
@RF00317 DS    0H                                                  0320 00231000
*       DEBREG=OCADEBAD;            /* DEB BASE REG                  */ 00232000
         L     DEBREG,OCADEBAD(,RWORK)                             0320 00233000
*       RESPECIFY                                                  0321 00234000
*         RPLREG RSTD;                                             0321 00235000
*       RPLREG=ADDR(OCASAV3);       /* SET UP                        */ 00236000
         LA    RPLREG,OCASAV3(,RWORK)                              0322 00237000
*       RPLREG=RPLREG+8;            /* RPL BASE REG                  */ 00238000
         AH    RPLREG,@CH00036                                     0323 00239000
*       RPLID='C3'X;                /* SET RPLID TO CHAR 'C'         */ 00240000
         MVI   RPLID(RPLREG),X'C3'                                 0324 00241000
*       RPLLEN='0030'X;             /* SET RPL LENGTH FIELD          */ 00242000
         MVI   RPLLEN(RPLREG),X'30'                                0325 00243000
*       RPLPLHPT=0;                 /* SET CHAIN FIELD TO 0          */ 00244000
         SLR   @08,@08                                             0326 00245000
         ST    @08,RPLPLHPT(,RPLREG)                               0326 00246000
*       RPLARG=RWORK;               /* ADDRESS OF OCA                */ 00247000
         ST    RWORK,RPLARG(,RPLREG)                               0327 00248000
*       RPLREQ='21'X;               /* SET REQUEST TYPE BYTE         */ 00249000
         MVI   RPLREQ(RPLREG),X'21'                                0328 00250000
*       RPLDACB=ACDACB;             /* ADDR USER'S ACB               */ 00251000
         SLR   @00,@00                                             0329 00252000
         ICM   @00,7,ACDACB(DEBREG)                                0329 00253000
         ST    @00,RPLDACB(,RPLREG)                                0329 00254000
*       REG9=DEBREG;                /* SAVE DEB BASE REG             */ 00255000
         LR    REG9,DEBREG                                         0330 00256000
*       RESPECIFY                                                  0331 00257000
*         ISTACDEB BASED(REG9);     /* DEB ADDRESSABILITY            */ 00258000
*       ADDR(ACDSSPAB)->PABDVTA=ADDR(PSAATCVT->ATCOCRT);/* INSURE  0332 00259000
*                                      GOOD ADDR             @ZM32036*/ 00260000
         L     @15,PSAATCVT                                        0332 00261000
         LA    @00,ATCOCRT(,@15)                                   0332 00262000
         STCM  @00,7,PABDVTA+104(REG9)                             0332 00263000
*       REG10=RWORK;                /* SAVE WORK AREA BASE           */ 00264000
         LR    REG10,RWORK                                         0333 00265000
*       REG11=RPLREG;               /* WORK ELEM ADDR FOR TPQ        */ 00266000
         LR    REG11,RPLREG                                        0334 00267000
*       RPLECB=0;                   /* INITIALIZE ECB IN WA          */ 00268000
         ST    @08,RPLECB(,RPLREG)                                 0335 00269000
*       RESPECIFY                                                  0336 00270000
*         RPLREG UNRSTD;                                           0336 00271000
*       RFY                                                        0337 00272000
*        (RWORK,                                                   0337 00273000
