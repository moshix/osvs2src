*                                                                  0971 01348000
*/********************************************************************/ 01349000
*/*                                                                  */ 01350000
*/*   ROUTINE NAME : DOICERL                                         */ 01351000
*/*                                                                  */ 01352000
*/*   FUNCTION : THIS SUBROUTINE RELEASES ALL QUEUED CONNECTION RE-  */ 01353000
*/*              QUESTS FOR THE APPLICATION.  THIS IS DONE BY PROCES-*/ 01354000
*/*              SING THE THREE ICE QUEUES IN THE APPLICATION RDT    */ 01355000
*/*              (THE ACQUIRE ICE QUEUE, THE LOGON ICE QUEUE, AND THE*/ 01356000
*/*              ACCEPT ICE QUEUE).  THE ACQUIRE ICE'S & LOGON ICE'S */ 01357000
*/*              ARE PROCESSED BY CALLING CLOSEDST.  THE ACCEPT ICE'S*/ 01358000
*/*              ARE PROCESSED BY POSTING THE RPH ASSOCIATED WITH    */ 01359000
*/*              EACH ICE AND FREESTORING EACH ICE.                  */ 01360000
*/*                                                                  */ 01361000
*/*   ROUTINES CALLED : NONE                                         */ 01362000
*/*                                                                  */ 01363000
*/*   MACROS USED : TPPOST - TO POST RPH FOR EACH ACCEPT ICE         */ 01364000
*/*                 FREESTOR - TO FREESTOR EACH ACCEPT ICE           */ 01365000
*/*                                                                  */ 01366000
*/********************************************************************/ 01367000
*                                                                  0971 01368000
*    /*SEGMENT(DOICERL)START*/DO;GOTO DOICERL@;DOICERL:END;        0971 01369000
         B     DOICERL@                                            0972 01370000
DOICERL  DS    0H                                                  0974 01371000
*   RFY                                                            0974 01372000
*     REG9 RSTD;                                                   0974 01373000
*   REG9=ADDR(RAPICEAQ);            /* ADDR ANCHOR                   */ 01374000
         LA    REG9,RAPICEAQ(,RDTREG)                              0975 01375000
*   CALL DOCLSQ;                    /* CLSDST                        */ 01376000
         BAL   @14,DOCLSQ                                          0976 01377000
*   IF OCAABORT=OFF                 /* IF SUCCESSFUL                 */ 01378000
*     THEN                                                         0977 01379000
         TM    OCAABORT(REG2),B'00100000'                          0977 01380000
         BNZ   @RF00977                                            0977 01381000
*     DO;                                                          0978 01382000
*       REG9=ADDR(RAPICELG);        /* ADDR ANCHOR                   */ 01383000
         LA    REG9,RAPICELG(,RDTREG)                              0979 01384000
*       CALL DOCLSQ;                /* CLSDST                        */ 01385000
         BAL   @14,DOCLSQ                                          0980 01386000
*       IF OCAABORT=OFF             /* IF SUCCESSFUL                 */ 01387000
*         THEN                                                     0981 01388000
         TM    OCAABORT(REG2),B'00100000'                          0981 01389000
         BNZ   @RF00981                                            0981 01390000
*         DO;                                                      0982 01391000
*           REG9=ADDR(RAPICEAC);    /* ADDR ANCHOR                   */ 01392000
         LA    REG9,RAPICEAC(,RDTREG)                              0983 01393000
*           REG3=RAPICEAC;          /* 1ST ICE ON ACCEPT Q           */ 01394000
         L     REG3,RAPICEAC(,RDTREG)                              0984 01395000
*           DO WHILE REG3^=REG9;    /* DO WHILE Q NOT EMPTY          */ 01396000
         B     @DE00985                                            0985 01397000
@DL00985 DS    0H                                                  0986 01398000
*             IF OCACLDEB=ON        /* ABEND IN PROCESS?     @X04BHJY*/ 01399000
*               THEN                                               0986 01400000
         TM    OCACLDEB(REG2),B'01000000'                          0986 01401000
         BO    @RT00986                                            0986 01402000
*               ;                   /* YES, BYPASS TPPOST    @X04BHJY*/ 01403000
*             ELSE                  /* NO, GO TPPOST         @X04BHJY*/ 01404000
*             DO;                   /*                       @X04BHJY*/ 01405000
*               RFY                                                0989 01406000
*                 REG5 RSTD;                                       0989 01407000
*               REG5=REG1;          /* SAVE INPUT RPH ADDR           */ 01408000
