SVTM     TITLE 'ISTOCCCH - FUNCTION CHANGE                             *00001000
                        '                                               00002000
ISTOCCCH CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTOCCCH  78.164'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*/********************************************************************/ 00013000
*/********************************************************************/ 00014000
*/*  INITIALIZE                                                      */ 00015000
*/*   1. COMPONENT RECOVERY RECORD                                   */ 00016000
*/*   2. BASE FOR OCW                                                */ 00017000
*/*   3. REG.SAVE PTR                                                */ 00018000
*/*  IF RPL CONTAINS A NIB PTR                                       */ 00019000
*/*    THEN CONTINUE                                                 */ 00020000
*/*    ELSE SET RTRN CODE = LOGICAL ERROR - INVALID CID              */ 00021000
*/*         AND EXIT TO THE 'UNROUTER'                               */ 00022000
*/*  INITIALIZE                                                      */ 00023000
*/*   1.  PTR TO REG.14 SAVE AREA                                    */ 00024000
*/*   2.  OCW  RTRN CODES                                            */ 00025000
*/*   3.  OCW  RPH PTR                                               */ 00026000
*/*   4.  OCW  NIB PTR                                               */ 00027000
*/********************************************************************/ 00028000
