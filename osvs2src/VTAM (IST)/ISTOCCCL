*            RFY ACEPTR RSTD;                                      1758 03102000
*            ACEPTR = RCPACETR;        /* GET @ FIRST ACE OF CHAIN   */ 03103000
         L     ACEPTR,RCPACETR(,@10)                               1759 03104000
*            /* SEARCH THROUGH CHAIN FOR ACE WITH MATCHING RPH.      */ 03105000
*            /* END OF CHAIN IS REACHED WHEN ACEOBJQ POINTS BACK TO  */ 03106000
*            /* RCPACETR (IF EMPTY, RCPACETR PTS TO ITSELF)  @D32CKJY*/ 03107000
*            DO WHILE ACEPTR^=ADDR(RCPACETR)                       1760 03108000
*                    & ACERPHA^=CPARMRG1;                          1760 03109000
