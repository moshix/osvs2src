         O     GPR02P,@CF01368                                     0946 00962000
*          DCL 1 FMZA0106 FIXED(31) STATIC LOCAL,                  0947 00963000
*              2 *        FIXED(8) INIT(184),                      0947 00964000
*              2 *        PTR(24) INIT(0);                         0947 00965000
*          GPR03F=FMZA0106;                                        0948 00966000
         L     GPR03F,FMZA0106                                     0948 00967000
*          GPR04F=19;                                              0949 00968000
