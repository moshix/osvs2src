*       RFY (GPR01P,GPR13P) UNRSTD;                                0942 00954000
*                                                                  0942 00955000
*       DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */              0943 00956000
*         GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/              0944 00957000
         L     GPR15P,ATCLCPTR                                     0944 00958000
*       END; /*LISATCVT*/                                          0945 00959000
*          GPR02P=GPR15P->ATCBPDA | '80000000'X;                   0946 00960000
