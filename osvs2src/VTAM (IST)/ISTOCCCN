         STM   GPR00P,GPR15P,RPHWORK(GPR01P)                            01009000
*                  RFY (GPR00P) UNRSTD;                            0966 01010000
*                  GPR15P=ADDR(LHAA0125);                          0967 01011000
         LA    GPR15P,LHAA0125                                     0967 01012000
*                  GPR14P=0;                                       0968 01013000
         SLR   GPR14P,GPR14P                                       0968 01014000
*                  GPR15P=GPR15P & '00FFFFFF'X;                    0969 01015000
         N     GPR15P,@CF05083                                     0969 01016000
*                  GPR15P=GPR15P | RPHWT1;                         0970 01017000
         O     GPR15P,@CF01392                                     0970 01018000
*                  CS (GPR14P,GPR15P,GPR01P->RPHRESMA);            0971 01019000
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         0971 01020000
*                  BC (7,LHAB0125);                                0972 01021000
*                                                                  0972 01022000
         BC    7,LHAB0125                                          0972 01023000
*/* TPEXIT  NONE */                                                0973 01024000
*                  DO;                                             0973 01025000
*                    RESPECIFY GPR15P RSTD;                        0974 01026000
*                                                                  0974 01027000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)  */                         0975 01028000
*/* END OF ATBUILD */                                              0975 01029000
*                  DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */   0975 01030000
*                    GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/   0976 01031000
         L     GPR15P,ATCLCPTR                                     0976 01032000
*                  END; /*LISATCVT*/                               0977 01033000
*                    RESPECIFY GPR00F RSTD;                        0978 01034000
*                    GPR00F=0;     /*SET PARAMETER REGISTER TO 0*/ 0979 01035000
         SLR   GPR00F,GPR00F                                       0979 01036000
*                    GPR15P=GPR15P->ATCAP31;                       0980 01037000
         L     GPR15P,ATCAP31(,GPR15P)                             0980 01038000
*                    DCL FMZL0130 BASED(GPR15P) LABEL VALUERANGE(*);    01039000
*                    CALL FMZL0130;                                0982 01040000
         BALR  @14,@15                                             0982 01041000
*                    RESPECIFY (GPR00F,GPR15P) UNRSTD;             0983 01042000
*                  END; /***** END OF TPEXIT  *****/               0984 01043000
*LHAB0125:;        GPR01P->RPHRESMA = 0;                           0985 01044000
LHAB0125 DS    0H                                                  0986 01045000
         SLR   @12,@12                                             0986 01046000
         ST    @12,RPHRESMA(,GPR01P)                               0986 01047000
*                  RFY (GPR00P) RSTD;                              0987 01048000
*LHAA0125:;        GEN (LM    GPR00P,GPR15P,RPHWORK(GPR01P))       0988 01049000
LHAA0125 DS    0H                                                  0989 01050000
*                  REFS(RPHWORK,GPR01P) SETS(GPR00P,GPR15P);       0989 01051000
         LM    GPR00P,GPR15P,RPHWORK(GPR01P)                            01052000
*                  RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;       0990 01053000
*                  END; /*****END OF TPWAIT *****/             /* WAIT  01054000
*OR COMPLETION        */                                           0991 01055000
*              ELSE;                   /* ALLOCATED DONE     @X04BHJY*/ 01056000
@RF00961 DS    0H                                                  0993 01057000
*            OCWLCPBA=RPHSRPRM;        /* SAVE NMLPB PTR     D303013 */ 01058000
         L     @12,RPHSRPRM(,RPHPTR)                               0993 01059000
         ST    @12,OCWLCPBA(,OCWPTR)                               0993 01060000
*            LCPTYPE=NMLPBTYP;         /* NMLPB TYPE         D303013 */ 01061000
         MVI   LCPTYPE(@12),X'27'                                  0994 01062000
*            LCPLNGTH=LENGTH(ISTNMLPB);/* NMLPB LENGTH       D303013 */ 01063000
         MVI   LCPLNGTH(@12),X'64'                                 0995 01064000
*          END;                        /* D303013 */               0996 01065000
*        ELSE                          /* ALLOCATION FAILED  @X04BHJY*/ 01066000
*          DO;                                                     0997 01067000
         B     @RC00959                                            0997 01068000
@RF00959 DS    0H                                                  0998 01069000
*            OCWLCPBA=CZERO;           /* CLEAR NMLPB PTR    @X04BHJY*/ 01070000
         SLR   @12,@12                                             0998 01071000
         ST    @12,OCWLCPBA(,OCWPTR)                               0998 01072000
*            OCWRC='24'X;              /* SYSTEM ERROR CODE  @X04BHJY*/ 01073000
         MVI   OCWRC(OCWPTR),X'24'                                 0999 01074000
*            OCWEC='01'X;              /* STG MGMT FAILURE   @X04BHJY*/ 01075000
         MVI   OCWEC(OCWPTR),X'01'                                 1000 01076000
*            OCWCLCD1=CHONE;           /* SET INTERNAL ERROR @X04BHJY*/ 01077000
         MVC   OCWCLCD1(2,OCWPTR),@CH00099                         1001 01078000
*          END;                                                    1002 01079000
*    END;                                                          1003 01080000
*  ELSE                                /* NMPLB ALREADY HERE @X04BHJY*/ 01081000
*    DO;                                                           1004 01082000
         B     @RC00936                                            1004 01083000
@RF00936 DS    0H                                                  1005 01084000
*      ISTNMLPB = ISTNMLPB&&ISTNMLPB;  /* CLR NMLPB TO ZERO  @X04BHJY*/ 01085000
         L     @12,OCWLCPBA(,OCWPTR)                               1005 01086000
         XC    ISTNMLPB(100,@12),ISTNMLPB(@12)                     1005 01087000
*      LCPTYPE=NMLPBTYP;               /* NMLPB TYPE         D303013 */ 01088000
