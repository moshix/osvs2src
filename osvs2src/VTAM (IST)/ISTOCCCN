*          DCL FMZL0106 BASED(GPR15P) LABEL VALUERANGE(*);         0951 00972000
*          CALL FMZL0106;                                          0952 00973000
*                                                                  0952 00974000
         BALR  @14,@15                                             0952 00975000
*       RFY (GPR13P) RSTD;                                         0953 00976000
*          GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);      0954 00977000
*                                                                  0954 00978000
           LM GPR02P,GPR13P,RPHWORK(GPR01P)                             00979000
*       RFY (GPR13P) UNRSTD;                                       0955 00980000
*                                                                  0955 00981000
*       RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;                  0956 00982000
*       END ;                                                      0957 00983000
*          /********* END OF REQSTORE EXPANSION **********/        0958 00984000
*      RFY CRETCODE RSTD;                                          0958 00985000
*                                                                  0958 00986000
*      /* RETURN CODES FROM REQSTORE:                        @X04BHJY*/ 00987000
*      /*       0 OR 4 = STORAGE SUCCESSFULLY ALLOCATED              */ 00988000
*      /*       8 = ALLOCATED BUT MUST WAIT FOR COMPLETION           */ 00989000
*      /*       12 OR OTHERS = STORAGE CANNOT BE ALLOCATED           */ 00990000
*                                                                  0959 00991000
*      IF CRETCODE =< C8               /* STORAGE ALLOCATED? @X04BHJY*/ 00992000
*        THEN                          /* YES                @X04BHJY*/ 00993000
         LA    @12,8                                               0959 00994000
         CR    CRETCODE,@12                                        0959 00995000
         BH    @RF00959                                            0959 00996000
*          DO;                                                     0960 00997000
*            IF CRETCODE=C8            /* SHOULD WE WAIT?    @X04BHJY*/ 00998000
*              THEN                    /* YES                @X04BHJY*/ 00999000
*                                                                  0961 01000000
         CR    CRETCODE,@12                                        0961 01001000
         BNE   @RF00961                                            0961 01002000
*/*  TPWAIT   */                                                   0962 01003000
*                  DO; /* BEGIN TPWAIT GENERATION*/                0962 01004000
*                  RFY (GPR01P,GPR14P,GPR15P) RSTD;                0963 01005000
*                  RFY (GPR00P) RSTD;                              0964 01006000
