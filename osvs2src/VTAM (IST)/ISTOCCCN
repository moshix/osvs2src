*DOLCPB: PROC OPTIONS(NOSAVEAREA,NOSAVE);                          0933 00897000
DOLCPB   DS    0H                                                  0934 00898000
*/********************************************************************/ 00899000
*/*                                                                  */ 00900000
*/* ROUTINE NAME : DOLCPB -- GET AND INITIALIZE NMLPB                */ 00901000
*/*                                                                  */ 00902000
*/* FUNCTION : OBTAINS AND INITIALIZES AN NMLPB.  IF AN ERROR OCCURS */ 00903000
*/*            IN OBTAINING THE NMLPB, THE OCW COMPLETION INDICATORS */ 00904000
*/*            WILL BE SET.                                          */ 00905000
*/*                                                                  */ 00906000
*/* INPUT : OCWPTR (R4) - PTR TO OPEN/CLOSE WORK AREA                */ 00907000
*/*                                                                  */ 00908000
*/* OUTPUT : SUCCESSFUL COMPLETION - OCWLCPBA WILL CONTAIN THE       */ 00909000
*/*             ADDRESS OF THE NMLPB                                 */ 00910000
*/*          ERROR - OCWLCPBA WILL BE 0                              */ 00911000
*/*                  OCWCLCD1 WILL BE 1 INDICATING ERROR             */ 00912000
*/*                  OCWRC WILL CONTAIN THE MAJOR RETURN CODE        */ 00913000
*/*                  OCWEC WILL CONTAIN THE MINOR RETURN CODE        */ 00914000
*/*                                                                  */ 00915000
*/* SUBROUTINES CALLED : NONE                                        */ 00916000
*/*                                                                  */ 00917000
*/* ROUTINES CALLED : NONE                                           */ 00918000
*/*                                                                  */ 00919000
*/* MACROS USED :                                                    */ 00920000
*/*            REQSTORE - OBTAIN AN NMLPB                            */ 00921000
*/*            TPWAIT - WAIT FOR STORAGE REQUEST                     */ 00922000
*/*                                                                  */ 00923000
*/********************************************************************/ 00924000
*C14SAVE=CR14PTR;                                                  0934 00925000
         LA    @15,624                                             0934 00926000
         ALR   @15,OCWPTR                                          0934 00927000
         L     @12,C14SVPTR(,@15)                                  0934 00928000
         ST    CR14PTR,C14SAVE(,@12)                               0934 00929000
*C14SVPTR=C14SVPTR+4;                                              0935 00930000
*                                                                  0935 00931000
         AL    @12,@CF00076                                        0935 00932000
         ST    @12,C14SVPTR(,@15)                                  0935 00933000
*/* IF AN NMLPB HAS ALREADY BEEN ALLOCATED (OR, MORE LIKELY, IT WAS  */ 00934000
*/* NOT RELEASED), OCWLCPBA WILL STILL HAVE ADDRESS OF IT    @X04BHJY*/ 00935000
*                                                                  0936 00936000
*IF OCWLCPBA=CZERO                     /* IS THERE AN NMLPB  D303013 */ 00937000
*  THEN                                /* NO--GO GET ONE     @X04BHJY*/ 00938000
         L     @15,OCWLCPBA(,OCWPTR)                               0936 00939000
         LTR   @15,@15                                             0936 00940000
         BNZ   @RF00936                                            0936 00941000
*    DO;                                                           0937 00942000
*       /*REQSTORE CBID(NMLPB)RPH(CPARMRG1)CLEAR(YES)*/            0938 00943000
*        DO;                                                       0938 00944000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)  */                         0939 00945000
*/* END OF ATBUILD */                                              0939 00946000
*                                                                  0939 00947000
*       RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;                    0939 00948000
