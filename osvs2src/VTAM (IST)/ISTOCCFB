SVTM     TITLE 'ISTOCCFB - FMCB BUILDER                                *00001000
                        '                                               00002000
         ISEQ  73,80                                       /*@D31KKMP*/ 00003000
ISTOCCFB CSECT ,                                                   0002 00004000
@MAINENT DS    0H                                                  0002 00005000
         USING *,@15                                               0002 00006000
         B     @PROLOG                                             0002 00007000
         DC    AL1(16)                                             0002 00008000
         DC    C'ISTOCCFB  78.081'                                 0002 00009000
         DROP  @15                                                      00010000
@PROLOG  STM   @14,@12,12(@13)                                     0002 00011000
         BALR  @11,0                                               0002 00012000
@PSTART  LA    @12,4095(,@11)                                      0002 00013000
         USING @PSTART,@11                                         0002 00014000
         USING @PSTART+4095,@12                                    0002 00015000
*/********************************************************************* 00016000
*/*       INITIALIZE                                                  * 00017000
*/*               1. COMPONENT RECOVERY RECORD                        * 00018000
*/*               2. PTR TO NEXT AVAILABLE REG. SAVE AREA             * 00019000
*/*               3. INTERNAL CODES, & WORK AREAS                     * 00020000
*/********************************************************************* 00021000
* DO;                                                              0449 00022000
*/***  CRRMAINT  PROCNM(ISTOCCFB)COMP(OC)FUNC(SUB)  ***/           0450 00023000
* DCL CMZK0010 PTR(31) REG(1);                                     0450 00024000
* RFY (CMZK0010) RSTD;                                             0451 00025000
*                                                                  0451 00026000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0452 00027000
* RFY ISTLCCW ;                                                    0452 00028000
* RFY ISTLCPB ;                                                    0453 00029000
* RFY ISTZSAVE;                                                    0454 00030000
* RFY ISTPFCRR;                                                    0455 00031000
* RFY ISTCFCRR;                                                    0456 00032000
* RFY ISTZCRR ;                                                    0457 00033000
* RFY ISTASCRR;                                                    0458 00034000
* RFY ISTAPCRR;                                                    0459 00035000
* RFY ISTCRA  ;                                                    0460 00036000
* RFY ISTCNSTA;                                                    0461 00037000
* RFY ISTOCCRR;                                                    0462 00038000
*/* END OF ATBUILD */                                              0463 00039000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0463 00040000
*CMZK0010->RPHCRR->CRACRR->NOCCFB = '1'B;                          0463 00041000
         L     @10,RPHCRR(,CMZK0010)                               0463 00042000
         L     @10,CRACRR(,@10)                                    0463 00043000
         OI    NOCCFB(@10),B'00000010'                             0463 00044000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0464 00045000
*CMZK0010->RPHCRR->CRACRR->OCCCRT = '0'B;                          0464 00046000
         XC    OCCCRT+1(2,@10),OCCCRT+1(@10)                       0464 00047000
         MVI   OCCCRT(@10),B'00000000'                             0464 00048000
*CMZK0010->RPHCRR->CRACRR->COCCFB = '1'B;                          0465 00049000
         OI    COCCFB(@10),B'00000010'                             0465 00050000
* END;                                                             0466 00051000
*CREG13 = CREG13 + OCWSLEN;  /* PTR NXT O/C SAVE AREA */           0467 00052000
         SLR   @10,@10                                             0467 00053000
         IC    @10,OCWSLEN(,COCWPTR)                               0467 00054000
         ALR   CREG13,@10                                          0467 00055000
*CINTCODE = 0;  /* INIT INTRNL CODE = SUCCESSFUL */                0468 00056000
         SLR   CINTCODE,CINTCODE                                   0468 00057000
*OCWCID = '00000000'X;   /* CLEAR CID WORK AREA */                 0469 00058000
         SLR   @10,@10                                             0469 00059000
         ST    @10,OCWCID(,COCWPTR)                                0469 00060000
*OCWSCPAD = OCWSCPAD&&OCWSCPAD;  /* INIT SCRTCH PAD FOR W.A. */    0470 00061000
         XC    OCWSCPAD(28,COCWPTR),OCWSCPAD(COCWPTR)              0470 00062000
*/********************************************************************* 00063000
*/*  GET CORE FOR & INITIALIZE THE FMCB                               * 00064000
*/*  IF IT IS THE FIRST OPEN FOR THE DESTINATION                      * 00065000
*/*    THEN GET CORE FOR THE APPROPRIATE NCB TYPE                     * 00066000
*/*         AND INITIALIZE THE NCB TYPE DEPENDENT FIELDS              * 00067000
*/*    ELSE DO NOT BUILD AN NCB SINCE ONE ALREADT EXISTS              * 00068000
*/*  INITIALIZE DATA COMMON TO ALL NCB'S & CHAIN THE FMCB-NCB PAIR    * 00069000
*/********************************************************************* 00070000
* DO;                                                              0471 00071000
*/***  CRRMAINT  PROCNM(ISTOCCFB)COMP(OC)FUNC(SUB)  ***/           0472 00072000
* DCL CMZK0012 PTR(31) REG(1);                                     0472 00073000
* RFY (CMZK0012) RSTD;                                             0473 00074000
*                                                                  0473 00075000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0474 00076000
*/* END OF ATBUILD */                                              0474 00077000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0474 00078000
*CMZK0012->RPHCRR->CRACRR->NOCCFB = '1'B;                          0474 00079000
         L     @10,RPHCRR(,CMZK0012)                               0474 00080000
         L     @10,CRACRR(,@10)                                    0474 00081000
         OI    NOCCFB(@10),B'00000010'                             0474 00082000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0475 00083000
*CMZK0012->RPHCRR->CRACRR->OCCCRT = '0'B;                          0475 00084000
         XC    OCCCRT+1(2,@10),OCCCRT+1(@10)                       0475 00085000
         MVI   OCCCRT(@10),B'00000000'                             0475 00086000
*CMZK0012->RPHCRR->CRACRR->COCCFB = '1'B;                          0476 00087000
         OI    COCCFB(@10),B'00000010'                             0476 00088000
* END;                                                             0477 00089000
*  /*SEGMENT(DOFMCB)REQUEST*/DO;GOTO DOFMCB;DOFMCB#:END;  /* INIT FMCB  00090000
*ORE */                                                            0478 00091000
         B     DOFMCB                                              0479 00092000
DOFMCB#  DS    0H                                                  0481 00093000
*IF OCWNCB = 0   /* IF FIRST FOR DEST */                           0481 00094000
*  THEN DO;                                                        0481 00095000
         ICM   @10,15,OCWNCB(COCWPTR)                              0481 00096000
         BNZ   @RF00481                                            0481 00097000
*          /*SEGMENT(DOXNCB)REQUEST*/DO;GOTO DOXNCB;DOXNCB#:END;  /* IN 00098000
*T NCB DPNDNT FIELDS */                                            0483 00099000
         B     DOXNCB                                              0484 00100000
DOXNCB#  DS    0H                                                  0486 00101000
*       END;                                                       0486 00102000
*  ELSE;                                                           0487 00103000
@RF00481 DS    0H                                                  0488 00104000
*  /*SEGMENT(DONCB)REQUEST*/DO;GOTO DONCB;DONCB#:END;  /* INIT COMMON N 00105000
*B FIELDS */                                                       0488 00106000
         B     DONCB                                               0489 00107000
DONCB#   DS    0H                                                  0491 00108000
*  /*SEGMENT(DOCHAIN)REQUEST*/DO;GOTO DOCHAIN;DOCHAIN#:END;  /* CHAIN F 00109000
*CB & NCB */                                                       0491 00110000
         B     DOCHAIN                                             0492 00111000
DOCHAIN# DS    0H                                                  0494 00112000
*/********************************************************************* 00113000
*/*  IF AN ERROR OCCURRED DURING FMCB/NCB BUILDING OR CHAINING        * 00114000
*/*    THEN RELEASE THE DAF LOCK FOR THE DESTINATION IF IT IS HELD    * 00115000
*/*         SET ERROR RETURN CODES                                    * 00116000
*/*         FREE ANY FMCB/NCB CORE THAT IS HELD                       * 00117000
*/*    ELSE JUST PREPARE TO RETURN TO CALLER                          * 00118000
*/********************************************************************* 00119000
*IF CINTCODE ^= 0                                                  0494 00120000
*  THEN   /*SEGMENT(DOUNDO)REQUEST*/DO;GOTO DOUNDO;DOUNDO#:END;  /* CLE 00121000
*N-UP AFTER ERRORS */                                              0494 00122000
         LTR   CINTCODE,CINTCODE                                   0494 00123000
         BZ    @RF00494                                            0494 00124000
         B     DOUNDO                                              0496 00125000
*  ELSE;                                                           0498 00126000
@RF00494 DS    0H                                                  0499 00127000
*/********************************************************************* 00128000
*/*  CLEAN UP AND RETURN TO CALLER                                    * 00129000
*/*    LOGIC REMOVED FOR CLEAR OCW CID AREA TO ZERO           @DM09566* 00130000
*/*    DECREMENT REG. SAVE AREA PTR                                   * 00131000
*/*    RETURN TO CALLER WITH RETURN CODE IN REG.15                    * 00132000
*/********************************************************************* 00133000
*/* OCWCID = 0    CLEAR OCW CID AREA                          @DM09566* 00134000
*CREG13 = CREG13 - OCWSLEN;  /*FB REG SAVE AREA PTR */             0499 00135000
@RC00494 SLR   @10,@10                                             0499 00136000
         IC    @10,OCWSLEN(,COCWPTR)                               0499 00137000
         SLR   CREG13,@10                                          0499 00138000
*RETURN CODE(OCWRC);   /* RETURN TO CALLER */                      0500 00139000
         SLR   @15,@15                                             0500 00140000
         IC    @15,OCWRC(,COCWPTR)                                 0500 00141000
         L     @14,12(,@13)                                        0500 00142000
         LM    @00,@12,20(@13)                                     0500 00143000
         BR    @14                                                 0500 00144000
*  /*SEGMENT(DOFMCB)START*/DO;GOTO DOFMCB@;DOFMCB:END;             0501 00145000
DOFMCB   DS    0H                                                  0504 00146000
*/********************************************************************* 00147000
*/* IF AN FMCB FOR THIS DEST DOES NOT ALREADY EXIST                   * 00148000
*/*   THEN GET CORE FOR AN FMCB                                       * 00149000
*/*        STORE THE ADDR OF THE FMCB IN OCW                          * 00150000
*/*        STORE FMCB LNGTH(DBL WRDS) & ID IN THE FMCB                * 00151000
*/*        SET 'FMCB CORE OBTAINED' FLAG                              * 00152000
*/*   IF CORE NOT AVAILABLE OR BUFFER THRESHOLD EXCEEDED              * 00153000
*/*     THEN SET INTERNAL FLAG TO INDICATE NO CORE,SO THE OPEN        * 00154000
*/*          DEST WILL FAIL                                           * 00155000
*/********************************************************************* 00156000
*IF CINTCODE = 0   /* IF NO ERRORS OCCURRED */                     0504 00157000
*  THEN IF OCWFMCB = 0  /* IF NO PRIOR FMCB EXISTS */              0504 00158000
         SLR   @10,@10                                             0504 00159000
         CR    CINTCODE,@10                                        0504 00160000
         BNE   @RF00504                                            0504 00161000
*         THEN DO;      /* GET CORE FOR FMCB */                    0505 00162000
*                                                                  0505 00163000
         C     @10,OCWFMCB(,COCWPTR)                               0505 00164000
         BNE   @RF00505                                            0505 00165000
*               /*------------------------------------------ @D31KKMP*/ 00166000
*               /* THESE MODIFICATIONS WERE FOR DCR-3398.0   @D31KKMP*/ 00167000
*               /*------------------------------------------ @D31KKMP*/ 00168000
*               IF NIBMODE=CONTMODE |  /*IF VARY CTL NODE OR @D31KKMP*/ 00169000
*                  NIBMODE=CLUSMODE |  /*CLUSTER CTL NODE OR @D31KKMP*/ 00170000
*                  NIBMODE=PSOLMODE |  /*PORT SOLICT NODE OR @D31KKMP*/ 00171000
*                  NIBMODE=TCM2MODE |  /*TCAM CTL NODE OR    @XL03HMP*/ 00172000
*                  NIBMODE=SSCPMODE    /*SESSION CTL NODE    @D31KKMP*/ 00173000
*                 THEN                 /*SPECIAL NODE, SO:   @D31KKMP*/ 00174000
         LA    @10,112                                             0507 00175000
         ALR   @10,COCWPTR                                         0507 00176000
         CLC   NIBMODE(8,@10),@CC04265                             0507 00177000
         BE    @RT00507                                            0507 00178000
         CLC   NIBMODE(8,@10),@CC04267                             0507 00179000
         BE    @RT00507                                            0507 00180000
         CLC   NIBMODE(8,@10),@CC03601                             0507 00181000
         BE    @RT00507                                            0507 00182000
         CLC   NIBMODE(8,@10),@CC04271                             0507 00183000
         BE    @RT00507                                            0507 00184000
         CLC   NIBMODE(8,@10),@CC04269                             0507 00185000
         BNE   @RF00507                                            0507 00186000
@RT00507 DS    0H                                                  0508 00187000
*                  /*REQSTORE CBID(FMLB)CLEAR(YES)*/               0508 00188000
*                   DO;                                            0508 00189000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)  */                         0509 00190000
*/* END OF ATBUILD */                                              0509 00191000
*                  DCL GPR01F FIXED(31) REG(01);                   0509 00192000
*                  DCL GPR01P PTR(31)   REG(01);                   0510 00193000
*                  DCL GPR02F FIXED(31) REG(02);                   0511 00194000
