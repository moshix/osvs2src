         L     GPR15P,ATCLCPTR                                     0542 00229000
*                  END; /*LISATCVT*/                               0543 00230000
*          GPR02P=GPR15P->ATCBPDA | '80000000'X;                   0544 00231000
         L     GPR02P,ATCBPDA(,GPR15P)                             0544 00232000
         O     GPR02P,@CF02254                                     0544 00233000
*          DCL 1 FMZA0020 FIXED(31) STATIC LOCAL,                  0545 00234000
*              2 *        FIXED(8) INIT(152),                      0545 00235000
*              2 *        PTR(24) INIT(0);                         0545 00236000
*          GPR03F=FMZA0020;                                        0546 00237000
         L     GPR03F,FMZA0020                                     0546 00238000
*          GPR04F=35;                                              0547 00239000
         LA    GPR04F,35                                           0547 00240000
