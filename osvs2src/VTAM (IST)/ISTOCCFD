         BNE   @RF00521                                            0521 00606000
@RT00521 DS    0H                                                  0522 00607000
*          OCWRBH = OFF;   /* RESET 'RQUIRED'BHSET ID FLAG */      0523 00608000
*          OCWPBH = OFF;   /* RESET 'PREFERRED' BHSET ID FLAG */   0524 00609000
         NI    OCWRBH(OCWPTR),B'11110011'                          0524 00610000
*          OCWBHSID = CNULLID;   /* PUT NULL ID IN OCW */          0525 00611000
         MVI   OCWBHSID(OCWPTR),X'00'                              0525 00612000
*         END;                                                     0526 00613000
*    ELSE DO;                                                      0527 00614000
         B     @RC00521                                            0527 00615000
