         LR    CRQRDID,CREG0                                       0499 00510000
*           SDVINCLD = CB0; /*RESET INCLD FLAG,DO FUNCTION IN RN */     00511000
         NI    SDVINCLD(CMDVTPTR),B'01111111'                      0500 00512000
*           CMDVDVTE->SDVCNT = CMDVDVTE->SDVCNT-C1;  /* DECR PROC MOD   00513000
*                                                  CNT IN DVTNAME NTRY* 00514000
         LH    @15,SDVCNT(,CMDVDVTE)                               0501 00515000
         BCTR  @15,0                                               0501 00516000
         STH   @15,SDVCNT(,CMDVDVTE)                               0501 00517000
*         END;                                                     0502 00518000
*    ELSE IF CRQRDID = CREG0   /* IF NEW ID = REQUIRED ID,IGNORE IT */  00519000
*           THEN DO;                                               0503 00520000
         B     @RC00496                                            0503 00521000
