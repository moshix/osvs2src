*  IF OCWRBH = OFF /* IF THIS FIRST REQUIRED BHSET ID */           0496 00503000
*    THEN DO;                                                      0496 00504000
         TM    OCWRBH(OCWPTR),B'00001000'                          0496 00505000
         BNZ   @RF00496                                            0496 00506000
*           OCWRBH = ON;   /* SET 'RQRD' BHSET ID FLAG */          0498 00507000
         OI    OCWRBH(OCWPTR),B'00001000'                          0498 00508000
