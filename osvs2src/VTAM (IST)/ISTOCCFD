SVTM     TITLE 'ISTOCCFD - FUNCTION DISTRIBUTION                       *00001000
                        '                                               00002000
ISTOCCFD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(8)                                              0001 00007000
         DC    C'ISTOCCFD'                                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*/********************************************************************* 00014000
*/* 1. INITIALIZE                                                     * 00015000
*/*      FLAGS & SWITCHES & RTRN CODES                                * 00016000
*/*      PTR TO NXT AVAILABLE REG.SAVE AREA                           * 00017000
*/*      COMPONENT RECOVERY RECORD                                    * 00018000
*/* 2. SCAN MASTER DVT FOR PROCESS MODULE ENTRIES.                    * 00019000
*/*   FOR  EACH ENTRY THAT IS MARKED TO BE INCLUDED IN A REAL DVT     * 00020000
*/*   AND  WHICH IS ALSO MARKED AS HAVING BEEN LOADED IN THE HOST     * 00021000
*/*   INVOKE  SRT FIND TO ENSURE THAT THE MODULE      HAS BEEN LOADED * 00022000
*/* 3. IF SRT FIND CANNOT RESOLVE THE MODULE NAME MARK THAT ENTRY     * 00023000
*/*   AS NOT HAVING BEEN LOADED & SET A FLAG TO INDICATE AT LEAST     * 00024000
*/*   ONE MODULE NOT LOADED THAT WAS TO BE INCLUDED IN A REAL DVT     * 00025000
*/********************************************************************* 00026000
*                                                                  0312 00027000
*   DO;                                                            0312 00028000
*/***  CRRMAINT  PROCNM(ISTOCCFD)COMP(OC)FUNC(SUB)  ***/           0313 00029000
*   DCL CMZK0008 PTR(31) REG(1);                                   0313 00030000
*   RFY (CMZK0008) RSTD;                                           0314 00031000
*                                                                  0314 00032000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0315 00033000
*   RFY ISTLCCW ;                                                  0315 00034000
*   RFY ISTLCPB ;                                                  0316 00035000
*   RFY ISTZSAVE;                                                  0317 00036000
*   RFY ISTPFCRR;                                                  0318 00037000
*   RFY ISTCFCRR;                                                  0319 00038000
*   RFY ISTZCRR ;                                                  0320 00039000
*   RFY ISTASCRR;                                                  0321 00040000
*   RFY ISTAPCRR;                                                  0322 00041000
*   RFY ISTCRA  ;                                                  0323 00042000
*   RFY ISTCNSTA;                                                  0324 00043000
*   RFY ISTOCCRR;                                                  0325 00044000
*/* END OF ATBUILD */                                              0326 00045000
*   /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                   0326 00046000
*CMZK0008->RPHCRR->CRACRR->NOCCFD = '1'B;                          0326 00047000
         L     @05,RPHCRR(,CMZK0008)                               0326 00048000
         L     @05,CRACRR(,@05)                                    0326 00049000
         OI    NOCCFD(@05),B'00000001'                             0326 00050000
*   /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                  0327 00051000
*CMZK0008->RPHCRR->CRACRR->OCCCRT = '0'B;                          0327 00052000
         XC    OCCCRT+1(2,@05),OCCCRT+1(@05)                       0327 00053000
         MVI   OCCCRT(@05),B'00000000'                             0327 00054000
*CMZK0008->RPHCRR->CRACRR->COCCFD = '1'B;                          0328 00055000
         OI    COCCFD(@05),B'00000001'                             0328 00056000
*   END;                                                           0329 00057000
*  RFY CREG0 RSTD;   /* RSTRCT REG.0 FOR DF PARM */                0330 00058000
*  RFY (CMDVTPTR,CMDVTEND) RSTD; /* RESTRICT REGS. 5 & 7 */        0331 00059000
*  CREG13 = CREG13 + OCWSLEN;  /*NXT REG SAVE AREA PTR*/           0332 00060000
         SLR   @06,@06                                             0332 00061000
         IC    @06,OCWSLEN(,OCWPTR)                                0332 00062000
         ALR   CREG13,@06                                          0332 00063000
*  CFDFLGA = CFDFLGA&&CFDFLGA;  /* INIT INTRNL SWTCH TO ZERO */    0333 00064000
         XR    CFDFLGA,CFDFLGA                                     0333 00065000
*  OCWRC = '00'X;     /* INIT RC = SUCCESSFUL */                   0334 00066000
         MVI   OCWRC(OCWPTR),X'00'                                 0334 00067000
*  OCWEC = '00'X;     /* INIT MINOR RTRN CODE */                   0335 00068000
         MVI   OCWEC(OCWPTR),X'00'                                 0335 00069000
*  OCWRBH = OFF;   /* RESET 'REQUIRED' BHSET ID FLAG */            0336 00070000
*  OCWPBH = OFF;   /* RESET 'PREFFERED' BHSET ID FLAG */           0337 00071000
         NI    OCWRBH(OCWPTR),B'11110011'                          0337 00072000
*  CMDVTPTR = CREG0;          /* INIT PTR TO MSTR DVT */           0338 00073000
         LR    CMDVTPTR,CREG0                                      0338 00074000
*  CMDVTEND = CMDVTPTR+SDVTOTAL*LENGTH(ISTSDVT); /*LAST MDVT ENTRY PTR* 00075000
         LH    CMDVTEND,SDVTOTAL(,CMDVTPTR)                        0339 00076000
         MH    CMDVTEND,@CH01402                                   0339 00077000
         ALR   CMDVTEND,CMDVTPTR                                   0339 00078000
*  DO WHILE CMDVTPTR < CMDVTEND;  /* SCAN MSTR DVT */              0340 00079000
         B     @DE00340                                            0340 00080000
@DL00340 DS    0H                                                  0341 00081000
*  IF SDVID = CRDVT  /* IF MSTR DVT ENTRY FOR DVTNAME */           0341 00082000
*    THEN;   /* LOOK AT NEXT MSTR DVT ENTRY */                     0341 00083000
         CLC   SDVID(4,CMDVTPTR),@CC02850                          0341 00084000
         BE    @RT00341                                            0341 00085000
*    ELSE IF SDVFLAGS = CXC0  /*IF INCLD & BLDL FLAGS SET */       0343 00086000
*           THEN     /* CHECK IF PROC.MOD.NAME IN SRT        @XL03HHG*/ 00087000
         CLI   SDVFLAGS(CMDVTPTR),X'C0'                            0343 00088000
         BNE   @RF00343                                            0343 00089000
*               IF SDVPMEPA^=C0 /* IF PROC NAME IN SRT       @XL03HHG*/ 00090000
*                 THEN;     /*LOOK AT NEXT MSTR DVT ENTRY */       0344 00091000
         L     @06,SDVPMEPA(,CMDVTPTR)                             0344 00092000
         LTR   @06,@06                                             0344 00093000
         BNZ   @RT00344                                            0344 00094000
*                 ELSE DO;     /* PROC.MOD NAME NOT FOUND */       0346 00095000
*                        SDVBLDL = '0'B; /*RESET BLDL FLAG */      0347 00096000
         NI    SDVBLDL(CMDVTPTR),B'10111111'                       0347 00097000
*                        CFDFLGA = CX40; /*SET NO PROC NAME FLG*/  0348 00098000
         LA    CFDFLGA,64                                          0348 00099000
*                      END;                                        0349 00100000
*           ELSE;                                                  0350 00101000
@RF00343 DS    0H                                                  0351 00102000
*  CMDVTPTR = CMDVTPTR+LENGTH(ISTSDVT); /* NXT MDVT ENTRY PTR */   0351 00103000
@RC00343 DS    0H                                                  0351 00104000
@RT00341 AL    CMDVTPTR,@CF01402                                   0351 00105000
*  END;                                                            0352 00106000
@DE00340 CR    CMDVTPTR,CMDVTEND                                   0352 00107000
         BL    @DL00340                                            0352 00108000
*  RFY CREG0 UNRSTD;   /* RELEASE REG.0 */                         0353 00109000
*  RFY CMDVTPTR UNRSTD;   /* RELEASE REG.5 */                      0354 00110000
*  RFY CMDVTEND UNRSTD;   /* RELEASE REG.7 */                      0355 00111000
*/********************************************************************* 00112000
*/* 1. INIT RQRD & PREFRD BHSET ID'S TO NULL ID                       * 00113000
*/* 2. IF DEST BEING OPENED IS A LOCAL C.U.,RN,LOCAL TERM,            * 00114000
*/*       LOGICAL UNIT OR CLUSTER CTRLR                               * 00115000
*/*      THEN NO BHSET RESOLUTION NEED BE PERFORMED                   * 00116000
*/*      ELSE SCAN THE MASTER DVT TO RESOLVE ANY PROCESS MODULE       * 00117000
*/*           NAMES WHICH MAY BE BHSETS INTO BHSET ID'S               * 00118000
*/********************************************************************* 00119000
*  RFY CRQRDID RSTD;   /* RSTRCT REG10 FOR 'REQUIRED' BHSET ID */  0356 00120000
*  RFY CPRFRDID RSTD;   /* RSTRCT REG.11 FOR 'PREFERRED' BHSET ID */    00121000
*  CRQRDID = CNULLID;  /* BHSET ID = NULL ID */                    0358 00122000
         SLR   @05,@05                                             0358 00123000
         LR    CRQRDID,@05                                         0358 00124000
*  CPRFRDID = CNULLID; /* INIT BHSET ID SAVE AREA TO NULL ID */    0359 00125000
         LR    CPRFRDID,@05                                        0359 00126000
*  IF CFDFLGA=CX80                       /* IF FAIL FLAG SET */    0360 00127000
*    | RPRENTRY=RPRENTLC                /* OR IF DEST IS A LOCAL NTRY * 00128000
*    | RPRENTRY=RPRENTRN                /* OR AN RN NTRY */        0360 00129000
*    | RPRNSNEA->RPRENTRY = RPRENTLC    /* OR A LOCAL TERMINAL */  0360 00130000
*    | RPRENTRY=RPRENTLU   /* OR A LOGICAL UNIT */                 0360 00131000
*    | RPRENTRY=RPRENTCC   /* OR AN SNA CLUSTER CTRL UNIT */       0360 00132000
*    THEN;                              /* RETURN */               0360 00133000
         C     CFDFLGA,@CF01130                                    0360 00134000
         BE    @RT00360                                            0360 00135000
         L     @05,OCWDRDTE(,OCWPTR)                               0360 00136000
         CLI   RPRENTRY(@05),X'45'                                 0360 00137000
         BE    @RT00360                                            0360 00138000
         CLI   RPRENTRY(@05),X'01'                                 0360 00139000
         BE    @RT00360                                            0360 00140000
         L     @02,RPRNSNEA(,@05)                                  0360 00141000
         CLI   RPRENTRY(@02),X'45'                                 0360 00142000
         BE    @RT00360                                            0360 00143000
         CLI   RPRENTRY(@05),X'81'                                 0360 00144000
         BE    @RT00360                                            0360 00145000
         CLI   RPRENTRY(@05),X'71'                                 0360 00146000
         BE    @RT00360                                            0360 00147000
*    ELSE DO; /* SCAN MSTR DVT AND FIND BHSET ID'S FOR PROC.MODS */     00148000
*           RFY CMDVTPTR RSTD;  /* RSTRCT REG.5 FOR MDVT ENTRY PTR */   00149000
*           RFY CMDVTEND RSTD;   /* RSTRCT REG.7 FOR END MDVT */   0364 00150000
*           RFY (CMDVNTRY,CMDVDVTE) RSTD;/*RSTRCT REG 6,8*/        0365 00151000
*           CMDVTPTR = OCWMDVT;       /* PTR 1ST MDVT NTRY */      0366 00152000
         L     CMDVTPTR,OCWMDVT(,OCWPTR)                           0366 00153000
*           CMDVTEND = CMDVTPTR+SDVTOTAL*LENGTH(ISTSDVT); /*LAST NTRY*/ 00154000
         LH    CMDVTEND,SDVTOTAL(,CMDVTPTR)                        0367 00155000
         MH    CMDVTEND,@CH01402                                   0367 00156000
         ALR   CMDVTEND,CMDVTPTR                                   0367 00157000
*           /*SEGMENT(RSLVMD)REQUEST*/DO;GOTO RSLVMD;RSLVMD#:END;  /* R 00158000
*SOLVE MDVT FOR BHSET ID'S */                                      0368 00159000
         B     RSLVMD                                              0369 00160000
RSLVMD#  DS    0H                                                  0371 00161000
*         END;                                                     0371 00162000
*/********************************************************************* 00163000
*/* 1. IF AN ERROR OCCURRED DURING BHSET RESOLUTION                   * 00164000
*/*      THEN NO FURTHER PROCESSING REQUIRED                          * 00165000
*/*      ELSE CALL ISTOCCSL TO HANDLE USER PROCESSING OPTIONS         * 00166000
*/*           & SELECTION OF THE BHSET FOR THE ID FOUND PREVIOUSLY    * 00167000
*/* 2. PUT THE RTRN CODE IN REG.15                                    * 00168000
*/* 3. POINT REG.13 AT THE CALLER'S SAVE AREA                         * 00169000
*/* 4. RESTORE REGS & RETURN TO CALLER                                * 00170000
*/********************************************************************* 00171000
*  RFY CREG15 RSTD;                                        /*@XL03HHG*/ 00172000
@RT00360 DS    0H                                                  0373 00173000
*  IF (CFDFLGA=CX80)                     /* IF FAILURE OCCURRED */ 0373 00174000
*    THEN;                               /* RETURN TO CALLER */    0373 00175000
         C     CFDFLGA,@CF01130                                    0373 00176000
         BE    @RT00373                                            0373 00177000
*    ELSE      /* HANDLE BHSET ID & PROCESS OPTNS */               0375 00178000
*     /*SEGMENT(SLCTBH)REQUEST*/DO;GOTO SLCTBH;SLCTBH#:END;        0375 00179000
         B     SLCTBH                                              0376 00180000
SLCTBH#  DS    0H                                                  0378 00181000
*  CREG15 = OCWRC   ;  /* STORE RTRN CODE */                       0378 00182000
@RT00373 SLR   CREG15,CREG15                                       0378 00183000
         IC    CREG15,OCWRC(,OCWPTR)                               0378 00184000
*  CREG13 = CREG13 - OCWSLEN;   /* FD REG SAVE AREA PTR */         0379 00185000
*                                                                  0379 00186000
         SLR   @14,@14                                             0379 00187000
         IC    @14,OCWSLEN(,OCWPTR)                                0379 00188000
         SLR   CREG13,@14                                          0379 00189000
*RETURN CODE(CREG15);                                              0380 00190000
         L     @14,12(,@13)                                        0380 00191000
         LM    @00,@12,20(@13)                                     0380 00192000
         BR    @14                                                 0380 00193000
*RFY CREG15 UNRSTD;                                        /*@XL03HHG*/ 00194000
*/********************************************************************* 00195000
*/* RESOLVE PROCESS MODULE ENTRIES IN MASTER DVT TO BHSET ID'S        * 00196000
*/*IF NECESSARY.                                                      * 00197000
*/* SCAN MASTER DVT                                                   * 00198000
*/*  1. IF A DVT NAME ENTRY IS FOUND                                  * 00199000
*/*       THEN SAVE PTR TO THIS ENTRY & LOOK AT NEXT ENTRY            * 00200000
*/*       ELSE 1. IF PROCESS IS MARKED FOR INCLUSION                  * 00201000
*/*                THEN IF IT IS MARKED TO BE PERFORMED IN HOST       * 00202000
*/*                      THEN IF ALL PROCESSES LOADED IN HOST         * 00203000
*/*                          THEN THIS PROCESS IS NOT A BHSET,SO      * 00204000
*/*                               EXAMINE THE NEXT ENTRY              * 00205000
*/*                          ELSE DETERMINE IF THIS ENTRY IS A BHSET  * 00206000
*/*                       ELSE PROCESS IS MARKED TO BE DONE IN RN     * 00207000
*/*                            SO RESOLVE THE NAME TO A BHSET ID      * 00208000
*/*                            & SET A FLAG TO INDICATE AT LEAST ONE  * 00209000
*/*                            & SET A FLAG TO INDICATE AT LEAST ONE  * 00210000
*/*                            BHSET SPECIFIED FOR THE RN.            * 00211000
*/*                ELSE PROCESS CAN BE IGNORED                        * 00212000
*/*  2. IF AN ERROR OCCURRS  (BHSET ID NOT FOUND)                     * 00213000
*/*       THEN END THE MASTER DVT SCAN - OPEN DEST WILL FAIL          * 00214000
*/*       ELSE CONTINUE SCANNING THE MASTER DVT                       * 00215000
*/********************************************************************* 00216000
* /*SEGMENT(RSLVMD)START*/DO;GOTO RSLVMD@;RSLVMD:END;              0382 00217000
RSLVMD   DS    0H                                                  0385 00218000
*           DO WHILE CMDVTPTR < CMDVTEND;  /* SCAN MST RDVT */     0385 00219000
         B     @DE00385                                            0385 00220000
@DL00385 DS    0H                                                  0386 00221000
*             IF SDVID = CRDVT  /* IF ENTRY IS FOR DVTNAME */      0386 00222000
*               THEN CMDVDVTE = CMDVTPTR; /* SAVE DVTNAME NTRY PTR */   00223000
         CLC   SDVID(4,CMDVTPTR),@CC02850                          0386 00224000
         BNE   @RF00386                                            0386 00225000
         LR    CMDVDVTE,CMDVTPTR                                   0387 00226000
*               ELSE IF SDVINCLD = CB1  /* IF PROC NAME IN BLDL LIST */ 00227000
*                      THEN IF SDVIN = CB1 /* & IF PROC MARKED INMOST * 00228000
         B     @RC00386                                            0388 00229000
@RF00386 TM    SDVINCLD(CMDVTPTR),B'10000000'                      0388 00230000
         BNO   @RF00388                                            0388 00231000
*                             THEN IF CFDFLGA=CX00 /* & IF BLDL GOOD  * 00232000
         TM    SDVIN(CMDVTPTR),B'00001000'                         0389 00233000
         BNO   @RF00389                                            0389 00234000
*                                    THEN;  /* GET NXT MDVT ENTRY */    00235000
         LTR   CFDFLGA,CFDFLGA                                     0390 00236000
         BZ    @RT00390                                            0390 00237000
*                                    ELSE /*INMST BHST ID*/        0392 00238000
*                                     /*SEGMENT(FNDIDIN)REQUEST*/DO;GOT 00239000
* FNDIDIN;FNDIDIN#:END;                                            0392 00240000
         B     FNDIDIN                                             0393 00241000
*                             ELSE  DO; /* OUTMOST SPECIFIED */    0395 00242000
         B     @RC00389                                            0395 00243000
@RF00389 DS    0H                                                  0396 00244000
*                                     CFDFLGA=CX20;/*SET OUTMOST FLAG*/ 00245000
         LA    CFDFLGA,32                                          0396 00246000
*                                      /*SEGMENT(FNDOUT)REQUEST*/DO;GOT 00247000
* FNDOUT;FNDOUT#:END;                                              0397 00248000
         B     FNDOUT                                              0398 00249000
FNDOUT#  DS    0H                                                  0400 00250000
*                                   END;                           0400 00251000
*                      ELSE; /* CONTINUE */                        0401 00252000
@RF00388 DS    0H                                                  0402 00253000
*           IF CFDFLGA = CX80  /* IF A BHSET ID NOT FOUND */       0402 00254000
*             THEN CMDVTPTR = CMDVTEND; /* END MDVT SCAN */        0402 00255000
@RC00388 DS    0H                                                  0402 00256000
@RC00386 C     CFDFLGA,@CF01130                                    0402 00257000
         BNE   @RF00402                                            0402 00258000
         LR    CMDVTPTR,CMDVTEND                                   0403 00259000
*             ELSE;   /* POINT TO NXT MDVT ENTRY */                0404 00260000
@RF00402 DS    0H                                                  0405 00261000
*            CMDVTPTR = CMDVTPTR+LENGTH(ISTSDVT);/* NXT ENTRY */   0405 00262000
         AL    CMDVTPTR,@CF01402                                   0405 00263000
*           END;                                                   0406 00264000
@DE00385 CR    CMDVTPTR,CMDVTEND                                   0406 00265000
         BL    @DL00385                                            0406 00266000
* /*SEGMENT(RSLVMD)FINISH*/DO;GOTO RSLVMD#;RSLVMD@:END;            0407 00267000
         B     RSLVMD#                                             0408 00268000
RSLVMD@  DS    0H                                                  0410 00269000
* /*SEGMENT(FNDIDIN)START*/DO;GOTO FNDIDIN@;FNDIDIN:END;           0410 00270000
         B     FNDIDIN@                                            0411 00271000
FNDIDIN  DS    0H                                                  0413 00272000
*/********************************************************************* 00273000
*/* THE PROCESS IN THE CURRENT MASTER DVT ENTRY IS MARKED TO BE       * 00274000
*/*INCLUDED IN A REAL DVT & PERFORMED IN THE HOST. HOWEVER IT         * 00275000
*/*MAY REPRESENT A BHSET SINCE NOT ALL MODULES NAMED IN THE           * 00276000
*/*MASTER DVT HAVE BEEN LOADED                                        * 00277000
*/* 1. IF THE BLDL FLAG IS SET IN THE CURRENT ENTRY                   * 00278000
*/*       THEN THE PROCESS WAS LOADED & WILL BE DONE IN THE HOST SO   * 00279000
*/*           NO BHSET ID RESOLUTION IS NECESSARY                     * 00280000
*/*       ELSE 1. INVOKE SRT FIND TO GET THE CORRESPONDING BHSET ID   * 00281000
*/*             FOR THE PROCESS NAME                                  * 00282000
*/*           2. IF A BHSET ID IS RETURNED                            * 00283000
*/*                THEN MAKE SURE IT IS A UNIQUE ID                   * 00284000
*/*                ELSE VERIFY THE REASON NO BHSET ID WAS FOUND       * 00285000
*/*                     AND SET A FLAG TO INDICATE OPEN DEST FAILED   * 00286000
*/********************************************************************* 00287000
*  IF SDVBLDL = CB1  /* IF PROCESS NAME FOUND BY BLDL */           0413 00288000
*    THEN;  /* FUNCTION WILL BE DONE IN HOST,RETURN TO CALLER */   0413 00289000
         TM    SDVBLDL(CMDVTPTR),B'01000000'                       0413 00290000
         BO    @RT00413                                            0413 00291000
*    ELSE DO;  /* PROCESS NAME NOT FOUND BY BLDL */                0415 00292000
*          CALL FINDID;           /* FIND BHSET ID FOR PROCESS NAME */  00293000
         BAL   @14,FINDID                                          0416 00294000
*          RFY CREG15 RSTD;                                /*@XL03HHG*/ 00295000
*          IF CREG15 = C0  /* IF BHSET ID FOUND */                 0418 00296000
*            THEN DO;                                              0418 00297000
         LTR   CREG15,CREG15                                       0418 00298000
         BNZ   @RF00418                                            0418 00299000
*                  RFY CREG15 UNRSTD;                      /*@XL03HHG*/ 00300000
*                  CALL MULTBHST;  /* LOOK MULTIPLE BHSET ID'S */  0421 00301000
         BAL   @14,MULTBHST                                        0421 00302000
*                 END;                                             0422 00303000
*            ELSE DO;  /* BHSET ID NOT FOUND */                    0423 00304000
         B     @RC00418                                            0423 00305000
@RF00418 DS    0H                                                  0424 00306000
*                   CALL DONOFIND;  /* CHECK SRT ERROR CDE */      0424 00307000
         BAL   @14,DONOFIND                                        0424 00308000
*                   CFDFLGA = CX80;  /* SET FAIL FLAG */           0425 00309000
         LA    CFDFLGA,128                                         0425 00310000
*                 END;                                             0426 00311000
*         END;                                                     0427 00312000
@RC00418 DS    0H                                                  0428 00313000
* /*SEGMENT(FNDIDIN)FINISH*/DO;GOTO FNDIDIN#;FNDIDIN@:END;         0428 00314000
@RT00413 DS    0H                                                  0429 00315000
         B     FNDIDIN#                                            0429 00316000
FNDIDIN@ DS    0H                                                  0431 00317000
* /*SEGMENT(FNDOUT)START*/DO;GOTO FNDOUT@;FNDOUT:END;              0431 00318000
         B     FNDOUT@                                             0432 00319000
FNDOUT   DS    0H                                                  0434 00320000
*/********************************************************************* 00321000
*/*  THE PROCESS IN THE CURRENT MASTER DVT ENTRY WAS MARKED TO        * 00322000
*/* BE PERFORMED IN THE RN                                            * 00323000
*/*  1. INVOKE SRT FIND TO GET THE BHSET ID CORRESPONDING             * 00324000
*/*    TO THE PROCESS NAME                                            * 00325000
*/*  2. IF A BHSET ID IS RETURNED                                     * 00326000
*/*       THEN IT IS THE BHSET PREFERRED BY THE USER                  * 00327000
*/*           1. IF PROCESS WAS NOT LOADED IN THE HOST                * 00328000
*/*                THEN IT IS REQUIRED TO BE DONE IN THE RN           * 00329000
*/*                     SO CHECK TO SEE THAT IT IS A UNIQUE ID        * 00330000
*/*                     BECAUSE ONLY 1 REQUIRED ID IS PERMITED        * 00331000
*/*                ELSE IT CAN BE PERFORMED IN THE HOST AS WELL AS RN * 00332000
*/*                      1. IF IT IS THE 1ST ID FOUND                 * 00333000
*/*                           THEN 1. SET A FLAG INDICATING THE 1ST   * 00334000
*/*                                  PREFERRED ID WAS FOUND           * 00335000
*/*                                2. SAVE THE ID & PTR TO CURRENT    * 00336000
*/*                                  MASTER DVT ENTRY                 * 00337000
*/*                           ELSE IGNORE THE ID                      * 00338000
*/*       ELSE 1.VERIFY THE REASON NO BHSET ID WAS FOUND              * 00339000
*/*            2. IF THE PROCESS WAS LOADED IN THE HOST               * 00340000
*/*                 THEN RESET THE RTRN CODE TO SUCCESS BECAUSE       * 00341000
*/*                     THE PROCESS CAN STILL BE DONE IN HOST         * 00342000
*/*                 ELSE SET INTERNAL FLAG INDICATING FAILURE         * 00343000
*/********************************************************************* 00344000
*  RFY CREG0 RSTD;  /*RSTRCT REG.0 FOR BHSET ID */                 0434 00345000
*  CALL FINDID;  /* FIND BHSET ID FOR PROCESS SPECIFIED OUTMOST */ 0435 00346000
         BAL   @14,FINDID                                          0435 00347000
*  RFY CREG15 RSTD;                                        /*@XL03HHG*/ 00348000
*  IF CREG15 = C0  /* IF BHSET ID FOUND */                         0437 00349000
*    THEN DO;                                                      0437 00350000
         LTR   CREG15,CREG15                                       0437 00351000
         BNZ   @RF00437                                            0437 00352000
*           RFY CREG15 UNRSTD;                             /*@XL03HHG*/ 00353000
*           IF SDVBLDL = CB0 /*IF PROCESS NAME NOT FOUND BY BLDL */     00354000
*             THEN CALL MULTBHST; /* LOOK FOR MULTIPLE BHSET ID'S */    00355000
         TM    SDVBLDL(CMDVTPTR),B'01000000'                       0440 00356000
         BNZ   @RF00440                                            0440 00357000
         BAL   @14,MULTBHST                                        0441 00358000
*             ELSE IF OCWPBH = OFF /* IF THIS IS 1ST PRFRD BHSET ID */  00359000
*                    THEN DO;                                      0442 00360000
         B     @RC00440                                            0442 00361000
@RF00440 TM    OCWPBH(OCWPTR),B'00000100'                          0442 00362000
         BNZ   @RF00442                                            0442 00363000
*                           OCWPBH = ON;   /* SET PRFRD BHSET ID FLAG * 00364000
