*    CMDVTPTR = CMDVTPTR+LENGTH(ISTSDVT); /* NXT MDVT ENTRY PTR */ 0563 00699000
@RC00554 AL    CMDVTPTR,@CF01402                                   0563 00700000
*  END;                                                            0564 00701000
@DE00553 CR    CMDVTPTR,CMDVTEND                                   0564 00702000
         BL    @DL00553                                            0564 00703000
* /*SEGMENT(MDUPDT)FINISH*/DO;GOTO MDUPDT#;MDUPDT@:END;            0565 00704000
         B     MDUPDT#                                             0566 00705000
*DONOFIND:PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0568 00706000
         B     @PB00004                                            0568 00707000
DONOFIND DS    0H                                                  0569 00708000
*/********************************************************************* 00709000
*/* DETERMINE THE REASON SRT FIND DID NOT RETURN A BHSET ID           * 00710000
*/*  1. SAVE REG.14 & INCR SAVE AREA PTR TO NXT AVAILABLE SAVE AREA   * 00711000
*/*  2. IF SRT FIND RTRN CODE = X'04'                                 * 00712000
*/*       THEN THE BHSET NAME IS NOT KNOWN TO VTAM                    * 00713000
*/*            THIS IS A LOGICAL ERROR.                               * 00714000
*/*           1. SET THE RETURN CODE = LOGICAL ERROR                  * 00715000
*/*       ELSE IF THE SRT FIND R.C. = X'08'                           * 00716000
*/*              THEN THERE WAS NO SRT DIRETORY.                      * 00717000
*/*                   THIS IS A SYSTEM ERROR                          * 00718000
*/*                  1. SET THE RTRN CODE = SYSTEM ERROR              * 00719000
*/*              ELSE THE SRT RETURN CODE IS OUTSIDE THE RANGE        * 00720000
*/*                   OF R.C.'S FOR SRT FIND.THIS IS AN UNEXPECTED    * 00721000
*/*                   SYSTEM ERROR                                    * 00722000
*/*                  1. SET RTRN CODE = SYSTEM ERROR                  * 00723000
*/********************************************************************* 00724000
*  RFY CREG14 RSTD;                                        /*@XL03HHG*/ 00725000
*  CR14SV = CREG14;  /* SAVE R14 */                                0570 00726000
         L     @15,CR14SVAD(,OCWPTR)                               0570 00727000
         ST    CREG14,CR14SV(,@15)                                 0570 00728000
*  CR14SVAD = CR14SVAD+CSVLN;  /* NXT R14 SAVE AREA PTR */         0571 00729000
         AL    @15,@CF00055                                        0571 00730000
         ST    @15,CR14SVAD(,OCWPTR)                               0571 00731000
*  IF CRTRNCDE = CX04                                              0572 00732000
*    THEN DO;                                                      0572 00733000
         CLI   CRTRNCDE(OCWPTR),4                                  0572 00734000
         BNE   @RF00572                                            0572 00735000
*          OCWRC = '08'X;     /* LOGICAL ERROR */                  0574 00736000
         MVI   OCWRC(OCWPTR),X'08'                                 0574 00737000
*          OCWEC = '50'X;     /* BHSET NAME NOT KNOWN TO VTAM */   0575 00738000
         MVI   OCWEC(OCWPTR),X'50'                                 0575 00739000
*         END;                                                     0576 00740000
*    ELSE DO;                                                      0577 00741000
         B     @RC00572                                            0577 00742000
@RF00572 DS    0H                                                  0578 00743000
*          IF CRTRNCDE = CX08                                      0578 00744000
*            THEN OCWEC = '02'X;     /* NO SRT DIRECTORY */        0578 00745000
         CLI   CRTRNCDE(OCWPTR),8                                  0578 00746000
         BNE   @RF00578                                            0578 00747000
         MVI   OCWEC(OCWPTR),X'02'                                 0579 00748000
*            ELSE OCWEC = '01'X;     /* RC OUTSIDE RANGE */        0580 00749000
         B     @RC00578                                            0580 00750000
@RF00578 MVI   OCWEC(OCWPTR),X'01'                                 0580 00751000
*          OCWRC = '24'X;     /* SYSTEM ERROR */                   0581 00752000
@RC00578 MVI   OCWRC(OCWPTR),X'24'                                 0581 00753000
*         END;                                                     0582 00754000
*  CR14SVAD = CR14SVAD-CSVLN;  /* CURRENT R14 SAVE PTR */          0583 00755000
@RC00572 L     @15,CR14SVAD(,OCWPTR)                               0583 00756000
         SL    @15,@CF00055                                        0583 00757000
         ST    @15,CR14SVAD(,OCWPTR)                               0583 00758000
*  CREG14  = CR14SV;  /* RESTORE R14 */                            0584 00759000
         L     CREG14,CR14SV(,@15)                                 0584 00760000
*  RFY CREG14 UNRSTD;                                      /*@XL03HHG*/ 00761000
*END DONOFIND;                                                     0586 00762000
*                                                                  0586 00763000
