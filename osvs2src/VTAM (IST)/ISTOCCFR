* RFY ISTAPCRR;                                                    0322 00032000
* RFY ISTCRA  ;                                                    0323 00033000
* RFY ISTCNSTA;                                                    0324 00034000
* RFY ISTOCCRR;                                                    0325 00035000
*/* END OF ATBUILD */                                              0326 00036000
* /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                     0326 00037000
*CMZK0007->RPHCRR->CRACRR->NOCCFR = '1'B;                          0326 00038000
         L     @05,RPHCRR(,CMZK0007)                               0326 00039000
         L     @05,CRACRR(,@05)                                    0326 00040000
         OI    NOCCFR(@05),B'00000100'                             0326 00041000
* /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                    0327 00042000
*CMZK0007->RPHCRR->CRACRR->OCCCRT = '0'B;                          0327 00043000
         XC    OCCCRT+1(2,@05),OCCCRT+1(@05)                       0327 00044000
         MVI   OCCCRT(@05),B'00000000'                             0327 00045000
*CMZK0007->RPHCRR->CRACRR->COCCFR = '1'B;                          0328 00046000
         OI    COCCFR(@05),B'00000100'                             0328 00047000
* END;                                                             0329 00048000
*R13 = R13 + OCWSLEN;                    /* NXT AVAIL SAVE AREA PTR */  00049000
         SLR   @05,@05                                             0330 00050000
         IC    @05,OCWSLEN(,R4)                                    0330 00051000
         ALR   R13,@05                                             0330 00052000
*OCWRC = '00'X;                          /* INIT OCW RTRN CDE */   0331 00053000
         MVI   OCWRC(R4),X'00'                                     0331 00054000
*OCWEC = '00'X;                          /* INIT OCW ERROR CDE */  0332 00055000
         MVI   OCWEC(R4),X'00'                                     0332 00056000
*/********************************************************************* 00057000
*/*  1. REMOVE THE FMCB FROM THE ACDEB-FMCB CHAIN & THE NCB-FMCB CHAIN* 00058000
*/*  2. REMOVE THE NCB FROM THE VTAM CTRL BLOCK STRUCTURE             * 00059000
*/*  3. IF NO ERRORS OCCURRED WHILE BREAKING THE CHAINS               * 00060000
*/*       THEN RELEASE THE FMCB & NCB CORE                            * 00061000
*/*       ELSE A SYSTEM ERROR OCCURRED AND NOTHING MORE CAN BE DONE   * 00062000
*/*            WITH THE FMCB & NCB                                    * 00063000
*/********************************************************************* 00064000
*  /*SEGMENT(DOUNLNK)REQUEST*/DO;GOTO DOUNLNK;DOUNLNK#:END;        0333 00065000
         B     DOUNLNK                                             0334 00066000
DOUNLNK# DS    0H                                                  0336 00067000
*/* UNCHAIN FMCB & NCB */                                          0336 00068000
*IF OCWRC = '00'X                        /* IF NO ERRORS OCCURRED */    00069000
*  THEN   /*SEGMENT(DOFREE)REQUEST*/DO;GOTO DOFREE;DOFREE#:END;       / 00070000
* FREE FMCB & NCB CORE */                                          0336 00071000
