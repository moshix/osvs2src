*DCL ZCTYPE BIT(8) CONSTANT('00'X);   /* ZCR TYPE                   */  01998000
*DCL ZCLNG FIXED(15) CONSTANT(172);   /* ZCRR LENGTH                */  01999000
*DCL ZCRSAVNO BIT(8) CONSTANT('06'X); /* NUMBER OF SAVEAREAS IN ZCRR */ 02000000
*DCL ZCRLSVNO BIT(8) CONSTANT('04'X); /* NUMBER OF SAVE AREAS IN   1024 02001000
*                                         LOCAL SUPPORT              */ 02002000
*/* *** EQUATES FOR ZCRRCDSV *** */                                1025 02003000
*                                                                  1025 02004000
*DCL ZCRMRFSB BIT(8) CONSTANT('0C'X);  /* REQUEST COMPLETION MOVE FSB   02005000
*                                         TO PROCESS                 */ 02006000
*DCL ZCRSRCHQ BIT(8) CONSTANT('10'X);  /* REQUEST COMPLETION SEARCH     02007000
*                                         REQUEST QUEUE              */ 02008000
