@DL00405 DS    0H                                                  0406 00203000
*                         IF OCWFMCB = CFMCBPTR                    0406 00204000
*                           THEN DO;    /* REMOVE FIRST FMCB ON CHAIN * 00205000
         L     @14,OCWFMCB(,R4)                                    0406 00206000
         CR    @14,CFMCBPTR                                        0406 00207000
         BNE   @RF00406                                            0406 00208000
*                                 ACDFMCB = FMCNXTCB;  /* UPDTE DEB PTR 00209000
*                                                        1ST FMCB */    00210000
         L     @10,OCWACDEB(,R4)                                   0408 00211000
         MVC   ACDFMCB(4,@10),FMCNXTCB(@14)                        0408 00212000
*                                 CFMCBPTR = 0;  /* END FMCB SCAN */    00213000
         SLR   CFMCBPTR,CFMCBPTR                                   0409 00214000
*                                END;                              0410 00215000
*                           ELSE DO;    /* LOOK AT NXT FMCB ON CHAIN */ 00216000
         B     @RC00406                                            0411 00217000
@RF00406 DS    0H                                                  0412 00218000
*                                 IF CFMCBPTR ->FMCNXTCB = OCWFMCB 0412 00219000
*                                   THEN DO;  /* UNCHAIN FMCB */   0412 00220000
         L     @14,OCWFMCB(,R4)                                    0412 00221000
