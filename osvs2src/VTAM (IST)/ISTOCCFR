SVTM     TITLE 'ISTOCCFR - FMCB RELEASE                                *00001000
                        '                                               00002000
         ISEQ  73,80                                                    00003000
ISTOCCFR CSECT ,                                                   0002 00004000
@MAINENT DS    0H                                                  0002 00005000
         USING *,@15                                               0002 00006000
         B     @PROLOG                                             0002 00007000
         DC    AL1(16)                                             0002 00008000
         DC    C'ISTOCCFR  78.051'                                 0002 00009000
         DROP  @15                                                      00010000
@PROLOG  STM   @14,@12,12(@13)                                     0002 00011000
         BALR  @12,0                                               0002 00012000
@PSTART  DS    0H                                                  0002 00013000
         USING @PSTART,@12                                         0002 00014000
*/********************************************************************* 00015000
*/*  INITIALIZE                                                       * 00016000
*/*   1. COMPONENT RECOVERY RECORD                                    * 00017000
*/*   2. RETURN CODES                                                 * 00018000
*/*   3. PTR TO NEXT AVAILABLE REG SAVE AREA                          * 00019000
*/********************************************************************* 00020000
* DO;                                                              0314 00021000
*/***  CRRMAINT  PROCNM(ISTOCCFR)FUNC(SUB)COMP(OC)  ***/           0315 00022000
* DCL CMZK0007 PTR(31) REG(1);                                     0315 00023000
* RFY (CMZK0007) RSTD;                                             0316 00024000
*                                                                  0316 00025000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0317 00026000
* RFY ISTZSAVE;                                                    0317 00027000
* RFY ISTPFCRR;                                                    0318 00028000
* RFY ISTCFCRR;                                                    0319 00029000
* RFY ISTZCRR ;                                                    0320 00030000
