         B     DOFREE                                              0338 00074000
*  ELSE;                                                           0340 00075000
@RF00336 DS    0H                                                  0341 00076000
*/********************************************************************* 00077000
*/*  PREPARE TO RETURN TO CALLER                                      * 00078000
*/*   1. CLEAR THE OCW CID AREA TO ZERO                               * 00079000
*/*   2. DECREMENT THE REG SAVE AREA PTR TO POINT TO FR'S SAVE AREA   * 00080000
*/*   RETURN TO CALLER WITH RTRN CODE IN REG.15                       * 00081000
*/********************************************************************* 00082000
*OCWCID = 0;                             /* CLEAR OCW CID AREA */  0341 00083000
@RC00336 SLR   @05,@05                                             0341 00084000
         ST    @05,OCWCID(,R4)                                     0341 00085000
*R13 = R13 - OCWSLEN;                   /* FR REG SAVE AREA PTR */ 0342 00086000
         SLR   @05,@05                                             0342 00087000
         IC    @05,OCWSLEN(,R4)                                    0342 00088000
         SLR   R13,@05                                             0342 00089000
*RETURN    CODE(OCWRC);                   /* RETURN TO CALLER */   0343 00090000
         SLR   @15,@15                                             0343 00091000
         IC    @15,OCWRC(,R4)                                      0343 00092000
         L     @14,12(,@13)                                        0343 00093000
         LM    @00,@12,20(@13)                                     0343 00094000
         BR    @14                                                 0343 00095000
*  /*SEGMENT(DOUNLNK)START*/DO;GOTO DOUNLNK@;DOUNLNK:END;          0344 00096000
DOUNLNK  DS    0H                                                  0347 00097000
*/********************************************************************* 00098000
*/*  REMOVE THE FMCB FROM THE ACDEB-FMCB CHAIN                        * 00099000
*/*  IF AN FMCB CANNOT BE LOCATED THRU THE OCW                        * 00100000
*/*    THEN NO FURTHER FMCB PROCESSING IS NECESSARY                   * 00101000
*/*    ELSE REMOVE THE FMCB FROM THE ACDEB                            * 00102000
*/*        IF THE CHAIN IS EMPTY                                      * 00103000
*/*          THEN GO TAKE THE FMCB OFF THE THE NCB CHAIN              * 00104000
*/*          ELSE 1. LOCK THE DEB                                     * 00105000
*/*               2. IF DEB LOCKED SUCCESSFULLY                       * 00106000
*/*                    THEN 1. SCAN THE FMCB CHAIN UNTIL THE ADDR OF  * 00107000
*/*                            THE FMCB MATCHES THE FMCB PTR IN OCW   * 00108000
*/*                         2. REMOVE THIS FMCB FROM THE CHAIN        * 00109000
*/*                         3. UNLOCK THE DEB                         * 00110000
*/*                    ELSE SET RTRN CODE = SYSTEM ERROR BECAUSE      * 00111000
*/*                         DEB COULD NOT BE LOCKED                   * 00112000
*/********************************************************************* 00113000
*IF OCWFMCB = 0                         /* IF NO FMCB PRESENT */   0347 00114000
*  THEN;                                /* RETURN TO CALLER */     0347 00115000
         ICM   @05,15,OCWFMCB(R4)                                  0347 00116000
         BZ    @RT00347                                            0347 00117000
*  ELSE DO;                             /* UNCHAIN FMCB FROM ACDEB 0349 00118000
*                                          AND NCB CHAINS */       0349 00119000
