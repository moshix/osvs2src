*  OCWIDVC = CTABNTRY;          /* INIT DEVICE CODE          @D31JKQG*/ 00313000
         MVC   OCWIDVC(4,OCWPTR),CTABNTRY(CTABLPTR)                0412 00314000
*  RFY CTABLPTR UNRSTD ;                                           0413 00315000
*  RFY EPTPTR RSTD;             /*   FROM NIB DEVTYPE        @D31JKQG*/ 00316000
*   /*SEGMENT(BLDMASK)REQUEST*/DO;GOTO BLDMASK;BLDMASK#:END;   /* BUILD 00317000
*QUALIFICATION MASK  @D31JKQG*/                                    0415 00318000
         B     BLDMASK                                             0416 00319000
