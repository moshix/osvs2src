*               CJPTR=SDVTPTR;                                     0813 01453000
         LR    CJPTR,SDVTPTR                                       0813 01454000
*              END;                                                0814 01455000
*         ELSE DO;                                                 0815 01456000
         B     @RC00810                                            0815 01457000
@RF00810 DS    0H                                                  0816 01458000
*               SDVINCLD=ON;                                       0816 01459000
*               SDVBLDL=ON;                                        0817 01460000
         OI    SDVINCLD(SDVTPTR),B'11000000'                       0817 01461000
*               IF SDVUNCON=ON                                     0818 01462000
*                  THEN CJPTR->SDVCNT=CJPTR->SDVCNT+1;             0818 01463000
         TM    SDVUNCON(SDVTPTR),B'10000000'                       0818 01464000
         BNO   @RF00818                                            0818 01465000
         LA    @09,1                                               0819 01466000
         AH    @09,SDVCNT(,CJPTR)                                  0819 01467000
         STH   @09,SDVCNT(,CJPTR)                                  0819 01468000
*                  ELSE DO; /* TEST QUALIFIER & PROC OPTNS*/       0820 01469000
         B     @RC00818                                            0820 01470000
@RF00818 DS    0H                                                  0821 01471000
*                   /*SEGMENT(TSTQUAL)REQUEST*/DO;GOTO TSTQUAL;TSTQUAL# 01472000
         B     TSTQUAL                                             0822 01473000
*END;                                                              0823 01474000
TSTQUAL# DS    0H                                                  0824 01475000
*                  IF SDVINCLD = ON                        /*@D31JKQG*/ 01476000
*                    THEN                                  /*@D31JKQG*/ 01477000
         TM    SDVINCLD(SDVTPTR),B'10000000'                       0824 01478000
         BNO   @RF00824                                            0824 01479000
*                      DO;                                         0825 01480000
*                        CJPTR -> SDVCNT = CJPTR -> SDVCNT + 1;    0826 01481000
