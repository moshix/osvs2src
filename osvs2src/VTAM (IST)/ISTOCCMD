SVTM     TITLE 'ISTOCCMD - MASTER DVT BUILD                            *00001000
                        '                                               00002000
ISTOCCMD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTOCCMD  76.343'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0289 00014000
*     DO;                                                          0289 00015000
*/***  CRRMAINT  PROCNM(ISTOCCMD)COMP(OC)FUNC(SUB)  ***/           0290 00016000
*     DCL CMZK0008 PTR(31) REG(1);                                 0290 00017000
*     RFY (CMZK0008) RSTD;                                         0291 00018000
*                                                                  0291 00019000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0292 00020000
*     RFY ISTLCCW ;                                                0292 00021000
*     RFY ISTLCPB ;                                                0293 00022000
*     RFY ISTZSAVE;                                                0294 00023000
*     RFY ISTPFCRR;                                                0295 00024000
*     RFY ISTCFCRR;                                                0296 00025000
*     RFY ISTZCRR ;                                                0297 00026000
*     RFY ISTASCRR;                                                0298 00027000
*     RFY ISTAPCRR;                                                0299 00028000
*     RFY ISTCRA  ;                                                0300 00029000
*     RFY ISTCNSTA;                                                0301 00030000
*     RFY ISTOCCRR;                                                0302 00031000
*/* END OF ATBUILD */                                              0303 00032000
*     /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                 0303 00033000
*CMZK0008->RPHCRR->CRACRR->NOCCMD = '1'B;                          0303 00034000
         L     @09,RPHCRR(,CMZK0008)                               0303 00035000
         L     @09,CRACRR(,@09)                                    0303 00036000
         OI    NOCCMD(@09),B'00000010'                             0303 00037000
*     /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                0304 00038000
*CMZK0008->RPHCRR->CRACRR->OCCCRT = '0'B;                          0304 00039000
         XC    OCCCRT+1(2,@09),OCCCRT+1(@09)                       0304 00040000
         MVI   OCCCRT(@09),B'00000000'                             0304 00041000
*CMZK0008->RPHCRR->CRACRR->COCCMD = '1'B;                          0305 00042000
         OI    COCCMD(@09),B'00000010'                             0305 00043000
*     END;                                                         0306 00044000
*/********************************************************************/ 00045000
*/*  INITIALIZE                                                      */ 00046000
*/*    1. COMPONENT RECOVERY RECORD                                  */ 00047000
*/*    2. PTR TO SKELETON DVT                                        */ 00048000
*/*    3. RTRN CODES                                                 */ 00049000
*/*    4. OCW SCRATCH PAD                                            */ 00050000
*/*  SAVE PTR TO SKELETON/MASTER DVT IN OCW                          */ 00051000
*/********************************************************************/ 00052000
*    RFY SDVTPTR RSTD;  /* RSTRCT REG.7 FOR SDVT NTRY PTR */       0307 00053000
*    RFY CR0 RSTD;                                        /* @XL03HHG*/ 00054000
*    SDVTPTR = CR0;                                                0309 00055000
         LR    SDVTPTR,CR0                                         0309 00056000
*    RFY CR0 UNRSTD;                                      /* @XL03HHG*/ 00057000
*    OCWMDVT = SDVTPTR;  /* INIT MSTR DVT PTR */                   0311 00058000
         ST    SDVTPTR,OCWMDVT(,OCWPTR)                            0311 00059000
*    OCWRC = '00'X;     /* INIT MAJOR R.C.  = 0 = SUCCESS */       0312 00060000
         MVI   OCWRC(OCWPTR),X'00'                                 0312 00061000
*    OCWEC = '00'X;     /* INIT MINOR R.C.  = 0 = SUCCESS */       0313 00062000
         MVI   OCWEC(OCWPTR),X'00'                                 0313 00063000
*    OCWSCPAD = OCWSCPAD&&OCWSCPAD;  /* INIT SCRATCH PAD = 0 */    0314 00064000
         XC    OCWSCPAD(28,OCWPTR),OCWSCPAD(OCWPTR)                0314 00065000
*/********************************************************************/ 00066000
*/*  MAKE SURE THAT DEST & MODE SPECIFIED BY USER ARE COMPATIBLE     */ 00067000
*/*  IF THE MODE NAME IS 'BASIC'                                     */ 00068000
*/*     AND IF THE DEST IS A LOGICAL UNIT                            */ 00069000
*/*           THEN SET RTRN CODE = LOGICAL ERROR & RETURN TO CALLER  */ 00070000
*/*           ELSE DEST & MODE ARE COMPATIBLE                        */ 00071000
*/*  IF THE MODE NAME IS 'RECORD'                                    */ 00072000
*/*     AND IF THE DEST IS NOT A LOGICAL UNIT                        */ 00073000
*/*    THEN SET RTRN CODE = LOGICAL ERROR & RETURN TO CALLER         */ 00074000
*/*    ELSE DEST & MODE ARE COMPATIBLE                               */ 00075000
*/********************************************************************/ 00076000
*   IF NIBMODE = 'ISTYCONT'                     /* CONTROL   @XL03HHG*/ 00077000
*     | NIBMODE = 'ISTYSSCP'                                       0315 00078000
*     | NIBMODE = 'ISTYCLUS'                                       0315 00079000
*     | NIBMODE = 'ISTYPSOL'                       /* MODE   @XL03HHG*/ 00080000
*     | NIBMODE = 'ISTYTCM2'           /* TCAM CONTROL MODE  @D31KKJY*/ 00081000
*     THEN                                                         0315 00082000
         L     @09,OCWNIB(,OCWPTR)                                 0315 00083000
         CLC   NIBMODE(8,@09),@CC03091                             0315 00084000
         BE    @RT00315                                            0315 00085000
         CLC   NIBMODE(8,@09),@CC03092                             0315 00086000
         BE    @RT00315                                            0315 00087000
         CLC   NIBMODE(8,@09),@CC03093                             0315 00088000
         BE    @RT00315                                            0315 00089000
         CLC   NIBMODE(8,@09),@CC03094                             0315 00090000
         BE    @RT00315                                            0315 00091000
         CLC   NIBMODE(8,@09),@CC03095                             0315 00092000
         BNE   @RF00315                                            0315 00093000
@RT00315 DS    0H                                                  0316 00094000
*       IF OCWARDTE->RAPALLF=ON        /* & PRIVILEGED USER  @XL03HHG*/ 00095000
*         THEN;                        /* THIS IS COMPATABLE @XL03HHG*/ 00096000
         L     @09,OCWARDTE(,OCWPTR)                               0316 00097000
         TM    RAPALLF(@09),B'10000000'                            0316 00098000
         BO    @RT00316                                            0316 00099000
*         ELSE                         /* NO,                @XL03HHG*/ 00100000
*           DO;                                                    0318 00101000
*             OCWRC='08'X;             /* LOGIC ERROR        @XL03HHG*/ 00102000
         MVI   OCWRC(OCWPTR),X'08'                                 0319 00103000
*             OCWEC='51'X;             /* UNAUTHORIZED USER  @XM07300*/ 00104000
         MVI   OCWEC(OCWPTR),X'51'                                 0320 00105000
*           END;                       /*                    @XL03HHG*/ 00106000
*     ELSE                             /* NON-PREVLDGD MODE  @XL03HHG*/ 00107000
*       IF NIBMODE = 'BASIC   '        /* USER'S NIB MARKED BASIC,OR */ 00108000
*         | NIBMODE = 'ISTYTCM1'       /* TCAM BASIC         @D31KKJY*/ 00109000
*         THEN                         /* YES                        */ 00110000
         B     @RC00315                                            0322 00111000
@RF00315 L     @09,OCWNIB(,OCWPTR)                                 0322 00112000
         CLC   NIBMODE(8,@09),@CC03096                             0322 00113000
         BE    @RT00322                                            0322 00114000
         CLC   NIBMODE(8,@09),@CC03097                             0322 00115000
         BNE   @RF00322                                            0322 00116000
@RT00322 DS    0H                                                  0323 00117000
*           IF RPRENTRY=RPRENTLU       /* RDTE A LOGICAL UNIT?       */ 00118000
*             THEN                     /* YES--THIS IS NOT COMPATABLE*/ 00119000
         L     @09,OCWDRDTE(,OCWPTR)                               0323 00120000
         CLI   RPRENTRY(@09),X'81'                                 0323 00121000
         BNE   @RF00323                                            0323 00122000
*               DO;                                                0324 00123000
*                 OCWRC='08'X;         /* INDICATE LOGIC ERROR       */ 00124000
         MVI   OCWRC(OCWPTR),X'08'                                 0325 00125000
*                 OCWEC='4E'X;         /* INDICATE DEVICE/MODE ERROR */ 00126000
         MVI   OCWEC(OCWPTR),X'4E'                                 0326 00127000
*               END;                   /* INCOMPATABLE       @XM07466*/ 00128000
*             ELSE;                    /* BASIC BUT NOT LOGICAL UNIT */ 00129000
@RF00323 DS    0H                                                  0329 00130000
*         ELSE                         /* NOT BASIC MODE             */ 00131000
*           IF NIBMODE='RECORD  '      /* USER'S NIB MARKED RECORD,  */ 00132000
*             | NIBMODE = 'ISTYTCM3'   /* TCAM RECORD MODE?  @D31KKJY*/ 00133000
*             THEN                     /* YES                        */ 00134000
         B     @RC00322                                            0329 00135000
@RF00322 L     @09,OCWNIB(,OCWPTR)                                 0329 00136000
         CLC   NIBMODE(8,@09),@CC03099                             0329 00137000
         BE    @RT00329                                            0329 00138000
         CLC   NIBMODE(8,@09),@CC03100                             0329 00139000
         BNE   @RF00329                                            0329 00140000
@RT00329 DS    0H                                                  0330 00141000
*               IF RPRENTRY=RPRENTLU   /* RDTE A LOGICAL UNIT?       */ 00142000
*                 THEN;                /* YES---THIS IS COMPATABLE   */ 00143000
         L     @09,OCWDRDTE(,OCWPTR)                               0330 00144000
         CLI   RPRENTRY(@09),X'81'                                 0330 00145000
         BE    @RT00330                                            0330 00146000
*                 ELSE                 /* RECORD BUT NOT LOGICAL UNIT*/ 00147000
*                   IF RCPRBASC=ON & RCPRRECD=ON /*          @XL03HHG*/ 00148000
*                     THEN;                                        0332 00149000
         L     @09,OCWDRDTE(,OCWPTR)                               0332 00150000
         TM    RCPRBASC(@09),B'11000000'                           0332 00151000
         BO    @RT00332                                            0332 00152000
*                     ELSE             /*                    @XM07466*/ 00153000
*                       DO;                                        0334 00154000
*                         OCWRC='08'X;    /* INDICATE A LOGIC ERROR  */ 00155000
         MVI   OCWRC(OCWPTR),X'08'                                 0335 00156000
*                         OCWEC='4E'X;    /* INDICATE DEVICE/MODE    */ 00157000
         MVI   OCWEC(OCWPTR),X'4E'                                 0336 00158000
*                       END;            /*  INCOMPATABLE     @XM07466*/ 00159000
*             ELSE;                    /* NOT BASIC NOR RCD  @XL03HHG*/ 00160000
*                                                                  0338 00161000
@RF00329 DS    0H                                                  0339 00162000
*   /* RETURN IF ANY ERROR HAD OCCURRED                              */ 00163000
*   IF OCWRC = '00'X                   /* ERROR FOUND?       @X04BHJY*/ 00164000
*     THEN                             /* NO ERROR           @X04BHJY*/ 00165000
@RC00329 DS    0H                                                  0339 00166000
@RC00322 DS    0H                                                  0339 00167000
@RC00315 CLI   OCWRC(OCWPTR),X'00'                                 0339 00168000
         BNE   @RF00339                                            0339 00169000
*       DO;                                                        0340 00170000
*          /*SEGMENT(EPTSCRH)REQUEST*/DO;GOTO EPTSCRH;EPTSCRH#:END;   / 00171000
* CHECK IF DUPLICATE EPT                                           0341 00172000
* ALREADY EXISTS     @D31JKQG*/                                    0341 00173000
         B     EPTSCRH                                             0342 00174000
EPTSCRH# DS    0H                                                  0344 00175000
*         IF OCWEPTAB = 0              /* EPT NOT FOUND,AND  @D31JKQG*/ 00176000
*            & OCWRC = 0               /* NO ERROR           @D31JKQG*/ 00177000
*           THEN                       /* YES                @D31JKQG*/ 00178000
         L     @09,OCWEPTAB(,OCWPTR)                               0344 00179000
         LTR   @09,@09                                             0344 00180000
         BNZ   @RF00344                                            0344 00181000
         CLI   OCWRC(OCWPTR),0                                     0344 00182000
         BNE   @RF00344                                            0344 00183000
*             DO;                                                  0345 00184000
*/********************************************************************/ 00185000
*/*  DETERMINE WHICH & HOW MANY PROCESS MODULES ARE TO BE INCLUDED   */ 00186000
*/*  IN THE REAL DVT TO BE BUILT FOR EACH DVT NAME IN THE SKELETON   */ 00187000
*/*  DVT. WHEN THIS IS DONE SUCCESSFULLY THE SKELETON DVT IS         */ 00188000
*/*  REFERRED TO AS THE MASTER DVT                                   */ 00189000
*/********************************************************************/ 00190000
*                                                                  0346 00191000
*               /* THE COUNT OF SDVT ENTRIES IS IN THE FIRST SDVT    */ 00192000
*               /* ENTRY'S SDVTOTAL FIELD. SINCE SDVPPTR IS UPDATED  */ 00193000
*               /* DURING LOOP, THE COUNT MUST BE PICKED UP BEFORE   */ 00194000
*               /* ENTERING THE LOOP                         @X04BHJY*/ 00195000
*               CWORK = SDVTOTAL;      /* PICK UP ENTRY COUNT        */ 00196000
         LH    CWORK,SDVTOTAL(,SDVTPTR)                            0346 00197000
*               DO CJ = 1 TO CWORK WHILE OCWRC = '00'X;    /*@XL03HHG*/ 00198000
         LA    CJ,1                                                0347 00199000
         B     @DE00347                                            0347 00200000
@DL00347 CLI   OCWRC(OCWPTR),X'00'                                 0347 00201000
         BNE   @DC00347                                            0347 00202000
*                  /*SEGMENT(CMDRSLV)REQUEST*/DO;GOTO CMDRSLV;CMDRSLV#: 00203000
*ND;                                                               0348 00204000
         B     CMDRSLV                                             0349 00205000
CMDRSLV# DS    0H                                                  0351 00206000
*                 SDVTPTR = SDVTPTR+LENGTH(ISTSDVT);               0351 00207000
         AL    SDVTPTR,@CF01488                                    0351 00208000
*               END;                                               0352 00209000
         AL    CJ,@CF00060                                         0352 00210000
@DE00347 CR    CJ,CWORK                                            0352 00211000
         BNH   @DL00347                                            0352 00212000
@DC00347 DS    0H                                                  0353 00213000
*               /* DELETE EXCEPTED DVC ENTRIES               @D31JKQG*/ 00214000
*                /*SEGMENT(DVCEHSK)REQUEST*/DO;GOTO DVCEHSK;DVCEHSK#:EN 00215000
*;                /* @D31JKQG*/                                    0353 00216000
         B     DVCEHSK                                             0354 00217000
DVCEHSK# DS    0H                                                  0356 00218000
*             END;                                                 0356 00219000
*           ELSE;                      /* ERROR OR EPT FOUND @D31JKQG*/ 00220000
@RF00344 DS    0H                                                  0358 00221000
*       END;                                                       0358 00222000
*     ELSE;                            /* ERROR SOMEWHERE    @X04BHJY*/ 00223000
@RF00339 DS    0H                                                  0360 00224000
*   RETURN CODE(OCWRC);                                            0360 00225000
         SLR   @15,@15                                             0360 00226000
         IC    @15,OCWRC(,OCWPTR)                                  0360 00227000
         L     @14,12(,@13)                                        0360 00228000
         LM    @00,@12,20(@13)                                     0360 00229000
         BR    @14                                                 0360 00230000
* /*SEGMENT(EPTSCRH)START*/DO;GOTO EPTSCRH@;EPTSCRH:END;           0361 00231000
EPTSCRH  DS    0H                                                  0364 00232000
*/********************************************************************* 00233000
*/* DETERMINE IF THERE IS ALREADY A EPT THAT WILL SATISIFY            * 00234000
*/* THIS OPEN DESTINATION NIB. FIRST CHECK IF THE MODE AND CONDITIONA*/ 00235000
*/* OPTIONS ARE EQUAL. THEN IF QUALIFICATION EQUIVALENT              */ 00236000
*/*  CHECK IF THE DVC ENTRIES ARE THE SAME.  IF MATCH FOUND UPDATE*/    00237000
*/* THE USE COUNTS.  IF NOT EQUAL RETURN WITH NOCOMPAR= SET.          * 00238000
*/********************************************************************/ 00239000
*                                                                  0364 00240000
*/*** START -TPLOCK (DVTLOCK)TYPE(XCLSIVE)- ***/                   0364 00241000
*   DO;                                                            0364 00242000
*   DCL GPR05F FIXED(31) REG(05);                                  0365 00243000
*   DCL GPR05P PTR(31)   REG(05);                                  0366 00244000
*   DCL GPR06F FIXED(31) REG(06);                                  0367 00245000
*   DCL GPR06P PTR(31)   REG(06);                                  0368 00246000
