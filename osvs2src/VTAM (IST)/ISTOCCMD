         STH   @09,SDVCNT(,CJPTR)                                  0826 01484000
*                        ODOQUAL =(ODOQUAL & OCWSDVQL);    /*@D31JKQG*/ 01485000
         NC    ODOQUAL(2,ODOPTR),OCWSDVQL(OCWPTR)                  0827 01486000
*                      END ;                                       0828 01487000
*                    ELSE                                          0829 01488000
*                      /* TURN OFF QUAL FLAG(S) FOR EXCEPTED MODULES */ 01489000
*                      ODOEQUAL=ODOEQUAL | OCWSDVQL;      /* @DM12945*/ 01490000
         B     @RC00824                                            0829 01491000
@RF00824 OC    ODOEQUAL(2,ODOPTR),OCWSDVQL(OCWPTR)                 0829 01492000
*                  END ;                                           0830 01493000
@RC00824 DS    0H                                                  0831 01494000
*              END;                                                0831 01495000
@RC00818 DS    0H                                                  0832 01496000
