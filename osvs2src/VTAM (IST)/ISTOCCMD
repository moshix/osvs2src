@DL00421 DS    0H                                                  0422 00328000
*    ODOPTR = EPTPTR + LENGTH(ISTEPT)+(EPTHECNT*LENGTH(ISTEPTE)) ; 0422 00329000
*                                                                  0422 00330000
         LA    @09,12                                              0422 00331000
         LR    ODOPTR,EPTPTR                                       0422 00332000
         ALR   ODOPTR,@09                                          0422 00333000
         MH    @09,EPTHECNT(,EPTPTR)                               0422 00334000
         ALR   ODOPTR,@09                                          0422 00335000
*    /* OCWIDVC AND ODODVC HAVE SAME DVC IF EITHER ALL BYTES MATCH   */ 00336000
*    /* OR THE UNMATCHING BYTES IN ODODVC ARE X'00' INSTEAD  @X04BHJY*/ 00337000
*                                                                  0423 00338000
*    DO CI=1 TO 4 WHILE ODODVC(CI)=OCWIDVC(CI)                     0423 00339000
*                      | ODODVC(CI) = '00'X;              /* @X04BHJY*/ 00340000
         LA    CI,1                                                0423 00341000
@DL00423 LR    @09,ODOPTR                                          0423 00342000
         ALR   @09,CI                                              0423 00343000
