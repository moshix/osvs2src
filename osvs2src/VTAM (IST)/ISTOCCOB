         ICM   @00,15,ACBAPID(@15)                                 0318 00251000
         BNZ   @RF00318                                            0318 00252000
*           DO;                     /* @ONE                          */ 00253000
*             OCXAPDLN=APLIDMAX;    /* SET DEFLT APPLID LEN          */ 00254000
         MVI   OCXAPDLN(RCOREXT),X'08'                             0320 00255000
*                                                                  0321 00256000
*/* APPLIDEF (GPR02P,GPR03P)  INVOKED.                               */ 00257000
*                                                                  0321 00258000
*             /*******************************************************/ 00259000
*             /*                                                     */ 00260000
*             /* INPUT: PARM1 GARBAGE PARM2 GARBAGE OUTPUT: PARM1    */ 00261000
*             /* ADDR OF THE JOBSTEP NAME PARM2 GARBAGE NOTES: THE   */ 00262000
*             /* INVOKING MODULE MUST PROVIDE INCLUDES FOR THE       */ 00263000
*             /* FOLLOWING FROM SYSLIB: CVT - COMM VECTOR TABLE      */ 00264000
*             /* IKJTCB - TASK CONTROL BLOCK IEFTIOT1 - TASK I/O     */ 00265000
*             /* TABLE                                               */ 00266000
*             /*                                                     */ 00267000
*             /*******************************************************/ 00268000
*                                                                  0321 00269000
*             DO;                                                  0321 00270000
*               GEN SETS(GPR02P) REFS(GPR03P);                     0322 00271000
         LA    GPR03P,16               GET ADDRESS OF CVT ADDRESS IN    00272000
*                                      LOW CORE.                        00273000
         L     GPR02P,0(,GPR03P)       GET ADDRESS OF CVT FROM LOW      00274000
*                                      CORE LOCATION.                   00275000
*               RFY                                                0323 00276000
*                (GPR02P,                                          0323 00277000
*                 GPR03P) RSTD;                                    0323 00278000
*               GPR03P=GPR02P->CVTTCBP;/* GET NEW/OLD TCB POINTER  0324 00279000
*                                      FROM THE CVT.                 */ 00280000
         L     GPR03P,CVTTCBP(,GPR02P)                             0324 00281000
*               GEN SETS(GPR02P);                                  0325 00282000
