         ST    @00,ACBAPID(,@15)                                   0333 00313000
*           END;                    /* @ONE                          */ 00314000
*         ELSE                      /* APPLID PRESENT @ONE           */ 00315000
*           DO;                     /* @ONE                          */ 00316000
         B     @RC00318                                            0335 00317000
