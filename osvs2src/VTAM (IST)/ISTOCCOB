*                    2 SAV40047 CHAR(4),                           0344 00381000
*                    2 SAV60047 CHAR(4),                           0344 00382000
*                    2 SAVE0047 CHAR(4);                           0344 00383000
*                 SAV10047=GPR01F;  /* SAVE REGS ONE,                */ 00384000
         ST    GPR01F,SAV10047(,GPR13P)                            0345 00385000
*                 SAV20047=GPR02F;  /* TWO,                          */ 00386000
         ST    GPR02F,SAV20047(,GPR13P)                            0346 00387000
*                 SAV40047=GPR04F;  /* FOUR,                         */ 00388000
         ST    GPR04F,SAV40047(,GPR13P)                            0347 00389000
*                 SAV60047=GPR06F;  /* SIX,                          */ 00390000
         ST    GPR06F,SAV60047(,GPR13P)                            0348 00391000
*                 SAVE0047=GPR14F;  /* AND FOURTEEN                  */ 00392000
         ST    GPR14F,SAVE0047(,GPR13P)                            0349 00393000
*                 GPR13P=GPR13P+20;                                0350 00394000
         AH    GPR13P,@CH02314                                     0350 00395000
*VC10047:                                                          0351 00396000
*                                                                  0351 00397000
*/*VSETLCK  (15,OBTAIN,LOCAL,(LOCAL,VALCHEK(VC20047)))               */ 00398000
*                                                                  0351 00399000
*                 DO;               /* DECLARE AND RESTRICT REGS   0351 00400000
*                                      2,10,11,12,13,14, AND 15      */ 00401000
