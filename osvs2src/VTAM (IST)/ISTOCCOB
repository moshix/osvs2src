*               RFY                                                0328 00291000
*                (GPR02P,                                          0328 00292000
*                 GPR03P) UNRSTD;                                  0328 00293000
*             END;                                                 0329 00294000
*                                                                  0329 00295000
*/* END OF APPLIDEF EXPANSION.                                       */ 00296000
*                                                                  0330 00297000
*                                   /* GET DEFLT STEPNAME            */ 00298000
*             RFY                                                  0330 00299000
*               GPR02P RSTD;        /* @ONE                          */ 00300000
*             OCXAPDDT(1:OCXAPDLN)=GPR02P->APLDSECT;/* COPY DATA   0331 00301000
*                                                            @XM09085*/ 00302000
