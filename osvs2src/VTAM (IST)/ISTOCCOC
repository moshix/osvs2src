*             RAPATLGN=ON;          /* TEMP. BLOCK LOGONS            */ 00636000
         OI    RAPATLGN(RDTREG),B'00010000'                        0497 00637000
*           ELSE                    /*                       @ZA13647*/ 00638000
*             RAPABLGN=ON;          /* 'NEVER ACCEPT' BIT            */ 00639000
         B     @RC00496                                            0498 00640000
@RF00496 OI    RAPABLGN(RDTREG),B'10000000'                        0498 00641000
*         END;                      /*                       @X50BHXL*/ 00642000
*       ELSE                                                       0500 00643000
*         ;                         /*                       @X50BHXL*/ 00644000
@RF00494 DS    0H                                                  0501 00645000
*     END;                                                         0501 00646000
*   ELSE                                                           0502 00647000
*     ;                                                            0502 00648000
@RF00480 DS    0H                                                  0503 00649000
*   IF ACBERFLG='00'X               /* IF ACB ERROR CODE NOT ON THEN    00650000
*                                      CONT.                 @ZA08960*/ 00651000
*     THEN                                                         0503 00652000
@RC00480 CLI   ACBERFLG(ACBREG),X'00'                              0503 00653000
         BNE   @RF00503                                            0503 00654000
*     IF(RAPRSV10='1'B)&((DEBREG->ACDTSKID->PSTTCBA->TCBJSCB->JSCBAUTH= 00655000
*         '1'B)|(DEBREG->ACDTSKID->PSTTCBA->TCBJSTCB=PSATOLD))     0504 00656000
*                                                                  0504 00657000
*     /***************************************************************/ 00658000
*     /*                                                             */ 00659000
*     /* IF AUTH=TSO WAS SPECIFIED ON APPL MACRO AND IF TCB IS APF   */ 00660000
*     /* AUTHORIZED OR TASK IS IN SUPERVISOR STATE/KEY 0 THEN THEN   */ 00661000
*     /* SET BIT IN ACDEB TO BYPASS SYNCH                    @ZA08960*/ 00662000
*     /*                                                             */ 00663000
*     /***************************************************************/ 00664000
*                                                                  0504 00665000
*       THEN                                                       0504 00666000
         TM    RAPRSV10(RDTREG),B'10000000'                        0504 00667000
         BNO   @RF00504                                            0504 00668000
         L     @11,ACDTSKID(,DEBREG)                               0504 00669000
         L     @11,PSTTCBA(,@11)                                   0504 00670000
         L     @02,TCBJSCB(,@11)                                   0504 00671000
         TM    JSCBAUTH(@02),B'00000001'                           0504 00672000
         BO    @RT00504                                            0504 00673000
         CLC   TCBJSTCB(4,@11),PSATOLD                             0504 00674000
         BNE   @RF00504                                            0504 00675000
@RT00504 DS    0H                                                  0505 00676000
*       DEBREG->ACTSOAPF='1'B;      /*                       @ZA08960*/ 00677000
         OI    ACTSOAPF(DEBREG),B'00001000'                        0505 00678000
*     ELSE                                                         0506 00679000
*       ;                           /* IF NOT APF AND AUTH = TSO DO     00680000
*                                      NOTHING               @ZA08960*/ 00681000
@RF00504 DS    0H                                                  0507 00682000
*   ELSE                                                           0507 00683000
*     ;                             /* IF ACB ERR CODE ON THEN DO  0507 00684000
*                                      NOTHING               @ZA08960*/ 00685000
@RF00503 DS    0H                                                  0508 00686000
*   RFY                                                            0508 00687000
*     REG14 RSTD;                                                  0508 00688000
*   REG14=OCXRG14A;                 /* RESTORE REG 14                */ 00689000
         L     REG14,OCXRG14A(,RWORKXT)                            0509 00690000
*   RFY                                                            0510 00691000
*     REG14 UNRSTD;                                                0510 00692000
*   RETURN;                                                        0511 00693000
@EL00002 DS    0H                                                  0511 00694000
@EF00002 DS    0H                                                  0511 00695000
@ER00002 BR    @14                                                 0511 00696000
*   END COC100;                                                    0512 00697000
