*/********************************************************************/ 00513000
*/*                                                                  */ 00514000
*/*  ROUTINE NAME : COC100                                           */ 00515000
*/*                                                                  */ 00516000
*/*  FUNCTION : THIS SUBROUTINE CHECKS TO SEE IF THE APPLICATION RDT */ 00517000
*/*             IS PASSWORD PROTECTED.  IF IT IS, THE USER SUPPLIED  */ 00518000
*/*             PASSWORD MUST MATCH THE PASSWORD IN THE APPL RDT. IF */ 00519000
*/*             NO MATCH, AN ERROR FLAG IS SET.  IF NO LOGONS ARE    */ 00520000
*/*             ALLOWED, THE "NEVER ACCEPT" BIT IN THE APPL RDT IS   */ 00521000
*/*             TURNED ON, AND PENDING LOGONS ARE RELEASED.          */ 00522000
*/*             IF THIS APPLICATION IS FLAGGED AS THE PROGRAM        */ 00523000
*/*             OPERATOR, ISTCFCPO IS CALLED TO SET UP PROGRAM       */ 00524000
*/*             OPERATOR CONTROL BLOCKS.                     @X50BHXL*/ 00525000
*/*                                                                  */ 00526000
*/*  ROUTINES CALLED : ISTCFCPO (BUILD PROG. OP. CONTROL BLOCKS)     */ 00527000
*/*                                                                  */ 00528000
*/*  MACROS USED : CRRMAINT                                  @X50BHXL*/ 00529000
*/*                                                                  */ 00530000
*/********************************************************************/ 00531000
*                                                                  0461 00532000
*COC100:                                                           0461 00533000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0461 00534000
         B     @PB00002                                            0461 00535000
COC100   DS    0H                                                  0462 00536000
*   RFY                                                            0462 00537000
*     REG14 RSTD;                                                  0462 00538000
*   OCXRG14A=REG14;                 /* SAVE REG 14                   */ 00539000
         ST    REG14,OCXRG14A(,RWORKXT)                            0463 00540000
*   RFY                                                            0464 00541000
*     REG14 UNRSTD;                                                0464 00542000
*   DEBREG=ACBDEB;                  /* SET UP DEB BASE REG           */ 00543000
         SLR   DEBREG,DEBREG                                       0465 00544000
         ICM   DEBREG,7,ACBDEB(ACBREG)                             0465 00545000
*   ACDRDTE=REG1;                   /* SAVE APPL RDTE PTR            */ 00546000
         ST    REG1,ACDRDTE(,DEBREG)                               0466 00547000
*   RDTREG=ACDRDTE;                 /* SET UP APPL RDT BASE          */ 00548000
         L     @11,ACDRDTE(,DEBREG)                                0467 00549000
         LR    RDTREG,@11                                          0467 00550000
*   IF RAPAPPWD^=BLANKS             /* IF PASSWD PROTECTED           */ 00551000
*     THEN                                                         0468 00552000
         CLC   RAPAPPWD(8,RDTREG),@CB03848                         0468 00553000
         BE    @RF00468                                            0468 00554000
*     IF ACBPASSW^=0                /* USER SUPPLIED PASSWD          */ 00555000
*       THEN                                                       0469 00556000
         L     @11,ACBPASSW(,ACBREG)                               0469 00557000
         LTR   @11,@11                                             0469 00558000
         BZ    @RF00469                                            0469 00559000
*       DO;                                                        0470 00560000
*         RFY                                                      0471 00561000
*           REG5 RSTD;                                             0471 00562000
*         REG5=ACBPASSW+1;          /* ACCOUNT FOR LNGTH BYTE        */ 00563000
         AH    @11,@CH00051                                        0472 00564000
         LR    REG5,@11                                            0472 00565000
*         IF REG5->PASSWD=RAPAPPWD  /* IF MATCH                      */ 00566000
*           THEN                                                   0473 00567000
         CLC   PASSWD(8,REG5),RAPAPPWD(RDTREG)                     0473 00568000
         BE    @RT00473                                            0473 00569000
*           ;                                                      0474 00570000
*         ELSE                                                     0475 00571000
*           ACBERFLG='24'X;         /* BAD PASSWD ERROR CODE         */ 00572000
         MVI   ACBERFLG(ACBREG),X'24'                              0475 00573000
*         RFY                                                      0476 00574000
*           REG5 UNRSTD;                                           0476 00575000
@RT00473 DS    0H                                                  0477 00576000
*       END;                                                       0477 00577000
*     ELSE                                                         0478 00578000
*       ACBERFLG='24'X;             /* BAD PASSWD ERROR CODE         */ 00579000
         B     @RC00469                                            0478 00580000
@RF00469 MVI   ACBERFLG(ACBREG),X'24'                              0478 00581000
*   ELSE                                                           0479 00582000
*     ;                                                            0479 00583000
@RF00468 DS    0H                                                  0480 00584000
*   IF ACBERFLG='00'X               /* IF ACB COPY ERR CODE 0        */ 00585000
*     THEN                                                         0480 00586000
@RC00468 CLI   ACBERFLG(ACBREG),X'00'                              0480 00587000
         BNE   @RF00480                                            0480 00588000
*     DO;                                                          0481 00589000
*       IF RAPAPPO=ON|RAPASPO=ON    /* IS THIS PGM OP?       @X50BHXL*/ 00590000
*         THEN                      /*                       @X50BHXL*/ 00591000
         TM    RAPAPPO(RDTREG),B'00000110'                         0482 00592000
         BZ    @RF00482                                            0482 00593000
*         DO;                       /*                       @X50BHXL*/ 00594000
*           REG1=REG1SAVE;          /* RESTORE RPH ADDRESS   @X50BHXL*/ 00595000
         L     REG1,REG1SAVE+120(,RWORKXT)                         0484 00596000
*           CALL ISTCFCPO;          /* SET UP PO CTL BLKS    @X50BHXL*/ 00597000
         L     @15,@CV03919                                        0485 00598000
         BALR  @14,@15                                             0485 00599000
*           DO;                                                    0486 00600000
*                                                                  0486 00601000
*/***  CRRMAINT  PROCNM(ISTOCCOC)COMP(OC)FUNC(RET)  **               */ 00602000
*                                                                  0487 00603000
*             DCL                                                  0487 00604000
*               CMZK0036 PTR(31) REG(1);                           0487 00605000
*             RFY                                                  0488 00606000
*              (CMZK0036) RSTD;                                    0488 00607000
*                                                                  0488 00608000
*             /*******************************************************/ 00609000
*             /*                                                     */ 00610000
*             /* ** CLEAR AND SET CURRENT PROCEDURE BIT              */ 00611000
*             /*                                                     */ 00612000
*             /*******************************************************/ 00613000
*                                                                  0489 00614000
*             CMZK0036->RPHCRR->CRACRR->OCCCRT='0'B;               0489 00615000
         L     @11,RPHCRR(,CMZK0036)                               0489 00616000
         L     @11,CRACRR(,@11)                                    0489 00617000
         XC    OCCCRT+1(2,@11),OCCCRT+1(@11)                       0489 00618000
         MVI   OCCCRT(@11),B'00000000'                             0489 00619000
*             CMZK0036->RPHCRR->CRACRR->COCCOC='1'B;               0490 00620000
         OI    COCCOC(@11),B'10000000'                             0490 00621000
*           END;                    /*                       @X50BHXL*/ 00622000
*         END;                      /*                       @X50BHXL*/ 00623000
*       ELSE                                                       0493 00624000
*         ;                         /*                       @X50BHXL*/ 00625000
@RF00482 DS    0H                                                  0494 00626000
*       IF ACBERFLG='00'X           /* ACB ERR CODE 0?       @X50BHXL*/ 00627000
*         THEN                                                     0494 00628000
         CLI   ACBERFLG(ACBREG),X'00'                              0494 00629000
         BNE   @RF00494                                            0494 00630000
*         DO;                       /* YES, CONTINUE OPEN    @X50BHXL*/ 00631000
*           IF ACBLOGON=OFF         /* IF MACRF LOGON=YES            */ 00632000
*             THEN                                                 0496 00633000
         TM    ACBLOGON(ACBREG),B'00001000'                        0496 00634000
