*                                      2,10,11,12,13,14, AND 15      */ 00430000
COCUNLCK DS    0H                                                  0434 00431000
*     RFY                                                          0434 00432000
*      (GPR02P,                                                    0434 00433000
*       GPR10P,                                                    0434 00434000
*       GPR11P,                                                    0434 00435000
*       GPR12P,                                                    0434 00436000
*       GPR13P,                                                    0434 00437000
*       GPR14P,                                                    0434 00438000
*       GPR15P) RSTD;               /* SAVE REGS 2 AND 10 THRU 14    */ 00439000
*     GEN REFS(GPR02F,GPR10F,GPR11F,GPR12F,GPR14F,GPR13P);         0435 00440000
         STM   GPR10F,GPR12F,0(GPR13P)                                  00441000
         ST    GPR14F,12(GPR13P)                                        00442000
         ST    GPR02F,16(GPR13P)                                        00443000
*                                   /* INVOKE MODESET TO CHANGE PSW     00444000
*                                      PROTECTION KEY TO 0           */ 00445000
*     GEN SETS(2);                                                 0436 00446000
         MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2                      00447000
*     GPR10P=GPR13P;                /* PUT SAVE AREA ADDR IN R 10  0437 00448000
*                                      INVOKE OS/VS SETLOCK MACRO    */ 00449000
         LR    GPR10P,GPR13P                                       0437 00450000
*     GEN REFS(GPR13P,PSALITA,FLC) SETS(GPR12F);                   0438 00451000
         SETLOCK RELEASE,TYPE=LOCAL,                                   *00452000
               RELATED=(LOCAL,ISTOCCOC(COCLOCK))                        00453000
*     GPR15P=GPR13P;                /* PUT RETURN CODE IN R 15 INVOKE   00454000
*                                      MODESET MACRO TO              */ 00455000
         LR    GPR15P,GPR13P                                       0439 00456000
*     DO;                           /* RESTORE PSW PROTECTION KEY    */ 00457000
*       GEN SETS(2);                                               0441 00458000
         MODESET KEYADDR=(2),WORKREG=2                                  00459000
*     END;                          /* RESTORE REGISTERS             */ 00460000
*     GEN REFS(GPR10F,GPR13P) SETS(GPR10F,GPR11F,GPR12F,GPR13F,GPR14F,  00461000
*         GPR02F);                                                 0443 00462000
         LR    GPR13F,GPR10F                                            00463000
         LM    GPR10F,GPR12F,0(GPR13P)                                  00464000
         L     GPR14F,12(GPR13P)                                        00465000
         L     GPR02F,16(GPR13P)                                        00466000
*     IF GPR15F=4 THEN                                             0444 00467000
         CH    GPR15F,@CH00094                                     0444 00468000
         BNE   @RF00444                                            0444 00469000
*       GPR15F=0;                                                  0445 00470000
         SLR   GPR15F,GPR15F                                       0445 00471000
*     RFY                                                          0446 00472000
*      (GPR02P,                                                    0446 00473000
