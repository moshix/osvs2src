*/********************************************************************/ 00778000
*/*                                                                  */ 00779000
*/*  ROUTINE NAME : COC300                                           */ 00780000
*/*                                                                  */ 00781000
*/*  FUNCTION : THIS SUBROUTINE PERFORMS ASYNCHRONOUS OPEN EXITING.  */ 00782000
*/*             IF AN ERROR HAS OCCURRED AND THERE IS A POINTER TO   */ 00783000
*/*             THE RDTE IN THE DEB, ALERT IS CALLED TO DEACTIVATE   */ 00784000
*/*             THE APPLICATION RDT ENTRY.  IF NO ERROR HAS OCCURRED,*/ 00785000
*/*             FIELDS IN THE APPL RDT ARE INITIALIZED.              */ 00786000
*/*                                                                  */ 00787000
*/*  ROUTINES CALLED : NONE                                          */ 00788000
*/*                                                                  */ 00789000
*/*  MACROS USED : ALERT - TO DEACTIVATE THE APPLICATION RDT ENTRY   */ 00790000
*/*                                                                  */ 00791000
*/********************************************************************/ 00792000
*                                                                  0532 00793000
*COC300:                                                           0532 00794000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0532 00795000
COC300   DS    0H                                                  0533 00796000
*   RFY                                                            0533 00797000
*     REG14 RSTD;                                                  0533 00798000
*   OCXRG14A=REG14;                 /* SAVE REG 14                   */ 00799000
         ST    REG14,OCXRG14A(,RWORKXT)                            0534 00800000
*   RFY                                                            0535 00801000
*     REG14 UNRSTD;                                                0535 00802000
*   DEBREG=ACBDEB;                  /* SET UP DEB BASE REG           */ 00803000
         SLR   DEBREG,DEBREG                                       0536 00804000
         ICM   DEBREG,7,ACBDEB(ACBREG)                             0536 00805000
*   IF ACBERFLG^='00'X              /* IF ACB CPY ER CD NOT 0        */ 00806000
*     THEN                                                         0537 00807000
         CLI   ACBERFLG(ACBREG),X'00'                              0537 00808000
         BE    @RF00537                                            0537 00809000
*     DO;                                                          0538 00810000
*       IF ACDRDTE^=0               /* IF DEB RDTE PTR NOT 0         */ 00811000
*         THEN                                                     0539 00812000
         ICM   @11,15,ACDRDTE(DEBREG)                              0539 00813000
         BZ    @RF00539                                            0539 00814000
*         DO;                                                      0540 00815000
*           RFY                                                    0541 00816000
*             ATCVTR RSTD;                                         0541 00817000
*           ATCVTR=OCXACVAD;        /* SET UP ATCVT BASE REG         */ 00818000
         L     ATCVTR,OCXACVAD(,RWORKXT)                           0542 00819000
*           REG1=ACBAPID;           /* GET APPLID DATA PTR           */ 00820000
*                                                                  0543 00821000
         L     REG1,ACBAPID(,ACBREG)                               0543 00822000
*           /*********************************************************/ 00823000
*           /*                                                       */ 00824000
*           /* ALERT (OFF,(REG1))                                    */ 00825000
*           /*                                                       */ 00826000
*           /*********************************************************/ 00827000
*                                                                  0544 00828000
*           GEN(LA 1,0(,REG1));                                    0544 00829000
         LA 1,0(,REG1)                                                  00830000
*           GEN(LCR 1,1);                                          0545 00831000
         LCR 1,1                                                        00832000
*           CALL ATCALERT;                                         0546 00833000
         L     @15,ATCALERT(,ATCVTR)                               0546 00834000
         BALR  @14,@15                                             0546 00835000
*           RFY                                                    0547 00836000
*             ATCVTR UNRSTD;                                       0547 00837000
*         END;                                                     0548 00838000
*       ELSE                                                       0549 00839000
*         ;                                                        0549 00840000
