*                                                                  0671 01230000
*   GEN(PRINT OFF);                                                0671 01231000
         PRINT OFF                                                      01232000
*/* LOGICAL CHANNEL COMMAND WORD                                      * 01233000
*/* ADD A DECLARE                                      003 10/18/74   * 01234000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0672 01235000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 01236000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 01237000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 01238000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 01239000
*              4  * BIT(1),            /* RESERVED                    * 01240000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0672 01241000
*                                         ERASE OR READ CONT          * 01242000
