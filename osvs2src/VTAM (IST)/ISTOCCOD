*/*       LEAVE ON(OCWDRDTE=RLEPTR->CSLRDTE)ANDDO */               0758 01268000
*         IF (OCWDRDTE=RLEPTR->CSLRDTE) THEN                       0758 01269000
         CLC   OCWDRDTE(4,OCWPTR),CSLRDTE(RLEPTR)                  0758 01270000
         BNE   @RF00758                                            0758 01271000
*           DO;/* FOR A  @D32CKMP*/                                0759 01272000
