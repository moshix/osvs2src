*                     /*SET HI ORDER BIT IN REG ON*/               0809 01421000
*               CPARMRG0 = CPARMRG0 | CHIBIT; /*HI BIT ON */       0809 01422000
         O     CPARMRG0,CHIBIT                                     0809 01423000
*               RFY CPARMRG0 UNRSTD;                               0810 01424000
*               RPHPTR = RPHPTR & CHIBTOFF;    /*REQUIRED INTERFACE */  01425000
