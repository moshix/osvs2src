*         ELSE                                                     0715 01121000
*         DO;                                                      0715 01122000
*          CALL DOSRTFND;           /* FIND THE RDTE PTR FOR SYMBOL */  01123000
         BAL   @14,DOSRTFND                                        0716 01124000
*                                   /* NAME */                     0717 01125000
*          IF OCWSRT = CON          /* WAS RDTE PTR FOUND */       0717 01126000
*           THEN                                                   0717 01127000
         TM    OCWSRT(OCWPTR),B'01000000'                          0717 01128000
