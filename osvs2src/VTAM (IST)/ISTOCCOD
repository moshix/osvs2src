         TM    ATCQKHLT(@08),B'00110000'                           0386 00180000
         BZ    @RF00386                                            0386 00181000
*  DO;                                                             0387 00182000
*   OCWRC = '08'X;               /*LOGICAL ERROR */                0388 00183000
         MVI   OCWRC(OCWPTR),X'08'                                 0388 00184000
*   OCWEC = '5B'X;               /* QUICK HALT IN PROGRESS*/       0389 00185000
         MVI   OCWEC(OCWPTR),X'5B'                                 0389 00186000
*  END;                                                            0390 00187000
* ELSE                                                             0391 00188000
*  DO;                                                             0391 00189000
