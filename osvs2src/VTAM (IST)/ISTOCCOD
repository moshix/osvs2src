         BNE   @RF01173                                            1173 02556000
*     DO;                              /*                    @D32CKMP*/ 02557000
*       IF ((RPLRLEN>0)                  | /* IF LOGON DATA  @DM13732*/ 02558000
*           (CSLRDTE->RPRENTRY=RPRENTLU) | /* OR LOGICAL             */ 02559000
*          ((CSLRDTE->RCPRBASC=CON)      & /* UNIT OR                */ 02560000
*           (CSLRDTE->RCPRRECD=CON)))      /* 3270                   */ 02561000
