*         DO;                       /* IF DEST OBTAINED      @D32CKXY*/ 00150000
*           CSLRDTEB=CON;           /* TURN ON BIT INDICATING BIND 0667 00151000
*                                      FAILURE NCSPL SHOULD BE SENT     00152000
*                                      IF BIND IS NOT SENT   @D32CKXY*/ 00153000
         OI    CSLRDTEB(@10),B'01000000'                           0667 00154000
*         END;                      /*                       @D32CKXY*/ 00155000
*       ELSE                                                       0669 00156000
*         ;                         /*                       @D32CKXY*/ 00157000
@RF00665 DS    0H                                                  0670 00158000
*       SORTPTR->SORTCID=SORTPTR->SORTCID&&SORTPTR->SORTCID;/* CLEAR    00159000
*                                      SORT SLOT             @D32CKXY*/ 00160000
         L     @10,SORTCID(,SORTPTR)                               0670 00161000
         LR    @09,@10                                             0670 00162000
         XR    @09,@10                                             0670 00163000
         ST    @09,SORTCID(,SORTPTR)                               0670 00164000
*       SORTPTR=SORTPTR+4;          /* PT TO NEXT SORT SLOT  @D32CKXY*/ 00165000
         AH    SORTPTR,@CH00045                                    0671 00166000
*       OCWODNAS=OCWODNAS+LENGTH(ISTCSL);/* PT TO NXT LIST ENTRY   0672 00167000
*                                                            @D32CKXY*/ 00168000
