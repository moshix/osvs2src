*       ISTCNSTA;                                                  0645 00069000
*     RFY                                                          0646 00070000
*       ISTOCCRR;                                                  0646 00071000
*                                                                  0646 00072000
*/* END OF ATBUILD                                                   */ 00073000
*/*  ** SET ENTERED BIT FOR THIS PROCEDURE **                        */ 00074000
*                                                                  0647 00075000
*     CMZK0011->RPHCRR->CRACRR->NOCCOE='1'B;                       0647 00076000
*                                                                  0647 00077000
         L     @10,RPHCRR(,CMZK0011)                               0647 00078000
         L     @10,CRACRR(,@10)                                    0647 00079000
         OI    NOCCOE(@10),B'00010000'                             0647 00080000
*/*  ** CLEAR AND SET CURRENT PROCEDURE BIT **                       */ 00081000
*                                                                  0648 00082000
*     CMZK0011->RPHCRR->CRACRR->OCCCRT='0'B;                       0648 00083000
         XC    OCCCRT+1(2,@10),OCCCRT+1(@10)                       0648 00084000
         MVI   OCCCRT(@10),B'00000000'                             0648 00085000
*     CMZK0011->RPHCRR->CRACRR->COCCOE='1'B;                       0649 00086000
         OI    COCCOE(@10),B'00010000'                             0649 00087000
*   END;                                                           0650 00088000
*   IF CSPRSC2F=COFF                /* IF RESOURCE 2 NAME NOT IN     */ 00089000
*     THEN                          /* CON SVCS PARM LIST    @D32CKMP*/ 00090000
         LA    @10,224                                             0651 00091000
         ALR   @10,OCWPTR                                          0651 00092000
         TM    CSPRSC2F(@10),B'00000010'                           0651 00093000
         BNZ   @RF00651                                            0651 00094000
*     CSPRSC2B=OCWARDTE->RPRNAME;   /* COPY RDT NAME TO CSP  @D32CKMP*/ 00095000
         L     @09,OCWARDTE(,OCWPTR)                               0652 00096000
         MVC   CSPRSC2B(8,@10),RPRNAME(@09)                        0652 00097000
*   ELSE                                                           0653 00098000
*     ;                             /* NO COPY NECESSARY     @D32CKMP*/ 00099000
@RF00651 DS    0H                                                  0654 00100000
*/********************************************************************/ 00101000
*/*  ROUTINE NAME: DOBNSET                                           */ 00102000
*/*  FUNCTION:                                                       */ 00103000
*/*       THIS ROUTINE IS INCLUDED BY THE MAINLINE TO TURN ON THE    */ 00104000
*/*       BIT THAT INDICATES BIND FAILURE IS TO BE SENT. THIS BIT    */ 00105000
*/*       IS TURNED ON FOR EACH ALLOCATED DEST                       */ 00106000
*/*  ROUTINES CALLED:                                                */ 00107000
*/*       NONE                                                       */ 00108000
*/*  MACROS USED:                                                    */ 00109000
*/*       NONE                                                       */ 00110000
*/********************************************************************/ 00111000
*                                                                  0654 00112000
*DOBNSET:                                                          0654 00113000
*   DO;                             /* NEW FOR BIND INTRLCK  @D32CKXY*/ 00114000
DOBNSET  DS    0H                                                  0655 00115000
*     RFY                                                          0655 00116000
*       ISTCSL BASED(OCWODNAS);     /* CSL LOC AT LST ENTRY  @D32CKXY*/ 00117000
*     OCWODCT1=0;                   /* INIT LOOP CTR         @D32CKXY*/ 00118000
         SLR   @10,@10                                             0656 00119000
         STH   @10,OCWODCT1(,OCWPTR)                               0656 00120000
*     OCWODNAS=OCWODLST;            /* PT TO FIRST LST NTRY  @D32CKXY*/ 00121000
