*      (EMCR2,                                                     2411 05936000
*       EMCR3) UNRSTD;                                             2411 05937000
*   END;                            /* CIDCTL GET DAF LOCK         2412 05938000
*                                      XCLUSIVE@D32CKXY              */ 05939000
*   IF CRETCODE=0 THEN                                             2413 05940000
