*     DO;                           /* IF A LIST REQUEST     @D32CKXY*/ 05899000
*       RFY                                                        2398 05900000
*         SORTPTR RSTD;             /*                       @D32CKXY*/ 05901000
*       SORTPTR=OCWODLST+(OCWDSTNO*LENGTH(ISTCSL))+OCWODCT1*4;/* PT     05902000
*                                      TO NTRY SORT SLOT     @D32CKXY*/ 05903000
