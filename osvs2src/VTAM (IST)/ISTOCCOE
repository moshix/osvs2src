*     ELSE                                                         0679 00191000
*       ;                           /* PTR SETUP RT ALREADY  @D32CKXY*/ 00192000
@RF00677 DS    0H                                                  0680 00193000
*   END DOBNSET;                    /*                       @D32CKXY*/ 00194000
*                                                                  0681 00195000
*/********************************************************************/ 00196000
*/* ROUTINE NAME: DOFNCOB                                            */ 00197000
*/* FUNCTION:                                                        */ 00198000
*/*      THIS ROUTINE PERFORMS THE FUNCTION OBTAIN FOR EACH DEST.    */ 00199000
*/*      THE EPT & DVT STRUCTURE IS BUILT AND THE FMCB AND NCB IS    */ 00200000
*/*      BUILT. IT IS INCLUDED BY MAINLINE                           */ 00201000
*/* ROUTINES CALLED:                                                 */ 00202000
*/*      DOVALINT - VALIDATES AND COPIES NIB                         */ 00203000
*/*      DOENVTST - ENSURES THAT AN ACTIVE CONNECTION STILL EXISTS   */ 00204000
*/*      DOPHYCON - VERIFIES THAT PHYSICAL CONNECTION EXISTS TO      */ 00205000
*/*                 THE LU                                           */ 00206000
*/*      ISTOCCFO - BUILDS EPT/DVT STRUCTURE                         */ 00207000
*/*      ISTOCCFB - BUILDS FMCB & NCB                                */ 00208000
*/*      ISTOCCRL - RELEASES EPT/DVT STRUCTURE IF ISTOCCFB FAILS     */ 00209000
*/*      DOSTCTL  - DRIVES NETWORK SLICITOR OR PORT SOLICITOR        */ 00210000
*/* MACROS USED:                                                     */ 00211000
*/*      CRRMAINT - FOOTPRINT                                        */ 00212000
*/*      TPUNLOCK - UNLOCK THE RDTE                                  */ 00213000
*/********************************************************************/ 00214000
*                                                                  0681 00215000
*DOFNCOB:                                                          0681 00216000
*   DO;                             /* NEW FOR BIND INTRLCK  @D32CKXY*/ 00217000
DOFNCOB  DS    0H                                                  0682 00218000
*     OCWODCT1=0;                   /* INIT LOOP CTR         @D32CKXY*/ 00219000
         SLR   @10,@10                                             0682 00220000
         STH   @10,OCWODCT1(,OCWPTR)                               0682 00221000
*     DO WHILE OCWODCT1<OCWDSTNO&OCWRC=0;/* LOOP UNTIL ALL RDTES ARE    00222000
*                                      PROCESSED OR AN ERROR OCCURS     00223000
*                                                            @D32CKXY*/ 00224000
         B     @DE00683                                            0683 00225000
@DL00683 DS    0H                                                  0684 00226000
*       RFY                                                        0684 00227000
*         NTRYPTR RSTD;             /*                       @D32CKXY*/ 00228000
*       NTRYPTR=OCWODLST+(OCWODCT1*LENGTH(ISTCSL));/* PT TO LIST   0685 00229000
*                                      ENTRY                 @D32CKXY*/ 00230000
         LH    NTRYPTR,OCWODCT1(,OCWPTR)                           0685 00231000
         MH    NTRYPTR,@CH00033                                    0685 00232000
         AL    NTRYPTR,OCWODLST(,OCWPTR)                           0685 00233000
*       CSLRDEF=NTRYPTR->CSLRDEF;   /* COPY ENTRY'S RDTE PTR TO    0686 00234000
*                                      OCWDRDTE              @D32CKXY*/ 00235000
         LA    @10,40                                              0686 00236000
         ALR   @10,OCWPTR                                          0686 00237000
         MVC   CSLRDEF(4,@10),CSLRDEF(NTRYPTR)                     0686 00238000
*       CSLNDEF=NTRYPTR->CSLNDEF;   /* COPY ENTRY'S NIB PTR TO OCWNIB   00239000
*                                                            @D32CKXY*/ 00240000
         MVC   CSLNDEF(4,@10),CSLNDEF(NTRYPTR)                     0687 00241000
*       RFY                                                        0688 00242000
*         NTRYPTR UNRSTD;           /*                       @D32CKXY*/ 00243000
*       IF OCWDSTNO=1 THEN                                         0689 00244000
         CLC   OCWDSTNO(2,OCWPTR),@CH00068                         0689 00245000
         BNE   @RF00689                                            0689 00246000
*         OCWNIB=OCWOENIB;          /* FOR SING REQST, GET NIB ADDR     00247000
*                                      FROM FLD ADDR NIB SAVE AREA 0690 00248000
*                                                            @D32CKXY*/ 00249000
         MVC   OCWNIB(4,OCWPTR),OCWOENIB(OCWPTR)                   0690 00250000
*       ELSE                                                       0691 00251000
*         ;                         /*                       @D32CKXY*/ 00252000
@RF00689 DS    0H                                                  0692 00253000
*       OCWFMCB=0;                  /* ZERO PTR BEF NEXT CALL TO   0692 00254000
*                                      ISTOCCFB              @D32CKXY*/ 00255000
         SLR   @10,@10                                             0692 00256000
         ST    @10,OCWFMCB(,OCWPTR)                                0692 00257000
*       OCWNCB=0;                   /* ZERO PTR BEF FB CALL  @D32CKXY*/ 00258000
         ST    @10,OCWNCB(,OCWPTR)                                 0693 00259000
*       IF CSLRDTEF=CON THEN                                       0694 00260000
         TM    CSLRDTEF+40(OCWPTR),B'10000000'                     0694 00261000
         BNO   @RF00694                                            0694 00262000
*         DO;                       /* IF DEST WAS OBT       @D32CKXY*/ 00263000
*           CPARMRG0=1;             /* INDICATE COPY FUNCTN  @D32CKXY*/ 00264000
         LA    CPARMRG0,1                                          0696 00265000
*           CALL DOVALINT;          /* VALIDATE & COPY NIB   @D32CKXY*/ 00266000
         BAL   @14,DOVALINT                                        0697 00267000
*           IF OCWRC=0 THEN                                        0698 00268000
         CLI   OCWRC(OCWPTR),0                                     0698 00269000
         BNE   @RF00698                                            0698 00270000
*             DO;                   /* IF NIB IS VALID       @D32CKXY*/ 00271000
*               CALL DOENVTST;      /* ENSURE DEST EXISTS    @D32CKXY*/ 00272000
         BAL   @14,DOENVTST                                        0700 00273000
*               IF CSLRDTEF=CON&OCWRC=0 THEN                       0701 00274000
         TM    CSLRDTEF+40(OCWPTR),B'10000000'                     0701 00275000
         BNO   @RF00701                                            0701 00276000
         CLI   OCWRC(OCWPTR),0                                     0701 00277000
         BNE   @RF00701                                            0701 00278000
*                 DO;               /* IF DEST FOUND         @D32CKXY*/ 00279000
*                   DO;             /* THIS BEGINS LOGICAL SEGMENT D    00280000
*                                      O P H Y C O N                 */ 00281000
*/*  *****************************************************************/ 00282000
*/*   ROUTINE NAME : DOPHYCON (SEGMENT)                              */ 00283000
*/*   FUNCTION:                                                      */ 00284000
*/*       INVOKED BY MAINLINE, THE DOPHYCON SEGMENT WILL TEST EACH   */ 00285000
*/*       DESTINATION TO SEE IF IT IS A LOGICAL UNIT (LU).  IT THEN  */ 00286000
*/*       TESTS TO SEE IF A PHYSICAL CONNECTION EXISTS TO THE LU     */ 00287000
*/*       (RPRCONCT SET).  IF NOT, AND IF THE SSCP IS NOT THE CALLER,*/ 00288000
*/*       THE SSCP IS INVOKED TO MAKE THE PHYSICAL CONNECTION.  A    */ 00289000
*/*       NCSPL IS ALLOCATED AND FORMATTED WITH A VERB CODE OF DIAL  */ 00290000
*/*       (START), AND QUEUED TO THE VPAB ASSOCIATED WITH THE HEADER */ 00291000
*/*       RDT OF THE LU.  AFTER POSTING BY THE SSCP, THE OCW RETURN  */ 00292000
*/*       CODES ARE CHECKED FOR SUCCESS.                             */ 00293000
*/*   ROUTINES CALLED:                                               */ 00294000
*/*       DOENVTST - TO RE-OBTAIN LOCKS                              */ 00295000
*/*   MACROS USED:                                                   */ 00296000
*/*       REQSTORE - FOR NCSPL                                       */ 00297000
*/*       GETSTORE - FOR NCSPL WORK AREA                             */ 00298000
*/*       TPWAIT - WAIT FOR DIAL                                     */ 00299000
*/*       RELSTORE - RELEASE NCSPL                                   */ 00300000
*/*       TPLOCK - GET VOCLOCK (SHARED)                              */ 00301000
*/*       TPQUE - NCSPL TO VPAB ASSOC WITH HEADER RDTE               */ 00302000
*/*       TPUNLOCK - REL. VOCLOCK AND RDT LOCK                       */ 00303000
*/*  *****************************************************************/ 00304000
*                                                                  0704 00305000
*                     RFY                                          0704 00306000
*                       CPARMR08 RSTD;                             0704 00307000
*                     RFY                                          0705 00308000
*                       ISTNCSPL BASED(CPARMR08);                  0705 00309000
*                     IF RPRENTRY=RPRENTLU&/* IF DEST. IS AN LU AND  */ 00310000
*                         (RPRCONCT=COFF|/* PHYSICAL CONN. DOES NOT     00311000
*                                      EXIST,                        */ 00312000
*                         RPRNSNEA->RCCRESET=CON)/* RESET IN PROGRESS   00313000
*                                                            @ZA19893*/ 00314000
*                       THEN                                       0706 00315000
         L     @10,OCWDRDTE(,OCWPTR)                               0706 00316000
         CLI   RPRENTRY(@10),X'81'                                 0706 00317000
         BNE   @RF00706                                            0706 00318000
         TM    RPRCONCT(@10),B'01000000'                           0706 00319000
         BZ    @RT00706                                            0706 00320000
         L     @10,RPRNSNEA(,@10)                                  0706 00321000
         TM    RCCRESET(@10),B'00000100'                           0706 00322000
         BNO   @RF00706                                            0706 00323000
@RT00706 DS    0H                                                  0707 00324000
*                       DO;                                        0707 00325000
*                         IF RAPALLF^=CON|/* CHECK TO SEE IF SSCP IS    00326000
*                                      CALLER.                       */ 00327000
*                             RPLVTUSE^=CON/* IF NOT, THEN HAVE TO 0708 00328000
*                                      REQUEST                       */ 00329000
*                           THEN    /* THAT SSCP DO THE DIAL         */ 00330000
         L     @10,OCWARDTE(,OCWPTR)                               0708 00331000
         TM    RAPALLF(@10),B'10000000'                            0708 00332000
         BNO   @RT00708                                            0708 00333000
         L     @10,OCWRPL(,OCWPTR)                                 0708 00334000
         TM    RPLVTUSE+16(@10),B'01000000'                        0708 00335000
         BO    @RF00708                                            0708 00336000
@RT00708 DS    0H                                                  0709 00337000
*                           DO;                                    0709 00338000
*                                                                  0709 00339000
*                             /***************************************/ 00340000
*                             /*                                     */ 00341000
*                             /* REQUEST A CLEARED STORAGE AREA FOR  */ 00342000
*                             /* NCSPL TO SEND DIAL REQUEST TO SSCP. */ 00343000
*                             /*                                     */ 00344000
*                             /***************************************/ 00345000
*                                                                  0710 00346000
*/*  REQSTORE CBID(NCSPL)CLEAR(YES)RPH(YES)                          */ 00347000
*                                                                  0710 00348000
*                             DO;                                  0710 00349000
*                                                                  0710 00350000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)                               */ 00351000
*/* END OF ATBUILD                                                   */ 00352000
*                                                                  0711 00353000
*                               RFY                                0711 00354000
*                                (GPR02P,                          0711 00355000
*                                 GPR03P,                          0711 00356000
*                                 GPR04P,                          0711 00357000
*                                 GPR15P) RSTD;                    0711 00358000
*                               RFY                                0712 00359000
*                                (GPR01P,                          0712 00360000
*                                 GPR13P) RSTD;                    0712 00361000
*                               GENERATE REFS(GPR01P,GPR02P,GPR13P)     00362000
*                                   SETS(RPHWORK);                 0713 00363000
           STM GPR02P,GPR13P,RPHWORK(GPR01P)                            00364000
*                               RFY                                0714 00365000
*                                (GPR01P,                          0714 00366000
*                                 GPR13P) UNRSTD;                  0714 00367000
*                               DO; /* LISATCVT                    0715 00368000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00369000
*                                 GPR15P=ATCLCPTR;/* SET UP ATCVT  0716 00370000
*                                      POINTER                       */ 00371000
         L     GPR15P,ATCLCPTR                                     0716 00372000
*                               END;/* LISATCVT                      */ 00373000
*                               GPR02P=GPR15P->ATCBPDA|'80000000'X;     00374000
         L     GPR02P,ATCBPDA(,GPR15P)                             0718 00375000
         O     GPR02P,@CF01465                                     0718 00376000
*                               DCL                                0719 00377000
*                                 1 FMZA0013 FIXED(31) STATIC LOCAL,    00378000
*                                  2 * FIXED(8) INIT(184),         0719 00379000
*                                  2 * PTR(24) INIT(0);            0719 00380000
*                               GPR03F=FMZA0013;                   0720 00381000
         L     GPR03F,FMZA0013                                     0720 00382000
*                               GPR04F=34;                         0721 00383000
         LA    GPR04F,34                                           0721 00384000
*                               GPR15P=GPR15P->ATCSMRQ;            0722 00385000
         L     GPR15P,ATCSMRQ(,GPR15P)                             0722 00386000
*                               DCL                                0723 00387000
*                                 FMZL0013 BASED(GPR15P) LABEL     0723 00388000
*                                     VALUERANGE(*);               0723 00389000
*                               CALL FMZL0013;                     0724 00390000
         BALR  @14,@15                                             0724 00391000
*                               RFY                                0725 00392000
*                                (GPR13P) RSTD;                    0725 00393000
*                               GENERATE REFS(GPR01P,RPHWORK) SETS(     00394000
*                                   GPR02P,GPR13P);                0726 00395000
