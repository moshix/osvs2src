SVTM     TITLE 'ISTOCCQU - QUIESCE ROUTINE                             *00001000
                        '                                               00002000
ISTOCCQU CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTOCCQU  75.136'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*                                                                  0314 00013000
*/********************************************************************/ 00014000
*/*  ROUTINE NAME: MAIN LINE                                         */ 00015000
*/*  FUNCTION:                                                       */ 00016000
*/*      THIS IS THE MAIN LINE FOR ISTOCCQU. IT'S FUNCTION IS TO     */ 00017000
*/*      ROUTE CONTROL TO THE PROPER FUNCTION OF SETLOGON. IF THE    */ 00018000
*/*      RPL INDICATES THAT THIS IS A FIRST TIME REQUEST, THEN THE   */ 00019000
*/*       ACTIVATE ROUTINE IS CALLED TO PROCESS LOGONS. FOR A        */ 00020000
*/*      QUIESCE REQUEST, THE QUIESCE ROUTINE IS CALLED. IF THE APPL */ 00021000
*/*      WANTS TO TEMPARILY BLOCK LOGONS, THEN RAPATLGN IS SET.      */ 00022000
*/*      FOR A SECOND TIME START, RAPATLGN IS RESET. THIS ROUTINE    */ 00023000
*/*      WILL CALL ROUTINES DOLOCK AND DOUNLOCK TO OBTAIN AND        */ 00024000
*/*      RELEASE LOCKS. EXIT IS TO ISTOCCUR.                         */ 00025000
*/*  ROUTINES CALLED:                                                */ 00026000
*/*      DOLOCK - OBTAIN THE VOC AND RDT LOCKS                       */ 00027000
*/*      ACTIVATE - PROCESS QUEUED LOGONS OR VARY LOGON TERMINALS    */ 00028000
*/*      QUIESCE - QUIESCE THE APPLICATION                           */ 00029000
*/*      DOUNLOCK - RELEASE THE LOCKS OBTAINED IN DOLOCK             */ 00030000
*/*  MACROS USED:                                                    */ 00031000
*/*      CRRMAINT - FOOT PRINT                                       */ 00032000
*/********************************************************************/ 00033000
*                                                                  0314 00034000
*   DO;                                                            0314 00035000
*                                                                  0314 00036000
*/***  CRRMAINT  PROCNM(ISTOCCQU)COMP(OC)FUNC(MAIN)  **              */ 00037000
*                                                                  0315 00038000
*     DCL                                                          0315 00039000
*       CMZK0006 PTR(31) REG(1);                                   0315 00040000
*     RFY                                                          0316 00041000
*      (CMZK0006) RSTD;                                            0316 00042000
*                                                                  0316 00043000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)                        */ 00044000
*                                                                  0317 00045000
*     RFY                                                          0317 00046000
*       ISTLCCW;                                                   0317 00047000
*     RFY                                                          0318 00048000
*       ISTLCPB;                                                   0318 00049000
*     RFY                                                          0319 00050000
*       ISTZSAVE;                                                  0319 00051000
*     RFY                                                          0320 00052000
*       ISTPFCRR;                                                  0320 00053000
*     RFY                                                          0321 00054000
*       ISTCFCRR;                                                  0321 00055000
*     RFY                                                          0322 00056000
*       ISTZCRR;                                                   0322 00057000
*     RFY                                                          0323 00058000
*       ISTASCRR;                                                  0323 00059000
*     RFY                                                          0324 00060000
*       ISTAPCRR;                                                  0324 00061000
*     RFY                                                          0325 00062000
*       ISTCRA;                                                    0325 00063000
*     RFY                                                          0326 00064000
*       ISTCNSTA;                                                  0326 00065000
*     RFY                                                          0327 00066000
*       ISTOCCRR;                                                  0327 00067000
*                                                                  0327 00068000
*/* END OF ATBUILD                                                   */ 00069000
*/*  ** SAVE CRR AND FRR POINTERS **                                 */ 00070000
*                                                                  0328 00071000
*     ADDR(CMZK0006->RPHCRR->CRAPROCR)->PFCCRRO=CMZK0006->RPHCRR-> 0328 00072000
*         CRACRR;                                                  0328 00073000
         L     @11,RPHCRR(,CMZK0006)                               0328 00074000
         LA    @05,312                                             0328 00075000
         ALR   @05,@11                                             0328 00076000
         L     @00,CRACRR(,@11)                                    0328 00077000
         ST    @00,PFCCRRO(,@05)                                   0328 00078000
*     ADDR(CMZK0006->RPHCRR->CRAPROCR)->PFCFRRO=CMZK0006->RPHCRR-> 0329 00079000
*         CRAFRR;                                                  0329 00080000
*                                                                  0329 00081000
         L     @00,CRAFRR(,@11)                                    0329 00082000
         ST    @00,PFCFRRO(,@05)                                   0329 00083000
*/*  ** SET UP POINTERS TO OUR CRR, FRR **                           */ 00084000
*                                                                  0330 00085000
*     CMZK0006->RPHCRR->CRACRR=ADDR(CMZK0006->RPHCRR->CRAPROCR);   0330 00086000
         LA    @05,CRAPROCR(,@11)                                  0330 00087000
         ST    @05,CRACRR(,@11)                                    0330 00088000
*     CMZK0006->RPHCRR->CRAFRR=0;                                  0331 00089000
*                                                                  0331 00090000
         SLR   @00,@00                                             0331 00091000
         ST    @00,CRAFRR(,@11)                                    0331 00092000
*/*  ** SET OUR COMPONENT PROCESS ID IN CRR **                       */ 00093000
*                                                                  0332 00094000
*     CMZK0006->RPHCRR->CRACRR->PFCFTA=TIDOCCQU;                   0332 00095000
*                                                                  0332 00096000
         MVI   PFCFTA(@05),X'20'                                   0332 00097000
*/*  ** CLEAR COMPONENT ENTERED INDICATORS **                        */ 00098000
*                                                                  0333 00099000
*     CMZK0006->RPHCRR->CRACRR->OCCNTRD='0'B;                      0333 00100000
*                                                                  0333 00101000
         XC    OCCNTRD+1(2,@05),OCCNTRD+1(@05)                     0333 00102000
         MVI   OCCNTRD(@05),B'00000000'                            0333 00103000
*/*  ** SET ENTERED BIT FOR THIS PROCEDURE **                        */ 00104000
*                                                                  0334 00105000
*     CMZK0006->RPHCRR->CRACRR->NOCCQU='1'B;                       0334 00106000
*                                                                  0334 00107000
         OI    NOCCQU(@05),B'10000000'                             0334 00108000
*/*  ** CLEAR AND SET CURRENT PROCEDURE BIT **                       */ 00109000
*                                                                  0335 00110000
*     CMZK0006->RPHCRR->CRACRR->OCCCRT='0'B;                       0335 00111000
         XC    OCCCRT+1(2,@05),OCCCRT+1(@05)                       0335 00112000
         MVI   OCCCRT(@05),B'00000000'                             0335 00113000
*     CMZK0006->RPHCRR->CRACRR->COCCQU='1'B;                       0336 00114000
         OI    COCCQU(@05),B'10000000'                             0336 00115000
*   END;                                                           0337 00116000
*   OCWPTR=RPHWEA;                  /* SET UP POINTER TO OCW         */ 00117000
         L     OCWPTR,RPHWEA(,RPHPTR)                              0338 00118000
*   SVEAREA=ADDR(OCWSAVE1);         /* SET UP PTR TO REG SAVE AREA   */ 00119000
         LA    SVEAREA,OCWSAVE1(,OCWPTR)                           0339 00120000
*   C14SVPTR=ADDR(OCWR14SV)+4;      /* SET UP R14 SAVE AREA          */ 00121000
         LA    @11,OCWR14SV(,OCWPTR)                               0340 00122000
         AL    @11,@CF00053                                        0340 00123000
         ST    @11,C14SVPTR+624(,OCWPTR)                           0340 00124000
*   CALL DOLOCK;                    /* LOCK RDTE IN STORAGE          */ 00125000
         BAL   @14,DOLOCK                                          0341 00126000
*   IF OCWRC^=0                     /* IF LOCKS NOT OBTAINED         */ 00127000
*     THEN                                                         0342 00128000
         CLI   OCWRC(OCWPTR),0                                     0342 00129000
         BNE   @RT00342                                            0342 00130000
*     ;                                                            0343 00131000
*   ELSE                                                           0344 00132000
*     IF RAPABLGN=CON|RAPAQUI=CON   /* ILLEGAL QUIESCE               */ 00133000
*       THEN                                                       0344 00134000
         L     @11,OCWARDTE(,OCWPTR)                               0344 00135000
         TM    RAPABLGN(@11),B'11000000'                           0344 00136000
         BZ    @RF00344                                            0344 00137000
*       DO;                                                        0345 00138000
*         OCWRC='08'X;              /* LOGICAL ERROR                 */ 00139000
         MVI   OCWRC(OCWPTR),X'08'                                 0346 00140000
*         OCWEC='66'X;              /* ILLEGAL QUIESCE               */ 00141000
         MVI   OCWEC(OCWPTR),X'66'                                 0347 00142000
*       END;                                                       0348 00143000
*     ELSE                                                         0349 00144000
*       DO;                                                        0349 00145000
         B     @RC00344                                            0349 00146000
@RF00344 DS    0H                                                  0350 00147000
*         IF RPLSTART=CON           /* START LOGONS FOR APPLI        */ 00148000
*           THEN                                                   0350 00149000
         L     @11,OCWRPL(,OCWPTR)                                 0350 00150000
         TM    RPLSTART(@11),B'01000000'                           0350 00151000
         BNO   @RF00350                                            0350 00152000
*           IF RAPASLGI=COFF        /* FIRST TIME FOR ACTIVATE?      */ 00153000
*             THEN                                                 0351 00154000
         L     @11,OCWARDTE(,OCWPTR)                               0351 00155000
         TM    RAPASLGI(@11),B'00100000'                           0351 00156000
         BNZ   @RF00351                                            0351 00157000
*             IF ATCQKHLT=CON|ATCSNHLT=CON/* HALT GOING ON           */ 00158000
*               THEN                                               0352 00159000
         L     @11,OCWACVT(,OCWPTR)                                0352 00160000
         TM    ATCQKHLT(@11),B'00110000'                           0352 00161000
         BZ    @RF00352                                            0352 00162000
*               DO;                                                0353 00163000
*                 OCWRC='08'X;      /* LOGICAL ERR                   */ 00164000
         MVI   OCWRC(OCWPTR),X'08'                                 0354 00165000
*                 OCWEC='5B'X;      /* HALT IN PROGRESS              */ 00166000
         MVI   OCWEC(OCWPTR),X'5B'                                 0355 00167000
*               END;                                               0356 00168000
*             ELSE                                                 0357 00169000
*               DO;                                                0357 00170000
         B     @RC00352                                            0357 00171000
@RF00352 DS    0H                                                  0358 00172000
*                 RAPASLGI=CON;     /* NOT FIRST TIME ANY MORE       */ 00173000
*                 RAPATLGN=COFF;    /* TEMP STOP LOGONS FLAG OFF     */ 00174000
         L     @11,OCWARDTE(,OCWPTR)                               0359 00175000
         OI    RAPASLGI(@11),B'00100000'                           0359 00176000
         NI    RAPATLGN(@11),B'11101111'                           0359 00177000
*                 CALL ACTIVATE;                                   0360 00178000
         BAL   @14,ACTIVATE                                        0360 00179000
*               END;                                               0361 00180000
*           ELSE                                                   0362 00181000
*             DO;                                                  0362 00182000
         B     @RC00351                                            0362 00183000
@RF00351 DS    0H                                                  0363 00184000
*               RAPATLGN=COFF;      /* APPL WILL ACCEPT LOGONS NOW   */ 00185000
         L     @11,OCWARDTE(,OCWPTR)                               0363 00186000
         NI    RAPATLGN(@11),B'11101111'                           0363 00187000
*               OCWRC=0;                                           0364 00188000
         MVI   OCWRC(OCWPTR),X'00'                                 0364 00189000
*               OCWEC=0;                                           0365 00190000
         MVI   OCWEC(OCWPTR),X'00'                                 0365 00191000
*             END;                                                 0366 00192000
*         ELSE                                                     0367 00193000
*           IF RPLSTOP=CON          /* STOP LOGONS TO THIS APPL      */ 00194000
*             THEN                                                 0367 00195000
         B     @RC00350                                            0367 00196000
@RF00350 L     @11,OCWRPL(,OCWPTR)                                 0367 00197000
         TM    RPLSTOP(@11),B'00100000'                            0367 00198000
         BNO   @RF00367                                            0367 00199000
*             RAPATLGN=CON;         /* STOP LOGONS TO THIS TERM FOR     00200000
*                                      NOW                           */ 00201000
         L     @11,OCWARDTE(,OCWPTR)                               0368 00202000
         OI    RAPATLGN(@11),B'00010000'                           0368 00203000
*           ELSE                                                   0369 00204000
*             IF RPLQUIES=CON       /* APPL SHUTTING DOWN?           */ 00205000
*               THEN                                               0369 00206000
         B     @RC00367                                            0369 00207000
@RF00367 L     @11,OCWRPL(,OCWPTR)                                 0369 00208000
         TM    RPLQUIES(@11),B'10000000'                           0369 00209000
         BNO   @RF00369                                            0369 00210000
*               CALL QUIESCE;       /* QUIESCE THIS APPL             */ 00211000
         BAL   @14,QUIESCE                                         0370 00212000
*             ELSE                                                 0371 00213000
*               DO;                                                0371 00214000
         B     @RC00369                                            0371 00215000
@RF00369 DS    0H                                                  0372 00216000
*                 OCWRC='24'X;      /* SYSTEM ERROR                  */ 00217000
         MVI   OCWRC(OCWPTR),X'24'                                 0372 00218000
*                 OCWEC='01'X;                                     0373 00219000
         MVI   OCWEC(OCWPTR),X'01'                                 0373 00220000
*               END;                                               0374 00221000
*       END;                                                       0375 00222000
@RC00369 DS    0H                                                  0375 00223000
@RC00367 DS    0H                                                  0375 00224000
@RC00350 DS    0H                                                  0376 00225000
*   CALL DOUNLOCK;                  /* LOCK RDTE IN STORAGE          */ 00226000
@RC00344 DS    0H                                                  0376 00227000
@RT00342 BAL   @14,DOUNLOCK                                        0376 00228000
*   RPHWEA=OCWPTR;                  /* OCW ADDR INTO RPH             */ 00229000
         ST    OCWPTR,RPHWEA(,RPHPTR)                              0377 00230000
*   RPLRTNCD=OCWRC;                 /* ERROR CODE INTO RPL           */ 00231000
         L     @11,OCWRPL(,OCWPTR)                                 0378 00232000
         MVC   RPLRTNCD(1,@11),OCWRC(OCWPTR)                       0378 00233000
*   RPLFDB2=OCWEC;                  /* DITTO                 @DM09103*/ 00234000
         MVC   RPLFDB2(1,@11),OCWEC(OCWPTR)                        0379 00235000
*   RFY                                                            0380 00236000
*     ISTATCVT BASED(OCWACVT);                                     0380 00237000
*   CALL ISTOCCUR;                  /* EXIT TO UNROUTER              */ 00238000
         L     @15,@CV03198                                        0381 00239000
         BALR  @14,@15                                             0381 00240000
*                                                                  0382 00241000
*/********************************************************************/ 00242000
*/*  ROUTINE NAME: QUIESCE                                           */ 00243000
*/*  FUNCTION:                                                       */ 00244000
*/*      THIS ROUTINE IS CALLED BY MAIN LINE TO SET A BIT IN THE     */ 00245000
*/*      APPLICATION'S RDT WHICH WILL BLOCK LOGONS TO THIS APPL.     */ 00246000
*/*      THE NUMBER OF QUEUED LOGONS WILL BE RETURNED IN THE RPL.    */ 00247000
*/*  ROUTINES CALLED:                                                */ 00248000
*/*      NONE                                                        */ 00249000
*/*  MACROS USED:                                                    */ 00250000
*/*      NONE                                                        */ 00251000
*/********************************************************************/ 00252000
*                                                                  0382 00253000
*QUIESCE:                                                          0382 00254000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0382 00255000
         B     @PB00002                                            0382 00256000
QUIESCE  DS    0H                                                  0383 00257000
*   C14SAVE=CR14PTR;                                               0383 00258000
         LA    @11,624                                             0383 00259000
         ALR   @11,OCWPTR                                          0383 00260000
         L     @05,C14SVPTR(,@11)                                  0383 00261000
         ST    CR14PTR,C14SAVE(,@05)                               0383 00262000
*   C14SVPTR=C14SVPTR+4;                                           0384 00263000
         AL    @05,@CF00053                                        0384 00264000
         ST    @05,C14SVPTR(,@11)                                  0384 00265000
*   RFY                                                            0385 00266000
*     CLGNPND RSTD;                                                0385 00267000
*   RAPAQUI=CON;                    /* INDICATE QUIESCE IN PROGRESS  */ 00268000
         L     @05,OCWARDTE(,OCWPTR)                               0386 00269000
         OI    RAPAQUI(@05),B'01000000'                            0386 00270000
*   CLGNPND=CZERO;                  /* CLEAR LOGONS PENDING COUNTER  */ 00271000
         SLR   CLGNPND,CLGNPND                                     0387 00272000
*   RAPATLGN=COFF;                  /* ENSURE TEMP BLK LOGONS BIT OFF*/ 00273000
         NI    RAPATLGN(@05),B'11101111'                           0388 00274000
*   ICEPTR=RAPICELG;                /* GET LOGON ICE POINTER         */ 00275000
         L     ICEPTR,RAPICELG(,@05)                               0389 00276000
*   DO WHILE(ICEPTR^=ADDR(RAPICELG));/* COUNT PENDING LOGONS         */ 00277000
         B     @DE00390                                            0390 00278000
@DL00390 DS    0H                                                  0391 00279000
*     CLGNPND=CLGNPND+1;            /* INCREMENT LOGON PENDING COUNT */ 00280000
         AL    CLGNPND,@CF00061                                    0391 00281000
*     ICEPTR=ICESUBJQ;              /* GET POINTER TO NEXT ICE       */ 00282000
         L     ICEPTR,ICESUBJQ(,ICEPTR)                            0392 00283000
*   END;                            /* END LOGON PENDING COUNT LOOP  */ 00284000
@DE00390 L     @05,OCWARDTE(,OCWPTR)                               0393 00285000
         LA    @05,RAPICELG(,@05)                                  0393 00286000
         CR    ICEPTR,@05                                          0393 00287000
         BNE   @DL00390                                            0393 00288000
*   RPLFDBK=CLGNPND;                /* INDICATE HOW MANY LOGONS PEND */ 00289000
         L     @05,OCWRPL(,OCWPTR)                                 0394 00290000
         STCM  CLGNPND,7,RPLFDBK(@05)                              0394 00291000
*   RFY                                                            0395 00292000
*     CLGNPND UNRSTD;                                              0395 00293000
*   C14SVPTR=C14SVPTR-4;                                           0396 00294000
         LA    @11,624                                             0396 00295000
         ALR   @11,OCWPTR                                          0396 00296000
         L     @05,C14SVPTR(,@11)                                  0396 00297000
         SL    @05,@CF00053                                        0396 00298000
         ST    @05,C14SVPTR(,@11)                                  0396 00299000
*   CR14PTR=C14SAVE;                                               0397 00300000
         L     CR14PTR,C14SAVE(,@05)                               0397 00301000
*   END QUIESCE;                                                   0398 00302000
@EL00002 DS    0H                                                  0398 00303000
@EF00002 DS    0H                                                  0398 00304000
@ER00002 BR    @14                                                 0398 00305000
*ACTIVATE:                                                         0399 00306000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0399 00307000
*                                                                  0399 00308000
ACTIVATE DS    0H                                                  0400 00309000
*/********************************************************************/ 00310000
*/*  ROUTINE NAME: ACTIVATE                                          */ 00311000
*/*  FUNCTION:                                                       */ 00312000
*/*      THIS ROUTINE IS CALLED BY MAIN LINE TO DETERMINE IF THERE   */ 00313000
*/*      ARE ANY LOGONS QUEUED ON THE LOGONTOME CHAIN OR IF THERE IS */ 00314000
*/*      A VARY LOGON CHAIN TO BE PROCESSED.                         */ 00315000
*/*  ROUTINES CALLED:                                                */ 00316000
*/*      LOGEXIT - SCHEDULES USERS EXITS FOR ANY QUEUED ICES         */ 00317000
*/*      DOLOGON - CALLS CONN SERV TO LOGON TERMS ON VARY LOGON      */ 00318000
*/*      CHAIN.                                                      */ 00319000
*/*  MACROS USED:                                                    */ 00320000
*/*      NONE                                                        */ 00321000
*/********************************************************************/ 00322000
*                                                                  0400 00323000
*   C14SAVE=CR14PTR;                                               0400 00324000
         LA    @11,624                                             0400 00325000
         ALR   @11,OCWPTR                                          0400 00326000
         L     @05,C14SVPTR(,@11)                                  0400 00327000
         ST    CR14PTR,C14SAVE(,@05)                               0400 00328000
*   C14SVPTR=C14SVPTR+4;                                           0401 00329000
         AL    @05,@CF00053                                        0401 00330000
         ST    @05,C14SVPTR(,@11)                                  0401 00331000
*   IF RAPICELG^=ADDR(RAPICELG)     /* REAL LOGONS PENDING?          */ 00332000
*     THEN                                                         0402 00333000
         L     @11,OCWARDTE(,OCWPTR)                               0402 00334000
         LA    @05,RAPICELG(,@11)                                  0402 00335000
         C     @05,RAPICELG(,@11)                                  0402 00336000
         BE    @RF00402                                            0402 00337000
*     CALL LOGEXIT;                 /* LOGON EXIT PROCESSING         */ 00338000
         BAL   @14,LOGEXIT                                         0403 00339000
*   ELSE                                                           0404 00340000
*     ;                                                            0404 00341000
@RF00402 DS    0H                                                  0405 00342000
*   IF RPRLGONF^=RPRLGONB           /* IF LOGONS PENDING             */ 00343000
*     THEN                                                         0405 00344000
         L     @11,OCWARDTE(,OCWPTR)                               0405 00345000
         CLC   RPRLGONF(4,@11),RPRLGONB(@11)                       0405 00346000
         BE    @RF00405                                            0405 00347000
*     DO;                                                          0406 00348000
*       CALL DOLOGON;               /* CALL CON SERV FOR LOG         */ 00349000
         BAL   @14,DOLOGON                                         0407 00350000
*     END;                                                         0408 00351000
*   ELSE                                                           0409 00352000
*     ;                                                            0409 00353000
@RF00405 DS    0H                                                  0410 00354000
*   C14SVPTR=C14SVPTR-4;                                           0410 00355000
         LA    @11,624                                             0410 00356000
         ALR   @11,OCWPTR                                          0410 00357000
         L     @05,C14SVPTR(,@11)                                  0410 00358000
         SL    @05,@CF00053                                        0410 00359000
         ST    @05,C14SVPTR(,@11)                                  0410 00360000
*   CR14PTR=C14SAVE;                                               0411 00361000
         L     CR14PTR,C14SAVE(,@05)                               0411 00362000
*   END ACTIVATE;                                                  0412 00363000
@EL00003 DS    0H                                                  0412 00364000
@EF00003 DS    0H                                                  0412 00365000
@ER00003 BR    @14                                                 0412 00366000
*LOGEXIT:                                                          0413 00367000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0413 00368000
*                                                                  0413 00369000
LOGEXIT  DS    0H                                                  0414 00370000
*/********************************************************************/ 00371000
*/*  ROUTINE NAME: LOGEXIT                                           */ 00372000
*/*  FUNCTION:                                                       */ 00373000
*/*      THIS ROUTINE IS CALLED BY ACTIVATE TO DETERMINE             */ 00374000
*/*      IF USER EXIT'S SHOULD BE SCHEDULED FOR ANY ICE'S QUEUED.    */ 00375000
*/*      THE APPLICATION'S LOGONTOME ICE CHAIN IS LOOPED THROUGH AND */ 00376000
*/*      LOGSCHED IS CALLED TO SCHEDULE THE EXIT FOR EACH ICE.       */ 00377000
*/*  ROUTINES CALLED:                                                */ 00378000
*/*      LOGSCHED - SCHE A UECB FOR ONE ICE                          */ 00379000
*/*  MACROS USED:                                                    */ 00380000
*/*        MODESET                                                   */ 00381000
*/********************************************************************/ 00382000
*                                                                  0414 00383000
*   C14SAVE=CR14PTR;                                               0414 00384000
         LA    @11,624                                             0414 00385000
         ALR   @11,OCWPTR                                          0414 00386000
         L     @05,C14SVPTR(,@11)                                  0414 00387000
         ST    CR14PTR,C14SAVE(,@05)                               0414 00388000
*   C14SVPTR=C14SVPTR+4;                                           0415 00389000
         AL    @05,@CF00053                                        0415 00390000
         ST    @05,C14SVPTR(,@11)                                  0415 00391000
*   ICEPTR=RAPICELG;                /* SET PTR TO ISTICE             */ 00392000
         L     @05,OCWARDTE(,OCWPTR)                               0416 00393000
         L     ICEPTR,RAPICELG(,@05)                               0416 00394000
*   DO;                             /* MODESET EXTKEY(ZERO)          */ 00395000
*     SPKA(0);                      /* SET PSW KEY                   */ 00396000
         SLR   @11,@11                                             0418 00397000
         SPKA  0(@11)                                              0418 00398000
*   END;                            /* MODESET EXTKEY(ZERO)          */ 00399000
*   IF ACBUEL^=CZERO                /* IF UEXL PTR NOT ZERO          */ 00400000
*     THEN                                                         0420 00401000
         L     @05,OCWACDEB(,OCWPTR)                               0420 00402000
         L     @05,ACDACB-1(,@05)                                  0420 00403000
         LA    @05,0(,@05)                                         0420 00404000
