*                         IF RTNCODE>ZERO /*        SUCCESSFUL?      */ 00558000
*                            THEN MDVTPTR=I; /*         NO-END SCAN  */ 00559000
         LTR   RTNCODE,RTNCODE                                     0557 00560000
         BNP   @RF00557                                            0557 00561000
         LR    MDVTPTR,I                                           0558 00562000
*                            ELSE;                                 0559 00563000
@RF00557 DS    0H                                                  0560 00564000
*                         END;                                     0560 00565000
*             ELSE;                                                0561 00566000
