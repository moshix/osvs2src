*              RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P) UNRSTD;    0617 00739000
*              END ;                                /*END OF FREESTOR*/ 00740000
*             /* FREE EPT CORE                   DM19033 */        0619 00741000
*             OCWEPTAB=ZERO;             /*     ZERO EPT POINTER      * 00742000
