*                         CALL DVTFREE; /*  FREE ALL RDVT CORE       */ 00824000
         BAL   @14,DVTFREE                                         0637 00825000
*                     /*FREESTOR AREA(OCWEPTAB)FETCHP(NO)SIZE(EPTHLEN)* 00826000
*                                                                  0638 00827000
*                      DO;                                         0638 00828000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0639 00829000
*/* END OF ATBUILD */ RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;      0639 00830000
*                     RFY ISTRPH BASED(GPR01P);                    0640 00831000
