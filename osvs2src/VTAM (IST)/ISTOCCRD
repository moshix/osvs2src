         LA    RTNCODE,4                                           0399 00087000
*             END;                                                 0400 00088000
*         ELSE DO;                  /*    YES-CALL ROUTINES TO BLD  */  00089000
         B     @RC00395                                            0401 00090000
@RF00395 DS    0H                                                  0402 00091000
*             CALL BLDREAL;         /*    EPT AND RDVTS             */  00092000
         BAL   @14,BLDREAL                                         0402 00093000
*             IF RTNCODE=ZERO       /*SET RETURN CODE DEPENDING     */  00094000
*                 THEN;             /*ON SUCCESS OF THE BUILD       */  00095000
         LTR   RTNCODE,RTNCODE                                     0403 00096000
         BZ    @RT00403                                            0403 00097000
