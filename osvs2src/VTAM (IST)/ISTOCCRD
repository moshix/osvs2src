@EL00006 DS    0H                                                  0670 00904000
@EF00006 DS    0H                                                  0670 00905000
@ER00006 BR    @14                                                 0670 00906000
*/********************************************************************/ 00907000
*/*                                                                  */ 00908000
*/*  ROUTINE NAME : LOADPROC - LOAD PROCESS MODULE ADDRESSES         */ 00909000
*/*                                                                  */ 00910000
*/*  FUNCTION : CALLED FOR EACH EPT ENTRY IN ORDER TO FILL THE       */ 00911000
*/*             ASSOCIATED DVT WITH THE ADDRESSES OF THE PROCESS     */ 00912000
*/*             MODULES.                                             */ 00913000
*/*                                                                  */ 00914000
*/*  INPUT : EPTEPTR - POINTS TO THE ENTRY POINT TABLE ENTRY         */ 00915000
*/*          MDVTPRT - POINTS TO HEADER ENTRY CORRESPONDING TO THE   */ 00916000
*/*                    ENTRY POINT TABLE ENTRY.                      */ 00917000
*/*                                                                  */ 00918000
*/*  OUTPUT : R15 = 0 IF DVT ADDRESSES FILLED IN SUCCESSFULLY        */ 00919000
*/*           R15 = 4 IF DVT ADDRESSES FILLED IN UNSUCCESSFULLY      */ 00920000
*/*                                                                  */ 00921000
*/*  ROUTINES : NONE                                                 */ 00922000
*/*                                                                  */ 00923000
*/*  MACROS : CRRMAINT - TO MAINTAIN A MODULE TRACE THRU VTAM        */ 00924000
*/*                                                                  */ 00925000
*/********************************************************************/ 00926000
*LOADPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0671 00927000
LOADPROC DS    0H                                                  0672 00928000
*/* FILLS IN THE REAL DVT WITH THE ADDRESSES OF THE PROCCESS MODULES */ 00929000
*/*  THAT ARE TO BE INCLUDED.                                        */ 00930000
*/* CALLED BY DVTFILL.                                               */ 00931000
*     C14SAVE=CR14PTR;                                             0672 00932000
