         MVI   OCCCRT(@02),B'00000000'                             0375 00033000
*CMZK0007->RPHCRR->CRACRR->COCCRD = '1'B;                          0376 00034000
         OI    COCCRD(@02),B'10000000'                             0376 00035000
*      END;                                                        0377 00036000
*     CSAVEPTR=CSAVEPTR+4*DIM(OCWSAVE1); /*SAVEAREA ADDR*/         0378 00037000
         AL    CSAVEPTR,@CF03024                                   0378 00038000
*     RTNCODE=ZERO;                                                0379 00039000
         SLR   RTNCODE,RTNCODE                                     0379 00040000
*     MDVTPTR=OCWMDVT;             /*SETUP MASTER DVT POINTER  */  0380 00041000
         L     MDVTPTR,OCWMDVT(,COCWPTR)                           0380 00042000
*     RFY R0 UNRSTD;                                               0381 00043000
*     RBLDCNT=ZERO;                 /*INITIALIZE BUILD COUNT        */  00044000
         SLR   RBLDCNT,RBLDCNT                                     0382 00045000
*     RFY I RSTD;                                                  0383 00046000
*/* THE ENTIRE MASTER DVT IS SCANNED TO CALCULATE THE NUMBER OF     */  00047000
*/* ENTRY POINT TABLE ENTRIES TO BE BUILT                           */  00048000
*     I=MDVTPTR+LENGTH(ISTSDVT)*SDVTOTAL;   /*SCAN DELIMITER        */  00049000
         LH    I,SDVTOTAL(,MDVTPTR)                                0384 00050000
         MH    I,@CH02093                                          0384 00051000
         ALR   I,MDVTPTR                                           0384 00052000
*     DO WHILE (MDVTPTR<I);         /*SCAN:                         */  00053000
         B     @DE00385                                            0385 00054000
@DL00385 DS    0H                                                  0386 00055000
*         IF SDVID='RDVT'           /*    DVT ENTRY?                */  00056000
*             THEN IF SDVCNT>ZERO   /*    MDLES TO INCLUDE?         */  00057000
         CLC   SDVID(4,MDVTPTR),@CC03250                           0386 00058000
         BNE   @RF00386                                            0386 00059000
*                 THEN RBLDCNT=RBLDCNT+1; /*YES-INCR. CNT           */  00060000
         LH    @02,SDVCNT(,MDVTPTR)                                0387 00061000
         LTR   @02,@02                                             0387 00062000
         BNP   @RF00387                                            0387 00063000
         AL    RBLDCNT,@CF00049                                    0388 00064000
*                 ELSE;                                            0389 00065000
@RF00387 DS    0H                                                  0390 00066000
*             ELSE;                                                0390 00067000
@RF00386 DS    0H                                                  0391 00068000
*         MDVTPTR=MDVTPTR+LENGTH(ISTSDVT); /*STEP TO NEXT ENTRY     */  00069000
         AL    MDVTPTR,@CF02093                                    0391 00070000
*     END;                          /*SCAN END                      */  00071000
*                                                                  0392 00072000
@DE00385 CR    MDVTPTR,I                                           0392 00073000
         BL    @DL00385                                            0392 00074000
*     MDVTPTR=OCWMDVT;             /*RESTORE MASTER DVT PTR        */   00075000
         L     MDVTPTR,OCWMDVT(,COCWPTR)                           0393 00076000
*     RFY I UNRSTD;                                                0394 00077000
*     IF RBLDCNT=ZERO               /*COUNT?                        */  00078000
*         THEN DO;                                                 0395 00079000
         LTR   RBLDCNT,RBLDCNT                                     0395 00080000
         BNZ   @RF00395                                            0395 00081000
*             OCWRC='08'X;          /*    NO-LOGIC ERROR            */  00082000
         MVI   OCWRC(COCWPTR),X'08'                                0397 00083000
