*                2 * FIXED(8) INIT(96),               /* STORAGE KEY */ 01077000
*                2 * FIXED(8) INIT(241),           /* SUBPOOL NUMBER */ 01078000
*                2 * FIXED(8) INIT('01'X);           /* REQUEST TYPE */ 01079000
*          GPR03F = CMZA0117;                                      0710 01080000
         L     GPR03F,CMZA0117                                     0710 01081000
*          RFY (GPR04P) RSTD;                                      0711 01082000
*          GPR04P = RDVTPTR;              /*PUT FREEMAIN ADDR IN REG4*/ 01083000
