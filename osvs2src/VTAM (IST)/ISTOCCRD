SVTM     TITLE 'ISTOCCRD - RDVTBLD                                     *00001000
                        '                                               00002000
ISTOCCRD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(8)                                              0001 00007000
         DC    C'ISTOCCRD'                                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
RDVTBLD  DS    0H                                                  0368 00014000
*                                                                  0368 00015000
*      DO;                                                         0369 00016000
*/***  CRRMAINT  PROCNM(ISTOCCRD)COMP(OC)FUNC(SUB)  ***/           0370 00017000
*      DCL CMZK0007 PTR(31) REG(1);                                0370 00018000
*      RFY (CMZK0007) RSTD;                                        0371 00019000
*                                                                  0371 00020000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0372 00021000
*      RFY ISTCNSTA;                                               0372 00022000
*      RFY ISTOCCRR;                                               0373 00023000
*/* END OF ATBUILD */                                              0374 00024000
*      /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                0374 00025000
*CMZK0007->RPHCRR->CRACRR->NOCCRD = '1'B;                          0374 00026000
         L     @02,RPHCRR(,CMZK0007)                               0374 00027000
         L     @02,CRACRR(,@02)                                    0374 00028000
         OI    NOCCRD(@02),B'10000000'                             0374 00029000
*      /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/               0375 00030000
*CMZK0007->RPHCRR->CRACRR->OCCCRT = '0'B;                          0375 00031000
