*                    2 * FIXED(8) INIT(64),          /* OPTION FLAGS */ 00722000
*                    2 * FIXED(8) INIT(96),           /* STORAGE KEY */ 00723000
*                    2 * FIXED(8) INIT(241),       /* SUBPOOL NUMBER */ 00724000
*                    2 * FIXED(8) INIT('01'X);       /* REQUEST TYPE */ 00725000
*              GPR03F = CMZA0079;                                  0610 00726000
         L     GPR03F,CMZA0079                                     0610 00727000
*              RFY (GPR04P) RSTD;                                  0611 00728000
*              GPR04P = OCWEPTAB;         /*PUT FREEMAIN ADDR IN REG4*/ 00729000
*                                                                  0612 00730000
         LR    GPR04P,@11                                          0612 00731000
