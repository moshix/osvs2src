*                     RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P) UNRSTD;  00859000
*                     END ;                         /*END OF FREESTOR*/ 00860000
*                           /* FETCHP ADDED    DM19033 */          0653 00861000
*                         OCWEPTAB=ZERO;  /*EPT AND ZERO POINTER     */ 00862000
         SLR   @00,@00                                             0653 00863000
         ST    @00,OCWEPTAB(,COCWPTR)                              0653 00864000
*                         RTNCODE=FOUR;  /* SET UNSUCCESSFUL CODE.   */ 00865000
         LA    RTNCODE,4                                           0654 00866000
