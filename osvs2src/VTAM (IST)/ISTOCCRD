         ST    CR14PTR,C14SAVE(,@10)                               0547 00533000
*     C14SVPTR=C14SVPTR+4;                                         0548 00534000
         AL    @10,@CF00038                                        0548 00535000
         ST    @10,C14SVPTR(,@11)                                  0548 00536000
*     RFY I RSTD;                                                  0549 00537000
*/* THE ENTIRE MASTER DVT IS SCANNED.  CALLS ARE MADE TO FILL IN THE */ 00538000
*/*  EPT AND TO DO THE RDVT FILL AND BUILD.                          */ 00539000
*     I=MDVTPTR+LENGTH(ISTSDVT)*SDVTOTAL; /*SET SCAN DELIMITER       */ 00540000
         LH    I,SDVTOTAL(,MDVTPTR)                                0550 00541000
         MH    I,@CH02093                                          0550 00542000
