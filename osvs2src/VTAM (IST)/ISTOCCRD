*                     GPR00F = EPTHLEN;                /*SIZE IN REG0*/ 00838000
         L     @11,OCWEPTAB(,COCWPTR)                              0642 00839000
         LH    GPR00F,EPTHLEN(,@11)                                0642 00840000
*                     DCL 1 CMZA0098 FIXED(31) STATIC LOCAL,       0643 00841000
*                           2 * FIXED(8) INIT(64),   /* OPTION FLAGS */ 00842000
*                           2 * FIXED(8) INIT(96),    /* STORAGE KEY */ 00843000
*                           2 * FIXED(8) INIT(241),/* SUBPOOL NUMBER */ 00844000
*                           2 * FIXED(8) INIT('01'X);/* REQUEST TYPE */ 00845000
*                     GPR03F = CMZA0098;                           0644 00846000
