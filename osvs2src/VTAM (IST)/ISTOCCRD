*     CSAVEPTR=CSAVEPTR-4*DIM(OCWSAVE1); /*RESTORE SAVEAREA ADDR */     00103000
@RC00395 SL    CSAVEPTR,@CF03024                                   0407 00104000
*     GEN REFS(R0,CSAVEPTR,CR14PTR,R12); /*   RESTORE REGISTERS     */  00105000
          L   CR14PTR,12(CSAVEPTR)                                      00106000
          LM   R0,R12,20(CSAVEPTR)                                      00107000
          BR   CR14PTR                                                  00108000
*/********************************************************************/ 00109000
*/*                                                                  */ 00110000
*/*  ROUTINE NAME : BLDREAL - REAL EPT BUILDER                       */ 00111000
*/*                                                                  */ 00112000
*/*  FUNCTION : GET CORE FOR ENTRY POINT TBL. INITIALIZE THE EPT     */ 00113000
*/*             HEADER. CALL ROUTINES TO BUILD AND FILL THE EPT      */ 00114000
*/*             AND THE REAL DVTS AND CHAIN THEM TO THE ATCVT        */ 00115000
*/*                                                                  */ 00116000
*/*  INPUT : RBLDCNT = NUMBER OF VALID ENTRIES (DVT POINTERS)        */ 00117000
*/*                    IN THE ENTRY POINT TABLE                      */ 00118000
*/*                                                                  */ 00119000
*/*  OUTPUT : OCWRC, OCWEC, R15 ^= 0 WHEN TPLOCK WAS NOT SUCCESSFUL  */ 00120000
*/*                                  OR GETSTORE UNSUCCESSFUL        */ 00121000
*/*           OCWRC, OCWEC, R15 = 0  WHEN BUILDING OF EPT AND DVTS   */ 00122000
*/*                                  WAS SUCCESSFUL                  */ 00123000
*/*                                                                  */ 00124000
*/*  ROUTINES : SPLTMSTR - CALLS EPTBLD WHICH LOOPS THRU MASTER DVT  */ 00125000
*/*                        CALLING EPTFILL TO CREATE A DVT WHEN      */ 00126000
*/*                        NECESSARY.FILLS IN LAST EPT ENTRY         */ 00127000
*/*             DVTFILL - FILLS IN THE DVTS WITH ADDRESSES OF THE    */ 00128000
*/*                       PROCESS MODULES.                           */ 00129000
*/*             DVTSERCH - SEARCHES THE ATCVT CHAIN OF DVTS. IF A    */ 00130000
*/*                        DUPLICATE IS FOUND THE DVT IS RELEASED    */ 00131000
*/*                        AND THE USE COUNT IN THE CHAINED DVT      */ 00132000
*/*                        IS INCREMENTED. IF NO DUPLICATE IS        */ 00133000
*/*                        FOUND THE DVT IS CHAINED TO THE ATCVT     */ 00134000
*/*                        CHAIN OF DVTS.                            */ 00135000
*/*             EPTSERCH - SEARCHES THE ATCVT CHAIN OF EPTS. IF A    */ 00136000
*/*                        DUPLICATE IS FOUND THE EPT IS RELEASED    */ 00137000
*/*                        AND THE USE COUNT IN THE CHAINED EPT      */ 00138000
*/*                        IS INCREMENTED. IF NO DUPLICATE IS        */ 00139000
*/*                        FOUND THE EPT IS CHAINED TO THE ATCVT     */ 00140000
*/*                        CHAIN OF EPTS.                            */ 00141000
*/*             DONOSTOR - HANDLES NON ZERO RETURN CODE FROM GETSTOR */ 00142000
*/*                                                                  */ 00143000
*/*  MACROS : GETSTOR - TO OBTAIN STORAGE FOR THE EPT(ENTRY PT TBL)  */ 00144000
*/*           TPLOCK & TPUNLOCK - TO PROVIDE PROTECTION TO THE ATCVT */ 00145000
*/*                              EPT AND DVT CHAINS WHILE DVTSERCH   */ 00146000
*/*                              AND EPTSERCH ARE PROCESSING.        */ 00147000
*/*                                                                  */ 00148000
*/********************************************************************/ 00149000
*BLDREAL: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0409 00150000
