         AL    EPTEPTR,@CF00032                                    0597 00693000
*             RTNCODE=ZERO;              /*   SET SUCCESSFUL RETURN  */ 00694000
         SLR   RTNCODE,RTNCODE                                     0598 00695000
*             END;                                                 0599 00696000
*         ELSE DO;                       /*  NO-SET END FLAG IN      */ 00697000
         B     @RC00588                                            0600 00698000
