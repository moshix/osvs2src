         ST    CR14PTR,C14SAVE(,@11)                               0394 00326000
*        C14SVPTR=C14SVPTR+4;                                      0395 00327000
         AH    @11,@CH00052                                        0395 00328000
         ST    @11,C14SVPTR(,@03)                                  0395 00329000
*        IF DDVT=OCWACVT->ATCFDVT     /* DOES CVT POINT TO D DVT     */ 00330000
*        THEN OCWACVT->ATCFDVT=DVTHFORW;      /* PUT NEXT DVT@ IN CVT * 00331000
         L     @03,OCWACVT(,R4)                                    0396 00332000
         C     DDVT,ATCFDVT(,@03)                                  0396 00333000
         BNE   @RF00396                                            0396 00334000
         MVC   ATCFDVT(4,@03),DVTHFORW(DDVT)                       0397 00335000
*        ELSE DO;                                                  0398 00336000
         B     @RC00396                                            0398 00337000
@RF00396 DS    0H                                                  0399 00338000
*          A=OCWACVT->ATCFDVT;                /* PREV DVT@            * 00339000
         L     @11,OCWACVT(,R4)                                    0399 00340000
         L     A,ATCFDVT(,@11)                                     0399 00341000
*          B=A->DVTHFORW;                    /* NEXT DVT@            */ 00342000
         L     B,DVTHFORW(,A)                                      0400 00343000
*            DO WHILE (B^=DDVT);             /* SEARCH FOR D DVT     */ 00344000
         B     @DE00401                                            0401 00345000
@DL00401 DS    0H                                                  0402 00346000
*            A=B;                                                  0402 00347000
         LR    A,B                                                 0402 00348000
*            B=B->DVTHFORW;                                        0403 00349000
         L     B,DVTHFORW(,B)                                      0403 00350000
*            END;                                                  0404 00351000
@DE00401 CR    B,DDVT                                              0404 00352000
         BNE   @DL00401                                            0404 00353000
*          A->DVTHFORW=B->DVTHFORW;          /* POINT AROUND D DVT   */ 00354000
         MVC   DVTHFORW(4,A),DVTHFORW(B)                           0405 00355000
*          END;                                                    0406 00356000
*        OCWODFST->EPTDVT=0;                 /* ZERO D DVT IN EPTAB  */ 00357000
@RC00396 L     @03,OCWODFST(,R4)                                   0407 00358000
         SLR   @00,@00                                             0407 00359000
         ST    @00,EPTDVT(,@03)                                    0407 00360000
*         /*FREESTOR AREA(DDVT)FETCHP(NO)SIZE(DVTHLEN)SAVE(YES)*/  0408 00361000
*          DO;                                                     0408 00362000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */                  0409 00363000
*/* END OF ATBUILD */                                              0409 00364000
*         RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                  0409 00365000
*         RFY ISTRPH BASED(GPR01P);                                0410 00366000
*         GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,                    0411 00367000
*                  GPR06P,GPR07P,GPR08P,GPR09P,                    0411 00368000
