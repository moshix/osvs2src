*/*                                                                  */ 00410000
*/*   ROUTINE NAME : DEQEPT - DEQUEUE EPT                            */ 00411000
*/*                                                                  */ 00412000
*/*   FUNCTION : TO DECHAIN THE EPT FROM THE ATCVT EPT CHAIN AND     */ 00413000
*/*              TO RELEASE THE EPT CORE.                            */ 00414000
*/*                                                                  */ 00415000
*/*   INPUT : OCWEPTAB - POINTS TO THE EPT                           */ 00416000
*/*                                                                  */ 00417000
*/*   OUTPUT : NONE                                                  */ 00418000
*/*                                                                  */ 00419000
*/*   ROUTINES CALLED : NONE                                         */ 00420000
*/*                                                                  */ 00421000
*/*   MACROS USED : FREESTOR - TO RETURN THE EPT CORE TO STORAGE     */ 00422000
*/*                            MANAGEMENT POOLS.                     */ 00423000
*/*                                                                  */ 00424000
*/********************************************************************/ 00425000
*DEQEPT:  PROC OPTIONS  (NOSAVE,NOSAVEAREA);                       0426 00426000
DEQEPT   DS    0H                                                  0427 00427000
*        C14SAVE=CR14PTR;                                          0427 00428000
         LA    @03,624                                             0427 00429000
         ALR   @03,R4                                              0427 00430000
         L     @11,C14SVPTR(,@03)                                  0427 00431000
         ST    CR14PTR,C14SAVE(,@11)                               0427 00432000
*        C14SVPTR=C14SVPTR+4;                                      0428 00433000
         AH    @11,@CH00052                                        0428 00434000
         ST    @11,C14SVPTR(,@03)                                  0428 00435000
*        IF OCWEPTAB= OCWACVT->ATCFEPT   /* DOES CVT POINT TO D EPTAB * 00436000
*        THEN OCWACVT->ATCFEPT = OCWEPTAB->EPTHFORM; /*NXT EPTAB - CVT* 00437000
         L     @03,OCWEPTAB(,R4)                                   0429 00438000
         L     @11,OCWACVT(,R4)                                    0429 00439000
         C     @03,ATCFEPT(,@11)                                   0429 00440000
         BNE   @RF00429                                            0429 00441000
         MVC   ATCFEPT(4,@11),EPTHFORM(@03)                        0430 00442000
*        ELSE DO;                                                  0431 00443000
         B     @RC00429                                            0431 00444000
@RF00429 DS    0H                                                  0432 00445000
*          A=OCWACVT->ATCFEPT;                /* PREV EPTAB@          * 00446000
         L     @11,OCWACVT(,R4)                                    0432 00447000
         L     A,ATCFEPT(,@11)                                     0432 00448000
*          B=A->EPTHFORM;                    /* NEXT EPTAB@          */ 00449000
