*/********************************************************************/ 00241000
*/*   ROUTINE NAME : RELDVT - RELEASE DVT                            */ 00242000
*/*                                                                  */ 00243000
*/*   FUNCTION : TO DECREMENT THE USE COUNT OF THE DVT AND IF THIS   */ 00244000
*/*              IS THE ONLY DESTINATION USING THE DVT, DEQDVT IS    */ 00245000
*/*              CALLED TO DEQUEUE AND RELEASE THE CORE              */ 00246000
*/*                                                                  */ 00247000
*/*   INPUT : OCWODFST - POINTS TO A DVT ADDRESS (EPT ENTRY)         */ 00248000
*/*                                                                  */ 00249000
*/*   OUTPUT : OCWODFST - POINTS TO THE NEXT EPT ENTRY               */ 00250000
*/*                                                                  */ 00251000
*/*   ROUTINES CALLED : DEQDVT - CALLED TO DEQUEUE THE DVT FROM      */ 00252000
*/*                              THE ATCVT DVT CHAIN AND RELEASE THE */ 00253000
*/*                              CORE                                */ 00254000
*/*                                                                  */ 00255000
*/*   MACROS USED : NONE                                             */ 00256000
*/*                                                                  */ 00257000
*/********************************************************************* 00258000
*RELDVT: PROC OPTIONS(NOSAVE,NOSAVEAREA);    /* RELEASE DVT          */ 00259000
RELDVT   DS    0H                                                  0380 00260000
*        C14SAVE=CR14PTR;                                          0380 00261000
         LA    @03,624                                             0380 00262000
         ALR   @03,R4                                              0380 00263000
         L     @11,C14SVPTR(,@03)                                  0380 00264000
         ST    CR14PTR,C14SAVE(,@11)                               0380 00265000
*        C14SVPTR=C14SVPTR+4;                                      0381 00266000
         AH    @11,@CH00052                                        0381 00267000
         ST    @11,C14SVPTR(,@03)                                  0381 00268000
*        DDVT=OCWODFST->EPTDVT-LENGTH(DVTHDR); /* DELETED DVT PTR    */ 00269000
         L     @03,OCWODFST(,R4)                                   0382 00270000
         L     DDVT,EPTDVT(,@03)                                   0382 00271000
         SH    DDVT,@CH00069                                       0382 00272000
*        DVTHUCNT=DVTHUCNT-1;                /* DECR DVT USE CNT     */ 00273000
         LH    @03,DVTHUCNT(,DDVT)                                 0383 00274000
         BCTR  @03,0                                               0383 00275000
         STH   @03,DVTHUCNT(,DDVT)                                 0383 00276000
*        IF DVTHUCNT=0                                             0384 00277000
*          THEN DO;                                                0384 00278000
         LTR   @03,@03                                             0384 00279000
         BNZ   @RF00384                                            0384 00280000
*          CALL DEQDVT;                                            0386 00281000
         BAL   @14,DEQDVT                                          0386 00282000
*          END;                                                    0387 00283000
*          ELSE;                                                   0388 00284000
@RF00384 DS    0H                                                  0389 00285000
*        OCWODFST=OCWODFST+LENGTH(ISTEPTE);  /* STEP FOR NEXT DVT PTR*/ 00286000
         LA    @03,12                                              0389 00287000
         AL    @03,OCWODFST(,R4)                                   0389 00288000
         ST    @03,OCWODFST(,R4)                                   0389 00289000
*        C14SVPTR=C14SVPTR-4;                                      0390 00290000
         LA    @03,624                                             0390 00291000
         ALR   @03,R4                                              0390 00292000
         L     @00,C14SVPTR(,@03)                                  0390 00293000
         SH    @00,@CH00052                                        0390 00294000
         ST    @00,C14SVPTR(,@03)                                  0390 00295000
*        CR14PTR=C14SAVE;                                          0391 00296000
         LR    @03,@00                                             0391 00297000
         L     CR14PTR,C14SAVE(,@03)                               0391 00298000
*        END   RELDVT;                                             0392 00299000
@EL00002 DS    0H                                                  0392 00300000
@EF00002 DS    0H                                                  0392 00301000
@ER00002 BR    @14                                                 0392 00302000
*/********************************************************************/ 00303000
*/*                                                                  */ 00304000
*/*   ROUTINE NAME : DEQDVT - DEQUEUE DVT                            */ 00305000
*/*                                                                  */ 00306000
*/*   FUNCTION : TO REMOVE THE DVT FROM THE ATCVT CHAIN OF DVTS AND  */ 00307000
*/*              TO RELEASE THE CORE OF THE DVT.                     */ 00308000
*/*                                                                  */ 00309000
*/*   INPUT : DDVT - POINTER TO THE DVT TO BE DEQUEUED AND RELEASED  */ 00310000
*/*                                                                  */ 00311000
*/*   OUTPUT : NONE                                                  */ 00312000
*/*                                                                  */ 00313000
*/*   ROUTINES CALLED : NONE                                         */ 00314000
*/*                                                                  */ 00315000
*/*   MACROS USED : FREESTOR - TO RETURN THE DVT CORE TO STORAGE     */ 00316000
*/*                            MANAGEMENT POOL                       */ 00317000
*/*                                                                  */ 00318000
*/********************************************************************/ 00319000
*DEQDVT: PROC OPTIONS(NOSAVE,NOSAVEAREA);                          0393 00320000
DEQDVT   DS    0H                                                  0394 00321000
*        C14SAVE=CR14PTR;                                          0394 00322000
         LA    @03,624                                             0394 00323000
         ALR   @03,R4                                              0394 00324000
