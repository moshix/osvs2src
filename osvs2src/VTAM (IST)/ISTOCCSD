         BALR  @14,@15                                             0301 00074000
*          RPHPTR=RPHPTR&HIGHOFF;   /* TURN OFF INDICATOR            */ 00075000
         N     RPHPTR,HIGHOFF                                      0302 00076000
*          IF RCREG^=CZERO          /* FIND SUCCESSFUL?              */ 00077000
*            THEN                   /* NO,                           */ 00078000
         LTR   RCREG,RCREG                                         0303 00079000
         BZ    @RF00303                                            0303 00080000
*              CALL DONOFIND;       /* INDICATE ERROR EXITS          */ 00081000
         BAL   @14,DONOFIND                                        0304 00082000
*            ELSE                   /* YES,                          */ 00083000
*              DO;                  /* DO THE FOLLOWING:             */ 00084000
