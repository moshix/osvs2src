         TITLE 'ISTOCCSD - SKELETON DVT READER ROUTINE                 *00001000
                        '                                               00002000
ISTOCCSD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTOCCSD  75.130'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @01,@12,24(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
*/********************************************************************/ 00015000
*/*   ROUTINE NAME : SKELETAL DVT READER                             */ 00016000
*/*                                                                  */ 00017000
*/*   FUNCTION : TO OBTAIN THE ADDRESS OF THE SKELETAL DVT & MAKE A  */ 00018000
*/*              COPY OF THE SKELETAL DVT (MASTER DVT) FOR USE BY    */ 00019000
*/*              ISTOCCFD,ISTOCCMD,ISTOCCRD IN THE BUILDING OF       */ 00020000
*/*              THE EPT AND DVTS FOR THE DESTINATION BEING OPENDSTED*/ 00021000
*/*                                                                  */ 00022000
*/*   INPUT : SEE PROLOGUE                                           */ 00023000
*/*                                                                  */ 00024000
*/*   OUTPUT : SEE PROLOGUE                                          */ 00025000
*/*                                                                  */ 00026000
*/*   ROUTINES CALLED : DONOSTR - TO HANDLE NON-ZERO RETURN CODE FROM*/ 00027000
*/*                               GETSTOR MACRO                      */ 00028000
*/*                     DONOFIND - TO HANDLE NON-ZERO RETURN CODE    */ 00029000
*/*                                FROM CALL TO SRTFIND(ISTOCCDF)    */ 00030000
*/*                     ISTOCCDF - TO OBTAIN THE ADDRESS OF THE      */ 00031000
*/*                                SKELETAL DVT.                     */ 00032000
*/*                                                                  */ 00033000
*/*   MACROS USED : CRRMAINT - FOR A MODULE TRACE THRU VTAM          */ 00034000
*/*                 GETSTOR - TO OBTAIN CORE IN WHICH TO MAKE A      */ 00035000
*/*                            COPY OF THE SKELETAL DVT              */ 00036000
*/********************************************************************/ 00037000
*                                                                  0288 00038000
*                                                                  0288 00039000
*                                                                  0288 00040000
*          DO;                                                     0288 00041000
*/***  CRRMAINT  PROCNM(ISTOCCSD)COMP(OC)FUNC(SUB)  ***/           0289 00042000
*          DCL CMZK0005 PTR(31) REG(1);                            0289 00043000
*          RFY (CMZK0005) RSTD;                                    0290 00044000
*                                                                  0290 00045000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0291 00046000
*          RFY ISTCNSTA;                                           0291 00047000
*          RFY ISTOCCRR;                                           0292 00048000
*/* END OF ATBUILD */                                              0293 00049000
*          /*** SET ENTERED BIT FOR THIS PROCEDURE ***/            0293 00050000
*CMZK0005->RPHCRR->CRACRR->NOCCSD = '1'B;                          0293 00051000
         L     @05,RPHCRR(,CMZK0005)                               0293 00052000
         L     @05,CRACRR(,@05)                                    0293 00053000
         OI    NOCCSD(@05),B'01000000'                             0293 00054000
*          /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/           0294 00055000
*CMZK0005->RPHCRR->CRACRR->OCCCRT = '0'B;                          0294 00056000
