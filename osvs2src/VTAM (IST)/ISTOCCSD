*          PARMPTR=PARMPTR|CMASK;   /* TURN ON HIGH ORDER BIT OF REG */ 00064000
         L     @05,CMASK                                           0298 00065000
         OR    PARMPTR,@05                                         0298 00066000
*          WKREG=CZERO;             /* CLEAR REG 2 FOR ISTOCCDF      */ 00067000
         SLR   WKREG,WKREG                                         0299 00068000
*          RPHPTR=RPHPTR|CMASK;     /*INDICATE MOD/DVT PROCESS NAME  */ 00069000
         OR    RPHPTR,@05                                          0300 00070000
*          CALL SRTDF;              /* FIND REQUESTED SKELETON DVT   */ 00071000
         L     @05,OCWACVT(,OCWPTR)                                0301 00072000
