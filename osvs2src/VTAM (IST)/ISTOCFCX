         TITLE 'ISTOCFCX--CLOSE UNDER ABEND                            *00001000
                        '                                               00002000
ISTOCFCX CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTOCFCX  77.057'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*/********************************************************************/ 00014000
*/*                                                                  */ 00015000
*/* ROUTINE NAME : ISTOCFCX                                  @Z40CKXL*/ 00016000
*/*                                                                  */ 00017000
*/* FUNCTION : (SEE PROLOGUE)                                        */ 00018000
*/*                                                                  */ 00019000
*/* INPUT : REG 1 - PTR TO RPH                                       */ 00020000
*/*                 RPHWEA = ADDR OF ISTOCA                          */ 00021000
*/*                 OCAOCXAD = ADDR OF ISTOCW                        */ 00022000
*/*                 OCADEBAD = ADDR OF ISTACDEB                      */ 00023000
*/*         REG 0 - PTR OF CALLER'S DOUBLE-WORD (FOR APPL NAME)      */ 00024000
*/*                                                                  */ 00025000
*/* OUTPUT : REGS 1 & 0 WILL STILL HAVE ORIGINAL CONTENTS            */ 00026000
*/*          APPL NAME, IF FOUND, PLACED IN CALLER'S DOUBLE-WORD     */ 00027000
*/*                                                                  */ 00028000
*/* SUBROUTINES CALLED: NONE                                         */ 00029000
*/*                                                                  */ 00030000
*/* ROUTINES CALLED :                                                */ 00031000
*/*             ISTOCCCC - COMPLETE CLSDST PROCESSING                */ 00032000
*/*                                                                  */ 00033000
*/* MACROS USED : CCRMAINT, FREESTOR                                 */ 00034000
*/*                                                                  */ 00035000
*/********************************************************************/ 00036000
*                                                                  0304 00037000
*                                                                  0304 00038000
*     DO;                                                          0304 00039000
*/***  CRRMAINT  PROCNM(ISTOCACX)COMP(OC)FUNC(MAIN)  ***/          0305 00040000
*     DCL CMZK0004 PTR(31) REG(1);                                 0305 00041000
*     RFY (CMZK0004) RSTD;                                         0306 00042000
*                                                                  0306 00043000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTOCCRR)  */                  0307 00044000
*     RFY ISTLCCW ;                                                0307 00045000
*     RFY ISTLCPB ;                                                0308 00046000
*     RFY ISTZSAVE;                                                0309 00047000
*     RFY ISTCFCRR;                                                0310 00048000
*     RFY ISTZCRR ;                                                0311 00049000
*     RFY ISTASCRR;                                                0312 00050000
*     RFY ISTAPCRR;                                                0313 00051000
*     RFY ISTCRA  ;                                                0314 00052000
