*     /*** SET ENTERED BIT FOR THIS PROCEDURE ***/                 0321 00078000
*CMZK0004->RPHCRR->CRACRR->NOCACX = '1'B;                          0321 00079000
         OI    NOCACX(@14),B'00000010'                             0321 00080000
*     /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/                0322 00081000
*CMZK0004->RPHCRR->CRACRR->OCCCRT = '0'B;                          0322 00082000
         XC    OCCCRT+1(2,@14),OCCCRT+1(@14)                       0322 00083000
         MVI   OCCCRT(@14),B'00000000'                             0322 00084000
*CMZK0004->RPHCRR->CRACRR->COCACX = '1'B;                          0323 00085000
         OI    COCACX(@14),B'00000010'                             0323 00086000
*     END;       /*@Z40CKXL*/                                      0324 00087000
*                                                                  0324 00088000
*   /* SET UP BASE ADDRESSES IN REGISTERS FOR CONTROL BLOCKS (REG 1  */ 00089000
*   /* ALREADY HAS ADDR OF RPH)                                      */ 00090000
*                                                                  0325 00091000
*   RFY (CXOCAPTR,CXOCWPTR,CXDEBPTR,CXRDTPTR) RSTD;                0325 00092000
*   CXOCAPTR = RPHWEA;                 /* OCA FROM RPHWEA            */ 00093000
         L     CXOCAPTR,RPHWEA(,CXRPHPTR)                          0326 00094000
*   CXOCWPTR = OCAOCXAD;               /* OCW FROM OCA               */ 00095000
         L     CXOCWPTR,OCAOCXAD(,CXOCAPTR)                        0327 00096000
*   CXDEBPTR = OCADEBAD;               /* DEB FROM OCA               */ 00097000
         L     CXDEBPTR,OCADEBAD(,CXOCAPTR)                        0328 00098000
*   CXRDTPTR = ACDRDTE & '0FFFFFFF'X;  /* APPL. RDTE FROM DEB , WITH    00099000
*                                         HI-ORDER BYTE SET TO ZERO  */ 00100000
         L     CXRDTPTR,ACDRDTE(,CXDEBPTR)                         0329 00101000
         N     CXRDTPTR,@CF02856                                   0329 00102000
*   RFY CXREG14 RSTD;                                              0330 00103000
*   GEN CODE SETS(OCASAV2,CXREG13,CXREG14);/*SET UP SVE AREA @ZM41228*/ 00104000
         ST    CXREG13,OCASAV2+4(,CXOCAPTR)                             00105000
         LA    CXREG14,OCASAV2(,CXOCAPTR)                               00106000
         ST    CXREG14,8(,CXREG13)                                      00107000
         LR    CXREG13,CXREG14                                          00108000
*   RFY CXREG14 UNRSTD;                                            0332 00109000
*   /* CLSDST OPERATION SHALL NOT BE PROCESSED IF DEB DOES NOT REFER */ 00110000
*   /* TO AN ACTIVE APPLICATION RDTE AND THAT RDTE (IF EXISTED) DOES */ 00111000
*   /* NOT POINT BACK TO THE DEB.                                    */ 00112000
*                                                                  0333 00113000
*   IF CXRDTPTR^=ZERO & RPRACT=BITON   /* ACTIVE APPL RDTE PRESENT?  */ 00114000
*     THEN                             /* YES                        */ 00115000
         SLR   @15,@15                                             0333 00116000
         CR    CXRDTPTR,@15                                        0333 00117000
         BE    @RF00333                                            0333 00118000
         TM    RPRACT(CXRDTPTR),B'10000000'                        0333 00119000
         BNO   @RF00333                                            0333 00120000
*       IF RDTDEB = CXDEBPTR           /* RDTE POINTS BACK TO DEB?   */ 00121000
*         THEN                         /* YES                        */ 00122000
         L     @14,RDTDEB+75(,CXRDTPTR)                            0334 00123000
         LA    @14,0(,@14)                                         0334 00124000
         CR    CXDEBPTR,@14                                        0334 00125000
         BNE   @RF00334                                            0334 00126000
*           DO;                                                    0335 00127000
*             OCACLDEB = BITON;        /* INDICATE CLSDEB IN PROCESS */ 00128000
*                                                                  0336 00129000
         OI    OCACLDEB(CXOCAPTR),B'01000000'                      0336 00130000
*             RFY IFGRPL BASED(CXRPLPTR);                          0337 00131000
*             RFY CXRPLPTR RSTD;                                   0338 00132000
*             CXRPLPTR=ADDR(OCASAV3);  /* USE OCASAV3 AS RPL         */ 00133000
         LA    CXRPLPTR,OCASAV3(,CXOCAPTR)                         0339 00134000
*             RPLID = 'C3'X;                                       0340 00135000
         MVI   RPLID(CXRPLPTR),X'C3'                               0340 00136000
*             RPLLEN = '0030'X;                                    0341 00137000
         MVI   RPLLEN(CXRPLPTR),X'30'                              0341 00138000
*             RPLPLHPT = ZERO;         /* CHAIN FIELD = 0            */ 00139000
         ST    @15,RPLPLHPT(,CXRPLPTR)                             0342 00140000
*             RPLARG = CXOCAPTR;       /* ADDR OF OCA                */ 00141000
         ST    CXOCAPTR,RPLARG(,CXRPLPTR)                          0343 00142000
*             RPLECB = ZERO;           /* ZERO OUT ECB               */ 00143000
*                                                                  0344 00144000
         ST    @15,RPLECB(,CXRPLPTR)                               0344 00145000
*             /* GET ADDRESS OF CVT TABLE                            */ 00146000
*                                                                  0345 00147000
*               DO; /* LISATCVT CVTBASE(PAGEABLE,(OCWACVT)) */     0345 00148000
*                                                                  0345 00149000
*               RFY ISTATCVT;                                      0346 00150000
*                 OCWACVT = ATCLCPTR; /*SET UP ATCVT POINTER*/     0347 00151000
         L     @07,ATCLCPTR                                        0347 00152000
         ST    @07,OCWACVT(,CXOCWPTR)                              0347 00153000
*               END; /*LISATCVT*/                                  0348 00154000
*             OCWSLEN = LNSVAREA;      /* LENGTH OF SAVEAREA         */ 00155000
         MVI   OCWSLEN(CXOCWPTR),X'48'                             0349 00156000
*             OCWTLEN=LENGTH(ISTOCW);  /* OCW LENGTH                 */ 00157000
         MVC   OCWTLEN(2,CXOCWPTR),@CH04046                        0350 00158000
*             OCWCLDEB = BITON;        /* INDICATE CLSDEB IN PROCESS */ 00159000
         OI    OCWCLDEB(CXOCWPTR),B'10000000'                      0351 00160000
*             OCWARDTE = CXRDTPTR;     /* APPLICATION RDTE           */ 00161000
         ST    CXRDTPTR,OCWARDTE(,CXOCWPTR)                        0352 00162000
*             OCWRPL = CXRPLPTR;       /* ADDR OF DUMMY RPL          */ 00163000
         ST    CXRPLPTR,OCWRPL(,CXOCWPTR)                          0353 00164000
*             OCWACDEB = CXDEBPTR;     /* ADDR OF DEB                */ 00165000
         ST    CXDEBPTR,OCWACDEB(,CXOCWPTR)                        0354 00166000
*             RPHWEA = CXOCWPTR;       /* ADDR OF OCW INTO RPH       */ 00167000
         ST    CXOCWPTR,RPHWEA(,CXRPHPTR)                          0355 00168000
*             RFY CXRPLPTR UNRSTD;                                 0356 00169000
*                                                                  0356 00170000
*             /* CALL ISTOCCCC TO COMPLETE PROCESSING OF CLSDST.     */ 00171000
*             /* ISTOCCCC SAVES AND RESTORES ALL REGISTERS EXCEPT    */ 00172000
*             /* REG 4                                               */ 00173000
*                                                                  0357 00174000
*             SVREG4 = REG4;           /* SAVE REG 4 TEMPORARILY     */ 00175000
         LR    @15,CXPARMR0                                        0357 00176000
         ST    REG4,SVREG4(,@15)                                   0357 00177000
*             CALL ISTOCCCC;                                       0358 00178000
         L     @15,@CV02862                                        0358 00179000
         BALR  @14,@15                                             0358 00180000
*             REG4 = SVREG4;           /* RESTORE REG 4              */ 00181000
*                                                                  0359 00182000
*                                                                  0359 00183000
         LR    @15,CXPARMR0                                        0359 00184000
         L     REG4,SVREG4(,@15)                                   0359 00185000
*               DO;                                                0360 00186000
*/***  CRRMAINT  PROCNM(ISTOCACX)COMP(OC)FUNC(RET)  ***/           0361 00187000
*               DCL CMZK0011 PTR(31) REG(1);                       0361 00188000
*               RFY (CMZK0011) RSTD;                               0362 00189000
*               /*** CLEAR AND SET CURRENT PROCEDURE BIT ***/      0363 00190000
*CMZK0011->RPHCRR->CRACRR->OCCCRT = '0'B;                          0363 00191000
         L     @15,RPHCRR(,CMZK0011)                               0363 00192000
         L     @15,CRACRR(,@15)                                    0363 00193000
         XC    OCCCRT+1(2,@15),OCCCRT+1(@15)                       0363 00194000
         MVI   OCCCRT(@15),B'00000000'                             0363 00195000
*CMZK0011->RPHCRR->CRACRR->COCACX = '1'B;                          0364 00196000
         OI    COCACX(@15),B'00000010'                             0364 00197000
*               END;                                               0365 00198000
*                                                                  0365 00199000
*             /* PLACE THE APPLICATION NAME IN THE DOUBLE-WORD       */ 00200000
*             /* ADDRESSED BY REG 0 (THIS IS SUPPLIED BY CALLER)     */ 00201000
*                                                                  0366 00202000
*             APPLNAME = RPRNAME;                                  0366 00203000
         LR    @15,CXPARMR0                                        0366 00204000
         MVC   APPLNAME(8,@15),RPRNAME(CXRDTPTR)                   0366 00205000
*           END;                                                   0367 00206000
*         ELSE;                        /* RDTE DOES NOT POINT TO DEB */ 00207000
@RF00334 DS    0H                                                  0369 00208000
*     ELSE;                            /* NO ACTIVE APPL RDTE        */ 00209000
@RF00333 DS    0H                                                  0370 00210000
*   RFY (CXOCAPTR,CXOCWPTR,CXRDTPTR,CXDEBPTR) UNRSTD;              0370 00211000
*   GEN(L     CXREG13,4(,CXREG13) RESTORE CALLERS SAVE AREA PTR)   0371 00212000
*      SETS(CXREG13);                                     /*@ZM41228*/  00213000
         L     CXREG13,4(,CXREG13) RESTORE CALLERS SAVE AREA PTR        00214000
*   IF OCAABORT = '0'B &                                  /*@ZA17873*/  00215000
*      OCARC = '0000'X                                    /*@ZA17873*/  00216000
*     THEN R15 = ZERO;                                    /*@ZA17873*/  00217000
         TM    OCAABORT(CXOCAPTR),B'00100000'                      0372 00218000
         BNZ   @RF00372                                            0372 00219000
         CLI   OCARC(CXOCAPTR),0                                   0372 00220000
         BNE   @RF00372                                            0372 00221000
