*/* LOGICAL CHANNEL COMMAND WORD                                      * 00240000
*/* ADD A DECLARE                                      003 10/18/74   * 00241000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0378 00242000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 00243000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 00244000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 00245000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 00246000
*              4  * BIT(1),            /* RESERVED                    * 00247000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0378 00248000
*                                         ERASE OR READ CONT          * 00249000
*              4  LCCWCTL BIT(4),      /* CONTROL TYPE                * 00250000
*                 5  LCCWMODE BIT(2),  /* MODE TYPE                   * 00251000
*                 5  LCCWTYPE BIT(2),  /* OPERATION TYPE              * 00252000
*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00253000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00254000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00255000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00256000
*                                         RECORD                      * 00257000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00258000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00259000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00260000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00261000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00262000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00263000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00264000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00265000
*/* LOGICAL COMMAND CODES */                                       0379 00266000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00267000
*     LXEC     BIT(8) CONSTANT('4B'X), /* RESET AT END OF COMMAND     * 00268000
*     LXO      BIT(8) CONSTANT('5B'X), /* RESET ORDERLY               * 00269000
*     LCCWPRGE BIT(8) CONSTANT('6B'X), /* PURGE                       * 00270000
