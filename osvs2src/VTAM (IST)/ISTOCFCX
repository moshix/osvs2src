*     LWRITE   BIT(2) CONSTANT('01'B),  /* WRITE TYPE LCCW            * 00284000
*     LCNTRL   BIT(2) CONSTANT('11'B),  /* CONTROL TYPE LCCW          * 00285000
*     LRESET   BIT(4) CONSTANT('1011'B),/* RESET COMMANDS             * 00286000
*     LTIC     BIT(8) CONSTANT('08'X),  /* LOGICAL TIC                * 00287000
*     LWR      BIT(6) CONSTANT('000001'B),  /* WRITE CONVERSATIONAL   * 00288000
*     LWB      BIT(6) CONSTANT('000101'B),  /* WRITE BLOCK            * 00289000
