*           RFY                                                    0268 00186000
*            (R7) UNRSTD;           /*                       @Z40GKF6*/ 00187000
*         END ELS02;                /*                       @Z40GKF6*/ 00188000
*     END ELS01;                    /*                       @Z40GKF6*/ 00189000
*   IF                              /*                       @ZA28592*/ 00190000
*       (REQKREG>BPCBTOTL)|         /* ASK FOR MORE BUFFERS  @ZA28592*/ 00191000
*       (REQKREG^>0)                /* THAN IN POOL          @ZA29969*/ 00192000
*     THEN                          /*                       @ZA28592*/ 00193000
