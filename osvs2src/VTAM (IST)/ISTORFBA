         TITLE 'ISTORFBA:  SMS BUFFER ALLOCATION ROUTINE  VS2          *00001000
                        '                                               00002000
ISTORFBA CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTORFBA  78.158'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @10,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@10                                         0001 00012000
*   R5=R2;                          /* SAVE INPUT R2         @ZA30526*/ 00013000
         LR    R5,R2                                               0209 00014000
*/* SAVE CALLER'S PSW KEY AND GET UNDER VTAM KEY             @ZA30526*/ 00015000
*                                                                  0210 00016000
*   DO;                             /* MODESET SAVEKEY(R2)         0210 00017000
*                                      EXTKEY(VTAM)                  */ 00018000
*     RESPECIFY                                                    0211 00019000
*      (GPR02F) RESTRICTED;                                        0211 00020000
*     IPK;                          /* SAVE CURRENT PSW KEY          */ 00021000
         IPK                                                       0212 00022000
*     SPKA(96);                     /* SET PSW KEY                   */ 00023000
         LA    @15,96                                              0213 00024000
         SPKA  0(@15)                                              0213 00025000
*     R2=GPR02F;                                                   0214 00026000
*     RESPECIFY                                                    0215 00027000
*      (GPR02F) UNRESTRICTED;                                      0215 00028000
*   END;                            /* MODESET SAVEKEY(R2)         0216 00029000
*                                      EXTKEY(VTAM)          @ZA30526*/ 00030000
*   SLL(R2,24);                     /* SHIFT CALLER'S KEY TO HIGH  0217 00031000
*                                      ORDER BYTE            @ZA30526*/ 00032000
         SLL   R2,24                                               0217 00033000
*   R14=(R14&'00FFFFFF'X)|R2;       /* SAVE CALLER'S KEY IN HIGH BYTE   00034000
*                                      OF R14                @ZA30526*/ 00035000
