         BNH   @DL00989                                            0996 00232000
@DC00989 DS    0H                                                  0997 00233000
*                   RFY                                            0997 00234000
*                    (FXBUFPTR,                                    0997 00235000
*                     PGBUFPTR) UNRSTD;                            0997 00236000
*                 END;                                             0998 00237000
*               ELSE                                               0999 00238000
*                 ;                                                0999 00239000
@RF00958 DS    0H                                                  1000 00240000
*             END;                                                 1000 00241000
*           ELSE                    /* POOL NOT BUILT                */ 00242000
*             DO;                                                  1001 00243000
         B     @RC00956                                            1001 00244000
@RF00956 DS    0H                                                  1002 00245000
*               IF BPDBPENT=1 THEN  /* ONLY ONE ENTRY                */ 00246000
         CLC   BPDBPENT(2,BPDPTR),@CH00050                         1002 00247000
         BNE   @RF01002                                            1002 00248000
*                 DO;                                              1003 00249000
*                   POOLID='SMS1';  /* MUST DELETE IT                */ 00250000
         MVC   POOLID(4),@CC07649                                  1004 00251000
*                   CALL DPOOL;                                    1005 00252000
