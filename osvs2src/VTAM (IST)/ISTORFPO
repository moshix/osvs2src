*       REGA=BPDLV;                 /* PUT THE LEN IN A REG          */ 00397000
*                                                                  1055 00398000
         LR    REGA,@04                                            1055 00399000
*       /*************************************************************/ 00400000
*       /*                                                           */ 00401000
*       /* ISSUE THE SYSTEM DEPENDENT GETMAIN FOR THE DIRECTORY      */ 00402000
*       /*                                                           */ 00403000
*       /*************************************************************/ 00404000
*                                                                  1056 00405000
*       REGC=231;                   /* SUBPOOL 231 FOR DIRECTORY-    */ 00406000
         LA    REGC,231                                            1056 00407000
*       DO;                         /* GETMAIN (EC) BNDRY LV(REGA) 1057 00408000
