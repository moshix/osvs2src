         TITLE 'ISTORFPO BUFFER POOL BUILD ROUTINE FOR STORAGE MANAGEME*00001000
               ENT      '                                               00002000
ISTORFPO CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTORFPO  77.083'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
    BALR  R15,0                                                         00011000
    USING *,15                                                          00012000
    STM   0,14,USERSAV                                                  00013000
    DROP  15                                                            00014000
    BALR  R12,0                                                         00015000
    USING *,R12                                                         00016000
*   SAVREG=ADDR(SAV1);                                             0926 00017000
         LA    SAVREG,SAV1                                         0926 00018000
*   RC=0;                           /* SET RC                        */ 00019000
         SLR   @14,@14                                             0927 00020000
         ST    @14,RC                                              0927 00021000
*   PREG3=R3;                       /* PUT FLAGS IN AREA FOR REFER   */ 00022000
         ST    R3,PREG3                                            0928 00023000
*   ATCVTPTR=R5;                    /* GET ADDR OF ATCVT             */ 00024000
         ST    R5,ATCVTPTR                                         0929 00025000
*   RFY                                                            0930 00026000
*     R5 UNRSTD;                                                   0930 00027000
*   BPDPTR=ATCBPDA;                 /* GET BUFFER POOL DIRECTORY ADDR*/ 00028000
         L     @04,ATCVTPTR                                        0931 00029000
         L     BPDPTR,ATCBPDA(,@04)                                0931 00030000
*   IF DELPOOL='1'B THEN            /* IS REQUEST TO DELETE          */ 00031000
*                                                                  0932 00032000
         TM    DELPOOL,B'10000000'                                 0932 00033000
         BNO   @RF00932                                            0932 00034000
*     /***************************************************************/ 00035000
*     /*                                                             */ 00036000
*     /* THE FOLLOWING CODE IS TO DELETE A POOL                      */ 00037000
*     /*                                                             */ 00038000
*     /***************************************************************/ 00039000
*                                                                  0933 00040000
*     DO;                                                          0933 00041000
*       IF BPDPTR=0 THEN            /* IF DIRECTORY ADDR IS 0        */ 00042000
         CR    BPDPTR,@14                                          0934 00043000
         BNE   @RF00934                                            0934 00044000
*         DO;                                                      0935 00045000
*           RC=8;                                                  0936 00046000
         MVC   RC(4),@CF00035                                      0936 00047000
*         END;                                                     0937 00048000
*       ELSE                                                       0938 00049000
*         DO;                                                      0938 00050000
         B     @RC00934                                            0938 00051000
@RF00934 DS    0H                                                  0939 00052000
*           POOLID=R2;              /* SET UP PARMS                  */ 00053000
         ST    R2,POOLID                                           0939 00054000
*           RFY                                                    0940 00055000
*            (R2,                                                  0940 00056000
*             R3,                                                  0940 00057000
*             SAVREG) UNRSTD;                                      0940 00058000
*           CALL DPOOL;             /* FOR DELETE POOL ROUT          */ 00059000
         BAL   @14,DPOOL                                           0941 00060000
*         END;                                                     0942 00061000
*     END;                                                         0943 00062000
*                                                                  0943 00063000
*   /*****************************************************************/ 00064000
*   /*                                                               */ 00065000
*   /* THE FOLLOWING CODE IS TO ADD A POOL                           */ 00066000
*   /*                                                               */ 00067000
*   /*****************************************************************/ 00068000
*                                                                  0944 00069000
*   ELSE                            /* IF REQUEST IS NOT DELETE      */ 00070000
*     DO;                                                          0944 00071000
         B     @RC00932                                            0944 00072000
@RF00932 DS    0H                                                  0945 00073000
*       IF BPDPTR=0 THEN            /* IF FRST TIME CREATE OUR       */ 00074000
         LTR   BPDPTR,BPDPTR                                       0945 00075000
         BNZ   @RF00945                                            0945 00076000
*         DO;                       /* OWN POOL                      */ 00077000
*           PARMAREA=PARWEPL;       /* SET UP OUR OWN PARMS          */ 00078000
         MVC   PARMAREA(12),PARWEPL                                0947 00079000
*           CALL BPOOL;             /* THEN GO BUILD THE POOL        */ 00080000
         BAL   @14,BPOOL                                           0948 00081000
*           BPDPTR=ATCBPDA;         /* GET SMS DIRECTORY ADDRESS     */ 00082000
         L     @14,ATCVTPTR                                        0949 00083000
         L     BPDPTR,ATCBPDA(,@14)                                0949 00084000
*         END;                                                     0950 00085000
*       ELSE                                                       0951 00086000
*         ;                                                        0951 00087000
@RF00945 DS    0H                                                  0952 00088000
*       IF RC=0 THEN                /* GOOD BUILD OR NOT FRST TIME   */ 00089000
         L     @14,RC                                              0952 00090000
         LTR   @14,@14                                             0952 00091000
         BNZ   @RF00952                                            0952 00092000
*         DO;                       /* SET UP TO BLD THE POOL        */ 00093000
*           PARMAREA=SAVDR234;      /* THAT THE USER REQUESTED       */ 00094000
         MVC   PARMAREA(12),SAVDR234                               0954 00095000
*           CALL BPOOL;             /* GO BLD THE POOL               */ 00096000
         BAL   @14,BPOOL                                           0955 00097000
*           IF RC=0 THEN            /* POOL BUILT                    */ 00098000
         L     @14,RC                                              0956 00099000
         LTR   @14,@14                                             0956 00100000
         BNZ   @RF00956                                            0956 00101000
*             DO;                   /* THE FOLLOWING CODE IS FOR DCR    00102000
*                                      1878                          */ 00103000
*               IF BPDBPENT=N THEN                                 0958 00104000
*                                                                  0958 00105000
         L     @14,N                                               0958 00106000
         CH    @14,BPDBPENT(,BPDPTR)                               0958 00107000
         BNE   @RF00958                                            0958 00108000
*/********************************************************************/ 00109000
*/*                                                                  */ 00110000
*/*                     CBIDPCBA  (DCR1878)                          */ 00111000
*/*                                                                  */ 00112000
*/********************************************************************/ 00113000
*                                                                  0959 00114000
*                 DO;                                              0959 00115000
*                   RFY                                            0960 00116000
*                    (REGA,                                        0960 00117000
*                     REGB,                                        0960 00118000
*                     REGC,                                        0960 00119000
*                     REGD) RSTD;                                  0960 00120000
*                   REGA=ADDR(BPDCBID);/* MOVE CBIDTBLE INTO BPDY    */ 00121000
         LA    REGA,BPDCBID(,BPDPTR)                               0961 00122000
*                   REGB=LENGTH(CBIDTBLE);                         0962 00123000
         LA    REGB,304                                            0962 00124000
*                   ENDBPE=BPDFBPE+((N-2)*LENGTH(ISTBPENT));/*     0963 00125000
*                                      CALCULATE ENDBPE              */ 00126000
         BCTR  @14,0                                               0963 00127000
         BCTR  @14,0                                               0963 00128000
         SLA   @14,4                                               0963 00129000
         AL    @14,BPDFBPE(,BPDPTR)                                0963 00130000
         LR    ENDBPE,@14                                          0963 00131000
*                   ENDCBID=REGA+REGB-8;/* CALCULATE THE END ADDR OF    00132000
*                                      CBIDTBLE                      */ 00133000
         LR    ENDCBID,REGA                                        0964 00134000
         ALR   ENDCBID,REGB                                        0964 00135000
         SL    ENDCBID,@CF00035                                    0964 00136000
*                   REGC=ADDR(CBIDTBLE);                           0965 00137000
         LA    REGC,CBIDTBLE                                       0965 00138000
*                   REGD=REGB;                                     0966 00139000
         LR    REGD,REGB                                           0966 00140000
*                   MVCL(REGA,REGC);                               0967 00141000
         MVCL  REGA,REGC                                           0967 00142000
*                   RFY                                            0968 00143000
*                    (REGA,                                        0968 00144000
*                     REGB,                                        0968 00145000
*                     REGC,                                        0968 00146000
*                     REGD) UNRSTD;                                0968 00147000
*                   RFY                                            0969 00148000
*                     CBIDPTR RSTD;                                0969 00149000
*                   DO CBIDPTR=ADDR(BPDCBID) TO ENDCBID BY LENGTH( 0970 00150000
*                         ISTCBIDT);                               0970 00151000
         LA    CBIDPTR,BPDCBID(,BPDPTR)                            0970 00152000
         B     @DE00970                                            0970 00153000
@DL00970 DS    0H                                                  0971 00154000
*                     DO BPEPTR=BPDFBPE TO ENDBPE BY LENGTH(ISTBPENT);  00155000
         L     BPEPTR,BPDFBPE(,BPDPTR)                             0971 00156000
         B     @DE00971                                            0971 00157000
@DL00971 DS    0H                                                  0972 00158000
*                       IF BPEFLAGS(1:8)=CBIDATR(1:8) THEN/* @ZM32642*/ 00159000
         CLC   BPEFLAGS(1,BPEPTR),CBIDATR(CBIDPTR)                 0972 00160000
         BNE   @RF00972                                            0972 00161000
*                         DO;       /* ATTRIBUTES MATCH              */ 00162000
*                           IF CBIDLEN<=BPEBFSIZ THEN              0974 00163000
         LH    @14,BPEBFSIZ(,BPEPTR)                               0974 00164000
         CH    @14,CBIDLEN(,CBIDPTR)                               0974 00165000
         BL    @RF00974                                            0974 00166000
*                             DO;   /* BUFFER FITS IN                */ 00167000
*                               IF(CBIDPCB=0)|(CBIDPCB->BPCBBSIZ>= 0976 00168000
*                                   BPEBFSIZ) THEN                 0976 00169000
         L     @07,CBIDPCB(,CBIDPTR)                               0976 00170000
         LTR   @07,@07                                             0976 00171000
         BZ    @RT00976                                            0976 00172000
         CH    @14,BPCBBSIZ(,@07)                                  0976 00173000
         BH    @RF00976                                            0976 00174000
@RT00976 DS    0H                                                  0977 00175000
*                                 CBIDPCB=BPEBPCB;/* NO              */ 00176000
         L     @14,BPEBPCB(,BPEPTR)                                0977 00177000
         ST    @14,CBIDPCB(,CBIDPTR)                               0977 00178000
*                               ELSE                               0978 00179000
*                                 ;                                0978 00180000
@RF00976 DS    0H                                                  0979 00181000
*                             END;                                 0979 00182000
*                           ELSE                                   0980 00183000
*                             ;                                    0980 00184000
@RF00974 DS    0H                                                  0981 00185000
*                         END;                                     0981 00186000
*                       ELSE                                       0982 00187000
*                         ;                                        0982 00188000
@RF00972 DS    0H                                                  0983 00189000
*                     END;                                         0983 00190000
         AL    BPEPTR,@CF00047                                     0983 00191000
@DE00971 CR    BPEPTR,ENDBPE                                       0983 00192000
         BNH   @DL00971                                            0983 00193000
*                   END;                                           0984 00194000
         AL    CBIDPTR,@CF00035                                    0984 00195000
@DE00970 CR    CBIDPTR,ENDCBID                                     0984 00196000
         BNH   @DL00970                                            0984 00197000
*                   RFY                                            0985 00198000
*                     CBIDPTR UNRSTD;                              0985 00199000
*                   RFY                                            0986 00200000
*                    (FXBUFPTR,                                    0986 00201000
*                     PGBUFPTR) RSTD;                              0986 00202000
*                   FXBUFPTR=BPDCHPCB;                             0987 00203000
         L     FXBUFPTR,BPDCHPCB(,BPDPTR)                          0987 00204000
*                   F='0'B;                                        0988 00205000
         NI    F,B'10111111'                                       0988 00206000
*                   DO PGBUFPTR=BPDFBPE TO ENDBPE BY LENGTH(ISTBPENT)   00207000
*                         WHILE F='0'B;                            0989 00208000
         L     PGBUFPTR,BPDFBPE(,BPDPTR)                           0989 00209000
         B     @DE00989                                            0989 00210000
@DL00989 TM    F,B'01000000'                                       0989 00211000
         BNZ   @DC00989                                            0989 00212000
*                     IF PGBUFPTR->BPEFLAGS(1:8)='44'X THEN/* DO   0990 00213000
*                                      ATTRIBUTES OF POOL MATCH THOSE   00214000
*                                      REQUIRED FOR PGBUF    @ZM32642*/ 00215000
         CLI   BPEFLAGS(PGBUFPTR),X'44'                            0990 00216000
         BNE   @RF00990                                            0990 00217000
*                       DO;         /* YES                           */ 00218000
*                         F='1'B;   /* TURN ON SWITCH                */ 00219000
         OI    F,B'01000000'                                       0992 00220000
*                         BPDPGPCB=PGBUFPTR->BPEBPCB;/* SET UP BPCB     00221000
*                                      ADDR FOR PGBUF POOL           */ 00222000
         L     @14,BPEBPCB(,PGBUFPTR)                              0993 00223000
         ST    @14,BPDPGPCB(,BPDPTR)                               0993 00224000
*                       END;                                       0994 00225000
*                     ELSE                                         0995 00226000
*                       ;                                          0995 00227000
@RF00990 DS    0H                                                  0996 00228000
*                   END;                                           0996 00229000
