*       BPCMINAV=BPCBTOTL;          /* INIT MIN BUF AVAILABILITY   1295 01253000
*                                                            @Z40GKF6*/ 01254000
         STH   @00,BPCMINAV(,BPCBPTR)                              1295 01255000
*       BPCBBSIZ=BUFSIZE1;          /* INIT. SIZE OF BUFFER  @Z40GKF6*/ 01256000
