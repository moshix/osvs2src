*                 RESPECIFY                                        1025 00299000
*                  (GPR00P,                                        1025 00300000
*                   GPR01P,                                        1025 00301000
*                   GPR15P) UNRESTRICTED;                          1025 00302000
*               END;                /* PGFREE(R) A(BEGADR) EA(ENDADR)   00303000
*                                      FREE THE CORE                 */ 00304000
*               DO;                 /* FREEMAIN (E) LV(BPDLV) A(BPDA)*/ 00305000
*                 RESPECIFY                                        1028 00306000
*                  (GPR01P) RESTRICTED;                            1028 00307000
*                 I005=I005&&I005;  /* INIT LIST                     */ 00308000
