*/********************************************************************/ 01198000
*/*                                                                  */ 01199000
*/*                          END OF GETS PROCESSING                  */ 01200000
*/*                                                                  */ 01201000
*/********************************************************************/ 01202000
*                                                                  1285 01203000
*                                                                  1285 01204000
*/********************************************************************/ 01205000
*/*                                                                  */ 01206000
*/*                          BPCB PROCESSING                         */ 01207000
*/*                                                                  */ 01208000
*/*  THE FOLLOWING PROCESSING INITIALIZES THE BPENTRY AND BPCB       */ 01209000
*/*  CONTROL BLOCKS FOR THIS BUFFER POOL.                            */ 01210000
*/*                                                                  */ 01211000
*/********************************************************************/ 01212000
*                                                                  1285 01213000
*   IF RC=0 THEN                    /* WAS PRIOR PROCESSING OK       */ 01214000
         SLR   @04,@04                                             1285 01215000
         C     @04,RC                                              1285 01216000
         BNE   @RF01285                                            1285 01217000
*     DO;                           /* YES - INIT THE BLOCKS         */ 01218000
*       BPEPID=POOLID;              /* SET POOL IDENTIFIER           */ 01219000
         MVC   BPEPID(4,BPEPTR),POOLID                             1287 01220000
*       BPEFLAGS=BPCBFLAG;                                         1288 01221000
         MVC   BPEFLAGS(2,BPEPTR),BPCBFLAG(BPCBPTR)                1288 01222000
*       BPEBFSIZ=BUFSIZE-8;         /* SET THE LENGTH OF A BUFFER    */ 01223000
         LH    @00,BUFSIZE                                         1289 01224000
         SL    @00,@CF00035                                        1289 01225000
         STH   @00,BPEBFSIZ(,BPEPTR)                               1289 01226000
*       BPEBPCB=BPCBPTR;            /* POINT TO BPCB FOR THIS POOL   */ 01227000
         ST    BPCBPTR,BPEBPCB(,BPEPTR)                            1290 01228000
*       BPDBPENT=BPDBPENT+1;        /* INCREMENT NO. OF PLS BUILT BY    01229000
*                                      1                             */ 01230000
*                                                                  1291 01231000
         LA    @00,1                                               1291 01232000
         AH    @00,BPDBPENT(,BPDPTR)                               1291 01233000
         STH   @00,BPDBPENT(,BPDPTR)                               1291 01234000
*       /*************************************************************/ 01235000
*       /*                                                           */ 01236000
*       /* BPENTRY IS INITIALIZED. THE FOLLOWING INITIALIZES THE BPCB*/ 01237000
*       /* FOR THIS POOL.                                            */ 01238000
*       /*                                                           */ 01239000
*       /*************************************************************/ 01240000
*                                                                  1292 01241000
*       BPCBPDTY=BPDPTR;            /* MAKE BPCB POINT TO BUFFER POOL   01242000
*                                      DIRECTORY                     */ 01243000
         ST    BPDPTR,BPCBPDTY(,BPCBPTR)                           1292 01244000
*       BPCBTOTL=BUFNO+BUFNO1;      /* NUMBER OF BUFFERS IN POOL     */ 01245000
         LH    @15,BUFNO                                           1293 01246000
         LH    @14,BUFNO1                                          1293 01247000
