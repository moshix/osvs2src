*     REGC,                                                        1046 00358000
*     REGD) RSTD;                                                  1046 00359000
*   SAVREG=ADDR(SAV2);              /* SET UP SAVE FOR THIS ROUT     */ 00360000
         LA    SAVREG,SAV2                                         1047 00361000
*   SAVREG->SVHSA=ADDR(SAV1);       /* SET PTR TO CALLER'S SAVE      */ 00362000
         LA    @04,SAV1                                            1048 00363000
         ST    @04,SVHSA(,SAVREG)                                  1048 00364000
*   RFY                                                            1049 00365000
*     SAVREG UNRSTD;                                               1049 00366000
*   IF BPDPTR=0 THEN                /* HAS NOT BEEN SET UP           */ 00367000
         LTR   BPDPTR,BPDPTR                                       1050 00368000
         BNZ   @RF01050                                            1050 00369000
*     DO;                           /* THEN SET IT UP                */ 00370000
*                                                                  1051 00371000
*       /*************************************************************/ 00372000
*       /*                                                           */ 00373000
*       /* CALCULATE THE LENGTH OF THE ENTIRE DIRECTORY              */ 00374000
*       /*                                                           */ 00375000
*       /*************************************************************/ 00376000
*                                                                  1052 00377000
*       BPDLV=PGSZ;                 /* OBTAIN ENTIRE PAGE FOR SMS CB    00378000
*                                                            @ZM32024*/ 00379000
         L     @04,@CF07624                                        1052 00380000
         ST    @04,BPDLV                                           1052 00381000
*       SMSCBLEN=LENGTH(BPDHDR)+N*(LENGTH(ISTBPENT)+LENGTH(ISTBPCB))+   00382000
