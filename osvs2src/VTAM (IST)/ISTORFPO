*     END;                                                         1221 01024000
@RC01156 DS    0H                                                  1222 01025000
*                                                                  1222 01026000
*/********************************************************************/ 01027000
*/*                                                                  */ 01028000
*/*                          GETS PROCESSING                         */ 01029000
*/*                                                                  */ 01030000
*/*  THE FOLLOWING CODE ACQUIRES STORAGE FOR THE BUFFER POOL TO BE   */ 01031000
*/*  BUILT. THE POOL IS FIXED IN REAL STORAGE IF REQUIRED. IN THE    */ 01032000
*/*  EVENT THAT THE GETMAIN FAILS, AND THE POOL BEING ACQUIRED IS THE*/ 01033000
*/*  STORAGE MANAGEMENT PRIVATE POOL, TEH BUFFER POOL DIRECTORY AND  */ 01034000
*/*  AND CONTROL BLOCKS ARE 'PAGE FREED' AND FREEMAINED.             */ 01035000
*/*                                                                  */ 01036000
*/********************************************************************/ 01037000
*                                                                  1222 01038000
*   IF RC=0 THEN                    /* WAS PRIOR PROCESSING OK       */ 01039000
@RF01154 SLR   @04,@04                                             1222 01040000
         C     @04,RC                                              1222 01041000
         BNE   @RF01222                                            1222 01042000
*     DO;                           /* YES - DO ABOVE FUNCTIONS      */ 01043000
*       IF BPCBFEXT=0 THEN                                         1224 01044000
         C     @04,BPCBFEXT(,BPCBPTR)                              1224 01045000
         BE    @RT01224                                            1224 01046000
*         ;                                                        1225 01047000
*       ELSE                                                       1226 01048000
*         DO;                                                      1226 01049000
*           RFY                                                    1227 01050000
*            (BUF1,                                                1227 01051000
*             BUF2) RSTD;                                          1227 01052000
*           BUF1=BPCBFEXT;          /* GET FIRST EXTENTION PTR       */ 01053000
         L     BUF1,BPCBFEXT(,BPCBPTR)                             1228 01054000
*           DO I=1 TO BUFNO1 BY 1;  /* CHAIN AND INIT BUFFERS IN   1229 01055000
