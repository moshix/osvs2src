         TM    USERFIX,B'01000000'                                 1272 01168000
         BNO   @RF01272                                            1272 01169000
@RT01272 DS    0H                                                  1273 01170000
*                 DO;               /* YES - FIX IT                  */ 01171000
*                   REGA=BPCBADR1;  /* SET START @ TO FIX            */ 01172000
         L     REGA,BPCBADR1(,BPCBPTR)                             1274 01173000
*                   REGB=BPCBADR2;  /* SET END @ TO FIX              */ 01174000
         L     REGB,BPCBADR2(,BPCBPTR)                             1275 01175000
*                   CALL PFIX;      /* FIX THE PAGES                 */ 01176000
         BAL   @14,PFIX                                            1276 01177000
*                 END;              /* PAGES FIXED                   */ 01178000
*               ELSE                                               1278 01179000
*                 ;                 /* POOL NOT TO BE FIXED          */ 01180000
@RF01272 DS    0H                                                  1279 01181000
*             END;                  /* POOL INIT'ED AND FIXED        */ 01182000
*           ELSE                                                   1280 01183000
*             RC=4;                 /* UNSUCCESSFUL GETMAIN          */ 01184000
         B     @RC01267                                            1280 01185000
@RF01267 MVC   RC(4),@CF00093                                      1280 01186000
*         END;                      /* END OF FUNCTIONS              */ 01187000
*     END;                                                         1282 01188000
*   ELSE                                                           1283 01189000
*     ;                             /* PRIOR PROCESSING FAILED       */ 01190000
@RF01222 DS    0H                                                  1284 01191000
*   RFY                                                            1284 01192000
*    (REGA,                                                        1284 01193000
*     REGB,                                                        1284 01194000
*     REGC) UNRSTD;                                                1284 01195000
*                                                                  1284 01196000
