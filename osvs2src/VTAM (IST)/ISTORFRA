         LR    @14,R0TASKID                                        0062 00050000
         O     @14,@CF00813                                        0062 00051000
         LR    R0MASK,@14                                          0062 00052000
*                     RFY R10BPCB RSTD;                            0063 00053000
*          IF R2CRAPTR=0             /* THIS FIRST CALL FOR CRAFIND  */ 00054000
*          THEN;                     /* YES */                     0064 00055000
         LTR   R2CRAPTR,R2CRAPTR                                   0064 00056000
         BZ    @RT00064                                            0064 00057000
*          ELSE                      /* NO, ADJUST TO POINT TO PREFIX*/ 00058000
*            R2CRAPTR=R2CRAPTR - PREFIX8;                          0066 00059000
         SL    R2CRAPTR,@CF00089                                   0066 00060000
*          /**********************************************************/ 00061000
*          /* GET BPCB ADDRESS OF POOL CONTAINING CRA CONTROL BLOCKS */ 00062000
*          /**********************************************************/ 00063000
*          R10BPCB=EVAL(ADDR(BPDCBID)+(CBICRA-1)*8)->CBIDPCB;      0067 00064000
@RT00064 L     @14,ATCBPDA(,R15ATCVT)                              0067 00065000
         LA    @14,BPDCBID(,@14)                                   0067 00066000
         AL    @14,@CF00815                                        0067 00067000
         L     R10BPCB,CBIDPCB(,@14)                               0067 00068000
*          R15=ATCSMABF;                                           0068 00069000
         L     R15,ATCSMABF(,R15ATCVT)                             0068 00070000
*          CALL ISTORABF;            /* CALL BUFFER FIND ROUTINE     */ 00071000
         BALR  @14,@15                                             0069 00072000
*                     RFY R10BPCB UNRSTD;                          0070 00073000
*                     RFY (R4COMP,R14) RSTD;                       0071 00074000
*          R4COMP=R13SAVR4;          /* RESTORE POINTER TO SAVEAREA  */ 00075000
         LR    R4COMP,R13SAVR4                                     0072 00076000
*          R2SAVED=R2CRAPTR;         /* UPDATE CRA PTR IN SAVEAREA   */ 00077000
         ST    R2CRAPTR,R2SAVED(,R4COMP)                           0073 00078000
*          LM(R0,R14,BUFSAVE);       /* RESTORE REGS FROM SAVEAREA   */ 00079000
         LM    R0,R14,BUFSAVE(R4COMP)                              0074 00080000
*                     RFY (R1,R8,R9,R10,R11,R12) RSTD;             0075 00081000
*          IF R2CRAPTR=0             /* WAS ALLOCATED BUFFER FOUND   */ 00082000
*          THEN;                     /* NO  */                     0076 00083000
         LTR   R2CRAPTR,R2CRAPTR                                   0076 00084000
         BZ    @RT00076                                            0076 00085000
*          ELSE                      /* YES, ADJUST TO PNT PAST PREFX*/ 00086000
*            R2CRAPTR=R2CRAPTR + PREFIX8;                          0078 00087000
