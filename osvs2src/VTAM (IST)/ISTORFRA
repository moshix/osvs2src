ISTORFRA CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTORFRA  78.025'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @07,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@07                                         0001 00012000
*                                                                  0043 00013000
*           DO; /* LISATCVT CVTBASE(PAGEABLE,(R15ATCVT)) */        0044 00014000
*             R15ATCVT = ATCLCPTR; /*SET UP ATCVT POINTER*/        0045 00015000
         L     R15ATCVT,ATCLCPTR                                   0045 00016000
*           END; /*LISATCVT*/                                      0046 00017000
*          R3SMBPCB=BPDSMPCB;        /* GET POINTER TO SMS BUFF POOL */ 00018000
         L     @06,ATCBPDA(,R15ATCVT)                              0047 00019000
         L     R3SMBPCB,BPDSMPCB(,@06)                             0047 00020000
*CSFAIL1:;                           /* LABEL FOR CS FAIL  @ZA26552*/   00021000
CSFAIL1  DS    0H                                                  0049 00022000
* IF BPCBFBA=0         /* NO MORE SAVE AREA BUFFERS?       @Z40GKF6*/   00023000
* THEN R2CRAPTR=0;     /* CANT FIND MORE CRA'S              @Z40GKF6*/  00024000
         L     @06,BPCBFBA-1(,R3SMBPCB)                            0049 00025000
         LA    @06,0(,@06)                                         0049 00026000
         LTR   @06,@06                                             0049 00027000
         BNZ   @RF00049                                            0049 00028000
         SLR   R2CRAPTR,R2CRAPTR                                   0050 00029000
* ELSE DO;             /* SAVE AREAS EXIST- FIND CRA'S      @Z40GKF6*/  00030000
