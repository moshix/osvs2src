*                     RFY R5SWAP UNRSTD;                           0057 00042000
*          STM(R0,R14,BUFSAVE);      /* SAVE REGISTERS IN SAVEAREA   */ 00043000
         STM   R0,R14,BUFSAVE(R4COMP)                              0058 00044000
*                     RFY (R1,R8,R9,R10,R11,R12,R14) UNRSTD;       0059 00045000
*          R13SAVR4=R4COMP;          /* SAVE POINTER TO SAVEAREA     */ 00046000
         LR    R13SAVR4,R4COMP                                     0060 00047000
*                     RFY R4COMP UNRSTD;                           0061 00048000
