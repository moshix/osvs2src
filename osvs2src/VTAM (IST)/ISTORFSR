         LR    @07,GPR03P                                          0123 00094000
         N     @07,CLEARMSK                                        0123 00095000
         CLR   @07,@08                                             0123 00096000
         BE    @RF00123                                            0123 00097000
*                   DO;             /*                       @G35RKD9*/ 00098000
*                     RFY                                          0125 00099000
*                      (GPR14P,                                    0125 00100000
*                       GPR15P,                                    0125 00101000
*                       GPR07F,                                    0125 00102000
*                       GPR06F) RSTD;/*                      @G35RKD9*/ 00103000
*                     GPR06F=GPR04F+LENGTH(ISTSMHDR);/* ADDRESS TO 0126 00104000
*                                      CLEAR                 @G35RKD9*/ 00105000
         LA    @08,16                                              0126 00106000
         LR    GPR06F,GPR04F                                       0126 00107000
         ALR   GPR06F,@08                                          0126 00108000
*                     GPR07F=GPR00F-LENGTH(ISTSMHDR);/* GET LENGTH 0127 00109000
*                                                            @G35RKD9*/ 00110000
         LCR   @08,@08                                             0127 00111000
         ALR   @08,GPR00F                                          0127 00112000
         LR    GPR07F,@08                                          0127 00113000
*                     GPR14P=GPR06F;/* FROM ADDRESS          @G35RKD9*/ 00114000
         LR    GPR14P,GPR06F                                       0128 00115000
*                     GPR15P=0;     /* FROM LENGTH           @G35RKD9*/ 00116000
         SLR   @08,@08                                             0129 00117000
         LR    GPR15P,@08                                          0129 00118000
*                     GEN CODE REFS(GPR06F,GPR07F,GPR14P,GPR15P)/* 0130 00119000
*                                                            @G35RKD9*/ 00120000
*                         SETS(GPR06F,GPR07F,GPR14P)/* CLEAR AREA  0130 00121000
*                                                            @G35RKD9*/ 00122000
*                         (MVCL GPR06F,GPR14P);/*            @G35RKD9*/ 00123000
         MVCL GPR06F,GPR14P                                             00124000
*                     RFY                                          0131 00125000
*                      (GPR14P,                                    0131 00126000
*                       GPR15P) UNRSTD;/*                    @G35RKD9*/ 00127000
*                   END;            /*                       @G35RKD9*/ 00128000
*                 RFY                                              0133 00129000
*                  (OLDREG,                                        0133 00130000
*                   NEWREG,                                        0133 00131000
*                   ATCVTPTR) RSTD;                                0133 00132000
@RF00123 DS    0H                                                  0134 00133000
*                 ATCVTPTR=ATCLCPTR;/* GET ATCVT POINTER     @G35RKD9*/ 00134000
         L     ATCVTPTR,ATCLCPTR                                   0134 00135000
*                 OLDREG=ATCORTBF;  /* GET TO-BE-FREE QUEUE HEADER 0135 00136000
*                                                            @G35RKD9*/ 00137000
         L     OLDREG,ATCORTBF(,ATCVTPTR)                          0135 00138000
*CSLOOP:                                                           0136 00139000
*                 ;                 /*                       @G35RKD9*/ 00140000
CSLOOP   DS    0H                                                  0137 00141000
*                 SMHCHTBF=OLDREG;  /* CHAIN QUEUE TO NEW ELEMENT  0137 00142000
*                                                            @G35RKD9*/ 00143000
         ST    OLDREG,SMHCHTBF(,FREEPTR)                           0137 00144000
*                 CS(OLDREG,FREEPTR,ATCORTBF);/* ADD NEW ELEMENT TO     00145000
*                                      QUEUE                 @G35RKD9*/ 00146000
         CS    OLDREG,@04,ATCORTBF(ATCVTPTR)                       0138 00147000
*                 BC(FAIL,CSLOOP);  /* CONTINUE UNTIL SUCCESSFUL   0139 00148000
*                                                            @G35RKD9*/ 00149000
         BC    7,CSLOOP                                            0139 00150000
*                 OLDREG=ATCORSRB;  /* GET TO-BE-FREE QUEUE GATE   0140 00151000
*                                                            @G35RKD9*/ 00152000
         L     OLDREG,ATCORSRB(,ATCVTPTR)                          0140 00153000
*NOSCHE:                                                           0141 00154000
*                 ;                 /*                       @G35RKD9*/ 00155000
NOSCHE   DS    0H                                                  0142 00156000
*                 IF OLDREG<=0      /* TO-BE-FREE QUEUE GATED OR SRB    00157000
*                                      NOT INITIALIZED YET   @G35RKD9*/ 00158000
*                   THEN                                           0142 00159000
         LTR   OLDREG,OLDREG                                       0142 00160000
         BNP   @RT00142                                            0142 00161000
*                   ;               /* DONT SCHEDULE ORMMG TO RUN-ITS   00162000
*                                      ALREADY RUNNING OR SRB HAS NOT   00163000
*                                      BE INITIALIZED YET    @G35RKD9*/ 00164000
*                 ELSE              /*                       @G35RKD9*/ 00165000
*                   DO;             /* ATTEMPT TO GATE QUEUE AND   0144 00166000
*                                      SCHEDULE ORMMG        @G35RKD9*/ 00167000
*                     NEWREG=OLDREG|GATEMASK;/* SET UP GATE  @G35RKD9*/ 00168000
         LR    NEWREG,OLDREG                                       0145 00169000
         O     NEWREG,@CF01548                                     0145 00170000
*                     CS(OLDREG,NEWREG,ATCORSRB);/* ATTEMPT TO GATE     00171000
*                                      QUEUE                 @G35RKD9*/ 00172000
         CS    OLDREG,@08,ATCORSRB(ATCVTPTR)                       0146 00173000
*                     BC(FAIL,NOSCHE);/* CHECK TO SEE IF QUEUE     0147 00174000
*                                      ALREADY GATED@G35RKD9         */ 00175000
         BC    7,NOSCHE                                            0147 00176000
*                     GPR04P=ATCORSRB;/* GET SRB ADDRESS     @G35RKD9*/ 00177000
         L     GPR04P,ATCORSRB(,ATCVTPTR)                          0148 00178000
*                     RFY                                          0149 00179000
*                      (OLDREG,                                    0149 00180000
*                       NEWREG,                                    0149 00181000
*                       ATCVTPTR) UNRSTD;                          0149 00182000
*                     GEN CODE SETS(GPR02F,GPR06F) REFS(ZERO);     0150 00183000
*                                                   /*       @G35RKD9*/ 00184000
         MODESET KEYADDR=ZERO,SAVEKEY=(2),WORKREG=6                     00185000
*                     GEN REFS(CVTLSMQ,CVTMAP,SRBFLNK);            0151 00186000
*                                        /* SCHEDULE SRB     @G35RKD9*/ 00187000
           SCHEDULE SRB=(GPR04P),SCOPE=LOCAL                            00188000
*                     GEN CODE SETS(GPR02F,GPR06F) REFS(ZERO);     0152 00189000
*                                                   /*       @G35RKD9*/ 00190000
         MODESET KEYADDR=(2),WORKREG=6                                  00191000
*                   END;            /* END SCHEDULING SRB    @G35RKD9*/ 00192000
*                 RFY                                              0154 00193000
*                  (GPR14P,                                        0154 00194000
*                   GPR15F) RSTD;   /*                       @G35RKD9*/ 00195000
@RT00142 DS    0H                                                  0155 00196000
*                 GPR04F=GPR03F;    /* GET INPUT PARM IN R4 TO RETURN   00197000
*                                                            @G35RKD9*/ 00198000
         LR    GPR04F,GPR03F                                       0155 00199000
*                 GPR14P=GPR10P;    /* RESTORE RETURN ADDRESS      0156 00200000
*                                                            @G35RKD9*/ 00201000
         LR    GPR14P,GPR10P                                       0156 00202000
*                 GPR01P=GPR09F;    /* RESTORE RPH POINTER   @G35RKD9*/ 00203000
         LR    GPR01P,GPR09F                                       0157 00204000
*                 GPR15F=0;         /* SET RETURN CODE       @G35RKD9*/ 00205000
         SLR   GPR15F,GPR15F                                       0158 00206000
*               END;                /* END VALID FREESTOR          0159 00207000
*                                      REQUEST-CHARACTERISTICS MATCH    00208000
*                                                            @G35RKD9*/ 00209000
*             ELSE                                                 0160 00210000
*               DO;                 /* CHARACTERISTICS DONT MATCH  0160 00211000
*                                                            @G35RKD9*/ 00212000
         B     @RC00121                                            0160 00213000
@RF00121 DS    0H                                                  0161 00214000
*                 GPR04F=GPR03F;    /* GET INPUT PARM IN R4 FOR    0161 00215000
*                                      RETURN                @G35RKD9*/ 00216000
         LR    GPR04F,GPR03F                                       0161 00217000
*                 GPR14P=GPR10P;    /* RETSTORE RETURN ADDRESS     0162 00218000
*                                                            @G35RKD9*/ 00219000
         LR    GPR14P,GPR10P                                       0162 00220000
*                 GPR15F=4;         /* SET RETURN CODE       @G35RKD9*/ 00221000
         LA    GPR15F,4                                            0163 00222000
*               END;                /*                       @G35RKD9*/ 00223000
*           END;                    /* END FREESTOR FOR CSA SUBPOOLS    00224000
*                                                            @G35RKD9*/ 00225000
*         ELSE                                                     0166 00226000
*           DO;                     /* FREESTOR F0R NON-CSA SUBPOOLS    00227000
*                                                            @G35RKD9*/ 00228000
         B     @RC00113                                            0166 00229000
@RF00113 DS    0H                                                  0167 00230000
*             RFY                                                  0167 00231000
*              (GPR07F,                                            0167 00232000
*               GPR08F) RSTD;                                      0167 00233000
