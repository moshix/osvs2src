         TITLE 'ISTORFSR - VS/2 GETSTOR/FREESTOR SERVICE ROUTINE       *00001000
                        '                                               00002000
ISTORFSR CSECT ,                                                   0001 00003000
ISTORCSR DS    0H                                                  0001 00004000
         ENTRY ISTORCSR                                                 00005000
@MAINENT DS    0H                                                  0001 00006000
         USING *,@15                                               0001 00007000
         B     @PROLOG                                             0001 00008000
         DC    AL2(@EP00001-@MAINENT)                                   00009000
         DC    AL1(16)                                             0001 00010000
         DC    C'ISTORFSR  77.266'                                 0001 00011000
         DROP  @15                                                      00012000
@PROLOG  BALR  @05,0                                               0001 00013000
@PSTART  DS    0H                                                  0001 00014000
         USING @PSTART,@05                                         0001 00015000
         AH    @15,4(,@15)                                         0001 00016000
         BR    @15                                                 0001 00017000
@EP00001 DS    0H                                                  0002 00018000
*   IF(GPR03F&OPMASK)^=0 THEN       /* GET OR FREE REQUEST           */ 00019000
         LR    @15,GPR03F                                          0104 00020000
         N     @15,OPMASK                                          0104 00021000
         LTR   @15,@15                                             0104 00022000
         BZ    @RF00104                                            0104 00023000
*FREE:                                                             0105 00024000
*     DO;                           /* IT IS A FREESTOR              */ 00025000
FREE     DS    0H                                                  0106 00026000
*FREESTOR:                                                         0106 00027000
*       DO;                         /* PROCESS A FREESTOR REQUEST    */ 00028000
FREESTOR DS    0H                                                  0107 00029000
*         RFY                                                      0107 00030000
*          (GPR10F,                                                0107 00031000
*           GPR09F) RSTD;                                          0107 00032000
*         GPR04F=ADDRMSK&GPR04F;    /* CLEAR HI BYTE         @ZM31427*/ 00033000
         N     GPR04F,ADDRMSK                                      0108 00034000
*         GPR10P=GPR14P;            /* SAVE RETURN ADDRESS           */ 00035000
         LR    GPR10P,GPR14P                                       0109 00036000
*         RFY                                                      0110 00037000
*           GPR14F UNRSTD;                                         0110 00038000
*         RFY                                                      0111 00039000
*           GPR06F RSTD;                                           0111 00040000
*         SPPARM=GPR03F&SPMASK;     /* ISOLATE SUBPOOL PASSED      0112 00041000
*                                                            @G35RKD9*/ 00042000
         LR    SPPARM,GPR03F                                       0112 00043000
         N     SPPARM,@CF02016                                     0112 00044000
*         IF(SPPARM=SP227)|         /* CSA SUBPOOL ?         @G35RKD9*/ 00045000
*             (SPPARM=SP228)|       /* CSA SUBPOOL ?         @G35RKD9*/ 00046000
*             (SPPARM=SP231)|       /* CSA SUBPOOL ?         @G35RKD9*/ 00047000
*             (SPPARM=SP241)|       /* CSA SUBPOOL ?         @G35RKD9*/ 00048000
*             (SPPARM=SP239)|       /* CSA SUBPOOL ?         @G35RKD9*/ 00049000
*             (SPPARM=SP245) THEN   /* FREE STORAGE THROUGH ORMMG  0113 00050000
*                                                            @G35RKD9*/ 00051000
         C     SPPARM,@CF02040                                     0113 00052000
         BE    @RT00113                                            0113 00053000
         C     SPPARM,@CF02042                                     0113 00054000
         BE    @RT00113                                            0113 00055000
         C     SPPARM,@CF02044                                     0113 00056000
         BE    @RT00113                                            0113 00057000
         C     SPPARM,@CF02046                                     0113 00058000
         BE    @RT00113                                            0113 00059000
         C     SPPARM,@CF02048                                     0113 00060000
         BE    @RT00113                                            0113 00061000
         C     SPPARM,@CF02050                                     0113 00062000
         BNE   @RF00113                                            0113 00063000
@RT00113 DS    0H                                                  0114 00064000
*           DO;                     /*                       @G35RKD9*/ 00065000
*             RFY                                                  0115 00066000
*               ISTSMHDR BASED(FREEPTR);/* BASE HEADER       @G35RKD9*/ 00067000
*             RFY                                                  0116 00068000
*               ISTATCVT BASED(ATCVTPTR);/* BASE ATCVT       @G35RKD9*/ 00069000
*             GPR09F=GPR01P;        /* SAVE RPH POINTER      @G35RKD9*/ 00070000
         LR    GPR09F,GPR01P                                       0117 00071000
*             FREEPTR=FREEPTR-LENGTH(ISTSMHDR);/* ADJUST POINTER   0118 00072000
*                                                            @G35RKD9*/ 00073000
         LA    @08,16                                              0118 00074000
         SLR   FREEPTR,@08                                         0118 00075000
*             GPR00F=GPR00F+LENGTH(ISTSMHDR);/* ADJUST LENGTH      0119 00076000
*                                                            @G35RKD9*/ 00077000
         ALR   GPR00F,@08                                          0119 00078000
*             SRL(SPPARM,8);        /* GET IN LOW ORDER BYTE @G35RKD9*/ 00079000
         SRL   SPPARM,8                                            0120 00080000
*             IF(SPPARM=SMHSBPL)&   /* SUBPOOL MATCHES       @G35RKD9*/ 00081000
*                 (GPR00F=SMHLONG) THEN/* LENGTH MATCHES     @G35RKD9*/ 00082000
         SLR   @08,@08                                             0121 00083000
         IC    @08,SMHSBPL(,FREEPTR)                               0121 00084000
         CR    SPPARM,@08                                          0121 00085000
         BNE   @RF00121                                            0121 00086000
         C     GPR00F,SMHLONG(,FREEPTR)                            0121 00087000
         BNE   @RF00121                                            0121 00088000
*               DO;                 /* VALID CALL- CHARACTERISTICS 0122 00089000
*                                      MATCH                 @G35RKD9*/ 00090000
*                 IF(GPR03P&CLEARMSK)^=0 THEN/* CLEAR REQUESTED    0123 00091000
*                                                            @G35RKD9*/ 00092000
