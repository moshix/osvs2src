*                   GPR03F=GPR00F;  /* LENGTH TO CLEAR               */ 00389000
         LR    GPR03F,GPR00F                                       0213 00390000
*                   GPR14P=GPR02P;  /* FROM ADDRESS                  */ 00391000
         LR    GPR14P,GPR02P                                       0214 00392000
*                   GPR15F=0;       /* FROM LENGTH                   */ 00393000
         SLR   GPR15F,GPR15F                                       0215 00394000
*                   GEN CODE REFS(GPR02P,GPR03F,GPR14P) SETS(GPR02P,    00395000
