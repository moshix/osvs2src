*                                                                  0170 00239000
         LR    GPR09F,GPR13F                                       0170 00240000
*/*                   SET STORAGE PROTECT KEY TO 0                   */ 00241000
*                                                                  0171 00242000
*             GEN CODE SETS(GPR02F,GPR06F) REFS(ZERO);             0171 00243000
         MODESET KEYADDR=ZERO,SAVEKEY=(2),WORKREG=6                     00244000
*             GPR06F=GPR02F;        /* SAVE PRIOR PROTECT KEY        */ 00245000
         LR    GPR06F,GPR02F                                       0172 00246000
*             RFY                                                  0173 00247000
*               GPR02F UNRSTD;                                     0173 00248000
*             RFY                                                  0174 00249000
*               GPR14F RSTD;                                       0174 00250000
*SETL1:                                                            0175 00251000
*             GEN CODE SETS(GPR11F,GPR12F,GPR13F,GPR14F) REFS(PSALITA,  00252000
*                 FLC);                                            0175 00253000
SETL1    DS    0H                                                  0175 00254000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        X00255000
               RELATED=(IEAVGM00,ISTORFSR(RELL1))                       00256000
*             RFY                                                  0176 00257000
*              (GPR02F,                                            0176 00258000
*               GPR14F,                                            0176 00259000
*               GPR15F) UNRSTD;                                    0176 00260000
*             RFY                                                  0177 00261000
*               GPR10F RSTD;                                       0177 00262000
*             IF GPR13F=4 THEN      /* DID THIS TASK ALREADY HAVE THE   00263000
*                                      LOCAL MEMORY LOCK             */ 00264000
         C     GPR13F,@CF00048                                     0178 00265000
         BNE   @RF00178                                            0178 00266000
*HELD:                                                             0179 00267000
*               GPR03F=GPR03F|LOCKMASK;/* YES - THEREFORE WE MUST NOT   00268000
*                                      RELEASE IT SO SET IND         */ 00269000
HELD     O     GPR03F,LOCKMASK                                     0179 00270000
*             ELSE                                                 0180 00271000
*               ;                   /* CALLER DID NOT HAVE LOCK      */ 00272000
@RF00178 DS    0H                                                  0181 00273000
*FSTOR1:                                                           0181 00274000
*             DO;                   /* CONTINUE FREESTOR PROCESS     */ 00275000
FSTOR1   DS    0H                                                  0182 00276000
*               GPR11F=GPR07F;      /* RESTORE REGS AFTER SETLOCK    */ 00277000
         LR    GPR11F,GPR07F                                       0182 00278000
*               GPR12F=GPR08F;                                     0183 00279000
         LR    GPR12F,GPR08F                                       0183 00280000
*               GPR13F=GPR09F;                                     0184 00281000
         LR    GPR13F,GPR09F                                       0184 00282000
*               GPR09P=GPR01P;      /* PRESERVE RPH POINTER          */ 00283000
         LR    GPR09P,GPR01P                                       0185 00284000
*               IF(GPR03F&FIXMASK)=0 THEN/* IS STORAGE PAGE- FIXED   */ 00285000
         LR    @15,GPR03F                                          0186 00286000
         N     @15,FIXMASK                                         0186 00287000
         LTR   @15,@15                                             0186 00288000
         BNZ   @RF00186                                            0186 00289000
*FST1:                                                             0187 00290000
*                 DO;               /* NO - ADJUST REGS FOR CLEAR AND   00291000
*                                      FREEING STORAGE               */ 00292000
FST1     DS    0H                                                  0188 00293000
*                   GPR01P=GPR04P;  /* SET UP START ADDRESS OF AREA     00294000
*                                      TO FREE                       */ 00295000
         LR    GPR01P,GPR04P                                       0188 00296000
*                   GPR08F=GPR03F;  /* SAVE INPUT PARM REG AROUND  0189 00297000
*                                      CLEAR AND FREEMAIN            */ 00298000
         LR    GPR08F,GPR03F                                       0189 00299000
*                 END FST1;         /* END PAGEABLE STOR PROC        */ 00300000
*               ELSE                /* STORAGE IS FIXED              */ 00301000
*PGFX:                                                             0191 00302000
*                 DO;               /* STORAGE MUST BE PGFREED       */ 00303000
         B     @RC00186                                            0191 00304000
@RF00186 DS    0H                                                  0191 00305000
PGFX     DS    0H                                                  0192 00306000
*PFREE:                                                            0192 00307000
*                   GPR07P=GPR04P;  /* SAVE STORAGE ADDRESS          */ 00308000
PFREE    LR    GPR07P,GPR04P                                       0192 00309000
*                   GPR08F=GPR00F;  /* SAVE LENGTH TO FREE           */ 00310000
         LR    GPR08F,GPR00F                                       0193 00311000
*                   GPR01P=GPR04P;  /* START ADDR TO PGREEE          */ 00312000
         LR    GPR01P,GPR04P                                       0194 00313000
*                   RFY                                            0195 00314000
*                    (GPR02P,                                      0195 00315000
*                     GPR00P) RSTD; /*                       @Z30APGE*/ 00316000
*                   GPR00P=GPR01P;  /* GET ADDR OF ECB FOR PGFREE  0196 00317000
*                                                            @Z30APGE*/ 00318000
         LR    GPR00P,GPR01P                                       0196 00319000
*                   GPR02P=GPR01P+GPR08F;/* END ADDR TO PGFREE     0197 00320000
*                                                            @Z30APGE*/ 00321000
         LR    GPR02P,GPR01P                                       0197 00322000
         ALR   GPR02P,GPR08F                                       0197 00323000
*                   GEN CODE SETS(GPR01P) REFS(GPR01P,PAGEFREE)    0198 00324000
*                       (ICM GPR01P,8,PAGEFREE);                   0198 00325000
         ICM GPR01P,8,PAGEFREE                                          00326000
*                   RFY                                            0199 00327000
*                     GPR15F RSTD;                                 0199 00328000
*                   GPR15P=FLCCVT;                                 0200 00329000
         L     GPR15P,FLCCVT                                       0200 00330000
*                   GEN CODE REFS(GPR04P,GPR14P,GPR15P,CVTVPSIB) SETS(  00331000
*                       GPR04P,GPR14P,GPR15P);                     0201 00332000
         L     GPR15P,CVTVPSIB(GPR15P)   GET ADDR OF PGFIX/PGFREE       00333000
         SR    GPR04P,GPR04P             CLEAR TCB POINTER              00334000
         BALR  GPR14P,GPR15P             CALL PGFREE                    00335000
*                   RFY                                            0202 00336000
*                    (GPR00P,                                      0202 00337000
*                     GPR02P) UNRSTD;/*                      @Z30APGE*/ 00338000
*                   RFY                                            0203 00339000
*                     GPR15F UNRSTD;                               0203 00340000
*                   GPR00F=GPR08F;  /* RESTORE LENGTH TO FREE        */ 00341000
         LR    GPR00F,GPR08F                                       0204 00342000
*                   GPR01P=GPR07P;  /* RESTORE ADDR TO FREE          */ 00343000
         LR    GPR01P,GPR07P                                       0205 00344000
*                   GPR08F=GPR03F;  /* PRESERVE FLAGS ACROSS FREEMAIN*/ 00345000
         LR    GPR08F,GPR03F                                       0206 00346000
*                 END PGFX;         /* STORAGE HAS BEEN PGFREED      */ 00347000
*/********************************************************************/ 00348000
*/*                                                                  */ 00349000
*/* REGISTER STATUS REPORT                                           */ 00350000
*/*                                                                  */ 00351000
*/*                0 = LENGTH TO BE FREED                            */ 00352000
*/*                1 = ADDRESS TO FREE                               */ 00353000
*/*                2 = WORKREG                                       */ 00354000
*/*                3 = INPUT PARAMETERS (FLAGS KEY SUBPOOL OP)       */ 00355000
*/*                4 = WORKREG AVAIL                                 */ 00356000
*/*                5 = BASE REG                                      */ 00357000
*/*                6 = ORIGINAL PROTECT KEY                          */ 00358000
*/*                7 = WORKREG AVAILABLE                             */ 00359000
*/*                8 = COPY OF INPUT PARAMETERS                      */ 00360000
*/*                9 = ADDRESS OF RPH                                */ 00361000
*/*               10 = RETURN ADDRESS                                */ 00362000
*/*               11 = CALLER REG                                    */ 00363000
*/*               12 = CALLER REG                                    */ 00364000
*/*               13 = CALLER REG                                    */ 00365000
*/*               14 = WORKREG (UNRSTD)                              */ 00366000
*/*               15 = WORKREG (UNRSTD)                              */ 00367000
*/*                                                                  */ 00368000
*/********************************************************************/ 00369000
*                                                                  0208 00370000
*CL:                                                               0208 00371000
*               IF(GPR03F&CLEARMSK)^=0 THEN                        0208 00372000
@RC00186 DS    0H                                                  0208 00373000
CL       LR    @15,GPR03F                                          0208 00374000
         N     @15,CLEARMSK                                        0208 00375000
         LTR   @15,@15                                             0208 00376000
         BZ    @RF00208                                            0208 00377000
*CLR:                                                              0209 00378000
*                 DO;               /* CLEAR TO 0 BEFORE FREEING     */ 00379000
CLR      DS    0H                                                  0210 00380000
*                   RFY                                            0210 00381000
*                     GPR04F UNRSTD;                               0210 00382000
*                   RFY                                            0211 00383000
*                    (GPR14F,                                      0211 00384000
*                     GPR15F,                                      0211 00385000
*                     GPR02P) RSTD; /*                       @G35RKD9*/ 00386000
