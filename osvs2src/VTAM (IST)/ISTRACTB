*              TRAANODF='1'B;               /*      APPL NAME FLAG   */ 00226000
         OI    TRAANODF(TRACPTR),B'00000010'                       0521 00227000
*              TRADATCT=LENGTH(TRAANODE);   /*      BUFFER LENGTH    */ 00228000
         STH   @04,TRADATCT(,TRACPTR)                              0522 00229000
*              TRACXTF='1'B;                /*      CONFIDENTIAL DATA*/ 00230000
         OI    TRACXTF(TRACPTR),B'00100000'                        0523 00231000
*             END;                          /* NO-SAVE CID/FMCB IN */   00232000
*            ELSE                           /*      TRAC FOR LATER   */ 00233000
*              TRACID=R5;                   /*      TO FIND DNCB    */  00234000
         B     @RC00512                                            0525 00235000
@RF00512 ST    R5,TRACID(,TRACPTR)                                 0525 00236000
*          TRABF='1'B;                      /*    BUFFER TRACE       */ 00237000
@RC00512 DS    0H                                                  0527 00238000
*          TRAFRST='1'B;                    /*    FIRST BUFFER       */ 00239000
         OI    TRABF(TRACPTR),B'01001000'                          0527 00240000
*          TRATYPE=TRTYPE;                  /*    C/B TYPE = TRAC    */ 00241000
         MVI   TRATYPE(TRACPTR),X'13'                              0528 00242000
*          TRALNG=LENGTH(ISTTRAC);          /*    LENGTH             */ 00243000
         MVI   TRALNG(TRACPTR),X'20'                               0529 00244000
*                                           /*    SET I/O DIRECTION  */ 00245000
*          IF (R6INPUT&INPTMSK)=FDBINPUT    /*    BASED ON R6 INPUT: */ 00246000
*            THEN                           /*     TPIOS FDB CALL    */ 00247000
         LR    @04,R6INPUT                                         0530 00248000
         N     @04,INPTMSK                                         0530 00249000
         CL    @04,FDBINPUT                                        0530 00250000
         BNE   @RF00530                                            0530 00251000
*              TRADIO='1'B;                 /*     IS ALWAYS INBOUND */ 00252000
         OI    TRADIO(TRACPTR),B'00001000'                         0531 00253000
*            ELSE DO;                       /*    CONTROL LAYER INPUT*/ 00254000
         B     @RC00530                                            0532 00255000
@RF00530 DS    0H                                                  0533 00256000
*              IF (R6INPUT&INPTMSK)=ZERO    /*     OUTBOUND?         */ 00257000
*                THEN;                      /*      YES-FLG=0(OUTBND)*/ 00258000
         LR    @04,R6INPUT                                         0533 00259000
         N     @04,INPTMSK                                         0533 00260000
         LTR   @04,@04                                             0533 00261000
         BZ    @RT00533                                            0533 00262000
*                ELSE                       /*      NO-FLG=1(INBOUND)*/ 00263000
*                  TRADIO='1'B;                                    0535 00264000
         OI    TRADIO(TRACPTR),B'00001000'                         0535 00265000
*             END;                                                 0536 00266000
*         END;                                                     0537 00267000
*        ELSE                               /*   REQSTORE FAILURE-   */ 00268000
*          NCBLTRC=NCBLTRC+1;               /*   INCR. LOST RCD CNT  */ 00269000
         B     @RC00508                                            0538 00270000
@RF00508 LA    @04,1                                               0538 00271000
         AH    @04,NCBLTRC(,NCBPTR)                                0538 00272000
         STH   @04,NCBLTRC(,NCBPTR)                                0538 00273000
*      IF (R6INPUT&INPTMSK) = FDBINPUT                             0539 00274000
*        THEN DO;                           /* INPUT IS A FDB */   0539 00275000
@RC00508 LR    @04,R6INPUT                                         0539 00276000
         N     @04,INPTMSK                                         0539 00277000
         CL    @04,FDBINPUT                                        0539 00278000
         BNE   @RF00539                                            0539 00279000
*      R2=CIDSAVR5;                         /*  GET CID              */ 00280000
         LR    R2,CIDSAVR5                                         0541 00281000
*      RFY SAVRCR5 RSTD;                    /*  SAVE REQSTORE RTNCODE*/ 00282000
*      SAVRCR5=RTNCODE;                     /*  ACROSS CIDCTL.       */ 00283000
*                                                                  0543 00284000
         LR    SAVRCR5,RTNCODE                                     0543 00285000
*       DO; /* CIDCTL(FINISH)  */                                  0544 00286000
*                                                                  0544 00287000
*       RFY (EMCR2,EMCR3) RSTD;                                    0545 00288000
*         GEN SETS(RPHWORK)                                        0546 00289000
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0546 00290000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00291000
*         EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                      0547 00292000
         SLR   EMCR3,EMCR3                                         0547 00293000
*         CALL ATCLCPTR->ATCCDFIN->ISTSDCCR; /*PROCESS REQUEST*/   0548 00294000
