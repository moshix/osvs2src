         TITLE 'ISTRACTB---BUFFER TRACE                                *00001000
                        '                                               00002000
ISTRACTB CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(8)                                              0001 00007000
         DC    C'ISTRACTB'                                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*/********************************************************************/ 00013000
*/*                                                                  */ 00014000
*/*  ROUTINE NAME : TBMAINLN - ISTRACTB MAIN LINE PROCEDURE          */ 00015000
*/*                                                                  */ 00016000
*/*  FUNCTION : TO PROVIDE MAIN LINE CONTROL FOR BUFFER TRACE. CALLS */ 00017000
*/*             A ROUTINE TO BUILD A TRACE RECORD (ISTTRAC) AND SETUP*/ 00018000
*/*             THE TRAC'S HEADER. IF THE DATA IS CONFIDENTIAL THE   */ 00019000
*/*             RECORD IS WRITTEN WITHOUT FILLING IN THE BUFFER.     */ 00020000
*/*             OTHERWISE, ROUTINES ARE INVOKED TO FILL THE TRAC     */ 00021000
*/*             BUFFER WITH CONTROL LAYER OR TPIOS DATA (TBSRPL OR   */ 00022000
*/*             TBSFDB RESPECTIVELY). AFTER THE BUFFER IS FILLED THE */ 00023000
*/*             HEADER INFORMATION IS COMPLETED AND THE TRACE RECORD */ 00024000
*/*             IS WRITTEN. THE WRITTING OF THE RECORD DEPENDS ON THE*/ 00025000
*/*             SYSTEM. IN DOS, RECORDING IS ACCOMPLISHED BY TPQUEING*/ 00026000
*/*             THE TRACE RECORD TO THE TRACE WRITER PAB. IN VS1 AND */ 00027000
*/*             VS2 THE GTRACE MACRO IS ISSUED, GTF TRACE COPIES THE */ 00028000
*/*             TRACE RECORD AND RETURNS CONTROL.                    */ 00029000
*/*                                                                  */ 00030000
*/*  INPUT : R6-BYTE 0 = X'00' - RPL OUTBOUND                        */ 00031000
*/*                      X'04' - RPL INBOUND                         */ 00032000
*/*                      X'20' - FDB INPUT                           */ 00033000
*/*             BYTES1-3 = ADDRESS OF RPL OR FDB DEPENDING ON BYTE 0 */ 00034000
*/*                                                                  */ 00035000
*/*  OUTPUT : NONE                                                   */ 00036000
*/*                                                                  */ 00037000
*/*  ROUTINES : TBTRAC - GETS CORE FOR TRACE RECORD. SETS UP HEADER. */ 00038000
*/*                      IF CONFIDENTIAL DATA, COMPLETES HEADER SO   */ 00039000
*/*                      THE RECORD IS READY TO WRITE.               */ 00040000
*/*             TBSFDB - CALLED TO HANDLE TPIOS INBOUND NON-         */ 00041000
*/*                      CONFIDENTIAL DATA. FILLS THE TRAC BUFFER    */ 00042000
*/*                      WITH FDB DATA                               */ 00043000
*/*             TBSRPL - CALLED TO HANDLE CONTROL/LAYER NON-         */ 00044000
*/*                      CONFIDENTIAL DATA. CALLS ROUTINES THAT FILL */ 00045000
*/*                      THE TRAC BUFFER WITH RPL DATA.              */ 00046000
*/*                                                                  */ 00047000
*/*  MACROS : VERSION - TO PROVIDE DIFFERENT PROCESSING DEPENDING ON */ 00048000
*/*                     THE SYSTEM.                                  */ 00049000
*/*           CIDCTL (FIND) - TO OBTAIN THE DAFLOCK AND NCB ADDRESS. */ 00050000
*/*           CIDCTL (FINISH) - TO RELEASE THE DAFLOCK.              */ 00051000
*/*           RELSTORE - TO RELEASE THE TRAC WHEN AN ERROR OCCURS. IN*/ 00052000
*/*                     VS1 AND VS2 TO RELEASE THE TRAC AFTER GTRACE */ 00053000
*/*                     HAS COPIED IT.                               */ 00054000
*/*           TPQUE - USED IN DOS TO QUEUE THE TRACE RECORD TO THE   */ 00055000
*/*                   TRACE WRITER (ISTRAETW) PAB FOR RECORDING.     */ 00056000
*/*           GTRACE - USED IN VS1 AND VS2 TO RECORD TRACE RECORDS.  */ 00057000
*/*                                                                  */ 00058000
*/********************************************************************/ 00059000
*TBMAINLN:                                                         0456 00060000
* R14SAVE=R14;                                    /*SAVE REG14       */ 00061000
TBMAINLN LR    R14SAVE,R14                                         0456 00062000
*  IF (SYSCVTPT->CVTGTFAV) = '0'B     /* IS GTRACE ACTIVE    @VS41640*/ 00063000
*    THEN;                                        /*  NO-EXIT        */ 00064000
         L     @11,SYSCVTPT                                        0457 00065000
         TM    CVTGTFAV(@11),B'10000000'                           0457 00066000
         BZ    @RT00457                                            0457 00067000
*    ELSE DO;                                     /*  YES-PROCEED    */ 00068000
*/********************************************************************/ 00069000
*/*                                                                  */ 00070000
*/*  ROUTINE NAME : TBTRAC - GET TRAC ROUTINE                        */ 00071000
*/*                                                                  */ 00072000
*/*  FUNCTION : TO OBTAIN CORE FOR THE TRACE RECORD. INITIALIZE THE  */ 00073000
*/*             HEADER. IF CONFIDENTIAL DATA IS INDICATED IN THE FMCB*/ 00074000
*/*             THE ENTIRE HEADER IS FILLED IN. IN THE CASE OF NORMAL*/ 00075000
*/*             DATA,THE LOST RECORD CNT AND THE DESTINATION NODE    */ 00076000
*/*             NAME AND THE APPLICATION NAME ARE NOT SET, RATHER    */ 00077000
*/*             THE CID IS STORED IN THE HEADER FOR LATER USE BY     */ 00078000
*/*             TBMAINLN.                                            */ 00079000
*/*                                                                  */ 00080000
*/*  INPUT : R6 - INPUT PARAMETERS AS DEFINED IN PROLOGUE.           */ 00081000
*/*                                                                  */ 00082000
*/*  OUTPUT : RTNCODE = 0 - TRACE RECORD BUILT SUCCESSFULLY.         */ 00083000
*/*                  ^= 0 - TRACE RECORD NOT BUILT BECAUSE THE CID   */ 00084000
*/*                         WAS INVALID OR BECAUSE NO STORAGE WAS    */ 00085000
*/*                         AVAILABLE.                               */ 00086000
*/*                                                                  */ 00087000
*/*  ROUTINES : NONE                                                 */ 00088000
*/*                                                                  */ 00089000
*/*  MACROS : CIDCTL - (FIND & FINISH) TO OBTAIN THE FMCB & DNCB     */ 00090000
*/*                    ADDRESSES,AND TO LOCK AND UNLOCK THE          */ 00091000
*/*                    DAFLOCK.                                      */ 00092000
*/*           REQSTORE - CALLED TO OBTAIN STORAGE FOR THE TRACE      */ 00093000
*/*                      RECORD                                      */ 00094000
*/*                                                                  */ 00095000
*/********************************************************************/ 00096000
*  RFY (R2,FMCBR2,R14) RSTD;                                       0460 00097000
*  RTNCODE = 0;                             /* INIT RETRN CODE */  0461 00098000
         SLR   RTNCODE,RTNCODE                                     0461 00099000
*  IF (R6INPUT&INPTMSK)=FDBINPUT            /*FDB INPUT?             */ 00100000
*    THEN DO;                               /* YES-OBTAIN CID FROM   */ 00101000
         LR    @11,R6INPUT                                         0462 00102000
         N     @11,INPTMSK                                         0462 00103000
         CL    @11,FDBINPUT                                        0462 00104000
         BNE   @RF00462                                            0462 00105000
*      RFY ISTFDB BASED(R6INPUT);           /*  FSB(R6-> TO 1ST FDB) */ 00106000
*      R14=R6INPUT                          /*    (ISTFSB PRECEDES   */ 00107000
*          -LENGTH(ISTFSB)                  /*    1ST FDB,FSB LENGTH */ 00108000
*          +LENGTH(ISTFDB);                 /*    INCLUDES 1ST FDB)  */ 00109000
         LR    R14,R6INPUT                                         0465 00110000
         AL    R14,@CF04445                                        0465 00111000
*      R2=FSBCID;                                                  0466 00112000
         L     R2,FSBCID(,R14)                                     0466 00113000
*  RFY CIDSAVR5 RSTD;                       /*SAVE CID TO DO CID     */ 00114000
*  CIDSAVR5=R2;                             /*FINISH.                */ 00115000
*                                                                  0468 00116000
         LR    CIDSAVR5,R2                                         0468 00117000
*   DO; /* CIDCTL(FIND) LOCK(XCLUSIVE) */                          0469 00118000
*                                                                  0469 00119000
*   RFY (EMCR2,EMCR3) RSTD;                                        0470 00120000
*     GEN SETS(RPHWORK)                                            0471 00121000
*        (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);      0471 00122000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00123000
*     EMCR3 = '00'X; /*CLEAR EXTRA BITS*/                          0472 00124000
         SLR   EMCR3,EMCR3                                         0472 00125000
*     EMCR3 = '02'X; /*INDICATE EXCLUSIVE LOCK*/                   0473 00126000
         LA    EMCR3,2                                             0473 00127000
*     CALL ATCLCPTR->ATCCDFND->ISTSDCCF; /*PROCESS REQUEST*/       0474 00128000
*                                                                  0474 00129000
         L     @11,ATCLCPTR                                        0474 00130000
         L     @15,ATCCDFND(,@11)                                  0474 00131000
         BALR  @14,@15                                             0474 00132000
*   RFY (EMCR2,EMCR3) UNRSTD;                                      0475 00133000
*   END; /*CIDCTL*/           /*GET FMCB&NCB ADDRS &   */          0476 00134000
*     END;                                                         0477 00135000
*    ELSE DO;                            /*  NO,LOAD DNCB @ FROM FMCB * 00136000
         B     @RC00462                                            0478 00137000
@RF00462 DS    0H                                                  0479 00138000
*      RFY (R0,FMCBSVR5) RSTD;                                     0479 00139000
*      R0 = FMCDNCBA;                    /* GET DNCB PTR */        0480 00140000
         L     R0,FMCDNCBA(,FMCBR2)                                0480 00141000
*      FMCBSVR5 = FMCBR2;                /* SAVE FMCB PTR */       0481 00142000
         LR    FMCBSVR5,FMCBR2                                     0481 00143000
*     END;                                                         0482 00144000
*  RFY NCBPTR RSTD;                                                0483 00145000
@RC00462 DS    0H                                                  0484 00146000
*  IF RTNCODE=ZERO                          /* DAF LOCK OBTAINED */     00147000
*    THEN DO;                               /* YES-GET CORE FOR TRAC */ 00148000
         LTR   RTNCODE,RTNCODE                                     0484 00149000
         BNZ   @RF00484                                            0484 00150000
*      NCBPTR=R0;                                                  0486 00151000
         LR    NCBPTR,R0                                           0486 00152000
*      RFY R0 UNRSTD;                                              0487 00153000
*       /*REQSTORE CBID(FMCB)CLEAR(YES)SAVE(YES)*/                 0488 00154000
*        DO;                                                       0488 00155000
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)  */                         0489 00156000
*/* END OF ATBUILD */                                              0489 00157000
*                                                                  0489 00158000
*       RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;                    0489 00159000
*                                                                  0489 00160000
*       RFY (GPR01P,GPR13P) RSTD;                                  0490 00161000
*          GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);      0491 00162000
*                                                                  0491 00163000
           STM GPR02P,GPR13P,RPHWORK(GPR01P)                            00164000
*       RFY (GPR01P,GPR13P) UNRSTD;                                0492 00165000
*                                                                  0492 00166000
*       DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */              0493 00167000
*         GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/              0494 00168000
         L     GPR15P,ATCLCPTR                                     0494 00169000
*       END; /*LISATCVT*/                                          0495 00170000
*          GPR02P=GPR15P->ATCBPDA | '80000000'X;                   0496 00171000
         L     GPR02P,ATCBPDA(,GPR15P)                             0496 00172000
         O     GPR02P,@CF01359                                     0496 00173000
*          DCL 1 FMZA0010 FIXED(31) STATIC LOCAL,                  0497 00174000
*              2 *        FIXED(8) INIT(152),                      0497 00175000
*              2 *        PTR(24) INIT(0);                         0497 00176000
*          GPR03F=FMZA0010;                                        0498 00177000
         L     GPR03F,FMZA0010                                     0498 00178000
*          GPR04F=25;                                              0499 00179000
         LA    GPR04F,25                                           0499 00180000
*          GPR15P=GPR15P->ATCSMRQ;                                 0500 00181000
         L     GPR15P,ATCSMRQ(,GPR15P)                             0500 00182000
*          DCL FMZL0010 BASED(GPR15P) LABEL VALUERANGE(*);         0501 00183000
*          CALL FMZL0010;                                          0502 00184000
*                                                                  0502 00185000
         BALR  @14,@15                                             0502 00186000
*       RFY (GPR13P) RSTD;                                         0503 00187000
*          GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);      0504 00188000
*                                                                  0504 00189000
           LM GPR02P,GPR13P,RPHWORK(GPR01P)                             00190000
*       RFY (GPR13P) UNRSTD;                                       0505 00191000
*                                                                  0505 00192000
*       RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;                  0506 00193000
*       END ;                                                      0507 00194000
*          /********* END OF REQSTORE EXPANSION **********/        0508 00195000
*      IF RTNCODE<EIGHT                     /*  CORE AVAILABLE?      */ 00196000
*        THEN DO;                           /*   YES-SETUP HEADER    */ 00197000
         LA    @04,8                                               0508 00198000
         CR    RTNCODE,@04                                         0508 00199000
         BNL   @RF00508                                            0508 00200000
*          RTNCODE=ZERO;                    /*    SET GOOD RTN CODE  */ 00201000
         SLR   @03,@03                                             0510 00202000
         LR    RTNCODE,@03                                         0510 00203000
*          TRACPTR=RPHSRPRM;                /*    SETUP TRAC BASE    */ 00204000
         L     TRACPTR,RPHSRPRM(,RPHPTR)                           0511 00205000
*          IF PROCFTX='1'B                  /*    CONFIDENTIAL DATA? */ 00206000
*            THEN DO;                       /*     YES-SETUP ENTIRE  */ 00207000
         TM    PROCFTX+48(FMCBR2),B'00000001'                      0512 00208000
         BNO   @RF00512                                            0512 00209000
*              RFY ISTRPRE BASED(NCBRDTE);  /*      TRAC HEADER.     */ 00210000
*              TRALRC=NCBLTRC;              /*      LOST RECORD CNT  */ 00211000
         LH    @00,NCBLTRC(,NCBPTR)                                0515 00212000
         STH   @00,TRALRC(,TRACPTR)                                0515 00213000
*              NCBLTRC=ZERO;                /*      REINITIALIZE     */ 00214000
