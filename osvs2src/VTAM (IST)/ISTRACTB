*/********************************************************************/ 00326000
*/*                                                                  */ 00327000
*/*  ROUTINE NAME : TBSFDB - TPIOS FDB INPUT PROCESSOR (DOS ONLY)    */ 00328000
*/*                                                                  */ 00329000
*/*  FUNCTION : TO PROCESS TPIOS INBOUND INPUT. MOVES DATA FROM      */ 00330000
*/*             FDBS UNTIL THE TRAC BUFFER IS FILLED OR UNTIL ALL    */ 00331000
*/*             FDB DATA IS MOVED, WHICHEVER OCCURS FIRST.           */ 00332000
*/*                                                                  */ 00333000
*/*  INPUT : R6INPUT-ADDRESS OF FIRST FDB. FDBS ARE CHAINED, AND THE */ 00334000
*/*                  END OF THE CHAIN IS ZERO.                       */ 00335000
*/*                                                                  */ 00336000
*/*  OUTPUT : A COMPLETED TRACE BUFFER.                              */ 00337000
*/*           TRADATCT=LENGTH OF DATA IN THE BUFFER.                 */ 00338000
*/*                                                                  */ 00339000
*/*  ROUTINES : NONE                                                 */ 00340000
*/*                                                                  */ 00341000
*/*  MACROS : NONE                                                   */ 00342000
*/*                                                                  */ 00343000
*/********************************************************************/ 00344000
*  RFY (BUFNX,LENR4) RSTD;                                         0563 00345000
*  RFY FDBR2 RSTD;                      /*SETUP FDB BASE-FIRST FDB   */ 00346000
*  RFY ISTFDB BASED(FDBR2);             /*ADDRESS IS AN INPUT        */ 00347000
*  FDBR2=R6INPUT;                       /*PARAMETER.                 */ 00348000
         LR    FDBR2,R6INPUT                                       0566 00349000
*  DO WHILE (TRADATCT<BUFCT) & (FDBR2^=ZERO); /* FILL BUFFER UNTIL   */ 00350000
         B     @DE00567                                            0567 00351000
@DL00567 DS    0H                                                  0568 00352000
*                                       /*FULL OR NO MORE INPUT      */ 00353000
*    IF FDBDATCT>(BUFCT-TRADATCT)       /*DATA TO MOVE>BUFFER LEFT?  */ 00354000
*      THEN                             /* YES-SET LENGTH TO MOVE =  */ 00355000
         L     @11,BUFCT                                           0568 00356000
         SH    @11,TRADATCT(,TRACPTR)                              0568 00357000
         CH    @11,FDBDATCT(,FDBR2)                                0568 00358000
         BNL   @RF00568                                            0568 00359000
*        LENR4=(BUFCT-TRADATCT);        /*  SIZE OF REMAINING BUFFER */ 00360000
         LR    LENR4,@11                                           0569 00361000
*      ELSE                             /* NO-SET LENGTH TO MOVE = TO*/ 00362000
*        LENR4=FDBDATCT;                /*  DATA SIZE IN FDB         */ 00363000
         B     @RC00568                                            0570 00364000
@RF00568 LH    LENR4,FDBDATCT(,FDBR2)                              0570 00365000
*    BUFNX=ADDR(TRABFFER)+TRADATCT;     /*ADDRESS TO MOVE TO IN TRAC */ 00366000
@RC00568 LA    BUFNX,TRABFFER(,TRACPTR)                            0571 00367000
         LH    @11,TRADATCT(,TRACPTR)                              0571 00368000
         ALR   BUFNX,@11                                           0571 00369000
*    BUFNX->BUFDATA(1:LENR4)=FDBDATAA->BUFDATA(1:LENR4);  /*MOVE DATA*/ 00370000
         LR    @03,LENR4                                           0572 00371000
         BCTR  @03,0                                               0572 00372000
         L     @09,FDBDATAA(,FDBR2)                                0572 00373000
         EX    @03,@SM04453                                        0572 00374000
*    FDBR2=FDBFDBA;                     /*GET ADDR OF NEXT FDB       */ 00375000
