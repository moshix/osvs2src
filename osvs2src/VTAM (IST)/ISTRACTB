*              RFY ISTRPRE BASED(ACDRDTE);  /*      FMCB->ACD->RDTE  */ 00221000
*              TRAANODE=RPRNAME;            /*      APPLICATION NAME */ 00222000
         L     @03,FMCDEBA(,FMCBR2)                                0520 00223000
         L     @03,ACDRDTE(,@03)                                   0520 00224000
