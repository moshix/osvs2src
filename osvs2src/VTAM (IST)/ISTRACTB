*       RFY (EMCR2,EMCR3) UNRSTD;                                  0549 00299000
*       END; /*CIDCTL*/                    /*  UNLOCK THE DAFLOCK.  */  00300000
*      RTNCODE=SAVRCR5;                     /*  RESTORE RETURN CODE  */ 00301000
         LR    RTNCODE,SAVRCR5                                     0551 00302000
*             END;                                                 0552 00303000
*        ELSE;                            /* DAF LOCK WAS NOT HELD */   00304000
