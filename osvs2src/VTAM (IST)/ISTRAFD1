*     ;                             /* NO-INDICATE NO DEB            */ 00225000
*   ELSE                                                           0643 00226000
*     DO;                           /* YES-THERE IS AN MPST          */ 00227000
*       RFY                                                        0644 00228000
*         ISTMPST BASED(ASXBMPST);                                 0644 00229000
*       DCEPTR=MPSDBTBL;            /* ADDRESS DCE FOR THIS MPST     */ 00230000
         L     @09,PSAAOLD                                         0645 00231000
