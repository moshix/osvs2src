         L     R15,PLSTMARA(,R1)                                   1556 02890000
*               CALL MAR;           /* GET DUMPED ADDRESS            */ 02891000
         BALR  @14,@15                                             1557 02892000
*               IF R15^=0 THEN      /* DID MAR ENCOUNTER AN ERROR    */ 02893000
         LTR   R15,R15                                             1558 02894000
         BNZ   @RT01558                                            1558 02895000
*                 GOTO QEND2;       /* YES - GO TO ERROR ROUTINE     */ 02896000
*               ELSE                                               1560 02897000
*                 DEBPTR=WORD;      /* NO-PICK UP FIELD              */ 02898000
         LR    @09,R0                                              1560 02899000
         L     DEBPTR,WORD(,@09)                                   1560 02900000
*             END;                  /* GET DEB POINTER FROM DCE      */ 02901000
*             DO;                   /* GETLOC LOC(ACDTSKID)ERRTN(QEND   02902000
*                                      3)RET(PSTPTR,WORD)            */ 02903000
*               R0=ADDR(ACDTSKID);  /* PICK UP @ OF AREA FOR MAR     */ 02904000
         LA    R0,ACDTSKID(,DEBPTR)                                1563 02905000
*               R15=PLSTMARA;       /* GET MAR ADDRESS               */ 02906000
         L     R15,PLSTMARA(,R1)                                   1564 02907000
*               CALL MAR;           /* GET DUMPED ADDRESS            */ 02908000
         BALR  @14,@15                                             1565 02909000
*               IF R15^=0 THEN      /* DID MAR ENCOUNTER AN ERROR    */ 02910000
         SLR   @09,@09                                             1566 02911000
         CR    R15,@09                                             1566 02912000
         BNE   @RT01566                                            1566 02913000
*                 GOTO QEND3;       /* YES - GO TO ERROR ROUTINE     */ 02914000
*               ELSE                                               1568 02915000
*                 PSTPTR=WORD;      /* NO-PICK UP FIELD              */ 02916000
         LR    @08,R0                                              1568 02917000
         L     PSTPTR,WORD(,@08)                                   1568 02918000
*             END;                  /* GET PST PTR FROM DEB          */ 02919000
*             IF PSTPTR=0 THEN                                     1570 02920000
         CR    PSTPTR,@09                                          1570 02921000
         BE    @RT01570                                            1570 02922000
*               ;                   /* NO PST - GO TO NEXT DEB       */ 02923000
*             ELSE                                                 1572 02924000
*               DO;                                                1572 02925000
*                 DO;               /* GETLOC LOC(PSTCRRS)ERRTN(QEND3   02926000
*                                      )RET(SMSCRA,WORD)             */ 02927000
*                   R0=ADDR(PSTCRRS);/* PICK UP @ OF AREA FOR MAR    */ 02928000
         LA    R0,PSTCRRS(,PSTPTR)                                 1574 02929000
*                   R15=PLSTMARA;   /* GET MAR ADDRESS               */ 02930000
         L     R15,PLSTMARA(,R1)                                   1575 02931000
*                   CALL MAR;       /* GET DUMPED ADDRESS            */ 02932000
         BALR  @14,@15                                             1576 02933000
*                   IF R15^=0 THEN  /* DID MAR ENCOUNTER AN ERROR    */ 02934000
         LTR   R15,R15                                             1577 02935000
         BNZ   @RT01577                                            1577 02936000
*                     GOTO QEND3;   /* YES - GO TO ERROR ROUTINE     */ 02937000
*                   ELSE                                           1579 02938000
*                     SMSCRA=WORD;  /* NO-PICK UP FIELD              */ 02939000
         LR    @09,R0                                              1579 02940000
         L     SMSCRA,WORD(,@09)                                   1579 02941000
*                 END;              /* GET SMS'S CRA FROM PST        */ 02942000
*                 IF SMSCRA=CRAPTR THEN/* IS CRA SMS'S CRA           */ 02943000
         CR    SMSCRA,CRAPTR                                       1581 02944000
         BNE   @RF01581                                            1581 02945000
*                   DO;             /* YES                           */ 02946000
*                     DO;           /* GETLOC LOC(PSTFLGA)ERRTN(QEND3   02947000
*                                      )RET(R0,BYTE)                 */ 02948000
*                       R0=ADDR(PSTFLGA);/* PICK UP @ OF AREA FOR MAR*/ 02949000
         LA    R0,PSTFLGA(,PSTPTR)                                 1584 02950000
*                       R15=PLSTMARA;/* GET MAR ADDRESS              */ 02951000
         L     R15,PLSTMARA(,R1)                                   1585 02952000
*                       CALL MAR;   /* GET DUMPED ADDRESS            */ 02953000
         BALR  @14,@15                                             1586 02954000
*                       IF R15^=0 THEN/* DID MAR ENCOUNTER AN ERROR  */ 02955000
         SLR   @09,@09                                             1587 02956000
         CR    R15,@09                                             1587 02957000
         BNE   @RT01587                                            1587 02958000
*                         GOTO QEND3;/* YES - GO TO ERROR ROUTINE    */ 02959000
*                       ELSE                                       1589 02960000
*                         R0=BYTE;  /* NO-PICK UP FIELD              */ 02961000
         LR    @08,R0                                              1589 02962000
         SLR   @09,@09                                             1589 02963000
         IC    @09,BYTE(,@08)                                      1589 02964000
         LR    R0,@09                                              1589 02965000
*                     END;          /* GET CRA IN USE FLAGS          */ 02966000
*                     IF(R0&USEFLAG2)^=0/* IS CRA CURRENTLY IN USE   */ 02967000
*                       THEN                                       1591 02968000
         SLR   @09,@09                                             1591 02969000
         LR    @08,R0                                              1591 02970000
         N     @08,@CF04888                                        1591 02971000
         CR    @08,@09                                             1591 02972000
         BNE   @RT01591                                            1591 02973000
*                       ;           /* YES - CRA IS IN USE           */ 02974000
*                     ELSE                                         1593 02975000
*                       RC=4;       /* INDICATE CRA NOT IN USE       */ 02976000
         L     @09,CLRSAV(,R13)                                    1593 02977000
         MVC   RC+16(4,@09),@CF00054                               1593 02978000
*                     R13=CLRSAV;   /* RESTORE CALLERS SAVEAREA    1594 02979000
*                                      POINTER                       */ 02980000
@RT01591 L     R13,CLRSAV(,R13)                                    1594 02981000
*                     RETURN;                                      1595 02982000
         B     @EL00008                                            1595 02983000
*                   END;                                           1596 02984000
*               END;                /* END PST OFF OF DEB            */ 02985000
@RF01581 DS    0H                                                  1598 02986000
*QEND3:                                                            1598 02987000
*             ;                                                    1598 02988000
@RT01570 DS    0H                                                  1598 02989000
QEND3    DS    0H                                                  1599 02990000
*             DO;                   /* GETLOC LOC(DCECHN)ERRTN(QEND2)   02991000
*                                      RET(DCEPTR,WORD)              */ 02992000
*               R0=ADDR(DCECHN);    /* PICK UP @ OF AREA FOR MAR     */ 02993000
         L     @09,DCEPTR                                          1600 02994000
         LA    R0,DCECHN(,@09)                                     1600 02995000
*               R15=PLSTMARA;       /* GET MAR ADDRESS               */ 02996000
         L     R15,PLSTMARA(,R1)                                   1601 02997000
*               CALL MAR;           /* GET DUMPED ADDRESS            */ 02998000
         BALR  @14,@15                                             1602 02999000
*               IF R15^=0 THEN      /* DID MAR ENCOUNTER AN ERROR    */ 03000000
         LTR   R15,R15                                             1603 03001000
         BNZ   @RT01603                                            1603 03002000
*                 GOTO QEND2;       /* YES - GO TO ERROR ROUTINE     */ 03003000
*               ELSE                                               1605 03004000
*                 DCEPTR=WORD;      /* NO-PICK UP FIELD              */ 03005000
         LR    @09,R0                                              1605 03006000
         L     @09,WORD(,@09)                                      1605 03007000
         ST    @09,DCEPTR                                          1605 03008000
*             END;                  /* GET NEXT DCE ON CHAIN         */ 03009000
*           END;                    /* END LOOPING THRU DCE CHAIN    */ 03010000
@DE01553 L     @09,DCEPTR                                          1607 03011000
         LTR   @09,@09                                             1607 03012000
         BNZ   @DL01553                                            1607 03013000
*QEND2:                                                            1608 03014000
*           ;                                                      1608 03015000
QEND2    DS    0H                                                  1609 03016000
*         END;                      /* END MPST ON MPLT              */ 03017000
*     END;                          /* END LOOPING THRU MPSTS        */ 03018000
@RT01517 LA    @09,1                                               1610 03019000
         AH    @09,K                                               1610 03020000
@DE01508 STH   @09,K                                               1610 03021000
         CH    @09,MPSTCNT                                         1610 03022000
         BNH   @DL01508                                            1610 03023000
*     DO;                           /* GETLOC LOC(MPLCHN)ERRTN(QEND)R   03024000
*                                      ET(MPLTPTR,WORD)              */ 03025000
*       R0=ADDR(MPLCHN);            /* PICK UP @ OF AREA FOR MAR     */ 03026000
         L     R0,MPLTPTR                                          1612 03027000
*       R15=PLSTMARA;               /* GET MAR ADDRESS               */ 03028000
         L     R15,PLSTMARA(,R1)                                   1613 03029000
*       CALL MAR;                   /* GET DUMPED ADDRESS            */ 03030000
         BALR  @14,@15                                             1614 03031000
*       IF R15^=0 THEN              /* DID MAR ENCOUNTER AN ERROR    */ 03032000
         LTR   R15,R15                                             1615 03033000
         BNZ   @RT01615                                            1615 03034000
*         GOTO QEND;                /* YES - GO TO ERROR ROUTINE     */ 03035000
*       ELSE                                                       1617 03036000
*         MPLTPTR=WORD;             /* NO-PICK UP FIELD              */ 03037000
         LR    @09,R0                                              1617 03038000
         L     @09,WORD(,@09)                                      1617 03039000
         ST    @09,MPLTPTR                                         1617 03040000
*     END;                          /* GET NEXT MPLT ON CHAIN        */ 03041000
*   END;                                                           1619 03042000
@DE01499 L     @09,MPLTPTR                                         1619 03043000
         LTR   @09,@09                                             1619 03044000
         BNZ   @DL01499                                            1619 03045000
*   RFY                                                            1620 03046000
*    (PSTPTR,                                                      1620 03047000
*     CRAQ,                                                        1620 03048000
*     SMSCRA,                                                      1620 03049000
*     DEBPTR,                                                      1620 03050000
*     CRAPTR) UNRSTD;                                              1620 03051000
*QEND:                                                             1621 03052000
*   R13=CLRSAV;                                                    1621 03053000
QEND     L     R13,CLRSAV(,R13)                                    1621 03054000
*   END QSEARCH2;                                                  1622 03055000
         B     @EL00008                                            1622 03056000
@PB00008 DS    0H                                                  1623 03057000
*   GEN(EJECT);                                                    1623 03058000
*                                                                  1623 03059000
         EJECT                                                          03060000
*   /*****************************************************************/ 03061000
*   /*                                                               */ 03062000
*   /* THIS ROUTINE WILL LOCATE THE ACDEB FOR VS2. IT WILL BE PASSED */ 03063000
*   /* BACK IN ACDEBPTR (REG3).                                      */ 03064000
*   /*                                                               */ 03065000
*   /*****************************************************************/ 03066000
*                                                                  1624 03067000
*ADDRDEB2:                                                         1624 03068000
*   PROC OPTIONS(NOSAVEAREA);                                      1624 03069000
         B     @PB00009                                            1624 03070000
ADDRDEB2 STM   @14,@12,12(@13)                                     1624 03071000
*   ACDEBPTR=ZERO;                  /* SET INITIALLY TO NO DEB FOUND */ 03072000
         SLR   ACDEBPTR,ACDEBPTR                                   1625 03073000
*   RFY                                                            1626 03074000
*     ISTDCE BASED(DCEPTR);                                        1626 03075000
*                                                                  1626 03076000
*/*  CHAIN THROUGH THE DCE CHAIN LOOKING FOR A DEB WITH A TCB ADDRESS   03077000
*    WHICH MATCHES THE TCB ADDRESS PASSED IN THE PARAMETER LIST      */ 03078000
*                                                                  1627 03079000
*   DO WHILE ACDEBPTR=ZERO&DCEPTR^=0;                              1627 03080000
         B     @DE01627                                            1627 03081000
@DL01627 DS    0H                                                  1628 03082000
*     ACDEBPTR=DCEDBPTR;            /* GET ADDRESS OF DEB            */ 03083000
         L     @09,DCEPTR                                          1628 03084000
         L     ACDEBPTR,DCEDBPTR(,@09)                             1628 03085000
*     IF ACDEBPTR->ACDTCBAD=PLSTATCB THEN                          1629 03086000
         CLC   ACDTCBAD(4,ACDEBPTR),PLSTATCB(R1)                   1629 03087000
         BE    @RT01629                                            1629 03088000
*       ;                                                          1630 03089000
*     ELSE                                                         1631 03090000
*       DO;                         /* NO-NOT THE DEB WE WANT        */ 03091000
*         DCEPTR=DCECHN;            /* GET NEXT DCE                  */ 03092000
         L     @09,DCEPTR                                          1632 03093000
         L     @09,DCECHN(,@09)                                    1632 03094000
         ST    @09,DCEPTR                                          1632 03095000
*         ACDEBPTR=ZERO;            /* CONTINUE LOOP                 */ 03096000
         SLR   ACDEBPTR,ACDEBPTR                                   1633 03097000
*       END;                                                       1634 03098000
*   END;                            /* END-DO WHILE                  */ 03099000
@RT01629 DS    0H                                                  1635 03100000
@DE01627 SLR   @09,@09                                             1635 03101000
         CR    ACDEBPTR,@09                                        1635 03102000
         BNE   @DC01627                                            1635 03103000
         C     @09,DCEPTR                                          1635 03104000
         BNE   @DL01627                                            1635 03105000
@DC01627 DS    0H                                                  1636 03106000
*   REG3=ACDEBPTR;                  /* SET ACDEBPTR IN REG3 SLOT OF     03107000
*                                      SAVEAREA                      */ 03108000
         ST    ACDEBPTR,REG3(,R13)                                 1636 03109000
*   END ADDRDEB2;                                                  1637 03110000
*                                                                  1637 03111000
@EL00009 DS    0H                                                  1637 03112000
@EF00009 DS    0H                                                  1637 03113000
@ER00009 LM    @14,@12,12(@13)                                     1637 03114000
         BR    @14                                                 1637 03115000
@PB00009 DS    0H                                                  1638 03116000
*/* TPEPILOG                                                         */ 03117000
*                                                                  1638 03118000
*   GEN(PRINT OFF);                                                1638 03119000
         PRINT OFF                                                      03120000
*   GEN(PRINT ON);                                                 1639 03121000
*                                                                  1639 03122000
         PRINT ON                                                       03123000
*/* END OF TPEPILOG                                                  */ 03124000
