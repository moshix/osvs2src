         LR    @08,R0                                              0769 00585000
         L     RDTPTR,WORD(,@08)                                   0769 00586000
*       END;                        /* GET FIRST RDT PTR             */ 00587000
*       FORSW=ON;                   /* INDICATE MOVING FORWARD THRU     00588000
*                                      CHAIN                         */ 00589000
         OI    FORSW,B'00100000'                                   0771 00590000
*       LASTGOOD=ZERO;              /* INITIALIZE LAST RDT LOOKED AT */ 00591000
         LR    LASTGOOD,@09                                        0772 00592000
*       DO WHILE RDTPTR^=QABPTR&RDTPTR^=LASTGOOD;/* SCAN THRU RDTS   */ 00593000
         B     @DE00773                                            0773 00594000
@DL00773 DS    0H                                                  0774 00595000
*         DO;                       /* BEGIN RDT2 PROCESSING         */ 00596000
*                                                                  0774 00597000
*           /*********************************************************/ 00598000
*           /*                                                       */ 00599000
*           /* FORMAT RDT HEADER                                     */ 00600000
*           /*                                                       */ 00601000
*           /*********************************************************/ 00602000
*                                                                  0775 00603000
*           RFY                                                    0775 00604000
*            (RDTEPTR,                                             0775 00605000
*             RDTPOS,                                              0775 00606000
*             TYPE,                                                0775 00607000
*             RDTPTR) RSTD;                                        0775 00608000
*           DO;                     /* GETLOC LOC(RPRELEN)ERRTN(RDT2E   00609000
*                                      RR)RET(WORKLEN,HWRD)          */ 00610000
*             R0=ADDR(RPRELEN);     /* PICK UP @ OF AREA FOR MAR     */ 00611000
         LA    R0,RPRELEN(,RDTPTR)                                 0777 00612000
*             R15=PLSTMARA;         /* GET MAR ADDRESS               */ 00613000
         L     R15,PLSTMARA(,R1)                                   0778 00614000
*             CALL MAR;             /* GET DUMPED ADDRESS            */ 00615000
         BALR  @14,@15                                             0779 00616000
*             IF R15^=0 THEN        /* DID MAR ENCOUNTER AN ERROR    */ 00617000
         LTR   R15,R15                                             0780 00618000
         BNZ   @RT00780                                            0780 00619000
*               GOTO RDT2ERR;       /* YES - GO TO ERROR ROUTINE     */ 00620000
*             ELSE                                                 0782 00621000
*               WORKLEN=HWRD;       /* NO-PICK UP FIELD              */ 00622000
         LR    @09,R0                                              0782 00623000
         LH    @09,HWRD(,@09)                                      0782 00624000
         ST    @09,WORKLEN                                         0782 00625000
*           END;                    /* GET RDT LENGTH                */ 00626000
*           WORKADD=RDTPTR;                                        0784 00627000
         ST    RDTPTR,WORKADD                                      0784 00628000
*           PTRNADDR=ADDR(CBRDT);                                  0785 00629000
         LA    @09,CBRDT                                           0785 00630000
         ST    @09,PTRNADDR                                        0785 00631000
*           CBLABEL=LRDT;                                          0786 00632000
         MVC   CBLABEL(8),@CC05031                                 0786 00633000
*           CALL RAFRMT;                                           0787 00634000
*                                                                  0787 00635000
         BAL   @14,RAFRMT                                          0787 00636000
*           /*********************************************************/ 00637000
*           /*                                                       */ 00638000
*           /* FORMAT RDTES                                          */ 00639000
*           /*                                                       */ 00640000
*           /*********************************************************/ 00641000
*                                                                  0788 00642000
*           DO;                     /* GETLOC LOC(RPRFLAGA)ERRTN(RDT2   00643000
*                                      ERR)RET(RDTPOS,BYTE)          */ 00644000
*             R0=ADDR(RPRFLAGA);    /* PICK UP @ OF AREA FOR MAR     */ 00645000
         LA    R0,RPRFLAGA(,RDTPTR)                                0789 00646000
*             R15=PLSTMARA;         /* GET MAR ADDRESS               */ 00647000
