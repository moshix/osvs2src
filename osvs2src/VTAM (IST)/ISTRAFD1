*     DO WHILE(SCBPTR^=ZERO);       /* DO WHILE THERE ARE SCB'S      */ 00396000
         B     @DE00704                                            0704 00397000
@DL00704 DS    0H                                                  0705 00398000
*       IF SCBPTR->SCBADR61=ADRAPC61/* A VTAM SCB ?                  */ 00399000
