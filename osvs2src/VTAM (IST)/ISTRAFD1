         BALR  @14,@15                                             0766 00577000
*         IF R15^=0 THEN            /* DID MAR ENCOUNTER AN ERROR    */ 00578000
         SLR   @09,@09                                             0767 00579000
         CR    R15,@09                                             0767 00580000
         BNE   @RT00767                                            0767 00581000
*           GOTO RDTERR;            /* YES - GO TO ERROR ROUTINE     */ 00582000
*         ELSE                                                     0769 00583000
