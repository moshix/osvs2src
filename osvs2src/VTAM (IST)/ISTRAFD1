         L     @04,SCBADR61(,SCBPTR)                               0705 00401000
         L     @09,ADRAPC61-1                                      0705 00402000
         LA    @09,0(,@09)                                         0705 00403000
         CR    @04,@09                                             0705 00404000
         BNE   @RF00705                                            0705 00405000
*         DO;                       /* YES-A VTAM SCB                */ 00406000
*           R0=SCBPTR->SCBCRA;      /* ADDRESS CRA                   */ 00407000
         L     R0,SCBCRA(,SCBPTR)                                  0707 00408000
*           IF R0^=ZERO             /* CRA AVAILABLE?                */ 00409000
*             THEN                  /* YES-CRA AVAILABLE             */ 00410000
         LTR   R0,R0                                               0708 00411000
         BZ    @RF00708                                            0708 00412000
*             DO;                                                  0709 00413000
*               HITSW=ON;           /* INDICATE ONE PRINTED          */ 00414000
         OI    HITSW,B'01000000'                                   0710 00415000
*               WORKADD=R0;         /* POINT TO CRA                  */ 00416000
         ST    R0,WORKADD                                          0711 00417000
*               CBLABEL=LCRA;                                      0712 00418000
         MVC   CBLABEL(8),@CC05045                                 0712 00419000
*               CALL RAFRMT;                                       0713 00420000
         BAL   @14,RAFRMT                                          0713 00421000
*             END;                  /* END-CRA AVAILABLE             */ 00422000
*           ELSE                                                   0715 00423000
*             ;                                                    0715 00424000
@RF00708 DS    0H                                                  0716 00425000
*         END;                      /* END-VTAM SCB                  */ 00426000
*       SCBPTR=SCBPTR->SCBCHAIN;    /* ACCESS NEXT SCB               */ 00427000
@RF00705 L     SCBPTR,SCBCHAIN(,SCBPTR)                            0717 00428000
*     END;                          /* END-DO WHILE                  */ 00429000
@DE00704 LTR   SCBPTR,SCBPTR                                       0718 00430000
         BNZ   @DL00704                                            0718 00431000
*     IF HITSW=OFF THEN             /* WERE ANY CRA'S PRINTED ?      */ 00432000
         TM    HITSW,B'01000000'                                   0719 00433000
         BNZ   @RF00719                                            0719 00434000
*       DO;                                                        0720 00435000
*         BUFFER=ERRMESS1;          /* NO-INDICATE ERROR             */ 00436000
         L     @04,PLSTOBUF(,R1)                                   0721 00437000
         MVI   BUFFER+36(@04),C' '                                 0721 00438000
         MVC   BUFFER+37(83,@04),BUFFER+36(@04)                    0721 00439000
         MVC   BUFFER(36,@04),ERRMESS1                             0721 00440000
*         MESS1LBL=LCRA;                                           0722 00441000
         MVC   MESS1LBL(8,@04),@CC05045                            0722 00442000
*         CALL ERRMSG;                                             0723 00443000
         BAL   @14,ERRMSG                                          0723 00444000
*       END;                                                       0724 00445000
*     RFY                                                          0725 00446000
*       SCBPTR UNRSTD;                                             0725 00447000
@RF00719 DS    0H                                                  0726 00448000
*   END;                                                           0726 00449000
*   R13=CLRSAV;                                                    0727 00450000
         L     R13,CLRSAV(,R13)                                    0727 00451000
*   END ISTRAFDA;                                                  0728 00452000
@EL00002 DS    0H                                                  0728 00453000
@EF00002 DS    0H                                                  0728 00454000
@ER00002 LM    @14,@12,12(@13)                                     0728 00455000
         BR    @14                                                 0728 00456000
@PB00002 DS    0H                                                  0729 00457000
*   GEN(EJECT);                                                    0729 00458000
         EJECT                                                          00459000
*ISTRAFDB:                                                         0730 00460000
*   PROC OPTIONS(NOSAVEAREA);                                      0730 00461000
*                                                                  0730 00462000
         B     @PB00003                                            0730 00463000
ISTRAFDB STM   @14,@12,12(@13)                                     0730 00464000
*/* **************************************************************** */ 00465000
*/*                                                                  */ 00466000
*/*  THIS PROCEDURE WILL FORMAT CONTROL BLOCKS FOR PRDUMP.           */ 00467000
*/*  THE ATCVT,QAB,RDTS,RDTES,NCBS, AND CONFT TABLES ARE FORMATED.   */ 00468000
*/*  THE MPLTS, MPSTS, DCES, DEBS, PSTS, AND FMCBS ARE ALSO FORMATED */ 00469000
*/*  SMS'S BPD, BPCBS ARE FORMATED ALONG WITH ALL ACTIVE CRA IN THE  */ 00470000
*/*  LP00 BUFFER POOL.                                               */ 00471000
*/*                                                                  */ 00472000
*/* **************************************************************** */ 00473000
*                                                                  0731 00474000
*   R15=ADDR(SAV4);                 /* SET UP SAVE AREA              */ 00475000
         LA    R15,SAV4                                            0731 00476000
*   BACKPTR=R13;                                                   0732 00477000
         ST    R13,BACKPTR(,R15)                                   0732 00478000
*   R13=R15;                                                       0733 00479000
*                                                                  0733 00480000
         LR    R13,R15                                             0733 00481000
*   /*****************************************************************/ 00482000
*   /*                                                               */ 00483000
*   /* FORMAT & PRINT ATCVT                                          */ 00484000
*   /*                                                               */ 00485000
*   /*****************************************************************/ 00486000
*                                                                  0734 00487000
*   WORKADD=ATCVTPTR;                                              0734 00488000
         ST    ATCVTPTR,WORKADD                                    0734 00489000
*   WORKLEN=LENGTH(ISTATCVT);                                      0735 00490000
         MVC   WORKLEN(4),@CF05802                                 0735 00491000
*   PTRNADDR=ADDR(CBATCVT);                                        0736 00492000
         LA    @09,CBATCVT                                         0736 00493000
         ST    @09,PTRNADDR                                        0736 00494000
*   CBLABEL=LATCVT;                                                0737 00495000
         MVC   CBLABEL(8),@CC04955                                 0737 00496000
*   CALL RAFRMT;                                                   0738 00497000
         BAL   @14,RAFRMT                                          0738 00498000
*   RFY                                                            0739 00499000
*     ISTATCVT BASED(ATCVTPTR);                                    0739 00500000
*                                                                  0739 00501000
*   /*****************************************************************/ 00502000
*   /*                                                               */ 00503000
*   /* PROCESS RDT CHAIN                                             */ 00504000
*   /*                                                               */ 00505000
*   /*****************************************************************/ 00506000
*                                                                  0740 00507000
*   DO;                             /* BEGIN FRMTRDTB PROCESSING     */ 00508000
*     RFY                                                          0741 00509000
*      (QABPTR) RSTD;                                              0741 00510000
*     RFY                                                          0742 00511000
*       ISTRPRE BASED(RDTPTR);                                     0742 00512000
*     RFY                                                          0743 00513000
*       ISTRAP BASED(RDTEPTR);                                     0743 00514000
*     DO;                           /* GETLOC LOC(ATCRDT)ERRTN(QABERR   00515000
*                                      )RET(QABPTR,WORD)             */ 00516000
*       R0=ADDR(ATCRDT);            /* PICK UP @ OF AREA FOR MAR     */ 00517000
         LA    R0,ATCRDT(,ATCVTPTR)                                0745 00518000
*       R15=PLSTMARA;               /* GET MAR ADDRESS               */ 00519000
         L     R15,PLSTMARA(,R1)                                   0746 00520000
*       CALL MAR;                   /* GET DUMPED ADDRESS            */ 00521000
         BALR  @14,@15                                             0747 00522000
*       IF R15^=0 THEN              /* DID MAR ENCOUNTER AN ERROR    */ 00523000
         SLR   @09,@09                                             0748 00524000
         CR    R15,@09                                             0748 00525000
         BNE   @RT00748                                            0748 00526000
*         GOTO QABERR;              /* YES - GO TO ERROR ROUTINE     */ 00527000
*       ELSE                                                       0750 00528000
*         QABPTR=WORD;              /* NO-PICK UP FIELD              */ 00529000
         LR    @08,R0                                              0750 00530000
         L     QABPTR,WORD(,@08)                                   0750 00531000
*     END;                          /* GET QAB PTR FROM ATCVT        */ 00532000
*     IF QABPTR=0 THEN              /* IS THERE A QAB ?              */ 00533000
         CR    QABPTR,@09                                          0752 00534000
         BE    @RT00752                                            0752 00535000
*       GOTO QABERR;                                               0753 00536000
*     ELSE                                                         0754 00537000
*       ;                                                          0754 00538000
*     RFY                                                          0755 00539000
*       ISTQAB BASED(QABPTR);                                      0755 00540000
*                                                                  0755 00541000
*     /***************************************************************/ 00542000
*     /*                                                             */ 00543000
*     /* FORMAT & PRINT QAB                                          */ 00544000
*     /*                                                             */ 00545000
*     /***************************************************************/ 00546000
*                                                                  0756 00547000
*     WORKADD=QABPTR;                                              0756 00548000
         ST    QABPTR,WORKADD                                      0756 00549000
*     WORKLEN=LENGTH(ISTQAB);                                      0757 00550000
