         LR    @09,R0                                              0794 00657000
         SLR   RDTPOS,RDTPOS                                       0794 00658000
         IC    RDTPOS,BYTE(,@09)                                   0794 00659000
*           END;                    /* GET RDTE POSITION - LAST RDTE    00660000
*                                      ?                             */ 00661000
*           DO;                     /* GETLOC LOC(RPRENTRY)ERRTN(RDT2   00662000
*                                      ERR)RET(TYPE,BYTE)            */ 00663000
*             R0=ADDR(RPRENTRY);    /* PICK UP @ OF AREA FOR MAR     */ 00664000
         LA    R0,RPRENTRY(,RDTPTR)                                0797 00665000
*             R15=PLSTMARA;         /* GET MAR ADDRESS               */ 00666000
         L     R15,PLSTMARA(,R1)                                   0798 00667000
*             CALL MAR;             /* GET DUMPED ADDRESS            */ 00668000
         BALR  @14,@15                                             0799 00669000
*             IF R15^=0 THEN        /* DID MAR ENCOUNTER AN ERROR    */ 00670000
         LTR   R15,R15                                             0800 00671000
         BNZ   @RT00800                                            0800 00672000
*               GOTO RDT2ERR;       /* YES - GO TO ERROR ROUTINE     */ 00673000
*             ELSE                                                 0802 00674000
*               TYPE=BYTE;          /* NO-PICK UP FIELD              */ 00675000
         LR    @09,R0                                              0802 00676000
         SLR   TYPE,TYPE                                           0802 00677000
         IC    TYPE,BYTE(,@09)                                     0802 00678000
*           END;                    /* GET RDT TYPE                  */ 00679000
*           RDTEPTR=RDTPTR;         /* INITIAL RDTE POINTER          */ 00680000
         LR    RDTEPTR,RDTPTR                                      0804 00681000
*           SAVELEN=WORKLEN;        /* INITIALIZE RDT LENGTH FOR   0805 00682000
*                                      TERMINAL RDTS                 */ 00683000
         L     @09,WORKLEN                                         0805 00684000
         ST    @09,SAVELEN                                         0805 00685000
*           RFY                                                    0806 00686000
*             ISTRPRE BASED(RDTEPTR);                              0806 00687000
*           IF TYPE=RPRENTAH THEN   /* IS THIS AN APPL RDT ?         */ 00688000
*                                                                  0807 00689000
         C     TYPE,@CF02326                                       0807 00690000
         BNE   @RF00807                                            0807 00691000
*             /*******************************************************/ 00692000
*             /*                                                     */ 00693000
*             /* FORMAT APPLICATION RDTES                            */ 00694000
*             /*                                                     */ 00695000
*             /*******************************************************/ 00696000
*                                                                  0808 00697000
