*           CALL RAFRMT;            /* FORMAT THE ACDEB              */ 00258000
         BAL   @14,RAFRMT                                          0653 00259000
*           HITSW=ON;               /* INDICATE ONE BLOCK PRINTED    */ 00260000
         OI    HITSW,B'01000000'                                   0654 00261000
*         END;                                                     0655 00262000
*         GEN(EJECT);                                              0656 00263000
*                                                                  0656 00264000
         EJECT                                                          00265000
*         /***********************************************************/ 00266000
*         /*                                                         */ 00267000
*         /* THIS SEGMENT WILL FORMAT THE RDT ASSOCIATED WITH THE    */ 00268000
*         /* ABENDING TASK.                                          */ 00269000
*         /*                                                         */ 00270000
*         /***********************************************************/ 00271000
*                                                                  0657 00272000
*         RFY                                                      0657 00273000
*           RDTPTR RSTD;                                           0657 00274000
*         RDTPTR=ACDEBPTR->ACDRDTE; /* PICK UP RDT ADDRESS FROM DEB  */ 00275000
         L     RDTPTR,ACDRDTE(,ACDEBPTR)                           0658 00276000
*         IF RDTPTR=ZERO            /* IS RDT AVAILABLE ?            */ 00277000
*           THEN                                                   0659 00278000
