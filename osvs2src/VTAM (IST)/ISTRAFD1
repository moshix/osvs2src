         ST    @09,PTRNADDR                                        0758 00554000
*     CBLABEL=LQAB;                                                0759 00555000
         MVC   CBLABEL(8),@CC04959                                 0759 00556000
*     CALL RAFRMT;                                                 0760 00557000
*                                                                  0760 00558000
         BAL   @14,RAFRMT                                          0760 00559000
*     /***************************************************************/ 00560000
*     /*                                                             */ 00561000
*     /* PROCESS RDT CHAIN                                           */ 00562000
*     /*                                                             */ 00563000
*     /***************************************************************/ 00564000
*                                                                  0761 00565000
*     DO;                           /* BEGIN RDT1 PROCESSING         */ 00566000
*       RFY                                                        0762 00567000
*        (LASTGOOD,                                                0762 00568000
*         RDTPTR) RSTD;                                            0762 00569000
