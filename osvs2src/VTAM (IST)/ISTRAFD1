*         DCEPTR=DCECHN;            /* ADDRESS NEXT DCE              */ 00345000
         L     @09,DCEPTR                                          0687 00346000
         L     @09,DCECHN(,@09)                                    0687 00347000
         ST    @09,DCEPTR                                          0687 00348000
*         CALL ADDRDEB2;            /* ADDRESS VS2 ACDEB             */ 00349000
         BAL   @14,ADDRDEB2                                        0688 00350000
*       END;                        /* END-DO WHILE THERE IS A DEB   */ 00351000
@DE00647 LTR   ACDEBPTR,ACDEBPTR                                   0689 00352000
         BNZ   @DL00647                                            0689 00353000
*     END;                          /* END - YES THERE IS A MPST     */ 00354000
*   IF HITSW=ON                     /* WERE ANY ACDEB'S FOUND ?      */ 00355000
*     THEN                                                         0691 00356000
@RT00641 TM    HITSW,B'01000000'                                   0691 00357000
         BO    @RT00691                                            0691 00358000
*     ;                             /* YES-SOME ACDEB'S WERE FOUND   */ 00359000
*   ELSE                            /* NO-NO ACDEBS WERE FOUND       */ 00360000
*     DO;                                                          0693 00361000
*       BUFFER=ERRMESS3;            /* INDICATE NO DEBS,FMCBS,OR RDTS*/ 00362000
