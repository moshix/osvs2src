         EX    @10,@SM04688                                     02S0691 00233000
*  ELSE;                                                        02S0692 00234000
@RF00690 DS    0H                                               02S0693 00235000
*RTEXTCT = LBUFENDA - TRFPTR + 1;       /* TEXT LEFT IN 1ST BUFFER */   00236000
         L     @10,LBUFENDA                                     02S0693 00237000
         SLR   @10,TRFPTR                                       02S0693 00238000
         AL    @10,@CF00060                                     02S0693 00239000
         STH   @10,RTEXTCT                                      02S0693 00240000
*IF RTEXTCT > 3                                                 02S0694 00241000
*  THEN DO;                             /* MOVE REMAINDER TO START OF   00242000
*                                         1ST BUFR TXT AREA */  02S0694 00243000
         C     @10,@CF00287                                     02S0694 00244000
         BNH   @RF00694                                         02S0694 00245000
*        RTEXTA = TRFPTR;               /* ADDR REMAINING TEXT        * 00246000
         ST    TRFPTR,RTEXTA                                    02S0696 00247000
*                                                               03S0697 00248000
*/********************************************************************* 00249000
*/* 1. MOVE REMAINING TEXT TO START OF TEXT AREA IN FIRST LINE BUFFER * 00250000
*/* 2. CALC # BYTES REQUIRED TO COMPLETE A GTF DATA BLOCK             * 00251000
*/* 3. UPDATE THE DATA COUNT IN THE FIRST BUFFER.                     * 00252000
*/*   SET IT = TO # BYTES REMAINING TO BE RECORDED & SAVE THIS VALUE  * 00253000
*/* 4.INIT SW TO EXECUTE FILL 1ST BUFFER LOOP                         * 00254000
*/* 5.EXECUTE LOOP UNTIL 1ST BUFFER TEXT AREA HAS BEEB FILLED         * 00255000
*/*     A. IF THERE ARE NO MORE BUFFERS IN BUFFER CHAIN               * 00256000
*/*           OR THE NEXT BUFFER HAS NO DATA                          * 00257000
*/*          THEN RESET  LOOP SW TO STOP TEXT MOVEMENT       */ 03S0697 00258000
*/*          ELSE 1. CALC START ADDR OF ADDITIONAL TEXT FOR 1ST BUFFER* 00259000
*/*               2. IF DATA COUNT FOR NXT BUFR > # BYTES RQRD        * 00260000
*/*                     TO FILL 1ST BUFFER                            * 00261000
*/*                    THEN 1.MOVE JUST ENOUGH TEXT TO FILL 1ST BUFFR * 00262000
*/*                         2.INCR   DATA COUNT OF FIRST BUFR BY      * 00263000
*/*                           AMNT OF TEXT JUST MOVED                 * 00264000
*/*                         3.DECR DATA COUNT OF BUFR FROM WHICH TEXT * 00265000
*/*                           WAS MOVED BY THE AMNT MOVED             * 00266000
*/*                         4.INCR DATA PTR IN THIS BUFFER BY AMNT    * 00267000
*/*                          MOVED SO IT POINTS TO NEW TEXT           * 00268000
*/*                         5.RESET THE  LOOP SW TO STOP TXT MOVEMENT * 00269000
*/*                    ELSE 1.MOVE ALL TEXT FROM NXT BUFR INTO 1ST BUF* 00270000
*/*                         2.INCR DATA COUNT OF FIRST BUFR BY        * 00271000
*/*                           AMNT OF TEXT MOVED                      * 00272000
*/*                         3.DECR AMNT OF TEXT RQRD TO FILL 1ST BUFFR* 00273000
*/*                           BY AMNT OF TEXT MOVED                   * 00274000
*/*                         4.IF AMNT OF TEXT RQRD FOR 1ST BUFFER     * 00275000
*/*                              IS = ZERO                            * 00276000
*/*                             THEN RESET  LOOP SW                   * 00277000
*/*                                  TO STOP TEXT MOVEMENT            * 00278000
*/*                             ELSE 1.INCR OFFSET TO BE USED IN CALC * 00279000
*/*                                    OF START ADDR OF ADDITIONAL TXT* 00280000
*/*                                    IN 1ST BUFFER                  * 00281000
*/*                                  2.UPDATE PTR TO NEXT BUFFER      * 00282000
*/*                                    IN CHAIN                       * 00283000
*/*                                  3. SET TEXT-MOVE LOOP SW ON SO   * 00284000
*/*                                    TEXT WILL CONTINUE TO BE MOVED * 00285000
*/*                                    INTO THE 1ST BUFFER            * 00286000
*/********************************************************************* 00287000
*                                                               03S0697 00288000
*N = RTEXTCT;                          /* INIT AMNT TEXT TO BE MOVED    00289000
*                                        WITH # BYTES TEXT REMAING IN   00290000
*                                        FIRST BUFFER                 * 00291000
         STH   @10,N                                            03S0697 00292000
*Q1 = FDBDATAA + RULENGTH;             /* INIT RECEIVING FIELD PTR      00293000
*                                        WITH ADDR OF TEXT AREA IN      00294000
*                                        FIRST BUFFER                 * 00295000
         LH    @10,RULENGTH                                     03S0698 00296000
