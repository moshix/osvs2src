*         END; /*LISATCVT*/                                     01S0639 00061000
*                                                               01S0639 00062000
*        VTAMCVTA = R2;                /* SAVE ADDR VTAM CVT */ 01S0640 00063000
         ST    R2,VTAMCVTA                                      01S0640 00064000
*        RFY R2 UNRSTD;                                         01S0641 00065000
*        ADDRRPH = R1;                  /* SAVE RPH PTR */      01S0642 00066000
         ST    R1,ADDRRPH                                       01S0642 00067000
*        GTFPLIST = GTFPLIST&&GTFPLIST; /* INIT GTF PARM LIST */        00068000
         XC    GTFPLIST(8),GTFPLIST                             01S0643 00069000
*        SWITCHES = SWITCHES&&SWITCHES; /* RESET ALL SWITCHES */        00070000
         XC    SWITCHES(1),SWITCHES                             01S0644 00071000
*        GTFDATAA = R6;               /* SET UP  GTF DATA ADDR       */ 00072000
         ST    R6,GTFDATAA                                      01S0645 00073000
*        GTFFMTID = 'FD'X;            /* FORMAT RTNE ID */      01S0646 00074000
