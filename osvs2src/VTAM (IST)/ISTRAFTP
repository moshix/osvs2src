@EL00001 DS    0H                                               01S0823 00688000
@EF00001 DS    0H                                               01S0823 00689000
@ER00001 BR    @14                                              01S0823 00690000
*DATAXTNT: PROC OPTIONS(NOSAVE,NOSAVEAREA);                     01S0824 00691000
DATAXTNT DS    0H                                               01S0825 00692000
*/********************************************************************* 00693000
*/* 1. CALCULATE LOGICAL LENGTH OF 1ST BUFFER,I.E. AMNT OF DATA 01S0825 00694000
*/* IN THE BUFFER */                                            01S0825 00695000
*/* 2. IF LOGICAL LENGTH >= DATA LIMIT IMPOSED BY GTF OR        01S0825 00696000
*/* THE PHYSICAL BUFFER SIZE */                                 01S0825 00697000
*/*      THEN SET GTF DATA LENGTH = DATA LIMIT & STORE IT       01S0825 00698000
*                                  IN THE GTF PARM LIST */      01S0825 00699000
*/*      ELSE SET GTF DATA LENGTH = LOGICAL LENGTH & STORE IT         * 00700000
*/* 3. CALCULATE ADDR OF LAST BYTE OF GTF DATA                        * 00701000
*/********************************************************************* 00702000
*                                                               01S0825 00703000
*RFY R14 RSTD;                         /* RSTRCT REG.14 FOR RTRN @ */   00704000
*LBUFLEN = FDBDATCT + FDBDATAA - GTFDATAA;   /* LOGICAL BUFFER LENGTH * 00705000
         L     @15,GTFDATAA                                     01S0826 00706000
         LH    @10,FDBDATCT(,@15)                               01S0826 00707000
         AL    @10,FDBDATAA(,@15)                               01S0826 00708000
         SLR   @10,@15                                          01S0826 00709000
         STH   @10,LBUFLEN                                      01S0826 00710000
*LBUFENDA = LBUFLEN + GTFDATAA - 1;    /* @ LAST BYTE OF LOGCL BUFR */  00711000
         ALR   @15,@10                                          01S0827 00712000
         BCTR  @15,0                                            01S0827 00713000
         ST    @15,LBUFENDA                                     01S0827 00714000
*IF LBUFLEN >= GTFLIM    /* IF LOGCL LEN >= DATA LIMIT       @XA08633*/ 00715000
*  THEN GTFLEN = GTFLIM;  /* SET GTF LEN = DATA LIMIT        @XA08633*/ 00716000
         LH    @15,GTFLIM                                       01S0828 00717000
         CR    @10,@15                                          01S0828 00718000
         BL    @RF00828                                         01S0828 00719000
         STH   @15,GTFLEN                                       01S0829 00720000
*  ELSE GTFLEN = LBUFLEN;              /* SET GTF LEN = LOGCL LEN     * 00721000
         B     @RC00828                                         01S0830 00722000
@RF00828 LH    @15,LBUFLEN                                      01S0830 00723000
         STH   @15,GTFLEN                                       01S0830 00724000
*GTFDATND = GTFDATAA + GTFLEN - 1;     /* ADDR LAST BYTE GTF DATA     * 00725000
@RC00828 LH    @15,GTFLEN                                       01S0831 00726000
         AL    @15,GTFDATAA                                     01S0831 00727000
         BCTR  @15,0                                            01S0831 00728000
         ST    @15,GTFDATND                                     01S0831 00729000
*RFY R14 UNRSTD;                       /* RELEASE REG.14 */     01S0832 00730000
*END DATAXTNT;                                                  01S0833 00731000
@EL00002 DS    0H                                               01S0833 00732000
@EF00002 DS    0H                                               01S0833 00733000
@ER00002 BR    @14                                              01S0833 00734000
*ADDRTRF:PROC OPTIONS(NOSAVE,NOSAVEAREA);                       01S0834 00735000
ADDRTRF  DS    0H                                               01S0835 00736000
*/********************************************************************* 00737000
*/* 1. DETERMINE HOW MANY BYTES THE TRAILER ISTTRF WILL HAVE TO BE    * 00738000
*/* ADJUSTED SO THAT IT WILL FIT WITHIN THE GTF DATA RECORD AS THE    * 00739000
*/* LAST LENGTH(ISTTRF) BYTES OF THE RECORD. THIS ADJUSTMENT,TRFDELTA,* 00740000
*/* IS ALSO THE # OF BYTES THAT WILL HAVE TO BE SAVED BEFORE PLACING  * 00741000
*/* THE TRAILER INTO THE GTF RECORD & RESTORED AFTER THE GTF HOOK HAS * 00742000
*/* BEEN ENGAGED                                                      * 00743000
*/*                                                                   * 00744000
*/* 2. RECALCULATE THE LENGTH OF THE GTF DATA BLOCK                   * 00745000
*/* 3. CALCULATE THE ADDR OF THE TRAILER,ISTTRF                       * 00746000
*/********************************************************************* 00747000
*                                                               01S0835 00748000
*RFY R14 RSTD;                         /* RSTRCT REG.14 FOR RTRN @ */   00749000
*RFY ISTPROCD BASED(ADDR(FMCPROCD));   /* BASE FMCB PROC OPTIONS      * 00750000
*IF PROCFTX = ON                       /* IF TEXT IS CONFIDENTIAL     * 00751000
*  THEN DO;                            /* PREVENT RECORDING IT */       00752000
         TM    PROCFTX+48(R5),B'00000001'                       01S0837 00753000
         BNO   @RF00837                                         01S0837 00754000
*        TRFPTR = TEXTPTR;             /* PUT TRLR AT START OF TEXT */  00755000
         L     @15,TEXTPTR                                      01S0839 00756000
         LR    TRFPTR,@15                                       01S0839 00757000
*        TRFDELTA = LENGTH(ISTTRF);    /* AMNT TXT TO BE OVERLAYED */   00758000
         LA    @10,17                                           01S0840 00759000
         STH   @10,TRFDELTA                                     01S0840 00760000
*        GTFLEN = TEXTPTR - GTFDATAA + LENGTH(ISTTRF);          01S0841 00761000
         SL    @15,GTFDATAA                                     01S0841 00762000
         ALR   @15,@10                                          01S0841 00763000
         STH   @15,GTFLEN                                       01S0841 00764000
*       END;                                                    01S0842 00765000
*  ELSE DO;                                                     01S0843 00766000
         B     @RC00837                                         01S0843 00767000
@RF00837 DS    0H                                               01S0844 00768000
*   IF GTFLEN = GTFLIM  /* IF GTF DATA LENGTH = MAX AMNT     @XA08633*/ 00769000
*     THEN TRFDELTA = LENGTH(ISTTRF); /* THE BUFFER IS FULL AND THE     00770000
*                                       AMNT OF DATA TO BE OVERLAYED    00771000
*                                       = LEN OF THE TRAILER */ 01S0844 00772000
         CLC   GTFLEN(2),GTFLIM                                 01S0844 00773000
         BNE   @RF00844                                         01S0844 00774000
         MVC   TRFDELTA(2),@CH02426                             01S0845 00775000
*     ELSE                                                      01S0846 00776000
*      IF (((ATCHBSIZ+7)|7)&&7) = LBUFLEN /* IF PHYS SIZE    @XA08633   00777000
*                                    = LOGICAL, OVERLAY LAST @XA08633   00778000
*                                    BYTES                   @XA08633*/ 00779000
*        THEN TRFDELTA = LENGTH(ISTTRF);                   /*@XA08633*/ 00780000
         B     @RC00844                                         01S0846 00781000
@RF00844 LA    @15,7                                            01S0846 00782000
         L     @10,VTAMCVTA                                     01S0846 00783000
         LH    @10,ATCHBSIZ(,@10)                               01S0846 00784000
         ALR   @10,@15                                          01S0846 00785000
         OR    @10,@15                                          01S0846 00786000
         XR    @10,@15                                          01S0846 00787000
         CH    @10,LBUFLEN                                      01S0846 00788000
         BNE   @RF00846                                         01S0846 00789000
         MVC   TRFDELTA(2),@CH02426                             01S0847 00790000
*        ELSE                                              /*@XA08633*/ 00791000
*         IF (GTFLIM -GTFLEN) >= LENGTH(ISTTRF)            /*@XA08633*/ 00792000
*           THEN DO;              /* TRAILER WILL FIT IN BUFFER */      00793000
         B     @RC00846                                         01S0848 00794000
@RF00846 LA    @15,17                                           01S0848 00795000
         LH    @10,GTFLEN                                       01S0848 00796000
         LH    @02,GTFLIM                                       01S0848 00797000
         SLR   @02,@10                                          01S0848 00798000
         CR    @02,@15                                          01S0848 00799000
         BL    @RF00848                                         01S0848 00800000
*                 TRFDELTA = 0;   /* NO DATA TO BE OVERLAYED */ 01S0850 00801000
         SLR   @02,@02                                          01S0850 00802000
         STH   @02,TRFDELTA                                     01S0850 00803000
*                 GTFLEN = GTFLEN+LENGTH(ISTTRF); /* ADD LEN OF TRAILER 00804000
*                                                 TO LEN OF GTF DATA */ 00805000
         ALR   @10,@15                                          01S0851 00806000
         STH   @10,GTFLEN                                       01S0851 00807000
*                END;                                           01S0852 00808000
*           ELSE DO;              /* TRAILER WON'T FIT IN BUFFER */     00809000
         B     @RC00848                                         01S0853 00810000
@RF00848 DS    0H                                               01S0854 00811000
*                 TRFDELTA = GTFLEN+LENGTH(ISTTRF)-GTFLIM; /*@XA08633*/ 00812000
         LH    @15,GTFLIM                                       01S0854 00813000
         LA    @10,17                                           01S0854 00814000
         AH    @10,GTFLEN                                       01S0854 00815000
         SLR   @10,@15                                          01S0854 00816000
         STH   @10,TRFDELTA                                     01S0854 00817000
*                                      /* AMNT OF DATA TO BE OVERLAYED* 00818000
*                 GTFLEN = GTFLIM;   /* GTF DATA LEN = MAX ALLOWED */   00819000
         STH   @15,GTFLEN                                       01S0855 00820000
*                END;                                           01S0856 00821000
*  TRFPTR = GTFDATND - TRFDELTA +1;    /* ADDR OF TRLR ISTTRF */        00822000
@RC00848 DS    0H                                               01S0857 00823000
@RC00846 DS    0H                                               01S0857 00824000
@RC00844 L     TRFPTR,GTFDATND                                  01S0857 00825000
         SH    TRFPTR,TRFDELTA                                  01S0857 00826000
         AL    TRFPTR,@CF00060                                  01S0857 00827000
*       END;                                                    01S0858 00828000
*                                                               01S0858 00829000
*/********************************************************************* 00830000
*/*  IF TRACE IS FOR LINE BUFFERS                                     * 00831000
*/*    THEN 1. LOCATE THE TRAILER ON THE NEXT LOWER WORD BDY          * 00832000
*/*            RELEATIVE TO THE START OF THE BUFFER TEXT              * 00833000
*/*         2. RECALCULATE THE AMNT OF TEXT TO BE OVERLAYED BY        * 00834000
*/*            THE TRAILER AND THE AMNT OF GTF DATA                   * 00835000
*/********************************************************************* 00836000
*                                                               01S0859 00837000
*IF LINETRSW = ON                      /* IF TRACE IS FOR LINE BUFFER * 00838000
*  THEN DO;                                                     01S0859 00839000
@RC00837 TM    LINETRSW,B'00100000'                             01S0859 00840000
         BNO   @RF00859                                         01S0859 00841000
*GTFTXTCT = TRFPTR - TEXTPTR;          /* GTF TEXT COUNT */     01S0861 00842000
         LR    @15,TRFPTR                                       01S0861 00843000
         SL    @15,TEXTPTR                                      01S0861 00844000
         STH   @15,GTFTXTCT                                     01S0861 00845000
*BDYDIFF = GTFTXTCT//4;                /* DIFF BETWEEEN TRAILER @ AND   00846000
*                                         RELEATIVE WORD BDY */ 01S0862 00847000
         LR    @08,@15                                          01S0862 00848000
         SRDA  @08,32                                           01S0862 00849000
         D     @08,@CF00052                                     01S0862 00850000
         STH   @08,BDYDIFF                                      01S0862 00851000
*TRFPTR = TRFPTR - BDYDIFF;             /* ADJ @TRF TO LOWER WRD BDY */ 00852000
         SLR   TRFPTR,@08                                       01S0863 00853000
*TRFDELTA = TRFDELTA + BDYDIFF;         /* RECALC AMNT TO BE SAVED */   00854000
         LH    @10,TRFDELTA                                     01S0864 00855000
         ALR   @10,@08                                          01S0864 00856000
         STH   @10,TRFDELTA                                     01S0864 00857000
*GTFLEN = GTFLEN - BDYDIFF;             /* RECALC AMNT GTF DATA */      00858000
         LH    @10,GTFLEN                                       01S0865 00859000
         SLR   @10,@08                                          01S0865 00860000
         STH   @10,GTFLEN                                       01S0865 00861000
*GTFTXTCT = GTFTXTCT - BDYDIFF;    /* RECALC AMNT TEXT IN GTF DATA */   00862000
         SLR   @15,@08                                          01S0866 00863000
         STH   @15,GTFTXTCT                                     01S0866 00864000
*       END;                                                    01S0867 00865000
*  ELSE;                                                        01S0868 00866000
@RF00859 DS    0H                                               01S0869 00867000
*RFY R14 UNRSTD;                        /* RELEASE REG.14 */    01S0869 00868000
*END ADDRTRF;                                                   01S0870 00869000
@EL00003 DS    0H                                               01S0870 00870000
@EF00003 DS    0H                                               01S0870 00871000
@ER00003 BR    @14                                              01S0870 00872000
*MOVEDATA:PROC OPTIONS(NOSAVE,NOSAVEAREA);                      01S0871 00873000
MOVEDATA DS    0H                                               01S0872 00874000
*/********************************************************************* 00875000
*/* MOVE N BYTES OF DATA FROM THE FIELD LOCATED BY Q2                 * 00876000
*/* TO THE FIELD LOCATED BY Q1                                        * 00877000
*/********************************************************************* 00878000
*                                                               01S0872 00879000
*RFY R14 RSTD;                         /* RSTRCT REG.14 FOR RTRN @ */   00880000
*DO WHILE N > 256;                     /* MOVE BLOCKS OF 256 BYTES    * 00881000
         B     @DE00873                                         01S0873 00882000
@DL00873 DS    0H                                               01S0874 00883000
*Q1->BUFDATA(1:256) = Q2->BUFDATA(1:256);                       01S0874 00884000
         L     @15,Q1                                           01S0874 00885000
         L     @10,Q2                                           01S0874 00886000
         MVC   BUFDATA(256,@15),BUFDATA(@10)                    01S0874 00887000
*Q1 = Q1+256;                                                   01S0875 00888000
         LA    @06,256                                          01S0875 00889000
         ALR   @15,@06                                          01S0875 00890000
         ST    @15,Q1                                           01S0875 00891000
*Q2 = Q2+256;                                                   01S0876 00892000
         ALR   @10,@06                                          01S0876 00893000
         ST    @10,Q2                                           01S0876 00894000
*N = N-256;                                                     01S0877 00895000
         LCR   @06,@06                                          01S0877 00896000
         AH    @06,N                                            01S0877 00897000
         STH   @06,N                                            01S0877 00898000
*END;                                                           01S0878 00899000
@DE00873 LH    @15,N                                            01S0878 00900000
         C     @15,@CF00680                                     01S0878 00901000
         BH    @DL00873                                         01S0878 00902000
*Q1->BUFDATA(1:N) = Q2->BUFDATA(1:N); /* MOVE WHATEVER IS LEFT        * 00903000
         L     @10,Q1                                           01S0879 00904000
         BCTR  @15,0                                            01S0879 00905000
         L     @06,Q2                                           01S0879 00906000
         EX    @15,@SM04695                                     01S0879 00907000
*RFY R14 UNRSTD;                       /* RELEASE REG.14 */     01S0880 00908000
*END MOVEDATA;                                                  01S0881 00909000
*                                                               01S0881 00910000
*                                                               01S0881 00911000
@EL00004 DS    0H                                               01S0881 00912000
@EF00004 DS    0H                                               01S0881 00913000
@ER00004 BR    @14                                              01S0881 00914000
*/* TPEPILOG   */                                               01S0882 00915000
* GEN (PRINT OFF);                                              01S0882 00916000
*                                                               01S0882 00917000
         PRINT OFF                                                      00918000
* GEN (PRINT ON);                                               01S0883 00919000
         PRINT ON                                                       00920000
*/* END OF TPEPILOG */                                          01S0884 00921000
*                                                               01S0884 00922000
*END ISTRAFTP                                                   01S0884 00923000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.      * 00924000
*/*%INCLUDE SYSLIB  (CVT     )                                        * 00925000
