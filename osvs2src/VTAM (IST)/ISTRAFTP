         L     @15,GTFDATAA                                     03S0702 00314000
         STH   @02,FDBDATCT(,@15)                               03S0702 00315000
*NEWDELTA = RTEXTCT;                   /* INIT OFFSET FOR START ADDR    00316000
*                                        OF ADDIT'L TEXT FOR 1ST BUF */ 00317000
         STH   @10,NEWDELTA                                     03S0703 00318000
*FILLSW = ON;                          /* INIT SW TO EXECUTE LOOP TO    00319000
*                                        CONTINUE FILLING 1ST BUFFER  * 00320000
         OI    FILLSW,B'01000000'                               03S0704 00321000
*DO WHILE FILLSW=ON;                   /* FILL 1ST BUFFER TXT AREA  */  00322000
         B     @DE00705                                         03S0705 00323000
@DL00705 DS    0H                                               03S0706 00324000
* IF P = 0 | P->FDBDATCT = 0           /* IF NO MORE TEXT IN BUF CHAIN* 00325000
*   THEN FILLSW = OFF;                 /* RESET FILL 1ST BUFR SW */     00326000
         L     @10,P                                            03S0706 00327000
