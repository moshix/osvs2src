*        PHYSSIZE = ATCHBSIZ - LENGTH(SRBSECT);/*SUBTR SRB   @XM42102*/ 00076000
         L     @10,VTAMCVTA                                     01S0647 00077000
         LH    PHYSSIZE,ATCHBSIZ(,@10)                          01S0647 00078000
         SL    PHYSSIZE,@CF04681                                01S0647 00079000
*        IF PHYSSIZE >= BUF256       /* DETERMINE BUF SIZE   @XM42102*/ 00080000
*          THEN GTFLIM = BUF256;     /* GTFLIMIT=256         @XA08633*/ 00081000
         LA    @10,256                                          01S0648 00082000
         CR    PHYSSIZE,@10                                     01S0648 00083000
         BL    @RF00648                                         01S0648 00084000
         STH   @10,GTFLIM                                       01S0649 00085000
*          ELSE GTFLIM = PHYSSIZE;      /* GTFLIMIT=BUF SIZE @XM42102*/ 00086000
*                                                               01S0650 00087000
         B     @RC00648                                         01S0650 00088000
@RF00648 STH   PHYSSIZE,GTFLIM                                  01S0650 00089000
*        IF (R3->NCBTYPE = ICTYPE)     /* IF NCB IS AN ICNCB          * 00090000
*         & (R5 = R3->NCBFMCBA)        /*  & FMCB BELONGS TO VARY     * 00091000
*         & (RPHCRR->CRACRR->ZCROB = OFF)      /*  & DATA IS INBOUND */ 00092000
*         & NSRUNETS = SRURLNTR        /* & RU IS FOR FUNCT MGMT */     00093000
*         THEN DO;                     /* RECORD NCP LINE BUFFERS     * 00094000
@RC00648 CLI   NCBTYPE(R3),X'06'                                01S0651 00095000
         BNE   @RF00651                                         01S0651 00096000
         C     R5,NCBFMCBA(,R3)                                 01S0651 00097000
         BNE   @RF00651                                         01S0651 00098000
         L     @10,RPHCRR(,RPHPTR)                              01S0651 00099000
         L     @10,CRACRR(,@10)                                 01S0651 00100000
         TM    ZCROB(@10),B'00010000'                           01S0651 00101000
         BNZ   @RF00651                                         01S0651 00102000
         L     @10,GTFDATAA                                     01S0651 00103000
         L     @10,FDBDATAA(,@10)                               01S0651 00104000
         CLC   NSRUNETS(3,@10),@CB03669                         01S0651 00105000
         BNE   @RF00651                                         01S0651 00106000
*/***********************************************************/  01S0653 00107000
*/*    1) TEST IF SLOWDOWN IS IN PROGRESS                   */  01S0653 00108000
*/*    2) IF SLOWDOWN IN PROGRESS RESET RN RDTE             */  01S0653 00109000
*/*       RPRLTASN TO OFF AND RESET LINE RDTE               */  01S0653 00110000
*/*       RPRLTASN TO OFF. THIS WILL ENABLE                 */  01S0653 00111000
*/*       RESTARTING LINE TRACE AFTER SLOWDOWN              */  01S0653 00112000
*/*       IS ENDED.                                         */  01S0653 00113000
*/***********************************************************/  01S0653 00114000
*IF TRRSTAT(7:8) = '11'B                 /*TEST FOR SLOWDOWN @XM10332*/ 00115000
*  THEN DO;                              /*SLOWDOWN ON       @XM10332*/ 00116000
         TM    TRRSTAT(@10),B'00000011'                         01S0653 00117000
         BNO   @RF00653                                         01S0653 00118000
*       RDTPTR = NCBRDTE;                /*RN RDTE           @XM10332*/ 00119000
         L     RDTPTR,NCBRDTE(,R3)                              01S0655 00120000
*       RPRLTASN = OFF;                  /*TURN OFF LINE TRC @XM10332*/ 00121000
         NI    RPRLTASN(RDTPTR),B'01111111'                     01S0656 00122000
*       DO WHILE RPRENTED ^=ON;          /*LOOP THRU RDTES   @XM10332*/ 00123000
         B     @DE00657                                         01S0657 00124000
@DL00657 DS    0H                                               01S0658 00125000
*         IF RPRENTRY = RPRENTLN         /*LINE RDTE         @XM10332*/ 00126000
*           THEN RPRLTASN = OFF;         /*TURN OFF LINE TRC @XM10332*/ 00127000
         CLI   RPRENTRY(RDTPTR),X'50'                           01S0658 00128000
         BNE   @RF00658                                         01S0658 00129000
         NI    RPRLTASN(RDTPTR),B'01111111'                     01S0659 00130000
*           ELSE;                        /*DUMMY ELSE        @XM10332*/ 00131000
@RF00658 DS    0H                                               01S0661 00132000
*         RDTPTR = RDTPTR + RPRELEN;     /*NEXT RDTE         @XM10332*/ 00133000
         AH    RDTPTR,RPRELEN(,RDTPTR)                          01S0661 00134000
*       END;                             /*END DOWHILE       @XM10332*/ 00135000
@DE00657 TM    RPRENTED(RDTPTR),B'00000100'                     01S0662 00136000
         BNO   @DL00657                                         01S0662 00137000
*      END;                              /*END SLOWDOWN      @XM10332*/ 00138000
*  ELSE;                                 /*DUMMY ELSE        @XM10332*/ 00139000
@RF00653 DS    0H                                               01S0665 00140000
*  IF TRRFMT2 = ON       /* IS THIS A FORMAT 2 LINE TRACE    @Z40GKZA*/ 00141000
*    THEN DO;            /* YES                              @Z40GKZA*/ 00142000
         L     @10,GTFDATAA                                     01S0665 00143000
         L     @10,FDBDATAA(,@10)                               01S0665 00144000
         TM    TRRFMT2(@10),B'00000010'                         01S0665 00145000
         BNO   @RF00665                                         01S0665 00146000
*       RFY (R10,R13) RSTD;                                /*@Z40GKZA*/ 00147000
*       R10 = ADDR(TLWRK);  /* PASS WORK AREA ADDRESS        @Z40GKZA*/ 00148000
         LA    R10,TLWRK                                        01S0668 00149000
*       R13 = ADDR(TLSAVE); /* PASS SAVE AREA ADDRESS        @Z40GKZA*/ 00150000
         LA    R13,TLSAVE                                       01S0669 00151000
*       CALL ISTRACTL;      /* PROCESS THE BUFFERS           @Z40GKZA*/ 00152000
         L     @15,@CV04649                                     01S0670 00153000
         BALR  @14,@15                                          01S0670 00154000
*       RFY (R10,R13) UNRSTD;                              /*@Z40GKZA*/ 00155000
*     END;                                                 /*@Z40GKZA*/ 00156000
*    ELSE DO;            /* NO                               @Z40GKZA*/ 00157000
         B     @RC00665                                         01S0673 00158000
@RF00665 DS    0H                                               01S0674 00159000
*                                                               02S0674 00160000
*/********************************************************************* 00161000
*/* RECORD DATA FROM NCP LINE BUFFERS                                 * 00162000
*/*  1. SETUP PTR TO NEXT LINE BUFFER IN BUFFER CHAIN                 * 00163000
*/*     INIT LOOP SW TO EXECUTE RECORDING LOOP                        * 00164000
*/*  2. EXECUTE RECORDING LOOP UNTIL ALL TEXT FROM ALL BUFFERS        * 00165000
*/*    HAS BEEN RECODED                                               * 00166000
*/*      A. CALC LENGTH OF LOGICAL BUFFER & GTF DATA BLOCK            * 00167000
*/*      B. CALC ADDR OF TRAILER TO BE PLACED AT END OF GTF DATA      * 00168000
*/*      C. SAVE ANY DATA THAT WILL BE OVERLAYED BY THE TRAILER       * 00169000
*/*      D. INIT TRAILER ISTTRF = 0                                   * 00170000
*/*      E. PUT DEST NODE NAME INTO TRAILER                           * 00171000
*/*      F. ENGAGE GTF HOOK VIA GTRACE                                * 00172000
*/*      G. RESTORE ANY DATA,THAT WAS PREVIOUSLY SAVED,INTO           * 00173000
*/*        AREA OCCUPIED BY THE TRAILER                               * 00174000
*/*      H. CALC TEXT REMAINING IN LOGICAL BUFFER                     * 00175000
*/*      J. IF NO TEXT REMAINS TO BE RECORDED                         * 00176000
*/*           THEN RESET LOOP SW TO TERMINATE RECORDING               * 00177000
*/*           ELSE MOVE AS MUCH OF THE REMAINDER AS POSSIBLE          * 00178000
*/*                INTO THE 1ST BUFFER & CONTINUE RECORDING           * 00179000
*/********************************************************************* 00180000
*                                                               02S0674 00181000
*LINETRSW = ON;                        /* SET LINE TRACE SW */  02S0674 00182000
*RECORDSW = ON;                        /* INIT RECORDING LOOP SW */     00183000
         OI    LINETRSW,B'10100000'                             02S0675 00184000
*TEXTPTR = ADDR(TRRDATA);             /* STRT @ TEXT IN 1ST BUFFER */   00185000
         L     @10,GTFDATAA                                     02S0676 00186000
         L     @07,FDBDATAA(,@10)                               02S0676 00187000
         LA    @07,TRRDATA(,@07)                                02S0676 00188000
         ST    @07,TEXTPTR                                      02S0676 00189000
*P = FDBFDBA;                          /* PTR TO NEXT BUFFER IN CHAIN * 00190000
         L     @10,FDBFDBA(,@10)                                02S0677 00191000
         ST    @10,P                                            02S0677 00192000
*DO WHILE RECORDSW=ON;                                          02S0678 00193000
         B     @DE00678                                         02S0678 00194000
@DL00678 DS    0H                                               02S0679 00195000
*CALL DATAXTNT;                        /* CALC LGCL BUF & GTF DATA LEN* 00196000
         BAL   @14,DATAXTNT                                     02S0679 00197000
*CALL ADDRTRF;                         /* CALC ADDR OF TRLR ISTTRF    * 00198000
         BAL   @14,ADDRTRF                                      02S0680 00199000
*RFY BUFDATA BASED(TRFPTR);                                     02S0681 00200000
*IF TRFDELTA > 0                                                02S0682 00201000
*  THEN DATASAVE = BUFDATA(1:TRFDELTA);  /* SAVE TEXT THAT      02S0682 00202000
*                                          WILL BE OVERLAYED */ 02S0682 00203000
         LH    @10,TRFDELTA                                     02S0682 00204000
         LTR   @10,@10                                          02S0682 00205000
         BNP   @RF00682                                         02S0682 00206000
         MVI   DATASAVE+1,C' '                                  02S0683 00207000
