         CR    @10,@02                                          03S0706 00329000
         BE    @RT00706                                         03S0706 00330000
         CH    @02,FDBDATCT(,@10)                               03S0706 00331000
         BNE   @RF00706                                         03S0706 00332000
@RT00706 DS    0H                                               03S0707 00333000
         NI    FILLSW,B'10111111'                               03S0707 00334000
*   ELSE DO;                                                    03S0708 00335000
         B     @RC00706                                         03S0708 00336000
@RF00706 DS    0H                                               03S0709 00337000
*         NEWDATAA = TEXTPTR + NEWDELTA;        /* START ADDR FOR       00338000
*                                                     ADDIT'L TEXT   */ 00339000
