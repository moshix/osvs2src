*Q2 = RTEXTA;                          /* INIT SENDING FIELD PTR WITH   00300000
*                                        ADDR OF TEXT LEFT IN 1ST BUFR* 00301000
         L     @10,RTEXTA                                       03S0699 00302000
         ST    @10,Q2                                           03S0699 00303000
*CALL MOVEDATA;                        /* MOVE TEXT TO START FILLING    00304000
*                                        1ST BUFFER           */        00305000
         BAL   @14,MOVEDATA                                     03S0700 00306000
*FILLCT = GTFTXTCT;                    /* AMNT TEXT RQRD TO FILL        00307000
*                                          1ST BUFFER            */     00308000
         LH    FILLCT,GTFTXTCT                                  03S0701 00309000
*FDBDATCT = RTEXTCT + RULENGTH;        /* UPDATE DATA COUNT IN 1ST BUF* 00310000
         LH    @10,RTEXTCT                                      03S0702 00311000
         LH    @02,RULENGTH                                     03S0702 00312000
