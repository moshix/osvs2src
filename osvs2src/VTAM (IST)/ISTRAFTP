         TITLE 'ISTRAFTP - OS/VS TPIOS BUFFER TRACE                    *00001000
                        '                                               00002000
ISTRAFTP CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'ISTRAFTP  76.205'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                               01S0002 00010000
         STM R0,R15,RPHWORK(R1)   SAVE REGS 0 - 15                      00011000
         BALR  R12,0          SET UP ADDRESSABILITY                     00012000
         USING *,R12                                                    00013000
         SR     R2,R2                                                   00014000
         ICM R2,7,@SIZDATD+1      GET DYNAMIC AREA SIZE                 00015000
*                                                               01S0616 00016000
* /*GETSTOR SIZE(R2)SAVE(NO)TYPE(FIXED)*/                       01S0617 00017000
*  DO;                                                          01S0617 00018000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)  */               01S0618 00019000
*/* END OF ATBUILD */                                           01S0618 00020000
* RFY (GPR00P,GPR01P,GPR02P,GPR03P) RSTD;                       01S0618 00021000
* GPR00F = R2;                                         /*SIZE IN REG0*/ 00022000
         LR    GPR00F,R2                                        01S0619 00023000
* DCL 1 CMZA0017 FIXED(31) STATIC LOCAL,                        01S0620 00024000
*       2 * FIXED(8) INIT(0),                        /* OPTION FLAGS */ 00025000
*       2 * FIXED(8) INIT(96),                        /* STORAGE KEY */ 00026000
*       2 * FIXED(8) INIT(227),                    /* SUBPOOL NUMBER */ 00027000
*       2 * FIXED(8) INIT('00'X);                    /* REQUEST TYPE */ 00028000
* GPR03F = CMZA0017;                                            01S0621 00029000
*                                                               01S0621 00030000
         L     GPR03F,CMZA0017                                  01S0621 00031000
* DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR02P)) */                 01S0622 00032000
*   GPR02P = ATCLCPTR; /*SET UP ATCVT POINTER*/                 01S0623 00033000
         L     GPR02P,ATCLCPTR                                  01S0623 00034000
* END; /*LISATCVT*/                                             01S0624 00035000
* CALL GPR02P -> ATCGTSTR;                            /*CALL ISTORFSR*/ 00036000
         L     @15,ATCGTSTR(,GPR02P)                            01S0625 00037000
         BALR  @14,@15                                          01S0625 00038000
* RFY (GPR00P,GPR01P,GPR02P,GPR03P) UNRSTD;                     01S0626 00039000
* END ;                                              /*END OF GETSTOR*/ 00040000
**GET DATA AREA      @Z40GKZA*/                                 01S0627 00041000
*RFY R2 UNRSTD;                                                 01S0628 00042000
*RFY R15 RSTD;                                                  01S0629 00043000
*IF R15 = 0                            /* IF CORE OBTAINED */   01S0630 00044000
*  THEN DO;                                                     01S0630 00045000
