         BCTR  @10,0                                            02S0683 00209000
         EX    @10,@SM04685                                     02S0683 00210000
*  ELSE;                                                        02S0684 00211000
@RF00682 DS    0H                                               02S0685 00212000
*ISTTRF = '0'B;                        /* INIT TRAILER = 0            * 00213000
         XC    ISTTRF+1(16,TRFPTR),ISTTRF+1(TRFPTR)             02S0685 00214000
         MVI   ISTTRF(TRFPTR),B'00000000'                       02S0685 00215000
*TRFDNODE = R4->NCBRDTE->RPRNAME;      /* DEST NODE NAME INTO TRLR    * 00216000
         L     @10,NCBRDTE(,R4)                                 02S0686 00217000
         MVC   TRFDNODE(8,TRFPTR),RPRNAME(@10)                  02S0686 00218000
*TRFINB = ON;                           /* SET INBOUND FLAG IN TRLR */  00219000
         OI    TRFINB(TRFPTR),B'10000000'                       02S0687 00220000
*R1 = ADDR(GTFPLIST);                   /* POINT TO GTF PARM LIST */    00221000
*                                                               02S0688 00222000
         LA    R1,GTFPLIST                                      02S0688 00223000
*GEN;                                                           02S0689 00224000
*                                                               02S0689 00225000
         GTRACE ID=ISTLNEID,MF=(E,(1))                                  00226000
*IF TRFDELTA > 0                                                02S0690 00227000
*  THEN BUFDATA(1:TRFDELTA) = DATASAVE(1:TRFDELTA); /* RESTORE TEXT */  00228000
         LH    @10,TRFDELTA                                     02S0690 00229000
