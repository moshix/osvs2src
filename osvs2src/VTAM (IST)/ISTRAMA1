*                           SWAP7REG=COMP9REG+MPSRMGO1;/* PREPARE TO    00330000
*                                      INCREMENT COUNT FIELD @ZM44506*/ 00331000
         LR    SWAP7REG,COMP9REG                                   0572 00332000
         AL    SWAP7REG,@CF04662                                   0572 00333000
*                           IF FULL8REG>=EMPTY/* IF NOT NEGATIVE,  0573 00334000
*                                      QUEUE IS UNGATED      @ZM44506*/ 00335000
*                             THEN                                 0573 00336000
         LTR   FULL8REG,FULL8REG                                   0573 00337000
