*                             DO;   /* SO GATE & BUMP CNT    @ZM44506*/ 00339000
*                               SWAP6REG=COMP8REG|HIBITON;/* GATE RDY   00340000
*                                      Q FOR SWAP                    */ 00341000
         LR    SWAP6REG,COMP8REG                                   0575 00342000
         O     SWAP6REG,@CF02061                                   0575 00343000
*                               CDS(COMP8REG,SWAP6REG,MPSRQFGA);/* 0576 00344000
*                                      UPDATE FIELDS IN MPST @ZM44506*/ 00345000
         CDS   COMP8REG,SWAP6REG,MPSRQFGA(MPS3PTR)                 0576 00346000
*                               BC(FAIL,TRAMLP1);/* SOMETHING CHANGED   00347000
*                                                            @ZM44506*/ 00348000
         BC    7,TRAMLP1                                           0577 00349000
*                             END;  /* END READY Q UNGATED   @ZM44506*/ 00350000
*                           ELSE                                   0579 00351000
*                             DO;   /* ALREADY GATED         @ZM44506*/ 00352000
         B     @RC00573                                            0579 00353000
@RF00573 DS    0H                                                  0580 00354000
*                               IF(COMP9REG&MPSRCNT)^=EMPTY/* ISOLATE   00355000
