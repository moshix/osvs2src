*   RFY                                                            0481 00039000
*     R13 UNRSTD;                                                  0481 00040000
*                                                                  0481 00041000
*/*                                                                  */ 00042000
*                                                                  0482 00043000
*   RFY                                                            0482 00044000
*    (ATCV2PTR) RSTD;                                              0482 00045000
*   DO;                             /* LISATCVT                    0483 00046000
*                                      CVTBASE(PAGEABLE,(ATCV2PTR))  */ 00047000
*     ATCV2PTR=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00048000
         L     ATCV2PTR,ATCLCPTR                                   0484 00049000
*   END;                            /* LISATCVT ESTABLISH ATCVT BASE */ 00050000
*/*                                                                  */ 00051000
*                                                                  0486 00052000
*   IF ATCV2PTR^=EMPTY              /* TEST FOR EXISTENCE OF ATCVT   */ 00053000
*       &ATCACTIV^=OFF              /* TEST FOR VTAM ACTIVE          */ 00054000
*       &RMPLASCB^=EMPTY            /* TEST FOR ASCB EXISTING        */ 00055000
