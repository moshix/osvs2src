         TITLE 'ISTRAMA1 - TASK TERMINATION VS2-VTAM                   *00001000
                        '                                               00002000
ISTRAMA1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTRAMA1  78.135'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         LM    @00,@01,20(@13)                                     0001 00018000
*   PGDQLIST(1:12)=PGDQLIST(1:12)&&PGDQLIST(1:12);/* CLEAR GETMAINED    00019000
*                                      MACRO PARAMETER LIST  @ZM41509*/ 00020000
         XC    PGDQLIST(12),PGDQLIST                               0474 00021000
*   RFY                                                            0475 00022000
*    (RMPL5PTR,                                                    0475 00023000
*     RMWAAPTR) RSTD;                                              0475 00024000
*   RMPL5PTR=PARMPTR;               /* ESTABLISH BASE FOR RMPL       */ 00025000
