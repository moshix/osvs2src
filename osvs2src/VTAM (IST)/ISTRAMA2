         ST    @07,MPSTPTR                                         0549 00353000
*   MPSRQUEG=ON;                    /* GATE THE MPST READY QUE     0550 00354000
*                                                            @Z401241*/ 00355000
         OI    MPSRQUEG(@07),B'10000000'                           0550 00356000
*/*  SPIN THROUGH ALL DCES CHAINED OFF THIS MPST                     */ 00357000
*/*  TURNING ON THE ABEND IN PROCESS FLAG IN                         */ 00358000
*/*  EACH ASSOCIATED DEB AND PST.                                    */ 00359000
*                                                                  0551 00360000
*   DCEPTR=MPSDBTBL;                /* PT TO FIRST DCE               */ 00361000
         L     @10,MPSDBTBL(,@07)                                  0551 00362000
