         TITLE 'ISTRAMA2 - MEMORY TERMINATION VS2-VTAM                 *00001000
                        '                                               00002000
ISTRAMA2 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTRAMA2  78.171'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  LA    @08,4095(,@12)                                      0001 00012000
         USING @PSTART,@12                                         0001 00013000
         USING @PSTART+4095,@08                                    0001 00014000
         L     @00,@SIZDATD                                        0001 00015000
         GETMAIN  R,LV=(0)                                              00016000
         LR    @11,@01                                             0001 00017000
         USING @DATD,@11                                           0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
RBPRFXLN EQU 64                                                         00020000
RBPRFLNA EQU 32                                                         00021000
PRBLEN   EQU 136                                                        00022000
SIRBLEN  EQU 200                                                        00023000
TIRBLEN  EQU 136                                                        00024000
IRBLEN   EQU 128                                                        00025000
SVRBLEN  EQU 224                                                        00026000
         EJECT                                                          00027000
*   RMPLPTR=PARMPTR;                /* SAVE POINTER TO RMPL          */ 00028000
         L     @10,PARMPTR(,R1)                                    0468 00029000
         ST    @10,RMPLPTR                                         0468 00030000
*                                                                  0469 00031000
*   /*****************************************************************/ 00032000
*   /*                                                               */ 00033000
*   /* VTAMACTV - ENTERED BY MAINLINE TO INSURE ISTAVT AND ATCVT ARE */ 00034000
*   /* AVAILABLE, IF THE ISTAVT IS NOT AVAIABLE IMMEDIATE RETURN IS  */ 00035000
*   /* MADE TO RTM WITH NO PROCESS TAKEN PLACE. IF THE AVT IS        */ 00036000
*   /* AVAILABLE A CHECK IS MADE FOR THE ATCVT, IF THE ATCVT IS NOT  */ 00037000
*   /* AVAILABLE, THE ISTAVT IS PAGEFREED, STORAGE FREEMAINED AND THE*/ 00038000
*   /* POINTER IN THE CVT IS ZEROED. EXIT IS THEN MADE TO RTM.       */ 00039000
*   /*                                                               */ 00040000
*   /*****************************************************************/ 00041000
*                                                                  0469 00042000
*   IF CVTATCVT=ZERO THEN           /* IS THE ISTAVT AVAILABLE       */ 00043000
         L     @07,CVTPTR                                          0469 00044000
         L     @07,CVTEXT2(,@07)                                   0469 00045000
         L     @05,CVTATCVT(,@07)                                  0469 00046000
         LTR   @05,@05                                             0469 00047000
         BZ    @RT00469                                            0469 00048000
*     GO TO RETRTM;                 /* NO WORK AVT NOT AVAIL - EXIT  */ 00049000
*                                                                  0470 00050000
*   /*****************************************************************/ 00051000
*   /*                                                               */ 00052000
*   /* IS VTAM MEMORY BEING TERMINATED                               */ 00053000
*   /*                                                               */ 00054000
*   /*****************************************************************/ 00055000
*                                                                  0471 00056000
*   IF AVTASID=RMPLASID             /* VTAM TERMINATED       @ZM32028*/ 00057000
