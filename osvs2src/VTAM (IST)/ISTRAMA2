*   DO WHILE DCEPTR^=0;                                            0552 00364000
         B     @DE00552                                            0552 00365000
@DL00552 DS    0H                                                  0553 00366000
*     ACDABEND=ON;                  /* FLAG ON IN ACDEB              */ 00367000
