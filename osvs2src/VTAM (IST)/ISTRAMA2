*   ;                                                              0546 00340000
MPSTFIND DS    0H                                                  0547 00341000
*   RFY                                                            0547 00342000
*    (MPCNTREG);                                                   0547 00343000
*   MPSTFND2=MPLADDR(ELEMREG);      /* PTR TO MPST IN RMWA           */ 00344000
         L     @10,RMPLPTR                                         0548 00345000
