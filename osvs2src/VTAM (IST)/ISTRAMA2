*           DO;                     /* MODESET EXTKEY(VTAM)          */ 00186000
*             SPKA(96);             /* SET PSW KEY                   */ 00187000
         LA    @10,96                                              0503 00188000
         SPKA  0(@10)                                              0503 00189000
*           END;                    /* MODESET EXTKEY(VTAM) VTAMS KEY   00190000
*                                                            @Z302044*/ 00191000
*                                                                  0504 00192000
*           /*********************************************************/ 00193000
