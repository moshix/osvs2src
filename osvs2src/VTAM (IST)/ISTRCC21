*           CRRFMCB=FMCBPTR;        /* SAVE FMCB POINTER             */ 00754000
@RF00621 ST    FMCBPTR,CRRFMCB(,CRRPTR)                            0663 00755000
*           CRRRPLS=ONEB;           /* SET RPL SATISFIED             */ 00756000
         OI    CRRRPLS(CRRPTR),B'10000000'                         0664 00757000
*           CRRFSB=FSBPTR;          /* SAVE FSB POINTER              */ 00758000
