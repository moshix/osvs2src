*               CALL DEQFSB;        /* DEQ OLDEST FSB ON DFASY Q     */ 00473000
         BAL   @14,DEQFSB                                          0551 00474000
*             END;                  /* GET FSB POINTER               */ 00475000
*           ELSE                                                   0553 00476000
*             DO;                   /* NO                            */ 00477000
         B     @RC00527                                            0553 00478000
@RF00527 DS    0H                                                  0554 00479000
*               FMCBPTR=ACDREFQ&ZAPREG;/* GET RESP DATA QUEUE        */ 00480000
