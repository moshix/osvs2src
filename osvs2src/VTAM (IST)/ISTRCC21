         TITLE 'ISTRCC21: CONTROL LAYER RECEIVE OUTBOUND PROCESSOR     *00001000
                        '                                               00002000
ISTRCC21 CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTRCC21  77.320'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*   ;                               /* INITIATE ASSEMBLER COMMENTING */ 00013000
*                                                                  0420 00014000
*/********************************************************************  00015000
*                                                                    */ 00016000
*/*                                                                  */ 00017000
*/*  CRRINIT - SEGMENT OF ISTRCC21                                   */ 00018000
*/*                                                                  */ 00019000
*/*          CRRINIT ESTABLISHES THE ADDRESSABILITY TO               */ 00020000
*/*          THE COMPONENT RECOVERY RECORD (CRR), THE                */ 00021000
*/*          REQUEST PARAMETER LIST (RPL), THE ACCESS                */ 00022000
*/*          METHOD CVT (ATCVT), AND THE MAJOR CONTROL               */ 00023000
*/*          BLOCK ( EITHER THE ACDEB OR THE FMCB,                   */ 00024000
*/*          DEPENDING UPON WHETHER THE REQUEST IS ANY               */ 00025000
*/*          OR SPECIFIC, RESPECTIVELY ) WHOSE TYPE IS               */ 00026000
*/*          UNKNOWN DURING THE EXECUTION OF CRRINIT.                */ 00027000
*/*          THE CRR RPL SATISFIED SWITCH, THE THREE RTYPE           */ 00028000
*/*          SWITCHES (CRRDFSYN, CRRDFASY, CRRRESP   )               */ 00029000
*/*          ARE CLEARED TO NOT ACTIVE STATUS.                       */ 00030000
*/*          THE MODULE AUDIT FIELD IS CLEARED AND THEN              */ 00031000
*/*          THE MODULE AUDIT AND AUDIT TRAIL SWITCHES               */ 00032000
*/*          ARE SET. THE RPL REQUEST TYPE IS CHECKED .              */ 00033000
*/*          IF THE REQUEST IS FOR RECORD DATA, THE                  */ 00034000
*/*          CRR RTYPE BITS ARE SET TO MATCH THE RPL RTYPE           */ 00035000
*/*          BITS.                                                   */ 00036000
*/*          IF  NO RPL RTYPES ARE ACTIVE, THEN THE CRR              */ 00037000
*/*          MAJOR RETURN CODE IS SET TO INDICATE THAT A             */ 00038000
*/*          LOGICAL ERROR HAS OCCURED WITH THE MINOR                */ 00039000
*/*          RETURN CODE SET TO INDICATE THAT NO RTYPE               */ 00040000
*/*          WAS SPECIFIED. NOTE: THE RPL RTYPE BIT FOR              */ 00041000
*/*          SYNCHRONOUS DATA REQUESTS (RPLNFSYN) IS A               */ 00042000
*/*          NEGATIVE LOGIC BIT, IE. '0'B INDICATES A                */ 00043000
*/*          REQUEST AND IS THE DEFAULT.                             */ 00044000
*/*                                                                  */ 00045000
*/********************************************************************  00046000
*                                                                    */ 00047000
*                                                                  0420 00048000
*   RESPECIFY                                                      0420 00049000
*     ISTCRA BASED(RPHCRR);         /* ADDRESS THE CRA               */ 00050000
*   RESPECIFY                                                      0421 00051000
*     ISTDCCRR BASED(CRACRR);       /* ADDRESS THE DCCRR             */ 00052000
*   CRRPTR=ADDR(CRRSPACE);          /* GET THE CRR POINTER           */ 00053000
         L     @15,RPHCRR(,RPHPTR)                                 0422 00054000
         L     CRRPTR,CRACRR(,@15)                                 0422 00055000
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0422 00056000
*   RESPECIFY                                                      0423 00057000
*     CRRSAREA BASED(CRRPTR);       /* ADDRESS THE CRR               */ 00058000
*   RPLPTR=CRRARPL;                 /* GET THE RPL POINTER           */ 00059000
         L     RPLPTR,CRRARPL(,CRRPTR)                             0424 00060000
*   RESPECIFY                                                      0425 00061000
*     IFGRPL BASED(RPLPTR);         /* ADDRESS THE RPL               */ 00062000
*   RPLRLEN=0;                      /* ZERO RECORD LENGTH@XM9777K    */ 00063000
         SLR   @15,@15                                             0426 00064000
         ST    @15,RPLRLEN(,RPLPTR)                                0426 00065000
*   RESPECIFY                                                      0427 00066000
*     ISTATCVT BASED(CRRATCVT);     /* ADDRESS THE CVT               */ 00067000
*   FMCBPTR=RPHMAJCB;               /* GET MAJ CB POINTER            */ 00068000
         L     FMCBPTR,RPHMAJCB-1(,RPHPTR)                         0428 00069000
         LA    FMCBPTR,0(,FMCBPTR)                                 0428 00070000
*   CRRRPLS=ZERB;                   /* CLEAR RPL SATISFIED           */ 00071000
*   CRRDFSYN=ZERB;                  /* CLEAR SYN RTYPE               */ 00072000
*   CRRDFASY=ZERB;                  /* CLEAR ASY RTYP                */ 00073000
*   CRRRESP=ZERB;                   /* CLEAR RESP RTYPE              */ 00074000
         NI    CRRRPLS(CRRPTR),B'01110001'                         0432 00075000
*   CRRMAS=''B;                     /* CLEAR MODULE AUDIT            */ 00076000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0433 00077000
*   CRRRC21=ONEB;                   /* SET AUDIT TRAIL               */ 00078000
         OI    CRRRC21(CRRPTR),B'10000000'                         0434 00079000
*   CRRRM21=ONEB;                   /* SET MODULE AUDIT              */ 00080000
         OI    CRRRM21(CRRPTR),B'10000000'                         0435 00081000
*   IF RPLREQ='1D'X                 /* IS REQUEST A READ ?           */ 00082000
*     THEN                                                         0436 00083000
         CLI   RPLREQ(RPLPTR),29                                   0436 00084000
         BNE   @RF00436                                            0436 00085000
*     CRRDFSYN=ONEB;                /* YES, SET NSNA RTYPE           */ 00086000
         OI    CRRDFSYN(CRRPTR),B'00000010'                        0437 00087000
*   ELSE                                                           0438 00088000
*     DO;                           /* NO                            */ 00089000
         B     @RC00436                                            0438 00090000
@RF00436 DS    0H                                                  0439 00091000
*       IF RPLRRESP=ONEB|RPLNFSYN=ZERB|RPLDFASY=ONEB THEN          0439 00092000
         TM    RPLRRESP+16(RPLPTR),B'00001010'                     0439 00093000
         BNZ   @RT00439                                            0439 00094000
         TM    RPLNFSYN+16(RPLPTR),B'00000100'                     0439 00095000
         BO    @RF00439                                            0439 00096000
@RT00439 DS    0H                                                  0440 00097000
*         DO;                       /* YES                           */ 00098000
*           IF RPLRRESP=ONEB        /* RESPONSE ?                    */ 00099000
*             THEN                                                 0441 00100000
         TM    RPLRRESP+16(RPLPTR),B'00001000'                     0441 00101000
         BNO   @RF00441                                            0441 00102000
*             CRRRESP=ONEB;         /* YES, SET RESP RTYPE           */ 00103000
         OI    CRRRESP(CRRPTR),B'00000100'                         0442 00104000
*           IF RPLNFSYN=ZERB        /* SYNCHRONOUS ?                 */ 00105000
*             THEN                                                 0443 00106000
@RF00441 TM    RPLNFSYN+16(RPLPTR),B'00000100'                     0443 00107000
         BNZ   @RF00443                                            0443 00108000
*             CRRDFSYN=ONEB;        /* YES, SET SYN RTYPE            */ 00109000
         OI    CRRDFSYN(CRRPTR),B'00000010'                        0444 00110000
*           IF RPLDFASY=ONEB        /* ASYNCHRONOUS ?                */ 00111000
*             THEN                                                 0445 00112000
@RF00443 TM    RPLDFASY+16(RPLPTR),B'00000010'                     0445 00113000
         BNO   @RF00445                                            0445 00114000
*             CRRDFASY=ONEB;        /* YES, SET ASY RTYPE            */ 00115000
         OI    CRRDFASY(CRRPTR),B'00001000'                        0446 00116000
*         END;                      /* RTYPES SET IN CRR             */ 00117000
*       ELSE                                                       0448 00118000
*         DO;                       /* NO                            */ 00119000
         B     @RC00439                                            0448 00120000
@RF00439 DS    0H                                                  0449 00121000
*           CRRRTNCD=CRRLOGIC;      /* SET LOGIC ERROR               */ 00122000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0449 00123000
*           CRRFDB2=CRRRNORT;       /* SET NO RTYPE                  */ 00124000
         MVI   CRRFDB2(CRRPTR),X'02'                               0450 00125000
*         END;                      /* ERROR RETURN IS SET           */ 00126000
*     END;                          /* CRR IS INITIALIZED            */ 00127000
@RC00439 DS    0H                                                  0453 00128000
*/********************************************************************  00129000
*                                                                    */ 00130000
*/*   END OF SEGMENT CRRINIT                                         */ 00131000
*/********************************************************************  00132000
*                                                                    */ 00133000
*                                                                  0453 00134000
*                                                                  0453 00135000
*/********************************************************************  00136000
*                                                                    */ 00137000
*/*                                                                  */ 00138000
*/*  ISTRCC21 - MAINLINE 1                                           */ 00139000
*/*                                                                  */ 00140000
*/*          THE FIRST PORTION OF THE ISTRCC21 MAINLINE              */ 00141000
*/*          INSURES THAT NO PREVIOUS ERRORS HAVE OCCURED            */ 00142000
*/*          BY TESTING THE CRR MAJOR RETURN CODE (CRRRTNCD)         */ 00143000
*/*          AND ISSUES MACRO TPESC IF ANY HAVE ( THEREBY            */ 00144000
*/*          PASSING CONTROL TO EITHER READ ANY PROCESSOR            */ 00145000
*/*          OR REQUEST FINISH, DEPENDING UPON THE DVT UNDER         */ 00146000
*/*          WHICH ISTRCC21 IS RUNNING. OTHERWISE,                   */ 00147000
*/*          THE MAJOR CONTROL BLOCK TYPE IS CHECKED. IF IT          */ 00148000
*/*          IS THE ACDEB, MACRO VMDESET IS ISSUED TO GET            */ 00149000
*/*          INTO KEY 0 SO THAT MACRO TPLOCK MAY BE ISSUED           */ 00150000
*/*          TO SERIALIZE THE USE OF THE ACDEB. MACRO VMDESET        */ 00151000
*/*          IS THEN ISSUED AGAIN TO RETURN TO VTAM KEY. IF,         */ 00152000
*/*          THE MAJOR CB IS THE FMCB THE LOCK HAS PREVIOUSLY        */ 00153000
*/*          OBTAINED BY REQUEST START (ISTDCC00). WHICH EVER        */ 00154000
*/*          TYPE THE MAJOR CB IS, THE POINTER TO THE RPL            */ 00155000
*/*          WAIT QUEUE IS OBTAINED ( IF THE ACDEB, THE              */ 00156000
*/*          FIELD NAME IS ACDRARQ AND IF FMCB, THE FIELD            */ 00157000
*/*          NAME IS FMCRPLH ).                                      */ 00158000
*/*                                                                  */ 00159000
*/********************************************************************  00160000
*                                                                    */ 00161000
*                                                                  0453 00162000
*   IF CRRRTNCD=CRRNOERR            /* ANY ERROR ?                   */ 00163000
*     THEN                                                         0453 00164000
@RC00436 CLI   CRRRTNCD(CRRPTR),X'00'                              0453 00165000
         BNE   @RF00453                                            0453 00166000
*     DO;                           /* NO                            */ 00167000
*       RESPECIFY                                                  0455 00168000
*         ISTACDEB BASED(RPHMAJCB); /* ADDRESS ACDEB                 */ 00169000
*       RESPECIFY                                                  0456 00170000
*         ISTFMCB BASED(FMCBPTR);   /* ADDRESS FMCB                  */ 00171000
*       RFY                                                        0457 00172000
*         ISTNCB BASED(FMCDNCBA);   /* COVER NCB             @D32CKNL*/ 00173000
*       IF ACDTYPE=ACDID            /* IS MAJ CB ACDEB ?             */ 00174000
*         THEN                                                     0458 00175000
         L     @15,RPHMAJCB-1(,RPHPTR)                             0458 00176000
         LA    @15,0(,@15)                                         0458 00177000
         CLI   ACDTYPE(@15),X'0F'                                  0458 00178000
         BNE   @RF00458                                            0458 00179000
*         DO;                       /* YES                           */ 00180000
*           IF CRRDBLCK=ZERB THEN                                  0460 00181000
         TM    CRRDBLCK(CRRPTR),B'00010000'                        0460 00182000
         BNZ   @RF00460                                            0460 00183000
*             DO;                   /* CHECK IF DEB LCKED    @D33SKD3*/ 00184000
*                                                                  0462 00185000
*/*VMDESET  ((15),ZERO)                                              */ 00186000
*                                                                  0462 00187000
*               DO;                 /* INVOKE MODESET MACRO          */ 00188000
*                 GEN;                                             0463 00189000
         MODESET EXTKEY=ZERO,WORKREG=2                                  00190000
*                 DCL                                              0464 00191000
*                   GPR15F FIXED(31) REG(15);                      0464 00192000
*                 DCL                                              0465 00193000
*                   GPR15P PTR(31) REG(15);                        0465 00194000
*                 RFY                                              0466 00195000
*                  (GPR15P) RSTD;                                  0466 00196000
*                 GPR15F=0;                                        0467 00197000
         SLR   GPR15F,GPR15F                                       0467 00198000
*                 RFY                                              0468 00199000
*                  (GPR15P) UNRSTD; /* PUT RETURN CODE IN RCREG      */ 00200000
*               END;                /* VMDESET GO TO SUPER KEY       */ 00201000
*               RFY                                                0470 00202000
*                (RPLPTR,                                          0470 00203000
*                 CRRPTR) UNRSTD;   /* RELEASE REGS          @D31KKNL*/ 00204000
*/*** START -TPLOCK (DEBLOCK)TYPE(XCLSIVE)- **                       */ 00205000
*                                                                  0471 00206000
*               DO;                                                0471 00207000
*                 DCL                                              0472 00208000
*                   GPR05F FIXED(31) REG(05);                      0472 00209000
*                 DCL                                              0473 00210000
*                   GPR05P PTR(31) REG(05);                        0473 00211000
*                 DCL                                              0474 00212000
*                   GPR06F FIXED(31) REG(06);                      0474 00213000
*                 DCL                                              0475 00214000
*                   GPR06P PTR(31) REG(06);                        0475 00215000
*                 DCL                                              0476 00216000
*                   GPR07F FIXED(31) REG(07);                      0476 00217000
*                 DCL                                              0477 00218000
*                   GPR07P PTR(31) REG(07);                        0477 00219000
*                 DCL                                              0478 00220000
*                   GPR08F FIXED(31) REG(08);                      0478 00221000
*                 DCL                                              0479 00222000
*                   GPR08P PTR(31) REG(08);                        0479 00223000
*                 DCL                                              0480 00224000
*                   GPR09F FIXED(31) REG(09);                      0480 00225000
*                 DCL                                              0481 00226000
*                   GPR09P PTR(31) REG(09);                        0481 00227000
*                 DCL                                              0482 00228000
*                   GPR10F FIXED(31) REG(10);                      0482 00229000
*                 DCL                                              0483 00230000
*                   GPR10P PTR(31) REG(10);                        0483 00231000
*                 DCL                                              0484 00232000
*                   GPR11F FIXED(31) REG(11);                      0484 00233000
*                 DCL                                              0485 00234000
*                   GPR11P PTR(31) REG(11);                        0485 00235000
*                 DCL                                              0486 00236000
*                   GPR12F FIXED(31) REG(12);                      0486 00237000
*                 DCL                                              0487 00238000
*                   GPR12P PTR(31) REG(12);                        0487 00239000
*                 DCL                                              0488 00240000
*                   GPR13F FIXED(31) REG(13);                      0488 00241000
*                 DCL                                              0489 00242000
*                   GPR13P PTR(31) REG(13);                        0489 00243000
*                 DCL                                              0490 00244000
*                   GPR14F FIXED(31) REG(14);                      0490 00245000
*                 DCL                                              0491 00246000
*                   GPR14P PTR(31) REG(14);                        0491 00247000
*                 DCL                                              0492 00248000
*                   GPR00F FIXED(31) REG(00);                      0492 00249000
*                 DCL                                              0493 00250000
*                   GPR00P PTR(31) REG(00);                        0493 00251000
*                 DCL                                              0494 00252000
*                   GPR01F FIXED(31) REG(01);                      0494 00253000
*                 DCL                                              0495 00254000
*                   GPR01P PTR(31) REG(01);                        0495 00255000
*                 DCL                                              0496 00256000
*                   GPR02F FIXED(31) REG(02);                      0496 00257000
*                 DCL                                              0497 00258000
*                   GPR02P PTR(31) REG(02);                        0497 00259000
*                 DCL                                              0498 00260000
*                   GPR03F FIXED(31) REG(03);                      0498 00261000
*                 DCL                                              0499 00262000
*                   GPR03P PTR(31) REG(03);                        0499 00263000
*                 DCL                                              0500 00264000
*                   GPR04F FIXED(31) REG(04);                      0500 00265000
*                 DCL                                              0501 00266000
*                   GPR04P PTR(31) REG(04);                        0501 00267000
*                 RFY                                              0502 00268000
*                  (GPR00P,                                        0502 00269000
*                   GPR01P,                                        0502 00270000
*                   GPR02P,                                        0502 00271000
*                   GPR03P,                                        0502 00272000
*                   GPR04P,                                        0502 00273000
*                   GPR15P) RSTD;                                  0502 00274000
*                 GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(     00275000
*                     RPHWORK);                                    0503 00276000
         STM GPR00P,GPR14P,RPHWORK(GPR01P)                              00277000
*                 GPR02P=ADDR(ACDLOCK);/* SET PARAM - LOCKWORD PTR   */ 00278000
         L     GPR02P,RPHMAJCB-1(,RPHPTR)                          0504 00279000
         LA    GPR02P,0(,GPR02P)                                   0504 00280000
         LA    GPR02P,ACDLOCK(,GPR02P)                             0504 00281000
*                 GPR04P=GPR01P->RPHCRR;/* SET PARAM - ISTCRA PTR    */ 00282000
         L     GPR04P,RPHCRR(,GPR01P)                              0505 00283000
*                 GPR00P=CRALMS05;  /* SET PARAM - LOCK LEVEL FLAG   */ 00284000
         LA    GPR00P,16                                           0506 00285000
*                 GPR03P=ADDR(GPR04P->CRAL5PTR);/* SET PARAM - LOCK     00286000
*                                      LEVEL PTR                     */ 00287000
         LA    GPR03P,CRAL5PTR(,GPR04P)                            0507 00288000
*                 GPR15P=ATCLCPTR->ATCAP36;/* ADDRESS OF LOCK ROUTINE*/ 00289000
         L     @10,ATCLCPTR                                        0508 00290000
         L     GPR15P,ATCAP36(,@10)                                0508 00291000
*                 DCL                                              0509 00292000
*                   FMZL0008 BASED(GPR15P) LABEL VALUERANGE(*);    0509 00293000
*                 CALL FMZL0008;                                   0510 00294000
         BALR  @14,@15                                             0510 00295000
*                 GENERATE REFS(GPR00P,GPR01P,GPR14P,RPHWORK) SETS(     00296000
*                     GPR00P,GPR01P,GPR02P,GPR03P,GPR04P,GPR05P,GPR06P, 00297000
*                     GPR07P,GPR08P,GPR09P,GPR10P,GPR11P,GPR12P,GPR13P, 00298000
*                     GPR14P,GPR15P);                              0511 00299000
         LM GPR00P,GPR15P,RPHWORK(GPR01P)                               00300000
*                 RFY                                              0512 00301000
*                  (GPR00P,                                        0512 00302000
*                   GPR01P,                                        0512 00303000
*                   GPR02P,                                        0512 00304000
*                   GPR03P,                                        0512 00305000
*                   GPR04P,                                        0512 00306000
*                   GPR15P) UNRSTD;                                0512 00307000
*               END;                                               0513 00308000
*                                                                  0513 00309000
*/*** END -TPLOCK (DEBLOCK)TYPE(XCLSIVE)- **                         */ 00310000
*                                                                  0514 00311000
*                                   /* LOCK THE DEB                  */ 00312000
*               RFY                                                0514 00313000
*                (RPLPTR,                                          0514 00314000
*                 CRRPTR,                                          0514 00315000
*                 FSBPTR) RSTD;     /* RESTRICT REGS         @D31KKNL*/ 00316000
*               CRRPTR=ADDR(CRRSPACE);/* RE-INIT CRR         @D31KKNL*/ 00317000
         L     @15,RPHCRR(,RPHPTR)                                 0515 00318000
         L     CRRPTR,CRACRR(,@15)                                 0515 00319000
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0515 00320000
*               RPLPTR=CRRARPL;     /* RE-INIT RPL           @D31KKNL*/ 00321000
         L     RPLPTR,CRRARPL(,CRRPTR)                             0516 00322000
*               CRRDBLCK=ONEB;      /* SET DEB LOCK FLG      @D33SKD3*/ 00323000
         OI    CRRDBLCK(CRRPTR),B'00010000'                        0517 00324000
*/*VMDESET  ((15),VTAM)                                              */ 00325000
*                                                                  0518 00326000
*               DO;                 /* INVOKE MODESET MACRO          */ 00327000
*                 GEN;                                             0519 00328000
         MODESET EXTKEY=VTAM,WORKREG=2                                  00329000
*                 RFY                                              0520 00330000
*                  (GPR15P) RSTD;                                  0520 00331000
*                 GPR15F=0;                                        0521 00332000
         SLR   GPR15F,GPR15F                                       0521 00333000
*                 RFY                                              0522 00334000
*                  (GPR15P) UNRSTD; /* PUT RETURN CODE IN RCREG      */ 00335000
*               END;                /* VMDESET GO TO VTAM KEY        */ 00336000
*             END;                  /*                       @D33SKD3*/ 00337000
*/********************************************************************  00338000
*                                                                    */ 00339000
*/*                                                                  */ 00340000
*/*  ISTRCC21 - MAINLINE 3                                           */ 00341000
*/*                                                                  */ 00342000
*/*          THIS PORTION OF THE MAINLINE CODE CHECKS THE            */ 00343000
*/*          MAJOR CONTROL BLOCK TYPE AND IF ACDEB,                  */ 00344000
*/*          EXECUTES SEGMENT FNDATAA, IF FMCB, EXECUTES             */ 00345000
*/*          SEGMENT FNDATAS. THESE SEGMENTS SEARCH THE              */ 00346000
*/*          APPROPRIATE DATA QUEUES FOR DATA WHICH WILL             */ 00347000
*/*          SATISFY THE RPL UNDER CONSIDERATION. AFTER              */ 00348000
*/*          ONE OF THE ABOVE SEGMENTS HAS COMPLETED                 */ 00349000
*/*          EXECUTION, THE CRR RPL SATISFIED SWITCH IS              */ 00350000
*/*          TESTED TO SEE IF QUALIFYING DATA WAS FOUND.             */ 00351000
*/*          IF NO DATA WAS FOUND, THE ACDEB LOCK IS RELEASED        */ 00352000
*/*          IF IT WAS OBTAINED AND A TPESC MACRO IS ISSUED.         */ 00353000
*/*          IF ASYNCHRONOUS OR RESPONSE DATA WAS FOUND              */ 00354000
*/*          A TPDVTS MACRO IS ISSUED TO PASS CONTROL                */ 00355000
*/*          TO THE ASYNCHRONOUS FEEDBACK PROCESSOR. IF              */ 00356000
*/*          NON-SNA OR SYNCHRONOUS SNA DATA WAS FOUND               */ 00357000
*/*          A CHECK FOR OUTBOUND PURGE REQUEST (BIT                 */ 00358000
*/*          FMCOBPRG IN THE FMCB) IS MADE, AND IF SO,               */ 00359000
*/*          SEGMENT PURGE IS EXECUTED. IF AFTER PURGE               */ 00360000
*/*          HAS COMPLETED EXECUTION, THE CRR RPL                    */ 00361000
*/*          SATISFIED SWITCH IS STILL ON, OR IF PURGE               */ 00362000
*/*          WAS NOT REQUESTED, A TPDVTS MACRO IS                    */ 00363000
*/*          ISSUED TO PASS CONTROL TO THE SYNCHRONOUS               */ 00364000
*/*          FEEDBACK PROCESSOR. IF PURGE WAS EXECUTED               */ 00365000
*/*          AND CRR RPL SATISFIED IS OFF ( NO DATA                  */ 00366000
*/*          REMAINS ON THE QUEUE ), ISTRCC63 IS CALLED              */ 00367000
*/*          ( AS AN IN LINE SUBROUTINE ) TO DO COMMON               */ 00368000
*/*          HOUSEKEEPING FUNCTIONS FOR THE SYNCHRONOUS              */ 00369000
*/*          DATA QUEUE AND THE ACDEB LOCK IS RELEASED               */ 00370000
*/*          IF IT WAS OBTAINED AND A TPESC MACRO IS ISSUED          */ 00371000
*/*          TO PASS CONTROL TO THE NEXT PROCESS.                    */ 00372000
*/*                                                                  */ 00373000
*/********************************************************************  00374000
*                                                                    */ 00375000
*                                                                  0525 00376000
*                                                                  0525 00377000
*/********************************************************************  00378000
*                                                                    */ 00379000
*/*                                                                  */ 00380000
*/*  FNDATAA - SEGMENT OF ISTRCC21                                   */ 00381000
*/*                                                                  */ 00382000
*/*           FNDATAA SEARCHES THE DATA QUEUES CHAINED               */ 00383000
*/*           FROM THE ACDEB TO FIND AN FMCB WHICH WILL              */ 00384000
*/*           SATISFY THE RPL UNDER CONSIDERATION ( EITHER           */ 00385000
*/*           THE RPL WHICH INITIATED THIS PASS THROUGH              */ 00386000
*/*           THE CONTROL LAYER OR AN RPL SELECTED FROM              */ 00387000
*/*           THE WAIT QUEUE ). THIS SEARCH IS DONE IN               */ 00388000
*/*           SUCH A MANNER AS TO GIVE PRIORITY TO DFASY             */ 00389000
*/*           DATA, THEN RESP DATA, THEN DFSYN DATA                  */ 00390000
*/*           IF A QUALIFIED FMCB IS FOUND,                          */ 00391000
*/*           THE FMCB LOCK IS OBTAINED, THE CRR RPL                 */ 00392000
*/*           SATISFIED SWITCH (CRRRPLS) IS TURNED ON,               */ 00393000
*/*           THE ADDRESS OF THE FMCB IS PLACED IN THE               */ 00394000
*/*           CRR, THE OLDEST FSB ON THE CORRESPONDING               */ 00395000
*/*           FMCB DATA QUEUE IS OBTAINED, AND THE FSB               */ 00396000
*/*           ADDRESS IS PLACED IN THE CRR. ALL CRR RTYPES           */ 00397000
*/*           EXCEPT THE ONE SATISFIED ARE TURNED OFF                */ 00398000
*/*           IN THE CRR. IF NO FMCB IS FOUND, THE CRRRPLS           */ 00399000
*/*           SWITCH IS LEFT OFF.                                    */ 00400000
*/*                                                                  */ 00401000
*/********************************************************************  00402000
*                                                                    */ 00403000
*                                                                  0525 00404000
*           FSBPTR=0;               /* CLEAR FSB POINTER             */ 00405000
@RF00460 SLR   @15,@15                                             0525 00406000
         LR    FSBPTR,@15                                          0525 00407000
*           FMCBPTR=ACDASFQ&ZAPREG; /* GET ASY DATA QUEUE            */ 00408000
         L     @05,RPHMAJCB-1(,RPHPTR)                             0526 00409000
         LA    @05,0(,@05)                                         0526 00410000
         L     FMCBPTR,ACDASFQ(,@05)                               0526 00411000
         N     FMCBPTR,@CF01332                                    0526 00412000
*           IF CRRDFASY=ONEB&FMCBPTR^=0/* DATA ON QUEUE ?            */ 00413000
*             THEN                                                 0527 00414000
         TM    CRRDFASY(CRRPTR),B'00001000'                        0527 00415000
         BNO   @RF00527                                            0527 00416000
         CR    FMCBPTR,@15                                         0527 00417000
         BE    @RF00527                                            0527 00418000
*             DO;                   /* YES                           */ 00419000
*/* TPFEL (,ACDASFQ) RETPTR(FMCBPTR)CHAIN(FMCFASYA)                  */ 00420000
*                                                                  0529 00421000
*               DO;                 /* BEGIN TPFEL CODE GENERATION   */ 00422000
*                 DCL                                              0530 00423000
*                   FMZCA016 BIT(32) CONSTANT('80000000'X);        0530 00424000
*                 DCL                                              0531 00425000
*                   FMZCB016 BIT(32) CONSTANT('7FFFFFFF'X);        0531 00426000
*                 RFY                                              0532 00427000
*                  (GPR00P,                                        0532 00428000
*                   GPR01P,                                        0532 00429000
*                   GPR14P,                                        0532 00430000
*                   GPR15P) RSTD;                                  0532 00431000
*                 GPR15P=ADDR(ACDASFQ);/* ADDRESS QUEUE POINTER      */ 00432000
         LA    GPR15P,ACDASFQ(,@05)                                0533 00433000
*                 GPR14P=GPR15P->PABWEQA|FMZCA016;                 0534 00434000
         L     @05,@CF01334                                        0534 00435000
         L     GPR14P,PABWEQA(,GPR15P)                             0534 00436000
         OR    GPR14P,@05                                          0534 00437000
*                 IF GPR14P=FMZCA016 THEN                          0535 00438000
         CLR   GPR14P,@05                                          0535 00439000
         BE    @RT00535                                            0535 00440000
*                   ;                                              0536 00441000
*                 ELSE                                             0537 00442000
*                   DO;                                            0537 00443000
