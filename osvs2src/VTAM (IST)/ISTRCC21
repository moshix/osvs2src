*                 GPR14P=FMZCA016;                                 0630 00679000
         LR    GPR14P,@05                                          0630 00680000
*                 GPR15P=ADDR(GPR15P->PABWEQA);                    0631 00681000
*                 GPR02P=GPR15P->PABWEQA|FMZCA016;                 0632 00682000
         O     @05,PABWEQA(,GPR15P)                                0632 00683000
         LR    GPR02P,@05                                          0632 00684000
*                 DO WHILE(GPR02P^=GPR14P);                        0633 00685000
