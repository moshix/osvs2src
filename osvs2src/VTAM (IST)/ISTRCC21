*                 RFY                                              0545 00460000
*                  (GPR00P,                                        0545 00461000
*                   GPR01P,                                        0545 00462000
*                   GPR14P,                                        0545 00463000
*                   GPR15P) UNRSTD;                                0545 00464000
*               END;                /* END TPFEL GENERATION          */ 00465000
*               CALL LOCKFMCB;      /* GET AND LOCK FMCB             */ 00466000
         BAL   @14,LOCKFMCB                                        0547 00467000
*               CRRDFSYN=ZERB;      /* CLEAR SYN RTYPE               */ 00468000
*               CRRRESP=ZERB;       /* CLEAR RESP RTYPE              */ 00469000
         NI    CRRDFSYN(CRRPTR),B'11111001'                        0549 00470000
*               REG7=ADDR(FMCFBAS); /* SET UP PTR FMCB DFASY Q       */ 00471000
