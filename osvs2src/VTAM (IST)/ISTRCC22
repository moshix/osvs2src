*               CRRRTNCD=CRRLOGIC;                                 0433 00199000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0433 00200000
*               CRRFDB2=CRRSINVR;                                  0434 00201000
         MVI   CRRFDB2(CRRPTR),X'3F'                               0434 00202000
*             END;                                                 0435 00203000
*         END;                                                     0436 00204000
*                                                                  0436 00205000
*/****************************************************************** */ 00206000
*/*         END CHK 3270                                             */ 00207000
*/****************************************************************** */ 00208000
*                                                                  0437 00209000
*       ELSE                                                       0437 00210000
*         ;                                                        0437 00211000
@RF00428 DS    0H                                                  0438 00212000
*       IF CRRRTNCD=CRRNOERR THEN                                  0438 00213000
@RC00428 CLI   CRRRTNCD(CRRPTR),X'00'                              0438 00214000
         BNE   @RF00438                                            0438 00215000
*         DO;                                                      0439 00216000
*           IF RPLSRESP=OFF THEN    /* IS THIS A REQUEST| RESPONSE   */ 00217000
         LA    @11,16                                              0440 00218000
         ALR   @11,RPLPTR                                          0440 00219000
         TM    RPLSRESP(@11),B'10000000'                           0440 00220000
         BNZ   @RF00440                                            0440 00221000
*             DO;                   /* STYPE = REQUEST               */ 00222000
*               IF RPLDATA=ON THEN  /* CHECK IF DATA| CONTROL        */ 00223000
         TM    RPLDATA(@11),B'10000000'                            0442 00224000
         BNO   @RF00442                                            0442 00225000
*/*  *****************************************************************/ 00226000
*/*   THE SENDDATA SEGMENT IS USED FOR PREPARING  DATA REQUESTS FOR  */ 00227000
*/*   TRANSMISSION.  IT CHECKS THE VALIDITY OF THE REQUESTS, I.E.    */ 00228000
*/*   IF THE RESPONSE IS VALID OR MATCHES THE OUTSTANDING RESPONSE   */ 00229000
*/*   IT ALSO SETS THE UNIQUE LCCW/RH AND LCPB/TH FIELDS FOR DATA    */ 00230000
*/*   TRANSMISSIONS.                                                 */ 00231000
*/*  *****************************************************************/ 00232000
*                                                                  0443 00233000
*                 DO;                                              0443 00234000
*                   IF RPLEX=ON|(RPLNFME=ON&RPLRRN=OFF) THEN       0444 00235000
         TM    RPLEX(@11),B'00000100'                              0444 00236000
         BO    @RT00444                                            0444 00237000
         TM    RPLNFME(@11),B'00000010'                            0444 00238000
         BNO   @RF00444                                            0444 00239000
         TM    RPLRRN(@11),B'00000001'                             0444 00240000
         BZ    @RT00444                                            0444 00241000
*                     GO TO PREPOST;/*                       @D31KKNL*/ 00242000
*                   ELSE                                           0446 00243000
*                     ;                                            0446 00244000
@RF00444 DS    0H                                                  0447 00245000
*                   IF RPLSCHED=ON THEN/* POST = SCHED REQUESTED     */ 00246000
         TM    RPLSCHED+16(RPLPTR),B'10000000'                     0447 00247000
         BNO   @RF00447                                            0447 00248000
*PREPOST:                                                          0448 00249000
*                     IF FMCFLGB5=ON THEN/* AND PREV SCHED NOT POSTD */ 00250000
PREPOST  TM    FMCFLGB5(FMCBPTR),B'00000100'                       0448 00251000
         BNO   @RF00448                                            0448 00252000
*                       DO;         /* SET ERROR IN CRR.             */ 00253000
*                         CRRRTNCD=CRRLOGIC;/* SET LOGIC ERROR AND   */ 00254000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0450 00255000
*                         CRRFDB2=CRRSDNP;/* PREV SCHED NOT POSTED   */ 00256000
         MVI   CRRFDB2(CRRPTR),X'34'                               0451 00257000
*                       END;                                       0452 00258000
*                     ELSE          /* PREV SCHED POSTED             */ 00259000
*                       GOTO RTNCD;                                0453 00260000
*                   ELSE            /* POST = RESP.                  */ 00261000
*                     IF FMCRSPLM^=0&/* RESP LIMIT=0 =>NOLIMIT       */ 00262000
*                         FMCPSTRS>=FMCRSPLM THEN                  0454 00263000
         B     @RC00447                                            0454 00264000
@RF00447 LH    @11,FMCRSPLM(,FMCBPTR)                              0454 00265000
         N     @11,@CF03645                                        0454 00266000
         LTR   @11,@11                                             0454 00267000
         BZ    @RF00454                                            0454 00268000
         CLC   FMCPSTRS(2,FMCBPTR),FMCRSPLM(FMCBPTR)               0454 00269000
         BL    @RF00454                                            0454 00270000
*                       DO;         /* RESP. LIMIT EXCEEDED ?        */ 00271000
*                         CRRRTNCD=CRRRLEX;/* THEN SET ERROR FOR THIS*/ 00272000
         MVI   CRRRTNCD(CRRPTR),X'2C'                              0456 00273000
