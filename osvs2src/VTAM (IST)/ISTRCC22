@RF00454 DS    0H                                                  0459 00279000
@RC00447 DS    0H                                                  0459 00280000
RTNCD    CLI   CRRRTNCD(CRRPTR),X'00'                              0459 00281000
         BNE   @RF00459                                            0459 00282000
*                     DO;           /* COPY CHAIN VALUE FROM RPL     */ 00283000
*                       CALL LCCWCOMD;/* SET UP LCCWS FOR DATA       */ 00284000
         BAL   @14,LCCWCOMD                                        0461 00285000
*                       LCCWPTR=LCPNXLCW;/* REQUEST OR RESPONSES.    */ 00286000
         L     LCCWPTR,LCPNXLCW(,LCPBPTR)                          0462 00287000
*                       RHQS=OFF;   /* SET RH FOR REQUEST.           */ 00288000
*                       RHDIRI=OFF; /* SET FLOW TO 'WITH'. PUT DATA     00289000
*                                      LENGTH AND                    */ 00290000
         NI    RHQS+4(LCCWPTR),B'01101111'                         0464 00291000
*                       LCCWPTR=ADDR(DCLLCCW2);/* ADDR. IN 2ND LCCW. */ 00292000
