QTOTOPS  DS    0H                                                  0725 01093000
*/*  *****************************************************************/ 01094000
*/*   THIS ROUTINE IS USED TO SET UP THE COMMON LCPB/TH FIELDS FOR   */ 01095000
*/*   REQUESTS AND QUEUE THE LCPB TO THE APPROPRIATE PAB.  IF THE    */ 01096000
*/*   REQUEST IS SYNCHRONOUS WITH THE DATA FLOW (CRRSW2 = OFF) THEN  */ 01097000
*/*   THE LCPB IS QUEUED TO THE TOP PAB(FMCPAB3),IF IT IS ASYNCHRON  */ 01098000
*/*   -OUS THEN THE LCPB IS QUEUED TO THE ATOP PAB(FMCPAB5).         */ 01099000
*/*  *****************************************************************/ 01100000
*                                                                  0725 01101000
*   DCL                                                            0725 01102000
*     PABPTR REG(5) PTR(31) RSTD;                                  0725 01103000
*   DCL                                                            0726 01104000
*     MAPPAB CHAR(LENGTH(ISTPAB)) BASED(PABPTR);                   0726 01105000
*   RFY                                                            0727 01106000
*    (RTNREG,                                                      0727 01107000
*     RCDEREG) RSTD;                                               0727 01108000
*   CRRRTN1=RTNREG;                 /* SAVE RETURN ADDRESS           */ 01109000
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0728 01110000
*   IF FMCCONF=ON THEN                                             0729 01111000
         TM    FMCCONF(FMCBPTR),B'00000001'                        0729 01112000
         BNO   @RF00729                                            0729 01113000
*     DO;                           /* IF SSCP INOP          @ZA17007*/ 01114000
*       CRRRTNCD='1C'X;             /* DO NOT TPQUE TOP      @ZA17007*/ 01115000
         MVI   CRRRTNCD(CRRPTR),X'1C'                              0731 01116000
*       CRRFDB2='02'X;              /* SET RPL RET CODE      @ZA17007*/ 01117000
         MVI   CRRFDB2(CRRPTR),X'02'                               0732 01118000
*       RCDEREG=4;                  /* NO TPQUE DONE         @ZA17007*/ 01119000
         LA    RCDEREG,4                                           0733 01120000
*     END;                          /* SSCP INOP             @ZA17007*/ 01121000
*   ELSE                                                           0735 01122000
*     DO;                           /* NO SSCP INOP          @ZA17007*/ 01123000
         B     @RC00729                                            0735 01124000
@RF00729 DS    0H                                                  0736 01125000
*       IF CRRSW2=OFF THEN          /* IS THIS A SYNCH REQ/RESP      */ 01126000
         TM    CRRSW2(CRRPTR),B'00100000'                          0736 01127000
         BNZ   @RF00736                                            0736 01128000
*         PABPTR=ADDR(FMCPAB3);     /* SET PTR TO TOP PAB            */ 01129000
         LA    PABPTR,FMCPAB3(,FMCBPTR)                            0737 01130000
*       ELSE                        /* ASYN REQ/RESP =>ATOP          */ 01131000
*         PABPTR=ADDR(FMCPAB5);                                    0738 01132000
*                                                                  0738 01133000
         B     @RC00736                                            0738 01134000
@RF00736 LA    PABPTR,FMCPAB5(,FMCBPTR)                            0738 01135000
*/*  TPQUE (LCPBPTR,MAPPAB) TRFCHK(RCDEREG)NORM                      */ 01136000
*                                                                  0739 01137000
*       DO;                                                        0739 01138000
@RC00736 DS    0H                                                  0740 01139000
*         DCL                                                      0740 01140000
*           GPR00F FIXED(31) REG(00);                              0740 01141000
*         DCL                                                      0741 01142000
*           GPR00P PTR(31) REG(00);                                0741 01143000
*         DCL                                                      0742 01144000
*           GPR01F FIXED(31) REG(01);                              0742 01145000
*         DCL                                                      0743 01146000
*           GPR01P PTR(31) REG(01);                                0743 01147000
*         DCL                                                      0744 01148000
