         TITLE 'ISTRCC22:SEND OUTBOUND MACRO PROCESSOR                 *00001000
                        '                                               00002000
ISTRCC22 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTRCC22  78.173'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*   CRRPTR=ADDR(CRRSPACE);          /* GET ADDR CRR                  */ 00013000
         L     @11,RPHCRR(,RPHPTR)                                 0373 00014000
         L     CRRPTR,CRACRR(,@11)                                 0373 00015000
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0373 00016000
*   LCPBPTR=CRRLCPB;                /* GET ADDR DCLCP                */ 00017000
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0374 00018000
*   RPLPTR=CRRARPL;                 /* GET ADDR RPL                  */ 00019000
         L     RPLPTR,CRRARPL(,CRRPTR)                             0375 00020000
*   FMCBPTR=CRRFMCB;                /* GET ADDR FMCB                 */ 00021000
         L     FMCBPTR,CRRFMCB(,CRRPTR)                            0376 00022000
*   LCCWPTR=LCPNXLCW;               /* GET ADDR 1ST LCCW             */ 00023000
         L     LCCWPTR,LCPNXLCW(,LCPBPTR)                          0377 00024000
*   CRRMAS=''B;                     /* ZERO OUT MODULE AUDIT SWS     */ 00025000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0378 00026000
*   CRRRM22=ON;                     /* SET MODULE AUDIT FOR RCC22    */ 00027000
         OI    CRRRM22(CRRPTR),B'01000000'                         0379 00028000
*   CRRRC22=ON;                     /* SET AUDIT TRAIL SWITCH        */ 00029000
         OI    CRRRC22(CRRPTR),B'01000000'                         0380 00030000
*   IF RPLDATA=ON THEN                                             0381 00031000
         TM    RPLDATA+16(RPLPTR),B'10000000'                      0381 00032000
         BNO   @RF00381                                            0381 00033000
*     LCPASYN=OFF;                  /* CHECK IF CONTROL = DATA ?     */ 00034000
         NI    LCPASYN(LCPBPTR),B'10111111'                        0382 00035000
*   ELSE                            /* NOT DATA SO TRANSLATE CTL.    */ 00036000
*/*  *****************************************************************/ 00037000
*/*   THIS SEGMENT IS USED TO TRANSLATE  THE THREE BYTE CONTROL      */ 00038000
*/*   PARAMETER FIELD IN THE RPL INTO A ONE BYTE CONTROL 'RU'-REQUEST*/ 00039000
*/*   UNIT FORM FOR RECORD TRANSMISSIONS.                            */ 00040000
*/*   IT DOES THIS BY SEARCHING A TABLE WITHIN ISTRCC22 WHICH HAS THE*/ 00041000
*/*   CONTROL BYTE CODE MATCHED WITH THE RPL BIT SETTING IN THE RES- */ 00042000
*/*   PECTIVE BYTE IN THE RPL CONTROL FIELD.  THIS IS DONE BY INDEX- */ 00043000
*/*   THROUGH AN ARRAY WHICH IS COMPOSED OF THE MASK FOR THE BYTE IN */ 00044000
*/*   THE RPL, THE CONTROL CODE, THE WITH/AGAINST FLOW MASK, AND THE */ 00045000
*/*   SYNCH/ASYNCH MASK FOR EACH RESPECTIVE CONTROL CODE IN EACH     */ 00046000
*/*   ELEMENT OF THE ARRAY.                                          */ 00047000
*/*   THE ARRAY HAS AT PRESENT A DIMENSION OF 10 ELEMENTS AND   8    */ 00048000
*/*   FIELDS (BYTE).                                                 */ 00049000
*/*  *****************************************************************/ 00050000
*                                                                  0383 00051000
*     DO;                                                          0383 00052000
         B     @RC00381                                            0383 00053000
@RF00381 DS    0H                                                  0384 00054000
*       CRRSW1=ON;                  /* FLAG FOR LOOP CNTRL           */ 00055000
         OI    CRRSW1(CRRPTR),B'01000000'                          0384 00056000
*       DO INDXREG=ADDR(SRCHTAB) TO(ADDR(SRCHTAB)+(LENGTH(SRCHTAB)*(DIM 00057000
*             (SRCHTAB)-1))) BY LENGTH(SRCHTAB) WHILE CRRSW1=ON;   0385 00058000
*                                                                  0385 00059000
         LA    INDXREG,SRCHTAB                                     0385 00060000
         B     @DE00385                                            0385 00061000
@DL00385 TM    CRRSW1(CRRPTR),B'01000000'                          0385 00062000
         BNO   @DC00385                                            0385 00063000
*         /***********************************************************/ 00064000
*         /*                                                         */ 00065000
*         /* SEARCH THROUGH THE TABLE MASKING THE RPL CONTROL FIELD  */ 00066000
*         /* WITH THE TABLE ELEMENT FIELD(RPLMSK) UNTIL THE CORRECT  */ 00067000
*         /* CONTROL TYPE IS FOUND OR IF NONE IS FOUND-EXIT.         */ 00068000
*         /*                                                         */ 00069000
*         /***********************************************************/ 00070000
*                                                                  0386 00071000
*         IF RPLMSK=(RPLPTR+INDXVAL(CBYTE))->LPRCTFLD THEN         0386 00072000
         SLR   @11,@11                                             0386 00073000
         IC    @11,CBYTE(,INDXREG)                                 0386 00074000
         SLA   @11,2                                               0386 00075000
         LR    @02,RPLPTR                                          0386 00076000
         AL    @02,INDXVAL-4(@11)                                  0386 00077000
         CLC   RPLMSK(1,INDXREG),LPRCTFLD(@02)                     0386 00078000
         BNE   @RF00386                                            0386 00079000
*           DO;                     /* COMPARE RPLFLD WITH TABLE     */ 00080000
*             IF RPLSRESP=ON THEN   /* IF THIS IS A RESPONSE         */ 00081000
         TM    RPLSRESP+16(RPLPTR),B'10000000'                     0388 00082000
         BNO   @RF00388                                            0388 00083000
*               DO;                 /* SET SYN/ASYN FLAG AS SPEC     */ 00084000
*                 IF SYNCHER1^=0 THEN                              0390 00085000
         CLI   SYNCHER1(INDXREG),0                                 0390 00086000
         BE    @RF00390                                            0390 00087000
*                   DO;             /* IS IN THE SEARCH TABLE        */ 00088000
*                     CRRSW0=ON;                                   0392 00089000
         OI    CRRSW0(CRRPTR),B'10000000'                          0392 00090000
*                     LCPASYN=ON;   /* SET LCPB HEADER ASY           */ 00091000
         OI    LCPASYN(LCPBPTR),B'01000000'                        0393 00092000
*                   END;                                           0394 00093000
*                 ELSE                                             0395 00094000
*                   DO;                                            0395 00095000
         B     @RC00390                                            0395 00096000
@RF00390 DS    0H                                                  0396 00097000
*                     CRRSW0=OFF;                                  0396 00098000
         NI    CRRSW0(CRRPTR),B'01111111'                          0396 00099000
*                     LCPASYN=OFF;  /* SET LCPB HEADER SYN           */ 00100000
         NI    LCPASYN(LCPBPTR),B'10111111'                        0397 00101000
*                   END;                                           0398 00102000
*                 RH1=FLOWMSK1;     /* SET FLOW ACCORDINGLY          */ 00103000
@RC00390 MVC   RH1+4(1,LCCWPTR),FLOWMSK1(INDXREG)                  0399 00104000
*               END;                                               0400 00105000
*             ELSE                  /* IF THIS IS A REQUEST          */ 00106000
*               DO;                                                0401 00107000
         B     @RC00388                                            0401 00108000
@RF00388 DS    0H                                                  0402 00109000
*                 IF SYNCHER^=0 THEN                               0402 00110000
         CLI   SYNCHER(INDXREG),0                                  0402 00111000
         BE    @RF00402                                            0402 00112000
*                   DO;             /* SET SYN/ASYN FLAG IN CRR      */ 00113000
*                     CRRSW0=ON;    /* ACCORDING TO TABLE            */ 00114000
         OI    CRRSW0(CRRPTR),B'10000000'                          0404 00115000
*                     LCPASYN=ON;                                  0405 00116000
         OI    LCPASYN(LCPBPTR),B'01000000'                        0405 00117000
*                   END;                                           0406 00118000
*                 ELSE                                             0407 00119000
*                   DO;                                            0407 00120000
         B     @RC00402                                            0407 00121000
@RF00402 DS    0H                                                  0408 00122000
*                     CRRSW0=OFF;                                  0408 00123000
         NI    CRRSW0(CRRPTR),B'01111111'                          0408 00124000
*                     LCPASYN=OFF;                                 0409 00125000
         NI    LCPASYN(LCPBPTR),B'10111111'                        0409 00126000
*                   END;                                           0410 00127000
*                 RH1=FLOWMSK;      /* AND SET FLOW IND.             */ 00128000
@RC00402 MVC   RH1+4(1,LCCWPTR),FLOWMSK(INDXREG)                   0411 00129000
*               END;                                               0412 00130000
*             RC22RU=CTLCDE;        /* MOVE CONTROL BYTE TO LCCW     */ 00131000
@RC00388 MVC   RC22RU(1,LCCWPTR),CTLCDE(INDXREG)                   0413 00132000
*             CRRSW1=OFF;           /* INDICATE CONTROL FOUND        */ 00133000
         NI    CRRSW1(CRRPTR),B'10111111'                          0414 00134000
*           END;                                                   0415 00135000
*       END;                                                       0416 00136000
@RF00386 AL    INDXREG,@CF00037                                    0416 00137000
@DE00385 LA    @11,SRCHTAB                                         0416 00138000
         AL    @11,@CF03624                                        0416 00139000
         CR    INDXREG,@11                                         0416 00140000
         BNH   @DL00385                                            0416 00141000
@DC00385 DS    0H                                                  0417 00142000
*       INDXREG=INDXREG-LENGTH(SRCHTAB);/* DECREMENT INDXREG         */ 00143000
         SL    INDXREG,@CF00037                                    0417 00144000
*       IF CRRSW1=ON THEN           /* IF NO CONTROL WAS FOUND       */ 00145000
         TM    CRRSW1(CRRPTR),B'01000000'                          0418 00146000
         BNO   @RF00418                                            0418 00147000
*         DO;                       /* SET ERROR CODES IN THE        */ 00148000
*           CRRRTNCD=CRRLOGIC;      /* MAJOR AND MINOR ERROR         */ 00149000
         MVI   CRRRTNCD(CRRPTR),X'08'                              0420 00150000
*           CRRFDB2=CRRSDNVC;       /* FIELDS IN THE CRR.            */ 00151000
         MVI   CRRFDB2(CRRPTR),X'38'                               0421 00152000
*         END;                                                     0422 00153000
*     END;                                                         0423 00154000
@RF00418 DS    0H                                                  0424 00155000
*   IF CRRRTNCD=CRRNOERR THEN       /* DID XLATECTL XLATE CONTROL?   */ 00156000
@RC00381 CLI   CRRRTNCD(CRRPTR),X'00'                              0424 00157000
         BNE   @RF00424                                            0424 00158000
*     DO;                           /* NO ERROR IN XLATE OR DATA     */ 00159000
*       RFY                                                        0426 00160000
*         ISTNCB BASED(FMCDNCBA);   /*                       @D32CKCF*/ 00161000
*       RFY                                                        0427 00162000
*         ISTDEVCH BASED(ADDR(NCBDEVCH));/*                  @D32CKCF*/ 00163000
*       IF DEVTCODE>=DEV3277&DEVTCODE<=DEV3275 THEN                0428 00164000
         LA    @11,36                                              0428 00165000
         AL    @11,FMCDNCBA(,FMCBPTR)                              0428 00166000
         CLI   DEVTCODE(@11),X'19'                                 0428 00167000
         BL    @RF00428                                            0428 00168000
         CLI   DEVTCODE(@11),X'1C'                                 0428 00169000
         BH    @RF00428                                            0428 00170000
*         DO;                                                      0429 00171000
*                                                                  0429 00172000
*/****************************************************************** */ 00173000
*/*                         CHK 3270                                 */ 00174000
*/*    CHECK FOR VALID REQUEST OPTIONS FOR 3270 DEVICE               */ 00175000
*/*                                                                  */ 00176000
*/****************************************************************** */ 00177000
*                                                                  0430 00178000
*           IF(RPLONLY=ON)&         /*                       @D33SKXW*/ 00179000
*               (RPLNFME=OFF|RPLEX=ON)&(RPLDATA=ON)&(RPLSRESP^=ON) THEN 00180000
         LA    @11,16                                              0430 00181000
         ALR   @11,RPLPTR                                          0430 00182000
         TM    RPLONLY(@11),B'00010000'                            0430 00183000
         BNO   @RF00430                                            0430 00184000
         TM    RPLEX(@11),B'00000100'                              0430 00185000
         BNZ   @GL00003                                            0430 00186000
         TM    RPLNFME(@11),B'00000010'                            0430 00187000
         BO    @RF00430                                            0430 00188000
@GL00003 LA    @11,16                                              0430 00189000
         ALR   @11,RPLPTR                                          0430 00190000
