*/********************************************************************* 01405000
*/* END OF SEGMENT UECB                                               * 01406000
*/********************************************************************* 01407000
*UECPTR = CRRRTN2;                          /* GET UECB ADDRESS       * 01408000
         L     UECPTR,CRRRTN2(,CRRPTR)                             1040 01409000
*RFY ISTUECB BASED(UECPTR);                 /* ADDRESS THE UECB       * 01410000
*RFY IFGRPL BASED(CRRARPL);                 /* ADDRESS THE VIRTUAL RPL* 01411000
*RFY ISTFDB BASED(ADDR(FSBFDB));             /* ADDRESS THE FSB  */     01412000
*        IF CRRRESP = ONEB                  /* RESPONSE ?             * 01413000
*           THEN UECTYPE = UECRESP;         /* SET EXIT TYPE RESPONSE * 01414000
         TM    CRRRESP(CRRPTR),B'00000100'                         1044 01415000
         BNO   @RF01044                                            1044 01416000
         MVI   UECTYPE(UECPTR),X'08'                               1045 01417000
*           ELSE UECTYPE = UECDFASY;        /* SET EXIT TYPE ASYNCH   * 01418000
         B     @RC01044                                            1046 01419000
@RF01044 MVI   UECTYPE(UECPTR),X'07'                               1046 01420000
*        UECDLEN = 0;                       /* SET LENGTH ZERO        * 01421000
@RC01044 SLR   @05,@05                                             1047 01422000
         ST    @05,UECDLEN(,UECPTR)                                1047 01423000
*        RPLRLEN = 0;                       /* SET LENGTH ZERO        * 01424000
         L     @15,CRRARPL(,CRRPTR)                                1048 01425000
         ST    @05,RPLRLEN(,@15)                                   1048 01426000
*        CRRRPLS = ONEB;                    /* SAY RPL SATISFIED      * 01427000
         OI    CRRRPLS(CRRPTR),B'10000000'                         1049 01428000
*RFY ISTUECB;                               /* DROP UECB              * 01429000
*RFY IFGRPL;                                /* DROP VIRTUAL RPL       * 01430000
*/********************************************************************* 01431000
*/* END OF SEGMENT SCHEDX                                             * 01432000
*/********************************************************************* 01433000
*                        UECPTR = CRRRTN2;  /* GET UECB ADDRESS       * 01434000
         L     UECPTR,CRRRTN2(,CRRPTR)                             1052 01435000
*RFY ISTUECB BASED(UECPTR);                 /* ADDRESS THE UECB       * 01436000
*                        IF CRRSW4 = ZERB   /* FROM FMCB ?            * 01437000
*                           THEN UECEXLST = FMCEXLST; /* YES, PLUG    * 01438000
         TM    CRRSW4(CRRPTR),B'00001000'                          1054 01439000
         BNZ   @RF01054                                            1054 01440000
         L     @05,FMCEXLST(,FMCBPTR)                              1055 01441000
         ST    @05,UECEXLST(,UECPTR)                               1055 01442000
*                                           /* LIST ADDR IN UECB      * 01443000
*   RFY (FMCBPTR,REG7,FSBPTR)UNRSTD;/*AS18799 @Z30APGH*/           1056 01444000
*                                                                  1056 01445000
@RF01054 DS    0H                                                  1057 01446000
*/*  TPQUE (UECPTR,UECPAB) NORM */                                 1057 01447000
*                                                                  1057 01448000
*         DO;                                                      1057 01449000
*         RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;           1058 01450000
*         GEN REFS(GPR01P) SETS(RPHWORK)                           1059 01451000
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      1059 01452000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              01453000
*         GPR00P=0;                                                1060 01454000
         SLR   GPR00P,GPR00P                                       1060 01455000
*         GPR15P=ADDR(UECPAB); /*HOLD PAB PTR*/                    1061 01456000
         LA    GPR15P,UECPAB(,UECPTR)                              1061 01457000
*         GPR04P=UECPTR; /*WORK ELE PTR*/                          1062 01458000
         LR    GPR04P,UECPTR                                       1062 01459000
*         GPR05P=GPR15P; /*PASS PAB PTR*/                          1063 01460000
         LR    GPR05P,GPR15P                                       1063 01461000
*         CALL ATCLCPTR->ATCAP37->ISTAPC37;                        1064 01462000
         L     @08,ATCLCPTR                                        1064 01463000
         L     @15,ATCAP37(,@08)                                   1064 01464000
         BALR  @14,@15                                             1064 01465000
*         GEN(NOPR  0)NOSETS NOREFS;                               1065 01466000
