         TITLE 'ISTRCC51: CONTROL LAYER RECORD INBOUND ROUTER          *00001000
                        '                                               00002000
ISTRCC51 CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTRCC51  76.322'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*CRRPTR = ADDR(CRRSPACE);                   /* GET THE CRR ADDRESS    * 00013000
         L     @10,RPHCRR(,RPHPTR)                                 0508 00014000
         L     CRRPTR,CRACRR(,@10)                                 0508 00015000
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0508 00016000
*RFY CRRSAREA BASED(CRRPTR);                /* ADDRESS THE CRR        * 00017000
*RFY ISTATCVT BASED(CRRATCVT);              /* ADDRESS THE ATCVT      * 00018000
*CRRMAS = ''B;                              /* CLEAR THE ACTIVE FIELD * 00019000
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0511 00020000
*CRRRM51 = ONEB;                            /* FLAG THIS MODULE ACTIVE* 00021000
         OI    CRRRM51(CRRPTR),B'00000100'                         0512 00022000
*CRRRC51 = ONEB;                            /* FLAG THIS MODULE ENTERD* 00023000
         OI    CRRRC51(CRRPTR),B'00000100'                         0513 00024000
*CRRRPLS = ZERB;                            /* TURN OFF RPL SATISFIED * 00025000
         NI    CRRRPLS(CRRPTR),B'01111111'                         0514 00026000
*REG7 = 0;                                  /* SET DVT POINTER TO ZERO* 00027000
         SLR   REG7,REG7                                           0515 00028000
*FSBPTR = CRRFSB;                           /* GET FSB ADDRESS        * 00029000
         L     FSBPTR,CRRFSB(,CRRPTR)                              0516 00030000
*RFY ISTFSB BASED(FSBPTR);                  /* ADDRESS THE FSB        * 00031000
*RHPTR = ADDR(FSBRHFLD);                    /* GET RH ADDRESS         * 00032000
         LA    RHPTR,FSBRHFLD(,FSBPTR)                             0518 00033000
*RFY ISTRH BASED(RHPTR);                    /* ADDRESS THE RH         * 00034000
*CRRFLGS = 0;                               /* CLEAR THE LOCAL FLAGS  * 00035000
         MVI   CRRFLGS(CRRPTR),X'00'                               0520 00036000
*RFY ISTFDB BASED(ADDR(FSBFDB));            /* BASE DATA BLOCK        * 00037000
*/********************************************************************* 00038000
*/*                                                                   * 00039000
*/* RTYPE - SEGMENT OF ISTRCC51                                       * 00040000
*/*      SET THE CRR RTYPE FIELD ACCORDING TO THE RH INDICATORS RHTYPE* 00041000
*/*      AND RHQS. IF RHQS = '1'B, THEN IT IS RESPONSE. IF RHTYPE =   * 00042000
*/*      '1'B, THEN IT IS CONTROL AND THE TYPE OF CONTROL MUST BE     * 00043000
*/*      BE CHECKED, BUT IF RHTYPE = '0'B, IT IS SYNCHRONOUS DATA.    * 00044000
*/*      IF CONTROL TYPE IS QUIESCE COMPLETE, CANCEL,LUS,BID,RTR, OR  * 00045000
*/*      CHASE,THE TYPE IS CONSIDERED TO BE SYNCHRONOUS DATA.         * 00046000
*/*                                                                   * 00047000
*/********************************************************************* 00048000
*CRRDFSYN = ZERB;                           /* SET SYN OFF            * 00049000
*CRRDFASY = ZERB;                           /* SET ASY OFF            * 00050000
*CRRNSNA = ZERB;                            /* SET NON-SNA OFF        * 00051000
*CRRRESP = ZERB;                            /* SET RESP OFF           * 00052000
         NI    CRRDFSYN(CRRPTR),B'11110000'                        0525 00053000
*IF RHQS = ONEB                             /* RH FOR RESPONSE ?      * 00054000
*   THEN CRRRESP = ONEB;                    /* YES, SET RESP ON       * 00055000
         TM    RHQS(RHPTR),B'10000000'                             0526 00056000
         BNO   @RF00526                                            0526 00057000
         OI    CRRRESP(CRRPTR),B'00000100'                         0527 00058000
*   ELSE DO;                                /* NO                     * 00059000
         B     @RC00526                                            0528 00060000
@RF00526 DS    0H                                                  0529 00061000
*        IF RHTYPE = ZERB                   /* IS RH FOR DATA ?       * 00062000
*           THEN CRRDFSYN = ONEB;           /* YES, SET SYN ON        * 00063000
         TM    RHTYPE(RHPTR),B'01000000'                           0529 00064000
         BNZ   @RF00529                                            0529 00065000
         OI    CRRDFSYN(CRRPTR),B'00000010'                        0530 00066000
*           ELSE DO;                        /* NO                     * 00067000
         B     @RC00529                                            0531 00068000
@RF00529 DS    0H                                                  0532 00069000
*                IF RHSENSE = ONEB          /* SENSE INCLUDED ?       * 00070000
*                   THEN RUPTR = FDBDATAA + 4;                     0532 00071000
         TM    RHSENSE(RHPTR),B'00000100'                          0532 00072000
         BNO   @RF00532                                            0532 00073000
         LA    RUPTR,4                                             0533 00074000
         AL    RUPTR,FDBDATAA+32(,FSBPTR)                          0533 00075000
*                   ELSE RUPTR = FDBDATAA;  /* POINT TO DATA          * 00076000
         B     @RC00532                                            0534 00077000
@RF00532 L     RUPTR,FDBDATAA+32(,FSBPTR)                          0534 00078000
*RFY ISTSRU BASED(RUPTR);                   /* ADDRESS THE RU         * 00079000
@RC00532 DS    0H                                                  0536 00080000
*                IF SRUQSREQ = CRRQC | SRUQSREQ = CRRCHASE         0536 00081000
*                   | SRUQSREQ = CRRCNCL                           0536 00082000
*                   THEN DO; CRRDFSYN = ONEB;   /* SET SYN FOR QC,    * 00083000
         CLI   SRUQSREQ(RUPTR),X'81'                               0536 00084000
         BE    @RT00536                                            0536 00085000
         CLI   SRUQSREQ(RUPTR),X'84'                               0536 00086000
         BE    @RT00536                                            0536 00087000
         CLI   SRUQSREQ(RUPTR),X'83'                               0536 00088000
         BNE   @RF00536                                            0536 00089000
@RT00536 DS    0H                                                  0537 00090000
         OI    CRRDFSYN(CRRPTR),B'00000010'                        0538 00091000
*                                           /* CANCEL OR CHASE        * 00092000
*                            IF SRUQSREQ=CRRCNCL THEN DO;          0539 00093000
         CLI   SRUQSREQ(RUPTR),X'83'                               0539 00094000
         BNE   @RF00539                                            0539 00095000
*                                                      FMCBPTR=CRRFMCB; 00096000
         L     FMCBPTR,CRRFMCB(,CRRPTR)                            0541 00097000
*                                                      FMCIBCE=FMCIBCEI 00098000
         OI    FMCIBCE(FMCBPTR),B'00001100'                        0542 00099000
*                                                      END;  /*XM19702* 00100000
*                        END;                               /* XM19465* 00101000
*                ELSE IF SRUQSREQ = SRLUS | SRUQSREQ = SRUSRBID    0545 00102000
*                   | SRUQSREQ = SRRTR     /* SET SYN FOR LUS,        * 00103000
*                   THEN CRRDFSYN = ONEB;  /* BID OR RTR              * 00104000
         B     @RC00536                                            0545 00105000
@RF00536 CLI   SRUQSREQ(RUPTR),X'04'                               0545 00106000
         BE    @RT00545                                            0545 00107000
         CLI   SRUQSREQ(RUPTR),X'C8'                               0545 00108000
         BE    @RT00545                                            0545 00109000
         CLI   SRUQSREQ(RUPTR),X'05'                               0545 00110000
         BNE   @RF00545                                            0545 00111000
@RT00545 DS    0H                                                  0546 00112000
         OI    CRRDFSYN(CRRPTR),B'00000010'                        0546 00113000
*                   ELSE CRRDFASY = ONEB;   /* SET ASY FOR ALL OTHERS * 00114000
         B     @RC00545                                            0547 00115000
@RF00545 OI    CRRDFASY(CRRPTR),B'00001000'                        0547 00116000
*                END;                       /* CONTROL CHECKED        * 00117000
@RC00545 DS    0H                                                  0548 00118000
@RC00536 DS    0H                                                  0549 00119000
*        END;                               /* NON-RESP CHECKED       * 00120000
@RC00529 DS    0H                                                  0550 00121000
*RFY ISTSRU;                                /* RELEASE RU             * 00122000
@RC00526 DS    0H                                                  0551 00123000
*/********************************************************************* 00124000
*/* END OF SEGMENT RTYPE                                              * 00125000
*/********************************************************************* 00126000
*                                                                  0551 00127000
*/*VMDESET  ((15),ZERO)*/                                          0551 00128000
*    DO;                                 /* INVOKE MODESET MACRO    */  00129000
*      GEN;                                                        0552 00130000
*                                                                  0552 00131000
         MODESET EXTKEY=ZERO,WORKREG=2                                  00132000
*    RFY (GPR15P) RSTD;                                            0553 00133000
*      GPR15F=0;                                                   0554 00134000
*                                                                  0554 00135000
         SLR   GPR15F,GPR15F                                       0554 00136000
*    RFY (GPR15P) UNRSTD;                /*PUT RETURN CODE IN RCREG*/   00137000
*    END;  /* VMDESET */ /*ITF1501 @Z30APGH*/                      0556 00138000
*/********************************************************************* 00139000
*/*                                                                   * 00140000
*/* CALL CKCHAIN TO DO SYNCHRONOUS PROCESSING. IF ASYNCHRONOUS, DO    * 00141000
*/* ASYN. AFTER ASYN CHECK FOR RPL SATISFIED. IF RPL SATISFIED TPDVTS * 00142000
*/* TO ASYNCHRONOUS FEEDBACK PROCESSOR. IF RPL NOT SATISFIED, TPESC   * 00143000
*/* TO FEEDBACK FINISH. IF SYNCHRONOUS FSB WAS DISCARDED BY PURGE,    * 00144000
*/* TPESC TO FEEDBACK FINISH.                                         * 00145000
*/*                                                                   * 00146000
*/********************************************************************* 00147000
*FMCBPTR = CRRFMCB;                         /* GET FMCB ADDRESS       * 00148000
         L     FMCBPTR,CRRFMCB(,CRRPTR)                            0557 00149000
*RFY ISTFMCB BASED(FMCBPTR);                /* ADDRESS FMCB           * 00150000
*RFY ISTACDEB BASED(FMCDEBA);               /* ADDRESS THE ACDEB      * 00151000
*RFY IFGACB BASED(ACDACB);                  /* ADDRESS THE ACB        * 00152000
*RFY ISTTH BASED(ADDR(FSBTHFLD));           /* ADDRESS THE TH         * 00153000
*RFY ISTDNCB BASED(FMCDNCBA);               /* ADDRESS THE DNCB       * 00154000
*RFY ISTNCB BASED(ADDR(DNCNCB));            /* ADDRESS THE NCB        * 00155000
*IF CRRDFSYN = ONEB                         /* SYNCHRONOUS ?          * 00156000
*   THEN DO;                                /* YES                    * 00157000
         TM    CRRDFSYN(CRRPTR),B'00000010'                        0564 00158000
         BNO   @RF00564                                            0564 00159000
*CHAIN:;                                 /*SYNCH DATA        @D31KKNL*/ 00160000
CHAIN    DS    0H                                                  0567 00161000
*/********************************************************************* 00162000
*/*                                                                   * 00163000
*/* CKCHAIN - INTERNAL SEGMENT   OF ISTRCC51                          * 00164000
*/*                                                                   * 00165000
*/* THIS SEGMENT   IS EXECUTED FOR DFSYN TYPE INPUT TO VERIFY THE     * 00166000
*/* CHAINING. IF A CHAINING EXCEPTION IS DETECTED, SEGMENT EXR IS     * 00167000
*/* EXECUTED. IN ANY CASE, SEGMENT SYNCH IS EXECUTED. IF, AFTER       * 00168000
*/* ALL OTHER PROCESSING IS COMPLETE, THE RPL FOUND SWITCH REMAINS    * 00169000
*/* ON (CRRRPLS), A TPDVTS MACRO IS ISSUED,  PASSING                  * 00170000
*/* CONTROL TO THE SYNCHRONOUS FEEDBACK PROCESSOR (ISTRCC52).         * 00171000
*/********************************************************************* 00172000
*IF RHTYPE = ONEB                           /* IS IT CONTROL ?        * 00173000
*    | (RHQS=ZERB & RHSENSE=ONEB)        /*EXCEPT REQUEST    @D31KKNL*/ 00174000
*   THEN;                                   /* YES, PROCESS AS SYNCH  * 00175000
         TM    RHTYPE(RHPTR),B'01000000'                           0567 00176000
         BO    @RT00567                                            0567 00177000
         TM    RHSENSE(RHPTR),B'00000100'                          0567 00178000
         BNO   @RF00567                                            0567 00179000
         TM    RHQS(RHPTR),B'10000000'                             0567 00180000
         BZ    @RT00567                                            0567 00181000
*   ELSE DO;                                /* NO, CHECK CHAIN        * 00182000
@RF00567 DS    0H                                                  0570 00183000
*        IF ((FMCIBCE = RHLAST | FMCIBCE = FMCIBCEI) &             0570 00184000
*            (RHCHAIN = RHFIRST | RHCHAIN = RHONLY)) |             0570 00185000
*           ((FMCIBCE ^= RHLAST & FMCIBCE ^= RHONLY) &             0570 00186000
*            (RHCHAIN = RHMIDLE | RHCHAIN = RHLAST))               0570 00187000
*           THEN DO;                        /* CHAINING VALID         * 00188000
         TM    FMCIBCE(FMCBPTR),B'00000100'                        0570 00189000
         BNO   @GL00008                                            0570 00190000
         TM    FMCIBCE(FMCBPTR),B'00001000'                        0570 00191000
         BZ    @GL00007                                            0570 00192000
@GL00008 TM    FMCIBCE(FMCBPTR),B'00001100'                        0570 00193000
         BNO   @GL00006                                            0570 00194000
@GL00007 TM    RHCHAIN(RHPTR),B'00000010'                          0570 00195000
         BNO   @GL00009                                            0570 00196000
         TM    RHCHAIN(RHPTR),B'00000001'                          0570 00197000
         BZ    @RT00570                                            0570 00198000
@GL00009 TM    RHCHAIN(RHPTR),B'00000011'                          0570 00199000
         BO    @RT00570                                            0570 00200000
@GL00006 TM    FMCIBCE(FMCBPTR),B'00000100'                        0570 00201000
         BNO   @GL00011                                            0570 00202000
         TM    FMCIBCE(FMCBPTR),B'00001000'                        0570 00203000
         BZ    @RF00570                                            0570 00204000
@GL00011 TM    FMCIBCE(FMCBPTR),B'00001100'                        0570 00205000
         BO    @RF00570                                            0570 00206000
         TM    RHCHAIN(RHPTR),B'00000011'                          0570 00207000
         BZ    @RT00570                                            0570 00208000
         TM    RHCHAIN(RHPTR),B'00000001'                          0570 00209000
         BNO   @RF00570                                            0570 00210000
         TM    RHCHAIN(RHPTR),B'00000010'                          0570 00211000
         BNZ   @RF00570                                            0570 00212000
@RT00570 DS    0H                                                  0571 00213000
*                IF RHCHAIN = RHFIRST       /* FIRST IN RH ?          * 00214000
*                   THEN FMCIBCE = RHFIRST; /* SET FIRST IN FMCB      * 00215000
         TM    RHCHAIN(RHPTR),B'00000010'                          0572 00216000
         BNO   @RF00572                                            0572 00217000
         TM    RHCHAIN(RHPTR),B'00000001'                          0572 00218000
         BNZ   @RF00572                                            0572 00219000
         NI    FMCIBCE(FMCBPTR),B'11111011'                        0573 00220000
         OI    FMCIBCE(FMCBPTR),B'00001000'                        0573 00221000
*                   ELSE                                           0574 00222000
*                IF RHCHAIN = RHMIDLE       /* MIDDLE IN RH ?         * 00223000
*                   THEN FMCIBCE = RHMIDLE; /* SET MIDDLE IN FMCB     * 00224000
         B     @RC00572                                            0574 00225000
@RF00572 TM    RHCHAIN(RHPTR),B'00000011'                          0574 00226000
         BNZ   @RF00574                                            0574 00227000
         NI    FMCIBCE(FMCBPTR),B'11110011'                        0575 00228000
*                   ELSE                                           0576 00229000
*                IF RHCHAIN = RHLAST        /* LAST IN RH ?           * 00230000
*                   THEN FMCIBCE = RHLAST;  /* SET LAST IN FMCB       * 00231000
         B     @RC00574                                            0576 00232000
@RF00574 TM    RHCHAIN(RHPTR),B'00000001'                          0576 00233000
         BNO   @RF00576                                            0576 00234000
         TM    RHCHAIN(RHPTR),B'00000010'                          0576 00235000
         BNZ   @RF00576                                            0576 00236000
         NI    FMCIBCE(FMCBPTR),B'11110111'                        0577 00237000
         OI    FMCIBCE(FMCBPTR),B'00000100'                        0577 00238000
*                   ELSE FMCIBCE = FMCIBCEI;                       0578 00239000
         B     @RC00576                                            0578 00240000
@RF00576 OI    FMCIBCE(FMCBPTR),B'00001100'                        0578 00241000
*                END;                       /* EXPECTED CHAIN IS SET  * 00242000
*           ELSE DO;                        /* CHAINING ERROR         * 00243000
         B     @RC00570                                            0580 00244000
@RF00570 DS    0H                                                  0581 00245000
*/********************************************************************* 00246000
*/*                                                                   * 00247000
*/* EXR - SEGMENT OF ISTRCC51                                         * 00248000
*/*                                                                   * 00249000
*/* THIS SEGMENT IS ENTERED ONLY IF A CHAINING EXCEPTION IS FOUND.    * 00250000
*/* IF A CHAINING EXCEPTION IS DISCOVERED ON AN EXCEPTION REQUEST     * 00251000
*/* THE CHAIN ERROR BIT IN THE SYSTEM SENSE IS TURNED ON. IF THE      * 00252000
*/* INPUT WAS NOT ALREADY AN EXCEPTION REQUEST, IT IS MADE INTO       * 00253000
*/* ONE. A CHAIN ERROR MAY OCCUR ONLY ON DFSYN,CONTROL=DATA REQUESTS. * 00254000
*/********************************************************************* 00255000
*RUPTR = FDBDATAA;                          /*POINT TO RU             * 00256000
         L     RUPTR,FDBDATAA+32(,FSBPTR)                          0581 00257000
*RFY ISTSRU BASED(RUPTR);                   /*ADDRESS THE RU          * 00258000
*        RHSENSE = ONEB;                    /* SET SENSE INCLUDED     * 00259000
         OI    RHSENSE(RHPTR),B'00000100'                          0583 00260000
*        SRUSENSE = 0;                      /* ZERO SENSE FIELD       * 00261000
         SLR   @10,@10                                             0584 00262000
         STCM  @10,15,SRUSENSE(RUPTR)                              0584 00263000
*        SRUSNS1 = SRUSTATE;                /* SET CHAIN ERROR        * 00264000
         MVI   SRUSNS1(RUPTR),X'20'                                0585 00265000
*        SRUSNS2 = SRUCHAIN;                                       0586 00266000
         MVI   SRUSNS2(RUPTR),X'02'                                0586 00267000
*RFY ISTSRU;                                /*DROP RU                 * 00268000
*/********************************************************************* 00269000
*/* END OF SEGMENT EXR                                                * 00270000
*/********************************************************************* 00271000
*                END;                       /* CHAIN ERROR PROCESSED  * 00272000
*        END;                               /* CHAINING CHECK DONE    * 00273000
@RC00570 DS    0H                                                  0590 00274000
*/********************************************************************* 00275000
*/*                                                                   * 00276000
*/* SYNCH - SEGMENT OF ISTRCC51                                       * 00277000
*/*                                                                   * 00278000
*/* THIS SEGMENT IS ENTERED TO HANDLE DFSYN INPUT.                    * 00279000
*/* IF A CEXIT HAS BEEN SPECIFIED, IT WILL ALWAYS BE SCHEDULED.       * 00280000
*/* REGARDLESS OF WHETHER OR NOT A CEXIT IS SCHEDULED, IF AN          * 00281000
*/* APPROPRIATE RPL IS AVAILABLE, IT WILL BE USED. IF THE FMCB        * 00282000
*/* SPECIFIES THAT AN ACB EXIT MAY BE USED (ASYIPX), THAT EXIT        * 00283000
*/* WILL BE SCHEDULED ONLY IF THE CEXIT HAS NOT BEEN SCHEDULED.       * 00284000
*/* IF THE INPUT IS DFSYN,CONTROL OR AN EXCEPTION REQUEST,            * 00285000
*/* THEN THE SIGNIFICANT PARTS OF THE RU WILL BE TRANSFERRED TO       * 00286000
*/* THE USER AS RPL PARAMETERS AND BOTH THE UECB DATA LENGTH          * 00287000
*/* AND THR RPLRLEN ARE ZERO. OTHERWISE, THESE DATA LENGTHS           * 00288000
*/* EQUAL THE DATA COUNT OF THE RH (3 BYTES).                         * 00289000
*/********************************************************************* 00290000
*RFY RUPTR UNRSTD;                                                 0590 00291000
@RT00567 DS    0H                                                  0591 00292000
*IF (FMCFBA & ZAPREG) ^= 0                  /* DATA ON QUEUE ?        * 00293000
*   THEN CRRSW5 = ONEB;                     /* YES, FLAG TO QUEUE     * 00294000
         L     @10,FMCFBA(,FMCBPTR)                                0591 00295000
         N     @10,@CF02707                                        0591 00296000
         LTR   @10,@10                                             0591 00297000
         BZ    @RF00591                                            0591 00298000
         OI    CRRSW5(CRRPTR),B'00000100'                          0592 00299000
*   ELSE DO;                                /* NO, TRY TO SATISFY     * 00300000
         B     @RC00591                                            0593 00301000
@RF00591 DS    0H                                                  0594 00302000
*        CALL FNDRPL;                       /* FIND AN RPL            * 00303000
         BAL   @14,FNDRPL                                          0594 00304000
*        IF CRRRPLS = ZERB                  /* RPL FOUND ?            * 00305000
*           THEN DO;                        /* NO, CHECK ACDEB        * 00306000
         TM    CRRRPLS(CRRPTR),B'10000000'                         0595 00307000
         BNZ   @RF00595                                            0595 00308000
*                IF FMCFLGA1 = ONEB         /* SATISFY SPECIFIC ?     * 00309000
*                   THEN CRRSW5 = ONEB;     /* YES, CAN'T USE DEB     * 00310000
         TM    FMCFLGA1(FMCBPTR),B'01000000'                       0597 00311000
         BNO   @RF00597                                            0597 00312000
         OI    CRRSW5(CRRPTR),B'00000100'                          0598 00313000
*                   ELSE DO;                                       0599 00314000
         B     @RC00597                                            0599 00315000
@RF00597 DS    0H                                                  0600 00316000
*                                CALL LKDEB; /* LOOK FOR RPL ON DEB   * 00317000
         BAL   @14,LKDEB                                           0600 00318000
*                                IF CRRRPLS = ZERB /* FIND AN RPL ?   * 00319000
*                                   THEN CRRSW5 = ONEB; /* NO, SET Q  * 00320000
         TM    CRRRPLS(CRRPTR),B'10000000'                         0601 00321000
         BNZ   @RF00601                                            0601 00322000
         OI    CRRSW5(CRRPTR),B'00000100'                          0602 00323000
*                        END;                                      0603 00324000
@RF00601 DS    0H                                                  0604 00325000
*                END;                                              0604 00326000
@RC00597 DS    0H                                                  0605 00327000
*        END;                                                      0605 00328000
@RF00595 DS    0H                                                  0606 00329000
*IF CRRSW5 = ONEB                           /* NEED TO KEEP FSB ?     * 00330000
*   THEN CALL XFERFSB;                      /* YES                    * 00331000
@RC00591 TM    CRRSW5(CRRPTR),B'00000100'                          0606 00332000
         BNO   @RF00606                                            0606 00333000
         BAL   @14,XFERFSB                                         0607 00334000
*RFY ISTPROCD;                              /* DROP OPTIONS           * 00335000
@RF00606 DS    0H                                                  0609 00336000
*/********************************************************************* 00337000
*/* END OF SEGMENT SYNCH                                              * 00338000
*/********************************************************************* 00339000
*/********************************************************************* 00340000
*/* END OF PROCEDURE CKCHAIN                                          * 00341000
*/********************************************************************* 00342000
*                 REG7 = 0;/* SET DVT PTR TO SYNCH FDBK @Z30APGH*/ 0609 00343000
         SLR   REG7,REG7                                           0609 00344000
*       END;                                /* DONE SYNCH PROCESSING  * 00345000
*  ELSE DO;                                 /* PROCESS ASYNSCRONOUS   * 00346000
         B     @RC00564                                            0611 00347000
@RF00564 DS    0H                                                  0612 00348000
*/********************************************************************* 00349000
*/*                                                                   * 00350000
*/* ASYN - SEGMENT OF ISTRCC51                                        * 00351000
*/*                                                                   * 00352000
*/* THIS SEGMENT IS ENTERED WHENEVER THE FSB TO BE HANDLED            * 00353000
*/* REPRESENTS RESP OR DFASY INPUT. IT'S FUNCTION IS TO FIND          * 00354000
*/* EITHER AN EXIT TO SCHEDULE OR AN RPL TO SATISFY. IN A NUMBER      * 00355000
*/* OF INSTANCES, LOGIC IS REQUIRED TO CHECK IF THE SEGMENT WAS       * 00356000
*/* ENTERED FOR DFASY OR RESP INPUT AND PROCEDE ACCORDINGLY.          * 00357000
*/* DFASY AND RESP INPUT MAY CAUSE AN EXIT TO BE SCHEDULED OR MAY     * 00358000
*/* SATISFY AN RPL, BUT NEVER BOTH. A CEXIT (EXIT LIST SPECIFIED      * 00359000
*/* IN THE FMCB) WILL ALWAYS BE SCHEDULED IF THE APPROPRIATE ONE      * 00360000
*/* IS ACTIVE. HOWEVER, IF DATA OF THE TYPE IN QUESTION IS ALREADY    * 00361000
*/* ON THE QUEUE, AN RPL MAY NOT BE SATISFIED AND AN ACB EXIT WILL    * 00362000
*/* NOT BE SCHEDULED. THIS IS TO INSURE THAT DATA IS PASSED TO THE    * 00363000
*/* USER IN THE ORDER OF ARRIVAL FROM THE LOGICAL UNIT.               * 00364000
*/********************************************************************* 00365000
*CRRRTN1 = FMCEXLST;                        /* POINT TO FMCB EXIT LIST* 00366000
         L     @10,FMCEXLST(,FMCBPTR)                              0612 00367000
         ST    @10,CRRRTN1(,CRRPTR)                                0612 00368000
*CALL FNDEXIT;                              /* TRY TO FIND EXIT       * 00369000
         BAL   @14,FNDEXIT                                         0613 00370000
*IF CRREXTS = ZERB                          /* EXIT NOT SCHEDULED ?   * 00371000
*   THEN DO;                                /* YES                    * 00372000
         TM    CRREXTS(CRRPTR),B'00000100'                         0614 00373000
