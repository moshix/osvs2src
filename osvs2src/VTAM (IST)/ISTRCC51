*/*                          WAITUP                                   * 01493000
*/********************************************************************* 01494000
*/* ROUTINE NAME: WAITUP                                              * 01495000
*/* FUNCTION:  WAIT FOR STORAGE                                       * 01496000
*/* OUTPUT: NONE                                                      * 01497000
*/* SUBROUTINES CALLED: NONE                                          * 01498000
*/* MACROS USED: TPWAIT                                               * 01499000
*WAITUP: PROC OPTIONS (NOSAVE,NOSAVEAREA);                         1079 01500000
*                                                                  1079 01501000
WAITUP   DS    0H                                                  1080 01502000
*/*  TPWAIT   */                                                   1080 01503000
*         DO; /* BEGIN TPWAIT GENERATION*/                         1080 01504000
*         RFY (GPR01P,GPR14P,GPR15P) RSTD;                         1081 01505000
*         RFY (GPR00P) RSTD;                                       1082 01506000
*         GEN (STM   GPR00P,GPR15P,RPHWORK(GPR01P))                1083 01507000
*         REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);                1083 01508000
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)                            01509000
*         RFY (GPR00P) UNRSTD;                                     1084 01510000
*         GPR15P=ADDR(LHAA0117);                                   1085 01511000
         LA    GPR15P,LHAA0117                                     1085 01512000
*         GPR14P=0;                                                1086 01513000
         SLR   GPR14P,GPR14P                                       1086 01514000
*         GPR15P=GPR15P & '00FFFFFF'X;                             1087 01515000
         N     GPR15P,@CF04644                                     1087 01516000
*         GPR15P=GPR15P | RPHWT1;                                  1088 01517000
         O     GPR15P,@CF01313                                     1088 01518000
*         CS (GPR14P,GPR15P,GPR01P->RPHRESMA);                     1089 01519000
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         1089 01520000
*         BC (7,LHAB0117);                                         1090 01521000
*                                                                  1090 01522000
         BC    7,LHAB0117                                          1090 01523000
*/* TPEXIT  NONE */                                                1091 01524000
*         DO;                                                      1091 01525000
*           RESPECIFY GPR15P RSTD;                                 1092 01526000
*                                                                  1092 01527000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)  */                         1093 01528000
*/* END OF ATBUILD */                                              1093 01529000
*                                                                  1093 01530000
*         DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */            1093 01531000
*           GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/            1094 01532000
         L     GPR15P,ATCLCPTR                                     1094 01533000
*         END; /*LISATCVT*/                                        1095 01534000
*           RESPECIFY GPR00F RSTD;                                 1096 01535000
*           GPR00F=0;     /*SET PARAMETER REGISTER TO 0*/          1097 01536000
         SLR   GPR00F,GPR00F                                       1097 01537000
*           GPR15P=GPR15P->ATCAP31;                                1098 01538000
         L     GPR15P,ATCAP31(,GPR15P)                             1098 01539000
*           DCL FMZL0122 BASED(GPR15P) LABEL VALUERANGE(*);        1099 01540000
*           CALL FMZL0122;                                         1100 01541000
