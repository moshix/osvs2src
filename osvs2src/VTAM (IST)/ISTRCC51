*   RPLPTR = RPLPTR & ZAPREG;               /* INSURE HIGH BIT CLEAR  * 00702000
         N     RPLPTR,@CF02707                                     0736 00703000
*END;                                       /* END SEARCH LOOP        * 00704000
@DE00718 SLR   @10,@10                                             0737 00705000
         CR    RPLPTR,@10                                          0737 00706000
         BNE   @DL00718                                            0737 00707000
*IF CRRBARAD ^= 0                           /* FIND AN RPL            * 00708000
*   THEN DO;                                /* YES                    * 00709000
*                                                                  0738 00710000
         L     @04,CRRBARAD(,CRRPTR)                               0738 00711000
         CR    @04,@10                                             0738 00712000
         BE    @RF00738                                            0738 00713000
*/* TPDEQ (CRRBARAD,QUEPTR->HEADER) RETPTR(RPLPTR) */              0740 00714000
*         DO; /* BEGIN TPDEQ CODE GENERATION */                    0740 00715000
*         DCL FMZCA031 BIT(32) CONSTANT('80000000'X);              0741 00716000
*         DCL FMZCB031 BIT(32) CONSTANT('7FFFFFFF'X);              0742 00717000
*         RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                  0743 00718000
*                                                                  0743 00719000
*         RFY ISTWKE  ;                                            0744 00720000
*         GPR15P=ADDR(QUEPTR->HEADER); /*ADDRESS QUEUE POINTER */  0745 00721000
         LR    GPR15P,QUEPTR                                       0745 00722000
*         RFY (GPR02P,GPR03P) RSTD;                                0746 00723000
*         STM(GPR02P,GPR03P,GPR01P->RPHWORK(5:12));                0747 00724000
         STM   GPR02P,GPR03P,RPHWORK+4(GPR01P)                     0747 00725000
*         GPR03P=CRRBARAD | FMZCA031;                              0748 00726000
         L     @10,@CF02181                                        0748 00727000
         OR    @04,@10                                             0748 00728000
         LR    GPR03P,@04                                          0748 00729000
*         GPR14P=FMZCA031;                                         0749 00730000
         LR    GPR14P,@10                                          0749 00731000
*         GPR15P=ADDR(GPR15P->PABWEQA);                            0750 00732000
