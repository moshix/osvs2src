*   RESPECIFY                                                      0716 00811000
*     ISTFDB BASED(WORKREG);        /* ADDRESS THE FDB               */ 00812000
*   DO I=0 BY 1 WHILE WORKREG^=0;   /* COUNT THE FDBS                */ 00813000
         SLR   I,I                                                 0717 00814000
         B     @DE00717                                            0717 00815000
@DL00717 DS    0H                                                  0718 00816000
*     WORKREG=FDBFDBA;              /* GET NEXT FDB                  */ 00817000
         L     WORKREG,FDBFDBA(,WORKREG)                           0718 00818000
*     WORKREG=WORKREG&ZAPREG;       /* CLEAR HIGH BIT                */ 00819000
         N     WORKREG,@CF01618                                    0719 00820000
*   END;                            /* ALL FDBS COUNTED              */ 00821000
         AL    I,@CF00046                                          0720 00822000
@DE00717 LTR   WORKREG,WORKREG                                     0720 00823000
         BNZ   @DL00717                                            0720 00824000
*   RESPECIFY                                                      0721 00825000
*     ISTFDB;                       /* DROP FDB                      */ 00826000
*   FMCBUFNB=FMCBUFNB+I;            /* BUMP BUFFER COUNT             */ 00827000
         LH    @12,FMCBUFNB(,FMCBPTR)                              0722 00828000
         N     @12,@CF04556                                        0722 00829000
         ALR   @12,I                                               0722 00830000
         STH   @12,FMCBUFNB(,FMCBPTR)                              0722 00831000
*   IF FMCBUFNB>FMCBUFLM            /* AT BUFFER LIMIT ?             */ 00832000
*     THEN                                                         0723 00833000
         CLC   FMCBUFNB(2,FMCBPTR),FMCBUFLM(FMCBPTR)               0723 00834000
         BNH   @RF00723                                            0723 00835000
*     DO;                           /* YES                           */ 00836000
*       IF FMCHOLD=ZERB             /* HOLD IN EFFECT ?              */ 00837000
*         THEN                                                     0725 00838000
         TM    FMCHOLD(FMCBPTR),B'10000000'                        0725 00839000
         BNZ   @RF00725                                            0725 00840000
*         DO;                       /* NO                            */ 00841000
*           CRRBFLGS=CRRHOLD;       /* SET DO HOLD                   */ 00842000
         MVI   CRRBFLGS(CRRPTR),X'04'                              0727 00843000
*           RESPECIFY                                              0728 00844000
*             SAVEPTR RESTRICTED;   /* GET SAVE POINTER              */ 00845000
*           SAVEPTR=ADDR(CRRSAVE1); /* GET SAVE AREA                 */ 00846000
         LA    SAVEPTR,CRRSAVE1(,CRRPTR)                           0729 00847000
*           CALL ISTRCC26;          /* ISSUE HOLD                    */ 00848000
         L     @12,CRRATCVT(,CRRPTR)                               0730 00849000
         L     @15,ATCRCC26(,@12)                                  0730 00850000
         BALR  @14,@15                                             0730 00851000
*           RESPECIFY                                              0731 00852000
*             SAVEPTR UNRESTRICTED; /* RELEASE SAVE AREA             */ 00853000
*         END;                      /* HOLD ISSUED                   */ 00854000
*     END;                          /* BUFFER COUNT UPDATED          */ 00855000
@RF00725 DS    0H                                                  0734 00856000
*   RFY                                                            0734 00857000
*     RETPTR RSTD;                  /* GET THE RETURN REG            */ 00858000
@RF00723 DS    0H                                                  0735 00859000
*   RETPTR=CRRRTN10;                /* RESTORE RETURN ADDR           */ 00860000
         L     RETPTR,CRRRTN10(,CRRPTR)                            0735 00861000
*   END XFERFSB;                    /* END XFERFSB                   */ 00862000
*                                                                  0736 00863000
@EL00002 DS    0H                                                  0736 00864000
@EF00002 DS    0H                                                  0736 00865000
@ER00002 BR    @14                                                 0736 00866000
*   /*****************************************************************/ 00867000
*   /*                                                               */ 00868000
*   /* NOTRUNC - TRUNCATION WAS NOT SPECIFIED, THE IF THE FSB IS NOT */ 00869000
*   /* IN PAGEABLE STORAGE CALL THE XFERFSB ROUTINE TO MOVE IT       */ 00870000
*   /*                                                               */ 00871000
*   /*****************************************************************/ 00872000
*                                                                  0737 00873000
*NOTRUNC:                                                          0737 00874000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0737 00875000
NOTRUNC  DS    0H                                                  0738 00876000
*   CRRRTN9=RETPTR;                 /* SAVE THE RETURN ADDR          */ 00877000
         ST    RETPTR,CRRRTN9(,CRRPTR)                             0738 00878000
*   RFY                                                            0739 00879000
*     RETPTR UNRSTD;                /* RELEASE THE REG               */ 00880000
*   RPLRLEN=CRRDTACT;               /* SAVE THE LENGTH MOVED         */ 00881000
         L     @12,CRRARPL(,CRRPTR)                                0740 00882000
         LH    @06,CRRDTACT(,CRRPTR)                               0740 00883000
         ST    @06,RPLRLEN(,@12)                                   0740 00884000
*   FSBTHCNT=FSBTHCNT-CRRBTLEN;     /* GET NEW COUNT OF DATA         */ 00885000
*                                                                  0741 00886000
         LH    @12,FSBTHCNT(,FSBPTR)                               0741 00887000
         SL    @12,CRRBTLEN(,CRRPTR)                               0741 00888000
         STH   @12,FSBTHCNT(,FSBPTR)                               0741 00889000
*   /*****************************************************************/ 00890000
*   /*                                                               */ 00891000
*   /* IF RCC52 WAS CALL FROM RCC21 THE DATA IS IN PAGEABLE STORAGE  */ 00892000
*   /* AND THERE IS NO NEED TO CALL XFERFSB                          */ 00893000
*   /*                                                               */ 00894000
*   /*****************************************************************/ 00895000
*                                                                  0742 00896000
*   IF CRRRC21=ZERB THEN            /* NOT CALLED BY RCC21           */ 00897000
         TM    CRRRC21(CRRPTR),B'10000000'                         0742 00898000
         BNZ   @RF00742                                            0742 00899000
*     CALL XFERFSB;                                                0743 00900000
         BAL   @14,XFERFSB                                         0743 00901000
*   RFY                                                            0744 00902000
*     SAVEPTR RSTD;                                                0744 00903000
@RF00742 DS    0H                                                  0745 00904000
*   SAVEPTR=ADDR(CRRSAVE1);                                        0745 00905000
         LA    SAVEPTR,CRRSAVE1(,CRRPTR)                           0745 00906000
*   CALL ISTRCC65;                                                 0746 00907000
         L     @12,CRRATCVT(,CRRPTR)                               0746 00908000
         L     @15,ATCRCC65(,@12)                                  0746 00909000
         BALR  @14,@15                                             0746 00910000
*   SAVEPTR=ADDR(CRRSAVE);                                         0747 00911000
*                                                                  0747 00912000
         LR    SAVEPTR,CRRPTR                                      0747 00913000
*   /*****************************************************************/ 00914000
*   /*                                                               */ 00915000
*   /* TURN OFF ALL THE RESPOND FLAGS IN THE RPL                     */ 00916000
*   /*                                                               */ 00917000
*   /*****************************************************************/ 00918000
*                                                                  0748 00919000
*   RPLNFME=ONEB;                   /* RESET FME IN THE RPL          */ 00920000
*   RPLRRN=ZERB;                    /* RESET RRN IN THE RPL          */ 00921000
*   RPLEX=ZERB;                     /* RESET EXCEPTION IN RPL        */ 00922000
*                                                                  0750 00923000
         L     @12,CRRARPL(,CRRPTR)                                0750 00924000
         OI    RPLNFME+16(@12),B'00000010'                         0750 00925000
         NI    RPLRRN+16(@12),B'11111010'                          0750 00926000
*   /*****************************************************************/ 00927000
*   /*                                                               */ 00928000
*   /* CALL THE HOUSE KEEP ANY ROUTINE AND SET THE DO POST FLAG      */ 00929000
*   /*                                                               */ 00930000
*   /*****************************************************************/ 00931000
*                                                                  0751 00932000
*   SAVEPTR=ADDR(CRRSAVE1);                                        0751 00933000
         LA    SAVEPTR,CRRSAVE1(,CRRPTR)                           0751 00934000
*   CALL ISTRCC63;                  /* CALL HOUSE KEEP ANY RTN       */ 00935000
         L     @12,CRRATCVT(,CRRPTR)                               0752 00936000
         L     @15,ATCRCC63(,@12)                                  0752 00937000
         BALR  @14,@15                                             0752 00938000
*   SAVEPTR=ADDR(CRRSAVE);                                         0753 00939000
         LR    SAVEPTR,CRRPTR                                      0753 00940000
*   CRRPOST=ONEB;                   /* SET THE DO POST               */ 00941000
         OI    CRRPOST(CRRPTR),B'00010000'                         0754 00942000
*   RFY                                                            0755 00943000
*     RETPTR RSTD;                                                 0755 00944000
*   RETPTR=CRRRTN9;                 /* RESTORE THE RETURN ADDR       */ 00945000
         L     RETPTR,CRRRTN9(,CRRPTR)                             0756 00946000
*   RETURN;                                                        0757 00947000
@EL00003 DS    0H                                                  0757 00948000
@EF00003 DS    0H                                                  0757 00949000
@ER00003 BR    @14                                                 0757 00950000
*   END NOTRUNC;                    /* END THE SUBRTN                */ 00951000
@PB00003 DS    0H                                                  0758 00952000
*/* TPEPILOG                                                         */ 00953000
*                                                                  0759 00954000
*   GEN(PRINT OFF);                                                0759 00955000
         PRINT OFF                                                      00956000
*/* TEMPLATE FOR CHAIN FIELD OF WORK ELEMENT */                    0760 00957000
*DCL  1  ISTWKE BDY(WORD) BASED(WKEPTR),                           0760 00958000
*        2  * CHAR(4),                 /* SPACE                       * 00959000
*        2  WKECHAIN PTR(31),          /* CHAIN POINTER               * 00960000
*           3 WKEFLAGS BIT(8),         /* FLAGS                       * 00961000
*             4 WKEGATE  BIT(1),       /* GATE BIT                    * 00962000
*             4 *        BIT(7),       /* RESERVED                    * 00963000
*           3 WKECHNGP PTR(24);        /* PTR ADDRESS                 * 00964000
*   GEN(PRINT ON);                                                 0761 00965000
*                                                                  0761 00966000
         PRINT ON                                                       00967000
*/* END OF TPEPILOG                                                  */ 00968000
*                                                                  0762 00969000
*   END ISTRCC52                                                   0762 00970000
