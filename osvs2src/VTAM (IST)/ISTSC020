         TITLE 'ISTSC020 - CECOM                                       *00001000
                        '                                               00002000
ISTSC020 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ISTSC020  75.177'                                 0001 00009000
S20CEC03 DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00181-S20CEC03)                                   00013000
         ENTRY S20CEC03                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  ST    @14,12(,@13)                                        0001 00016000
         STM   @00,@12,20(@13)                                     0001 00017000
         BALR  @11,0                                               0001 00018000
@PSTART  DS    0H                                                  0001 00019000
         USING @PSTART,@11                                         0001 00020000
         AH    @15,4(,@15)                                         0001 00021000
         BR    @15                                                 0001 00022000
@EP00001 DS    0H                                                  0002 00023000
         EJECT                                                          00024000
*        SVESC020(2) = R13;             /* SET UP BACK PTR           */ 00025000
         ST    R13,SVESC020+4(,WAPTR)                              0065 00026000
*        BACKSAVE(3) = ADDR(SVESC020);  /* PUT FORWARD CHAIN IN LAST */ 00027000
         LA    @12,SVESC020(,WAPTR)                                0066 00028000
         ST    @12,BACKSAVE+8(,R13)                                0066 00029000
*        R13 = ADDR(SVESC020);          /* POINT TO SAVE AREA FOR MOD*/ 00030000
         LR    R13,@12                                             0067 00031000
*        $C2WAY='0'B;                   /* SET 2 WAY SWITCH OFF      */ 00032000
         NI    $C2WAY(WAPTR),B'01111111'                           0068 00033000
*        $CINCT=ZERO;                   /* ZERO OUT IN COUNT PARAM   */ 00034000
         L     @12,ZERO                                            0069 00035000
         ST    @12,$CINCT(,WAPTR)                                  0069 00036000
*        $CINADR=ZERO;                  /* ZERO OUT IN ADDR PARAM    */ 00037000
         ST    @12,$CINADR(,WAPTR)                                 0070 00038000
*        IF $TCECM = '1'B               /* IF IT IS A TCEC CALL      */ 00039000
*          THEN GOTO S20CEC2;           /* THEN GO TO PROCESS IT     */ 00040000
         TM    $TCECM(WAPTR),B'00000001'                           0071 00041000
         BO    @RT00071                                            0071 00042000
*          ELSE GOTO S20CEC1;           /* ELSE PROCESS CECOM CALL   */ 00043000
         B     S20CEC1                                             0073 00044000
*/********************************************************************/ 00045000
*/*                           C E C O M                              */ 00046000
*/*                                                                  */ 00047000
*/*  THIS SECTION HANDLES ALL 'CECOM' CALLS. IT VERIFIES THE VALIDITY*/ 00048000
*/*  OF THE INPUT PARMMETER FOR BOTH 1-WAY AND 2-WAY COMMUNICATIONS. */ 00049000
*/*  IT ALSO TACKS ON THE PROPER ID TO THE MESSAGE                   */ 00050000
*/********************************************************************/ 00051000
*S20CEC1:                               /* START OF $CCOM CALL       */ 00052000
*        CECMAP=R1;                     /* SET UP PARM PTR           */ 00053000
S20CEC1  LR    CECMAP,R1                                           0074 00054000
*        IF $CEINADR ^=0                /* TWO WAY COMMUNICATION?    */ 00055000
*              THEN                     /* YES - CONTINUE BELOW      */ 00056000
         SLR   @12,@12                                             0075 00057000
         L     @10,$CEINADR(,CECMAP)                               0075 00058000
         CR    @10,@12                                             0075 00059000
         BE    @RF00075                                            0075 00060000
*/***************************************/                         0076 00061000
*/* CK FOR VALID 'IN' COUNT.            */                         0076 00062000
*/*  IN CNT > 61 = ERROR (RET CODE=8)   */                         0076 00063000
*/*  SET 2WAY COMMUNICATION SW          */                         0076 00064000
*/*  ZERO OUT READ (IN) BUFFER          */                         0076 00065000
*/***************************************/                         0076 00066000
*                DO;                                               0076 00067000
*                  $C2WAY='1'B;         /* TURN ON TWO WAY SWITCH    */ 00068000
         OI    $C2WAY(WAPTR),B'10000000'                           0077 00069000
*                  IF $CEINCT < 0       /* IF 'IN' CNT IS LT ZERO    */ 00070000
*                    THEN GOTO S20CRC08; /* ERR RET WITH RET CODE=8  */ 00071000
         L     @09,$CEINCT(,CECMAP)                                0078 00072000
         CR    @09,@12                                             0078 00073000
         BL    @RT00078                                            0078 00074000
*                  IF $CEINCT > 61 THEN /* IF 'IN' CNT IS GT 61 THEN */ 00075000
         C     @09,@CF00726                                        0080 00076000
         BH    @RT00080                                            0080 00077000
*                    GOTO S20CRC08;     /*  RET TO CALLER W RC=8     */ 00078000
*                  $CINADR=$CEINADR;    /* PASS 'IN' ADDR TO MSG I/O */ 00079000
         ST    @10,$CINADR(,WAPTR)                                 0082 00080000
*                  R14=$CINADR;         /* PUT BUFFER ADDR INTO REG14*/ 00081000
         LR    R14,@10                                             0083 00082000
*                  $CINCT=$CEINCT;      /* PASS 'IN' CNT TO MSG I/O  */ 00083000
         ST    @09,$CINCT(,WAPTR)                                  0084 00084000
*                  R15=$CEINCT;         /* LNGTH OF BUF - 1 IN REG15 */ 00085000
         LR    R15,@09                                             0085 00086000
*            GEN REFS(R15,S20CEC1A) ( EX R15,S20CEC1A);  /* ZERO OUT */ 00087000
          EX R15,S20CEC1A                                               00088000
*                  $CINADR=$CINADR+1;   /* ALLOW 1 BYTE FOR LNGTH FLD*/ 00089000
         LA    @12,1                                               0087 00090000
         AL    @12,$CINADR(,WAPTR)                                 0087 00091000
         ST    @12,$CINADR(,WAPTR)                                 0087 00092000
*                END;                                              0088 00093000
*/***************************************/                         0089 00094000
*/* VERIFY 'OUT' MSG DATA.              */                         0089 00095000
*/*  OUT CNT < 1 = ERROR (RET CDE=8)    */                         0089 00096000
*/*  OUT CNT > 61 ITS RESET TO 61       */                         0089 00097000
*/***************************************/                         0089 00098000
*        IF $CEOUTCT<'01'X THEN         /* IF OUT CNT IS LT ONE      */ 00099000
@RF00075 L     @12,$CEOUTCT(,CECMAP)                               0089 00100000
         LTR   @12,@12                                             0089 00101000
         BNP   @RT00089                                            0089 00102000
*          GOTO S20CRC08;               /*  THEN GO SET RET CODE     */ 00103000
*        $COUTAD=ADDR($WORK);           /* SET UP 'OUT' MSG PARAMETER*/ 00104000
         LA    @10,$WORK(,WAPTR)                                   0091 00105000
         ST    @10,$COUTAD(,WAPTR)                                 0091 00106000
*        IF $CEOUTCT > 61               /* IF PARM CNT       @YA04062*/ 00107000
*             THEN                     /* GTR THAN 61        @YA04062*/ 00108000
         LA    @10,61                                              0092 00109000
         CR    @12,@10                                             0092 00110000
         BNH   @RF00092                                            0092 00111000
*               $COUTCT=61;            /* TRUNCATE TO        @YA04062*/ 00112000
         ST    @10,$COUTCT(,WAPTR)                                 0093 00113000
*                ELSE                  /* 61 ELSE USE CNT    @YA04062*/ 00114000
*        $COUTCT=$CEOUTCT;              /* FOR MSG I/O MODULE        */ 00115000
         B     @RC00092                                            0094 00116000
@RF00092 L     @12,$CEOUTCT(,CECMAP)                               0094 00117000
         ST    @12,$COUTCT(,WAPTR)                                 0094 00118000
*        CECOMSW='1'B;                  /* SET CECOM SW ON           */ 00119000
@RC00092 OI    CECOMSW,B'10000000'                                 0095 00120000
*        GEN  (BAL RETR,S20C01);        /* SET UP MSG PREFIX         */ 00121000
         BAL RETR,S20C01                                                00122000
*        $WORK(I:I+$COUTCT-1)=CEOUTMSG; /* MOVE MSG TO $WORK + PREFIX*/ 00123000
         LH    @12,I(,WAPTR)                                       0097 00124000
         N     @12,@CF00786                                        0097 00125000
         ALR   @12,WAPTR                                           0097 00126000
         L     @10,$COUTCT(,WAPTR)                                 0097 00127000
         LR    @09,@10                                             0097 00128000
         BCTR  @09,0                                               0097 00129000
         L     @08,$CEOUTAD(,CECMAP)                               0097 00130000
         EX    @09,@SM00787                                        0097 00131000
*        $COUTCT = $COUTCT+I-1;         /* ADD PREFIX LEN TO OUT CNT */ 00132000
         LH    @12,I(,WAPTR)                                       0098 00133000
         N     @12,@CF00786                                        0098 00134000
         ALR   @10,@12                                             0098 00135000
         BCTR  @10,0                                               0098 00136000
         ST    @10,$COUTCT(,WAPTR)                                 0098 00137000
*        GOTO S20CEC2D;                 /* GO TO COMMON EXIT TO MSGIO*/ 00138000
         B     S20CEC2D                                            0099 00139000
*S20CEC1A: GEN REFS(R14) ( XC 0(0,R14),0(R14));  /* ZERO OUT READ BUF*/ 00140000
S20CEC1A  XC 0(0,R14),0(R14)                                            00141000
*        GEN   (EJECT);                                            0101 00142000
         EJECT                                                          00143000
*/********************************************************************/ 00144000
*/*                           T C E C                                */ 00145000
*/*                                                                  */ 00146000
*/*  THIS SECTION HANDLES ALL 'TCEC' CALLS. IT TAKES A MSG NUMBER AND*/ 00147000
*/*  USES IT AS AN INDEX TO A MSG TABLE. IT WILL ALSO SUBSTITUTE ANY */ 00148000
*/*  DATA FROM $INBUFR INTO THE MSG AND DELETE EXTRA BLANKS FROM THE */ 00149000
*/*  SUBSTITUTED LINE UNLESS REQUESTED NOT TO.                       */ 00150000
*/*      R1=PARAMETER LIST POINTER                                   */ 00151000
*/********************************************************************/ 00152000
*S20CEC2:                                                          0102 00153000
*        $TCECM='0'B;                   /* RESET TCEC SW IN RCA      */ 00154000
S20CEC2  NI    $TCECM(WAPTR),B'11111110'                           0102 00155000
*        IF $MULLNSW='1'B THEN          /* IF MULTIPLE LINE CALL     */ 00156000
         TM    $MULLNSW(WAPTR),B'00100000'                         0103 00157000
         BO    @RT00103                                            0103 00158000
*            GOTO S20CEC2A;             /* BR TO CHK IF SUBSTITUTION */ 00159000
*        SUBINDX='1'X;                  /* INITIALIZE $INBUFR INDEX  */ 00160000
         LA    @12,1                                               0105 00161000
         STH   @12,SUBINDX(,WAPTR)                                 0105 00162000
*        MSG#=$TCMSG#;                  /* GET CALLER'S MSG NUMBER   */ 00163000
         L     @12,$TCMSG#(,R1)                                    0106 00164000
         STH   @12,MSG#(,WAPTR)                                    0106 00165000
*        IF $TCWTO='1'B THEN            /*IF THE WRT TO OPER SW IS ON*/ 00166000
         TM    $TCWTO(R1),B'00001000'                              0107 00167000
         BNO   @RF00107                                            0107 00168000
*          $CNSLFG='1'B;                /* TURN ON CONSOLE FLG-MSG IO*/ 00169000
         OI    $CNSLFG(WAPTR),B'01000000'                          0108 00170000
*        $COUTAD=ADDR($WORK);           /* SET UP PTR TO 'OUT' MSG   */ 00171000
@RF00107 LA    @12,$WORK(,WAPTR)                                   0109 00172000
         ST    @12,$COUTAD(,WAPTR)                                 0109 00173000
*        CECOMSW='0'B;                  /* SET CECOM SW OFF          */ 00174000
         NI    CECOMSW,B'01111111'                                 0110 00175000
*        GEN  (BAL RETR,S20C01);        /* GO SET UP PREFIX ID'S     */ 00176000
         BAL RETR,S20C01                                                00177000
*        IF $TCLOCM='1'B THEN           /* IF WE DO NOT HAVE TO LOC  */ 00178000
         TM    $TCLOCM(R1),B'00010000'                             0112 00179000
         BO    @RT00112                                            0112 00180000
*          GOTO S20CEC2A;               /* MSG IN TBL THEN BR AROUND */ 00181000
*        $COUTCT=ZERO;                  /* ZERO OUT 'OUT' COUNT AREA */ 00182000
         L     @12,ZERO                                            0114 00183000
         ST    @12,$COUTCT(,WAPTR)                                 0114 00184000
*        GEN  (BAL RETR,S20CMLOC);      /* LOCATE MSG IN MSG TBL     */ 00185000
         BAL RETR,S20CMLOC                                              00186000
*        IF Y=ZERO THEN                 /* IF NO MSG TEXT WAS FOUND  */ 00187000
         SLR   @12,@12                                             0116 00188000
         IC    @12,Y(,WAPTR)                                       0116 00189000
         CL    @12,ZERO                                            0116 00190000
         BNE   @RF00116                                            0116 00191000
*          DO;                                                     0117 00192000
*            $COUTCT=I-1;               /* SET OUT CNT = PREFIX LNGTH*/ 00193000
         LH    @12,I(,WAPTR)                                       0118 00194000
         N     @12,@CF00786                                        0118 00195000
         BCTR  @12,0                                               0118 00196000
         ST    @12,$COUTCT(,WAPTR)                                 0118 00197000
*            GOTO S20CEC2C;             /* BRANCH AROUND SUBSTITUTION*/ 00198000
         B     S20CEC2C                                            0119 00199000
*          END;                                                    0120 00200000
*        $WORK(I:I+Y)=MSGDAT  ;         /* MOVE MSG TO $WORK+PREFIX  */ 00201000
@RF00116 LH    @12,I(,WAPTR)                                       0121 00202000
         N     @12,@CF00786                                        0121 00203000
         ALR   @12,WAPTR                                           0121 00204000
         SLR   @10,@10                                             0121 00205000
         IC    @10,Y(,WAPTR)                                       0121 00206000
         EX    @10,@SM00791                                        0121 00207000
*        $COUTCT=$COUTCT+I-1;           /* ADD PREFIX LEN TO OUT CNT */ 00208000
         LH    @12,I(,WAPTR)                                       0122 00209000
         N     @12,@CF00786                                        0122 00210000
         AL    @12,$COUTCT(,WAPTR)                                 0122 00211000
         BCTR  @12,0                                               0122 00212000
         ST    @12,$COUTCT(,WAPTR)                                 0122 00213000
*/***************************************/                         0123 00214000
*/* SUBSTITUTION CHECK                  */                         0123 00215000
*/***************************************/                         0123 00216000
*S20CEC2A:                                                         0123 00217000
*        IF $TCSUBDA^='1'B THEN         /* IF NO SUBSTITUTION        */ 00218000
S20CEC2A TM    $TCSUBDA(R1),B'10000000'                            0123 00219000
         BNO   @RT00123                                            0123 00220000
*          GO TO S20CEC2B;              /* THEN BR AROUND            */ 00221000
*        DO X=I TO $COUTCT BY 1;        /* LOOP UNTIL SUBSTITION DONE*/ 00222000
         LH    @12,I(,WAPTR)                                       0125 00223000
         N     @12,@CF00786                                        0125 00224000
         ST    @12,X                                               0125 00225000
         B     @DE00125                                            0125 00226000
@DL00125 DS    0H                                                  0126 00227000
*          IF $WORK(X)=DELMTR THEN      /* IF ITS A SUB CHAR THEN    */ 00228000
         ALR   @12,WAPTR                                           0126 00229000
         CLC   $WORK-1(1,@12),DELMTR                               0126 00230000
         BNE   @RF00126                                            0126 00231000
*            IF SUBINDX<81              /* CHK IF SUB DATA AVAILABLE */ 00232000
*              THEN DO;                                            0127 00233000
         LH    @12,SUBINDX(,WAPTR)                                 0127 00234000
         N     @12,@CF00786                                        0127 00235000
         C     @12,@CF00736                                        0127 00236000
         BNL   @RF00127                                            0127 00237000
*                $WORK(X)=$INBUFR(SUBINDX);   /* MOVE SUB DATA TO MSG*/ 00238000
         L     @10,X                                               0129 00239000
         ALR   @10,WAPTR                                           0129 00240000
         ALR   @12,WAPTR                                           0129 00241000
         MVC   $WORK-1(1,@10),$INBUFR-1(@12)                       0129 00242000
*                SUBINDX=SUBINDX+1;     /* UPDATE INDEX              */ 00243000
         LH    @12,SUBINDX(,WAPTR)                                 0130 00244000
         N     @12,@CF00786                                        0130 00245000
         AL    @12,@CF00103                                        0130 00246000
         STH   @12,SUBINDX(,WAPTR)                                 0130 00247000
*                   END;                                           0131 00248000
*              ELSE                     /* IF SUB DATA NOT AVAILABLE */ 00249000
*                $WORK(X)=BLANK;        /*  MOVE BLANK TO MSG        */ 00250000
         B     @RC00127                                            0132 00251000
@RF00127 LR    @12,WAPTR                                           0132 00252000
         AL    @12,X                                               0132 00253000
         MVC   $WORK-1(1,@12),BLANK                                0132 00254000
*        END;                                                      0133 00255000
@RC00127 DS    0H                                                  0133 00256000
@RF00126 LA    @12,1                                               0133 00257000
         AL    @12,X                                               0133 00258000
         ST    @12,X                                               0133 00259000
@DE00125 CL    @12,$COUTCT(,WAPTR)                                 0133 00260000
         BNH   @DL00125                                            0133 00261000
* S20CEC2B:                                                        0134 00262000
*        IF $MULLNSW='1'B THEN          /* IF MULTIPLE LINE CALL     */ 00263000
S20CEC2B TM    $MULLNSW(WAPTR),B'00100000'                         0134 00264000
         BNO   @RF00134                                            0134 00265000
*          DO;                                                     0135 00266000
*            $MULLNSW='0'B;             /* TURN OFF MULT LINE SW     */ 00267000
         NI    $MULLNSW(WAPTR),B'11011111'                         0136 00268000
*            IF $LSTLNSW='1'B           /* IF LAST LINE OF MULT CALL */ 00269000
*              THEN DO;                                            0137 00270000
         TM    $LSTLNSW(WAPTR),B'00010000'                         0137 00271000
         BNO   @RF00137                                            0137 00272000
*                $LSTLNSW='0'B;         /*  TURN OFF LAST LINE SW    */ 00273000
         NI    $LSTLNSW(WAPTR),B'11101111'                         0139 00274000
*                GOTO S20CEC2C;         /*  BR TO CHK IF 2-WAY       */ 00275000
         B     S20CEC2C                                            0140 00276000
*                   END;                                           0141 00277000
*              ELSE                     /* IF NOT LAST LINE          */ 00278000
*                GOTO S20CEC2D;         /*  BR AROUND 2-WAY CHK      */ 00279000
*          END;                                                    0143 00280000
*/***************************************/                         0144 00281000
*/* TWO-WAY MESSAGE CHECK               */                         0144 00282000
*/***************************************/                         0144 00283000
*S20CEC2C:                                                         0144 00284000
*        IF $TC2WAY='1'B THEN           /* IF IT IS A 2-WAY CALL THEN*/ 00285000
@RF00134 DS    0H                                                  0144 00286000
S20CEC2C TM    $TC2WAY(R1),B'01000000'                             0144 00287000
         BNO   @RF00144                                            0144 00288000
*          DO;                                                     0145 00289000
*            $CINCT=$TCINCT;            /* SET UP 'IN' COUNT         */ 00290000
         L     @12,$TCINCT(,R1)                                    0146 00291000
         ST    @12,$CINCT(,WAPTR)                                  0146 00292000
*            $CINADR=ADDR($INBUFR);     /* SET UP 'IN' ADDR          */ 00293000
         LA    @10,$INBUFR(,WAPTR)                                 0147 00294000
         ST    @10,$CINADR(,WAPTR)                                 0147 00295000
*            R14=$CINADR;               /* PUT BUFFER ADDR INTO REG14*/ 00296000
         LR    R14,@10                                             0148 00297000
*            R15=$CINCT;                /* PUT BUF LNGTH - 1 IN REG15*/ 00298000
         LR    R15,@12                                             0149 00299000
*      GEN REFS(R15,S20CEC1A) ( EX R15,S20CEC1A);  /* ZERO OUT IN BUF*/ 00300000
          EX R15,S20CEC1A                                               00301000
*            $CINADR=$CINADR+1;         /* ALLOW 1 BYTE FOR LNGTH FLD*/ 00302000
         LA    @12,1                                               0151 00303000
         AL    @12,$CINADR(,WAPTR)                                 0151 00304000
         ST    @12,$CINADR(,WAPTR)                                 0151 00305000
*            $C2WAY='1'B;               /* INDICATE TO MSGI/O 2-WAY  */ 00306000
         OI    $C2WAY(WAPTR),B'10000000'                           0152 00307000
*          END;                                                    0153 00308000
*/***************************************/                         0154 00309000
*/* CALLING MESSAGE I/O                 */                         0154 00310000
*/***************************************/                         0154 00311000
*S20CEC2D:                                                         0154 00312000
*        R4=R1;                         /* SAVE INPUT PARM PTR       */ 00313000
@RF00144 DS    0H                                                  0154 00314000
S20CEC2D LR    R4,R1                                               0154 00315000
*        IF $TCNOID='1'B                /* IS MSG PREFIX TO BE DRPD  */ 00316000
*              THEN                     /* IF YES DO THE FOLLOWING   */ 00317000
         TM    $TCNOID(R1),B'00100000'                             0155 00318000
         BNO   @RF00155                                            0155 00319000
*                  DO;                                             0156 00320000
*                      $COUTAD=$COUTAD+7;   /* ADD PTR PAST PREFIX   */ 00321000
         LA    @12,7                                               0157 00322000
         L     @10,$COUTAD(,WAPTR)                                 0157 00323000
         ALR   @10,@12                                             0157 00324000
         ST    @10,$COUTAD(,WAPTR)                                 0157 00325000
*                      $COUTCT=$COUTCT-7;   /* REDUCE MSG COUNT      */ 00326000
         LCR   @12,@12                                             0158 00327000
         AL    @12,$COUTCT(,WAPTR)                                 0158 00328000
         ST    @12,$COUTCT(,WAPTR)                                 0158 00329000
*                  END;                                            0159 00330000
*        R1=ADDR($CCOMDAT);             /* SET POINTER TO PARM       */ 00331000
@RF00155 LA    R1,$CCOMDAT(,WAPTR)                                 0160 00332000
*        CALL ISTCC011;                 /* CALL MESSAGE I/O          */ 00333000
         L     @15,@CV00132                                        0161 00334000
         BALR  @14,@15                                             0161 00335000
*        R1=R4;                         /* RESTORE INPUT PARM        */ 00336000
         LR    R1,R4                                               0162 00337000
*        IF $TCNOID='1'B                /* WAS ID DROPPED            */ 00338000
*              THEN                     /* IF YES DO THE FOLLOWING   */ 00339000
         TM    $TCNOID(R1),B'00100000'                             0163 00340000
         BNO   @RF00163                                            0163 00341000
*                  DO;                                             0164 00342000
*                      $COUTAD=$COUTAD-7;   /* RESET ADDRESS         */ 00343000
         LA    @12,7                                               0165 00344000
         L     @10,$COUTAD(,WAPTR)                                 0165 00345000
         SLR   @10,@12                                             0165 00346000
         ST    @10,$COUTAD(,WAPTR)                                 0165 00347000
*                      $COUTCT=$COUTCT+7;   /* RESET COUNT           */ 00348000
         AL    @12,$COUTCT(,WAPTR)                                 0166 00349000
         ST    @12,$COUTCT(,WAPTR)                                 0166 00350000
*                  END;                                            0167 00351000
*/***************************************/                         0168 00352000
*/* CHECK IF NECESARY TO WAIT ON I/O    */                         0168 00353000
*/***************************************/                         0168 00354000
*S20CEC2E:     ;                                                   0168 00355000
@RF00163 DS    0H                                                  0168 00356000
S20CEC2E DS    0H                                                  0169 00357000
*                                       /* WAS CALL DIRECT & MUST    */ 00358000
*           IF $POSTFLG='1'B &          /* WE WAIT TILL I/O COMPLETE */ 00359000
*           $TCKABND='1'B & $ABTERM='1'B                           0169 00360000
*              THEN                     /* DO THE FOLLOWING          */ 00361000
         TM    $POSTFLG(WAPTR),B'01000000'                         0169 00362000
         BNO   @RF00169                                            0169 00363000
         TM    $TCKABND(R1),B'00000100'                            0169 00364000
         BNO   @RF00169                                            0169 00365000
         TM    $ABTERM(WAPTR),B'00000010'                          0169 00366000
         BNO   @RF00169                                            0169 00367000
*                  DO;                                             0170 00368000
*                       RCAPTR=$RCA;    /* GET PTR TO R. C. AREA     */ 00369000
         L     RCAPTR,$RCA(,WAPTR)                                 0171 00370000
*                       R1=ADDR($TLTECB2);  /* SET UP REG 1          */ 00371000
         LA    R1,$TLTECB2(,RCAPTR)                                0172 00372000
*    CALL I00C1002;     /* WAIT   @D32JPPW*/                       0173 00373000
         L     @15,@CV00742                                        0173 00374000
         BALR  @14,@15                                             0173 00375000
*                       GOTO S20CEC2E;  /* LOOP BACK FOR CHECK       */ 00376000
         B     S20CEC2E                                            0174 00377000
*                  END;                                            0175 00378000
