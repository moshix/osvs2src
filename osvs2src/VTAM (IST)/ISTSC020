*                 END;                                             0294 00774000
*        GOTO S20C03;                   /* BR AROUND                 */ 00775000
         B     S20C03                                              0295 00776000
*S20C02A:                                                          0296 00777000
*        $WORK(1)=LETTERI;              /* BUILDING MSGID -FIRST LET */ 00778000
S20C02A  MVC   $WORK(1,WAPTR),LETTERI                              0296 00779000
