         B     @EL00001                                            0254 00659000
*        GEN  (EJECT);                                             0255 00660000
         EJECT                                                          00661000
*/********************************************************************/ 00662000
*/* THIS SUBROUTINE TAKES CARE OF PUTTING THE PREFIX ID ON THE MSG.  */ 00663000
*/* IF THE PREFIX IS FOR A TOLTEP MSG THEN                           */ 00664000
*/*      'ITAXXXX' WHERE XXXX IS INCLUDED AS PART OF EACH MSG TEXT   */ 00665000
*/*   IN DOS 'FXXXX' WHERE XXXX IS AS ABOVE                          */ 00666000
*/* IF THE PREFIX IS FOR AN OLT MSG THEN                             */ 00667000
*/*      'IXXNNNZ' WHERE XX  = COMPONENT NAME SUPPLIED BY THE OLT    */ 00668000
*/*                      NNN = MSG NUMBER SUPPLIED BY THE OLT        */ 00669000
*/*                      Z   = 'I' OR 'D' INDICATING 1-WAY OR 2-WAY  */ 00670000
*/*      'ITA100I' OR 'ITA101D' IS USED WHEN THE OLT FAILS TO SUPPLY */ 00671000
*/*                             THE COMPONENT NAME                   */ 00672000
*/*   IN DOS THE COMPONENT NAME IS NOT USED                          */ 00673000
*/*      'F100I' OR 'F101D'                                          */ 00674000
*/********************************************************************/ 00675000
*S20C01:                                                           0256 00676000
*        RETSAV=R14;                    /* SAVE RETURN ADDRESS       */ 00677000
S20C01   ST    R14,RETSAV(,WAPTR)                                  0256 00678000
*        I='0'X;                        /* ZERO OUT INDEX VALUE      */ 00679000
         SLR   @12,@12                                             0257 00680000
         STH   @12,I(,WAPTR)                                       0257 00681000
*        IF $TCKIDON='1'B THEN          /* IF ID ON BIT SET THEN     */ 00682000
         TM    $TCKIDON(R1),B'00000010'                            0258 00683000
         BO    @RT00258                                            0258 00684000
*          GOTO S20C03;                 /* RETURN TO THE CALLER      */ 00685000
*        $WORK(1)=BLANK;                /* BLANK OUT                 */ 00686000
         MVC   $WORK(1,WAPTR),BLANK                                0260 00687000
*        $WORK(2:N)=$WORK(1:N-1);       /* $WORK BUFFER              */ 00688000
         LH    @12,N                                               0261 00689000
         N     @12,@CF00786                                        0261 00690000
         BCTR  @12,0                                               0261 00691000
         BCTR  @12,0                                               0261 00692000
         MVC   @TS00001(256),$WORK(WAPTR)                          0261 00693000
         EX    @12,@SM00800                                        0261 00694000
*        IF CECOMSW='1'B THEN           /* IF PREFIX IS FOR OLT MSG  */ 00695000
         TM    CECOMSW,B'10000000'                                 0262 00696000
         BO    @RT00262                                            0262 00697000
*          GOTO S20C02;                 /*   THEN BR                 */ 00698000
*        IF $DRIVER = DOS87             /* IF SYSTEM IS DOS          */ 00699000
*          THEN DO;                                                0264 00700000
         CLC   $DRIVER(1,WAPTR),DOS87                              0264 00701000
         BNE   @RF00264                                            0264 00702000
*            I=1;                       /* GET LENGTH OF INT PREFIX  */ 00703000
         MVC   I(2,WAPTR),@CH00103                                 0266 00704000
*            $WORK(1)=DOSPREFX;         /* MOVE PREFIX TO MSG        */ 00705000
         MVC   $WORK(1,WAPTR),DOSPREFX                             0267 00706000
*               END;                                               0268 00707000
*          ELSE DO;                     /*IF SYSTEM IS OTHER THAN DOS*/ 00708000
         B     @RC00264                                            0269 00709000
@RF00264 DS    0H                                                  0270 00710000
*            I=LENGTH(ITA);             /* GET LENGTH OF INT PREFIX  */ 00711000
         LA    @12,3                                               0270 00712000
         STH   @12,I(,WAPTR)                                       0270 00713000
*            $WORK(1:I)=ITA;            /* MOVE PREFIX TO MSG        */ 00714000
         BCTR  @12,0                                               0271 00715000
         EX    @12,@SM00802                                        0271 00716000
*               END;                                               0272 00717000
*        GOTO S20C03;                   /* BR AROUND                 */ 00718000
         B     S20C03                                              0273 00719000
*S20C02:                                                           0274 00720000
*        IF #MSGIDAC^='0000'X & $DRIVER^=DOS87 /*IF OLT GAVE A MSGID */ 00721000
*          THEN GOTO S20C02A;           /* AND SYSTEM NOT DOS, BR    */ 00722000
S20C02   CLC   #MSGIDAC(2,WAPTR),@CB00767                          0274 00723000
         BE    @RF00274                                            0274 00724000
         CLC   $DRIVER(1,WAPTR),DOS87                              0274 00725000
         BNE   @RT00274                                            0274 00726000
*        IF $DRIVER = DOS87             /* IF SYSTEM IS DOS          */ 00727000
*          THEN                                                    0276 00728000
@RF00274 CLC   $DRIVER(1,WAPTR),DOS87                              0276 00729000
         BNE   @RF00276                                            0276 00730000
*          IF $C2WAY='1'B               /*  IF ITS A 2-WAY REQUEST   */ 00731000
*            THEN DO;                                              0277 00732000
         TM    $C2WAY(WAPTR),B'10000000'                           0277 00733000
         BNO   @RF00277                                            0277 00734000
*              I=LENGTH(DOS2WAY);       /*  GET LENGTH OF PREFIX     */ 00735000
         LA    @12,6                                               0279 00736000
         STH   @12,I(,WAPTR)                                       0279 00737000
*              $WORK(1:I)=DOS2WAY;      /*  MOVE PREFIX TO MSG       */ 00738000
         BCTR  @12,0                                               0280 00739000
         EX    @12,@SM00804                                        0280 00740000
*                 END;                                             0281 00741000
*            ELSE DO;                   /*  IF ITS A 1-WAY REQUEST   */ 00742000
         B     @RC00277                                            0282 00743000
@RF00277 DS    0H                                                  0283 00744000
*              I=LENGTH(DOS1WAY);       /*  GET LENGTH OF PREFIX     */ 00745000
