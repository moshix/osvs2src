         TITLE 'ISTSC024-DPRINT                                        *00001000
                        '                                               00002000
ISTSC024 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSC024  75.238'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @00,@12,20(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
*   R14=R13;                        /* HOLD CALLER'S SVAREA PTR      */ 00015000
         LR    R14,R13                                             0077 00016000
*   R13=ADDR(SVESC024);             /* POINT TO MY SAVE AREA         */ 00017000
         LA    R13,SVESC024(,WAPTR)                                0078 00018000
*   BACKSAVE(3)=R13;                /* PUT MY SVAREA PTR IN CALLR    */ 00019000
         STCM  R13,15,BACKSAVE+8(R14)                              0079 00020000
*   SVESC024(2)=R14;                /* PUT CALLER'S SVAREA PTR IN MY    00021000
*                                      SAVE AREA                     */ 00022000
         ST    R14,SVESC024+4(,WAPTR)                              0080 00023000
*   $24SAV13=$R13PTR;               /* SAVE PTR TO DPR CALLER        */ 00024000
         L     @14,$R13PTR(,WAPTR)                                 0081 00025000
         ST    @14,$24SAV13(,WAPTR)                                0081 00026000
*   $R13PTR=R13;                    /* MAKE RCM RET TO ME            */ 00027000
         ST    R13,$R13PTR(,WAPTR)                                 0082 00028000
*   $NOTRRCH=ON;                    /* NO RC HDLG & TRACE YET        */ 00029000
         OI    $NOTRRCH(WAPTR),B'00000010'                         0083 00030000
*   $24FLGS=0;                      /* RESET DPR FLAGS               */ 00031000
         MVI   $24FLGS(WAPTR),X'00'                                0084 00032000
*   $24RETCD=0;                     /* RESET DPR RET CODE            */ 00033000
         MVI   $24RETCD(WAPTR),X'00'                               0085 00034000
*   $PRTONLY=ON;                    /* PRINT ON AP ONLY              */ 00035000
         OI    $PRTONLY(WAPTR),B'00001000'                         0086 00036000
*   DPRPTR=R1;                      /* GET DPR LIST PTR VIA R1       */ 00037000
         LR    DPRPTR,R1                                           0087 00038000
*   PRTPTR=$PRTCTLW;                /* SET UP PTR TO CTL WORD        */ 00039000
         L     PRTPTR,$PRTCTLW(,DPRPTR)                            0088 00040000
*   TECBPTR=$PRTTECB;               /* SET UP TECB BASE REG          */ 00041000
         L     TECBPTR,$PRTTECB(,DPRPTR)                           0089 00042000
*   IF $ERRORDP=ON                  /* IF AN ERROR DPRINT,           */ 00043000
*     THEN                          /* TURN ON ERROR BITS            */ 00044000
         TM    $ERRORDP(PRTPTR),B'00000010'                        0090 00045000
         BNO   @RF00090                                            0090 00046000
*     DO;                           /* IN SCT                        */ 00047000
*       $TESTERR=ON;                                               0092 00048000
*       $ASTERIK=ON;                                               0093 00049000
         OI    $TESTERR(WAPTR),B'11000000'                         0093 00050000
*     END;                                                         0094 00051000
*   IF($LOOPTST=ON|$LOOPERR=ON)     /* IF TL OR EL OPTIONS AND       */ 00052000
*       &$NOPRT=OFF                 /* NPR OPTIONSS IN EFFECT        */ 00053000
*     THEN                          /* THEN                          */ 00054000
@RF00090 TM    $LOOPTST(WAPTR),B'00000011'                         0095 00055000
         BZ    @RF00095                                            0095 00056000
         TM    $NOPRT(WAPTR),B'00000100'                           0095 00057000
         BNZ   @RF00095                                            0095 00058000
*     DO;                           /* TURN OFF $CATERR IN SCT       */ 00059000
*       $CATERR=OFF;                /* AND                           */ 00060000
         NI    $CATERR(WAPTR),B'11011111'                          0097 00061000
*       GOTO S24C9990;              /* RETURN TO OLT                 */ 00062000
         B     S24C9990                                            0098 00063000
*     END;                                                         0099 00064000
*                                                                  0099 00065000
*/********************************************************************/ 00066000
*/* AN ERROR DPRINT CAN BE EXECUTED WHEN NO-ERROR-PRINT OPTION IS IN */ 00067000
*/* EFFECT ONLY WHEN THERE IS A CATASTROPHIC OR A FIRST ERROR        */ 00068000
*/* MESSAGE TO BE PRINTED                                            */ 00069000
*/********************************************************************/ 00070000
*                                                                  0100 00071000
*   IF $ERRORDP=ON&$NERRPRT=ON      /* IF ERROR                      */ 00072000
*     THEN                          /* DPRINT &                      */ 00073000
@RF00095 TM    $ERRORDP(PRTPTR),B'00000010'                        0100 00074000
         BNO   @RF00100                                            0100 00075000
         TM    $NERRPRT(WAPTR),B'00000100'                         0100 00076000
         BNO   @RF00100                                            0100 00077000
*     DO;                           /* NEP, AND                      */ 00078000
*       IF($CATERR=ON               /* IF CAT ERR                    */ 00079000
*           &$FSTCAT=OFF)|          /* & MSG NOT                     */ 00080000
*           ($FSTCOMM=ON            /* PRNTD OR FE                   */ 00081000
*           &$FSTFE=OFF)            /* & MSG NOT                     */ 00082000
*         THEN                                                     0102 00083000
         TM    $CATERR(WAPTR),B'00100000'                          0102 00084000
         BNO   @GL00003                                            0102 00085000
         TM    $FSTCAT(WAPTR),B'01000000'                          0102 00086000
         BZ    @RT00102                                            0102 00087000
@GL00003 TM    $FSTCOMM(WAPTR),B'00000001'                         0102 00088000
         BNO   @RF00102                                            0102 00089000
         TM    $FSTFE(WAPTR),B'10000000'                           0102 00090000
         BZ    @RT00102                                            0102 00091000
*         GOTO S24C0100;            /* PRTD GO ON                    */ 00092000
*       ELSE                                                       0104 00093000
*         GOTO S24C9990;            /* ELSE RC=0                     */ 00094000
         B     S24C9990                                            0104 00095000
*     END;                                                         0105 00096000
*S24C0100:                                                         0106 00097000
*   CVD($RTN#,DPRWORK2);            /* CONV BIN RTN# TO DECIMAL      */ 00098000
@RF00100 DS    0H                                                  0106 00099000
S24C0100 L     @14,$LOWADDR(,WAPTR)                                0106 00100000
         LH    @11,RTNDISP(,@14)                                   0106 00101000
         ALR   @11,@14                                             0106 00102000
         SLR   @14,@14                                             0106 00103000
         IC    @14,$RTN#(,@11)                                     0106 00104000
         CVD   @14,@TS00001                                        0106 00105000
         MVC   DPRWORK2(2,WAPTR),@TS00001+6                        0106 00106000
*   DPRWORK2=DPRWORK2/16;           /* GET RID OF SIGN BITS CONVERT     00107000
*                                      RTN# TO EBCDIC                */ 00108000
*                                                                  0107 00109000
         LH    @14,DPRWORK2(,WAPTR)                                0107 00110000
         N     @14,@CF01073                                        0107 00111000
         SRL   @14,4                                               0107 00112000
         STH   @14,DPRWORK2(,WAPTR)                                0107 00113000
*   /*****************************************************************/ 00114000
*   /*                                                               */ 00115000
*   /* ITACONV FROM(DPRWORK2)TO($ESECTID(8))COUNT(2)TYPE(HE) INVOKED */ 00116000
*   /*                                                               */ 00117000
*   /*****************************************************************/ 00118000
*                                                                  0108 00119000
*   DCL                                                            0108 00120000
*     1 ITATCPRM BDY(WORD),         /* CONVERT PARAMETER LIST        */ 00121000
*      2 * CHAR(4) INIT('0102F4F1'X),                              0108 00122000
*      2 ITATCFR PTR,               /* ADDRESS OF FROM FIELD         */ 00123000
*      2 ITATCTO PTR,               /* ADDRESS OF TO FIELD           */ 00124000
*      2 * BIT(8) INIT(0),          /* FLAGS FOR CONVERT MODULE      */ 00125000
*       3 ITATCTP BIT(1),           /* TYPE OF CONVERT REQUIRED      */ 00126000
*      2 ITATCNT FIXED(8);          /* NUMBER OF BYTES TO CONVERT    */ 00127000
*   ITATCFR=ADDR(DPRWORK2);         /* SET THE FROM ADDRESS          */ 00128000
         LA    @14,DPRWORK2(,WAPTR)                                0109 00129000
         ST    @14,ITATCFR                                         0109 00130000
*   ITATCTO=ADDR($ESECTID(8));      /* SET THE TO ADDRESS            */ 00131000
         LA    @14,$ESECTID+7(,WAPTR)                              0110 00132000
         ST    @14,ITATCTO                                         0110 00133000
*   ITATCTP='1'B;                   /* SET THE TYPE OF CONVERT BIT   */ 00134000
         OI    ITATCTP,B'10000000'                                 0111 00135000
*   ITATCNT=2;                      /* GIVE THE CONVERT BYTE COUNT   */ 00136000
         MVI   ITATCNT,X'02'                                       0112 00137000
*   R1=ADDR(ITATCPRM);              /* POINT TO CONVERT PARM LIST    */ 00138000
         LA    R1,ITATCPRM                                         0113 00139000
*   CALL ISTSC021;                  /* CALL CONVERT DIRECTLY         */ 00140000
         L     @15,@CV00875                                        0114 00141000
         BALR  @14,@15                                             0114 00142000
*   $ESECTID=SECTID;                /* SAVE FOR FE & CATERR MSGS     */ 00143000
         L     @14,$LOWADDR(,WAPTR)                                0115 00144000
         MVC   $ESECTID(8,WAPTR),SECTID(@14)                       0115 00145000
*/********************************************************************/ 00146000
*/* SEARCH FOR DEVICE ADDRESS                                        */ 00147000
*/********************************************************************/ 00148000
*                                                                  0116 00149000
*   $24CATMG=' ';                   /* BLANK OUT DEVADR & SYMNME     */ 00150000
