*           $24UNADR=$PDEVADR;      /* ELSE USE PRIMARY DEVICE       */ 00187000
         L     @14,$PDEVADR(,WAPTR)                                0130 00188000
         ST    @14,$24UNADR(,WAPTR)                                0130 00189000
*           IF $SYMNAME=ON          /* IF THERE IS A SYMB            */ 00190000
*             THEN                                                 0131 00191000
         TM    $SYMNAME(WAPTR),B'00001000'                         0131 00192000
         BNO   @RF00131                                            0131 00193000
*             $24SYMNE=$SYNAME;     /* NAME, USE IT                  */ 00194000
         MVC   $24SYMNE(8,WAPTR),$SYNAME(WAPTR)                    0132 00195000
*         END;                                                     0133 00196000
@RF00131 DS    0H                                                  0134 00197000
*     END;                                                         0134 00198000
*   ELSE                                                           0135 00199000
*     DO;                           /* OTHERWISE                     */ 00200000
         B     @RC00122                                            0135 00201000
@RF00122 DS    0H                                                  0136 00202000
*       $24UNADR(2:4)=              /* USE ADDR FROM FIRST           */ 00203000
