         LTR   @14,@14                                             0122 00168000
         BZ    @RT00122                                            0122 00169000
         CLI   TECBEVOC(TECBPTR),0                                 0122 00170000
         BNE   @RF00122                                            0122 00171000
@RT00122 DS    0H                                                  0123 00172000
*     DO;                                                          0123 00173000
*       IF $NDR=ON                  /* IF THERE IS NO PRIM DEV       */ 00174000
*         THEN                                                     0124 00175000
         TM    $NDR(WAPTR),B'00100000'                             0124 00176000
         BNO   @RF00124                                            0124 00177000
*         DO;                                                      0125 00178000
*           $24DEVAD='NONE';        /* DEV ADDR IS 'NONE'            */ 00179000
         MVC   $24DEVAD(4,WAPTR),@CC00878                          0126 00180000
*           GOTO S24C0150;          /* BYPASS CONVERT OF UNIT ADR    */ 00181000
         B     S24C0150                                            0127 00182000
*         END;                                                     0128 00183000
*       ELSE                                                       0129 00184000
*         DO;                                                      0129 00185000
