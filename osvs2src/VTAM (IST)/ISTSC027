         DC    AL1(16)                                             0001 00008000
         DC    C'ISTSC027  76.259'                                 0001 00009000
S27C1000 DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00643-S27C1000)                                   00013000
         ENTRY S27C1000                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  ST    @14,12(,@13)                                        0001 00016000
