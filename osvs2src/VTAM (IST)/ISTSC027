         BR    @15                                                 0001 00022000
@EP00001 DS    0H                                                  0002 00023000
*   R14=R13;                        /* HOLD CALLER'S SVAREA PTR      */ 00024000
         LR    R14,R13                                             0405 00025000
*   R13=ADDR(SVESC027);             /* PT TO MY SAVEAREA             */ 00026000
