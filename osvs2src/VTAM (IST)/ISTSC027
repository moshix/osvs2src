*     GOTO S27C0350;                /* RC=08 AND RET TO OLT          */ 00045000
*   IF $XIOCDSA=0                   /* IF CDSADDR NOT GIVEN          */ 00046000
*     THEN                          /* THEN                          */ 00047000
