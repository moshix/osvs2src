*   IFTSYMOV=CDSSYMBN;              /* PUT THE SYMBOLIC NAME IN THE     00361000
*                                      LIST                          */ 00362000
         MVC   IFTSYMOV(8,@09),CDSSYMBN(CDSPTR)                    0236 00363000
*   IFTPABPR=ADDR(S29CIFTX);        /* POINT TO THE PARAMETER LIST 0237 00364000
*                                      EXTENSION                     */ 00365000
         LA    @11,S29CIFTX                                        0237 00366000
         ST    @11,IFTPABPR(,@09)                                  0237 00367000
*   CVTPTR=$CVTSAVE;                /* POINT TO CVT BEFORE TPQUE   0238 00368000
*                                      DESTROYS R2                   */ 00369000
         L     @09,RCAPTR(,WAPTR)                                  0238 00370000
         L     CVTPTR,$CVTSAVE(,@09)                               0238 00371000
*   RESPECIFY                                                      0239 00372000
*     ISTATCVT BASED(CVTPTR);       /* POINT TO THE CVT FOR VTAM     */ 00373000
*   RESPECIFY                                                      0240 00374000
*     ISTDYPAB BASED(ADDR(ATCVTIPB));/* DONT USE DYNAMIC PAB BUT USE    00375000
*                                      THE PAB WITHIN IT             */ 00376000
*   R0=ADDR(ISTTIE);                /* POINT TO ELEMENT TO BE QUEUED */ 00377000
         LR    R0,@14                                              0241 00378000
*   R1=ADDR(DYPPAB);                /* POINT TO PAB TO BE QUEUED TO  */ 00379000
         LA    R1,DYPPAB+656(,CVTPTR)                              0242 00380000
*   CALL I00C1000;                  /* CALL TPQUE                    */ 00381000
