         BALR  @14,@15                                             0243 00383000
*   R1=ADDR(TIEPTR2);               /* POINT TO ECB                  */ 00384000
         LA    R1,S29CTIE                                          0244 00385000
         LA    R1,TIEPTR2(,R1)                                     0244 00386000
*   CALL I00C1002;                  /* CALL WAIT                     */ 00387000
         L     @15,@CV02095                                        0245 00388000
         BALR  @14,@15                                             0245 00389000
*   IF IFTSTDAF='1'B THEN           /* CHECK FOR DAF RETURNED      0246 00390000
*                                                            @D32JPGH*/ 00391000
         LA    @14,S29CIFTP                                        0246 00392000
         TM    IFTSTDAF(@14),B'00100000'                           0246 00393000
         BO    @RT00246                                            0246 00394000
*     GO TO S29C0050;               /* GO TO LOADER CALL     @D32JPGH*/ 00395000
*   DAF=S29CDAF;                    /* MOVE THE DAF BACK IN  @D32JPGH*/ 00396000
         LH    DAF,S29CDAF                                         0248 00397000
         N     DAF,@CF02103                                        0248 00398000
*   IF IFTFLNNM='1'B THEN           /* CHECK FOR LINE NAME RETURNED     00399000
*                                                            @D32JPGH*/ 00400000
