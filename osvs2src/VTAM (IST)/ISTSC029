         TITLE 'ISTSC029,GETCONFG FUNCTION. RETURN CDS INFO            *00001000
                        '                                               00002000
* IBM DISK OPERATING SYSTEM                                             00003000
* TOLTEP - 5745-SC-TLT                                                  00004000
  ISEQ 73,80                                                            00005000
ISTSC029 CSECT ,                                                   0002 00006000
@MAINENT DS    0H                                                  0002 00007000
         USING *,@15                                               0002 00008000
         B     @PROLOG                                             0002 00009000
         DC    AL1(16)                                             0002 00010000
         DC    C'ISTSC029  75.225'                                 0002 00011000
         DROP  @15                                                      00012000
@PROLOG  ST    @14,12(,@13)                                        0002 00013000
         STM   @01,@12,24(@13)                                     0002 00014000
         BALR  @12,0                                               0002 00015000
@PSTART  DS    0H                                                  0002 00016000
         USING @PSTART,@12                                         0002 00017000
*                                                                  0150 00018000
*   /*****************************************************************/ 00019000
*   /*                                                               */ 00020000
*   /* BEGINNING OF EXECUTABLE CODE FOR MODULE                       */ 00021000
*   /*                                                               */ 00022000
*   /*****************************************************************/ 00023000
*                                                                  0151 00024000
*   SVESC029(2)=R13;                /* SAVE BACK LINK TO SAVEAREA    */ 00025000
         ST    R13,SVESC029+4(,WAPTR)                              0151 00026000
*   BACKSAVE(3)=ADDR(SVESC029);     /* SETUP CHAIN TO NEXT    SAVEARA*/ 00027000
         LA    @14,SVESC029(,WAPTR)                                0152 00028000
         ST    @14,BACKSAVE+8(,R13)                                0152 00029000
*   R13=ADDR(SVESC029);             /* POINT TO INTERNAL SAVEAREA    */ 00030000
*                                                                  0153 00031000
         LR    R13,@14                                             0153 00032000
*   /*****************************************************************/ 00033000
*   /*                                                               */ 00034000
*   /* SAVE PARAMETER FOR LOADER CALL                                */ 00035000
*   /*                                                               */ 00036000
*   /*****************************************************************/ 00037000
*                                                                  0154 00038000
*   GETMAP=R1;                      /* SAVE POINTER TO CALLERS SAVE     00039000
*                                      AREA THROUGHOUT PRGM          */ 00040000
         LR    GETMAP,R1                                           0154 00041000
*   LDRCDSSZ=$GETBUFF;              /* STORE COUNT IN LIST           */ 00042000
         SLR   @14,@14                                             0155 00043000
         IC    @14,$GETBUFF(,GETMAP)                               0155 00044000
         STH   @14,LDRCDSSZ                                        0155 00045000
*   LDRADR=$GETCDSA;                /* INPUT BUFFER ADDRESS          */ 00046000
         L     @14,$GETCDSA(,GETMAP)                               0156 00047000
         ST    @14,LDRADR                                          0156 00048000
*   CDSPTR=$GETBADD;                /* CDS POINTER                   */ 00049000
         L     CDSPTR,$GETBADD(,GETMAP)                            0157 00050000
*   DAF=-1;                         /* NO DAF FOUND          @D32JPGH*/ 00051000
*                                                                  0158 00052000
         SLR   DAF,DAF                                             0158 00053000
         BCTR  DAF,0                                               0158 00054000
*   /*****************************************************************/ 00055000
*   /*                                                               */ 00056000
*   /* CHECK FOR INVALID CALL AND THEN REJECT IT                     */ 00057000
*   /*                                                               */ 00058000
*   /*****************************************************************/ 00059000
*                                                                  0159 00060000
*   RC=12;                          /* PRESET THE RETURNCODE INCASE     00061000
*                                      OF BRANCH                     */ 00062000
*                                                                  0159 00063000
         LA    RC,12                                               0159 00064000
*   /*****************************************************************/ 00065000
*   /*                                                               */ 00066000
*   /* VALID CALL, CONTINUE PROCESSING THE REQUEST. GET THE PROPER   */ 00067000
*   /* BASIC CDS POINTER WHICH IS EITHER PASSED IN THE PARAMETER LIST*/ 00068000
*   /* OR IS THE ADDRESS OF THE CDS DATA IN THE SCT.                 */ 00069000
*   /*                                                               */ 00070000
*   /*****************************************************************/ 00071000
*                                                                  0160 00072000
*   IF CDSPTR=0 THEN                /* IF THE CDS POINTER IN       0160 00073000
*                                      PARAMETER LIST IS ZERO, THIS     00074000
*                                      IS A SPECIAL REQUEST          */ 00075000
         LTR   CDSPTR,CDSPTR                                       0160 00076000
         BNZ   @RF00160                                            0160 00077000
*     IF $NDR='1'B THEN             /* CHECK TO SEE IF THERE IS ANY     00078000
*                                      CDS DATA IN THE SCT TO DO A 0161 00079000
*                                      GETCONFG ON                   */ 00080000
         TM    $NDR(WAPTR),B'00100000'                             0161 00081000
         BO    @RT00161                                            0161 00082000
*       GO TO S29C0070;             /* IF NO DATA, INDICATE NOT FOUND*/ 00083000
*     ELSE                                                         0163 00084000
*       CDSPTR=ADDR($PDEVADR);      /* PRETEND OLT POINTED TO SCT  0163 00085000
*                                      ADDR                          */ 00086000
         LA    CDSPTR,$PDEVADR(,WAPTR)                             0163 00087000
*   RESPECIFY                                                      0164 00088000
*     CDSMAP BASED(CDSPTR);         /* BASE CDS ON ADDRESS PASSED    */ 00089000
*                                                                  0164 00090000
@RF00160 DS    0H                                                  0165 00091000
*   /*****************************************************************/ 00092000
*   /*                                                               */ 00093000
*   /* ASSUME REMOTE SYMBOLIC NAMED DEVICE. IF THE ASSUMPTION IS     */ 00094000
*   /* INVALID, THE BITS WILL BE REVERSED AND THE UNIT ADDRESS FIELD */ 00095000
*   /* WILL BE USED.                                                 */ 00096000
*   /*                                                               */ 00097000
*   /*****************************************************************/ 00098000
*                                                                  0165 00099000
*   LDRNAME=CDSSYMBN;               /* PUT IN SYMBOLIC NAME          */ 00100000
         MVC   LDRNAME(8),CDSSYMBN(CDSPTR)                         0165 00101000
*   LDRREM='1'B;                    /* REMOTE FILE                   */ 00102000
         OI    LDRREM,B'00001000'                                  0166 00103000
*   IF CDSREMSY='1'B THEN           /* IF GOOD GUESS,LOAD REM CDS    */ 00104000
         TM    CDSREMSY(CDSPTR),B'00000010'                        0167 00105000
         BO    @RT00167                                            0167 00106000
*     GO TO S29C0100;               /* TPQUE                 @D32JPGH*/ 00107000
*   LDRREM='0'B;                    /* SEARCH LOCAL FILE             */ 00108000
         NI    LDRREM,B'11110111'                                  0169 00109000
*   IF CDSSYMB='1'B THEN            /* IF GOOD GUESS, LOAD LOC CDS   */ 00110000
         TM    CDSSYMB(CDSPTR),B'00001000'                         0170 00111000
         BO    @RT00170                                            0170 00112000
*     GO TO S29C0050;               /* GO TO LOADER CALL             */ 00113000
*                                                                  0171 00114000
*   /*****************************************************************/ 00115000
*   /*                                                               */ 00116000
*   /* ATTEMPT TO CONVERT THE UNIT ADDRESS FIELD TO HEX. IF IT WILL  */ 00117000
*   /* NOT CONVERT IT IS A SYMBOLIC NAME. ITACONV                    */ 00118000
*   /* FROM(CDSUA)TO(LDRNAME) INVOKED                                */ 00119000
*   /*                                                               */ 00120000
*   /*****************************************************************/ 00121000
*                                                                  0172 00122000
*   DCL                                                            0172 00123000
*    (ITATCF,                                                      0172 00124000
*     ITATCT) CHAR(*) BASED;        /* GENERATE WORK VARABLES        */ 00125000
*   DCL                                                            0173 00126000
*     ITATCTS FIXED;                /* TEMP SPOT FOR ALTERED BYTE    */ 00127000
*   RFY                                                            0174 00128000
*     ITATCF BASED(ADDR(CDSUA));    /* MAP VARIABLE OVER FROM        */ 00129000
*   RFY                                                            0175 00130000
*     ITATCT BASED(ADDR(LDRNAME));  /* MAP VARIBABLE OVER TO         */ 00131000
*   ITATCTS=ITATCT(LENGTH(LDRNAME)+1:LENGTH(LDRNAME)+1);/* SAVE BYTE    00132000
*                                      PAST TO FIELD                 */ 00133000
         LA    @14,LDRNAME                                         0176 00134000
         SLR   ITATCTS,ITATCTS                                     0176 00135000
         IC    ITATCTS,ITATCT+8(,@14)                              0176 00136000
*   UNPK(ITATCT(1:LENGTH(LDRNAME)+1),ITATCF(1:LENGTH(CDSUA)+1));/* 0177 00137000
*                                      EXPAND HEX TO EBCDIC          */ 00138000
         UNPK  ITATCT(9,@14),ITATCF(5,CDSPTR)                      0177 00139000
*   TR(ITATCT(1:LENGTH(LDRNAME)),$TRTBL);/* CONVERT FA-FF TO C1-C6   */ 00140000
         L     @09,RCAPTR(,WAPTR)                                  0178 00141000
         L     @09,$TRTBLA(,@09)                                   0178 00142000
         TR    ITATCT(8,@14),$TRTBL(@09)                           0178 00143000
*   ITATCT(LENGTH(LDRNAME)+1:LENGTH(LDRNAME)+1)=ITATCTS;/* RESTORE 0179 00144000
*                                      BYTE PAST TO FIELD            */ 00145000
         STC   ITATCTS,ITATCT+8(,@14)                              0179 00146000
*   LDRNAME(1:1)='G';               /* FIRST CHARACTER ALWAYS "G"    */ 00147000
         MVI   LDRNAME,C'G'                                        0180 00148000
*S29C0050:                                                         0181 00149000
*   R1=ADDR(LDRPARM);               /* POINT TO PARAMETER LIST       */ 00150000
S29C0050 LA    R1,LDRPARM                                          0181 00151000
*   CALL ISTCC013;                  /* LOAD CDS INTO CALLER          */ 00152000
*                                                                  0182 00153000
         L     @15,@CV02075                                        0182 00154000
         BALR  @14,@15                                             0182 00155000
*   /*****************************************************************/ 00156000
*   /*                                                               */ 00157000
*   /* THE RETURN CODE PASSED BACK TO THE CALLER IS 0 IF ALL THE DATA*/ 00158000
*   /* IS PASSED BACK, 8 IF ONLY SOME OF THE DATA WAS AND 12 IF THERE*/ 00159000
*   /* WAS ANY TYPE OF I/O ERROR AND THE LOADER IS CAPABLE OF        */ 00160000
*   /* RECOVERING FROM IT.                                           */ 00161000
*   /*                                                               */ 00162000
*   /*****************************************************************/ 00163000
*                                                                  0183 00164000
*S29C0060:                                                         0183 00165000
*   CDSPTR=$GETCDSA;                /* POINT TO REC BUF      @ZD10877*/ 00166000
S29C0060 L     CDSPTR,$GETCDSA(,GETMAP)                            0183 00167000
*   RC=0;                           /* SET RETURN CODE               */ 00168000
         SLR   RC,RC                                               0184 00169000
*   IF R0>$GETBUFF THEN             /* IF CDS IS TO BIG, SET THE   0185 00170000
*                                      RETURN CODE                   */ 00171000
         SLR   @14,@14                                             0185 00172000
         IC    @14,$GETBUFF(,GETMAP)                               0185 00173000
         CR    R0,@14                                              0185 00174000
         BNH   @RF00185                                            0185 00175000
*     RC=8;                         /* CDS LARGER THAN BUFFER        */ 00176000
         LA    RC,8                                                0186 00177000
*   IF R15^=0 THEN                  /* IF NOT FOUND,OR ERROR         */ 00178000
@RF00185 LTR   R15,R15                                             0187 00179000
         BZ    @RF00187                                            0187 00180000
*     DO;                                                          0188 00181000
*S29C0070:                                                         0189 00182000
*       RC=12;                      /* SET RETURN CODE TO INDICATE   */ 00183000
S29C0070 LA    RC,12                                               0189 00184000
*     END;                                                         0190 00185000
*   ELSE                            /* IF CDS WAS BROUGHT IN O.K.  0191 00186000
*                                      MODIFY DAF AND SYMBOLIC NAMES */ 00187000
*     IF $GETBUFF>=26&CDSBYTCT>=26 THEN/* MODIFY ONLY IF BYTES HAVE     00188000
*                                      BEEN BROUGHT INTO STORAGE ON     00189000
*                                      THIS REQUEST                  */ 00190000
         B     @RC00187                                            0191 00191000
@RF00187 CLI   $GETBUFF(GETMAP),26                                 0191 00192000
         BL    @RF00191                                            0191 00193000
         CLI   CDSBYTCT(CDSPTR),26                                 0191 00194000
         BL    @RF00191                                            0191 00195000
*       IF CDSSYMB='1'B&CDSCLASS='44'X THEN                        0192 00196000
         TM    CDSSYMB(CDSPTR),B'00001000'                         0192 00197000
         BNO   @RF00192                                            0192 00198000
         CLI   CDSCLASS(CDSPTR),68                                 0192 00199000
         BNE   @RF00192                                            0192 00200000
*         DO;                       /* ALTER DAF ON REMOTE DEV       */ 00201000
*           IF DAF^=-1 THEN         /* CHECK FOR A GOOD COMPLETION 0194 00202000
*                                                            @D32JPGH*/ 00203000
         C     DAF,@CF02057                                        0194 00204000
         BE    @RF00194                                            0194 00205000
*             CDSDAF=DAF;           /* MOVE THE DAF BACK INTO THE CDS   00206000
*                                                            @D32JPGH*/ 00207000
         STH   DAF,CDSDAF(,CDSPTR)                                 0195 00208000
*         END;                      /* END OF DAF UPDATE IN CDS      */ 00209000
*       ELSE                                                       0197 00210000
*         IF $GETBUFF>=28&CDSBYTCT>=28 THEN/* CHECK FOR THE RIGHT  0197 00211000
*                                      SYMBOLIC NAME IN AN RN CDS    */ 00212000
         B     @RC00192                                            0197 00213000
@RF00192 CLI   $GETBUFF(GETMAP),28                                 0197 00214000
         BL    @RF00197                                            0197 00215000
         CLI   CDSBYTCT(CDSPTR),28                                 0197 00216000
         BL    @RF00197                                            0197 00217000
*           IF CDSCLASS='40'X&(CDSTYPE='06'X|CDSTYPE='09'X) THEN   0198 00218000
         CLI   CDSCLASS(CDSPTR),64                                 0198 00219000
         BNE   @RF00198                                            0198 00220000
         CLI   CDSTYPE(CDSPTR),6                                   0198 00221000
         BE    @RT00198                                            0198 00222000
         CLI   CDSTYPE(CDSPTR),9                                   0198 00223000
         BNE   @RF00198                                            0198 00224000
@RT00198 DS    0H                                                  0199 00225000
*             DO;                                                  0199 00226000
*               DO I=3 TO $DEVCNTS; /* LOOK THROUGHTHE DEVICE TABLE     00227000
*                                      FOR DEVICE WITH THE SAME UNIT    00228000
*                                      ADDRESS AS THE 370X           */ 00229000
         LA    I,3                                                 0200 00230000
         B     @DE00200                                            0200 00231000
@DL00200 DS    0H                                                  0201 00232000
*                 IF $DEVUNAD(I)=CDSUA THEN                        0201 00233000
         LR    @14,I                                               0201 00234000
         MH    @14,@CH00286                                        0201 00235000
         L     @14,$DEVUNAD-68(@14,WAPTR)                          0201 00236000
         CL    @14,CDSUA(,CDSPTR)                                  0201 00237000
         BE    @RT00201                                            0201 00238000
*                   GO TO S29C0075; /* LEAVE WHEN FOUND              */ 00239000
*               END;                /* END OF LOOP TO LOOK FOR UA  0203 00240000
*                                      MATCH                         */ 00241000
         AL    I,@CF00073                                          0203 00242000
@DE00200 CH    I,$DEVCNTS(,WAPTR)                                  0203 00243000
         BNH   @DL00200                                            0203 00244000
*               GO TO S29C0080;     /* NO MATCH FOUND, EXIT ROUTINE  */ 00245000
         B     S29C0080                                            0204 00246000
*S29C0075:                          /* GET 370X NAME FOR $SYSMN(I) 0205 00247000
*                                      WITH INQUIRE MACRO            */ 00248000
*               RESPECIFY                                          0205 00249000
*                (R5,                                              0205 00250000
*                 R6) RESTRICTED;   /* REGS FOR THE INQUIRE MACRO    */ 00251000
S29C0075 DS    0H                                                  0206 00252000
*               R5=ADDR($TLTACB);   /* ADDRESS OF THE TOLTEP ACB     */ 00253000
         L     R5,RCAPTR(,WAPTR)                                   0206 00254000
*               R6=ADDR(CDSRNAME);  /* ADDRESS OF THE RECEIVING    0207 00255000
*                                      BUFFER                        */ 00256000
         LA    R6,CDSRNAME(,CDSPTR)                                0207 00257000
*               NIBSYM=$SYSMN(I);   /* FILL IN THE APPROIATE SYMBOLIC   00258000
*                                      NAME                          */ 00259000
         LA    @14,S29C1020                                        0208 00260000
         LR    @09,I                                               0208 00261000
         MH    @09,@CH00286                                        0208 00262000
         ALR   @09,WAPTR                                           0208 00263000
         MVC   NIBSYM(8,@14),$SYSMN-68(@09)                        0208 00264000
*               GEN REFS(R5,R6,S29C1000)                           0209 00265000
*                   (INQUIRE RPL=S29C1000,ACB=(R5),AREA=(R6));     0209 00266000
         INQUIRE RPL=S29C1000,ACB=(R5),AREA=(R6)                        00267000
*               RESPECIFY                                          0210 00268000
*                (R5,                                              0210 00269000
*                 R6) UNRESTRICTED; /* DONE WITH REGISTERS           */ 00270000
*               IF R15=0 THEN       /* IF NCPNAME WAS FOUND, STORE C    00271000
*                                      AT END OF NAME                */ 00272000
         LTR   R15,R15                                             0211 00273000
         BNZ   @RF00211                                            0211 00274000
*                 DO;               /* GOOD RETURNCODE, FIND BLANK   */ 00275000
*                   DO I=2 TO 8;    /* FIND THE FIRST BLANK AT END OF   00276000
*                                      NCP NAME                      */ 00277000
         LA    I,2                                                 0213 00278000
@DL00213 DS    0H                                                  0214 00279000
*                     IF CDSRNAME(I)=' ' THEN/* IF THIS CHARACTER IS    00280000
*                                      A BLNAK, ALTER IT             */ 00281000
         LR    @14,CDSPTR                                          0214 00282000
         ALR   @14,I                                               0214 00283000
         CLI   CDSRNAME-1(@14),C' '                                0214 00284000
         BNE   @RF00214                                            0214 00285000
*                       DO;         /* YES, WE FOUND THE FIRST BLANK,   00286000
*                                      ALTER IT                      */ 00287000
*                         CDSRNAME(I)='C';/* REMOTE CDSNAME HAS A C     00288000
*                                      SUFFIX                        */ 00289000
         LR    @14,CDSPTR                                          0216 00290000
         ALR   @14,I                                               0216 00291000
         MVI   CDSRNAME-1(@14),C'C'                                0216 00292000
*                         GO TO S29C0080;/* EXIT SEARCH ROUTINE      */ 00293000
         B     S29C0080                                            0217 00294000
*                       END;        /* FIRST BLANK ALTERED EXIT      */ 00295000
*                   END;            /* TRY NEXT CHARACTER            */ 00296000
@RF00214 AL    I,@CF00073                                          0219 00297000
         C     I,@CF00056                                          0219 00298000
         BNH   @DL00213                                            0219 00299000
*                 END;              /* BAD RETURNCODE IF DO NOT    0220 00300000
*                                      EXECUTED                      */ 00301000
*S29C0080:                                                         0221 00302000
*             END;                  /* END OF LOCAL UPDATE OF 370X 0221 00303000
*                                      NCP NAME                      */ 00304000
@RF00211 DS    0H                                                  0221 00305000
S29C0080 DS    0H                                                  0222 00306000
*S29C0090:                                                         0222 00307000
*   R15=RC;                         /* SET RETURN CODE               */ 00308000
@RF00198 DS    0H                                                  0222 00309000
@RF00197 DS    0H                                                  0222 00310000
@RC00192 DS    0H                                                  0222 00311000
@RF00191 DS    0H                                                  0222 00312000
@RC00187 DS    0H                                                  0222 00313000
S29C0090 LR    R15,RC                                              0222 00314000
*   R0=DAF;                         /* RETURN DAF TO CALLER  @D32JPGH*/ 00315000
         LR    R0,DAF                                              0223 00316000
*   R13=SVESC029(2);                /* RESTORE SAVEAREA POINTER      */ 00317000
         L     R13,SVESC029+4(,WAPTR)                              0224 00318000
*   RETURN;                                                        0225 00319000
*                                                                  0225 00320000
@EL00001 DS    0H                                                  0225 00321000
@EF00001 DS    0H                                                  0225 00322000
@ER00001 L     @14,12(,@13)                                        0225 00323000
         LM    @01,@12,24(@13)                                     0225 00324000
         BR    @14                                                 0225 00325000
*   /*****************************************************************/ 00326000
*   /*                                                               */ 00327000
*   /* GET THE PROPER DAF FROM THE IFT ROUTINE FOR THE SYMB. NAME    */ 00328000
*   /* PASSED IN THE CDS                                             */ 00329000
*   /*                                                               */ 00330000
*   /*****************************************************************/ 00331000
*                                                                  0226 00332000
*S29C0100:                                                         0226 00333000
*   ISTTIE=ISTTIE&&ISTTIE;          /* ZERO OUT THE TIE              */ 00334000
S29C0100 LA    @14,S29CTIE                                         0226 00335000
         XC    ISTTIE(16,@14),ISTTIE(@14)                          0226 00336000
*   TIETYPE=TITYPE;                 /* SET THE TYPE OF CONTROL BLOCK    00337000
*                                      FIELD                         */ 00338000
         MVI   TIETYPE(@14),X'18'                                  0227 00339000
*   TIELNGTH=LENGTH(ISTTIE);        /* STORE THE LENGTH OF THE     0228 00340000
*                                      CONTROL BLOCK                 */ 00341000
         MVI   TIELNGTH(@14),X'10'                                 0228 00342000
*   TIEREQ=TIEREQ5;                 /* STORE THE TYPE OF USE OF THIS    00343000
*                                      TIE                           */ 00344000
         MVI   TIEREQ(@14),X'05'                                   0229 00345000
*   TIEPTR1=ADDR(ISTIFTPL);         /* POINT TO THE PARAMETER LIST   */ 00346000
         LA    @09,S29CIFTP                                        0230 00347000
         ST    @09,TIEPTR1(,@14)                                   0230 00348000
*   ISTIFTPL=ISTIFTPL&&ISTIFTPL;    /* ZERO THE PARAMETER LIST       */ 00349000
         XC    ISTIFTPL(44,@09),ISTIFTPL(@09)                      0231 00350000
*   IFTSTDAF='1'B;                  /* ASK FOR THE DAF TO BE RETURNED*/ 00351000
*   IFTFLNNM='1'B;                  /* ASK FOR LINE NAME             */ 00352000
         OI    IFTSTDAF(@09),B'00110000'                           0233 00353000
*   IFTTPPST='1'B;                  /* ASK THAT AN ECB BE POSTED ON     00354000
*                                      CONPLETION                    */ 00355000
         OI    IFTTPPST(@09),B'10000000'                           0234 00356000
*   IFTECBPR=ADDR(TIEPTR2);         /* USE SECOND POINTER IN TIE FOR    00357000
*                                      AN ECB                        */ 00358000
         LA    @11,TIEPTR2(,@14)                                   0235 00359000
