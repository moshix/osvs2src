         TITLE 'ISTSC02C-MORECORE                                      *00001000
                        '                                               00002000
ISTSC02C CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSC02C  75.177'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @02,@12,28(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
         ST    @13,@SA00001+4                                      0001 00015000
         LA    @14,@SA00001                                        0001 00016000
         ST    @14,8(,@13)                                         0001 00017000
         LR    @13,@14                                             0001 00018000
         EJECT                                                          00019000
*                                                                  0052 00020000
*/********************************************************************/ 00021000
*/*      FIRST TIME INITIALIZATION                                   */ 00022000
*/********************************************************************/ 00023000
*                                                                  0053 00024000
*   R15=0;                          /* SET RTN CODE REG TO 0         */ 00025000
         SLR   R15,R15                                             0053 00026000
*   MINPTR=0;                       /* SET CONSTANTS TO ZERO         */ 00027000
         SLR   @14,@14                                             0054 00028000
         ST    @14,MINPTR                                          0054 00029000
*   MAXPTR=0;                                                      0055 00030000
         ST    @14,MAXPTR                                          0055 00031000
*   IF $FSTMCIS='1'B                /* HAS A MORE CORE BEEN          */ 00032000
*     THEN                          /* ISSUIED                       */ 00033000
         TM    $FSTMCIS(WAPTR),B'00000100'                         0056 00034000
         BO    @RT00056                                            0056 00035000
*     GOTO S2CC0001;                /* BR-YES SWITCH IS ON ELSE NO   */ 00036000
*   R7=$OLTPTR;                     /* PICK UP PTR TO OLT            */ 00037000
         L     R7,$OLTPTR(,WAPTR)                                  0058 00038000
*   R6=((R7+$RTSIZE+15)&'FFFFFFF8'X);/* COMPUTE PTR FREE + 8         */ 00039000
         L     @09,$LOWADDR(,WAPTR)                                0059 00040000
         LH    @11,RTNDISP(,@09)                                   0059 00041000
         ALR   @11,@09                                             0059 00042000
         LR    R6,R7                                               0059 00043000
         AL    R6,$RTSIZE(,@11)                                    0059 00044000
         AL    R6,@CF00034                                         0059 00045000
         N     R6,@CF00704                                         0059 00046000
*   IF R6>$HIMOREC                  /* ANY CORE TO USE               */ 00047000
*     THEN                          /* BR-NO TO RTN CODE 8 ROUTIN    */ 00048000
         L     @09,$HIMOREC(,WAPTR)                                0060 00049000
         CR    R6,@09                                              0060 00050000
         BH    @RT00060                                            0060 00051000
*     GOTO S2CC0090;                /* ELSE YES                      */ 00052000
*   $FSTMCIS='1'B;                  /* SET FLAG TO IND 1ST TIM DN    */ 00053000
         OI    $FSTMCIS(WAPTR),B'00000100'                         0062 00054000
*   R6=R6-8;                        /* SET FREE PTR IN TWA           */ 00055000
         SL    R6,@CF00074                                         0063 00056000
*   $FRELPTR=R6;                                                   0064 00057000
         ST    R6,$FRELPTR(,WAPTR)                                 0064 00058000
*   COREFWD=0;                      /* SET FWD PTR TO ZERO           */ 00059000
         ST    @14,COREFWD(,R6)                                    0065 00060000
*   CORESZE=($HIMOREC-R6);          /* SET SIZE OF FREE AREA         */ 00061000
         SLR   @09,R6                                              0066 00062000
         ST    @09,CORESZE(,R6)                                    0066 00063000
*   FREE='1'B;                      /* SET INDICATOR AREA FREE       */ 00064000
         OI    FREE(R6),B'00000001'                                0067 00065000
*/********************************************************************/ 00066000
*                                                                  0068 00067000
*   GEN(EJECT);                                                    0068 00068000
         EJECT                                                          00069000
*                                                                  0069 00070000
*/********************************************************************/ 00071000
*/*      COMMON INITIALIZE                                           */ 00072000
*/********************************************************************/ 00073000
*                                                                  0069 00074000
*S2CC0001:                                                         0069 00075000
*   ;                                                              0069 00076000
S2CC0001 DS    0H                                                  0070 00077000
*   R5=$MORCMAX+8;                  /* ADD 8 TO MIN & MAX SIZES      */ 00078000
         LA    @14,8                                               0070 00079000
         L     R5,$MORCMAX(,MORMAP)                                0070 00080000
         ALR   R5,@14                                              0070 00081000
*   R4=$MORCMIN+8;                  /*                               */ 00082000
         AL    @14,$MORCMIN(,MORMAP)                               0071 00083000
         LR    R4,@14                                              0071 00084000
*   R7=0;                           /* SET WORK REGS TO ZERO         */ 00085000
         SLR   R7,R7                                               0072 00086000
*   R8=0;                           /*                               */ 00087000
         SLR   R8,R8                                               0073 00088000
*   R6=$FRELPTR;                    /* SET R6 FOR MANAG AREA         */ 00089000
         L     R6,$FRELPTR(,WAPTR)                                 0074 00090000
*/********************************************************************/ 00091000
*/*      SEARCH FOR POTENTIAL MAX & MIN/BTWN AREAS                   */ 00092000
*/********************************************************************/ 00093000
*                                                                  0075 00094000
*S2CC0002:                                                         0075 00095000
*   ;                                                              0075 00096000
S2CC0002 DS    0H                                                  0076 00097000
*   IF FREE='0'B                    /* TEST FOR FREE AREA            */ 00098000
*     THEN                          /* IF BIT ON AREA FREE           */ 00099000
         TM    FREE(R6),B'00000001'                                0076 00100000
         BZ    @RT00076                                            0076 00101000
*     GOTO S2CC0008;                /* BIT OFF AREA IN USE           */ 00102000
*   R3=(CORESZE&'FFFFFFFE'X);       /* SET R3 WITH SIZE OF FREE      */ 00103000
         L     R3,CORESZE(,R6)                                     0078 00104000
         N     R3,@CF00711                                         0078 00105000
*   IF R7=0                         /* TEST FOR ANY MAX FOUND        */ 00106000
*     THEN                          /* YET IF NOT CHECK 1ST MAX      */ 00107000
         LTR   R7,R7                                               0079 00108000
         BZ    @RT00079                                            0079 00109000
*     GOTO S2CC0003;                /* IS THIS AREA LESS THAN        */ 00110000
*   IF R3^<R7                       /* ONE FOUND MAX                 */ 00111000
*     THEN                          /* BR- NO TO CHECK FOR MIN       */ 00112000
         CR    R3,R7                                               0081 00113000
         BNL   @RT00081                                            0081 00114000
*     GOTO S2CC0004;                /*                               */ 00115000
*S2CC0003:                                                         0083 00116000
*   ;                               /* CHECK MAX                     */ 00117000
S2CC0003 DS    0H                                                  0084 00118000
*   IF R5^>R3                       /* IF REQUESTED MAX NOT          */ 00119000
*     THEN                          /* GTR THAN AREA AVAIBLE         */ 00120000
         CR    R5,R3                                               0084 00121000
         BNH   @RT00084                                            0084 00122000
*     GOTO S2CC0005;                /* SAVE PTR                      */ 00123000
*S2CC0004:                                                         0086 00124000
*   ;                               /* CHECK MIN                     */ 00125000
S2CC0004 DS    0H                                                  0087 00126000
*   IF R4>R3                        /* IF REQUESTED MIN GTR THAN     */ 00127000
*     THEN                          /* AREA AVAILABLE GOTO           */ 00128000
         CR    R4,R3                                               0087 00129000
         BH    @RT00087                                            0087 00130000
*     GOTO S2CC0008;                /* CHECK NXT AREA                */ 00131000
*   IF $MORLEVL='03'X               /* TEST FOR LEVEL 3 MACRO        */ 00132000
*     THEN                          /* IF YES TRY BTWEEN             */ 00133000
         CLI   $MORLEVL(MORMAP),X'03'                              0089 00134000
         BE    @RT00089                                            0089 00135000
*     GOTO S2CC0006;                /*                               */ 00136000
*   IF R8=0                         /* ANY PREVIOUS MIN'S FOUND      */ 00137000
*     THEN                          /* NO GO SAVE POTENTIAL MINS     */ 00138000
         LTR   R8,R8                                               0091 00139000
         BZ    @RT00091                                            0091 00140000
*     GOTO S2CC0007;                /*                               */ 00141000
*   IF R8>R3                        /* IS MINIMUM GTR THAN AREA      */ 00142000
*     THEN                          /* REQUESTED                     */ 00143000
         CR    R8,R3                                               0093 00144000
         BH    @RT00093                                            0093 00145000
*     GOTO S2CC0007;                /* SAVE PTR                      */ 00146000
*   GOTO S2CC0008;                  /* IF NO CHECK NXT AREA          */ 00147000
         B     S2CC0008                                            0095 00148000
*S2CC0005:                                                         0096 00149000
*   ;                               /* POTENTIAL MAX                 */ 00150000
S2CC0005 DS    0H                                                  0097 00151000
*   MAXPTR=R6;                      /* SAVE PTR FOR POTENT. MAX      */ 00152000
         ST    R6,MAXPTR                                           0097 00153000
*   R7=R3;                          /* SAVE SIZE OF POT. MAX         */ 00154000
         LR    R7,R3                                               0098 00155000
*   GOTO S2CC0008;                  /* GO CHECK NXT AREA             */ 00156000
         B     S2CC0008                                            0099 00157000
*S2CC0006:                                                         0100 00158000
*   ;                               /*                               */ 00159000
S2CC0006 DS    0H                                                  0101 00160000
*   IF R8^<R3                       /* IS REQ SIZE GTR THAN          */ 00161000
*     THEN                          /* FOUND MINIMUM                 */ 00162000
         CR    R8,R3                                               0101 00163000
         BNL   @RT00101                                            0101 00164000
*     GOTO S2CC0008;                /* YES GO TRY NXT AREA           */ 00165000
*S2CC0007:                                                         0103 00166000
*   ;                               /*                               */ 00167000
S2CC0007 DS    0H                                                  0104 00168000
*   MINPTR=R6;                      /* SAVE POTENTIAL MIN PTR        */ 00169000
         ST    R6,MINPTR                                           0104 00170000
*   R8=R3;                          /* SAVE POT SIZE                 */ 00171000
         LR    R8,R3                                               0105 00172000
*S2CC0008:                          /*                               */ 00173000
*   IF COREFWD=0                    /* IF COREFWD=0 WE ARE AT END    */ 00174000
*     THEN                          /* OF LOOP MAX-MIN SET           */ 00175000
S2CC0008 L     @14,COREFWD(,R6)                                    0106 00176000
         LTR   @14,@14                                             0106 00177000
         BZ    @RT00106                                            0106 00178000
*     GOTO S2CC0010;                /* IF NOT PICK UP PTR AND        */ 00179000
*   R6=COREFWD;                     /* RE LOOP                       */ 00180000
         LR    R6,@14                                              0108 00181000
*   GOTO S2CC0002;                  /*                               */ 00182000
         B     S2CC0002                                            0109 00183000
*/********************************************************************/ 00184000
*                                                                  0110 00185000
*   GEN(EJECT);                                                    0110 00186000
         EJECT                                                          00187000
*                                                                  0111 00188000
*/********************************************************************/ 00189000
*/*      GIVE  MAX OR MIN OR BETWEEN  CORE                           */ 00190000
*/********************************************************************/ 00191000
*                                                                  0111 00192000
*S2CC0010:                                                         0111 00193000
*   ;                               /* ANY MAX FOUND                 */ 00194000
S2CC0010 DS    0H                                                  0112 00195000
*   IF R7=0                         /* BR-NO                         */ 00196000
*     THEN                          /*                               */ 00197000
         LTR   R7,R7                                               0112 00198000
         BZ    @RT00112                                            0112 00199000
*     GOTO S2CC0011;                /*                               */ 00200000
*   R0=$MORCMAX;                    /* YES SET SIZE FOR CALLER       */ 00201000
         L     R0,$MORCMAX(,MORMAP)                                0114 00202000
*   R1=MAXPTR+8;                    /* YES SET PTR FOR CALLER        */ 00203000
         L     @14,MAXPTR                                          0115 00204000
         LA    R1,8                                                0115 00205000
         ALR   R1,@14                                              0115 00206000
*   R7=MAXPTR;                                                     0116 00207000
         LR    R7,@14                                              0116 00208000
*   GOTO S2CC0013;                  /*                               */ 00209000
         B     S2CC0013                                            0117 00210000
*S2CC0011:                                                         0118 00211000
*   ;                               /*                               */ 00212000
S2CC0011 DS    0H                                                  0119 00213000
*   IF R8=0                         /* ANY MIN/BTWEEN FOUND          */ 00214000
*     THEN                          /* BR-NO TO RTN CODE 8           */ 00215000
         LTR   R8,R8                                               0119 00216000
         BZ    @RT00119                                            0119 00217000
*     GOTO S2CC0090;                /*                               */ 00218000
*   R7=MINPTR;                      /* SET UP FOR NEW MOREC PTR      */ 00219000
         L     R7,MINPTR                                           0121 00220000
*   MINPTR=R7+8;                    /* SET UP ADDR+8                 */ 00221000
         LA    @14,8                                               0122 00222000
         LR    @09,R7                                              0122 00223000
         ALR   @09,@14                                             0122 00224000
         ST    @09,MINPTR                                          0122 00225000
*   IF $MORLEVL^='03'X              /* IS MACRO LEVEL 3              */ 00226000
*     THEN                          /* BR-NO-GIVE MIN                */ 00227000
         CLI   $MORLEVL(MORMAP),X'03'                              0123 00228000
         BNE   @RT00123                                            0123 00229000
*     GOTO S2CC0012;                /*                               */ 00230000
*   R0=R8-8;                        /* GET SIZE FOR CALLER           */ 00231000
         LR    R0,R8                                               0125 00232000
         SLR   R0,@14                                              0125 00233000
*   R1=MINPTR;                      /* SET UP ADDRESS + 8            */ 00234000
         LR    R1,@09                                              0126 00235000
*   R6=R1-8;                        /* SET POINTER FOR FREE BIT      */ 00236000
         LCR   @14,@14                                             0127 00237000
         ALR   @14,R1                                              0127 00238000
         LR    R6,@14                                              0127 00239000
*   GOTO S2CC0021;                  /*                               */ 00240000
         B     S2CC0021                                            0128 00241000
*S2CC0012:                                                         0129 00242000
*   ;                               /*                               */ 00243000
S2CC0012 DS    0H                                                  0130 00244000
*   R1=MINPTR;                      /* SET UP ADDRESS + 8            */ 00245000
         L     R1,MINPTR                                           0130 00246000
*   R5=R4;                          /* GET NEW MORE CORE REQ         */ 00247000
         LR    R5,R4                                               0131 00248000
*   R0=$MORCMIN;                    /* GET MIN LENGTH                */ 00249000
         L     R0,$MORCMIN(,MORMAP)                                0132 00250000
*S2CC0013:                                                         0133 00251000
*   ;                               /*                               */ 00252000
S2CC0013 DS    0H                                                  0134 00253000
*   R6=R7;                          /* GET OLD FREE PTR INTO R6      */ 00254000
         LR    R6,R7                                               0134 00255000
*   R7=CORESZE;                     /* PICK UP OLD FREE SIZE         */ 00256000
         L     R7,CORESZE(,R6)                                     0135 00257000
*   R5=(((R5+7)/8)*8);              /* ROUND SIZE UPTO DWORD         */ 00258000
         LA    @14,8                                               0136 00259000
         LA    @10,7                                               0136 00260000
         ALR   @10,R5                                              0136 00261000
         SRDA  @10,32                                              0136 00262000
         DR    @10,@14                                             0136 00263000
         SLA   @11,3                                               0136 00264000
         LR    R5,@11                                              0136 00265000
*   IF 8>(R7-R5)                    /* ANY SIZE LEFT FOR MORECORE    */ 00266000
*     THEN                          /* BR-NONE LEFT                  */ 00267000
         LR    @09,R7                                              0137 00268000
         SLR   @09,R5                                              0137 00269000
         CR    @09,@14                                             0137 00270000
         BL    @RT00137                                            0137 00271000
*     GOTO S2CC0021;                /*                               */ 00272000
*/********************************************************************/ 00273000
*/*           UPDATE MORE/FREE CORE CHAIN                            */ 00274000
*/********************************************************************/ 00275000
*                                                                  0139 00276000
*S2CC0020:                                                         0139 00277000
*   ;                                                              0139 00278000
S2CC0020 DS    0H                                                  0140 00279000
*   R8=R6+R5;                       /* SET PTR TO NEW FREE AREA      */ 00280000
         LR    R8,R6                                               0140 00281000
         ALR   R8,R5                                               0140 00282000
*   COREFWDN=COREFWD;               /* SET UP CHAIN IN NEW FREE      */ 00283000
         L     @14,COREFWD(,R6)                                    0141 00284000
         ST    @14,COREFWDN(,R8)                                   0141 00285000
*   CORESZEN=CORESZE-R5;            /* AND JUST GOTTEN AREA          */ 00286000
         L     @14,CORESZE(,R6)                                    0142 00287000
         SLR   @14,R5                                              0142 00288000
         ST    @14,CORESZEN(,R8)                                   0142 00289000
*   FREEN='1'B;                     /* SET FLAGS IN NEW FOR FREE     */ 00290000
         OI    FREEN(R8),B'00000001'                               0143 00291000
*   CORESZE=R5;                     /* SET SIZE                      */ 00292000
         ST    R5,CORESZE(,R6)                                     0144 00293000
*   COREFWD=R8;                     /* SET CHAIN PTR IN GOTTEN       */ 00294000
         ST    R8,COREFWD(,R6)                                     0145 00295000
*S2CC0021:                                                         0146 00296000
*   ;                               /* TO NEW FREE                   */ 00297000
S2CC0021 DS    0H                                                  0147 00298000
*   FREE='0'B;                      /* SET GOTTEN AREA NOT FREE      */ 00299000
         NI    FREE(R6),B'11111110'                                0147 00300000
*   GOTO S2CC0092;                  /* GOTO RTN CODE 0               */ 00301000
         B     S2CC0092                                            0148 00302000
*/********************************************************************/ 00303000
*                                                                  0149 00304000
*   GEN(EJECT);                                                    0149 00305000
         EJECT                                                          00306000
*                                                                  0150 00307000
*/********************************************************************/ 00308000
