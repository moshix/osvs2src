         TITLE 'ROUTINE SERVICE MODULE - ISTSC02E                      *00001000
                        '                                               00002000
* IBM DISK OPERATING SYSTEM                                             00003000
* TOLTEP - 5745-SC-TLT                                                  00004000
          ISEQ 73,80                                                    00005000
ISTSC02E CSECT ,                                                   0002 00006000
@MAINENT DS    0H                                                  0002 00007000
         USING *,@15                                               0002 00008000
         B     @PROLOG                                             0002 00009000
         DC    AL1(16)                                             0002 00010000
         DC    C'ISTSC02E  75.177'                                 0002 00011000
         DROP  @15                                                      00012000
@PROLOG  ST    @14,12(,@13)                                        0002 00013000
         STM   @00,@12,20(@13)                                     0002 00014000
         BALR  @12,0                                               0002 00015000
@PSTART  DS    0H                                                  0002 00016000
         USING @PSTART,@12                                         0002 00017000
*   R15=R13;                        /* SET BACKSAVE PTR              */ 00018000
         LR    R15,R13                                             0043 00019000
*   R13=ADDR(SVESC02E);             /* PICK UP PTR TO SAVE AREA      */ 00020000
         LA    R13,SVESC02E(,WAPTR)                                0044 00021000
*   BACKSAVE(3)=R13;                /* CHAIN BACKAREA & THIS AREA    */ 00022000
         ST    R13,BACKSAVE+8(,R15)                                0045 00023000
*   SVESC02E(2)=R15;                /* TOGETHER                      */ 00024000
         ST    R15,SVESC02E+4(,WAPTR)                              0046 00025000
*   RTNMAP=R1;                      /* SAVE PARM POINTER.            */ 00026000
         LR    RTNMAP,R1                                           0047 00027000
*   $RETMASK='00'X;                 /* RESET RETURN CODE MASK BYTE   */ 00028000
         MVI   $RETMASK(WAPTR),X'00'                               0048 00029000
*   $TRTN#=$RTNENUM;                /* ROUTINE NUMBER FOR TRACE      */ 00030000
         SLR   @09,@09                                             0049 00031000
         IC    @09,$RTNENUM(,RTNMAP)                               0049 00032000
         STC   @09,$TRTN#(,WAPTR)                                  0049 00033000
*   $TESTERR=OFF;                   /* RESET TEST ERROR BIT          */ 00034000
         NI    $TESTERR(WAPTR),B'01111111'                         0050 00035000
*   RTNSEL(1:8)=$RT0108;            /* FIRST BYTE OF ROUTINE FLAGS   */ 00036000
         MVC   RTNSEL(1,WAPTR),$RT0108(WAPTR)                      0051 00037000
*   RTNSEL(9:16)=$RT0916;           /* SECOND BYTE ROUTINE FLAGS     */ 00038000
         MVC   RTNSEL+1(1,WAPTR),$RT0916(WAPTR)                    0052 00039000
*   RTNSEL(17:256)=$ROUTMSK;        /* LAST 14 BYTES ROUTINE FLAGS   */ 00040000
         MVC   RTNSEL+2(30,WAPTR),$ROUTMSK(WAPTR)                  0053 00041000
*/********************************************************************/ 00042000
*/* CHECK THE SELECTED ROUTINE NUMBER IN THE ROUTINE PARAMETER LIST  */ 00043000
*/* AGAINST A SET OF FLAGS REPRESENTING THE ROUTINES SELECTED FOR    */ 00044000
*/* EXECUTION BY THE OPERATOR.                                       */ 00045000
*/********************************************************************/ 00046000
*                                                                  0054 00047000
*   X=$RTNENUM-1;                   /* DECREMENT ROUTINE SELECTION   */ 00048000
         BCTR  @09,0                                               0054 00049000
         LR    X,@09                                               0054 00050000
*   R7=X/8;                         /* FIND ROUTINE MASK BYTE        */ 00051000
         LR    R7,X                                                0055 00052000
         SRL   R7,3                                                0055 00053000
*   R6=X//8;                        /* SAVE THE REMAINDER            */ 00054000
         LR    @10,X                                               0056 00055000
         SRDA  @10,32                                              0056 00056000
         D     @10,@CF00039                                        0056 00057000
         LR    R6,@10                                              0056 00058000
*   R1=0;                           /* RESET R1 TO ZEROS             */ 00059000
         SLR   R1,R1                                               0057 00060000
*   GEN(IC R1,MASKLIST(R6));        /* SELECT PROPER BIT MASK        */ 00061000
         IC R1,MASKLIST(R6)                                             00062000
*   R7=R7+ADDR($INBUFR);            /* CALCULATE ADDRESS OF ROUTINE     00063000
*                                      FLAGS FOR THIS ROUTINE.       */ 00064000
         LA    @09,$INBUFR(,WAPTR)                                 0059 00065000
         ALR   R7,@09                                              0059 00066000
*   GEN(EX R1,TSTMSK);              /* TEST FOR SELECTED FLAG ON     */ 00067000
         EX R1,TSTMSK                                                   00068000
*   BC(8,S2EC0005);                 /* BRANCH IF NOT ON.             */ 00069000
         BC    8,S2EC0005                                          0061 00070000
*/********************************************************************/ 00071000
*/* CHECK FOR MANUAL INTERVENTION REQUIRED BY THIS ROUTINE.          */ 00072000
*/********************************************************************/ 00073000
*                                                                  0062 00074000
*   IF $RTNMI=ON                    /* CHECK FOR MI REQUIRED         */ 00075000
*       &$MANINTV=OFF               /* AND MANUAL INTERVENTION IS NOT   00076000
*                                      ALLOWED.                      */ 00077000
*     THEN                                                         0062 00078000
         TM    $RTNMI(RTNMAP),B'10000000'                          0062 00079000
         BNO   @RF00062                                            0062 00080000
         TM    $MANINTV(WAPTR),B'10000000'                         0062 00081000
         BNZ   @RF00062                                            0062 00082000
*     DO;                                                          0063 00083000
*                                                                  0063 00084000
*/* ******************************************************************/ 00085000
*/*  MANUAL INTERVETION IS REQUIRED BY THIS ROUTINE BUT IS NOT       */ 00086000
*/*  ALLOWED PER OPERATOR OPTIONS. RTN BYPASSED.                     */ 00087000
*/* ******************************************************************/ 00088000
*                                                                  0064 00089000
*       GEN(BAL R14,S2EC0050);      /* CALL MSG SUBROUTINE           */ 00090000
*                                                                  0064 00091000
         BAL R14,S2EC0050                                               00092000
*       /*************************************************************/ 00093000
*       /*                                                           */ 00094000
*       /* TCEC MSGN(210)SUB(YES) INVOKED                            */ 00095000
*       /*                                                           */ 00096000
*       /*************************************************************/ 00097000
*                                                                  0065 00098000
*       $TCECM='1'B;                /* SET TCEC SW IN RCA            */ 00099000
         OI    $TCECM(WAPTR),B'00000001'                           0065 00100000
*       R1=ADDR($TCA07);            /* SET UP PARM LIST $TR          */ 00101000
         LA    R1,$TCA07                                           0066 00102000
*       CALL #EXIT;                 /* CALL CECOM                    */ 00103000
         L     @15,#EXITA(,WAPTR)                                  0067 00104000
         BALR  @14,@15                                             0067 00105000
*       DCL                                                        0068 00106000
*         1 $TCA07,                 /* TCEC PARM LIST                */ 00107000
*          2 $TCB07 FIXED(32) INIT(210),/* MSG NUMBER                */ 00108000
*          2 $TCC07 FIXED(32) INIT(0),/* INCT LEN                    */ 00109000
*          2 $TCD07 BIT(8) INIT('10000000'B),/* FLAG                 */ 00110000
*          2 $TCE07 CHAR(4) INIT('FFFFFFFF'X);/* MSSAGE MASK         */ 00111000
*       GO TO S2EC0005;                                            0069 00112000
         B     S2EC0005                                            0069 00113000
*     END;                                                         0070 00114000
*   IF $CTEQTT=ON                   /* IF CT ON SME LINE AS TT       */ 00115000
*       &$DCL=ON THEN               /* AND ROUTINE MACRO EXPANSION 0071 00116000
*                                      INDICATES IT CANNOT BE RUN IN    00117000
*                                      THIS CASE BECAUSE OF POSSIBLE    00118000
*                                      HANG UPS WITH TWO WAY       0071 00119000
*                                      MESSAGES, THEN BYPASS THIS  0071 00120000
*                                      ROUTINE.                      */ 00121000
@RF00062 TM    $CTEQTT(WAPTR),B'00010000'                          0071 00122000
         BNO   @RF00071                                            0071 00123000
         TM    $DCL(RTNMAP),B'00100000'                            0071 00124000
         BNO   @RF00071                                            0071 00125000
*     DO;                                                          0072 00126000
*                                                                  0072 00127000
*/********************************************************************/ 00128000
*/* EITHER CT OR AP IS ON SAME LINE AS A TEST TERMINAL AND THIS      */ 00129000
*/* ROUTINE CANNOT BE EXECUTED UNDER THAT CONDITION.                 */ 00130000
*/****************************************************************** */ 00131000
*                                                                  0073 00132000
*       GEN(BAL R14,S2EC0050);      /* CALL MSG SUBROUTINE           */ 00133000
*                                                                  0073 00134000
         BAL R14,S2EC0050                                               00135000
*       /*************************************************************/ 00136000
*       /*                                                           */ 00137000
*       /* TCEC MSGN(210)SUB(YES)MSGMSK('40000000'X) INVOKED         */ 00138000
*       /*                                                           */ 00139000
*       /*************************************************************/ 00140000
*                                                                  0074 00141000
*       $TCECM='1'B;                /* SET TCEC SW IN RCA            */ 00142000
         OI    $TCECM(WAPTR),B'00000001'                           0074 00143000
*       R1=ADDR($TCA08);            /* SET UP PARM LIST $TR          */ 00144000
         LA    R1,$TCA08                                           0075 00145000
*       CALL S20CEC03;              /* CALL TCEC-MULTIPLE LN         */ 00146000
         L     @15,@CV00643                                        0076 00147000
         BALR  @14,@15                                             0076 00148000
*       DCL                                                        0077 00149000
*         1 $TCA08,                 /* TCEC PARM LIST                */ 00150000
*          2 $TCB08 FIXED(32) INIT(210),/* MSG NUMBER                */ 00151000
*          2 $TCC08 FIXED(32) INIT(0),/* INCT LEN                    */ 00152000
*          2 $TCD08 BIT(8) INIT('10000000'B),/* FLAG                 */ 00153000
*          2 $TCE08 CHAR(4) INIT('40000000'X);/* MSSAGE MASK         */ 00154000
*       GO TO S2EC0005;                                            0078 00155000
         B     S2EC0005                                            0078 00156000
*     END;                                                         0079 00157000
*   ELSE                                                           0080 00158000
*     GO TO S2EC0020;                                              0080 00159000
*S2EC0005:                                                         0081 00160000
*   RET14=$RTNEEXT;                 /* SET RETURN REGISTER IN OLTS 0081 00161000
*                                      SAVE AREA TO POINT TO NEXT  0081 00162000
*                                      ROUTINE.                      */ 00163000
S2EC0005 L     @09,$R13PTR(,WAPTR)                                 0081 00164000
         L     @04,$RTNEEXT(,RTNMAP)                               0081 00165000
         ST    @04,RET14(,@09)                                     0081 00166000
*   R15=4;                                                         0082 00167000
         LA    R15,4                                               0082 00168000
*   GOTO S2EC0015;                                                 0083 00169000
         B     S2EC0015                                            0083 00170000
*S2EC0010:                                                         0084 00171000
*   R15=0;                          /* ZERO RETURN CODE              */ 00172000
S2EC0010 SLR   R15,R15                                             0084 00173000
*S2EC0015:                                                         0085 00174000
*   RET15=R15;                      /* RESET RETURN CODE REGISTER IN    00175000
*                                      OLTS SAVE AREA.               */ 00176000
S2EC0015 L     @09,$R13PTR(,WAPTR)                                 0085 00177000
         ST    R15,RET15(,@09)                                     0085 00178000
*   R13=SVESC02E(2);                /* RESTORE R13 TO ORIG VALUE     */ 00179000
         L     R13,SVESC02E+4(,WAPTR)                              0086 00180000
*   RETURN;                         /* RETURN VIA SAVE AREA MGR.     */ 00181000
@EL00001 DS    0H                                                  0087 00182000
@EF00001 DS    0H                                                  0087 00183000
@ER00001 L     @14,12(,@13)                                        0087 00184000
         LM    @00,@12,20(@13)                                     0087 00185000
         BR    @14                                                 0087 00186000
*S2EC0020:                                                         0088 00187000
*   $RTN#=$RTNENUM;                 /* MOVE RTN# INTO SECTION PREF   */ 00188000
S2EC0020 L     @09,$LOWADDR(,WAPTR)                                0088 00189000
         LH    @04,RTNDISP(,@09)                                   0088 00190000
         ALR   @04,@09                                             0088 00191000
         IC    @09,$RTNENUM(,RTNMAP)                               0088 00192000
         STC   @09,$RTN#(,@04)                                     0088 00193000
*   $REFNUM=$RTNEREF;               /* SET UP REF NUMB               */ 00194000
         MVC   $REFNUM(1,@04),$RTNEREF(RTNMAP)                     0089 00195000
*   GO TO S2EC0010;                 /* RETURN.                       */ 00196000
         B     S2EC0010                                            0090 00197000
*/********************************************************************/ 00198000
*/* THE FOLLOWING INSTRUCTION IS THE TARGET OF AN EX (EXECUTE) INSTR.*/ 00199000
*/********************************************************************/ 00200000
*                                                                  0091 00201000
*TSTMSK:                                                           0091 00202000
