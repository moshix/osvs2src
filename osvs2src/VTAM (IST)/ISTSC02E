S2EC0050 SLR   R1,R1                                               0092 00212000
         IC    R1,$RTNENUM(,RTNMAP)                                0092 00213000
*   CVD(R1,$INBUFR(70:77));         /* ROUTINE NUMBER IN PACKED    0093 00214000
*                                      DECIMAL.                      */ 00215000
         CVD   R1,$INBUFR+69(,WAPTR)                               0093 00216000
*   UNPK($INBUFR(1:4),$INBUFR(75:77));/* UNPACK                      */ 00217000
         UNPK  $INBUFR(4,WAPTR),$INBUFR+74(3,WAPTR)                0094 00218000
*   MVZ($INBUFR(4:4),$INBUFR(3:3)); /* ZONE TO SIGN.                 */ 00219000
         MVZ   $INBUFR+3(1,WAPTR),$INBUFR+2(WAPTR)                 0095 00220000
*   GEN(BR R14);                    /* RETURN                        */ 00221000
         BR R14                                                         00222000
