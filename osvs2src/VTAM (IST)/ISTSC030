         TITLE 'ISTSC030 - ACQUISITION/VERIFY FUNCTION                 *00001000
                        '                                               00002000
ISTSC030 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ISTSC030  77.194'                                 0001 00009000
S30C1000 DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00389-S30C1000)                                   00013000
         ENTRY S30C1000                                                 00014000
S30C0010 DS    0H                                                  0001 00015000
         USING *,@15                                               0001 00016000
         B     @PROLOG                                             0001 00017000
         DC    AL2(@EP00701-S30C0010)                                   00018000
         ENTRY S30C0010                                                 00019000
