*S34C3000:                                                         0606 00027000
*   R13=ADDR(S34SAVE);              /* POINT TO MY SAVEAREA          */ 00028000
S34C3000 LA    R13,S34SAVE                                         0606 00029000
*   RPHSAVE=R1;                     /* WE GET PTR TO RPH IN R1       */ 00030000
         ST    R1,RPHSAVE                                          0607 00031000
*   ZCRCHAIN=CRACRR;                /* SAVE CURRENT POINTERS FOR   0608 00032000
