*       RCSAVE=4;                   /* KILL THE BUILD                */ 01955000
         MVC   RCSAVE(4),@CF00053                                  1016 01956000
*     END;                                                         1017 01957000
*   ELSE                                                           1018 01958000
*     DO;                           /* IF FIRST TIME FOR MACRO       */ 01959000
         B     @RC01006                                            1018 01960000
@RF01006 DS    0H                                                  1019 01961000
*       KWDPTR=GPR1;                /* R1 CONTAINS PTR TO FIRST    1019 01962000
*                                      KEYWORD IN TEXT BUFFER        */ 01963000
         LR    KWDPTR,GPR1                                         1019 01964000
*       KWTBLPTR=ADDR(LBLDWTBL);    /* GET ADDRESS OF LBUILD KEY-WORD   01965000
*                                      TABLE                         */ 01966000
         LA    KWTBLPTR,LBLDWTBL                                   1020 01967000
*       GPR1=ALCHDR;                /* SET R1=ADDR BUILD AREA        */ 01968000
         L     GPR1,ALCHDR(,ALCAPTR)                               1021 01969000
*       LBLD='1'B;                  /* LBUILD MACRO IN PROCESS FLAG  */ 01970000
