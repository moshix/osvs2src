*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */ 00856000
         L     @15,@CV02970                                        0695 00857000
         BALR  @14,@15                                             0695 00858000
*                   RESPECIFY                                      0696 00859000
*                     CMZR0085 UNRSTD;                             0696 00860000
*                 END;                                             0697 00861000
*                 RC=0;             /* SET RETURN CODE               */ 00862000
         SLR   RC,RC                                               0698 00863000
*               END;                                               0699 00864000
*             ELSE                  /* FOR RETURN CODES OTHER THAN   */ 00865000
*               DO;                 /* 0 AND 4                       */ 00866000
         B     @RC00688                                            0700 00867000
@RF00688 DS    0H                                                  0701 00868000
