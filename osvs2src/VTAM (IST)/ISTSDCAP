         AL    @09,@CF03472                                        0761 01075000
         L     BLOCKPTR,ALCFAREA-4868(,@09)                        0761 01076000
*/********************************************************************/ 01077000
*/* DO-WHILE THERE ARE BLOCKS LEFT                                   */ 01078000
*/********************************************************************/ 01079000
*                                                                  0762 01080000
*                         DO WHILE(BLOCKPTR^=0);                   0762 01081000
         B     @DE00762                                            0762 01082000
@DL00762 DS    0H                                                  0763 01083000
*                           FROMPTR=ADDR(INADATA);/* FROMPTR= @OF  0763 01084000
