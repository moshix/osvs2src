*                   RESPECIFY                                      1049 02060000
*                     CMZR0165 UNRSTD;                             1049 02061000
*                 END;              /*                       @X50BHMX*/ 02062000
*               END;                /*                       @X50BHMX*/ 02063000
*           ELSE                                                   1052 02064000
*             ;                     /* NO DSP - DO NOTHING   @X50BHMX*/ 02065000
@RF01039 DS    0H                                                  1053 02066000
*           LBUILD='1'B;            /* SET THE LBUILD FOOTPRINT      */ 02067000
@RC01039 OI    LBUILD,B'10000000'                                  1053 02068000
*           RDTMAXID=ATCMAXID;      /* COPY MAXID FROM ATCVT TO LCL     02069000
*                                      HDR                           */ 02070000
         IC    @11,ATCMAXID(,ATCPT)                                1054 02071000
         STC   @11,RDTMAXID(,RDTPTR)                               1054 02072000
*           RFY                                                    1055 02073000
*            (REGX,                                                1055 02074000
