         LA    @11,ALCSAV3(,ALCAPTR)                               0724 00949000
         ST    @11,SAVCLLD(,SAVREG)                                0724 00950000
*                     SAVSAV=SAVREG;/* SAVE CALLER'S SAVE@           */ 00951000
         ST    SAVREG,SAVSAV                                       0725 00952000
*                     SAVREG=ADDR(ALCSAV3);/* SET UP OUR SAVE@       */ 00953000
         LR    SAVREG,@11                                          0726 00954000
*                     SAVCLLR=SAVSAV;/* SAVE CALLRS SAVE@ IN OURS    */ 00955000
         L     @11,SAVSAV                                          0727 00956000
         ST    @11,SAVCLLR(,SAVREG)                                0727 00957000
*/********************************************************************/ 00958000
*/* THE FOLLOWING SETS THE BIT INDICATING THAT THE ENTRY             */ 00959000
*/* IS THE LAST IN THE RDT SEGMENT BEING EXAMINED                    */ 00960000
*/********************************************************************/ 00961000
*                                                                  0728 00962000
*                     IF HDRTYPE=RPRENTAH/* APPL ENTRY               */ 00963000
