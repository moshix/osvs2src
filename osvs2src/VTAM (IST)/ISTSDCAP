         SLR   @11,@11                                             1136 02320000
         ST    @11,RCSAVE                                          1136 02321000
*             END;                                                 1137 02322000
*           ELSE                    /* END OF TEXT STRING            */ 02323000
*             RCSAVE=8;             /* SO END DO-WHILE LOOP          */ 02324000
         B     @RC01126                                            1138 02325000
@RF01126 MVC   RCSAVE(4),@CF00059                                  1138 02326000
*       END;                        /* END OF DO-WHILE LOOP          */ 02327000
