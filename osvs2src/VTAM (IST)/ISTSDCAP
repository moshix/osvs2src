         L     @11,SAVSAV                                          1074 02137000
         ST    @11,SAVCLLR(,SAVREG)                                1074 02138000
*   IF LBLD='0'B THEN               /* PROCESSING LBUILD?            */ 02139000
         TM    LBLD,B'01000000'                                    1075 02140000
         BNZ   @RF01075                                            1075 02141000
*     DO;                                                          1076 02142000
*       CALL ISTSDCC1;                                             1077 02143000
         L     @15,@CV02969                                        1077 02144000
