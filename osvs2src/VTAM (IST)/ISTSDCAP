*         GPR1=KWDPTR;              /* AND ADDR OF KEY WD IN MAC     */ 02264000
         LR    GPR1,KWDPTR                                         1116 02265000
*         MLCKWD=KWDPTR;            /* ALSO KW@ IN MLCA (SET FOR   1117 02266000
*                                      ERRFMT-ISTSCCC2)              */ 02267000
         ST    KWDPTR,MLCKWD(,MLCAPTR)                             1117 02268000
*         CALL ISTINCPH;                                           1118 02269000
         L     @15,@CV02967                                        1118 02270000
         BALR  @14,@15                                             1118 02271000
*         KWDPTR=GPR1;              /* SAVE NEXT KEY WORD@           */ 02272000
         LR    KWDPTR,GPR1                                         1119 02273000
*         IF RC=0 THEN              /* GOOD RETURN-ROUT@ IN R0       */ 02274000
         LTR   RC,RC                                               1120 02275000
         BNZ   @RF01120                                            1120 02276000
*           DO;                                                    1121 02277000
*             LINKREG=GPR0;         /* SET ROUT@ IN LINKAGE REG      */ 02278000
         LR    LINKREG,GPR0                                        1122 02279000
*             CALL PHDLROUT;                                       1123 02280000
         BALR  @14,@15                                             1123 02281000
*             RCSAVE=RC;            /* SAVE RETURN CODE              */ 02282000
         ST    RC,RCSAVE                                           1124 02283000
*           END;                                                   1125 02284000
*         ELSE                                                     1126 02285000
*           IF RC=4 THEN            /* RC4 = KEY-WORD NOT FOUND      */ 02286000
         B     @RC01120                                            1126 02287000
@RF01120 C     RC,@CF00053                                         1126 02288000
         BNE   @RF01126                                            1126 02289000
*             DO;                                                  1127 02290000
*                                                                  1127 02291000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 02292000
*                                                                  1128 02293000
