         LA    CMZR0086,CMZP0086                                   0705 00889000
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */ 00890000
         L     @15,@CV02970                                        0706 00891000
         BALR  @14,@15                                             0706 00892000
*                   RESPECIFY                                      0707 00893000
*                     CMZR0086 UNRSTD;                             0707 00894000
*                 END;                                             0708 00895000
*                 RC=0;             /* SET RETURN CODE               */ 00896000
         SLR   RC,RC                                               0709 00897000
*               END;                                               0710 00898000
*           END;                                                   0711 00899000
