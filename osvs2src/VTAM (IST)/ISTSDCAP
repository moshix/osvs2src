         AL    @11,RLENGTH                                         0738 01000000
         N     @11,@CF03106                                        0738 01001000
         ST    @11,RLENGTH                                         0738 01002000
*                     GPR1=MLCRPH;  /* PREPARE FOR GETSTOR           */ 01003000
*                                                                  0739 01004000
         L     GPR1,MLCRPH(,MLCAPTR)                               0739 01005000
*                     /***********************************************/ 01006000
*                     /*                                             */ 01007000
*                     /* GETSTOR SAVE(YES)SIZE(RLENGTH)              */ 01008000
*                     /*                                             */ 01009000
*                     /***********************************************/ 01010000
*                                                                  0740 01011000
