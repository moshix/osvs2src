*                           BLOCKPTR=INAFWD;/* GET NEXT BLOCK'S ADDR */ 01118000
         L     BLOCKPTR,INAFWD(,BLOCKPTR)                          0773 01119000
*                         END;      /* END OF MOVE LOOP              */ 01120000
@DE00762 LTR   BLOCKPTR,BLOCKPTR                                   0774 01121000
         BNZ   @DL00762                                            0774 01122000
*                         RC=8;     /* SET GOOD RETURN CODE          */ 01123000
         LA    RC,8                                                0775 01124000
*                       END;        /* END OF PROCESS FOR SPACE    0776 01125000
*                                      AVAILABLE RET CODE FROM     0776 01126000
