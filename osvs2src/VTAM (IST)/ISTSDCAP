         TITLE 'ISTSDCAP APPLICATION/LOCAL RDT BUILD ROUTINE           *00001000
                        '                                               00002000
ISTSDCAP CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ISTSDCAP  77.070'                                 0001 00009000
ISTSDCLC DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00443-ISTSDCLC)                                   00013000
         ENTRY ISTSDCLC                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @12,0                                               0001 00017000
@PSTART  DS    0H                                                  0001 00018000
         USING @PSTART,@12                                         0001 00019000
         AH    @15,4(,@15)                                         0001 00020000
         BR    @15                                                 0001 00021000
@EP00001 DS    0H                                                  0002 00022000
           USING @DATD-(MLCDAT3-ISTMLCA),MLCAPTR    ADDRESABIL FOR DATA 00023000
*   SAVCLLD=ADDR(MLCSAV3);          /* SAVE OUR SAVE@ IN CALLRS      */ 00024000
         LA    @11,MLCSAV3(,MLCAPTR)                               0429 00025000
         ST    @11,SAVCLLD(,SAVREG)                                0429 00026000
*   SAVSAV=SAVREG;                  /* SAVE CALLER'S SAVE@           */ 00027000
         ST    SAVREG,SAVSAV                                       0430 00028000
*   SAVREG=ADDR(MLCSAV3);           /* SET UP OUR SAVE@              */ 00029000
         LR    SAVREG,@11                                          0431 00030000
*   SAVCLLR=SAVSAV;                 /* SAVE CALLER'S SAVE@ IN OURS   */ 00031000
         L     @11,SAVSAV                                          0432 00032000
         ST    @11,SAVCLLR(,SAVREG)                                0432 00033000
*   TBLPTR=ADDR(APLMHT);            /* POINT TO APPL MAC TABL        */ 00034000
         LA    TBLPTR,APLMHT                                       0433 00035000
*   HDRTYPE=RPRENTAH;               /* SET HEADER TYPE TO APPL. USED    00036000
*                                      AS SWITCH DURING CODE TO    0434 00037000
*                                      DISTINQUISH APPL FROM LOCAL 0434 00038000
*                                      *************************     */ 00039000
         MVI   HDRTYPE,X'02'                                       0434 00040000
*   CALL SKELBLD;                   /* CALL SKELBLD                0435 00041000
*                                      *************************     */ 00042000
         BAL   @14,SKELBLD                                         0435 00043000
*   IF RCSAVE=0 THEN                                               0436 00044000
         L     @07,RCSAVE                                          0436 00045000
         LTR   @07,@07                                             0436 00046000
         BNZ   @RF00436                                            0436 00047000
*     DO;                                                          0437 00048000
*       RDTPTR=MLCSKEL;             /* SET UP RDT BASE               */ 00049000
         L     RDTPTR,MLCSKEL(,MLCAPTR)                            0438 00050000
*       RDTPRIAP='1'B;              /* INDICATE APPL.RDT ENTRY       */ 00051000
         OI    RDTPRIAP(RDTPTR),B'00010000'                        0439 00052000
*     END;                                                         0440 00053000
*   SAVREG=SAVCLLR;                 /* RESTORE CALLER'S SAVE ADDR    */ 00054000
@RF00436 L     SAVREG,SAVCLLR(,SAVREG)                             0441 00055000
*   RETURN CODE(RCSAVE);                                           0442 00056000
         L     @15,RCSAVE                                          0442 00057000
         L     @14,12(,@13)                                        0442 00058000
         LM    @00,@12,20(@13)                                     0442 00059000
         BR    @14                                                 0442 00060000
*                                                                  0443 00061000
*/********************************************************************/ 00062000
*/*                      ISTSDCLC  ROUTINE LOCAL BUILD               */ 00063000
*/* THIS ROUTINE CONTROLS BUILDING OF THE LOCAL RDT SEGMENT          */ 00064000
*/*   NOTE: THIS ROUTINE AND ISTSDCAP ARE BOTH ENTRIES TO THIS MODULE*/ 00065000
*/********************************************************************/ 00066000
*                                                                  0443 00067000
*ISTSDCLC:                                                         0443 00068000
*   ENTRY;                                                         0443 00069000
@EP00443 DS    0H                                                  0444 00070000
*   SAVCLLD=ADDR(MLCSAV3);          /* SAVE OUR SAVE@ IN CALLERS     */ 00071000
         LA    @11,MLCSAV3(,MLCAPTR)                               0444 00072000
         ST    @11,SAVCLLD(,SAVREG)                                0444 00073000
*   SAVSAV=SAVREG;                  /* SAVE CALLER'S SAVE@           */ 00074000
         ST    SAVREG,SAVSAV                                       0445 00075000
*   SAVREG=ADDR(MLCSAV3);           /* SET UP THIS ROUT'S SAVE@      */ 00076000
         LR    SAVREG,@11                                          0446 00077000
*   SAVCLLR=SAVSAV;                 /* SAVE CALLER'S SAVE@ IN OURS   */ 00078000
         L     @11,SAVSAV                                          0447 00079000
         ST    @11,SAVCLLR(,SAVREG)                                0447 00080000
*   TBLPTR=ADDR(LCLMHT);            /* POINT TO LOCAL MACRO TABLE    */ 00081000
         LA    TBLPTR,LCLMHT                                       0448 00082000
*   HDRTYPE=RPRENTLH;               /* SET HEADER TYPE TO LOCAL. USED   00083000
*                                      AS SWITCH DURING CODE TO    0449 00084000
*                                      DISTINGUISH LOCAL FROM APPL 0449 00085000
*                                      *************************     */ 00086000
         MVI   HDRTYPE,X'03'                                       0449 00087000
*   CALL SKELBLD;                   /* CALL SKEL BLD               0450 00088000
*                                      *************************     */ 00089000
         BAL   @14,SKELBLD                                         0450 00090000
*   IF RCSAVE=0 THEN                                               0451 00091000
         L     @07,RCSAVE                                          0451 00092000
         LTR   @07,@07                                             0451 00093000
         BNZ   @RF00451                                            0451 00094000
*     DO;                                                          0452 00095000
*       RDTPTR=MLCSKEL;             /* SET UP RDT BASE               */ 00096000
         L     RDTPTR,MLCSKEL(,MLCAPTR)                            0453 00097000
*       RDTPRILC='1'B;              /* INDICATE LOCAL RDT ENTRY      */ 00098000
         OI    RDTPRILC(RDTPTR),B'00001000'                        0454 00099000
*     END;                                                         0455 00100000
*   SAVREG=SAVCLLR;                 /* RESTORE CALLER'S SAVE ADDR    */ 00101000
@RF00451 L     SAVREG,SAVCLLR(,SAVREG)                             0456 00102000
*   RETURN CODE(RCSAVE);                                           0457 00103000
         L     @15,RCSAVE                                          0457 00104000
         L     @14,12(,@13)                                        0457 00105000
         LM    @00,@12,20(@13)                                     0457 00106000
         BR    @14                                                 0457 00107000
*   RFY                                                            0458 00108000
*     RDTPTR UNRSTD;                                               0458 00109000
*                                                                  0459 00110000
*/********************************************************************/ 00111000
*/*                       SKELBLD ROUTINE                            */ 00112000
*/* CONTROLS GETTING THE ALCA AREA PUTTING THE DUMMY APPL OR LOCAL   */ 00113000
*/* HEADER IN THE BUILD AREA PROCESSING THE DEFINITION DECK AND      */ 00114000
*/* FREEING UP THE ALCA AND THE BUILD AREAS                          */ 00115000
*/********************************************************************/ 00116000
*                                                                  0459 00117000
*SKELBLD:                                                          0459 00118000
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0459 00119000
SKELBLD  STM   @14,@12,12(@13)                                     0459 00120000
*   SAVCLLD=ADDR(MLCSAV4);          /* SAVE OUR SAVE@ IN CALLRS      */ 00121000
         LA    @02,MLCSAV4(,MLCAPTR)                               0460 00122000
         ST    @02,SAVCLLD(,SAVREG)                                0460 00123000
*   SAVSAV=SAVREG;                  /* SAVE CALLER'S SAVE@           */ 00124000
         ST    SAVREG,SAVSAV                                       0461 00125000
*   SAVREG=ADDR(MLCSAV4);           /* SET UP OUR SAVE@              */ 00126000
         LR    SAVREG,@02                                          0462 00127000
*   SAVCLLR=SAVSAV;                 /* SAVE CALLER'S SAVE@ IN OURS   */ 00128000
*                                                                  0463 00129000
         L     @02,SAVSAV                                          0463 00130000
         ST    @02,SAVCLLR(,SAVREG)                                0463 00131000
*   /*****************************************************************/ 00132000
*   /*                                                               */ 00133000
*   /* SEGMENT(GETALCA)REQUEST                                       */ 00134000
*   /*                                                               */ 00135000
*   /*****************************************************************/ 00136000
*                                                                  0464 00137000
*   DO;                                                            0464 00138000
*     GOTO GETALCA;                                                0465 00139000
         B     GETALCA                                             0465 00140000
*GETALCA#:                                                         0466 00141000
*   END;                                                           0466 00142000
GETALCA# DS    0H                                                  0467 00143000
*                                                                  0467 00144000
*/* SEGMENT(GETALCA)START                                            */ 00145000
*                                                                  0467 00146000
*   DO;                                                            0467 00147000
*     GOTO GETALCA@;                                               0468 00148000
         B     GETALCA@                                            0468 00149000
*GETALCA:                                                          0469 00150000
*   END;                                                           0469 00151000
*                                                                  0469 00152000
GETALCA  DS    0H                                                  0470 00153000
*/********************************************************************/ 00154000
*/*                GETALCA ROUTINE                                   */ 00155000
*/* IN-LINE SUB-ROUTINE TO OBTAIN THE AREA FOR APPL/LOCAL            */ 00156000
*/* COMMUNICATION AREA (ALCA) AND INITIALIZE IT. THE ALCA IS DYNAMIC */ 00157000
*/* SPACE FOR APPL OR LOCAL. SEE DSECT ISTALCA FOR DESCRIPTION       */ 00158000
*/********************************************************************/ 00159000
*                                                                  0470 00160000
*   RFY                                                            0470 00161000
*    (RPHPTR,                                                      0470 00162000
*     BLOCKPTR) RSTD;                                              0470 00163000
*   RLENGTH=LENALCA;                /* SET UP LENGTH FOR GETSTOR     */ 00164000
         L     @11,LENALCA                                         0471 00165000
         ST    @11,RLENGTH                                         0471 00166000
*   RPHPTR=MLCRPH;                  /* SET UP RPH@ FOR GETSTOR ISSUE    00167000
*                                      GETSTOR MACRO FOR ISTALCA     */ 00168000
         L     RPHPTR,MLCRPH(,MLCAPTR)                             0472 00169000
*GETSTR1:                                                          0473 00170000
*   ;                                                              0473 00171000
*                                                                  0473 00172000
GETSTR1  DS    0H                                                  0474 00173000
*/*  GETSTOR SIZE(RLENGTH)CLEAR(YES)TYPE(PAGEABLE)ADSPACE(PRIVATE)   */ 00174000
*                                                                  0474 00175000
*   DO;                                                            0474 00176000
*                                                                  0474 00177000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)                        */ 00178000
*/* END OF ATBUILD                                                   */ 00179000
*                                                                  0475 00180000
*     RFY                                                          0475 00181000
*      (GPR00P,                                                    0475 00182000
*       GPR01P,                                                    0475 00183000
*       GPR02P,                                                    0475 00184000
*       GPR03P) RSTD;                                              0475 00185000
*     RFY                                                          0476 00186000
*       ISTRPH BASED(GPR01P);                                      0476 00187000
*     GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,GPR09P, 00188000
*         GPR10P,GPR11P,GPR12P,GPR13P) SETS(RPHWORK);              0477 00189000
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS      00190000
*     GPR00F=RLENGTH;               /* SIZE IN REG0                  */ 00191000
         L     GPR00F,RLENGTH                                      0478 00192000
*     DCL                                                          0479 00193000
*       1 CMZA0008 FIXED(31) STATIC LOCAL,                         0479 00194000
*        2 * FIXED(8) INIT(96),     /* OPTION FLAGS                  */ 00195000
*        2 * FIXED(8) INIT(96),     /* STORAGE KEY                   */ 00196000
*        2 * FIXED(8) INIT(229),    /* SUBPOOL NUMBER                */ 00197000
*        2 * FIXED(8) INIT('00'X);  /* REQUEST TYPE                  */ 00198000
*     GPR03F=CMZA0008;                                             0480 00199000
         L     GPR03F,CMZA0008                                     0480 00200000
*     DO;                           /* LISATCVT                    0481 00201000
*                                      CVTBASE(PAGEABLE,(GPR02P))    */ 00202000
*       GPR02P=ATCLCPTR;            /* SET UP ATCVT POINTER          */ 00203000
         L     GPR02P,ATCLCPTR                                     0482 00204000
*     END;                          /* LISATCVT                      */ 00205000
*     CALL GPR02P->ATCGTSTR;        /* CALL ISTORFSR                 */ 00206000
         L     @15,ATCGTSTR(,GPR02P)                               0484 00207000
         BALR  @14,@15                                             0484 00208000
*     RFY                                                          0485 00209000
*      (GPR00P,                                                    0485 00210000
*       GPR01P,                                                    0485 00211000
*       GPR02P,                                                    0485 00212000
*       GPR03P) UNRSTD;                                            0485 00213000
*   END;                            /* END OF GETSTOR                */ 00214000
*                                                                  0486 00215000
*   /*****************************************************************/ 00216000
*   /*                                                               */ 00217000
*   /*                                                       @X04BHQH*/ 00218000
*   /*                                                               */ 00219000
*   /*****************************************************************/ 00220000
*                                                                  0487 00221000
*   IF RC=0 THEN                    /* IF GETSTOR WAS SUCCESS        */ 00222000
         LTR   RC,RC                                               0487 00223000
         BNZ   @RF00487                                            0487 00224000
*     DO;                           /* DO SET UP ALCA                */ 00225000
*       MLCSECCA=RPHSRPRM;          /* GET ADDR OF STORAGE           */ 00226000
         L     @11,RPHSRPRM(,GPR01P)                               0489 00227000
         ST    @11,MLCSECCA(,MLCAPTR)                              0489 00228000
*       ALCAPTR=RPHSRPRM;           /* SET UP ALCA ADDRESABIL        */ 00229000
         LR    ALCAPTR,@11                                         0490 00230000
*/*  GETSTOR SIZE(MLCGETSZ)SAVE(YES)TYPE(PAGEABLE)ADSPACE(PRIVATE)   */ 00231000
*                                                                  0491 00232000
*       DO;                                                        0491 00233000
*                                                                  0491 00234000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)                        */ 00235000
*/* END OF ATBUILD                                                   */ 00236000
*                                                                  0492 00237000
*         RFY                                                      0492 00238000
*          (GPR00P,                                                0492 00239000
*           GPR01P,                                                0492 00240000
*           GPR02P,                                                0492 00241000
*           GPR03P) RSTD;                                          0492 00242000
*         RFY                                                      0493 00243000
*           ISTRPH BASED(GPR01P);                                  0493 00244000
*         GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,    00245000
*             GPR09P,GPR10P,GPR11P,GPR12P,GPR13P) SETS(RPHWORK);   0494 00246000
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS      00247000
*         GPR00F=MLCGETSZ;          /* SIZE IN REG0                  */ 00248000
         L     GPR00F,MLCGETSZ(,MLCAPTR)                           0495 00249000
*         DCL                                                      0496 00250000
*           1 CMZA0026 FIXED(31) STATIC LOCAL,                     0496 00251000
*            2 * FIXED(8) INIT(64), /* OPTION FLAGS                  */ 00252000
*            2 * FIXED(8) INIT(96), /* STORAGE KEY                   */ 00253000
*            2 * FIXED(8) INIT(229),/* SUBPOOL NUMBER                */ 00254000
*            2 * FIXED(8) INIT('00'X);/* REQUEST TYPE                */ 00255000
*         GPR03F=CMZA0026;                                         0497 00256000
         L     GPR03F,CMZA0026                                     0497 00257000
*         DO;                       /* LISATCVT                    0498 00258000
*                                      CVTBASE(PAGEABLE,(GPR02P))    */ 00259000
*           GPR02P=ATCLCPTR;        /* SET UP ATCVT POINTER          */ 00260000
         L     GPR02P,ATCLCPTR                                     0499 00261000
*         END;                      /* LISATCVT                      */ 00262000
*         CALL GPR02P->ATCGTSTR;    /* CALL ISTORFSR                 */ 00263000
         L     @15,ATCGTSTR(,GPR02P)                               0501 00264000
         BALR  @14,@15                                             0501 00265000
*         RFY                                                      0502 00266000
*          (GPR00P,                                                0502 00267000
*           GPR01P,                                                0502 00268000
*           GPR02P,                                                0502 00269000
*           GPR03P) UNRSTD;                                        0502 00270000
*       END;                        /* END OF GETSTOR                */ 00271000
*                                                                  0503 00272000
*       /*************************************************************/ 00273000
*       /*                                                           */ 00274000
*       /*                                                   @X04BHQH*/ 00275000
*       /*                                                           */ 00276000
*       /*************************************************************/ 00277000
*                                                                  0504 00278000
*       IF RC=0 THEN                /* IF GETSTOR WAS SUCCESS        */ 00279000
         SLR   @11,@11                                             0504 00280000
         CR    RC,@11                                              0504 00281000
         BNE   @RF00504                                            0504 00282000
*         DO;                       /* BUILD AREA HAS BEEN OBTAIND   */ 00283000
*           BLOCKPTR=RPHSRPRM;      /* SET UP INTER DSECT INALCB     */ 00284000
         L     @02,RPHSRPRM(,GPR01P)                               0506 00285000
         LR    BLOCKPTR,@02                                        0506 00286000
*           INALEN=0;               /* SET LENGTH AND                */ 00287000
         ST    @11,INALEN(,BLOCKPTR)                               0507 00288000
*           INAFWD=0;               /* FORWARD POINTER TO ZERO       */ 00289000
         ST    @11,INAFWD(,BLOCKPTR)                               0508 00290000
*           ALCFAREA=RPHSRPRM;      /* SET FIRST AND LAST ALCA       */ 00291000
         LR    @11,ALCAPTR                                         0509 00292000
         AL    @11,@CF03472                                        0509 00293000
         ST    @02,ALCFAREA-4868(,@11)                             0509 00294000
*           ALCLAREA=RPHSRPRM;      /* POINTERS TO FIRST BLOCK       */ 00295000
*                                                                  0510 00296000
         LR    @11,ALCAPTR                                         0510 00297000
         AL    @11,@CF03473                                        0510 00298000
         ST    @02,ALCLAREA-4872(,@11)                             0510 00299000
*           /*********************************************************/ 00300000
*           /*                                                       */ 00301000
*           /* SEGMENT(MDLINIT)REQUEST                               */ 00302000
*           /*                                                       */ 00303000
*           /*********************************************************/ 00304000
*                                                                  0511 00305000
*           DO;                                                    0511 00306000
*             GOTO MDLINIT;                                        0512 00307000
         B     MDLINIT                                             0512 00308000
*MDLINIT#:                                                         0513 00309000
*           END;                                                   0513 00310000
*                                                                  0513 00311000
MDLINIT# DS    0H                                                  0514 00312000
*/* GO INIT REST OF ALCA                                             */ 00313000
*                                                                  0514 00314000
*                                                                  0514 00315000
*/* SEGMENT(MDLINIT)START                                            */ 00316000
*                                                                  0514 00317000
*           DO;                                                    0514 00318000
*             GOTO MDLINIT@;                                       0515 00319000
         B     MDLINIT@                                            0515 00320000
*MDLINIT:                                                          0516 00321000
*           END;                                                   0516 00322000
*                                                                  0516 00323000
MDLINIT  DS    0H                                                  0517 00324000
*/********************************************************************/ 00325000
*/*              MDLINIT START INTERNAL IN-LINE SUB                  */ 00326000
*/*       THIS ROUTINE SETS THE DEFAULTS IN THE APPL LOCAL AND       */ 00327000
*/*       TERMINAL MODEL ENTRIES IN THE ALCA. THESE MODELS ARE USED  */ 00328000
*/*       TO BE MOVED IN TO EACH ENTRY BEFOR THE NON DEFAULT         */ 00329000
*/*       PARAMETERS ARE SET SO THE DEFAULTS DO NOT HAVE TO BE SET   */ 00330000
*/*       EACH TIME                                                  */ 00331000
*/********************************************************************/ 00332000
*                                                                  0517 00333000
*           RFY                                                    0517 00334000
*             RDTEPTR RSTD;                                        0517 00335000
*           RFY                                                    0518 00336000
*             ISTRPRE BASED(RDTEPTR);                              0518 00337000
*           RFY                                                    0519 00338000
*             ISTRCPRE BASED(RDTEPTR);                             0519 00339000
*           RFY                                                    0520 00340000
*             ISTRAP BASED(RDTEPTR);                               0520 00341000
*           RFY                                                    0521 00342000
*             ISTRLC BASED(RDTEPTR);                               0521 00343000
*           RFY                                                    0522 00344000
*             ISTRTR BASED(RDTEPTR);                               0522 00345000
*           RFY                                                    0523 00346000
*             ISTDEVCH BASED(RDTEPTR);/* RDTEPTR LOADED DYNAMICALLY  */ 00347000
*           RDTEPTR=ADDR(ALCHDMDL); /* PICK UP MODEL ADDRESS         */ 00348000
         LA    RDTEPTR,ALCHDMDL(,ALCAPTR)                          0524 00349000
*           RPRENTRY=HDRTYPE;       /* SET UP TYPE                   */ 00350000
         MVC   RPRENTRY(1,RDTEPTR),HDRTYPE                         0525 00351000
*           RPRELEN=LENGTH(ISTRDT); /* SET UP THE LENGTH             */ 00352000
         MVC   RPRELEN(2,RDTEPTR),@CH03463                         0526 00353000
*           RPRHDTYP=HDRTYPE;       /* SET UP HEADER TYPE    @D32CKCK*/ 00354000
         MVC   RPRHDTYP(1,RDTEPTR),HDRTYPE                         0527 00355000
*           RPRNAME=MLCCONFG;       /* ENTRY NAME                    */ 00356000
         MVC   RPRNAME(8,RDTEPTR),MLCCONFG(MLCAPTR)                0528 00357000
*           IF HDRTYPE=RPRENTAH THEN/* IF A APPL ENTRY               */ 00358000
         CLI   HDRTYPE,X'02'                                       0529 00359000
         BNE   @RF00529                                            0529 00360000
*             DO;                   /* SET IT UP                     */ 00361000
*               RPRNOSRT='1'B;      /* SET NO SRT BIT IN APPL HEADER    00362000
*                                                            @XA08112*/ 00363000
         OI    RPRNOSRT(RDTEPTR),B'10000000'                       0531 00364000
*               RDTEPTR=ADDR(ALCAPMDL);/* SET UP APPL MODEL          */ 00365000
         LA    RDTEPTR,ALCAPMDL(,ALCAPTR)                          0532 00366000
*               RPRELEN=LENGTH(ISTRAP);/* ENTRY LENGTH               */ 00367000
         MVC   RPRELEN(2,RDTEPTR),@CH03464                         0533 00368000
*               RPRENTRY=RPRENTAP;  /* APPL ENTRY TYPE               */ 00369000
         MVI   RPRENTRY(RDTEPTR),X'55'                             0534 00370000
*               RPRNAME='';         /* APPL NAME                     */ 00371000
         MVI   RPRNAME(RDTEPTR),C' '                               0535 00372000
         MVC   RPRNAME+1(7,RDTEPTR),RPRNAME(RDTEPTR)               0535 00373000
*               RPRNOSRT='1'B;      /* SET NO SRT BIT IN ALL APPLS 0536 00374000
*                                                            @XA08112*/ 00375000
         OI    RPRNOSRT(RDTEPTR),B'10000000'                       0536 00376000
*               RPRHDTYP=RPRENTAH;  /* SET UP HEADER TYPE    @D32CKCK*/ 00377000
         MVI   RPRHDTYP(RDTEPTR),X'02'                             0537 00378000
*               RAPAPPWD='';        /* PASSWORD                      */ 00379000
         MVI   RAPAPPWD(RDTEPTR),C' '                              0538 00380000
         MVC   RAPAPPWD+1(7,RDTEPTR),RAPAPPWD(RDTEPTR)             0538 00381000
*               RAPBFACT=1;         /* BUFFER LIMIT FACTOR TO      0539 00382000
*                                      MULTIPLY TERMINAL BUFFER LIMIT*/ 00383000
         MVI   RAPBFACT(RDTEPTR),X'01'                             0539 00384000
*/********************************************************************/ 00385000
*/* THE REMAINDER OF THE APPL ENTRY MODEL IS ZERO BECAUSE THE ALCA   */ 00386000
*/* IS ZEROED BY GETSTOR                                             */ 00387000
*/********************************************************************/ 00388000
*                                                                  0540 00389000
*             END;                                                 0540 00390000
*           ELSE                                                   0541 00391000
*             DO;                   /* THE ENTRY IS A LOCAL          */ 00392000
         B     @RC00529                                            0541 00393000
@RF00529 DS    0H                                                  0542 00394000
*               RDTEPTR->RDTNODID=3;/* SET 3 NODES AS DEFAULT D31KKQH*/ 00395000
         MVI   RDTNODID(RDTEPTR),X'03'                             0542 00396000
*               RDTEPTR->RDTCRSDS=BLANK8;/*                  @X50BHMX*/ 00397000
         MVC   RDTCRSDS(8,RDTEPTR),@CC02956                        0543 00398000
*               RDTEPTR->RDTCRSPW=BLANK8;/*                  @X50BHMX*/ 00399000
         MVC   RDTCRSPW(8,RDTEPTR),@CC02956                        0544 00400000
*               RDTEPTR=ADDR(ALCLCMDL);/* GET ADR OF DUMMY LOCAL     */ 00401000
         LA    RDTEPTR,ALCLCMDL(,ALCAPTR)                          0545 00402000
*               RPRENTRY=RPRENTLC;  /* LOCAL ENTRY TYPE              */ 00403000
         MVI   RPRENTRY(RDTEPTR),X'45'                             0546 00404000
*               RPRNOSRT='1'B;      /* SET NO SRT BIT IN ALL LOCALS     00405000
