         BNZ   @RF00203                                            0203 00038000
*     DO;                           /* IF ON CONTINUE, ELSE EXIT     */ 00039000
*       LFTPRT='00'X;               /*                       @DA04743*/ 00040000
         MVI   LFTPRT,X'00'                                        0205 00041000
*/********************************************************************/ 00042000
*/* WHILE THE RETURN CODE = 0, THIS CODE PREPARES THE PHR PARAMETERS */ 00043000
*/* AND CALLS PHR.  UPON RETURN FROM PHR, THE RETURN CODE AND THE @  */ 00044000
*/* OF THE NEXT MACRO KEYWORD ARE SAVED.                             */ 00045000
*/********************************************************************/ 00046000
*                                                                  0206 00047000
*       RFY                                                        0206 00048000
*        (GPR0,                                                    0206 00049000
*         GPR1,                                                    0206 00050000
*         GPR14,                                                   0206 00051000
*         GPR15) RSTD;              /* RESTRICT REGS                 */ 00052000
*       ENTRYPTR=ADDR(RNCRNMDL);                                   0207 00053000
         LA    ENTRYPTR,RNCRNMDL(,RNCAREG)                         0207 00054000
*       RFY                                                        0208 00055000
*        (ISTRRN,                                                  0208 00056000
*         ISTRDT) BASED(ENTRYPTR);                                 0208 00057000
*       RFY                                                        0209 00058000
*         ISTRDT BASED(ENTRYPTR);                                  0209 00059000
*       GPR1=MLCKWD;                                               0210 00060000
         L     GPR1,MLCKWD(,MLCAREG)                               0210 00061000
*       RC=0;                                                      0211 00062000
         SLR   @10,@10                                             0211 00063000
         STH   @10,RC                                              0211 00064000
*       DO WHILE(RC=0);                                            0212 00065000
         B     @DE00212                                            0212 00066000
@DL00212 DS    0H                                                  0213 00067000
*         CALL DOWHILE;                                            0213 00068000
