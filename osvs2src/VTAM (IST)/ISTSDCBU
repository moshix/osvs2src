         TITLE 'BUILD MACRO HANDLER                                    *00001000
                        '                                               00002000
ISTSDCBU CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCBU  78.077'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*/********************************************************************/ 00014000
*/*                     START PROCESSING                             */ 00015000
*/********************************************************************/ 00016000
*                                                                  0198 00017000
*   RNCSAV1(2)=GPR13;               /* BACKWARD POINT                */ 00018000
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0198 00019000
*   GPR13=ADDR(RNCSAV1);            /* SET UP OUR SAVE REG           */ 00020000
         LA    GPR13,RNCSAV1(,RNCAREG)                             0199 00021000
*   MLCSAV3(3)=GPR13;               /* FORWARD POINT                 */ 00022000
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0200 00023000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */ 00024000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0201 00025000
*   GENERATE;                                                      0202 00026000
*                                                                  0202 00027000
           USING @DATD,11                 ADDRESSABILITY FOR DATA       00028000
*/********************************************************************/ 00029000
*/* IF PCCU FOOTPRINT SET AND BUILD FOOTPRINT NOT SET-THEN OK        */ 00030000
*/*    ELSE-KILL BUILD                                               */ 00031000
*/********************************************************************/ 00032000
*                                                                  0203 00033000
*   IF RNCGPCCU=ON&RNCGBLD=OFF THEN                                0203 00034000
         TM    RNCGPCCU(RNCAREG),B'10000000'                       0203 00035000
         BNO   @RF00203                                            0203 00036000
