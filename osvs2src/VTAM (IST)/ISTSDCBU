*/********************************************************************/ 00495000
*/* FROM THE SUBAREA VALUE SPECIFIED ON THE BUILD MACRO              */ 00496000
*/* SET UP THE X PORTION OR MNT INDEXING PORTION OF THE RPRCOMID     */ 00497000
*/* THE RPRDAF. ALSO SAVE THE VALUE SPECIFIED IN THE RDTNODID        */ 00498000
*/* FIELD.                                                           */ 00499000
*/********************************************************************/ 00500000
*                                                                  0343 00501000
*BLDSUBAR:                                                         0343 00502000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0343 00503000
BLDSUBAR DS    0H                                                  0344 00504000
*   RFY                                                            0344 00505000
*    (REGX,                                                        0344 00506000
*     XSHIFT) RSTD;                                                0344 00507000
*   REGX=PHRF7NUM(1);               /* PUT INPUT SUBAREA IN RDT AND  */ 00508000
         SLR   REGX,REGX                                           0345 00509000
         IC    REGX,PHRF7NUM(,PHR1REG)                             0345 00510000
*   RDTNODID=REGX;                  /* IN A REGISTER SO IT CAN BE    */ 00511000
         STC   REGX,RDTNODID(,ENTRYPTR)                            0346 00512000
*   XSHIFT=16-ATCXRANG;             /* SHIFTED INTO THE X-PART OF    */ 00513000
         LA    XSHIFT,16                                           0347 00514000
         SLR   @04,@04                                             0347 00515000
         IC    @04,ATCXRANG(,ATCVTPTR)                             0347 00516000
         SLR   XSHIFT,@04                                          0347 00517000
*   GEN REFS(XSHIFT,REGX) SETS(REGX);                              0348 00518000
     SLL REGX,0(XSHIFT)                                                 00519000
*   RPRDAF=REGX;                    /* THE DAF IN THE COM ID. THE    */ 00520000
         STH   REGX,RPRDAF(,ENTRYPTR)                              0349 00521000
*   RPRSAF=0;                       /* SAF MUST BE CLEARED.          */ 00522000
         SLR   @04,@04                                             0350 00523000
         STH   @04,RPRSAF(,ENTRYPTR)                               0350 00524000
*   GPR15=0;                        /* RETURN CODE-CONTINUE          */ 00525000
         LR    GPR15,@04                                           0351 00526000
*   RFY                                                            0352 00527000
*    (REGX,                                                        0352 00528000
*     XSHIFT) UNRSTD;                                              0352 00529000
*   END BLDSUBAR;                                                  0353 00530000
@EL00005 DS    0H                                                  0353 00531000
@EF00005 DS    0H                                                  0353 00532000
@ER00005 BR    @14                                                 0353 00533000
*                                                                  0354 00534000
*/********************************************************************/ 00535000
*/* THE FOLLOWING ROUTINE SETS UP THE RDTMAXID FIELD OF THE RDT      */ 00536000
*/* THIS IS DONE BY TAKING THE USER SUPPLIED VALUE FOR THE MAXSUBA   */ 00537000
*/* PARAMETER ON THE BUILD MACRO AND ROUNDING THE VALUE UP TO THE    */ 00538000
*/* NEXT POWER OF TWO MINUS ONE. WHAT THIS DOES IS TO TURN ON ALL    */ 00539000
*/* THE BITS OF THE RDTMAXID FIELD TO THE RIGHT OF AND INCLUDING THE */ 00540000
*/* HIGHEST BIT NEEDED TO EXPRESS THE SPECIFIED MAXSUBA.             */ 00541000
*/********************************************************************/ 00542000
*                                                                  0354 00543000
*BLDMAXSB:                                                         0354 00544000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0354 00545000
BLDMAXSB DS    0H                                                  0355 00546000
*   RFY                                                            0355 00547000
*    (XSHIFT,                                                      0355 00548000
*     REGX) RSTD;                                                  0355 00549000
*   XSHIFT=PHRF7NUM(1);                                            0356 00550000
         SLR   XSHIFT,XSHIFT                                       0356 00551000
         IC    XSHIFT,PHRF7NUM(,PHR1REG)                           0356 00552000
*   REGX=1;                                                        0357 00553000
*                                                                  0357 00554000
         LA    REGX,1                                              0357 00555000
*/********************************************************************/ 00556000
*/* THE FOLLOWING SLL SETS UP THE HIGHEST POWER OF TWO DETERMINED BY */ 00557000
*/* SRA WHICH WILL GO TO ZERO WHEN IT IS REACHED.                    */ 00558000
*/********************************************************************/ 00559000
*                                                                  0358 00560000
*MAXLBL:                                                           0358 00561000
*   GEN REFS(XSHIFT,REGX,MAXLBL) SETS(REGX,XSHIFT);                0358 00562000
MAXLBL   DS    0H                                                  0358 00563000
                  SLL REGX,1                                            00564000
                  SRA XSHIFT,1                                          00565000
                  BNZ MAXLBL                                            00566000
                  BCTR REGX,0                                           00567000
*   RDTMAXID=REGX;                                                 0359 00568000
         STC   REGX,RDTMAXID(,ENTRYPTR)                            0359 00569000
*   GPR15=0;                                                       0360 00570000
         SLR   GPR15,GPR15                                         0360 00571000
*   RFY                                                            0361 00572000
*    (XSHIFT,                                                      0361 00573000
*     REGX) UNRSTD;                                                0361 00574000
*   END BLDMAXSB;                                                  0362 00575000
@EL00006 DS    0H                                                  0362 00576000
@EF00006 DS    0H                                                  0362 00577000
@ER00006 BR    @14                                                 0362 00578000
*                                                                  0363 00579000
