         TITLE 'ISTSDCC2-ERROR FORMAT                                  *00001000
                        '                                               00002000
ISTSDCC2 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCC2  75.147'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0216 00014000
*/********************************************************************/ 00015000
*/*                 ENTRY LINKAGE                                    */ 00016000
*/********************************************************************/ 00017000
*                                                                  0216 00018000
*   DATDREG=ADDR(MLCERDAT);         /* SET UP DYNAMIC DATA REG       */ 00019000
         LA    DATDREG,MLCERDAT(,MLCAREG)                          0216 00020000
*   GENERATE NOREFS NOSETS;                                        0217 00021000
           USING @DATD,DATDREG          @ABILITY FOR DATA               00022000
*   MLCERSAV(2)=SAVREG;             /* SET UP SAVE AREA POINTERS BY  */ 00023000
         ST    SAVREG,MLCERSAV+4(,MLCAREG)                         0218 00024000
*   SVLSA=ADDR(MLCERSAV);           /* SYSDEF CONVENTIONS            */ 00025000
         LA    @10,MLCERSAV(,MLCAREG)                              0219 00026000
         ST    @10,SVLSA(,SAVREG)                                  0219 00027000
*   SAVREG=ADDR(MLCERSAV);                                         0220 00028000
         LR    SAVREG,@10                                          0220 00029000
*   PARMSAVE=GPR1;                  /* SAVE INPUT PARAMETER POINTER  */ 00030000
         LR    PARMSAVE,GPR1                                       0221 00031000
*/********************************************************************/ 00032000
*/* THE FIRST MESSAGE TO BE SENT IS EITHER IST320I, IST321I, OR      */ 00033000
*/* IST322I.  THE FIRST 8 BYTES (MLCCNFG2) OF THE BUFFER (MLCERBF3)  */ 00034000
*/* HAVE BEEN ALREADY FILLED IN BY MAIN-LINE (ISTSDCRC).  THE NEXT   */ 00035000
*/* 24 CHARACTERS (MLCFII) MUST BE FILLED IN BY THIS MODULE PRIOR    */ 00036000
*/* TO ISSUANCE OF TPMSG.  THE BASIC ERROR TYPE IS INDICATED BY THE  */ 00037000
*/* INPUT VALUE 'CERR', WHICH IS AN INDEX INTO TABLE 'ERRTAB', FOR   */ 00038000
*/* THOSE ENTRIES HAVING SECONDARY ERROR LEVELS, THE INPUT VALUE     */ 00039000
*/* 'CLEV' IS USED AS AN INDEX INTO TABLE 'LEVTAB'.  CERR ALSO IS    */ 00040000
*/* USED TO INDEX INTO TABLE 'ERRLEN' WHICH CONTAINS THE APPROPRIATE */ 00041000
*/* OFFSET INTO THE BUFFER FOR THE LEVELS OF A GIVEN ERROR.          */ 00042000
*/********************************************************************/ 00043000
*                                                                  0222 00044000
*FILLBUF:                                                          0222 00045000
*   MLCFIII=ERRTAB(CERR);           /* FILL IN MAJOR ERROR TYPE      */ 00046000
FILLBUF  MVI   MLCFIII+21(MLCAREG),C' '                            0222 00047000
         MVC   MLCFIII+22(2,MLCAREG),MLCFIII+21(MLCAREG)           0222 00048000
         SLR   @10,@10                                             0222 00049000
         IC    @10,CERR(,GPR1)                                     0222 00050000
         LR    @06,@10                                             0222 00051000
         MH    @06,@CH01851                                        0222 00052000
         LA    @04,ERRTAB-21(@06)                                  0222 00053000
         MVC   MLCFIII(21,MLCAREG),0(@04)                          0222 00054000
*   IF ERRLEN(CERR)=0               /* LEVEL TO GO IN MESSAGE?       */ 00055000
*     THEN                                                         0223 00056000
         ALR   @10,@10                                             0223 00057000
         LH    @10,ERRLEN-2(@10)                                   0223 00058000
         LTR   @10,@10                                             0223 00059000
         BZ    @RT00223                                            0223 00060000
*     ;                             /* NO-BUFFER IS READY            */ 00061000
*   ELSE                            /* YES-FILL IN REST OF BUFFER    */ 00062000
*     MLCFIII(ERRLEN(CERR):         /* LOCATE THE BEGINNING OF       */ 00063000
*         LENGTH(MLCFIII))          /* INTERNAL FIELD. USING REST    */ 00064000
*         =LEVTAB(CLEV,             /* OF FIELD FOR LENGTH, FILL     */ 00065000
*         1:LENGTH(LEVTAB));        /* IN ERROR LEVEL                */ 00066000
         SLR   @10,@10                                             0225 00067000
         IC    @10,CERR(,GPR1)                                     0225 00068000
         ALR   @10,@10                                             0225 00069000
         LH    @10,ERRLEN-2(@10)                                   0225 00070000
         ST    @10,@TF00001                                        0225 00071000
         ALR   @10,MLCAREG                                         0225 00072000
         LA    @06,24                                              0225 00073000
         SL    @06,@TF00001                                        0225 00074000
         SLR   @04,@04                                             0225 00075000
         IC    @04,CLEV(,GPR1)                                     0225 00076000
         MH    @04,@CH01869                                        0225 00077000
         LA    @15,LEVTAB-17(@04)                                  0225 00078000
         EX    @06,@SM01953                                        0225 00079000
*SETMSG:                            /* FIND MSG ID FOR INPUT ACTION  */ 00080000
*   IF CACT='6' THEN                /* 320I FOR ACTION 6             */ 00081000
@RT00223 DS    0H                                                  0226 00082000
SETMSG   CLI   CACT(GPR1),C'6'                                     0226 00083000
         BNE   @RF00226                                            0226 00084000
*     MSGNO=ADDR('320I');                                          0227 00085000
         LA    MSGNO,@CC01813                                      0227 00086000
*   ELSE                                                           0228 00087000
*     IF CACT='1' THEN              /* 322I FOR ACTION 1             */ 00088000
         B     @RC00226                                            0228 00089000
@RF00226 CLI   CACT(GPR1),C'1'                                     0228 00090000
         BNE   @RF00228                                            0228 00091000
*       MSGNO=ADDR('322I');                                        0229 00092000
         LA    MSGNO,@CC01815                                      0229 00093000
*     ELSE                          /* 321I OTHERWISE                */ 00094000
*       MSGNO=ADDR('321I');                                        0230 00095000
*                                                                  0230 00096000
         B     @RC00228                                            0230 00097000
@RF00228 LA    MSGNO,@CC01816                                      0230 00098000
*/********************************************************************/ 00099000
*/* MESSAGE MAY NOW BE SENT                                          */ 00100000
*/********************************************************************/ 00101000
*/*TPMSG MSGID((MSGNO))MSG((ADDR(MLCERBF3)))OPTIONS(NCSPLSID)PLAREA(MLC 00102000
*RK4)                                                                */ 00103000
*                                                                  0231 00104000
*   DO;                                                            0231 00105000
@RC00228 DS    0H                                                  0231 00106000
@RC00226 DS    0H                                                  0232 00107000
*     DCL                                                          0232 00108000
*       REG1004 REG(1) PTR;                                        0232 00109000
*     RFY                                                          0233 00110000
*      (REG1004) RSTD;                                             0233 00111000
*     RFY                                                          0234 00112000
*      (EMCR13) RSTD;                                              0234 00113000
*     DCL                                                          0235 00114000
*       1 * BASED(REG1004),                                        0235 00115000
*        2 CMZID004 PTR(31),                                       0235 00116000
*        2 *,                                                      0235 00117000
*         3 CMZRL004 PTR(8),                                       0235 00118000
*         3 CMZRA004 PTR(24) BDY(BYTE),                            0235 00119000
*        2 CMZEA004 PTR(31),                                       0235 00120000
*        2 CMZVP004 PTR(31);                                       0235 00121000
*                                                                  0235 00122000
*/*XREGSET  PID('PLAREA')PPARM(MLCWRK4)RREG(,REG1004)                */ 00123000
*                                                                  0236 00124000
*     DO;                                                          0236 00125000
*       REG1004=ADDR(MLCWRK4);      /* SET RREG SYMBOL TO ADDRESS OF    00126000
*                                      PPARM OPERAND                 */ 00127000
         LA    REG1004,MLCWRK4(,MLCAREG)                           0237 00128000
*     END;                          /* XREGSET                       */ 00129000
*     CMZID004=0;                                                  0239 00130000
         SLR   @10,@10                                             0239 00131000
         ST    @10,CMZID004(,REG1004)                              0239 00132000
*     CMZRL004=0;                                                  0240 00133000
         MVI   CMZRL004(REG1004),X'00'                             0240 00134000
*     CMZRA004=0;                                                  0241 00135000
