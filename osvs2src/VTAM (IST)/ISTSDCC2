         LA    @10,MLCERBF3(,MLCAREG)                              0245 00144000
         ST    @10,CMZVP004(,REG1004)                              0245 00145000
*     DCL                                                          0246 00146000
*       CMZSA004(18) PTR(31) BASED(EMCR13);                        0246 00147000
*     CMZSA004(5)=ADDR(NCSPLSID);                                  0247 00148000
         L     @10,MLCNCSPL(,MLCAREG)                              0247 00149000
         LA    @10,NCSPLSID(,@10)                                  0247 00150000
         ST    @10,CMZSA004+16(,EMCR13)                            0247 00151000
*     DCL                                                          0248 00152000
*       REG15004 REG(15) PTR;                                      0248 00153000
*     RFY                                                          0249 00154000
*       REG15004 RSTD;                                             0249 00155000
*     DO;                           /* LISATCVT                    0250 00156000
*                                      CVTBASE(PAGEABLE,(REG15004))  */ 00157000
*       REG15004=ATCLCPTR;          /* SET UP ATCVT POINTER          */ 00158000
         L     REG15004,ATCLCPTR                                   0251 00159000
*     END;                          /* LISATCVT                      */ 00160000
*     DCL                                                          0253 00161000
*       CMTA004 BASED(REG15004) ENTRY VALUERANGE(*);               0253 00162000
*     REG15004=REG15004->ATCMSGP;                                  0254 00163000
         L     REG15004,ATCMSGP(,REG15004)                         0254 00164000
*     CALL CMTA004;                                                0255 00165000
         BALR  @14,@15                                             0255 00166000
*     RESPECIFY                                                    0256 00167000
*      (REG1004,                                                   0256 00168000
*       REG15004) UNRESTRICTED;                                    0256 00169000
*     RFY                                                          0257 00170000
*      (EMCR13) UNRSTD;                                            0257 00171000
*   END;                                                           0258 00172000
*                                                                  0258 00173000
*/********************************************************************/ 00174000
*/* THE TYPE OF ERROR HAVING BEEN INDICATED BY THE ABOVE MESSAGE, AN */ 00175000
*/* ADDITIONAL MESSAGE, IST323I, IS SENT TO SHOW THE INPUT VALUES.   */ 00176000
*/* THE FIRST 8 BYTES (MLCNAME) AND THE SECOND 8 BYTES (MLCMACRO) OF */ 00177000
*/* THE BUFFER (MLCERBF2) HAVE ALREADY BEEN FILLED BY MHR.  THE      */ 00178000
*/* KEYWORD (MLCFII) MUST BE FILLED IN NOW.                          */ 00179000
*/********************************************************************/ 00180000
*                                                                  0259 00181000
*   GPR1=PARMSAVE;                  /* RESTORE PARAMETER POINTER     */ 00182000
         LR    GPR1,PARMSAVE                                       0259 00183000
*   MLCFII=BLANK;                   /* BLANK OUT KEYWORD FIELD       */ 00184000
         MVI   MLCFII+1(MLCAREG),C' '                              0260 00185000
         MVC   MLCFII+2(6,MLCAREG),MLCFII+1(MLCAREG)               0260 00186000
         MVI   MLCFII(MLCAREG),C' '                                0260 00187000
*   IF CLEV<4                       /* WAS ERROR IN NAME OR MACRO?   */ 00188000
*     THEN                                                         0261 00189000
         CLI   CLEV(GPR1),4                                        0261 00190000
         BL    @RT00261                                            0261 00191000
*     ;                             /* YES-KEYWORD IS IRREVELANT     */ 00192000
*   ELSE                            /* NO-SET UP KEYWORD             */ 00193000
*USEKEYWD:                                                         0263 00194000
*     DO;                                                          0263 00195000
USEKEYWD DS    0H                                                  0264 00196000
*       IF LSTR>0                   /* WAS KEYWORD SUPPLIED?         */ 00197000
*         THEN                                                     0264 00198000
         CLI   LSTR(GPR1),0                                        0264 00199000
         BNH   @RF00264                                            0264 00200000
*         MLCFII(1:LSTR)=           /* YES-USE IT.                   */ 00201000
*             TEXTSTRG(1:LSTR);                                    0265 00202000
         SLR   @10,@10                                             0265 00203000
         IC    @10,LSTR(,GPR1)                                     0265 00204000
         BCTR  @10,0                                               0265 00205000
         EX    @10,@SM01955                                        0265 00206000
*       ELSE                        /* NO-LOOK FOR KEYWORD           */ 00207000
*SCAN:                                                             0266 00208000
*         DO;                                                      0266 00209000
         B     @RC00264                                            0266 00210000
@RF00264 DS    0H                                                  0266 00211000
SCAN     DS    0H                                                  0267 00212000
*           DO I=1 TO 9 WHILE       /* INDEX TO END OF KEYWORD       */ 00213000
*                 (C(I)^=' '&C(I)^=','&C(I)^='=');                 0267 00214000
         LA    I,1                                                 0267 00215000
@DL00267 L     @10,MLCKWD(,MLCAREG)                                0267 00216000
         LR    @04,@10                                             0267 00217000
         ALR   @04,I                                               0267 00218000
         AL    @04,@CF01957                                        0267 00219000
         CLI   C(@04),C' '                                         0267 00220000
         BE    @DC00267                                            0267 00221000
         LR    @04,@10                                             0267 00222000
         ALR   @04,I                                               0267 00223000
         AL    @04,@CF01957                                        0267 00224000
         CLI   C(@04),C','                                         0267 00225000
         BE    @DC00267                                            0267 00226000
         ALR   @10,I                                               0267 00227000
         AL    @10,@CF01957                                        0267 00228000
         CLI   C(@10),C'='                                         0267 00229000
         BE    @DC00267                                            0267 00230000
*           END;                                                   0268 00231000
         AL    I,@CF00061                                          0268 00232000
         C     I,@CF01833                                          0268 00233000
         BNH   @DL00267                                            0268 00234000
@DC00267 DS    0H                                                  0269 00235000
*           IF I^=1 THEN                                           0269 00236000
         C     I,@CF00061                                          0269 00237000
         BE    @RF00269                                            0269 00238000
*             I=I-1;                /* LENGTH OF KEYWORD             */ 00239000
         BCTR  I,0                                                 0270 00240000
*           ELSE                                                   0271 00241000
*             ;                     /* IF NONE USE DELIMETER         */ 00242000
@RF00269 DS    0H                                                  0272 00243000
*           MLCFII(1:I)=C(1:I);     /* PUT KEYWORD INTO BUFFER       */ 00244000
         LR    @10,I                                               0272 00245000
         BCTR  @10,0                                               0272 00246000
         L     @04,MLCKWD(,MLCAREG)                                0272 00247000
         EX    @10,@SM01958                                        0272 00248000
*         END SCAN;                                                0273 00249000
*     END USEKEYWD;                                                0274 00250000
*                                                                  0274 00251000
@RC00264 DS    0H                                                  0275 00252000
*/********************************************************************/ 00253000
*/* BEFORE PRINTING THE MESSAGE ANY UNPRINTABLES MUST BE CHANGED TO  */ 00254000
*/* DOTS                                                             */ 00255000
*/********************************************************************/ 00256000
*                                                                  0275 00257000
*   TR(MLCFII,TTABLE);              /* CONVERT UNPRINTABLES          */ 00258000
@RT00261 TR    MLCFII(8,MLCAREG),TTABLE                            0275 00259000
*/*TPMSG MSGID('323I')MSG((ADDR(MLCERBF2)))OPTIONS(NCSPLSID)PLAREA(MLCW 00260000
