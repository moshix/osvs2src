@EF00001 DS    0H                                                  0306 00336000
@ER00001 LM    @14,@12,12(@13)                                     0306 00337000
         BR    @14                                                 0306 00338000
*                                                                  0307 00339000
*/********************************************************************/ 00340000
*/*                 TABLES FOR ERROR MESSAGES                        */ 00341000
*/********************************************************************/ 00342000
*                                                                  0307 00343000
*   DCL                                                            0307 00344000
*     ERRTAB(8) CHAR(21) STATIC LOCAL/* INDEXED BY CERR              */ 00345000
*         INIT('COMBINATION ERROR    ','PARAMETER CONFLICT   ',/*  0307 00346000
*                                                            @XM09189*/ 00347000
*         'DUPLICATE            ','INVALID              ',         0307 00348000
*         'SEQUENCE ERROR       ','SYNTAX ERROR         ',         0307 00349000
*         'EXTRA                ','MISSING              ');/*      0307 00350000
*                                                            @DM12564*/ 00351000
*   DCL                                                            0308 00352000
*     ERRLEN(8) FIXED(15) STATIC LOCAL/* INDEXED BY CERR             */ 00353000
*         INIT(0,0,11,9,0,0,7,9);                                  0308 00354000
*                                                                  0308 00355000
*/********************************************************************/ 00356000
*/* NOTE: BECAUSE THE COMPILER ASSUMES VARIABLE LENGTH SUBSTRINGS TO */ 00357000
*/* BE OF EQUAL LENGTH EVEN IF THEY ARE NOT, THE ELEMENT LENGTH OF   */ 00358000
*/* THIS ARRAY MUST BE LARGE ENOUGH TO PAD BLANKS TO THE RIGHT OF    */ 00359000
*/* MLCFIII IN THE WORST POSSIBLE CASE ('EXTRA' XXXX...XX').  IF THE */ 00360000
*/* COMPILER CHANGES TO HANDLE VARIABLE LENGTH SUBSTRINGS AS IT DOES */ 00361000
*/* FIXED LENGTH SUBSTRINGS (ALLOWS FOR DIFFERENT LENGTHS), THE      */ 00362000
*/* ELEMENT LENGTH CAN BE REDUCED FROM 17 (24-7) TO 9 (LENGTH OF     */ 00363000
*/* 'PARAMETER').                                                    */ 00364000
*/********************************************************************/ 00365000
*                                                                  0309 00366000
*   DCL                                                            0309 00367000
*     LEVTAB(5) CHAR(17) STATIC LOCAL/* INDEXED BY CLEV              */ 00368000
*         INIT('         ','NAME     ','MACRO    ','PARAMETER',    0309 00369000
*         'VALUE    ');                                            0309 00370000
*                                                                  0310 00371000
*/********************************************************************/ 00372000
*/*             TRANSLATE TABLE FOR MESSAGES                         */ 00373000
*/********************************************************************/ 00374000
*                                                                  0310 00375000
*   DCL                                                            0310 00376000
*     1 TTABLE CHAR(256) STATIC LOCAL,                             0310 00377000
*      2 TTABLE1(256) CHAR(1) INIT((64)'.',' ',(10)'.','.','<','(','+', 00378000
*            '|','&',(10)'.','$','*',')',';','^','-','/',(9)'.',',','%' 00379000
*            ,'_','>','?',(10)'.',':','#',                         0310 00380000
*'@',                                                              0310 00381000
*            '''','=',(66)'.','A','B','C','D','E','F','G','H','I',(7)'. 00382000
*            ,'J','K','L','M','N','O','P','Q','R',(8)'.','S','T','U','V 00383000
*            ,'W','X','Y','Z',(6)'.','0','1','2','3','4','5','6','7','8 00384000
*            ,'9',(6)'.');                                         0310 00385000
*                                                                  0310 00386000
*/* TPEPILOG                                                         */ 00387000
*                                                                  0311 00388000
*   GEN(PRINT OFF);                                                0311 00389000
         PRINT OFF                                                      00390000
*   DECLARE                                                        0312 00391000
*     EMCR0 PTR REG(0),                                            0312 00392000
*     EMCR1 PTR REG(1),                                            0312 00393000
*     EMCR2 PTR REG(2),                                            0312 00394000
*     EMCR3 PTR REG(3),                                            0312 00395000
*     EMCR4 PTR REG(4),                                            0312 00396000
*     EMCR5 PTR REG(5),                                            0312 00397000
*     EMCR6 PTR REG(6),                                            0312 00398000
*     EMCR7 PTR REG(7),                                            0312 00399000
*     EMCR8 PTR REG(8),                                            0312 00400000
*     EMCR9 PTR REG(9),                                            0312 00401000
*     EMCR10 PTR REG(10),                                          0312 00402000
*     EMCR11 PTR REG(11),                                          0312 00403000
