         TITLE 'ISTSDCC4-EBCHEX                                        *00001000
                        '                                               00002000
ISTSDCC4 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCC4  75.130'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         USING @DATD,DATREG                                             00014000
*   DATREG=ADDR(RNCDAT3);           /* RNCDAT3 TEMPORARY MUST BE   0171 00015000
*                                      CHANGED BEFORE RELEASE        */ 00016000
         LA    DATREG,RNCDAT3(,RNCAREG)                            0171 00017000
*   WORKBUF=BLANK;                  /* BLANK THE WORKBUF             */ 00018000
         MVI   WORKBUF+1,C' '                                      0172 00019000
         MVC   WORKBUF+2(39),WORKBUF+1                             0172 00020000
         MVI   WORKBUF,C' '                                        0172 00021000
*   IF R0>0&R0<41 THEN              /* IS STRING LENGTH >0 & <41     */ 00022000
         SLR   @14,@14                                             0173 00023000
         CR    R0,@14                                              0173 00024000
         BNH   @RF00173                                            0173 00025000
         CH    R0,@CH00723                                         0173 00026000
         BNL   @RF00173                                            0173 00027000
*     DO;                                                          0174 00028000
*       RLENGTH=R0;                 /* YES PUT LENGTH IN RLENGTH     */ 00029000
         LR    RLENGTH,R0                                          0175 00030000
*       TOPTR=ADDR(WORKBUF)+40-RLENGTH;/* SET TO PTR FOR MOVE        */ 00031000
         LA    TOPTR,WORKBUF                                       0176 00032000
         AH    TOPTR,@CH01167                                      0176 00033000
         SLR   TOPTR,RLENGTH                                       0176 00034000
*       WRKCHR(1:RLENGTH)=STRING(1:RLENGTH);/* MOVE STRING TO WRKBF  */ 00035000
         LR    @10,RLENGTH                                         0177 00036000
         BCTR  @10,0                                               0177 00037000
         EX    @10,@SM01171                                        0177 00038000
*       PARMSAVE=PARMR;             /* SAVE REG 1 OVER TRT           */ 00039000
         LR    PARMSAVE,PARMR                                      0178 00040000
*       PARMR=0;                    /* ZERO OUT REG 1                */ 00041000
         LR    PARMR,@14                                           0179 00042000
*       TRT(WORKBUF,TTABLE);        /* CHECK VALIDITY                */ 00043000
         TRT   WORKBUF(41),TTABLE                                  0180 00044000
*       IF PARMR=0 THEN             /* TRT WENT O.K.?                */ 00045000
         SLR   @14,@14                                             0181 00046000
         CR    PARMR,@14                                           0181 00047000
         BNE   @RF00181                                            0181 00048000
*         DO;                       /* YES, GO ON                    */ 00049000
*           TR(WORKBUF,TTABLE2);    /* TRANSLATE CHARACTERS          */ 00050000
         TR    WORKBUF(41),TTABLE2                                 0183 00051000
*           PACK(WRK3,STR3);        /* PACK STRING                   */ 00052000
         PACK  WRK3(8),STR3(15)                                    0184 00053000
*           PACK(WRK2,STR2);                                       0185 00054000
         PACK  WRK2(8),STR2(15)                                    0185 00055000
*           PACK(WRK1,STR1);                                       0186 00056000
         PACK  WRK1(7),STR1(13)                                    0186 00057000
*           SLIDE2=WRK2(1:7);       /* MOVE PACKED STRING TOGETHER   */ 00058000
         MVC   SLIDE2(7),WRK2                                      0187 00059000
*           SLIDE1=WRK1(1:6);                                      0188 00060000
         MVC   SLIDE1(6),WRK1                                      0188 00061000
*           RLENGTH=(RLENGTH+1)/2;  /* GET LENGTH OF PACKED STRING   */ 00062000
         LA    @02,1                                               0189 00063000
         ALR   @02,RLENGTH                                         0189 00064000
         SRDA  @02,32                                              0189 00065000
         D     @02,@CF00174                                        0189 00066000
         LR    RLENGTH,@03                                         0189 00067000
*           FROMPTR=ADDR(WORKBUF)+40-RLENGTH;/* SET PTR FOR MOVE     */ 00068000
         LA    FROMPTR,WORKBUF                                     0190 00069000
         AH    FROMPTR,@CH01167                                    0190 00070000
         SLR   FROMPTR,RLENGTH                                     0190 00071000
*           PARMR=PARMSAVE;         /* RESTORE REG 1                 */ 00072000
         LR    PARMR,PARMSAVE                                      0191 00073000
*           STRING(1:RLENGTH)=HEXSTRNG(1:RLENGTH);/* MOVE IN STRG    */ 00074000
         LR    @10,RLENGTH                                         0192 00075000
         BCTR  @10,0                                               0192 00076000
         EX    @10,@SM01173                                        0192 00077000
*           R0=RLENGTH;             /* SET R0 FOR RETURN             */ 00078000
         LR    R0,RLENGTH                                          0193 00079000
*           RC=0;                   /* SET RETURN CODE               */ 00080000
         LR    RC,@14                                              0194 00081000
*         END;                                                     0195 00082000
*       ELSE                        /* PARMR NOT = 0                 */ 00083000
*         DO;                                                      0196 00084000
         B     @RC00181                                            0196 00085000
@RF00181 DS    0H                                                  0197 00086000
*           RC=4;                   /* NOTE ERROR CONDITION          */ 00087000
         LA    RC,4                                                0197 00088000
*         END;                                                     0198 00089000
*     END;                                                         0199 00090000
*   ELSE                            /* STRNG LENGTH 0 OR > 41        */ 00091000
*     DO;                                                          0200 00092000
         B     @RC00173                                            0200 00093000
@RF00173 DS    0H                                                  0201 00094000
*       RC=4;                       /* ERROR CONDITION               */ 00095000
         LA    RC,4                                                0201 00096000
*     END;                                                         0202 00097000
*   SVR0=R0;                        /* PUT R0 IN CALLERS SAVEAREA    */ 00098000
@RC00173 ST    R0,SVR0(,SAVREG)                                    0203 00099000
*   SVR15=RC;                       /* PUT RC IN CALLERS SAVEAREA    */ 00100000
         ST    RC,SVR15(,SAVREG)                                   0204 00101000
*   RETURN;                                                        0205 00102000
*                                                                  0205 00103000
@EL00001 DS    0H                                                  0205 00104000
@EF00001 DS    0H                                                  0205 00105000
@ER00001 LM    @14,@12,12(@13)                                     0205 00106000
         BR    @14                                                 0205 00107000
*/* TPEPILOG                                                         */ 00108000
*                                                                  0206 00109000
*   GEN(PRINT OFF);                                                0206 00110000
         PRINT OFF                                                      00111000
*   GEN(PRINT ON);                                                 0207 00112000
*                                                                  0207 00113000
         PRINT ON                                                       00114000
*/* END OF TPEPILOG                                                  */ 00115000
*                                                                  0208 00116000
*   END ISTSDCC4;                                                  0208 00117000
         B     @EL00001                                            0208 00118000
@DATA    DS    0H                                                       00119000
@CH01167 DC    H'40'                                                    00120000
@CH00723 DC    H'41'                                                    00121000
@SM01171 MVC   WRKCHR(0,TOPTR),STRING(PARMR)                            00122000
