*          RFY REG15033 RSTD;                                   02S0222 00375000
*                                                               02S0222 00376000
*        DO; /* LISATCVT CVTBASE(PAGEABLE,(REG15033)) */        02S0223 00377000
*          REG15033 = ATCLCPTR; /*SET UP ATCVT POINTER*/        02S0224 00378000
         L     REG15033,ATCLCPTR                                02S0224 00379000
*        END; /*LISATCVT*/                                      02S0225 00380000
*          DCL CMTA033 BASED(REG15033) ENTRY VALUERANGE(*);     02S0226 00381000
*          REG15033 = REG15033->ATCMSGP;                        02S0227 00382000
         L     REG15033,ATCMSGP(,REG15033)                      02S0227 00383000
*          CALL CMTA033;                                        02S0228 00384000
         BALR  @14,@15                                          02S0228 00385000
