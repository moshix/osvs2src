*        END;                         /*@D32CKQH*/              02S0231 00389000
*                                                          /*@XM10670*/ 00390000
*       GPR15F = 8;                                        /*@XM10670*/ 00391000
         LA    GPR15F,8                                         02S0232 00392000
*     END;                                                 /*@XM10670*/ 00393000
*   ELSE                                                        02S0234 00394000
*/* GET EXCLUSIVE CONTROL OF THE VOCLOCK                             */ 00395000
*     DO;                                                       02S0234 00396000
         B     @RC00202                                         02S0234 00397000
@RF00202 DS    0H                                               02S0235 00398000
*/********************************************************************/ 00399000
*/*                                                                  */ 00400000
*/* SEGMENT NAME: GETLOCK                                            */ 00401000
*/*                                                                  */ 00402000
*/* FUNCTION: TPLOCK IS USED TO OBTAIN EXCLUSIVE CONTROL OF THE      */ 00403000
*/* VOCLOCK (VARY/OPEN/CLOSE) GEFORE SRTADDS CAN BE DONE. THE LOCK   */ 00404000
*/* IS HELD UNTIL VARY RELEASES IT BECAUSE VARY MUST DO SOME SNT     */ 00405000
*/* PROCESSING BEFOR OPEN/CLOSE GETS TO IT.                          */ 00406000
*/*                                                                  */ 00407000
*/* INPUT: NONE                                                      */ 00408000
*/*                                                                  */ 00409000
*/* OUTPUT: VOCLOCK IS HELD BY THIS TASK OR SUBTASK                  */ 00410000
*/*                                                                  */ 00411000
*/* ROUTINES CALLED: NONE                                            */ 00412000
*/*                                                                  */ 00413000
*/* MACRO USED: TPLOCK                                               */ 00414000
*/*                                                                  */ 00415000
*/********************************************************************/ 00416000
*RFY RPHPTR RSTD;                                               03S0235 00417000
*   RPHPTR=MLCRPH;                                              03S0236 00418000
*                                                               03S0236 00419000
         L     RPHPTR,MLCRPH(,MLCAPTR)                          03S0236 00420000
*/*** START -TPLOCK (VOCLOCK)TYPE(XCLSIVE)- ***/                03S0237 00421000
*    DO;                                                        03S0237 00422000
*    RFY ISTLCCW ;                                              03S0238 00423000
*    RFY ISTLCPB ;                                              03S0239 00424000
*    RFY ISTZSAVE;                                              03S0240 00425000
*    RFY ISTPFCRR;                                              03S0241 00426000
*    RFY ISTCFCRR;                                              03S0242 00427000
*    RFY ISTZCRR ;                                              03S0243 00428000
*    RFY ISTASCRR;                                              03S0244 00429000
*    RFY ISTAPCRR;                                              03S0245 00430000
*    RFY ISTCRA  ;                                              03S0246 00431000
*                                                               03S0246 00432000
*    RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P,GPR15P) RSTD;      03S0247 00433000
*        GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(RPHWORK);     00434000
         STM GPR00P,GPR14P,RPHWORK(GPR01P)                              00435000
*        GPR15P = ATCAP36;              /*ADDRESS OF LOCK ROUTINE*/     00436000
         L     GPR15P,ATCAP36(,ATCVTPTR)                        03S0249 00437000
