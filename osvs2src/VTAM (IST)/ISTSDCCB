*IF RPRENTRY = RPRENTPX |  /* IS THIS AN LUX OR PUX          @X50CKZA*/ 01537000
*   RPRENTRY = RPRENTLX                                    /*@X50CKZA*/ 01538000
*  THEN SNTDUMFG = '1'B;  /* YES  SET DUMMY ENTRY FLAG       @X50CKZA*/ 01539000
         CLI   RPRENTRY(RDTEPTR),X'72'                          02S0564 01540000
         BE    @RT00564                                         02S0564 01541000
         CLI   RPRENTRY(RDTEPTR),X'10'                          02S0564 01542000
         BNE   @RF00564                                         02S0564 01543000
@RT00564 DS    0H                                               02S0565 01544000
         OI    SNTDUMFG(SNTEPTR),B'00100000'                    02S0565 01545000
*  ELSE;                                                        02S0566 01546000
@RF00564 DS    0H                                               02S0567 01547000
*RFY GPR14P UNRSTD;                                             02S0567 01548000
*RFY ISTSNT;                                                    02S0568 01549000
*END SNTSUB;                                                    02S0569 01550000
