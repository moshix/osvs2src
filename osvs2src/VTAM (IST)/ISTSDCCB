*/********************************************************************/ 01810000
*/*                                                                  */ 01811000
*/* ROUTINE NAME: ERRSUB1                                            */ 01812000
*/*                                                                  */ 01813000
*/* FUNCTION: CALLED BY MAINLINE AFTER AN UNRECOVERABLE ERROR WHILE  */ 01814000
*/*             (1)SPACE FOR THE SNT HAS BEEN OBTAINED.              */ 01815000
*/*             (2)THE VOCLOCK IS BEING HELD.                        */ 01816000
*/*             (3)ENTRIES MAY HAVE BEEN MADE TO THE SRT.            */ 01817000
*/*           ERRSUB1 UNDOES THESE CONDITIONS BY CALLING THE MACROS  */ 01818000
*/*           AND ROUTINES BELOW.                                    */ 01819000
*/*                                                                  */ 01820000
*/* INPUT: TPMSGID - ADDR OF ADDR OF ID OF MESSAGE TO BE ISSUED      */ 01821000
*/*                                                                  */ 01822000
*/* OUTPUT: RETURN CODE PASSED FROM ERRSUB2                          */ 01823000
*/*                                                                  */ 01824000
*/* ROUTINES CALLED: ISTSDCD1 - REMOVE ANY SRT ENTRIES               */ 01825000
*/*                  ISTSDCD2 - DELETE THE SNT                       */ 01826000
*/*                  ERRSUB2 - DO ERROR PROCESSING WHICH IS          */ 01827000
*/*                            INDEPENDENT OF WHEN ERROR OCCURRED    */ 01828000
*/*                                                                  */ 01829000
*/* MACRO USED: TPUNLOCK - RELEASE THE VOCLOCK                       */ 01830000
*/*                                                                  */ 01831000
*/********************************************************************/ 01832000
*ERRSUB1: PROC OPTIONS(NOSAVE,NOSAVEAREA);                      02S0665 01833000
ERRSUB1  DS    0H                                               02S0666 01834000
*DCL ERRSAV1 PTR;                  /* TO SAVE RETURN ADDRESS         */ 01835000
*RFY (RPHPTR,ATCVTPTR,CONFTPTR,GPR14P) RSTD;                    02S0667 01836000
*   ERRSAV1=GPR14P;                                             02S0668 01837000
         LR    ERRSAV1,GPR14P                                   02S0668 01838000
*   CONFTPTR=ATCCONFT;                                          02S0669 01839000
         L     CONFTPTR,ATCCONFT(,ATCVTPTR)                     02S0669 01840000
*   CALL ISTSDCD1;                                              02S0670 01841000
         L     @15,@CV00788                                     02S0670 01842000
         BALR  @14,@15                                          02S0670 01843000
*   IF RDTNODID^=ZERO THEN /* SNT NEEDS DELETING             @ZA30391*/ 01844000
         L     @15,MLCSKEL(,MLCAPTR)                            02S0671 01845000
         CLI   RDTNODID(@15),0                                  02S0671 01846000
         BE    @RF00671                                         02S0671 01847000
*   CALL ISTSDCD2;                                              02S0672 01848000
         L     @15,@CV00789                                     02S0672 01849000
         BALR  @14,@15                                          02S0672 01850000
*   RPHPTR=MLCRPH;                                              02S0673 01851000
*                                                               02S0673 01852000
@RF00671 L     RPHPTR,MLCRPH(,MLCAPTR)                          02S0673 01853000
*/*** START -TPUNLOCK (VOCLOCK)- ***/                           02S0674 01854000
*    DO;                                                        02S0674 01855000
*/* ATBUILD (ISTRPH,ISTLOK,ISTCRA)  */                          02S0675 01856000
*/* END OF ATBUILD */                                           02S0675 01857000
*                                                               02S0675 01858000
*    RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P,GPR15P) RSTD;      02S0675 01859000
*        GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(RPHWORK);     01860000
         STM GPR00P,GPR15P,RPHWORK(GPR01P)                              01861000
*        GPR02P = ADDR(ATCVOCLK);       /*SET PARAM - LOCKWORD PTR*/    01862000
         LA    GPR02P,ATCVOCLK(,ATCVTPTR)                       02S0677 01863000
*        GPR04P = GPR01P->RPHCRR;       /*SET PARAM - ISTCRA PTR*/      01864000
         L     GPR04P,RPHCRR(,GPR01P)                           02S0678 01865000
*        GPR00P = CRALMS02;             /*SET PARAM - LOCK LEVEL FLAG*/ 01866000
         LA    GPR00P,2                                         02S0679 01867000
*        GPR03P = ADDR(GPR04P->CRAL2PTR);                       02S0680 01868000
         LA    GPR03P,CRAL2PTR(,GPR04P)                         02S0680 01869000
*                                       /*SET PARAM - LOCK LEVEL PTR*/  01870000
*                                                               02S0681 01871000
*    DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */              02S0681 01872000
*      GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/              02S0682 01873000
         L     GPR15P,ATCLCPTR                                  02S0682 01874000
*    END; /*LISATCVT*/                                          02S0683 01875000
*        GPR15P=GPR15P->ATCAP33;                                02S0684 01876000
         L     GPR15P,ATCAP33(,GPR15P)                          02S0684 01877000
*        DCL FMZL0062 BASED(GPR15P) LABEL VALUERANGE(*);        02S0685 01878000
*        CALL FMZL0062;                                         02S0686 01879000
         BALR  @14,@15                                          02S0686 01880000
*        GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(GPR00P,GPR01P 01881000
*GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,GPR09P,GPR10P,GPR11P, 01882000
*PR12P,GPR13P,GPR14P,GPR15P);                                   02S0687 01883000
*                                                               02S0687 01884000
         LM GPR00P,GPR15P,RPHWORK(GPR01P)                               01885000
*    RFY (GPR00P,GPR01P,GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;    02S0688 01886000
*        END;                                                   02S0689 01887000
*/*** END -TPUNLOCK (VOCLOCK)- ***/                             02S0690 01888000
*RFY GPR15F RSTD;                                               02S0690 01889000
*                                                               02S0690 01890000
