         MVI   CMZRL033(REG1033),X'00'                          02S0213 00356000
*          CMZRA033 = 0;                                        02S0214 00357000
         STCM  @10,7,CMZRA033(REG1033)                          02S0214 00358000
*          CMZEA033 = 0;                                        02S0215 00359000
