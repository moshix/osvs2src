*    RESPECIFY (REG1053,REG15053)  UNRESTRICTED;                02S0640 01756000
*                                                               02S0640 01757000
*  RFY (EMCR13) UNRSTD;                                         02S0641 01758000
*  END;                   /*@D32CKQH*/                          02S0642 01759000
*                 GPR15F=0;        /* PREPARE TO IGNORE LOWER NODES. */ 01760000
         SLR   @09,@09                                          02S0643 01761000
         LR    GPR15F,@09                                       02S0643 01762000
