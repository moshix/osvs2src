*                                        /*OF PPARM OPERAND          */ 00123000
*      END;  /* XREGSET */                                      02S0137 00124000
*        CMZID006 = 0;                                          02S0138 00125000
         ST    @10,CMZID006(,REG1006)                           02S0138 00126000
*        CMZRL006 = 0;                                          02S0139 00127000
         MVI   CMZRL006(REG1006),X'00'                          02S0139 00128000
*        CMZRA006 = 0;                                          02S0140 00129000
         STCM  @10,7,CMZRA006(REG1006)                          02S0140 00130000
*        CMZEA006 = 0;                                          02S0141 00131000
