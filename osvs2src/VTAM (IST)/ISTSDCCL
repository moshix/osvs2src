*   IF PHRF34VF(1)=PHRVFAOK         /* IF PARM VALID                 */ 00819000
*     THEN                          /* SAVE LOGON CHAR TABL NAME     */ 00820000
         CLI   PHRF34VF(PHR1REG),X'00'                             0404 00821000
         BNE   @RF00404                                            0404 00822000
*     RCPLGNCT=PHRF34SP(1);                                        0405 00823000
         L     @04,MLCPTR3(,MLCAREG)                               0405 00824000
         MVC   RCPLGNCT(8,@04),PHRF34SP(PHR1REG)                   0405 00825000
*   ELSE                                                           0406 00826000
*     DO;                                                          0406 00827000
