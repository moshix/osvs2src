         TITLE 'ISTSDCCL - CLUSTER MACRO HNDLR                         *00001000
                        '                                               00002000
ISTSDCCL CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(8)                                              0001 00007000
         DC    C'ISTSDCCL'                                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*   RNCSAV1(2)=GPR13;               /* BACKWARD POINT                */ 00014000
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0185 00015000
*   GPR13=ADDR(RNCSAV1);            /* SET UP OUR SAVE REG           */ 00016000
         LA    GPR13,RNCSAV1(,RNCAREG)                             0186 00017000
*   MLCSAV3(3)=GPR13;               /* FORWARD POINT                 */ 00018000
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0187 00019000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */ 00020000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0188 00021000
*   GENERATE;                                                      0189 00022000
*                                                                  0189 00023000
           USING @DATD,11                 ADDRESSABILITY FOR DATA       00024000
*/********************************************************************/ 00025000
*/* WHILE THE RETURN CODE = 0, THIS CODE PREPARES THE PHR PARAMETERS */ 00026000
*/* AND CALLS PHR.  UPON RETURN FROM PHR, THE RETURN CODE AND THE @  */ 00027000
*/* OF THE NEXT MACRO KEYWORD ARE SAVED.                             */ 00028000
*/********************************************************************/ 00029000
*                                                                  0190 00030000
*   RFY                                                            0190 00031000
*    (GPR0,                                                        0190 00032000
*     GPR1,                                                        0190 00033000
*     GPR15) RSTD;                  /* RESTRICT REGS                 */ 00034000
*                                                                  0190 00035000
*   /*****************************************************************/ 00036000
*   /*                                                               */ 00037000
*   /* SEGMENT(CLPRSCN)REQUEST                                       */ 00038000
*   /*                                                               */ 00039000
*   /*****************************************************************/ 00040000
*                                                                  0191 00041000
*   DO;                                                            0191 00042000
*     GOTO CLPRSCN;                                                0192 00043000
         B     CLPRSCN                                             0192 00044000
*CLPRSCN#:                                                         0193 00045000
*   END;                                                           0193 00046000
*                                                                  0193 00047000
CLPRSCN# DS    0H                                                  0194 00048000
*   /*****************************************************************/ 00049000
*   /*                                                               */ 00050000
*   /*                                                       @D32CKMX*/ 00051000
*   /*                                                               */ 00052000
*   /*****************************************************************/ 00053000
*                                                                  0194 00054000
*   IF RNCGCLPU^='1'B               /* IS PU FOOTPRINT OFF?  @D32CKMX*/ 00055000
*     THEN                                                         0194 00056000
         TM    RNCGCLPU(RNCAREG),B'00001000'                       0194 00057000
         BO    @RF00194                                            0194 00058000
*     DO;                           /*                       @D32CKMX*/ 00059000
*       GPR1=RPRENTCL;              /* INDICATE CALLR IS CLSTR       */ 00060000
         LA    GPR1,112                                            0196 00061000
*       CALL ISTSDCRS;              /* CALL SIFTDEF                  */ 00062000
         L     @15,@CV01241                                        0197 00063000
         BALR  @14,@15                                             0197 00064000
*       ENTRYPTR=GPR1;              /* SAVE ENTRY POINTER            */ 00065000
         LR    ENTRYPTR,GPR1                                       0198 00066000
*       GPR1=MLCKWD;                /* SETUP REG1 -> KEYWORD         */ 00067000
         L     GPR1,MLCKWD(,MLCAREG)                               0199 00068000
*       RC=0;                       /* INITIALIZE RETURN CODE        */ 00069000
         SLR   RC,RC                                               0200 00070000
*       DO WHILE(RC=0);             /* GO PROCESS KEYWORDS           */ 00071000
         B     @DE00201                                            0201 00072000
@DL00201 DS    0H                                                  0202 00073000
*         CALL DOWHILE;                                            0202 00074000
         BAL   @14,DOWHILE                                         0202 00075000
*DOLOOP:                                                           0203 00076000
*         ;                         /* USED TO GEN RETURN ADDR       */ 00077000
DOLOOP   DS    0H                                                  0204 00078000
*/********************************************************************/ 00079000
*/* AT THIS POINT A RETURN CODE OF 0 MEANS CONTINUE THE DO WHILE     */ 00080000
*/* LOOP.  OTHERWISE, THE LOOP IS DISCONTINUED WITH A RETURN CODE OF */ 00081000
*/* 8 OR 4.                                                          */ 00082000
*/********************************************************************/ 00083000
*                                                                  0204 00084000
*       END;                                                       0204 00085000
*                                                                  0204 00086000
@DE00201 SLR   @14,@14                                             0204 00087000
         CR    RC,@14                                              0204 00088000
         BE    @DL00201                                            0204 00089000
*/********************************************************************/ 00090000
*/* AT THIS POINT IF RC=8, INDICATING END OF STRING, WE CALL AREA    */ 00091000
*/* HANDLER TO MOVE THE MODEL AREA INTO THE BUILD AREA.  UPON RETURN */ 00092000
*/* WE UPDATE THE CURRENT ENTRY POINTER TO THIS ENTRY. ALSO, ANY     */ 00093000
*/* COUNTS THAT NEED BE UPDATED, ARE DONE AT THIS POINT.             */ 00094000
*/********************************************************************/ 00095000
*                                                                  0205 00096000
*       IF RC=8 THEN                                               0205 00097000
         CH    RC,@CH00036                                         0205 00098000
         BNE   @RF00205                                            0205 00099000
*         DO;                                                      0206 00100000
*           GPR0=0;                 /* INDICATE ADDR OF MODEL PASSED */ 00101000
         LR    GPR0,@14                                            0207 00102000
*           GPR1=ENTRYPTR;          /* PASS ADDR CLUSTER ENTRY MODEL    00103000
*                                                            @D32CKMX*/ 00104000
         LR    GPR1,ENTRYPTR                                       0208 00105000
*           CALL ISTSDCC1;          /* CALL AREA HANDLER             */ 00106000
         L     @15,@CV01237                                        0209 00107000
         BALR  @14,@15                                             0209 00108000
*           RC=GPR15;               /* SAVE AREAHANDLER RET. CODE    */ 00109000
         LR    RC,GPR15                                            0210 00110000
*           IF RC=0 THEN                                           0211 00111000
         LTR   RC,RC                                               0211 00112000
         BNZ   @RF00211                                            0211 00113000
*             DO;                   /*                       @D32CKMX*/ 00114000
*/********************************************************************/ 00115000
*/* PLACE CLUSTER BUILD AREA ADDR IN RNCA AND                        */ 00116000
*/* SET CLUSTER ISTATUS BIT THE SAME AS THE TERMINAL ISTATUS BIT     */ 00117000
*/********************************************************************/ 00118000
*                                                                  0213 00119000
*               RNCCL=GPR1;                                        0213 00120000
         ST    GPR1,RNCCL(,RNCAREG)                                0213 00121000
*               IF MLCPTR3->RTRRBISA='1'B THEN                     0214 00122000
         L     @14,MLCPTR3(,MLCAREG)                               0214 00123000
         TM    RTRRBISA(@14),B'01000000'                           0214 00124000
         BNO   @RF00214                                            0214 00125000
*                 GPR1->RCLRBISA='1'B;                             0215 00126000
         OI    RCLRBISA(GPR1),B'01000000'                          0215 00127000
*               ELSE                                               0216 00128000
*                 GPR1->RCLRBISA='0'B;                             0216 00129000
*                                                                  0216 00130000
         B     @RC00214                                            0216 00131000
@RF00214 NI    RCLRBISA(GPR1),B'10111111'                          0216 00132000
*/********************************************************************/ 00133000
*/*                     BIT HAS BEEN SET                             */ 00134000
*/********************************************************************/ 00135000
*/********************************************************************/ 00136000
*/* UPDATE COUNT OF CLSTR RDT ENTRIES IN RN,GROUP, AND LINE ENTRIES  */ 00137000
*/********************************************************************/ 00138000
*                                                                  0217 00139000
*               RNCRN->RRNCTECL=RNCRN->RRNCTECL+1;/* INCR. RN ETY    */ 00140000
@RC00214 L     @14,RNCRN(,RNCAREG)                                 0217 00141000
         LA    @10,1                                               0217 00142000
         LH    @08,RRNCTECL(,@14)                                  0217 00143000
         ALR   @08,@10                                             0217 00144000
         STH   @08,RRNCTECL(,@14)                                  0217 00145000
*               RNCGP->RGPCTECL=RNCGP->RGPCTECL+1;/* INCR. GP ETY    */ 00146000
         L     @14,RNCGP(,RNCAREG)                                 0218 00147000
         LH    @08,RGPCTECL(,@14)                                  0218 00148000
         ALR   @08,@10                                             0218 00149000
         STH   @08,RGPCTECL(,@14)                                  0218 00150000
*               RNCLN->RLNCTECL=RNCLN->RLNCTECL+1;/* INCR. LN ETY    */ 00151000
         L     @14,RNCLN(,RNCAREG)                                 0219 00152000
         AH    @10,RLNCTECL(,@14)                                  0219 00153000
         STH   @10,RLNCTECL(,@14)                                  0219 00154000
*             END;                                                 0220 00155000
*           ELSE                                                   0221 00156000
*             ;                                                    0221 00157000
@RF00211 DS    0H                                                  0222 00158000
*         END;                                                     0222 00159000
*       ELSE                                                       0223 00160000
*         ;                         /* AREAHANDLER RAN OUT OF CORE   */ 00161000
@RF00205 DS    0H                                                  0224 00162000
*     END;                                                         0224 00163000
*   ELSE                                                           0225 00164000
*     ;                             /* RTN CODE NOT 8                */ 00165000
@RF00194 DS    0H                                                  0226 00166000
*   RNCGCLPU='0'B;                  /* CLEAR PU CALLED BY CLUSTER  0226 00167000
*                                      FTPT                          */ 00168000
         NI    RNCGCLPU(RNCAREG),B'11110111'                       0226 00169000
*   RNCGCLUS='1'B;                  /* SET CLUSTER FOOTPRINT         */ 00170000
         OI    RNCGCLUS(RNCAREG),B'00000010'                       0227 00171000
*   GPR13=RNCSAV1(2);               /* RESTORE CALLERS REGS          */ 00172000
         L     GPR13,RNCSAV1+4(,RNCAREG)                           0228 00173000
*/********************************************************************/ 00174000
*/* AT THIS POINT RC CONTAINS EITHER A 0 OR A 4 INDICATING A GOOD    */ 00175000
*/* RETURN CODE OR AN ERROR RETURN CODE WHICH WILL KILL THE BUILD.   */ 00176000
*/********************************************************************/ 00177000
*                                                                  0229 00178000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00179000
         LR    @15,RC                                              0229 00180000
         L     @14,12(,@13)                                        0229 00181000
         LM    @00,@12,20(@13)                                     0229 00182000
         BR    @14                                                 0229 00183000
*DOWHILE:                                                          0230 00184000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0230 00185000
DOWHILE  DS    0H                                                  0231 00186000
*   GPR0=ADDR(CLSKWTBL);            /* POINT TO KYWD TABLES          */ 00187000
         LA    GPR0,CLSKWTBL                                       0231 00188000
*   PHR1REG=ADDR(RNCPHR1);          /* POINT TO PHR OUTPUT AREA      */ 00189000
         LA    PHR1REG,RNCPHR1(,RNCAREG)                           0232 00190000
*   RFY                                                            0233 00191000
*     ISTPHROA BASED(PHR1REG);      /* SET UP STRUCTURE BASE         */ 00192000
*   CALL ISTINCPH;                  /* CALL PHR                      */ 00193000
         L     @15,@CV01240                                        0234 00194000
         BALR  @14,@15                                             0234 00195000
*   RC=GPR15;                       /* SAV RETURN CODE FROM PHR      */ 00196000
         LR    RC,GPR15                                            0235 00197000
*   KWDPTR=GPR1;                    /* SAVE UPDATED KEYWORD PTR      */ 00198000
         LR    KWDPTR,GPR1                                         0236 00199000
*   IF RC=0 THEN                                                   0237 00200000
         LTR   RC,RC                                               0237 00201000
         BNZ   @RF00237                                            0237 00202000
*     DO;                                                          0238 00203000
*                                                                  0238 00204000
*/********************************************************************/ 00205000
*/* IF THE RETURN CODE FROM PHR = 0, CALL THE PARAMETER HANDLER      */ 00206000
*/* WHOSE ADDRESS WAS RETURNED IN GPR0 BY PHR.                       */ 00207000
*/* NOTE: THE EXTERNAL PARAMETER HANDLERS WILL RETURN TO THIS        */ 00208000
*/*       ROUTINE VIA A RETURN.  ALL INTERNAL PARAMETER HANDLERS     */ 00209000
*/*       WILL RETURN TO THIS ROUTINE VIA A GOTO 'MAINSUB'.          */ 00210000
*/* NOTE: ALL PARAMETER HANDLERS MUST HAVE A 0 OR 4 RETURN CODE IN   */ 00211000
*/*       GPR15 TO INDICATE CONTINUE PROCESSING OR KILL THE BUILD.   */ 00212000
*/********************************************************************/ 00213000
*                                                                  0239 00214000
*       GPR15=GPR0;                 /* SET UP PARM HANDLER SBRTN ADR */ 00215000
         LR    GPR15,GPR0                                          0239 00216000
*       CALL PARMHDLR;              /* GO SETUP RDT ENTRY            */ 00217000
         BALR  @14,@15                                             0240 00218000
*MAINSUB:                                                          0241 00219000
*       ;                           /* RTN LABEL FOR INTERNAL PROCS  */ 00220000
MAINSUB  DS    0H                                                  0242 00221000
*       RC=GPR15;                   /* SAVE RETURN CODE              */ 00222000
         LR    RC,GPR15                                            0242 00223000
*     END;                                                         0243 00224000
*   ELSE                                                           0244 00225000
*     DO;                                                          0244 00226000
*                                                                  0244 00227000
         B     @RC00237                                            0244 00228000
@RF00237 DS    0H                                                  0245 00229000
*/********************************************************************/ 00230000
*/* IF THE RETURN CODE FROM PHR = 4 INDICATING THE KEYWORD WAS NOT   */ 00231000
*/* FOUND IN THE PARAMETER TABLE, WRITE THE APPROPRIATE ERROR        */ 00232000
*/* MESSAGE, SET THE RETURN CODE = 0 AND CONTINUE THE DO WHILE LOOP. */ 00233000
*/********************************************************************/ 00234000
*                                                                  0245 00235000
*       IF RC=4 THEN                                               0245 00236000
         CH    RC,@CH00052                                         0245 00237000
         BNE   @RF00245                                            0245 00238000
*         DO;                       /* KEYWORD NOT FOUND             */ 00239000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 00240000
*                                                                  0247 00241000
*           DO;                                                    0247 00242000
*             DCL                                                  0248 00243000
*               1 CMZP0004 STATIC LOCAL,/* PARAMETER LIST PASSED TO     00244000
*                                      ERROR FORMAT ROUTINE          */ 00245000
*                2 CMZL0004 FIXED(8) INIT(4),/* LEVEL PARAMETER    0248 00246000
*                                      (CLEV)                        */ 00247000
*                2 CMZE0004 FIXED(8) INIT(4),/* ERROR PARAMETER    0248 00248000
*                                      (CERR)                        */ 00249000
*                2 CMZA0004 CHAR(1) INIT('1'),/* ACTION PARAMETER  0248 00250000
*                                      (CACT)                        */ 00251000
*                2 CMZS0004 FIXED(8) INIT(0);/* LENGTH OF STRING   0248 00252000
*                                      PARAM                         */ 00253000
*             DCL                                                  0249 00254000
*               CMZR0004 REGISTER(1);/* ESTABLISH PARAMeter          */ 00255000
*             RESPECIFY                                            0250 00256000
*               CMZR0004 RSTD;      /* REGISTER FOR                  */ 00257000
*             CMZR0004=ADDR(CMZP0004);/* ADDRESS OF PARAMETER LIST   */ 00258000
         LA    CMZR0004,CMZP0004                                   0251 00259000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 00260000
         L     @15,@CV01275                                        0252 00261000
         BALR  @14,@15                                             0252 00262000
*             RESPECIFY                                            0253 00263000
*               CMZR0004 UNRSTD;                                   0253 00264000
*           END;                                                   0254 00265000
*           RC=0;                   /* WONT KILL BUILD               */ 00266000
         SLR   RC,RC                                               0255 00267000
*         END;                                                     0256 00268000
*       ELSE                                                       0257 00269000
*         DO;                                                      0257 00270000
*                                                                  0257 00271000
         B     @RC00245                                            0257 00272000
@RF00245 DS    0H                                                  0258 00273000
*/********************************************************************/ 00274000
*/* IF THE RETURN CODE FROM PHR = 8 OR 12 OR 16 INDICATING AN END OF */ 00275000
*/* STRING OR SYNTAX ERROR OR FUNCTION ERROR, RESPECTIVELY,          */ 00276000
*/* THE FOLLOWING ROUTINE IS EXECUTED.                               */ 00277000
*/********************************************************************/ 00278000
*                                                                  0258 00279000
*           IF RC=8                 /* END OF STRING ?               */ 00280000
*             THEN                                                 0258 00281000
         CH    RC,@CH00036                                         0258 00282000
         BNE   @RF00258                                            0258 00283000
*             IF RCLCTNGP>0         /* GPOLL?                @D32CKMX*/ 00284000
*               THEN                                               0259 00285000
         ICM   @14,3,RCLCTNGP(ENTRYPTR)                            0259 00286000
         BP    @RT00259                                            0259 00287000
*               ;                   /* YES, DO NOTHING               */ 00288000
*             ELSE                                                 0261 00289000
*               DO;                 /* NO, ERROR                     */ 00290000
*/* ERRFMT    LEVEL(PARM)  ERROR(MISS)  ACTION(KILLBLD)  STRING('GPOLL' 00291000
*                                                                    */ 00292000
*                                                                  0262 00293000
*                 DO;                                              0262 00294000
*                   DCL                                            0263 00295000
*                     1 CMZP0005 STATIC LOCAL,/* PARAMETER LIST    0263 00296000
*                                      PASSED TO ERROR FORMAT ROUTINE*/ 00297000
*                      2 CMZL0005 FIXED(8) INIT(4),/* LEVEL PARAMETER   00298000
*                                      (CLEV)                        */ 00299000
*                      2 CMZE0005 FIXED(8) INIT(8),/* ERROR PARAMETER   00300000
*                                      (CERR)                        */ 00301000
*                      2 CMZA0005 CHAR(1) INIT('6'),/* ACTION      0263 00302000
*                                      PARAMETER (CACT)              */ 00303000
*                      2 CMZS0005 FIXED(8) INIT(5),/* LENGTH OF    0263 00304000
*                                      STRING PARAM                  */ 00305000
*                      2 CMZM0005 CHAR(5) INIT('GPOLL');/* PARAMETER    00306000
*                                      STRING                        */ 00307000
*                   DCL                                            0264 00308000
*                     CMZR0005 REGISTER(1);/* ESTABLISH PARAMeter    */ 00309000
*                   RESPECIFY                                      0265 00310000
*                     CMZR0005 RSTD;/* REGISTER FOR                  */ 00311000
*                   CMZR0005=ADDR(CMZP0005);/* ADDRESS OF PARAMETER     00312000
*                                      LIST                          */ 00313000
         LA    CMZR0005,CMZP0005                                   0266 00314000
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */ 00315000
         L     @15,@CV01275                                        0267 00316000
         BALR  @14,@15                                             0267 00317000
*                   RESPECIFY                                      0268 00318000
*                     CMZR0005 UNRSTD;                             0268 00319000
*                 END;                                             0269 00320000
*                 RC=4;                                            0270 00321000
         LA    RC,4                                                0270 00322000
*               END;                /* END RTN CODE = 8              */ 00323000
*           ELSE                                                   0272 00324000
*             DO;                   /* NO, THEN SOME TYPE OF ERROR   */ 00325000
         B     @RC00258                                            0272 00326000
@RF00258 DS    0H                                                  0273 00327000
*               IF RC=12            /* SYNTAX ERROR ?                */ 00328000
*                 THEN              /* FUNCTION ERROR                */ 00329000
         CH    RC,@CH00032                                         0273 00330000
         BNE   @RF00273                                            0273 00331000
*/* ERRFMT    LEVEL(PARM)  ERROR(SYN)  ACTION(KILLBLD)               */ 00332000
*                                                                  0274 00333000
*                 DO;                                              0274 00334000
*                   DCL                                            0275 00335000
*                     1 CMZP0006 STATIC LOCAL,/* PARAMETER LIST    0275 00336000
*                                      PASSED TO ERROR FORMAT ROUTINE*/ 00337000
*                      2 CMZL0006 FIXED(8) INIT(4),/* LEVEL PARAMETER   00338000
*                                      (CLEV)                        */ 00339000
*                      2 CMZE0006 FIXED(8) INIT(6),/* ERROR PARAMETER   00340000
*                                      (CERR)                        */ 00341000
*                      2 CMZA0006 CHAR(1) INIT('6'),/* ACTION      0275 00342000
*                                      PARAMETER (CACT)              */ 00343000
*                      2 CMZS0006 FIXED(8) INIT(0);/* LENGTH OF    0275 00344000
*                                      STRING PARAM                  */ 00345000
*                   DCL                                            0276 00346000
*                     CMZR0006 REGISTER(1);/* ESTABLISH PARAMeter    */ 00347000
*                   RESPECIFY                                      0277 00348000
*                     CMZR0006 RSTD;/* REGISTER FOR                  */ 00349000
*                   CMZR0006=ADDR(CMZP0006);/* ADDRESS OF PARAMETER     00350000
*                                      LIST                          */ 00351000
         LA    CMZR0006,CMZP0006                                   0278 00352000
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */ 00353000
         L     @15,@CV01275                                        0279 00354000
         BALR  @14,@15                                             0279 00355000
*                   RESPECIFY                                      0280 00356000
*                     CMZR0006 UNRSTD;                             0280 00357000
*                 END;                                             0281 00358000
*               ELSE                                               0282 00359000
*                                                                  0282 00360000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(KILLBLD)             */ 00361000
*                                                                  0282 00362000
*                 DO;                                              0282 00363000
         B     @RC00273                                            0282 00364000
@RF00273 DS    0H                                                  0283 00365000
*                   DCL                                            0283 00366000
*                     1 CMZP0007 STATIC LOCAL,/* PARAMETER LIST    0283 00367000
*                                      PASSED TO ERROR FORMAT ROUTINE*/ 00368000
*                      2 CMZL0007 FIXED(8) INIT(4),/* LEVEL PARAMETER   00369000
*                                      (CLEV)                        */ 00370000
*                      2 CMZE0007 FIXED(8) INIT(4),/* ERROR PARAMETER   00371000
*                                      (CERR)                        */ 00372000
*                      2 CMZA0007 CHAR(1) INIT('6'),/* ACTION      0283 00373000
*                                      PARAMETER (CACT)              */ 00374000
*                      2 CMZS0007 FIXED(8) INIT(0);/* LENGTH OF    0283 00375000
*                                      STRING PARAM                  */ 00376000
*                   DCL                                            0284 00377000
*                     CMZR0007 REGISTER(1);/* ESTABLISH PARAMeter    */ 00378000
*                   RESPECIFY                                      0285 00379000
*                     CMZR0007 RSTD;/* REGISTER FOR                  */ 00380000
*                   CMZR0007=ADDR(CMZP0007);/* ADDRESS OF PARAMETER     00381000
*                                      LIST                          */ 00382000
         LA    CMZR0007,CMZP0007                                   0286 00383000
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */ 00384000
         L     @15,@CV01275                                        0287 00385000
         BALR  @14,@15                                             0287 00386000
*                   RESPECIFY                                      0288 00387000
*                     CMZR0007 UNRSTD;                             0288 00388000
*                 END;                                             0289 00389000
*               RC=4;               /* RET CODE IS FOUR FOR 12,16, &    00390000
*                                      20                            */ 00391000
@RC00273 LA    RC,4                                                0290 00392000
*             END;                  /* END RTN CODE ^= 8             */ 00393000
*         END;                      /* END RTN CODE ^= 4             */ 00394000
@RC00258 DS    0H                                                  0293 00395000
*     END;                          /* END RNT CODE ^= 0             */ 00396000
@RC00245 DS    0H                                                  0294 00397000
*   GPR1=KWDPTR;                    /* SETUP KEYWORD PTR             */ 00398000
@RC00237 LR    GPR1,KWDPTR                                         0294 00399000
*   MLCKWD=GPR1;                    /* UPDATE MLCA KEYWORD PTR       */ 00400000
         ST    GPR1,MLCKWD(,MLCAREG)                               0295 00401000
*   RFY                                                            0296 00402000
*     GPR14 RSTD;                                                  0296 00403000
*   GPR14=ADDR(DOLOOP);             /* SETUP RETURN ADDRESS          */ 00404000
         LA    GPR14,DOLOOP                                        0297 00405000
*   RFY                                                            0298 00406000
*    (GPR0,                                                        0298 00407000
*     GPR1,                                                        0298 00408000
*     GPR14,                                                       0298 00409000
*     GPR15) UNRSTD;                /* UNRESTRICT REGS               */ 00410000
*CLSBHSET:                                                         0299 00411000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0299 00412000
*                                                                  0299 00413000
         B     @PB00003                                            0299 00414000
CLSBHSET DS    0H                                                  0300 00415000
*/********************************************************************/ 00416000
*/* THIS ROUTINE WILL PROCESS THE BHSET PARAMETER OF THE CLUSTER     */ 00417000
*/* MACRO IF 'NONE' OR 'DYNAMIC' HAS BEEN SPECIFIED THE APPROPRIATE  */ 00418000
*/* FLAG IS SET IN THE RDT. IF A SET NAME WAS SPECIFIED IT IS MOVED  */ 00419000
*/* TO THE RDT. IF A VALIDITY OR INVALID COMBINATION ERROR OCCURS,   */ 00420000
*/* ERRFMT IS ISSUED AND THE PARAMETER IS IGNORED.                   */ 00421000
*/********************************************************************/ 00422000
*                                                                  0300 00423000
*   RFY                                                            0300 00424000
*     ISTRTR BASED(MLCPTR3);        /* COVER TERMINAL MODEL          */ 00425000
*   IF PHRF34SP(1)='NONE    '       /* CHECK FOR 'NONE'              */ 00426000
*       |PHRF34SP(1)='        '     /* OR BLANKS                     */ 00427000
*     THEN                                                         0301 00428000
         CLC   PHRF34SP(8,PHR1REG),@CC01301                        0301 00429000
         BE    @RT00301                                            0301 00430000
         CLC   PHRF34SP(8,PHR1REG),@CC01302                        0301 00431000
         BNE   @RF00301                                            0301 00432000
@RT00301 DS    0H                                                  0302 00433000
*     DO;                           /* IF 'NONE' SPECIFIED           */ 00434000
*       RTRRDBHN=ON;                /* TURN ON 'NONE' FLG IN RDT     */ 00435000
*       RTRRDBHD=OFF;               /* INSURE DYNAMIC FLAG OFF       */ 00436000
         L     @04,MLCPTR3(,MLCAREG)                               0304 00437000
         OI    RTRRDBHN(@04),B'10000000'                           0304 00438000
         NI    RTRRDBHD(@04),B'10111111'                           0304 00439000
*     END;                                                         0305 00440000
*   ELSE                                                           0306 00441000
*     IF PHRF34SP(1)='DYNAMIC ' THEN                               0306 00442000
*                                                                  0306 00443000
         B     @RC00301                                            0306 00444000
@RF00301 CLC   PHRF34SP(8,PHR1REG),@CC01303                        0306 00445000
         BNE   @RF00306                                            0306 00446000
*/********************************************************************/ 00447000
*/* IF DYNAMIC IS SPECIFIED THE SYSCNTRL MACRO MUST HAVE HAD THE     */ 00448000
*/* MODIFY BLOCK HANDLER SET ASSOCIATION (BHSASSC) SUBPARM LISTED    */ 00449000
*/* AS AN OPTION ENTRY.                                              */ 00450000
*/********************************************************************/ 00451000
*                                                                  0307 00452000
*       IF RNCRN->RRNCAMBH=ON       /* BHSASSC SET BY SYSCNTRL       */ 00453000
*         THEN                                                     0307 00454000
         L     @04,RNCRN(,RNCAREG)                                 0307 00455000
         TM    RRNCAMBH(@04),B'00001000'                           0307 00456000
         BNO   @RF00307                                            0307 00457000
*         DO;                                                      0308 00458000
*                                                                  0308 00459000
*/********************************************************************/ 00460000
*/* SET 'DYNAMIC' FLG IN MODEL AND TURN OFF 'NONE' FLAG              */ 00461000
*/********************************************************************/ 00462000
*                                                                  0309 00463000
*           RTRRDBHD=ON;                                           0309 00464000
*           RTRRDBHN=OFF;                                          0310 00465000
         L     @04,MLCPTR3(,MLCAREG)                               0310 00466000
         OI    RTRRDBHD(@04),B'01000000'                           0310 00467000
         NI    RTRRDBHN(@04),B'01111111'                           0310 00468000
*         END;                                                     0311 00469000
*       ELSE                                                       0312 00470000
*         DO;                                                      0312 00471000
*                                                                  0312 00472000
         B     @RC00307                                            0312 00473000
@RF00307 DS    0H                                                  0313 00474000
*/* ERRFMT    LEVEL(VALUE)  ERROR(COMB)  ACTION(IGNORE)              */ 00475000
*                                                                  0313 00476000
*           DO;                                                    0313 00477000
*             DCL                                                  0314 00478000
*               1 CMZP0008 STATIC LOCAL,/* PARAMETER LIST PASSED TO     00479000
*                                      ERROR FORMAT ROUTINE          */ 00480000
*                2 CMZL0008 FIXED(8) INIT(5),/* LEVEL PARAMETER    0314 00481000
*                                      (CLEV)                        */ 00482000
*                2 CMZE0008 FIXED(8) INIT(1),/* ERROR PARAMETER    0314 00483000
*                                      (CERR)                        */ 00484000
*                2 CMZA0008 CHAR(1) INIT('1'),/* ACTION PARAMETER  0314 00485000
*                                      (CACT)                        */ 00486000
*                2 CMZS0008 FIXED(8) INIT(0);/* LENGTH OF STRING   0314 00487000
*                                      PARAM                         */ 00488000
*             DCL                                                  0315 00489000
*               CMZR0008 REGISTER(1);/* ESTABLISH PARAMeter          */ 00490000
*             RESPECIFY                                            0316 00491000
*               CMZR0008 RSTD;      /* REGISTER FOR                  */ 00492000
*             CMZR0008=ADDR(CMZP0008);/* ADDRESS OF PARAMETER LIST   */ 00493000
         LA    CMZR0008,CMZP0008                                   0317 00494000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 00495000
         L     @15,@CV01275                                        0318 00496000
         BALR  @14,@15                                             0318 00497000
*             RESPECIFY                                            0319 00498000
*               CMZR0008 UNRSTD;                                   0319 00499000
*           END;                    /*                               */ 00500000
*/* TURN OFF 'DYNAMIC' FLG IN RDT AND SET THE 'NONE' ON              */ 00501000
*/********************************************************************/ 00502000
*                                                                  0321 00503000
*           RTRRDBHD=OFF;                                          0321 00504000
*           RTRRDBHN=ON;                                           0322 00505000
         L     @04,MLCPTR3(,MLCAREG)                               0322 00506000
         OI    RTRRDBHN(@04),B'10000000'                           0322 00507000
         NI    RTRRDBHD(@04),B'10111111'                           0322 00508000
*         END;                                                     0323 00509000
*     ELSE                                                         0324 00510000
*       DO;                                                        0324 00511000
*                                                                  0324 00512000
         B     @RC00306                                            0324 00513000
@RF00306 DS    0H                                                  0325 00514000
*/********************************************************************/ 00515000
*/* MOVE SET NAME TO RDT AND TURN OFF 'NONE' & 'DYN' FLG             */ 00516000
*/********************************************************************/ 00517000
*                                                                  0325 00518000
*         RTRBHSN1=PHRF34SP(1);                                    0325 00519000
         L     @04,MLCPTR3(,MLCAREG)                               0325 00520000
         MVC   RTRBHSN1(8,@04),PHRF34SP(PHR1REG)                   0325 00521000
*         RTRRDBHN=OFF;             /* DYNAMIC FLAG OFF              */ 00522000
*         RTRRDBHD=OFF;             /* NONE FLAG OFF                 */ 00523000
         NI    RTRRDBHN(@04),B'00111111'                           0327 00524000
*       END;                                                       0328 00525000
*   RFY                                                            0329 00526000
*    (GPR14,                                                       0329 00527000
*     GPR15) RSTD;                                                 0329 00528000
@RC00306 DS    0H                                                  0329 00529000
@RC00301 DS    0H                                                  0330 00530000
*   GPR15=0;                                                       0330 00531000
         SLR   GPR15,GPR15                                         0330 00532000
*   GPR14=ADDR(MAINSUB);                                           0331 00533000
         LA    GPR14,MAINSUB                                       0331 00534000
*   RFY                                                            0332 00535000
*    (GPR14,                                                       0332 00536000
*     GPR15) UNRSTD;                                               0332 00537000
*   END CLSBHSET;                                                  0333 00538000
@EL00003 DS    0H                                                  0333 00539000
@EF00003 DS    0H                                                  0333 00540000
@ER00003 BR    @14                                                 0333 00541000
*CLSPOLL:                                                          0334 00542000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0334 00543000
*                                                                  0334 00544000
CLSPOLL  DS    0H                                                  0335 00545000
*/********************************************************************/ 00546000
*/* THIS ROUTINE WILL MOVE THE POLLING CHARACTERS TO THE RDT         */ 00547000
*/* EXTENSION AREA. THE LENGTH AND POINTER TO THESE CHARACTERS ARE   */ 00548000
*/* FOUND IN THE VTAM PARAMETER HANDLER FUNCTION ONE OUTPUT AREA     */ 00549000
*/********************************************************************/ 00550000
*                                                                  0335 00551000
*   RFY                                                            0335 00552000
*    (GPR0,                                                        0335 00553000
*     GPR1) RSTD;                                                  0335 00554000
*   GPR0=PHRF1SLN;                  /* SET UP PARMS FOR CONVERSION 0336 00555000
*                                      RTN                           */ 00556000
         L     GPR0,PHRF1SLN(,PHR1REG)                             0336 00557000
*   GPR1=PHRF1SAD;                  /* GPR0=LENGTH OF STRING - GPR1 =   00558000
*                                      PTR                           */ 00559000
         L     GPR1,PHRF1SAD(,PHR1REG)                             0337 00560000
*   CALL ISTSDCC4;                  /* TO STRING.                    */ 00561000
         L     @15,@CV01238                                        0338 00562000
         BALR  @14,@15                                             0338 00563000
*   GPR1=0;                         /* SET UP PARMS FOR AREA HANDLER    00564000
*                                      RTN. GPR1=0                   */ 00565000
         SLR   GPR1,GPR1                                           0339 00566000
*   GPR0=PHRF1SLN/2;                /* - GPR0=LENGTH OF POLLING CHARS   00567000
*                                      IN BYTES.                     */ 00568000
         L     @08,PHRF1SLN(,PHR1REG)                              0340 00569000
         SRDA  @08,32                                              0340 00570000
         D     @08,@CF00234                                        0340 00571000
         LR    GPR0,@09                                            0340 00572000
*   RCLCTNGP=GPR0;                  /* PUT LENGTH IN ENTRY           */ 00573000
         STH   GPR0,RCLCTNGP(,ENTRYPTR)                            0341 00574000
*   CALL ISTSDCC1;                                                 0342 00575000
         L     @15,@CV01237                                        0342 00576000
         BALR  @14,@15                                             0342 00577000
*   RFY                                                            0343 00578000
*     GPR15 RSTD;                                                  0343 00579000
*   IF GPR15=0                      /* IF STORAGE WAS AVAILABLE FOR     00580000
*                                      POLLING                       */ 00581000
*     THEN                                                         0344 00582000
         SLR   @14,@14                                             0344 00583000
         CR    GPR15,@14                                           0344 00584000
         BNE   @RF00344                                            0344 00585000
*     DO;                           /* CHARS MOVE THEM TO THE RDT  0345 00586000
*                                      EXT.                          */ 00587000
*       POLLEXT(1:RCLCTNGP)=POLLCHAR(1:RCLCTNGP);                  0346 00588000
*                                                                  0346 00589000
         LH    @04,RCLCTNGP(,ENTRYPTR)                             0346 00590000
         BCTR  @04,0                                               0346 00591000
         L     @03,PHRF1SAD(,PHR1REG)                              0346 00592000
         EX    @04,@SM01593                                        0346 00593000
*/********************************************************************/ 00594000
*/* SAVE RELATIVE OFFSET (INTO RDT EXTENSION) IN CLUSTER RDT ENTRY.  */ 00595000
*/* LENGTH WAS SAVED BEFORE AREAHDLR CALL. POLL CHARS ARE FOR        */ 00596000
*/* STATION.                                                         */ 00597000
*/********************************************************************/ 00598000
*                                                                  0347 00599000
*       RCLGPOLA=GPR0;              /* SET OFFSET                    */ 00600000
         ST    GPR0,RCLGPOLA(,ENTRYPTR)                            0347 00601000
*       GPR15=0;                                                   0348 00602000
         LR    GPR15,@14                                           0348 00603000
*     END;                                                         0349 00604000
*   ELSE                                                           0350 00605000
*     GPR15=4;                                                     0350 00606000
         B     @RC00344                                            0350 00607000
@RF00344 LA    GPR15,4                                             0350 00608000
*   RFY                                                            0351 00609000
*     GPR14 RSTD;                                                  0351 00610000
@RC00344 DS    0H                                                  0352 00611000
*   GPR14=ADDR(MAINSUB);            /* SET UP RETURN ADDRESS         */ 00612000
         LA    GPR14,MAINSUB                                       0352 00613000
*   RFY                                                            0353 00614000
*    (GPR0,                                                        0353 00615000
*     GPR1,                                                        0353 00616000
*     GPR14,                                                       0353 00617000
*     GPR15) UNRSTD;                                               0353 00618000
*   END CLSPOLL;                                                   0354 00619000
@EL00004 DS    0H                                                  0354 00620000
@EF00004 DS    0H                                                  0354 00621000
@ER00004 BR    @14                                                 0354 00622000
*                                                                  0355 00623000
*/********************************************************************/ 00624000
*/*        XMITLIM PARAMETER SUBROUTINE                              */ 00625000
*/********************************************************************/ 00626000
*                                                                  0355 00627000
*CLSXMTLM:                                                         0355 00628000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0355 00629000
*                                                                  0355 00630000
CLSXMTLM DS    0H                                                  0356 00631000
*/********************************************************************/ 00632000
*/* THIS ROUTINE WILL MOVE IF SPECIFIED, THE NUMBER OF TRANSMISSION  */ 00633000
*/* THE NCP WILL RECEIVE FROM OR SEND TO THIS STATION OR BOTH. IF NO */ 00634000
*/* NUMBER IS SPECIFIED A CHECK IS MADE TO SEE IF 'NO' WAS THE PARM. */ 00635000
*/* IF SO THER IS NO LIMIT TRANSMISSIONS. IF NEITHER A COUNT OR      */ 00636000
*/* 'NO' WERE GIVEN, AN ERROR MSG IS PRINTED AND THE DEFAULT OF 'NO' */ 00637000
*/* IS TAKEN. IF A COUNT IS SPECIFIED IT MUST BE >0<256.             */ 00638000
*/********************************************************************/ 00639000
*                                                                  0356 00640000
*   IF PHRF34SP(1)='000000NO'|PHRF34SL(1)=0 THEN/* TO SEE IF PARM WAS   00641000
*                                      'NO'                          */ 00642000
         CLC   PHRF34SP(8,PHR1REG),@CC01316                        0356 00643000
         BE    @RT00356                                            0356 00644000
         ICM   @04,3,PHRF34SL(PHR1REG)                             0356 00645000
         BNZ   @RF00356                                            0356 00646000
@RT00356 DS    0H                                                  0357 00647000
*     RTRTRMAX=0;                   /* IF 'NO' SET XMISSION CNT=0    */ 00648000
         L     @04,MLCPTR3(,MLCAREG)                               0357 00649000
         MVI   RTRTRMAX(@04),X'00'                                 0357 00650000
*   ELSE                                                           0358 00651000
*     DO;                           /* A NUM WAS SPECIFIED           */ 00652000
         B     @RC00356                                            0358 00653000
@RF00356 DS    0H                                                  0359 00654000
*       PACK(PHRF34SP(1),PHRF34SP(1));/* CONVERT IT TO BINARY        */ 00655000
         PACK  PHRF34SP(8,PHR1REG),PHRF34SP(8,PHR1REG)             0359 00656000
*       CVB(CVBNUM,PHRF34SP(1));    /* AND CK FOR VALID              */ 00657000
         CVB   @04,PHRF34SP(,PHR1REG)                              0360 00658000
         ST    @04,CVBNUM                                          0360 00659000
*       RTRTRMAX=CVBNUM2;           /* IF OK - STORE IT IN           */ 00660000
         L     @04,MLCPTR3(,MLCAREG)                               0361 00661000
         MVC   RTRTRMAX(1,@04),CVBNUM2                             0361 00662000
*     END;                                                         0362 00663000
*   RFY                                                            0363 00664000
*    (GPR14,                                                       0363 00665000
*     GPR15) RSTD;                                                 0363 00666000
@RC00356 DS    0H                                                  0364 00667000
*   GPR15=0;                        /* SET RTN CODE TO 0             */ 00668000
         SLR   GPR15,GPR15                                         0364 00669000
*   GPR14=ADDR(MAINSUB);                                           0365 00670000
         LA    GPR14,MAINSUB                                       0365 00671000
*   RFY                                                            0366 00672000
*    (GPR14,                                                       0366 00673000
*     GPR15) UNRSTD;                                               0366 00674000
*   END CLSXMTLM;                                                  0367 00675000
@EL00005 DS    0H                                                  0367 00676000
@EF00005 DS    0H                                                  0367 00677000
@ER00005 BR    @14                                                 0367 00678000
*                                                                  0368 00679000
*/********************************************************************/ 00680000
*/*        ITBMODE PARAMETER SUBROUTINE                              */ 00681000
*/********************************************************************/ 00682000
*                                                                  0368 00683000
*CLSITBMD:                                                         0368 00684000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0368 00685000
*                                                                  0368 00686000
CLSITBMD DS    0H                                                  0369 00687000
*/********************************************************************/ 00688000
*/* THIS ROUTINE WILL PROCESS THE ITBMODE PARAMETER OF THE TERMINAL  */ 00689000
*/* MACRO. IF EITHER SUBPARM IS INVALID AN ERROR MSG IS PUT OUT AND  */ 00690000
*/* THE DEFAULT IS TAKEN. IF THE SUBPARMS ARE VALID THE APPROPRIATE  */ 00691000
*/* BITS ARE TURNED ON IN THE TERMINAL  MODEL IF OTHER THAN THE      */ 00692000
*/* DEFAULT VALUE IS SPECIFIED.                                      */ 00693000
*/********************************************************************/ 00694000
*                                                                  0369 00695000
*   RTRRCIT1=OFF;                                                  0369 00696000
*   RTRRCIT2=OFF;                                                  0370 00697000
         L     @04,MLCPTR3(,MLCAREG)                               0370 00698000
         NI    RTRRCIT1(@04),B'00111111'                           0370 00699000
*   IF PHRF2VAL(1)=1                /* IF 1ST SUBPARM OK AND WAS     */ 00700000
*     THEN                          /* 'YES' TURN ON FLAG IN         */ 00701000
         CLC   PHRF2VAL(4,PHR1REG),@CF00039                        0371 00702000
         BNE   @RF00371                                            0371 00703000
*     RTRRCIT1=ON;                  /* TERM ENTRY. IF 'NO'           */ 00704000
         OI    RTRRCIT1(@04),B'10000000'                           0372 00705000
*   ELSE                                                           0373 00706000
*     ;                             /* TURN FLAG OFF                 */ 00707000
@RF00371 DS    0H                                                  0374 00708000
*   IF PHRF2NVL>1                   /* MORE THAN ONE PARM            */ 00709000
*     THEN                          /* PROCESS SECOND                */ 00710000
         LA    @04,1                                               0374 00711000
         C     @04,PHRF2NVL(,PHR1REG)                              0374 00712000
         BNL   @RF00374                                            0374 00713000
*     IF PHRF2VAL(2)=1              /* IF 2ND SUBPARM OK AND WAS     */ 00714000
*       THEN                        /* 'YES' TURN ON FLAG IN TERM    */ 00715000
         C     @04,PHRF2VAL+4(,PHR1REG)                            0375 00716000
         BNE   @RF00375                                            0375 00717000
*       RTRRCIT2=ON;                /* ENTRY. IF 'NO' LEAVE FLAG     */ 00718000
         L     @04,MLCPTR3(,MLCAREG)                               0376 00719000
         OI    RTRRCIT2(@04),B'01000000'                           0376 00720000
*     ELSE                                                         0377 00721000
*       ;                           /* OFF.                          */ 00722000
@RF00375 DS    0H                                                  0378 00723000
*   ELSE                                                           0378 00724000
*     ;                             /* ONLY ONE PARM SPECIFIED       */ 00725000
@RF00374 DS    0H                                                  0379 00726000
*   RFY                                                            0379 00727000
*    (GPR14,                                                       0379 00728000
*     GPR15) RSTD;                                                 0379 00729000
*   GPR15=0;                        /* SET RTN CODE TO 0             */ 00730000
         SLR   GPR15,GPR15                                         0380 00731000
*   GPR14=ADDR(MAINSUB);                                           0381 00732000
         LA    GPR14,MAINSUB                                       0381 00733000
*   RFY                                                            0382 00734000
*    (GPR14,                                                       0382 00735000
*     GPR15) UNRSTD;                                               0382 00736000
*   END CLSITBMD;                                                  0383 00737000
@EL00006 DS    0H                                                  0383 00738000
@EF00006 DS    0H                                                  0383 00739000
@ER00006 BR    @14                                                 0383 00740000
*                                                                  0384 00741000
*/********************************************************************/ 00742000
*/*          LGAPL PARAMETER SUBROUTINE                              */ 00743000
*/* IF THE LOGAPPL PARAMETER IS CODED AND IS VALID MOVE IT IN        */ 00744000
*/* TO THE CONNECTION SERVICES PREFIX. IF IT IS IN ERROR THEN        */ 00745000
*/* BLANK OUT THE AREA SINCE IT MAY HAVE BEEN CODED ON A HIGHER      */ 00746000
*/* LEVEL MACRO. ALSO IF AN ERROR WRITE ERROR MESSAGE.               */ 00747000
*/********************************************************************/ 00748000
*                                                                  0384 00749000
*CLSLGAPL:                                                         0384 00750000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0384 00751000
CLSLGAPL DS    0H                                                  0385 00752000
*   IF PHRF34VF(1)=PHRVFAOK         /* IF PARM VALID                 */ 00753000
*     THEN                          /* SAVE APPLIC NAME              */ 00754000
         CLI   PHRF34VF(PHR1REG),X'00'                             0385 00755000
         BNE   @RF00385                                            0385 00756000
*     RCPLGNAP=PHRF34SP(1);                                        0386 00757000
         L     @04,MLCPTR3(,MLCAREG)                               0386 00758000
         MVC   RCPLGNAP(8,@04),PHRF34SP(PHR1REG)                   0386 00759000
*   ELSE                                                           0387 00760000
*     DO;                                                          0387 00761000
*                                                                  0387 00762000
         B     @RC00385                                            0387 00763000
@RF00385 DS    0H                                                  0388 00764000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 00765000
*                                                                  0388 00766000
*       DO;                                                        0388 00767000
*         DCL                                                      0389 00768000
*           1 CMZP0009 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   00769000
*                                      FORMAT ROUTINE                */ 00770000
*            2 CMZL0009 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)   */ 00771000
*            2 CMZE0009 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR)   */ 00772000
*            2 CMZA0009 CHAR(1) INIT('1'),/* ACTION PARAMETER (CACT) */ 00773000
*            2 CMZS0009 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 00774000
*         DCL                                                      0390 00775000
*           CMZR0009 REGISTER(1);   /* ESTABLISH PARAMeter           */ 00776000
*         RESPECIFY                                                0391 00777000
*           CMZR0009 RSTD;          /* REGISTER FOR                  */ 00778000
*         CMZR0009=ADDR(CMZP0009);  /* ADDRESS OF PARAMETER LIST     */ 00779000
         LA    CMZR0009,CMZP0009                                   0392 00780000
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */ 00781000
         L     @15,@CV01275                                        0393 00782000
         BALR  @14,@15                                             0393 00783000
*         RESPECIFY                                                0394 00784000
*           CMZR0009 UNRSTD;                                       0394 00785000
*       END;                                                       0395 00786000
*       RCPLGNAP='';                                               0396 00787000
         L     @04,MLCPTR3(,MLCAREG)                               0396 00788000
         MVI   RCPLGNAP(@04),C' '                                  0396 00789000
         MVC   RCPLGNAP+1(7,@04),RCPLGNAP(@04)                     0396 00790000
*     END;                                                         0397 00791000
*   RFY                                                            0398 00792000
*    (GPR14,                                                       0398 00793000
*     GPR15) RSTD;                                                 0398 00794000
@RC00385 DS    0H                                                  0399 00795000
*   GPR15=0;                                                       0399 00796000
         SLR   GPR15,GPR15                                         0399 00797000
*   GPR14=ADDR(MAINSUB);                                           0400 00798000
