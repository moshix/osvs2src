         LTR   @14,@14                                             0411 00196000
         BZ    @RF00411                                            0411 00197000
*     DO;                                                          0412 00198000
*                                                                  0412 00199000
*       /*************************************************************/ 00200000
*       /*                                                           */ 00201000
*       /* SEGMENT(RECOVER)REQUEST                                   */ 00202000
*       /*                                                           */ 00203000
*       /*************************************************************/ 00204000
*                                                                  0413 00205000
*       DO;                                                        0413 00206000
*         GOTO RECOVER;                                            0414 00207000
         B     RECOVER                                             0414 00208000
*RECOVER#:                                                         0415 00209000
*       END;                        /* RECOVER FROM ALL ERRORS BY  0415 00210000
*                                      DELETING ALL TABLES           */ 00211000
RECOVER# DS    0H                                                  0416 00212000
*     END;                                                         0416 00213000
*   ELSE                                                           0417 00214000
*     DO;                           /* NO ERROR - DO NOTHING         */ 00215000
         B     @RC00411                                            0417 00216000
