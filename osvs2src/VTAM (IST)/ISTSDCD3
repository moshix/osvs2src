*       IF CHAINPTR=0               /* WAS TERM/UTERM/COMP/LU RDTE 0418 00218000
*                                      FOUND?                @ZA12744*/ 00219000
*         THEN                                                     0418 00220000
         LTR   CHAINPTR,CHAINPTR                                   0418 00221000
         BZ    @RT00418                                            0418 00222000
*         ;                         /* NO - DO NOTHING       @ZA12744*/ 00223000
*       ELSE                                                       0420 00224000
*         DO;                       /* YES - CONTINUE        @ZA12744*/ 00225000
*                                                                  0420 00226000
*           /*********************************************************/ 00227000
*           /*                                                       */ 00228000
*           /* SEGMENT(ZEROALL)REQUEST                               */ 00229000
*           /*                                                       */ 00230000
*           /*********************************************************/ 00231000
*                                                                  0421 00232000
*           DO;                                                    0421 00233000
*             GOTO ZEROALL;                                        0422 00234000
