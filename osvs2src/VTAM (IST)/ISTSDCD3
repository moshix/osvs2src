@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
       USING @DATD,11                                                   00014000
*   DATREG=ADDR(MLCDAT4);           /* ADDRESS FOR DATA              */ 00015000
         LA    DATREG,MLCDAT4(,MLCAPTR)                            0366 00016000
*/********************************************************************/ 00017000
*/* NOW DO LINKAGE                                                   */ 00018000
*/********************************************************************/ 00019000
*                                                                  0367 00020000
*   SAVCLLD=ADDR(MLCSAV4);          /* SAVE OUR SAVE ADDR IN CALLERS */ 00021000
         LA    @14,MLCSAV4(,MLCAPTR)                               0367 00022000
         ST    @14,SAVCLLD(,SAVREG)                                0367 00023000
*   MLCSAV4(2)=SAVREG;              /* SAVE CALLERS SAVE ADDR IN OURS*/ 00024000
         ST    SAVREG,MLCSAV4+4(,MLCAPTR)                          0368 00025000
*   SAVREG=ADDR(MLCSAV4);           /* SET UP OUR SAVE ADDR          */ 00026000
         LA    SAVREG,MLCSAV4(,MLCAPTR)                            0369 00027000
*                                                                  0370 00028000
*/********************************************************************/ 00029000
*/* NOW BEGIN THE REAL PROCESSING OF THIS MODULE                     */ 00030000
*/********************************************************************/ 00031000
*                                                                  0370 00032000
*   ISTLDPRM=ISTLDPRM&&ISTLDPRM;    /* ZERO PARM LIST                */ 00033000
         LA    @14,1284                                            0370 00034000
