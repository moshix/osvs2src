*   MODCHAIN=0;                                                    0375 00049000
         ST    @14,MODCHAIN                                        0375 00050000
*   USSCHAIN=0;                                                    0376 00051000
         ST    @14,USSCHAIN                                        0376 00052000
*   RCSAVE=0;                                                      0377 00053000
         ST    @14,RCSAVE                                          0377 00054000
*   CHAINPTR=0;                                                    0378 00055000
*                                                                  0378 00056000
         SLR   CHAINPTR,CHAINPTR                                   0378 00057000
*/********************************************************************/ 00058000
*/*                                                                  */ 00059000
*/*  THIS IS THE MAIN ROUTINE THAT DRIVES ALL OF THE ADD             */ 00060000
*/*  AND DELETE LOGIC FOR THE TABLES IN THE RDTES                    */ 00061000
*/*                                                                  */ 00062000
*/********************************************************************/ 00063000
*                                                                  0379 00064000
*   RDTEPTR=MLCSKEL;                /* SKELETON RDTE                 */ 00065000
         L     RDTEPTR,MLCSKEL(,MLCAPTR)                           0379 00066000
*/*  DOUNTIL(RPRENTED='1'B|RCSAVE^=0)                                */ 00067000
*                                                                  0380 00068000
*   DO UNTIL(RPRENTED='1'B|RCSAVE^=0);/* LOOP THRU RDT SEGMENT       */ 00069000
@DL00380 DS    0H                                                  0381 00070000
*     RDTEPTR=RDTEPTR+RPRELEN;      /* SKIP FIRST RDTE AND ALWAYS GET   00071000
*                                      THE NEXT RDTE ADDRESS         */ 00072000
         AH    RDTEPTR,RPRELEN(,RDTEPTR)                           0381 00073000
*     IF RPRENTRY=RPRENTTR|         /* IS RDTE A TERM ENTRY?         */ 00074000
*         RPRENTRY=RPRENTUT|        /* OR IS IT A UTERM ENTRY?       */ 00075000
*         RPRENTRY=RPRENTCM|        /* OR IS IT A COMP ENTRY?        */ 00076000
*         RPRENTRY=RPRENTLU         /* OR IS IT A LU ENTRY?          */ 00077000
*       THEN                                                       0382 00078000
         CLI   RPRENTRY(RDTEPTR),X'80'                             0382 00079000
