*       ;                           /* CONTINUE TO LOOK FOR AN RDTE     00172000
*                                      WITH A TABLE                  */ 00173000
@RF00382 DS    0H                                                  0409 00174000
*/********************************************************************/ 00175000
*/*                                                                  */ 00176000
*/*      END OF IN-LINE ROUTINE: TBLDPINT                            */ 00177000
*/*                                                                  */ 00178000
*/********************************************************************/ 00179000
*                                                                  0409 00180000
*   END;                            /* END(DOUNTIL)                  */ 00181000
@DE00380 TM    RPRENTED(RDTEPTR),B'00000100'                       0409 00182000
         BO    @DC00380                                            0409 00183000
         L     @14,RCSAVE                                          0409 00184000
         LTR   @14,@14                                             0409 00185000
         BZ    @DL00380                                            0409 00186000
@DC00380 DS    0H                                                  0410 00187000
*#DC00004:                                                         0410 00188000
*   ;                               /* LAST RDTE FOUND - END MAIN  0410 00189000
*                                      PROCESSING                    */ 00190000
#DC00004 DS    0H                                                  0411 00191000
*   IF RCSAVE^=0                    /* CHECK TO SEE IF ALL PROCESSING   00192000
*                                      OK                            */ 00193000
*     THEN                                                         0411 00194000
