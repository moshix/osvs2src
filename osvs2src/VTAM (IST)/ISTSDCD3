@RT00382 DS    0H                                                  0383 00087000
*       DO;                         /* YES. IT IS AN ENTRY TYPE THAT    00088000
*                                      MAY HAVE A TABLE TO LOAD      */ 00089000
*                                                                  0384 00090000
*/********************************************************************/ 00091000
*/*                                                                  */ 00092000
*/*     TBLDPINT:                                                    */ 00093000
*/* THIS CODE INITIALIZES THE PARAMETERS FOR THE TABLOAD             */ 00094000
*/* ROUTINE FOR EACH TYPE OF TABLE THAT MIGHT BE LOADED.             */ 00095000
*/*                                                                  */ 00096000
*/********************************************************************/ 00097000
*                                                                  0384 00098000
*         NAMEDISP=ADDR(RCPLGNCT)-RDTEPTR;/* GET DISPLACEMENT INTO 0384 00099000
*                                      RDTE FOR TABLE NAME           */ 00100000
         MVC   NAMEDISP(4),@CF02905                                0384 00101000
*         ADDRDISP=ADDR(RCPINTA)-RDTEPTR;/* GET DISPLACEMENT INTO  0385 00102000
*                                      RDTE FOR TABLE ADDRESS        */ 00103000
         MVC   ADDRDISP(4),@CF02906                                0385 00104000
*         CHAINDSP=ADDR(RPRNCBA)-RDTEPTR;/* GET DISPLACEMENT INTO  0386 00105000
*                                      RDTE FOR TABLE CHAIN          */ 00106000
         LA    CHAINDSP,40                                         0386 00107000
*         CHAINPTR=ADDR(INTCHAIN);  /* POINT TO INTERPRET TABLE CHAIN   00108000
*                                      TO INDICAT WHICH TABLE TO LOAD*/ 00109000
         LA    CHAINPTR,INTCHAIN                                   0387 00110000
*         CALL TABLOAD;             /* GO LOAD ANY INTERPRET TABLE 0388 00111000
*                                      THAT MAY EXIST IN THIS RDTE   */ 00112000
         BAL   @14,TABLOAD                                         0388 00113000
*         IF RCSAVE=0&RPRENTRY=RPRENTLU/* 1ST TABLE LOAD OK AND THE     00114000
*                                      RDTE AN LU?                   */ 00115000
*           THEN                                                   0389 00116000
         L     @14,RCSAVE                                          0389 00117000
