         LM    @00,@12,20(@13)                                     0427 00249000
         BR    @14                                                 0427 00250000
*/********************************************************************/ 00251000
*/*  END OF PROCESSING FOR THIS RDT                                  */ 00252000
*/********************************************************************/ 00253000
*                                                                  0428 00254000
*TABLOAD:                                                          0428 00255000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0428 00256000
*                                                                  0428 00257000
TABLOAD  DS    0H                                                  0429 00258000
*/********************************************************************/ 00259000
*/*                                                                  */ 00260000
*/*  THIS ROUTINE DETERMINES IF THERE IS ANY TABLE TO LOAD/          */ 00261000
*/* DELETE AND SET UP THE PARAMENTERS FOR LOADRT AND ALOAD/          */ 00262000
*/* ADELETE                                                          */ 00263000
*/*                                                                  */ 00264000
*/********************************************************************/ 00265000
*                                                                  0429 00266000
*   RFY                                                            0429 00267000
*     GPR14 RSTD;                                                  0429 00268000
*   SAVADDR1=GPR14;                 /* SAVE REG 14                   */ 00269000
         ST    GPR14,SAVADDR1                                      0430 00270000
*   RFY                                                            0431 00271000
*     GPR14 UNRSTD;                                                0431 00272000
*   IF EVAL(RDTEPTR+NAMEDISP)->TABLNAME^=BLANKS/* IS THERE A TABLE 0432 00273000
*                                      SPECIFIED                     */ 00274000
*     THEN                                                         0432 00275000
         LR    @14,RDTEPTR                                         0432 00276000
         AL    @14,NAMEDISP                                        0432 00277000
         CLC   TABLNAME(8,@14),BLANKS                              0432 00278000
         BE    @RF00432                                            0432 00279000
*     DO;                           /* YES. THERE IS A TABLE TO CHECK*/ 00280000
*       TESTPTR=CHAINPTR->BASE1;    /* SET UP POINTER                */ 00281000
         L     TESTPTR,BASE1(,CHAINPTR)                            0434 00282000
*/*     DOWHILE(TESTPTR^=0)                                          */ 00283000
*                                                                  0435 00284000
*       DO WHILE(TESTPTR^=0);       /* CHECK CHAIN TO SEE IF TABLE 0435 00285000
*                                      ALREADY LOADED                */ 00286000
         B     @DE00435                                            0435 00287000
@DL00435 DS    0H                                                  0436 00288000
*         IF EVAL(RDTEPTR+NAMEDISP)->TABLNAME=EVAL(TESTPTR+NAMEDISP)->  00289000
*             TABLNAME              /* CHECK FOR NAME MATCH BETWEEN     00290000
*                                      CURRENT NAME AND NAME IN RDTE    00291000
*                                      ON CHAIN                      */ 00292000
*           THEN                                                   0436 00293000
         L     @14,NAMEDISP                                        0436 00294000
         LR    @08,RDTEPTR                                         0436 00295000
         ALR   @08,@14                                             0436 00296000
         ALR   @14,TESTPTR                                         0436 00297000
         CLC   TABLNAME(8,@08),TABLNAME(@14)                       0436 00298000
         BNE   @RF00436                                            0436 00299000
*           DO;                     /* YES. MATCH FOUND              */ 00300000
*             EVAL(RDTEPTR+ADDRDISP)->TABLADDR=EVAL(TESTPTR+ADDRDISP)-> 00301000
*                 TABLADDR;         /* COPY ADDR OF LOADED TABLE TO     00302000
*                                      ADDR OF CURRENT TABLE         */ 00303000
         L     @14,ADDRDISP                                        0438 00304000
         LR    @08,RDTEPTR                                         0438 00305000
         ALR   @08,@14                                             0438 00306000
         ALR   @14,TESTPTR                                         0438 00307000
         L     @14,TABLADDR(,@14)                                  0438 00308000
         ST    @14,TABLADDR(,@08)                                  0438 00309000
*             TESTPTR=0;            /* SET END OF LOOP INDICATOR     */ 00310000
         SLR   TESTPTR,TESTPTR                                     0439 00311000
*           END;                                                   0440 00312000
*         ELSE                                                     0441 00313000
*           TESTPTR=EVAL(TESTPTR+CHAINDSP)->TABCHAIN;/* WRONG TABLE.    00314000
*                                      UPDATE POINTER TO NEXT TABLE     00315000
*                                      IN CHAIN                      */ 00316000
         B     @RC00436                                            0441 00317000
@RF00436 LR    @14,TESTPTR                                         0441 00318000
         ALR   @14,CHAINDSP                                        0441 00319000
         L     @14,TABCHAIN(,@14)                                  0441 00320000
         LR    TESTPTR,@14                                         0441 00321000
*       END;                        /* END(DOWHILE)                  */ 00322000
@RC00436 DS    0H                                                  0442 00323000
@DE00435 LTR   TESTPTR,TESTPTR                                     0442 00324000
         BNZ   @DL00435                                            0442 00325000
*#DC00008:                                                         0443 00326000
*       ;                                                          0443 00327000
#DC00008 DS    0H                                                  0444 00328000
*       IF(MLCINT='0'B&EVAL(RDTEPTR+ADDRDISP)->TABLADDR^=0)|(MLCINT='1' 00329000
*           &EVAL(RDTEPTR+ADDRDISP)->TABLADDR=0)/* DETERMINE IF A  0444 00330000
*                                      LOAD CALL AND TABLE ADDRES NOT   00331000
*                                      = ZERO OR IF IT IS A DELETE 0444 00332000
*                                      CALL AND TABLE ADDRESS IS ZERO*/ 00333000
*         THEN                                                     0444 00334000
         TM    MLCINT(MLCAPTR),B'10000000'                         0444 00335000
         BNZ   @GL00006                                            0444 00336000
         LR    @14,RDTEPTR                                         0444 00337000
         AL    @14,ADDRDISP                                        0444 00338000
         L     @14,TABLADDR(,@14)                                  0444 00339000
         LTR   @14,@14                                             0444 00340000
         BNZ   @RT00444                                            0444 00341000
@GL00006 TM    MLCINT(MLCAPTR),B'10000000'                         0444 00342000
         BNO   @RF00444                                            0444 00343000
         LR    @14,RDTEPTR                                         0444 00344000
         AL    @14,ADDRDISP                                        0444 00345000
         L     @14,TABLADDR(,@14)                                  0444 00346000
         LTR   @14,@14                                             0444 00347000
         BZ    @RT00444                                            0444 00348000
*         ;                         /* YESYES TO ABOVE TESTS         */ 00349000
*       ELSE                                                       0446 00350000
*         DO;                       /* NO. END OF CHAIN AND NO MATCH.   00351000
*                                      ADD/DELETE TABLE              */ 00352000
@RF00444 DS    0H                                                  0447 00353000
*           LDPMBR=EVAL(RDTEPTR+NAMEDISP)->TABLNAME;/* SET TABLE NAME   00354000
*                                      TO BE LOADED                  */ 00355000
         LR    @14,RDTEPTR                                         0447 00356000
         AL    @14,NAMEDISP                                        0447 00357000
         MVC   LDPMBR+1284(8,MLCAPTR),TABLNAME(@14)                0447 00358000
*           GPR1=ADDR(MLCLDPRM);    /* GET ADDR OF ALOAD/ADELETE   0448 00359000
*                                      PARAMETER STRING              */ 00360000
         LA    GPR1,MLCLDPRM(,MLCAPTR)                             0448 00361000
*           CALL LOADRT;            /* GO LOAD/DELETE THE TABLE      */ 00362000
         BAL   @14,LOADRT                                          0449 00363000
*         END;                                                     0450 00364000
*     END;                                                         0451 00365000
*   ELSE                                                           0452 00366000
*     ;                                                            0452 00367000
@RF00432 DS    0H                                                  0453 00368000
*   RFY                                                            0453 00369000
*     GPR14 RSTD;                                                  0453 00370000
@RC00432 DS    0H                                                  0454 00371000
*   GPR14=SAVADDR1;                 /* RESTORE REG 14                */ 00372000
         L     GPR14,SAVADDR1                                      0454 00373000
*   RFY                                                            0455 00374000
*     GPR14 UNRSTD;                                                0455 00375000
*   END TABLOAD;                                                   0456 00376000
*                                                                  0456 00377000
@EL00002 DS    0H                                                  0456 00378000
@EF00002 DS    0H                                                  0456 00379000
@ER00002 BR    @14                                                 0456 00380000
*/********************************************************************/ 00381000
*/* END OF TABLE LOAD(TABLOAD) PROCEDURE                             */ 00382000
*/********************************************************************/ 00383000
*                                                                  0457 00384000
*LOADRT:                                                           0457 00385000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0457 00386000
*                                                                  0457 00387000
LOADRT   DS    0H                                                  0458 00388000
*/********************************************************************/ 00389000
*/*                                                                  */ 00390000
*/* THIS ROUTINE DETERMINES IF THIS CALL IS A LOAD                   */ 00391000
*/* OR A DELETE AND THEN CALLS ISTINCAL/ISTINCAD AND                 */ 00392000
*/* THEN ADDS THE RDTE TO THE TABLE CHAIN                            */ 00393000
*/*                                                                  */ 00394000
*/********************************************************************/ 00395000
*                                                                  0458 00396000
*   RFY                                                            0458 00397000
*     GPR14 RSTD;                                                  0458 00398000
*   SAVADDR2=GPR14;                 /* SAVE REG 14 IN LOADRT         */ 00399000
         ST    GPR14,SAVADDR2                                      0459 00400000
*   RFY                                                            0460 00401000
*     GPR14 UNRSTD;                                                0460 00402000
*   IF MLCINT='0'B                  /* IS THIS CALL AN ADD?  @D32KCMX*/ 00403000
*     THEN                                                         0461 00404000
         TM    MLCINT(MLCAPTR),B'10000000'                         0461 00405000
         BNZ   @RF00461                                            0461 00406000
*     DO;                           /* YES - LOAD TABLE INTO SYSTEM     00407000
*                                      AND ADD RDTE TO TABLE CHAIN   */ 00408000
*       CALL ISTINCAL;              /* LOAD THE TABLE SPECIFIED IN 0463 00409000
*                                      RDTE                          */ 00410000
         L     @15,@CV02855                                        0463 00411000
         BALR  @14,@15                                             0463 00412000
*       RCSAVE=GPR15;               /* SAVE RETURN CODE              */ 00413000
         ST    GPR15,RCSAVE                                        0464 00414000
*       IF RCSAVE^=0                /* DID LOAD FAIL                 */ 00415000
*         THEN                                                     0465 00416000
         L     @14,RCSAVE                                          0465 00417000
         LTR   @14,@14                                             0465 00418000
         BNZ   @RT00465                                            0465 00419000
*         ;                         /* YES - DO NOTHING MORE         */ 00420000
*       ELSE                                                       0467 00421000
*         DO;                       /* NO - CONTINUE PROCESSING      */ 00422000
*           EVAL(RDTEPTR+CHAINDSP)->TABCHAIN=CHAINPTR->BASE1;/* ADD     00423000
*                                      RDTE TO TABLE CHAIN           */ 00424000
         LR    @14,RDTEPTR                                         0468 00425000
         ALR   @14,CHAINDSP                                        0468 00426000
         L     @08,BASE1(,CHAINPTR)                                0468 00427000
         ST    @08,TABCHAIN(,@14)                                  0468 00428000
*           EVAL(RDTEPTR+ADDRDISP)->TABLADDR=GPR0;/* SET TABLE ADDR     00429000
*                                      INTO CURRENT RDTE             */ 00430000
         LR    @14,RDTEPTR                                         0469 00431000
         AL    @14,ADDRDISP                                        0469 00432000
         ST    GPR0,TABLADDR(,@14)                                 0469 00433000
*           CHAINPTR->BASE1=RDTEPTR;/* UPDATE CHAIN TO POINT TO NEW     00434000
