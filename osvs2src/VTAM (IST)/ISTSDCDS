         TITLE 'ISTSDCDS - VTAM SYSTEM DEFINITION DIALSET MACRO HANDLER*00001000
               R        '                                               00002000
ISTSDCDS CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCDS  75.130'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0161 00014000
*/********************************************************************/ 00015000
*/*                       ENTRY LINKAGE                              */ 00016000
*/********************************************************************/ 00017000
*                                                                  0161 00018000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP DYNAMIC DATA REG       */ 00019000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0161 00020000
*   GENERATE NOREFS NOSETS;                                        0162 00021000
         USING @DATD,DATDREG            @ABILITY FOR DATA               00022000
*   SVBACKWD=ADDR(SAVIN);           /* POINT TO CALLER'S SAVE AREA   */ 00023000
         ST    SAVREG,SVBACKWD(,RNCAREG)                           0163 00024000
*   SVFORWRD=ADDR(SAVOUT);          /* POINT TO NEW SAVE AREA        */ 00025000
         LA    @10,SAVOUT(,RNCAREG)                                0164 00026000
         ST    @10,SVFORWRD(,SAVREG)                               0164 00027000
*   SAVREG=ADDR(SAVOUT);            /* SAVREG READY FOR A CALL OUT   */ 00028000
         LR    SAVREG,@10                                          0165 00029000
*   RFY                                                            0166 00030000
*     SAVIN;                        /* INPUT SAVE AREA NOT @ABLE     */ 00031000
*/********************************************************************/ 00032000
*/*          THE BASE MODEL FOR THE LCDO (LOGICAL CONNECT DIAL       */ 00033000
*/*          OUT) IS CREATED AND SUPPLIED WITH DEFAULT VALUES        */ 00034000
*/*          BY SIFTDEF (ISTSDCRS).  AT ENTRY TO SIFTDEF             */ 00035000
*/*          REG 1 CONTAINS THE RDT ENTRY TYPE.                      */ 00036000
*/********************************************************************/ 00037000
*                                                                  0167 00038000
*   GPR1=RPRENTDS;                  /* RDT ENTRY TYPE                */ 00039000
         LA    GPR1,32                                             0167 00040000
*   CALL ISTSDCRS;                  /* CREATE LCDO MODEL W/ DEFAULTS */ 00041000
         L     @15,@CV01167                                        0168 00042000
         BALR  @14,@15                                             0168 00043000
*/********************************************************************/ 00044000
*/*          UPON RETURN FROM SIFTDEF REG 1 CONTAINS @ OF THE        */ 00045000
*/*          LCDO ENTRY MODEL.                                       */ 00046000
*/*          THE MODEL IS MOVED INTO THE BUILD AREA BY AREA          */ 00047000
*/*          HANDLER (ISTSDCC1).  AT ENTRY TO AREA HANDLER           */ 00048000
*/*            REG 0 CONTAINS 0                                      */ 00049000
*/*            REG 1 CONTAINS @ OF LCDO ENTRY MODEL (JUST SET        */ 00050000
*/*                           BY SIFTDEF)                            */ 00051000
*/********************************************************************/ 00052000
*                                                                  0169 00053000
*   GPR0=0;                                                        0169 00054000
         SLR   GPR0,GPR0                                           0169 00055000
*   CALL ISTSDCC1;                  /* MOVE LCDO MODEL TO BUILD AREA */ 00056000
         L     @15,@CV01166                                        0170 00057000
         BALR  @14,@15                                             0170 00058000
*/********************************************************************/ 00059000
*/*          UPON RETURN FROM AREA HANDLER                           */ 00060000
*/*            REG 0 CONTAINS OFFSET TO LCDO IN THE BUILD AREA       */ 00061000
*/*            REG 1 CONTAINS @ OF LCDO IN THE BUILD AREA            */ 00062000
*/*            REG 15 CONTAINS A RETURN CODE                         */ 00063000
*/*                   0 IF MOVE WAS SUCCESSFUL                       */ 00064000
*/*                  ^0 IF STORAGE NOT AVAILABLE.  IN THIS CASE      */ 00065000
*/*                     A MESSAGE AS BEEN PUT OUT BY AREA            */ 00066000
*/*                     HANDLER,  REGS 0 AND 1 DO NOT CONTAIN        */ 00067000
*/*                     THE OFFSET AND @ INDICATED ABOVE.            */ 00068000
*/********************************************************************/ 00069000
*                                                                  0171 00070000
*   IF GPR15^=0 THEN                                               0171 00071000
         LTR   GPR15,GPR15                                         0171 00072000
         BZ    @RF00171                                            0171 00073000
*     RC=4;                         /* WAS LCDO MOVE SUCCESSFUL?   0172 00074000
*                                      NO-SET RETURN CODE. NEXT INSTR   00075000
*                                      AT 'RETRN'                    */ 00076000
         LA    RC,4                                                0172 00077000
*   ELSE                                                           0173 00078000
*FILLCDO:                                                          0173 00079000
*     DO;                           /* YES-FILL IN LCDO SKELETON     */ 00080000
         B     @RC00171                                            0173 00081000
@RF00171 DS    0H                                                  0173 00082000
FILLCDO  DS    0H                                                  0174 00083000
*       RFY                                                        0174 00084000
*        (ISTRDS,                                                  0174 00085000
*         ISTRPRE) BASED(GPR1);                                    0174 00086000
*                                                                  0174 00087000
*/********************************************************************/ 00088000
*/*          SINCE THE COMMUNICATION ID FIELD IN THE RDT             */ 00089000
*/*          ENTRY PREFIX IS NOT USED PRIOR TO GENEND PRO-           */ 00090000
*/*          CESSING, IT MAY BE USED TO SAVE A POINTER TO THE        */ 00091000
*/*          LCDO.  GENEND WILL USE THE LCDO POINTER PRIOR TO        */ 00092000
*/*          FILLING IN THE COMMUNICATION ID FIELD.                  */ 00093000
*/********************************************************************/ 00094000
*                                                                  0175 00095000
*       RPRCOMID=GPR0;              /* OFFSET TO LCDO                */ 00096000
         ST    GPR0,RPRCOMID(,GPR1)                                0175 00097000
*/********************************************************************/ 00098000
*/*          THE POINTER TO THE DOT (DIAL OUT TABLE) IN A            */ 00099000
*/*          DIALSET (EQUIVALENT TO LCDO) OF THE RDT IS NOT          */ 00100000
*/*          NEEDED UNTIL THE DOT IS CREATED BY GENEND.  THUS        */ 00101000
*/*          THE POINTER TO THE DOT MAY BE USED TO CHAIN THE         */ 00102000
*/*          LCDO'S TOGETHER.                                        */ 00103000
*/********************************************************************/ 00104000
*                                                                  0176 00105000
*       RDSADDOT=RNCDS;             /* PLACE NEW LCDO AT HEAD OF     */ 00106000
         L     @07,RNCDS(,RNCAREG)                                 0176 00107000
         ST    @07,RDSADDOT(,GPR1)                                 0176 00108000
*       RNCDS=ADDR(ISTRDS);         /* LCDO CHAIN                    */ 00109000
         ST    GPR1,RNCDS(,RNCAREG)                                0177 00110000
*       RC=0;                       /* SET VALID RETURN CODE         */ 00111000
         SLR   RC,RC                                               0178 00112000
*     END FILLCDO;                                                 0179 00113000
*RETRN:                                                            0180 00114000
*   SAVREG=SVBACKWD;                /* POINT TO INPUT SAVE AREA      */ 00115000
@RC00171 DS    0H                                                  0180 00116000
RETRN    L     SAVREG,SVBACKWD(,RNCAREG)                           0180 00117000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00118000
         LR    @15,RC                                              0181 00119000
         L     @14,12(,@13)                                        0181 00120000
         LM    @00,@12,20(@13)                                     0181 00121000
         BR    @14                                                 0181 00122000
*/* TPEPILOG                                                         */ 00123000
*                                                                  0182 00124000
*   GEN(PRINT OFF);                                                0182 00125000
         PRINT OFF                                                      00126000
*   GEN(PRINT ON);                                                 0183 00127000
*                                                                  0183 00128000
         PRINT ON                                                       00129000
*/* END OF TPEPILOG                                                  */ 00130000
*                                                                  0184 00131000
*   END ISTSDCDS;                                                  0184 00132000
@EL00001 DS    0H                                                  0184 00133000
@EF00001 DS    0H                                                  0184 00134000
@ER00001 LM    @14,@12,12(@13)                                     0184 00135000
         BR    @14                                                 0184 00136000
@DATA    DS    0H                                                       00137000
@DATD    DSECT                                                          00138000
         DS    0F                                                       00139000
ISTSDCDS CSECT                                                          00140000
         DS    0F                                                       00141000
@DATD    DSECT                                                          00142000
         DS    0D                                                       00143000
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA                00144000
@ENDDATD EQU   *                                                        00145000
ISTSDCDS CSECT                                                          00146000
         DS    0F                                                       00147000
@SIZDATD DC    AL1(0)                                                   00148000
         DC    AL3(@ENDDATD-@DATD)                                      00149000
@CV01166 DC    V(ISTSDCC1)                                              00150000
@CV01167 DC    V(ISTSDCRS)                                              00151000
         DS    0D                                                       00152000
@00      EQU   00                      EQUATES FOR REGISTERS 0-15       00153000
