*                    2 CMZVP138 PTR(31);                        01S1077 02015000
*                                                               01S1077 02016000
*/*XREGSET  PID('PLAREA')PPARM(MLCWRK4)RREG(,REG1138)                */ 02017000
*                                                               01S1078 02018000
*                 DO;                                           01S1078 02019000
*                   REG1138=ADDR(MLCWRK4);/* SET RREG SYMBOL TO 01S1079 02020000
*                                      ADDRESS OF PPARM OPERAND      */ 02021000
         LA    REG1138,MLCWRK4(,MLCAREG)                        01S1079 02022000
*                 END;              /* XREGSET                       */ 02023000
*                 CMZID138=0;                                   01S1081 02024000
         SLR   @10,@10                                          01S1081 02025000
         ST    @10,CMZID138(,REG1138)                           01S1081 02026000
*                 CMZRL138=0;                                   01S1082 02027000
         MVI   CMZRL138(REG1138),X'00'                          01S1082 02028000
*                 CMZRA138=0;                                   01S1083 02029000
