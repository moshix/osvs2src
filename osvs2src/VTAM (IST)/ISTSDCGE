*       END;                        /*                       @D32CKQH*/ 02382000
*       RCSAVE=4;                                               01S1207 02383000
         LA    RCSAVE,4                                         01S1207 02384000
*     END;                                                      01S1208 02385000
*   ELSE                                                        01S1209 02386000
*     DO;                           /* LOAD SUCCESSFUL, RRT ADDR IS     02387000
*                                      IN REG 0                      */ 02388000
         B     @RC01176                                         01S1209 02389000
@RF01176 DS    0H                                               01S1210 02390000
*/********************************************************************/ 02391000
*/* SET PTR TO FIRST RRT ENTRY FOR SCANRRT & CLR PORT NAME SAVEAREA  */ 02392000
*/********************************************************************/ 02393000
*                                                               01S1210 02394000
*       RFY                                                     01S1210 02395000
