         TM    RRNCAMBH(@10),B'00001000'                        01S0592 00541000
         BNZ   @RF00592                                         01S0592 00542000
*       DO;                         /* BHSASSC OPTION NOT SPECIFIED     00543000
*                                      ON SYSCNTRL           @Z40GKBE*/ 00544000
*         MLCNAME=NOLABEL;          /* SET UP MLCA           @Z40GKBE*/ 00545000
         MVC   MLCNAME(8,MLCAREG),@CC03177                      01S0594 00546000
*         MLCKWD=ADDR(OPTKEYWD);    /*                       @Z40GKBE*/ 00547000
