*               RRTEPTR=RRTEPTR+LENGTH(ISTRRTE);                01S1300 02669000
         B     @RC01291                                         01S1300 02670000
@RF01291 AH    RRTEPTR,@CH00032                                 01S1300 02671000
*         END;                                                  01S1301 02672000
@RC01291 DS    0H                                               01S1301 02673000
@RC01257 DS    0H                                               01S1301 02674000
@DE01256 CH    RCSAVE,@CH00042                                  01S1301 02675000
         BE    @DL01256                                         01S1301 02676000
*         IF RCSAVE=4 THEN                                      01S1302 02677000
         CH    RCSAVE,@CH00040                                  01S1302 02678000
         BE    @RT01302                                         01S1302 02679000
*           ;                       /* ERROR, SKIP TO END            */ 02680000
*         ELSE                                                  01S1304 02681000
*           DO;                                                 01S1304 02682000
*             RRTERMNM(1:1)='00'X;  /* MARK THIS RRTE MATCHED        */ 02683000
         MVI   RRTERMNM(RRTEPTR),X'00'                          01S1305 02684000
*             IF RRTEPTR=FIRSTNM    /* IF IT IS FIRST NOT            */ 02685000
*               THEN                                            01S1306 02686000
         CR    RRTEPTR,FIRSTNM                                  01S1306 02687000
         BNE   @RF01306                                         01S1306 02688000
*               DO;                 /* PREVIOUSLY MATCHED, FIND NEXT */ 02689000
*                 DO WHILE RRTERMNM(1:1)='00'X;/* NOT MATCHED.       */ 02690000
         B     @DE01308                                         01S1308 02691000
@DL01308 DS    0H                                               01S1309 02692000
*                   RRTEPTR=RRTEPTR+LENGTH(ISTRRTE);            01S1309 02693000
         AH    RRTEPTR,@CH00032                                 01S1309 02694000
*                 END;                                          01S1310 02695000
@DE01308 CLI   RRTERMNM(RRTEPTR),X'00'                          01S1310 02696000
         BE    @DL01308                                         01S1310 02697000
*                 FIRSTNM=RRTEPTR;                              01S1311 02698000
         LR    FIRSTNM,RRTEPTR                                  01S1311 02699000
*               END;                                            01S1312 02700000
*             ELSE                                              01S1313 02701000
*               ;                                               01S1313 02702000
@RF01306 DS    0H                                               01S1314 02703000
*           END;                                                01S1314 02704000
*         RFY                                                   01S1315 02705000
*           RRTEPTR UNRSTD;                                     01S1315 02706000
@RT01302 DS    0H                                               01S1316 02707000
*       END;                                                    01S1316 02708000
*   END                             /* SCAN                          */ 02709000
*         ;                                                     01S1317 02710000
*                                                               01S1317 02711000
@RT01250 DS    0H                                               01S1317 02712000
@DE01243 LTR   RCSAVE,RCSAVE                                    01S1317 02713000
         BNZ   @DC01243                                         01S1317 02714000
         TM    RPRENTED(RDTEPTR),B'00000100'                    01S1317 02715000
         BNO   @DL01243                                         01S1317 02716000
@DC01243 DS    0H                                               01S1318 02717000
*/* SEGMENT(SCANRRT)FINISH                                           */ 02718000
*                                                               01S1318 02719000
*   DO;                                                         01S1318 02720000
*     GOTO SCANRRT#;                                            01S1319 02721000
         B     SCANRRT#                                         01S1319 02722000
*SCANRRT@:                                                      01S1320 02723000
*   END;                                                        01S1320 02724000
SCANRRT@ DS    0H                                               01S1321 02725000
*                                                               01S1321 02726000
*/********************************************************************/ 02727000
*/* OFFSETS CONVERTS RELATIVE OFFSETS TO REAL OFFSETS.  A            */ 02728000
*/* RELATIVE OFFSET IS ESSENTIALLY THE DISTANCE FROM THE BEGINNING   */ 02729000
*/* OF THE TABLE/STRING AREA TO A PIECE OF INFORMATION WITHIN THE    */ 02730000
*/* TABLE/STRING BUILD AREA (IGNORING THE FWD PTR AND LENGTH USED    */ 02731000
*/* FIELDS AND THE DISCONTINUITIES BETWEEN BUILD AREAS).  A REAL     */ 02732000
*/* OFFSET IS THE DISTANCE BETWEEN THE BEGINNING OF THE RN RDTE AND  */ 02733000
*/* A PIECE OF INFORMATION IN THE AREA BETWEEN THE LAST ENTRY        */ 02734000
*/* AND THE END OF THE SKELETON.  TWO OTHER OFFSETS ARE ALSO         */ 02735000
*/* COMPUTED: THE OFFSET TO THE FIRST GROUP RDTE AND THE OFFSET TO   */ 02736000
*/* THE LAST ENTRY IN THE SKELETON.  THE REASON FOR USING OFFSETS IS */ 02737000
*/* THAT THEY ARE INVARIANT OVER RELOCATION, ALLOWING US TO WRITE    */ 02738000
*/* THE SKELETON TO EXTERNAL STORAGE AND THEN READ IT BACK ANYWHERE  */ 02739000
*/* WITHOUT AFFECTING THEIR VALIDITY.  THE OFFSETS ARE CONVERTED TO  */ 02740000
*/* POINTERS IN A COMPLETED SEGMENT BY CBDEF. SET SWITCHED DAF TO    */ 02741000
*/* REAL DAF EXCEPT FOR THE PX AND LX ENTRIES.                       */ 02742000
*/********************************************************************/ 02743000
*/* SEGMENT(OFFSETS)START                                            */ 02744000
*                                                               01S1321 02745000
*   DO;                                                         01S1321 02746000
*     GOTO OFFSETS@;                                            01S1322 02747000
         B     OFFSETS@                                         01S1322 02748000
*OFFSETS:                                                       01S1323 02749000
*   END;                                                        01S1323 02750000
OFFSETS  DS    0H                                               01S1324 02751000
*   RDTEPTR=MLCSKEL;                                            01S1324 02752000
         L     RDTEPTR,MLCSKEL(,MLCAREG)                        01S1324 02753000
*   RFY                                                         01S1325 02754000
*    (ISTRGP,                                                   01S1325 02755000
*     ISTRLN,                                                   01S1325 02756000
*     ISTRPT,                                                   01S1325 02757000
*     ISTRTR,                                                   01S1325 02758000
*     ISTRCL,                                                   01S1325 02759000
*     ISTRDS) BASED(RDTEPTR);                                   01S1325 02760000
*   RPRSWDAF=RPRDAF;                /*                       @D32CKCK*/ 02761000
         MVC   RPRSWDAF(2,RDTEPTR),RPRDAF(RDTEPTR)              01S1326 02762000
*/*NEXTRDT                                                           */ 02763000
*                                                               01S1327 02764000
*   DO WHILE RPRENTED^=ON;          /* PROCESS ALL RDT ENTRIES       */ 02765000
         B     @DE01327                                         01S1327 02766000
@DL01327 DS    0H                                               01S1328 02767000
*     RDTEPTR=RDTEPTR+RPRELEN;      /* BUMP TO NEXT ENTRY            */ 02768000
         AH    RDTEPTR,RPRELEN(,RDTEPTR)                        01S1328 02769000
*     IF RPRENTRY=RPRENTPX          /* PX OR LX?             @D32CKCK*/ 02770000
*         |RPRENTRY=RPRENTLX        /*                       @D32CKCK*/ 02771000
*       THEN                                                    01S1329 02772000
         CLI   RPRENTRY(RDTEPTR),X'72'                          01S1329 02773000
         BE    @RT01329                                         01S1329 02774000
         CLI   RPRENTRY(RDTEPTR),X'10'                          01S1329 02775000
         BE    @RT01329                                         01S1329 02776000
*       ;                           /*                       @D32CKCK*/ 02777000
*     ELSE                                                      01S1331 02778000
*       RPRSWDAF=RPRDAF;            /*                       @D32CKCK*/ 02779000
         MVC   RPRSWDAF(2,RDTEPTR),RPRDAF(RDTEPTR)              01S1331 02780000
*     IF RPRENTRY=RPRENTTR          /* PROCESS OFFSETS               */ 02781000
*         |RPRENTRY=RPRENTUT        /* FOR UTERM AND                 */ 02782000
*         |RPRENTRY=RPRENTCM        /* COMPONENT OR                  */ 02783000
*       THEN                                                    01S1332 02784000
@RT01329 CLI   RPRENTRY(RDTEPTR),X'80'                          01S1332 02785000
         BE    @RT01332                                         01S1332 02786000
         CLI   RPRENTRY(RDTEPTR),X'75'                          01S1332 02787000
         BE    @RT01332                                         01S1332 02788000
         CLI   RPRENTRY(RDTEPTR),X'90'                          01S1332 02789000
         BNE   @RF01332                                         01S1332 02790000
@RT01332 DS    0H                                               01S1333 02791000
*       DO;                         /* TERM ENTRIES                  */ 02792000
*         IF RTRCTDIA=0 THEN                                    01S1334 02793000
         ICM   @10,3,RTRCTDIA(RDTEPTR)                          01S1334 02794000
         BZ    @RT01334                                         01S1334 02795000
*           ;                                                   01S1335 02796000
*         ELSE                                                  01S1336 02797000
*           RTRDIAA=RTRDIAA+RNCOFFST;                           01S1336 02798000
         L     @10,RTRDIAA(,RDTEPTR)                            01S1336 02799000
         AL    @10,RNCOFFST(,RNCAREG)                           01S1336 02800000
         ST    @10,RTRDIAA(,RDTEPTR)                            01S1336 02801000
*         IF RTRCTPOL=0 THEN                                    01S1337 02802000
@RT01334 ICM   @10,3,RTRCTPOL(RDTEPTR)                          01S1337 02803000
         BZ    @RT01337                                         01S1337 02804000
*           ;                                                   01S1338 02805000
*         ELSE                                                  01S1339 02806000
*           RTRPOLA=RTRPOLA+RNCOFFST;                           01S1339 02807000
         L     @10,RTRPOLA(,RDTEPTR)                            01S1339 02808000
         AL    @10,RNCOFFST(,RNCAREG)                           01S1339 02809000
         ST    @10,RTRPOLA(,RDTEPTR)                            01S1339 02810000
*       END;                                                    01S1340 02811000
*     ELSE                                                      01S1341 02812000
*       IF RPRENTRY=RPRENTDS THEN                               01S1341 02813000
         B     @RC01332                                         01S1341 02814000
@RF01332 CLI   RPRENTRY(RDTEPTR),X'20'                          01S1341 02815000
         BNE   @RF01341                                         01S1341 02816000
*         IF RDSADDOT=0             /* ADD OFFSET TO DIALSET DOT     */ 02817000
