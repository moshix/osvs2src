*                 CMZVP138=0;                                   01S1085 02033000
         ST    @10,CMZVP138(,REG1138)                           01S1085 02034000
*                 DCL                                           01S1086 02035000
*                   CMZAA138 CHAR(4) INIT('337I');              01S1086 02036000
*                 CMZID138=ADDR(CMZAA138);                      01S1087 02037000
         LA    @10,CMZAA138                                     01S1087 02038000
         ST    @10,CMZID138(,REG1138)                           01S1087 02039000
*                 CMZVP138=ADDR(MLCERBF1);                      01S1088 02040000
         LA    @10,MLCERBF1(,MLCAREG)                           01S1088 02041000
         ST    @10,CMZVP138(,REG1138)                           01S1088 02042000
*                 DCL                                           01S1089 02043000
*                   CMZSA138(18) PTR(31) BASED(EMCR13);         01S1089 02044000
*                 CMZSA138(5)=ADDR(NCSPLSID);                   01S1090 02045000
         L     @10,MLCNCSPL(,MLCAREG)                           01S1090 02046000
         LA    @10,NCSPLSID(,@10)                               01S1090 02047000
         ST    @10,CMZSA138+16(,EMCR13)                         01S1090 02048000
*                 DCL                                           01S1091 02049000
*                   REG15138 REG(15) PTR;                       01S1091 02050000
*                 RFY                                           01S1092 02051000
*                   REG15138 RSTD;                              01S1092 02052000
*                 DO;               /* LISATCVT                 01S1093 02053000
*                                      CVTBASE(PAGEABLE,(REG15138))  */ 02054000
*                   REG15138=ATCLCPTR;/* SET UP ATCVT POINTER        */ 02055000
         L     REG15138,ATCLCPTR                                01S1094 02056000
*                 END;              /* LISATCVT                      */ 02057000
*                 DCL                                           01S1096 02058000
*                   CMTA138 BASED(REG15138) ENTRY VALUERANGE(*);        02059000
*                 REG15138=REG15138->ATCMSGP;                   01S1097 02060000
         L     REG15138,ATCMSGP(,REG15138)                      01S1097 02061000
*                 CALL CMTA138;                                 01S1098 02062000
         BALR  @14,@15                                          01S1098 02063000
*                 RESPECIFY                                     01S1099 02064000
*                  (REG1138,                                    01S1099 02065000
*                   REG15138) UNRESTRICTED;                     01S1099 02066000
*                 RFY                                           01S1100 02067000
*                  (EMCR13) UNRSTD;                             01S1100 02068000
*               END;                                            01S1101 02069000
*               RCSAVE=4;           /* SET ERROR RC.                 */ 02070000
         LA    RCSAVE,4                                         01S1102 02071000
*             END;                                              01S1103 02072000
*       END;                                                    01S1104 02073000
@RT01069 DS    0H                                               01S1104 02074000
@RT01067 DS    0H                                               01S1105 02075000
*     LNTABLEP=LNTABLEP+4;          /* GET NEXT DITT/DIVT            */ 02076000
@RC01018 AH    LNTABLEP,@CH00040                                01S1105 02077000
*     LNPTR=LNTABENT;               /* LINE RDTE.                    */ 02078000
         L     LNPTR,LNTABENT(,LNTABLEP)                        01S1106 02079000
*   END                             /* DODTDV                        */ 02080000
*         ;                                                     01S1107 02081000
@DE01015 SLR   @10,@10                                          01S1107 02082000
         CR    LNPTR,@10                                        01S1107 02083000
         BE    @DC01015                                         01S1107 02084000
         CR    RCSAVE,@10                                       01S1107 02085000
         BE    @DL01015                                         01S1107 02086000
@DC01015 DS    0H                                               01S1108 02087000
*   RFY                                                         01S1108 02088000
*    (LNPTR,                                                    01S1108 02089000
*     DDPTR) UNRSTD;                                            01S1108 02090000
*                                                               01S1108 02091000
*/* SEGMENT(CHKDTDV)FINISH                                           */ 02092000
*                                                               01S1109 02093000
*   DO;                                                         01S1109 02094000
*     GOTO CHKDTDV#;                                            01S1110 02095000
         B     CHKDTDV#                                         01S1110 02096000
*CHKDTDV@:                                                      01S1111 02097000
*   END;                                                        01S1111 02098000
CHKDTDV@ DS    0H                                               01S1112 02099000
*                                                               01S1112 02100000
*/********************************************************************/ 02101000
*/* RN2BHID COMPLETES THE WORK OF RNSKEL1 BY MOVING THE              */ 02102000
*/* TABLES AND STRINGS INTO THE RDT EXTENSION. THEN IT MATCHES       */ 02103000
*/* ALLOCATION ENTRIES (UTERM, TERM AND COMPONENT) TO BHSET NAMES    */ 02104000
*/* AND MOVES THE ID'S TO THE ALLOCATION PREFIX.                     */ 02105000
*/********************************************************************/ 02106000
*/* SEGMENT(RN2BHID)START                                            */ 02107000
*                                                               01S1112 02108000
*   DO;                                                         01S1112 02109000
*     GOTO RN2BHID@;                                            01S1113 02110000
         B     RN2BHID@                                         01S1113 02111000
*RN2BHID:                                                       01S1114 02112000
*   END;                                                        01S1114 02113000
RN2BHID  DS    0H                                               01S1115 02114000
*   BLOCKPTR=RNCFTBLS;                                          01S1115 02115000
         L     BLOCKPTR,RNCFTBLS(,RNCAREG)                      01S1115 02116000
*   RFY                                                         01S1116 02117000
*    (MVCLR1A,                                                  01S1116 02118000
*     MVCLR1L,                                                  01S1116 02119000
*     MVCLR2A,                                                  01S1116 02120000
*     MVCLR2L) RSTD;                                            01S1116 02121000
*   MVCLR1A=MLCSKEL+RNCOFFST;                                   01S1117 02122000
         L     MVCLR1A,MLCSKEL(,MLCAREG)                        01S1117 02123000
         AL    MVCLR1A,RNCOFFST(,RNCAREG)                       01S1117 02124000
*   DO WHILE BLOCKPTR^=0;           /* MOVE ALL BUILD AREAS INTO SKEL*/ 02125000
         B     @DE01118                                         01S1118 02126000
@DL01118 DS    0H                                               01S1119 02127000
*     MVCLR1L=BLDARLNG;             /* BUILD AREA LENGTH             */ 02128000
         L     MVCLR1L,BLDARLNG(,BLOCKPTR)                      01S1119 02129000
*     MVCLR2A=ADDR(BLDAREA);        /* BUILD AREA ADDRESS            */ 02130000
         LA    MVCLR2A,BLDAREA(,BLOCKPTR)                       01S1120 02131000
*     MVCLR2L=MVCLR1L;                                          01S1121 02132000
         LR    MVCLR2L,MVCLR1L                                  01S1121 02133000
*     MVCL(MVCLR1A,MVCLR2A);                                    01S1122 02134000
*                                                               01S1122 02135000
         MVCL  MVCLR1A,MVCLR2A                                  01S1122 02136000
*/********************************************************************/ 02137000
*/*    MVCL INCREMENTS RDT PTR (MVCLR1A) TO NEXT AVAILABLE SPACE     */ 02138000
*/********************************************************************/ 02139000
*                                                               01S1123 02140000
*     BLOCKPTR=BLDARFWD;                                        01S1123 02141000
         L     BLOCKPTR,BLDARFWD(,BLOCKPTR)                     01S1123 02142000
*   END;                                                        01S1124 02143000
@DE01118 SLR   @10,@10                                          01S1124 02144000
         CR    BLOCKPTR,@10                                     01S1124 02145000
         BNE   @DL01118                                         01S1124 02146000
*   RFY                                                         01S1125 02147000
*    (MVCLR1A,                                                  01S1125 02148000
*     MVCLR1L,                                                  01S1125 02149000
*     MVCLR2A,                                                  01S1125 02150000
*     MVCLR2L) UNRSTD;                                          01S1125 02151000
*                                                               01S1125 02152000
*/********************************************************************/ 02153000
*/*            PROCESS BHSET ID'S                                    */ 02154000
*/********************************************************************/ 02155000
*                                                               01S1126 02156000
*   RFY                                                         01S1126 02157000
*    (ISTRPRE,                                                  01S1126 02158000
*     ISTRCPRE) BASED(RDTEPTR);                                 01S1126 02159000
*   RFY                                                         01S1127 02160000
*     RDTEPTR RSTD;                                             01S1127 02161000
*   IF RRNCTBHS=0                   /* IF THERE ARE ANY              */ 02162000
*     THEN                                                      01S1128 02163000
         L     @06,MLCSKEL(,MLCAREG)                            01S1128 02164000
         CH    @10,RRNCTBHS(,@06)                               01S1128 02165000
         BE    @RT01128                                         01S1128 02166000
*     ;                             /* BHSETS, START AT THE          */ 02167000
*   ELSE                                                        01S1130 02168000
*     DO;                           /* FIRST RDT ENTRY,              */ 02169000
*       RRNBHSET=RRNBHSET+RNCOFFST;                             01S1131 02170000
         L     @10,MLCSKEL(,MLCAREG)                            01S1131 02171000
         L     @06,RRNBHSET(,@10)                               01S1131 02172000
         AL    @06,RNCOFFST(,RNCAREG)                           01S1131 02173000
         ST    @06,RRNBHSET(,@10)                               01S1131 02174000
*       RDTEPTR=MLCSKEL;            /* THEN LOOP THROUGH ALL         */ 02175000
         LR    RDTEPTR,@10                                      01S1132 02176000
*       DO WHILE RPRENTED=OFF;      /* ENTRIES, LOOKING FOR          */ 02177000
         B     @DE01133                                         01S1133 02178000
@DL01133 DS    0H                                               01S1134 02179000
*         RDTEPTR=RDTEPTR+RPRELEN;                              01S1134 02180000
         AH    RDTEPTR,RPRELEN(,RDTEPTR)                        01S1134 02181000
*         IF RPRENTRY=RPRENTUT|     /* UTERMS, TERMS AND             */ 02182000
*             RPRENTRY=RPRENTTR|    /* COMPONENTS.                   */ 02183000
*             RPRENTRY=RPRENTCM THEN                            01S1135 02184000
         CLI   RPRENTRY(RDTEPTR),X'75'                          01S1135 02185000
         BE    @RT01135                                         01S1135 02186000
         CLI   RPRENTRY(RDTEPTR),X'80'                          01S1135 02187000
         BE    @RT01135                                         01S1135 02188000
         CLI   RPRENTRY(RDTEPTR),X'90'                          01S1135 02189000
         BNE   @RF01135                                         01S1135 02190000
@RT01135 DS    0H                                               01S1136 02191000
*           DO;                                                 01S1136 02192000
*             IF RTRBHSN1=BLANKS THEN                           01S1137 02193000
         CLC   RTRBHSN1(8,RDTEPTR),@CC03177                     01S1137 02194000
         BE    @RT01137                                         01S1137 02195000
*               ;                                               01S1138 02196000
*             ELSE                                              01S1139 02197000
*               DO;                                             01S1139 02198000
*                 RFY                                           01S1140 02199000
*                   BHSEPTR RSTD;                               01S1140 02200000
*                 BHSEPTR=RRNBHSET+MLCSKEL;                     01S1141 02201000
         L     @10,MLCSKEL(,MLCAREG)                            01S1141 02202000
         L     BHSEPTR,RRNBHSET(,@10)                           01S1141 02203000
         ALR   BHSEPTR,@10                                      01S1141 02204000
*                 DO WHILE BHSNAME^=RTRBHSN1&BHSNAME(1:1)^='FF'X;       02205000
         B     @DE01142                                         01S1142 02206000
@DL01142 DS    0H                                               01S1143 02207000
*                   BHSEPTR=BHSEPTR+LENGTH(ISTBHSAT);           01S1143 02208000
         AH    BHSEPTR,@CH02277                                 01S1143 02209000
*                 END;                                          01S1144 02210000
@DE01142 CLC   BHSNAME(8,BHSEPTR),RTRBHSN1(RDTEPTR)             01S1144 02211000
         BE    @DC01142                                         01S1144 02212000
         CLI   BHSNAME(BHSEPTR),X'FF'                           01S1144 02213000
         BNE   @DL01142                                         01S1144 02214000
@DC01142 DS    0H                                               01S1145 02215000
*                 IF BHSNAME(1:1)^='FF'X THEN                   01S1145 02216000
         CLI   BHSNAME(BHSEPTR),X'FF'                           01S1145 02217000
         BE    @RF01145                                         01S1145 02218000
*                   DO;                                         01S1146 02219000
*                     RCPBHSI1=BHSINDEX;                        01S1147 02220000
         MVC   RCPBHSI1(1,RDTEPTR),BHSINDEX(BHSEPTR)            01S1147 02221000
*                     RCPBHSI2=BHSINDEX;                        01S1148 02222000
         MVC   RCPBHSI2(1,RDTEPTR),BHSINDEX(BHSEPTR)            01S1148 02223000
*                   END;                                        01S1149 02224000
*                 ELSE                                          01S1150 02225000
*                   ;                                           01S1150 02226000
@RF01145 DS    0H                                               01S1151 02227000
*                 RFY                                           01S1151 02228000
*                   BHSEPTR UNRSTD;                             01S1151 02229000
*               END;                                            01S1152 02230000
*           END;                                                01S1153 02231000
*         ELSE                                                  01S1154 02232000
*           ;                                                   01S1154 02233000
@RF01135 DS    0H                                               01S1155 02234000
*       END;                                                    01S1155 02235000
@RC01135 DS    0H                                               01S1155 02236000
@DE01133 TM    RPRENTED(RDTEPTR),B'00000100'                    01S1155 02237000
         BZ    @DL01133                                         01S1155 02238000
*     END;                                                      01S1156 02239000
*   RFY                                                         01S1157 02240000
*     RDTEPTR UNRSTD;                                           01S1157 02241000
*                                                               01S1157 02242000
@RT01128 DS    0H                                               01S1158 02243000
*/* SEGMENT(RN2BHID)FINISH                                           */ 02244000
*                                                               01S1158 02245000
*   DO;                                                         01S1158 02246000
*     GOTO RN2BHID#;                                            01S1159 02247000
         B     RN2BHID#                                         01S1159 02248000
*RN2BHID@:                                                      01S1160 02249000
*   END;                                                        01S1160 02250000
RN2BHID@ DS    0H                                               01S1161 02251000
*                                                               01S1161 02252000
*/********************************************************************/ 02253000
*/* RRTRTN CONSTRUCTS THE RESOURCE RESOLUTION TABLE NAME             */ 02254000
*/* IN A FASHION SIMILAR TO THAT OF THE BHSAT AND THEN LOADS IT.     */ 02255000
*/********************************************************************/ 02256000
*/* SEGMENT(RRTRTN)START                                             */ 02257000
*                                                               01S1161 02258000
*   DO;                                                         01S1161 02259000
*     GOTO RRTRTN@;                                             01S1162 02260000
         B     RRTRTN@                                          01S1162 02261000
*RRTRTN:                                                        01S1163 02262000
*   END;                                                        01S1163 02263000
RRTRTN   DS    0H                                               01S1164 02264000
*   RFY                                                         01S1164 02265000
*     LDDELPTR RSTD;                /* SET UP PARAMETERS             */ 02266000
*   LDDELPTR=ADDR(RNCLDPRM);        /* TO CALL ALOAD TO              */ 02267000
         LR    LDDELPTR,RNCAREG                                 01S1165 02268000
         AL    LDDELPTR,@CF03549                                01S1165 02269000
*   LDPDCBA=ADDR(RNCLMDCB);         /* LOAD RESOURCE                 */ 02270000
         LA    @10,RNCLMDCB(,RNCAREG)                           01S1166 02271000
         ST    @10,LDPDCBA(,LDDELPTR)                           01S1166 02272000
*   LDPPDS=RRNNMNCP;                /* RESOLUTION TABLE              */ 02273000
         L     @10,MLCSKEL(,MLCAREG)                            01S1167 02274000
         MVC   LDPPDS(8,LDDELPTR),RRNNMNCP(@10)                 01S1167 02275000
*   LDPMBR=RRNNCPNM;                /* NCP NAME IS 1-7 CHARS -       */ 02276000
         MVI   LDPMBR+7(LDDELPTR),C' '                          01S1168 02277000
         MVC   LDPMBR(7,LDDELPTR),RRNNCPNM(@10)                 01S1168 02278000
*   DO CTR3=2 BY 1 WHILE LDPMBR(CTR3:CTR3)^=' ';/* - REPLACE         */ 02279000
         LA    CTR3,2                                           01S1169 02280000
         B     @DE01169                                         01S1169 02281000
@DL01169 DS    0H                                               01S1170 02282000
*   END;                            /* FIRST BLANK WITH 'R'          */ 02283000
         AH    CTR3,@CH00049                                    01S1170 02284000
@DE01169 LR    @10,LDDELPTR                                     01S1170 02285000
         ALR   @10,CTR3                                         01S1170 02286000
         AL    @10,@CF03418                                     01S1170 02287000
         CLI   LDPMBR(@10),C' '                                 01S1170 02288000
         BNE   @DL01169                                         01S1170 02289000
*   LDPMBR(CTR3:CTR3)='R';          /* FOR MEMBER NAME               */ 02290000
         LR    @10,LDDELPTR                                     01S1171 02291000
         ALR   @10,CTR3                                         01S1171 02292000
