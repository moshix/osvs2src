         ST    @06,RRNLASTE(,@10)                               01S1355 02873000
*   RFY                                                         01S1356 02874000
*     RDTEPTR UNRSTD;                                           01S1356 02875000
*   RCSAVE=8;                       /* RETURN OF EIGHT IS SUCCESSFUL    02876000
*                                      END OF GEN                    */ 02877000
         LA    RCSAVE,8                                         01S1357 02878000
*/* SEGMENT(OFFSETS)FINISH                                           */ 02879000
*                                                               01S1358 02880000
*   DO;                                                         01S1358 02881000
*     GOTO OFFSETS#;                                            01S1359 02882000
         B     OFFSETS#                                         01S1359 02883000
*OFFSETS@:                                                      01S1360 02884000
*   END;                                                        01S1360 02885000
OFFSETS@ DS    0H                                               01S1361 02886000
*                                                               01S1361 02887000
*/********************************************************************/ 02888000
*/*     1. CREATE VTAM ONLY CID'S IN DUMMY TERMINAL RDTE'S.          */ 02889000
*/*     2. CREATE FALSE NAMES IN DUMMY TERM RDTE'S FOR USE BY VARY.  */ 02890000
*/*     3. CREATE CID'S IN DUMMY LX RDTE'S.                          */ 02891000
*/********************************************************************/ 02892000
*/* SEGMENT(SPECIUS)START                                            */ 02893000
*                                                               01S1361 02894000
*   DO;                                                         01S1361 02895000
*     GOTO SPECIUS@;                                            01S1362 02896000
         B     SPECIUS@                                         01S1362 02897000
*SPECIUS:                                                       01S1363 02898000
*   END;                                                        01S1363 02899000
SPECIUS  DS    0H                                               01S1364 02900000
*   RFY                                                         01S1364 02901000
*    (ISTRPRE,                                                  01S1364 02902000
*     ISTRGP,                                                   01S1364 02903000
*     ISTRLN,                                                   01S1364 02904000
*     ISTRPT,                                                   01S1364 02905000
*     ISTRTR,                                                   01S1364 02906000
*     ISTRCL) BASED(RDTEPTR);                                   01S1364 02907000
*   RFY                                                         01S1365 02908000
*     ISTRLX BASED(RDTEPTR);        /*                       @D32CKCK*/ 02909000
*   RFY                                                         01S1366 02910000
*     RDTEPTR RSTD;                                             01S1366 02911000
*   RFY                                                         01S1367 02912000
*     ISTDEVCH BASED(ADDR(RPRDEVCH));                           01S1367 02913000
*   SPECIAL=ON;                                                 01S1368 02914000
         OI    SPECIAL,B'01000000'                              01S1368 02915000
*   IF RPRENTRY=RPRENTGP            /* IS THIS GROUP ENTRY           */ 02916000
*       |RPRENTRY=RPRENTDS          /* OR DIALSET ENTRY              */ 02917000
*     THEN                                                      01S1369 02918000
         CLI   RPRENTRY(RDTEPTR),X'30'                          01S1369 02919000
         BE    @RT01369                                         01S1369 02920000
         CLI   RPRENTRY(RDTEPTR),X'20'                          01S1369 02921000
         BE    @RT01369                                         01S1369 02922000
*     ;                             /* THEY DO NOT HAVE CIDS         */ 02923000
*   ELSE                                                        01S1371 02924000
*     IF(RPRENTRY=RPRENTTR&RTRVTERM=ON)|RPRENTRY=RPRENTUT THEN  01S1371 02925000
         CLI   RPRENTRY(RDTEPTR),X'80'                          01S1371 02926000
         BNE   @GL00019                                         01S1371 02927000
         TM    RTRVTERM(RDTEPTR),B'00000100'                    01S1371 02928000
         BO    @RT01371                                         01S1371 02929000
@GL00019 CLI   RPRENTRY(RDTEPTR),X'75'                          01S1371 02930000
         BNE   @RF01371                                         01S1371 02931000
@RT01371 DS    0H                                               01S1372 02932000
*       DO;                         /*                       @DL31HCK*/ 02933000
*         RDTCTSNT=RDTCTSNT+1;      /*                       @DL31HCK*/ 02934000
         L     @10,MLCSKEL(,MLCAREG)                            01S1373 02935000
         LA    @09,1                                            01S1373 02936000
         AH    @09,RDTCTSNT(,@10)                               01S1373 02937000
         STH   @09,RDTCTSNT(,@10)                               01S1373 02938000
*         RPRDAF=RDTCTSNT;          /*                       @DL31HCK*/ 02939000
         STH   @09,RPRDAF(,RDTEPTR)                             01S1374 02940000
