*               END;                /* XREGSET                       */ 01916000
*               CMZID129=0;                                     01S1039 01917000
         SLR   @10,@10                                          01S1039 01918000
         ST    @10,CMZID129(,REG1129)                           01S1039 01919000
*               CMZRL129=0;                                     01S1040 01920000
         MVI   CMZRL129(REG1129),X'00'                          01S1040 01921000
*               CMZRA129=0;                                     01S1041 01922000
