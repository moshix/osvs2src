         B     SPCIAL@                                          01S1406 03040000
*SPCIAL:                                                        01S1407 03041000
*   END;                                                        01S1407 03042000
SPCIAL   DS    0H                                               01S1408 03043000
*   RFY                                                         01S1408 03044000
*    (RDTEPTR,                                                  01S1408 03045000
*     RRTEPTR) RSTD;                                            01S1408 03046000
*   IF RPRENTRY=RPRENTLN&RLNPORT=ON /* IF ENTRY IS A                 */ 03047000
*     THEN                          /* PORT, ON THE FIRST            */ 03048000
         CLI   RPRENTRY(RDTEPTR),X'50'                          01S1409 03049000
         BNE   @RF01409                                         01S1409 03050000
         TM    RLNPORT(RDTEPTR),B'00010000'                     01S1409 03051000
         BNO   @RF01409                                         01S1409 03052000
*     IF NAMESAVE=BLANKS            /* PASS SAVE THE NAME,           */ 03053000
*       THEN                                                    01S1410 03054000
         CLC   NAMESAVE(8),@CC03177                             01S1410 03055000
         BNE   @RF01410                                         01S1410 03056000
*       DO;                         /* FILL IN THE PORT              */ 03057000
*         NAMESAVE=RPRNAME;         /* COM ID DAF AND USE            */ 03058000
         MVC   NAMESAVE(8),RPRNAME(RDTEPTR)                     01S1412 03059000
*         RPRNAME=RPTCTNM;          /* THE PORT NAME FOR A           */ 03060000
         MVC   RPRNAME(8,RDTEPTR),RPTCTNM(RDTEPTR)              01S1413 03061000
*         RPTLNDAF=RRTRSID;         /* SECOND PASS AT THIS           */ 03062000
         MVC   RPTLNDAF(2,RDTEPTR),RRTRSID(RRTEPTR)             01S1414 03063000
*         RDTEPTR=RDTEPTR-RPRPELEN; /* ENTRY. BACK UP TO             */ 03064000
         SH    RDTEPTR,RPRPELEN(,RDTEPTR)                       01S1415 03065000
*       END;                        /* PRIOR RDTE TO LOOP.           */ 03066000
*     ELSE                                                      01S1417 03067000
*       DO;                         /* ON SECOND PASS,               */ 03068000
         B     @RC01410                                         01S1417 03069000
@RF01410 DS    0H                                               01S1418 03070000
*         RPRNAME=NAMESAVE;         /* RESTORE ENTRY NAME,           */ 03071000
         MVC   RPRNAME(8,RDTEPTR),NAMESAVE                      01S1418 03072000
*         NAMESAVE=BLANKS;          /* SET THE ENTRY DAF,            */ 03073000
         MVC   NAMESAVE(8),@CC03177                             01S1419 03074000
*         RPRDAF=RRTRSID;           /* AND SAVE PORT ADDR            */ 03075000
         MVC   RPRDAF(2,RDTEPTR),RRTRSID(RRTEPTR)               01S1420 03076000
*         PORTPTR=RDTEPTR;          /* FOR RELATED VTERMS            */ 03077000
         LR    PORTPTR,RDTEPTR                                  01S1421 03078000
*       END;                        /* AND UTERMS.                   */ 03079000
*/********************************************************************/ 03080000
*/* IF NOT A PORT THEN ITS A 2980 SO MUST HANDLE IT                  */ 03081000
*/********************************************************************/ 03082000
*                                                               01S1423 03083000
*   ELSE                                                        01S1423 03084000
*     DO;                           /* IF NOT A PORT                 */ 03085000
         B     @RC01409                                         01S1423 03086000
@RF01409 DS    0H                                               01S1424 03087000
*       RPRDAF=RRTRSID;             /* SET THE ENTRY DAF             */ 03088000
         MVC   RPRDAF(2,RDTEPTR),RRTRSID(RRTEPTR)               01S1424 03089000
*       TPTR=RDTEPTR-RPRPELEN;      /* POINT AT PREVIOUS ENTRY       */ 03090000
         LR    TPTR,RDTEPTR                                     01S1425 03091000
         SH    TPTR,RPRPELEN(,RDTEPTR)                          01S1425 03092000
*       IF RPRNAME=TPTR->RPRNAME THEN                           01S1426 03093000
         CLC   RPRNAME(8,RDTEPTR),RPRNAME(TPTR)                 01S1426 03094000
         BNE   @RF01426                                         01S1426 03095000
*         DO;                       /* 2980 HAS TWO ENTIES WITH SAME    03096000
*                                      NAME                          */ 03097000
*           RPRNAME(2:8)=TPTR->RPRNAME(1:7);/* RIGHT ONE BYTE        */ 03098000
         MVC   RPRNAME+1(7,RDTEPTR),RPRNAME(TPTR)               01S1428 03099000
*           RPRNAME(1:1)='$';       /* DOLLAR SIGN IN IT             */ 03100000
         MVI   RPRNAME(RDTEPTR),C'$'                            01S1429 03101000
*         END;                                                  01S1430 03102000
*       ELSE                                                    01S1431 03103000
*         ;                                                     01S1431 03104000
@RF01426 DS    0H                                               01S1432 03105000
*     END;                                                      01S1432 03106000
*   RFY                                                         01S1433 03107000
*    (RDTEPTR,                                                  01S1433 03108000
*     RRTEPTR) UNRSTD;                                          01S1433 03109000
*                                                               01S1433 03110000
@RC01409 DS    0H                                               01S1434 03111000
*/* SEGMENT(SPCIAL)FINISH                                            */ 03112000
*                                                               01S1434 03113000
*   DO;                                                         01S1434 03114000
*     GOTO SPCIAL#;                                             01S1435 03115000
         B     SPCIAL#                                          01S1435 03116000
*SPCIAL@:                                                       01S1436 03117000
*   END;                                                        01S1436 03118000
*                                                               01S1436 03119000
SPCIAL@  DS    0H                                               01S1437 03120000
*/* TPEPILOG                                                         */ 03121000
*                                                               01S1437 03122000
*   GEN(PRINT OFF);                                             01S1437 03123000
         PRINT OFF                                                      03124000
*   DECLARE                                                     01S1438 03125000
*     EMCR0 PTR REG(0),                                         01S1438 03126000
*     EMCR1 PTR REG(1),                                         01S1438 03127000
*     EMCR2 PTR REG(2),                                         01S1438 03128000
*     EMCR3 PTR REG(3),                                         01S1438 03129000
*     EMCR4 PTR REG(4),                                         01S1438 03130000
*     EMCR5 PTR REG(5),                                         01S1438 03131000
*     EMCR6 PTR REG(6),                                         01S1438 03132000
*     EMCR7 PTR REG(7),                                         01S1438 03133000
*     EMCR8 PTR REG(8),                                         01S1438 03134000
*     EMCR9 PTR REG(9),                                         01S1438 03135000
*     EMCR10 PTR REG(10),                                       01S1438 03136000
*     EMCR11 PTR REG(11),                                       01S1438 03137000
*     EMCR12 PTR REG(12),                                       01S1438 03138000
*     EMCR13 PTR REG(13),                                       01S1438 03139000
*     EMCR14 PTR REG(14),                                       01S1438 03140000
*     EMCR15 PTR REG(15);                                       01S1438 03141000
*   GEN(PRINT ON);                                              01S1439 03142000
*                                                               01S1439 03143000
         PRINT ON                                                       03144000
*/* END OF TPEPILOG                                                  */ 03145000
*                                                               01S1440 03146000
*   END ISTSDCGE                                                01S1440 03147000
*                                                               01S1440 03148000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 03149000
