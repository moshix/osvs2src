*               DCL                                             01S1044 01928000
*                 CMZAA129 CHAR(4) INIT('338I');                01S1044 01929000
*               CMZID129=ADDR(CMZAA129);                        01S1045 01930000
         LA    @10,CMZAA129                                     01S1045 01931000
         ST    @10,CMZID129(,REG1129)                           01S1045 01932000
*               CMZVP129=ADDR(MLCERBF1);                        01S1046 01933000
         LA    @10,MLCERBF1(,MLCAREG)                           01S1046 01934000
         ST    @10,CMZVP129(,REG1129)                           01S1046 01935000
*               DCL                                             01S1047 01936000
*                 CMZSA129(18) PTR(31) BASED(EMCR13);           01S1047 01937000
*               CMZSA129(5)=ADDR(NCSPLSID);                     01S1048 01938000
         L     @10,MLCNCSPL(,MLCAREG)                           01S1048 01939000
         LA    @10,NCSPLSID(,@10)                               01S1048 01940000
         ST    @10,CMZSA129+16(,EMCR13)                         01S1048 01941000
*               DCL                                             01S1049 01942000
*                 REG15129 REG(15) PTR;                         01S1049 01943000
*               RFY                                             01S1050 01944000
*                 REG15129 RSTD;                                01S1050 01945000
