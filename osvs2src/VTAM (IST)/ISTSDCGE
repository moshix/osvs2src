@RC01341 DS    0H                                               01S1354 02865000
@RC01332 DS    0H                                               01S1354 02866000
@DE01327 TM    RPRENTED(RDTEPTR),B'00000100'                    01S1354 02867000
         BNO   @DL01327                                         01S1354 02868000
*   RRNLASTE=RDTEPTR-MLCSKEL;       /* SET OFFSET TO LAST ENTRY      */ 02869000
         L     @10,MLCSKEL(,MLCAREG)                            01S1355 02870000
         LR    @06,RDTEPTR                                      01S1355 02871000
