*               CALL CMTA129;                                   01S1056 01955000
         BALR  @14,@15                                          01S1056 01956000
*               RESPECIFY                                       01S1057 01957000
*                (REG1129,                                      01S1057 01958000
*                 REG15129) UNRESTRICTED;                       01S1057 01959000
*               RFY                                             01S1058 01960000
*                (EMCR13) UNRSTD;                               01S1058 01961000
*             END;                                              01S1059 01962000
*             RCSAVE=4;                                         01S1060 01963000
         LA    RCSAVE,4                                         01S1060 01964000
*           END;                                                01S1061 01965000
*       END;                                                    01S1062 01966000
*     ELSE                                                      01S1063 01967000
*       DO;                         /* DIVT FLAG OFF, SO             */ 01968000
         B     @RC01018                                         01S1063 01969000
@RF01018 DS    0H                                               01S1064 01970000
*         DO WHILE DTERDTEA^=0      /* LOOP TO FIND IF ANY           */ 01971000
*               &DTELCTI^=DTEND;    /* DITT RDTE PTR IS              */ 01972000
         B     @DE01064                                         01S1064 01973000
@DL01064 DS    0H                                               01S1065 01974000
*           DDPTR=DDPTR+LENGTH(ISTDTE);/* ZERO.                      */ 01975000
         AH    DDPTR,@CH01672                                   01S1065 01976000
*         END;                                                  01S1066 01977000
@DE01064 ICM   @10,15,DTERDTEA(DDPTR)                           01S1066 01978000
         BZ    @DC01064                                         01S1066 01979000
         CLI   DTELCTI(DDPTR),X'FF'                             01S1066 01980000
         BNE   @DL01064                                         01S1066 01981000
@DC01064 DS    0H                                               01S1067 01982000
*         IF DTELCTI=DTEND          /* IF A ZERO RDTE PTR            */ 01983000
*           THEN                                                01S1067 01984000
         CLI   DTELCTI(DDPTR),X'FF'                             01S1067 01985000
         BE    @RT01067                                         01S1067 01986000
*           ;                       /* WAS FOUND, THAT IS            */ 01987000
*         ELSE                      /* OK IN MTA IF A                */ 01988000
*           IF RPTUTERM^=0          /* UTERM EXISTS. BUT A           */ 01989000
*             THEN                                              01S1069 01990000
         ICM   @10,15,RPTUTERM(LNPTR)                           01S1069 01991000
         BNZ   @RT01069                                         01S1069 01992000
*             ;                     /* ZERO RDTE PTR WITH-           */ 01993000
*           ELSE                                                01S1071 01994000
*             DO;                   /* OUT A UTERM KILLS             */ 01995000
*               MLCNAME=RPRNAME;    /* THE BUILD.                    */ 01996000
         MVC   MLCNAME(8,MLCAREG),RPRNAME(LNPTR)                01S1072 01997000
*/*TPMSG MSGID('337I')MSG((ADDR(MLCERBF1)))OPTIONS(NCSPLSID)PLAREA(MLCW 01998000
*K4)                                                                 */ 01999000
*                                                               01S1073 02000000
*               DO;                                             01S1073 02001000
