*                                      CVTBASE(PAGEABLE,(REG15129))  */ 01947000
*                 REG15129=ATCLCPTR;/* SET UP ATCVT POINTER          */ 01948000
         L     REG15129,ATCLCPTR                                01S1052 01949000
*               END;                /* LISATCVT                      */ 01950000
*               DCL                                             01S1054 01951000
*                 CMTA129 BASED(REG15129) ENTRY VALUERANGE(*);  01S1054 01952000
*               REG15129=REG15129->ATCMSGP;                     01S1055 01953000
