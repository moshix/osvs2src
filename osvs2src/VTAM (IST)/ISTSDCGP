*             CMZR0007=ADDR(CMZP0007);/* ADDRESS OF PARAMETER LIST   */ 00668000
         LA    CMZR0007,CMZP0007                                   0385 00669000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 00670000
