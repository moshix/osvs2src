*/*                                                                  */ 01154000
*/*            POLIMIT PARAMETER SUBROUTINE                          */ 01155000
*/*                                                                  */ 01156000
*/* THIS ROUTINE CONVERTS THE FIRST PARM TO HEX AND PUTS IT IN THE   */ 01157000
*/* RDT AND THEN WILL SET THE RDT FLAGS TO INDICATE QUEUE OR WAIT.   */ 01158000
*/*                                                                  */ 01159000
*/********************************************************************/ 01160000
*                                                                  0521 01161000
*GRPPOLMT:                                                         0521 01162000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0521 01163000
GRPPOLMT DS    0H                                                  0522 01164000
*   IF PHRF34NS=0 THEN                                             0522 01165000
         ICM   @09,15,PHRF34NS(PHRREG)                             0522 01166000
         BZ    @RT00522                                            0522 01167000
*     ;                                                            0523 01168000
*   ELSE                                                           0524 01169000
*     DO;                                                          0524 01170000
*       PACK(PHRF34SP(1),PHRF34SP(1));                             0525 01171000
         PACK  PHRF34SP(8,PHRREG),PHRF34SP(8,PHRREG)               0525 01172000
*       CVB(CVBNUM,PHRF34SP(1));                                   0526 01173000
         CVB   @09,PHRF34SP(,PHRREG)                               0526 01174000
         ST    @09,CVBNUM                                          0526 01175000
*       RLNPOLMT=CVBNUM2;                                          0527 01176000
         L     @09,MLCPTR1(,MLCAREG)                               0527 01177000
         MVC   RLNPOLMT(1,@09),CVBNUM2                             0527 01178000
*       IF PHRF34NS=1 THEN                                         0528 01179000
         CLC   PHRF34NS(4,PHRREG),@CF00039                         0528 01180000
         BE    @RT00528                                            0528 01181000
*         ;                                                        0529 01182000
*       ELSE                                                       0530 01183000
*         DO;                                                      0530 01184000
*           IF PHRF34SP(2)='000QUEUE'/* POLLIMIT WITH 'QUEUE'?       */ 01185000
*             THEN                                                 0531 01186000
         CLC   PHRF34SP+12(8,PHRREG),@CC01411                      0531 01187000
         BNE   @RF00531                                            0531 01188000
*             RNCGQUE=ON;           /* YES. SET INDICATOR            */ 01189000
         OI    RNCGQUE(RNCAREG),B'10000000'                        0532 01190000
*           ELSE                                                   0533 01191000
*             IF PHRF34SP(2)='0000WAIT'/* POLIMIT WITH 'WAIT'?       */ 01192000
*               THEN                                               0533 01193000
         B     @RC00531                                            0533 01194000
@RF00531 CLC   PHRF34SP+12(8,PHRREG),@CC01412                      0533 01195000
         BNE   @RF00533                                            0533 01196000
*               RNCGNOW=ON;         /* YES. SET INDICATOR            */ 01197000
         OI    RNCGNOW(RNCAREG),B'00100000'                        0534 01198000
*             ELSE                                                 0535 01199000
*               ;                                                  0535 01200000
@RF00533 DS    0H                                                  0536 01201000
*         END;                                                     0536 01202000
@RC00531 DS    0H                                                  0537 01203000
*     END;                                                         0537 01204000
@RT00528 DS    0H                                                  0538 01205000
*   GPR15=0;                                                       0538 01206000
@RT00522 SLR   GPR15,GPR15                                         0538 01207000
*   END GRPPOLMT;                                                  0539 01208000
@EL00017 DS    0H                                                  0539 01209000
@EF00017 DS    0H                                                  0539 01210000
@ER00017 BR    @14                                                 0539 01211000
*                                                                  0540 01212000
*/********************************************************************/ 01213000
*/*                                                                  */ 01214000
