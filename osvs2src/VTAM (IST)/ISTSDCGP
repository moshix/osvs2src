*       MLCPTR6->RCPLGNAP=PHRF34SP(1);/* SET IN LU           @D32CKMX*/ 00825000
         L     @09,MLCPTR6(,MLCAREG)                               0426 00826000
         MVC   RCPLGNAP(8,@09),PHRF34SP(PHRREG)                    0426 00827000
*     END;                          /*                       @D32CKMX*/ 00828000
