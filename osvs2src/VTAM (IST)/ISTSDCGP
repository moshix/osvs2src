@EF00008 DS    0H                                                  0409 00746000
@ER00008 BR    @14                                                 0409 00747000
*                                                                  0410 00748000
*/********************************************************************/ 00749000
*/*                                                                  */ 00750000
*/*            ITBMODE PARAMETER SUBROUTINE                          */ 00751000
*/*                                                                  */ 00752000
*/* THIS ROUTINE WILL PROCESS THE ITBMODE PARAMETER OF THE GROUP     */ 00753000
*/* MACRO. IF EITHER SUBPARM IS INVALID AN ERROR MSG IS PUT OUT AND  */ 00754000
*/* THE DEFAULT IS TAKEN. IF THE SUBPARMS ARE VALID THE APPROPRIATE  */ 00755000
*/* BITS ARE TURNED ON IN THE TERM SIFT AREA IF OTHER THAN THE       */ 00756000
*/* DEFAULT VALUE IS SPECIFIED.                                      */ 00757000
*/*                                                                  */ 00758000
*/********************************************************************/ 00759000
*                                                                  0410 00760000
*GRPITBMD:                                                         0410 00761000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0410 00762000
GRPITBMD DS    0H                                                  0411 00763000
*   IF PHRF2VAL(1)=ITBMDYES         /* WAS 'YES' TURN ON FLAG IN     */ 00764000
*     THEN                                                         0411 00765000
         CLC   PHRF2VAL(4,PHRREG),@CF00039                         0411 00766000
         BNE   @RF00411                                            0411 00767000
*     RTRRCIT1=ON;                  /* TERM SIFT AREA. IF 'NO'       */ 00768000
         L     @09,MLCPTR3(,MLCAREG)                               0412 00769000
         OI    RTRRCIT1(@09),B'10000000'                           0412 00770000
*   ELSE                                                           0413 00771000
*     RTRRCIT1=OFF;                 /* TURN FLAG OFF.                */ 00772000
         B     @RC00411                                            0413 00773000
@RF00411 L     @09,MLCPTR3(,MLCAREG)                               0413 00774000
         NI    RTRRCIT1(@09),B'01111111'                           0413 00775000
*   IF PHRF2NVL>1                   /* MORE THAN ONE SUBPARM?        */ 00776000
*     THEN                          /* YES                           */ 00777000
@RC00411 LA    @09,1                                               0414 00778000
         C     @09,PHRF2NVL(,PHRREG)                               0414 00779000
         BNL   @RF00414                                            0414 00780000
*     IF PHRF2VAL(2)=ITBMDYES       /* IF 2ND SUBPARM OK AND WAS     */ 00781000
*       THEN                                                       0415 00782000
         C     @09,PHRF2VAL+4(,PHRREG)                             0415 00783000
         BNE   @RF00415                                            0415 00784000
*       RTRRCIT2=ON;                /* 'YES' TURN ON FLAG IN TERM    */ 00785000
         L     @09,MLCPTR3(,MLCAREG)                               0416 00786000
         OI    RTRRCIT2(@09),B'01000000'                           0416 00787000
*     ELSE                                                         0417 00788000
*       RTRRCIT2=OFF;                                              0417 00789000
         B     @RC00415                                            0417 00790000
@RF00415 L     @09,MLCPTR3(,MLCAREG)                               0417 00791000
         NI    RTRRCIT2(@09),B'10111111'                           0417 00792000
*   ELSE                                                           0418 00793000
*     ;                             /* ONLY ONE SUBPARM              */ 00794000
@RF00414 DS    0H                                                  0419 00795000
*   GPR15=0;                        /* SET RTN CODE TO 0             */ 00796000
@RC00414 SLR   GPR15,GPR15                                         0419 00797000
*   GPR14=ADDR(MAINSUB);                                           0420 00798000
         LA    GPR14,MAINSUB                                       0420 00799000
*   END GRPITBMD;                                                  0421 00800000
@EL00009 DS    0H                                                  0421 00801000
@EF00009 DS    0H                                                  0421 00802000
@ER00009 BR    @14                                                 0421 00803000
*                                                                  0422 00804000
*/********************************************************************/ 00805000
*/*                                                                  */ 00806000
*/*          LOGAPPL PARAMETER SUBROUTINE                            */ 00807000
*/*                                                                  */ 00808000
*/* THIS ROUTINE WILL PLACE THE LOGAPPL NAME IN THE RDT FIELD IF IT  */ 00809000
*/* IS GOOD. IF BAD THE RDT FIELD IS BLANKED AND AN ERROR MSG GIVEN. */ 00810000
