         TITLE 'ISTSDCGP - SYSTEM DEFINITION GROUP MACRO PROCESSOR     *00001000
                        '                                               00002000
ISTSDCGP CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCGP  76.324'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  LA    @12,4095(,@10)                                      0001 00012000
         USING @PSTART,@10                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
*   RNCSAV1(2)=GPR13;               /* BACKWARD POINT                */ 00015000
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0204 00016000
*   GPR13=ADDR(RNCSAV1);            /* SET UP OUR SAVE REG           */ 00017000
         LA    GPR13,RNCSAV1(,RNCAREG)                             0205 00018000
*   MLCSAV3(3)=GPR13;               /* FORWARD POINT                 */ 00019000
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0206 00020000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */ 00021000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0207 00022000
*   GENERATE NOREFS NOSETS;                                        0208 00023000
*                                                                  0208 00024000
           USING @DATD,11                 ADDRESSABILITY FOR DATA       00025000
*/********************************************************************/ 00026000
*/* SET UP THE DEFAULT PARAMETERS PRIOR TO PROCESSING THE GROUP      */ 00027000
*/* MACRO KEYWORDS.                                                  */ 00028000
*/********************************************************************/ 00029000
*                                                                  0209 00030000
*   GPR1=RPRENTGP;                                                 0209 00031000
         LA    GPR1,48                                             0209 00032000
*   CALL ISTSDCRS;                  /* CALL SIFTDEF                  */ 00033000
         L     @15,@CV01245                                        0210 00034000
         BALR  @14,@15                                             0210 00035000
*   GPR1=MLCKWD;                                                   0211 00036000
         L     GPR1,MLCKWD(,MLCAREG)                               0211 00037000
*   RC=0;                                                          0212 00038000
         SLR   @07,@07                                             0212 00039000
         STH   @07,RC                                              0212 00040000
*   RNCGVSEQ='0'B;                                                 0213 00041000
*   RNCGTWX='0'B;                                                  0214 00042000
         NI    RNCGVSEQ(RNCAREG),B'11111100'                       0214 00043000
*   RNCGQUE='0'B;                   /* TURN OFF POLIMIT 'QUEUE'      */ 00044000
*   RNCGNOW='0'B;                   /* AND 'WAIT' INDICATORS         */ 00045000
         NI    RNCGQUE(RNCAREG),B'01011111'                        0216 00046000
*   DO WHILE(RC=0);                                                0217 00047000
         B     @DE00217                                            0217 00048000
@DL00217 DS    0H                                                  0218 00049000
*     CALL DOWHILE;                                                0218 00050000
         BAL   @14,DOWHILE                                         0218 00051000
*DOLOOP:                                                           0219 00052000
*     ;                                                            0219 00053000
*                                                                  0219 00054000
DOLOOP   DS    0H                                                  0220 00055000
*/********************************************************************/ 00056000
*/* AT THIS POINT A RETURN CODE OF 0 MEANS CONTINUE THE DO WHILE     */ 00057000
*/* LOOP.  OTHERWISE, THE LOOP IS DISCONTINUED WITH A RETURN CODE OF */ 00058000
*/* 8 OR 4.                                                          */ 00059000
*/********************************************************************/ 00060000
*                                                                  0220 00061000
*   END;                                                           0220 00062000
*                                                                  0220 00063000
@DE00217 LH    @07,RC                                              0220 00064000
         LTR   @07,@07                                             0220 00065000
         BZ    @DL00217                                            0220 00066000
*/********************************************************************/ 00067000
*/* AT THIS POINT IF RC=8, INDICATING END OF STRING, WE CALL AREA    */ 00068000
*/* HANDLER TO MOVE THE MODEL AREA INTO THE BUILD AREA.  UPON RETURN */ 00069000
*/* WE UPDATE THE CURRENT ENTRY POINTER TO THIS ENTRY. ALSO, ANY     */ 00070000
*/* COUNTS THAT NEED BE UPDATED, ARE DONE AT THIS POINT.             */ 00071000
*/********************************************************************/ 00072000
*                                                                  0221 00073000
*   IF RC=8 THEN                                                   0221 00074000
         CH    @07,@CH00036                                        0221 00075000
         BNE   @RF00221                                            0221 00076000
*     DO;                           /*                       @XM00168*/ 00077000
