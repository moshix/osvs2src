         NI    RTRRDBHD(@09),B'10111111'                           0390 00680000
*         END;                                                     0391 00681000
*     ELSE                                                         0392 00682000
*       DO;                                                        0392 00683000
         B     @RC00374                                            0392 00684000
@RF00374 DS    0H                                                  0393 00685000
*         RTRBHSN1=PHRF34SP(1);     /* MOVE SET NAME TO              */ 00686000
         L     @09,MLCPTR3(,MLCAREG)                               0393 00687000
         MVC   RTRBHSN1(8,@09),PHRF34SP(PHRREG)                    0393 00688000
*         RTRRDBHN=OFF;             /* RDT AND TURN OFF 'NONE' FLG   */ 00689000
         NI    RTRRDBHN(@09),B'01111111'                           0394 00690000
*       END;                                                       0395 00691000
