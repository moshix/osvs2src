*       PACK(PHRF34SP(1),PHRF34SP(1));/* CONVERT IT TO BINARY        */ 00731000
         PACK  PHRF34SP(8,PHRREG),PHRF34SP(8,PHRREG)               0403 00732000
*       CVB(CVBNUM,PHRF34SP(1));    /* AND CK FOR VALID              */ 00733000
         CVB   @09,PHRF34SP(,PHRREG)                               0404 00734000
         ST    @09,CVBNUM                                          0404 00735000
*       RTRTRMAX=CVBNUM2;           /* IF OK - STORE IT IN           */ 00736000
         L     @09,MLCPTR3(,MLCAREG)                               0405 00737000
