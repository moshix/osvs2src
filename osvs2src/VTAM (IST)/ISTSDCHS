         TITLE 'ISTSDCHS - HOST MACRO HNDLR                            *00001000
                        '                                               00002000
ISTSDCHS CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCHS  77.308'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*   IF RNCGHOST='1'B THEN           /* A PRECEEDING HOST MACRO HAS 0244 00014000
*                                      ALREADY BEEN INTERPRETED    0244 00015000
*                                                            @XA19636*/ 00016000
         TM    RNCGHOST(RNCAREG),B'00010000'                       0244 00017000
         BNO   @RF00244                                            0244 00018000
*     RC=0;                         /* EXIT WITH GOOD RTN CODE TO  0245 00019000
*                                      IGNORE CURRENT HOST MACRO   0245 00020000
*                                                            @XA19636*/ 00021000
         SLR   RC,RC                                               0245 00022000
*   ELSE                            /* NO PRECEEDING HOST MACRO    0246 00023000
*                                                            @XA19636*/ 00024000
*     DO;                           /*                       @XA19636*/ 00025000
         B     @RC00244                                            0246 00026000
@RF00244 DS    0H                                                  0247 00027000
*       RNCSAV1(2)=GPR13;           /* BACKWARD POINT                */ 00028000
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0247 00029000
*       GPR13=ADDR(RNCSAV1);        /* SET UP OUR SAVE REG           */ 00030000
         LA    GPR13,RNCSAV1(,RNCAREG)                             0248 00031000
*       MLCSAV3(3)=GPR13;           /* FORWARD POINT                 */ 00032000
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0249 00033000
*       DATDREG=ADDR(RNCDAT1);      /* SET UP OUR DATA REG           */ 00034000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0250 00035000
*       GENERATE;                                                  0251 00036000
*                                                                  0251 00037000
           USING @DATD,11                 ADDRESSABILITY FOR DATA       00038000
*/********************************************************************/ 00039000
*/* WHILE THE RETURN CODE = 0, THIS CODE PREPARES THE PHR PARAMETERS */ 00040000
*/* AND CALLS PHR.  UPON RETURN FROM PHR, THE RETURN CODE AND THE @  */ 00041000
*/* OF THE NEXT MACRO KEYWORD ARE SAVED.                             */ 00042000
*/********************************************************************/ 00043000
*                                                                  0252 00044000
*       RFY                                                        0252 00045000
*        (GPR0,                                                    0252 00046000
*         GPR1,                                                    0252 00047000
*         GPR14,                                                   0252 00048000
*         GPR15) RSTD;              /* RESTRICT REGS                 */ 00049000
*       ENTRYPTR=RNCRN;             /*                       @XA19636*/ 00050000
         L     ENTRYPTR,RNCRN(,RNCAREG)                            0253 00051000
*       RFY                                                        0254 00052000
*         ISTRRN BASED(ENTRYPTR);                                  0254 00053000
*       GPR1=MLCKWD;                                               0255 00054000
         L     GPR1,MLCKWD(,MLCAREG)                               0255 00055000
*       RC=0;                                                      0256 00056000
         SLR   RC,RC                                               0256 00057000
*       STMODSW='0'B;               /*                       @DL31HCK*/ 00058000
         NI    STMODSW,B'11011111'                                 0257 00059000
*       DO WHILE(RC=0);                                            0258 00060000
         B     @DE00258                                            0258 00061000
@DL00258 DS    0H                                                  0259 00062000
*         CALL DOWHILE;                                            0259 00063000
         BAL   @14,DOWHILE                                         0259 00064000
*DOLOOP:                                                           0260 00065000
*         ;                                                        0260 00066000
*                                                                  0260 00067000
DOLOOP   DS    0H                                                  0261 00068000
*/********************************************************************/ 00069000
*/* AT THIS POINT A RETURN CODE OF 0 MEANS CONTINUE THE DO WHILE     */ 00070000
*/* LOOP.  OTHERWISE, THE LOOP IS DISCONTINUED WITH A RETURN CODE OF */ 00071000
*/* 8 OR 4.                                                          */ 00072000
*/********************************************************************/ 00073000
*                                                                  0261 00074000
*       END;                                                       0261 00075000
@DE00258 SLR   @03,@03                                             0261 00076000
         CR    RC,@03                                              0261 00077000
         BE    @DL00258                                            0261 00078000
*       IF RC=8 THEN                                               0262 00079000
         CH    RC,@CH00035                                         0262 00080000
         BNE   @RF00262                                            0262 00081000
*         DO;                       /* RC = 8 = END OF STRING        */ 00082000
*           RC=0;                   /* RC = O = OK FOR MODULE EXIT 0264 00083000
*                                                            @XA19636*/ 00084000
         LR    RC,@03                                              0264 00085000
*         END;                      /*                       @XA19636*/ 00086000
*       RNCGHOST='1'B;              /* SET HOST MACRO FP             */ 00087000
@RF00262 OI    RNCGHOST(RNCAREG),B'00010000'                       0266 00088000
*       GPR13=RNCSAV1(2);           /* RESTORE CALLERS REGS          */ 00089000
         L     GPR13,RNCSAV1+4(,RNCAREG)                           0267 00090000
*     END;                          /*                       @XA19636*/ 00091000
*/********************************************************************/ 00092000
*/* AT THIS POINT RC CONTAINS EITHER A 0 OR A 4 INDICATING A GOOD    */ 00093000
*/* RETURN CODE OR AN ERROR RETURN CODE WHICH WILL KILL THE BUILD.   */ 00094000
*/********************************************************************/ 00095000
*                                                                  0269 00096000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00097000
@RC00244 LR    @15,RC                                              0269 00098000
         L     @14,12(,@13)                                        0269 00099000
         LM    @00,@12,20(@13)                                     0269 00100000
         BR    @14                                                 0269 00101000
*DOWHILE:                                                          0270 00102000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0270 00103000
*                                                                  0270 00104000
DOWHILE  DS    0H                                                  0271 00105000
*/********************************************************************/ 00106000
*/* 1ST LEVEL INNER PROC CALLED BY THE OUTER PROC'S DO WHILE ROUTINE */ 00107000
*/********************************************************************/ 00108000
*                                                                  0271 00109000
*   GPR0=ADDR(HSTKWTBL);                                           0271 00110000
         LA    GPR0,HSTKWTBL                                       0271 00111000
*   PHR1REG=ADDR(RNCPHR1);                                         0272 00112000
         LA    PHR1REG,RNCPHR1(,RNCAREG)                           0272 00113000
*   RFY                                                            0273 00114000
*     ISTPHROA BASED(PHR1REG);                                     0273 00115000
*   CALL ISTINCPH;                  /* CALL PHR                      */ 00116000
         L     @15,@CV02020                                        0274 00117000
         BALR  @14,@15                                             0274 00118000
*   RC=GPR15;                                                      0275 00119000
         LR    RC,GPR15                                            0275 00120000
*   KWDPTR=GPR1;                                                   0276 00121000
         ST    GPR1,KWDPTR                                         0276 00122000
*   IF RC=0 THEN                                                   0277 00123000
         LTR   RC,RC                                               0277 00124000
         BNZ   @RF00277                                            0277 00125000
*     DO;                                                          0278 00126000
*                                                                  0278 00127000
*/********************************************************************/ 00128000
*/* IF THE RETURN CODE FROM PHR = 0, CALL THE PARAMETER HANDLER      */ 00129000
*/* WHOSE ADDRESS WAS RETURNED IN GPR0 BY PHR.                       */ 00130000
*/* NOTE: THE EXTERNAL PARAMETER HANDLERS WILL RETURN TO THIS        */ 00131000
*/*       ROUTINE VIA A RETURN.  ALL INTERNAL PARAMETER HANDLERS     */ 00132000
*/*       WILL RETURN TO THIS ROUTINE VIA A GOTO 'MAINSUB'.          */ 00133000
*/* NOTE: ALL PARAMETER HANDLERS MUST HAVE A 0 OR 4 RETURN CODE IN   */ 00134000
*/*       GPR15 TO INDICATE CONTINUE PROCESSING OR KILL THE BUILD.   */ 00135000
*/********************************************************************/ 00136000
*                                                                  0279 00137000
*       GPR15=GPR0;                                                0279 00138000
         LR    GPR15,GPR0                                          0279 00139000
*       CALL PARMHDLR;                                             0280 00140000
         BALR  @14,@15                                             0280 00141000
*MAINSUB:                                                          0281 00142000
*       ;                                                          0281 00143000
MAINSUB  DS    0H                                                  0282 00144000
*       RC=GPR15;                                                  0282 00145000
         LR    RC,GPR15                                            0282 00146000
*     END;                                                         0283 00147000
*   ELSE                                                           0284 00148000
*     DO;                                                          0284 00149000
*                                                                  0284 00150000
         B     @RC00277                                            0284 00151000
@RF00277 DS    0H                                                  0285 00152000
*/********************************************************************/ 00153000
*/* IF THE RETURN CODE FROM PHR = 4 INDICATING THE KEYWORD WAS NOT   */ 00154000
*/* FOUND IN THE PARAMETER TABLE, WRITE THE APPROPRIATE ERROR        */ 00155000
*/* MESSAGE, SET THE RETURN CODE = 0 AND CONTINUE THE DO WHILE LOOP. */ 00156000
*/********************************************************************/ 00157000
*                                                                  0285 00158000
*       IF RC=4 THEN                                               0285 00159000
         CH    RC,@CH00051                                         0285 00160000
         BNE   @RF00285                                            0285 00161000
*         DO;                                                      0286 00162000
*                                                                  0286 00163000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 00164000
*                                                                  0287 00165000
*           DO;                                                    0287 00166000
*             DCL                                                  0288 00167000
*               1 CMZP0003 STATIC LOCAL,/* PARAMETER LIST PASSED TO     00168000
*                                      ERROR FORMAT ROUTINE          */ 00169000
