         L     GPR14P,RPHRESMA(,GPR04P)                            0462 00402000
         O     GPR14P,@CF01196                                     0462 00403000
*             GPR15P=GPR14P|RPHPT1; /* LOAD REG 15 WITH FLAG FIELD 0463 00404000
*                                      AND TURN POST FLAG ON         */ 00405000
         LR    GPR15P,GPR14P                                       0463 00406000
         O     GPR15P,@CF01186                                     0463 00407000
*             CS(GPR14P,GPR15P,GPR04P->RPHRESMA);/* COMPARE REG 14 0464 00408000
*                                      WITH FLAG FIELD--IF WT/EXT BIT   00409000
*                                      ON, TURN ON POST FLAG         */ 00410000
         CS    GPR14P,@15,RPHRESMA(GPR04P)                         0464 00411000
*             BC(4,FMZL0029);       /* IF WT/EXT BIT WAS OFF, LOOP TO   00412000
*                                      TEST CONTENTION FLAG          */ 00413000
         BC    4,FMZL0029                                          0465 00414000
*             BC(8,FMZT0029);       /* IF WT/EXT BIT WAS OFF, BRANCH    00415000
*                                      TO BYPASS CODE GEN            */ 00416000
         BC    8,FMZT0029                                          0466 00417000
*           END;                    /* IF POST FLAG IS ON, FALL THRU    00418000
*                                      BOTTOM OF LOOP                */ 00419000
*         ELSE                                                     0468 00420000
*           ;                       /* WT/EXT CONTENTION FLAG OFF    */ 00421000
@RF00460 DS    0H                                                  0469 00422000
*         GPR00F=0;                 /* SET REG 0 TO INDICATE ENA     */ 00423000
         SLR   GPR00F,GPR00F                                       0469 00424000
*         DO;                       /* LISATCVT                    0470 00425000
*                                      CVTBASE(PAGEABLE,(GPR15P))    */ 00426000
*           GPR15P=ATCLCPTR;        /* SET UP ATCVT POINTER          */ 00427000
         L     GPR15P,ATCLCPTR                                     0471 00428000
*         END;                      /* LISATCVT PUT POINTER TO ATCVT    00429000
*                                      IN IN REG 15                  */ 00430000
*         CALL GPR15P->ATCAP32;     /* CALL ISTAPC32: TPSCHED        */ 00431000
         L     @12,ATCAP32(,GPR15P)                                0473 00432000
         LR    @15,@12                                             0473 00433000
         BALR  @14,@15                                             0473 00434000
*FMZT0029:                                                         0474 00435000
*         DO;                       /* MODESET KEYADDR(GPR06F)     0474 00436000
*                                      WORKREG(GPR05F)               */ 00437000
FMZT0029 DS    0H                                                  0475 00438000
*           GPR05F=GPR06F;          /* GET KEY FORM SAVE LOCATION    */ 00439000
         LR    GPR05F,GPR06F                                       0475 00440000
*           SPKA(GPR05F);           /* SET PSW KEY                   */ 00441000
         SPKA  0(GPR05F)                                           0476 00442000
*         END;                      /* MODESET KEYADDR(GPR06F)     0477 00443000
*                                      WORKREG(GPR05F)               */ 00444000
*         RESPECIFY                                                0478 00445000
*          (GPR00F,                                                0478 00446000
*           GPR01F,                                                0478 00447000
*           GPR04F,                                                0478 00448000
*           GPR05F,                                                0478 00449000
*           GPR06F,                                                0478 00450000
*           GPR14F,                                                0478 00451000
*           GPR15F) UNRSTD;         /* UNRESTRICT REGS 0,1, 4,5,14,     00452000
*                                      AND 15                        */ 00453000
*       END;                        /* ***END OF TPPOST GENERATION***   00454000
*                                      POST SDCRC RPH        @XA08116*/ 00455000
*     END;                          /*                       @XA08116*/ 00456000
*   SAVREG=SAFREG10;                /* RESTORE SAVE AREA REG @XA08116*/ 00457000
@RT00435 LR    SAVREG,SAFREG10                                     0481 00458000
*   GEN SETS(R15) REFS(SAVREG);                                    0482 00459000
*                                                                  0482 00460000
*                            /* RESTORE REGS,R15 = 0,RETURN  @XA08116*/ 00461000
         LM    14,12,12(SAVREG)                            /*@XA08116*/ 00462000
         SLR   R15,R15                                     /*@XA08116*/ 00463000
         BR    14                                          /*@XA08116*/ 00464000
*/* TPEPILOG                                                         */ 00465000
