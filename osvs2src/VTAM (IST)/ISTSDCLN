         TITLE 'ISTSDCLN - SYSTEM DEFINITION LINE MACRO HANDLER        *00001000
                        '                                               00002000
ISTSDCLN CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCLN  76.112'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  LA    @12,4095(,@10)                                      0001 00012000
         USING @PSTART,@10                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
*   RNCSAV1(2)=GPR13;               /* BACKWARD POINT                */ 00015000
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0203 00016000
*   GPR13=ADDR(RNCSAV1);            /* SET UP OUR SAVE REG           */ 00017000
         LA    GPR13,RNCSAV1(,RNCAREG)                             0204 00018000
*   MLCSAV3(3)=GPR13;               /* FORWARD POINT                 */ 00019000
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0205 00020000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */ 00021000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0206 00022000
*   GENERATE;                                                      0207 00023000
           USING @DATD,11                 ADDRESSABILITY FOR DATA       00024000
*   IF RNCGGRP='1'B                 /* CHECK GROUP MACRO FP          */ 00025000
*     THEN                                                         0208 00026000
         TM    RNCGGRP(RNCAREG),B'00001000'                        0208 00027000
         BNO   @RF00208                                            0208 00028000
*     DO;                           /* IF ON CONTINUE, ELSE EXIT     */ 00029000
*/********************************************************************/ 00030000
*/* WHILE THE RETURN CODE = 0, THIS CODE PREPARES THE PHR PARAMETERS */ 00031000
*/* AND CALLS PHR.  UPON RETURN FROM PHR, THE RETURN CODE AND THE @  */ 00032000
*/* OF THE NEXT MACRO KEYWORD ARE SAVED.                             */ 00033000
*/********************************************************************/ 00034000
*                                                                  0210 00035000
*       RFY                                                        0210 00036000
*        (GPR0,                                                    0210 00037000
*         GPR1,                                                    0210 00038000
*         GPR15,                                                   0210 00039000
*         GPR14) RSTD;              /* RESTRICT REGS                 */ 00040000
*       GPR1=RPRENTLN;                                             0211 00041000
         LA    GPR1,80                                             0211 00042000
*       CALL ISTSDCRS;              /* CALL SIFTDEF                  */ 00043000
         L     @15,@CV01289                                        0212 00044000
         BALR  @14,@15                                             0212 00045000
*       GPR1=MLCKWD;                                               0213 00046000
         L     GPR1,MLCKWD(,MLCAREG)                               0213 00047000
*       RC=0;                                                      0214 00048000
         SLR   @04,@04                                             0214 00049000
         STH   @04,RC                                              0214 00050000
*       RNCGVSEQ='0'B;                                             0215 00051000
*       RNCGCTRM='0'B;                                             0216 00052000
         NI    RNCGVSEQ(RNCAREG),B'11111010'                       0216 00053000
*       IF RNCGQUE='1'B             /* COPY 'POLIMIT' INDICATORS     */ 00054000
*         THEN                                                     0217 00055000
         TM    RNCGQUE(RNCAREG),B'10000000'                        0217 00056000
         BNO   @RF00217                                            0217 00057000
*         RNCLQUE='1'B;             /* FROM GROUP                    */ 00058000
         OI    RNCLQUE(RNCAREG),B'01000000'                        0218 00059000
*       ELSE                                                       0219 00060000
*         RNCLQUE='0'B;                                            0219 00061000
         B     @RC00217                                            0219 00062000
@RF00217 NI    RNCLQUE(RNCAREG),B'10111111'                        0219 00063000
*       IF RNCGNOW='1'B THEN                                       0220 00064000
@RC00217 TM    RNCGNOW(RNCAREG),B'00100000'                        0220 00065000
         BNO   @RF00220                                            0220 00066000
*         RNCLNOW='1'B;                                            0221 00067000
         OI    RNCLNOW(RNCAREG),B'00010000'                        0221 00068000
*       ELSE                                                       0222 00069000
*         RNCLNOW='0'B;                                            0222 00070000
         B     @RC00220                                            0222 00071000
@RF00220 NI    RNCLNOW(RNCAREG),B'11101111'                        0222 00072000
*       DO WHILE(RC=0);                                            0223 00073000
@RC00220 B     @DE00223                                            0223 00074000
@DL00223 DS    0H                                                  0224 00075000
*         CALL DOWHILE;                                            0224 00076000
         BAL   @14,DOWHILE                                         0224 00077000
*DOLOOP:                                                           0225 00078000
*         ;                                                        0225 00079000
*                                                                  0225 00080000
DOLOOP   DS    0H                                                  0226 00081000
*/********************************************************************/ 00082000
*/* AT THIS POINT A RETURN CODE OF 0 MEANS CONTINUE THE DO WHILE     */ 00083000
*/* LOOP.  OTHERWISE, THE LOOP IS DISCONTINUED WITH A RETURN CODE OF */ 00084000
*/* 8 OR 4.                                                          */ 00085000
*/********************************************************************/ 00086000
*                                                                  0226 00087000
*       END;                                                       0226 00088000
@DE00223 ICM   @04,3,RC                                            0226 00089000
         BZ    @DL00223                                            0226 00090000
*     END;                                                         0227 00091000
*   ELSE                                                           0228 00092000
*     RC=0;                                                        0228 00093000
*                                                                  0228 00094000
         B     @RC00208                                            0228 00095000
@RF00208 SLR   @04,@04                                             0228 00096000
         STH   @04,RC                                              0228 00097000
*/********************************************************************/ 00098000
*/* AT THIS POINT IF RC=8, INDICATING END OF STRING, WE CALL AREA    */ 00099000
*/* HANDLER TO MOVE THE MODEL AREA INTO THE BUILD AREA.  UPON RETURN */ 00100000
*/* WE UPDATE THE CURRENT ENTRY POINTER TO THIS ENTRY. ALSO, ANY     */ 00101000
*/* COUNTS THAT NEED BE UPDATED, ARE DONE AT THIS POINT.             */ 00102000
*/********************************************************************/ 00103000
*                                                                  0229 00104000
*   IF RC=8 THEN                                                   0229 00105000
@RC00208 CLC   RC(2),@CH00036                                      0229 00106000
         BNE   @RF00229                                            0229 00107000
*     IF RLNRAEP='1'B THEN                                         0230 00108000
         L     @04,MLCPTR1(,MLCAREG)                               0230 00109000
         TM    RLNRAEP(@04),B'00000100'                            0230 00110000
         BNO   @RF00230                                            0230 00111000
*       RC=0;                                                      0231 00112000
         SLR   @04,@04                                             0231 00113000
         STH   @04,RC                                              0231 00114000
*     ELSE                                                         0232 00115000
*       DO;                                                        0232 00116000
         B     @RC00230                                            0232 00117000
@RF00230 DS    0H                                                  0233 00118000
*         IF RLNPORT='1'B           /* PORT ENTRY                    */ 00119000
*           THEN                                                   0233 00120000
         L     @04,MLCPTR1(,MLCAREG)                               0233 00121000
         TM    RLNPORT(@04),B'00010000'                            0233 00122000
         BNO   @RF00233                                            0233 00123000
*           DO;                                                    0234 00124000
*             RFY                                                  0235 00125000
*               ISTRPRE BASED(MLCPTR1);                            0235 00126000
*             RPRELEN=LENGTH(ISTRPT);                              0236 00127000
         MVC   RPRELEN(2,@04),@CH02076                             0236 00128000
*             RPTUTERM=0;           /*                       @XM05981*/ 00129000
         SLR   @03,@03                                             0237 00130000
         ST    @03,RPTUTERM(,@04)                                  0237 00131000
*           END;                                                   0238 00132000
*         ELSE                                                     0239 00133000
*           IF RLNRATRN='1'B&RGPADIL='1'B/*                  @DM13802*/ 00134000
*             THEN                                                 0239 00135000
         B     @RC00233                                            0239 00136000
@RF00233 L     @04,MLCPTR1(,MLCAREG)                               0239 00137000
         TM    RLNRATRN(@04),B'00000010'                           0239 00138000
         BNO   @RF00239                                            0239 00139000
         L     @04,RNCGP(,RNCAREG)                                 0239 00140000
         TM    RGPADIL(@04),B'10000000'                            0239 00141000
         BO    @RT00239                                            0239 00142000
*             ;                     /*                       @DM13802*/ 00143000
*           ELSE                                                   0241 00144000
*             RLNRACAT='0'B;        /*                       @DM13802*/ 00145000
@RF00239 L     @04,MLCPTR1(,MLCAREG)                               0241 00146000
         NI    RLNRACAT(@04),B'01111111'                           0241 00147000
*         IF RNCGP->RGPASS='1'B THEN                               0242 00148000
@RT00239 DS    0H                                                  0242 00149000
@RC00233 L     @04,RNCGP(,RNCAREG)                                 0242 00150000
         TM    RGPASS(@04),B'01000000'                             0242 00151000
         BNO   @RF00242                                            0242 00152000
*           ADDR(RPRDEVCH)->DEVCSSL='1'B;                          0243 00153000
         L     @04,MLCPTR1(,MLCAREG)                               0243 00154000
         OI    DEVCSSL+44(@04),B'01000000'                         0243 00155000
*         ELSE                                                     0244 00156000
*           IF RNCGP->RGPABSC='1'B THEN                            0244 00157000
         B     @RC00242                                            0244 00158000
@RF00242 L     @04,RNCGP(,RNCAREG)                                 0244 00159000
         TM    RGPABSC(@04),B'00100000'                            0244 00160000
         BNO   @RF00244                                            0244 00161000
*             ADDR(RPRDEVCH)->DEVCBSC='1'B;                        0245 00162000
         L     @04,MLCPTR1(,MLCAREG)                               0245 00163000
         OI    DEVCBSC+44(@04),B'10000000'                         0245 00164000
*           ELSE                                                   0246 00165000
*             ;                                                    0246 00166000
@RF00244 DS    0H                                                  0247 00167000
*         GPR0=0;                                                  0247 00168000
@RC00242 SLR   GPR0,GPR0                                           0247 00169000
*         GPR1=MLCPTR1;                                            0248 00170000
         L     GPR1,MLCPTR1(,MLCAREG)                              0248 00171000
*         CALL ISTSDCC1;            /* CALL AREA HANDLER             */ 00172000
         L     @15,@CV01286                                        0249 00173000
         BALR  @14,@15                                             0249 00174000
*         RC=GPR15;                                                0250 00175000
         STH   GPR15,RC                                            0250 00176000
*         IF RC=0 THEN                                             0251 00177000
         LH    @04,RC                                              0251 00178000
         LTR   @04,@04                                             0251 00179000
         BNZ   @RF00251                                            0251 00180000
*           DO;                                                    0252 00181000
*             RNCLLN=RNCLN;         /*                       @XM09016*/ 00182000
         MVC   RNCLLN(4,RNCAREG),RNCLN(RNCAREG)                    0253 00183000
*             RNCLN=GPR1;           /*                       @XM09016*/ 00184000
         ST    GPR1,RNCLN(,RNCAREG)                                0254 00185000
*             RFY                                                  0255 00186000
*               ISTRLN BASED(RNCLN);/*                       @XM09016*/ 00187000
*             IF RLNRAPEP='1'B&     /* CHECK FOR PEP LINE    @X50BHMX*/ 00188000
*                 RLNRBUSE='0'B     /* AND USE IN EP         @X50BHMX*/ 00189000
*               THEN                                               0256 00190000
         L     @04,RNCLN(,RNCAREG)                                 0256 00191000
         TM    RLNRAPEP(@04),B'00000001'                           0256 00192000
         BNO   @RF00256                                            0256 00193000
         TM    RLNRBUSE(@04),B'10000000'                           0256 00194000
         BZ    @RT00256                                            0256 00195000
*               ;                   /* LEAVE BIT AS ZERO     @X50BHMX*/ 00196000
*             ELSE                                                 0258 00197000
*               RLNRBISA='1'B;      /* LINE IS FOR NCP       @X50BHMX*/ 00198000
@RF00256 L     @04,RNCLN(,RNCAREG)                                 0258 00199000
         OI    RLNRBISA(@04),B'01000000'                           0258 00200000
*             RRNCTELI=RRNCTELI+1;  /* UPDATE RN LINE COUNT          */ 00201000
@RT00256 L     @04,RNCRN(,RNCAREG)                                 0259 00202000
         LA    @03,1                                               0259 00203000
         LH    @09,RRNCTELI(,@04)                                  0259 00204000
         ALR   @09,@03                                             0259 00205000
         STH   @09,RRNCTELI(,@04)                                  0259 00206000
*             RGPCTELN=RGPCTELN+1;  /* UPDATE GROUP LINE COUNT       */ 00207000
         L     @04,RNCGP(,RNCAREG)                                 0260 00208000
         AH    @03,RGPCTELN(,@04)                                  0260 00209000
         STH   @03,RGPCTELN(,@04)                                  0260 00210000
*             IF RLNANSWR='0'B      /* WAS ANSWER=YES SPECIFED?    0261 00211000
*                                                            @X50BHMX*/ 00212000
*               THEN                                               0261 00213000
         L     @04,RNCLN(,RNCAREG)                                 0261 00214000
         TM    RLNANSWR(@04),B'01000000'                           0261 00215000
         BZ    @RT00261                                            0261 00216000
*               ;                   /* NO - LEAVE BIT AS ZERO      0262 00217000
*                                                            @X50BHMX*/ 00218000
*             ELSE                                                 0263 00219000
*               RLNGANSW='1'B;      /* YES - ANSWER=YES GENED      0263 00220000
*                                                            @X50BHMX*/ 00221000
         L     @04,RNCLN(,RNCAREG)                                 0263 00222000
         OI    RLNGANSW(@04),B'00100000'                           0263 00223000
*             RNCGDLLN='0'B;        /*                       @XL03HCK*/ 00224000
@RT00261 NI    RNCGDLLN(RNCAREG),B'11111110'                       0264 00225000
*           END;                                                   0265 00226000
*         ELSE                                                     0266 00227000
*           RC=4;                                                  0266 00228000
         B     @RC00251                                            0266 00229000
@RF00251 MVC   RC(2),@CH00052                                      0266 00230000
*       END;                                                       0267 00231000
*   ELSE                                                           0268 00232000
*     ;                                                            0268 00233000
@RF00229 DS    0H                                                  0269 00234000
*   RNCGLINE='1'B;                  /* SET LINE MACRO FP             */ 00235000
@RC00229 OI    RNCGLINE(RNCAREG),B'00000100'                       0269 00236000
*   GPR13=RNCSAV1(2);               /* RESTORE CALLERS REGS          */ 00237000
         L     GPR13,RNCSAV1+4(,RNCAREG)                           0270 00238000
*   RFY                                                            0271 00239000
*     ISTRLN BASED(MLCPTR1);        /*                       @XM09016*/ 00240000
*/********************************************************************/ 00241000
*/* AT THIS POINT RC CONTAINS EITHER A 0 OR A 4 INDICATING A GOOD    */ 00242000
*/* RETURN CODE OR AN ERROR RETURN CODE WHICH WILL KILL THE BUILD.   */ 00243000
*/********************************************************************/ 00244000
*                                                                  0272 00245000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00246000
         LH    @15,RC                                              0272 00247000
         L     @14,12(,@13)                                        0272 00248000
         LM    @00,@12,20(@13)                                     0272 00249000
         BR    @14                                                 0272 00250000
*DOWHILE:                                                          0273 00251000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0273 00252000
DOWHILE  DS    0H                                                  0274 00253000
*   GPR0=ADDR(LINKWTBL);                                           0274 00254000
         LA    GPR0,LINKWTBL                                       0274 00255000
*   PHR1REG=ADDR(RNCPHR1);                                         0275 00256000
         LA    PHR1REG,RNCPHR1(,RNCAREG)                           0275 00257000
*   RFY                                                            0276 00258000
*     ISTPHROA BASED(PHR1REG);                                     0276 00259000
*   CALL ISTINCPH;                  /* CALL PHR                      */ 00260000
         L     @15,@CV01285                                        0277 00261000
         BALR  @14,@15                                             0277 00262000
*   RC=GPR15;                                                      0278 00263000
         STH   GPR15,RC                                            0278 00264000
*   KWDPTR=GPR1;                                                   0279 00265000
         LR    KWDPTR,GPR1                                         0279 00266000
*   IF RC=0 THEN                                                   0280 00267000
         LH    @08,RC                                              0280 00268000
         LTR   @08,@08                                             0280 00269000
         BNZ   @RF00280                                            0280 00270000
*     DO;                                                          0281 00271000
*                                                                  0281 00272000
*/********************************************************************/ 00273000
*/* IF THE RETURN CODE FROM PHR = 0, CALL THE PARAMETER HANDLER      */ 00274000
*/* WHOSE ADDRESS WAS RETURNED IN GPR0 BY PHR.                       */ 00275000
*/* NOTE: THE EXTERNAL PARAMETER HANDLERS WILL RETURN TO THIS        */ 00276000
*/*       ROUTINE VIA A RETURN.  ALL INTERNAL PARAMETER HANDLERS     */ 00277000
*/*       WILL RETURN TO THIS ROUTINE VIA A GOTO 'MAINSUB'.          */ 00278000
*/* NOTE: ALL PARAMETER HANDLERS MUST HAVE A 0 OR 4 RETURN CODE IN   */ 00279000
*/*       GPR15 TO INDICATE CONTINUE PROCESSING OR KILL THE BUILD.   */ 00280000
*/********************************************************************/ 00281000
*                                                                  0282 00282000
*       GPR15=GPR0;                                                0282 00283000
         LR    GPR15,GPR0                                          0282 00284000
*       CALL PARMHDLR;                                             0283 00285000
         BALR  @14,@15                                             0283 00286000
*MAINSUB:                                                          0284 00287000
*       ;                                                          0284 00288000
MAINSUB  DS    0H                                                  0285 00289000
*       RC=GPR15;                                                  0285 00290000
         STH   GPR15,RC                                            0285 00291000
*     END;                                                         0286 00292000
*   ELSE                                                           0287 00293000
*     DO;                                                          0287 00294000
*                                                                  0287 00295000
         B     @RC00280                                            0287 00296000
@RF00280 DS    0H                                                  0288 00297000
*/********************************************************************/ 00298000
*/* IF THE RETURN CODE FROM PHR = 4 INDICATING THE KEYWORD WAS NOT   */ 00299000
*/* FOUND IN THE PARAMETER TABLE, WRITE THE APPROPRIATE ERROR        */ 00300000
*/* MESSAGE, SET THE RETURN CODE = 0 AND CONTINUE THE DO WHILE LOOP. */ 00301000
*/********************************************************************/ 00302000
*                                                                  0288 00303000
*       IF RC=4 THEN                                               0288 00304000
         CLC   RC(2),@CH00052                                      0288 00305000
         BNE   @RF00288                                            0288 00306000
*         DO;                                                      0289 00307000
*                                                                  0289 00308000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 00309000
*                                                                  0290 00310000
*           DO;                                                    0290 00311000
*             DCL                                                  0291 00312000
*               1 CMZP0003 STATIC LOCAL,/* PARAMETER LIST PASSED TO     00313000
*                                      ERROR FORMAT ROUTINE          */ 00314000
*                2 CMZL0003 FIXED(8) INIT(4),/* LEVEL PARAMETER    0291 00315000
*                                      (CLEV)                        */ 00316000
*                2 CMZE0003 FIXED(8) INIT(4),/* ERROR PARAMETER    0291 00317000
*                                      (CERR)                        */ 00318000
*                2 CMZA0003 CHAR(1) INIT('1'),/* ACTION PARAMETER  0291 00319000
*                                      (CACT)                        */ 00320000
*                2 CMZS0003 FIXED(8) INIT(0);/* LENGTH OF STRING   0291 00321000
*                                      PARAM                         */ 00322000
*             DCL                                                  0292 00323000
*               CMZR0003 REGISTER(1);/* ESTABLISH PARAMeter          */ 00324000
*             RESPECIFY                                            0293 00325000
*               CMZR0003 RSTD;      /* REGISTER FOR                  */ 00326000
*             CMZR0003=ADDR(CMZP0003);/* ADDRESS OF PARAMETER LIST   */ 00327000
         LA    CMZR0003,CMZP0003                                   0294 00328000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 00329000
         L     @15,@CV01287                                        0295 00330000
         BALR  @14,@15                                             0295 00331000
*             RESPECIFY                                            0296 00332000
*               CMZR0003 UNRSTD;                                   0296 00333000
*           END;                                                   0297 00334000
*           RC=0;                                                  0298 00335000
         SLR   @08,@08                                             0298 00336000
         STH   @08,RC                                              0298 00337000
*         END;                                                     0299 00338000
*       ELSE                                                       0300 00339000
*         DO;                                                      0300 00340000
*                                                                  0300 00341000
         B     @RC00288                                            0300 00342000
@RF00288 DS    0H                                                  0301 00343000
*/********************************************************************/ 00344000
*/* IF THE RETURN CODE FROM PHR = 8 OR 12 INDICATING AN END OF       */ 00345000
*/* STRING OR SYNTAX ERROR, THE FOLLOWING ROUTINE IS EXECUTED.       */ 00346000
*/********************************************************************/ 00347000
*                                                                  0301 00348000
*           IF RC=8                 /* END OF PARM STRING            */ 00349000
*             THEN                                                 0301 00350000
         LA    @08,8                                               0301 00351000
         CH    @08,RC                                              0301 00352000
         BNE   @RF00301                                            0301 00353000
*             DO;                   /* CHECK PARM VAL                */ 00354000
*/********************************************************************/ 00355000
*/* DIAL LOGIC ROUTINE STARTS HERE.                                  */ 00356000
*/********************************************************************/ 00357000
*                                                                  0303 00358000
*               RCSAVE=8;                                          0303 00359000
         LA    @04,8                                               0303 00360000
         LR    RCSAVE,@04                                          0303 00361000
*               IF RGPADIL='1'B     /* DIAL                          */ 00362000
*                 THEN                                             0304 00363000
         L     @04,RNCGP(,RNCAREG)                                 0304 00364000
         TM    RGPADIL(@04),B'10000000'                            0304 00365000
         BNO   @RF00304                                            0304 00366000
*                 DO;               /* YES                           */ 00367000
*                   ADDR(RPRDEVCH)->DEVCSWL='1'B;/*          @DL31HCK*/ 00368000
         L     @03,MLCPTR1(,MLCAREG)                               0306 00369000
         OI    DEVCSWL+44(@03),B'00010000'                         0306 00370000
*                   IF RGPASDLC='1'B/* SDLC LINE?            @D32CKCK*/ 00371000
*                     THEN                                         0307 00372000
         TM    RGPASDLC(@04),B'00010000'                           0307 00373000
         BNO   @RF00307                                            0307 00374000
*                     DO;           /*                       @D32CKCK*/ 00375000
*                       IF RLNRACNT='00'B/* NO CALL?         @D32CKCK*/ 00376000
*                         THEN                                     0309 00377000
         TM    RLNRACNT(@03),B'11000000'                           0309 00378000
         BNZ   @RF00309                                            0309 00379000
*                         RLNRACAT='1'B;/* SET CALL DEFAULT  @D32CKCK*/ 00380000
         OI    RLNRACAT(@03),B'10000000'                           0310 00381000
*                       ELSE                                       0311 00382000
*                         ;         /*                       @D32CKCK*/ 00383000
@RF00309 DS    0H                                                  0312 00384000
*                       IF RLNRACAT='1'B/* CALL?             @D32CKCK*/ 00385000
*                         THEN                                     0312 00386000
         L     @04,MLCPTR1(,MLCAREG)                               0312 00387000
         TM    RLNRACAT(@04),B'10000000'                           0312 00388000
         BNO   @RF00312                                            0312 00389000
*                         DO;       /*                       @D32CKCK*/ 00390000
*                           IF RNCGANS='1'B|LANSWER='1'B/* ANS     0314 00391000
*                                      GRP+LIN               @D32CKCK*/ 00392000
*                             THEN                                 0314 00393000
         TM    RNCGANS(RNCAREG),B'00010000'                        0314 00394000
         BO    @RT00314                                            0314 00395000
         TM    LANSWER,B'01000000'                                 0314 00396000
         BO    @RT00314                                            0314 00397000
*                             ;     /*                       @D32CKCK*/ 00398000
*                           ELSE                                   0316 00399000
*                             RLNANSWR='1'B;/* SET ANS       @D32CKCK*/ 00400000
         L     @04,MLCPTR1(,MLCAREG)                               0316 00401000
         OI    RLNANSWR(@04),B'01000000'                           0316 00402000
*                         END;      /*                       @D32CKCK*/ 00403000
*                       ELSE                                       0318 00404000
*                         DO;       /*                       @D32CKCK*/ 00405000
         B     @RC00312                                            0318 00406000
@RF00312 DS    0H                                                  0319 00407000
*                           IF RNCGANS='1'B|LANSWER='1'B/* ANS     0319 00408000
*                                      GRP+LIN               @D32CKCK*/ 00409000
*                             THEN                                 0319 00410000
         TM    RNCGANS(RNCAREG),B'00010000'                        0319 00411000
