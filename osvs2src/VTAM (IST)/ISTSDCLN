         B     @RC00550                                            0552 01233000
@RF00550 CLC   PHRF2VAL(4,PHR1REG),@CF00039                        0552 01234000
         BNE   @RF00552                                            0552 01235000
*       RLNRACAT=OFF;               /* TURN OFF CALL IN FLAG         */ 01236000
         L     @08,MLCPTR1(,MLCAREG)                               0553 01237000
         NI    RLNRACAT(@08),B'01111111'                           0553 01238000
*     ELSE                                                         0554 01239000
*       ;                                                          0554 01240000
@RF00552 DS    0H                                                  0555 01241000
*   GPR14=ADDR(MAINSUB);                                           0555 01242000
@RC00550 LA    GPR14,MAINSUB                                       0555 01243000
*   GPR15=0;                        /* RETURN CODE                   */ 01244000
         SLR   GPR15,GPR15                                         0556 01245000
*   END LINCALL;                                                   0557 01246000
*                                                                  0557 01247000
@EL00016 DS    0H                                                  0557 01248000
@EF00016 DS    0H                                                  0557 01249000
@ER00016 BR    @14                                                 0557 01250000
*/********************************************************************/ 01251000
*/*                                                                  */ 01252000
*/*            DIRECTN PARAMETER SUBROUTINE                          */ 01253000
*/*                                                                  */ 01254000
*/* THIS ROUTINE WILL SET THE RDT FLAGS AS PER THE PARAMETER SETTING */ 01255000
*/* FOR IN, OUT, OR INOUT(BOTH) AS DECODED BY PHR.                   */ 01256000
*/*                                                                  */ 01257000
*/********************************************************************/ 01258000
*                                                                  0558 01259000
*LINDRCTN:                                                         0558 01260000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0558 01261000
LINDRCTN DS    0H                                                  0559 01262000
*   IF PHRF2VAL(1)=DRCALIN THEN                                    0559 01263000
         CLC   PHRF2VAL(4,PHR1REG),@CF00039                        0559 01264000
         BNE   @RF00559                                            0559 01265000
*     DO;                                                          0560 01266000
*       DEVINPUT=ON;                                               0561 01267000
*       DEVOTPUT=OFF;                                              0562 01268000
         L     @08,MLCPTR4(,MLCAREG)                               0562 01269000
         OI    DEVINPUT(@08),B'10000000'                           0562 01270000
         NI    DEVOTPUT(@08),B'10111111'                           0562 01271000
*     END;                                                         0563 01272000
*   ELSE                                                           0564 01273000
*     IF PHRF2VAL(1)=DRCALOUT THEN                                 0564 01274000
         B     @RC00559                                            0564 01275000
@RF00559 CLC   PHRF2VAL(4,PHR1REG),@CF00234                        0564 01276000
         BNE   @RF00564                                            0564 01277000
*       DO;                                                        0565 01278000
*         DEVINPUT=OFF;                                            0566 01279000
*         DEVOTPUT=ON;                                             0567 01280000
         L     @08,MLCPTR4(,MLCAREG)                               0567 01281000
         OI    DEVOTPUT(@08),B'01000000'                           0567 01282000
         NI    DEVINPUT(@08),B'01111111'                           0567 01283000
*       END;                                                       0568 01284000
*     ELSE                                                         0569 01285000
*       DO;                                                        0569 01286000
         B     @RC00564                                            0569 01287000
@RF00564 DS    0H                                                  0570 01288000
*         DEVINPUT=ON;                                             0570 01289000
*         DEVOTPUT=ON;                                             0571 01290000
         L     @08,MLCPTR4(,MLCAREG)                               0571 01291000
         OI    DEVINPUT(@08),B'11000000'                           0571 01292000
*       END;                                                       0572 01293000
*   GPR14=ADDR(MAINSUB);                                           0573 01294000
@RC00564 DS    0H                                                  0573 01295000
@RC00559 LA    GPR14,MAINSUB                                       0573 01296000
*   GPR15=0;                        /* RETURN CODE                   */ 01297000
         SLR   GPR15,GPR15                                         0574 01298000
*   END LINDRCTN;                                                  0575 01299000
*                                                                  0575 01300000
@EL00017 DS    0H                                                  0575 01301000
@EF00017 DS    0H                                                  0575 01302000
@ER00017 BR    @14                                                 0575 01303000
*/********************************************************************/ 01304000
*/*                                                                  */ 01305000
*/*                AUTO PARAMETER SUBROUTINE                         */ 01306000
*/*                                                                  */ 01307000
*/********************************************************************/ 01308000
*                                                                  0576 01309000
*LINEAUTO:                                                         0576 01310000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0576 01311000
LINEAUTO DS    0H                                                  0577 01312000
*   RLNAUTOD=ON;                    /*                       @D32CKCK*/ 01313000
         L     @08,MLCPTR1(,MLCAREG)                               0577 01314000
         OI    RLNAUTOD(@08),B'00100000'                           0577 01315000
*   GPR15=0;                        /* RETURN CODE                   */ 01316000
         SLR   GPR15,GPR15                                         0578 01317000
*   GPR14=ADDR(MAINSUB);            /* LOAD RETURN REGISTER          */ 01318000
         LA    GPR14,MAINSUB                                       0579 01319000
*   END LINEAUTO;                                                  0580 01320000
*                                                                  0580 01321000
@EL00018 DS    0H                                                  0580 01322000
@EF00018 DS    0H                                                  0580 01323000
@ER00018 BR    @14                                                 0580 01324000
*/********************************************************************/ 01325000
*/*                                                                  */ 01326000
*/* THIS ROUTINE WILL PROCESS THE BHSET PARAMETER OF THE LINE MACRO. */ 01327000
*/* IF 'NONE' OR 'DYNAMIC' HAS BEEN SPECIFIED THE APPROPRIATE FLAG   */ 01328000
*/* IS SET IN THE RDT. IF A SET NAME WAS SPECIFIED IT IS MOVED TO    */ 01329000
*/* THE RDT. IF A VALIDITY OR INVALID COMBINATION ERROR OCCURS,      */ 01330000
*/* ERRFMT IS ISSUED AND THE PARAMETER IS IGNORED.                   */ 01331000
*/*                                                                  */ 01332000
*/********************************************************************/ 01333000
*                                                                  0581 01334000
*LINBHSET:                                                         0581 01335000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0581 01336000
LINBHSET DS    0H                                                  0582 01337000
*   IF PHRF34SP(1,1:4)='NONE'|PHRF34SP(1)=BLANK8 THEN              0582 01338000
         CLC   PHRF34SP(4,PHR1REG),@CC01443                        0582 01339000
         BE    @RT00582                                            0582 01340000
         CLC   PHRF34SP(8,PHR1REG),@CC01271                        0582 01341000
         BNE   @RF00582                                            0582 01342000
@RT00582 DS    0H                                                  0583 01343000
*     DO;                           /* IF 'NONE' SPECIFIED           */ 01344000
*       RTRRDBHD=OFF;                                              0584 01345000
*       RTRRDBHN=ON;                /* TURN ON 'NONE' FLG IN RDT     */ 01346000
         L     @08,MLCPTR3(,MLCAREG)                               0585 01347000
         OI    RTRRDBHN(@08),B'10000000'                           0585 01348000
         NI    RTRRDBHD(@08),B'10111111'                           0585 01349000
*     END;                                                         0586 01350000
*   ELSE                                                           0587 01351000
*     IF PHRF34SP(1,1:7)='DYNAMIC' THEN                            0587 01352000
*                                                                  0587 01353000
         B     @RC00582                                            0587 01354000
@RF00582 CLC   PHRF34SP(7,PHR1REG),@CC01444                        0587 01355000
         BNE   @RF00587                                            0587 01356000
*/********************************************************************/ 01357000
*/*                                                                  */ 01358000
*/* IF DYNAMIC IS SPECIFIED THE SYSCNTRL MACRO MUST HAVE HAD THE     */ 01359000
*/* MODIFY BLOCK HANDLER SET ASSOCIATION (BHSASSC) SUBPARM LISTED    */ 01360000
*/* AS AN OPTION ENTRY.                                              */ 01361000
*/*                                                                  */ 01362000
*/********************************************************************/ 01363000
*                                                                  0588 01364000
*       IF RRNCAMBH='1'B            /* IF BHSASSC SET BY SYSCNTRL    */ 01365000
*         THEN                                                     0588 01366000
         L     @08,RNCRN(,RNCAREG)                                 0588 01367000
         TM    RRNCAMBH(@08),B'00001000'                           0588 01368000
         BNO   @RF00588                                            0588 01369000
*         DO;                                                      0589 01370000
*           RTRRDBHD=ON;            /* SET 'DYNAMIC' FLG IN RDT      */ 01371000
*           RTRRDBHN=OFF;           /* AND TURN OFF 'NONE' FLAG      */ 01372000
         L     @08,MLCPTR3(,MLCAREG)                               0591 01373000
         OI    RTRRDBHD(@08),B'01000000'                           0591 01374000
         NI    RTRRDBHN(@08),B'01111111'                           0591 01375000
*         END;                                                     0592 01376000
*       ELSE                                                       0593 01377000
*         DO;                                                      0593 01378000
*                                                                  0593 01379000
         B     @RC00588                                            0593 01380000
@RF00588 DS    0H                                                  0594 01381000
*/* ERRFMT    LEVEL(PARM)  ERROR(COMB)  ACTION(IGNORE)               */ 01382000
*                                                                  0594 01383000
*           DO;                                                    0594 01384000
*             DCL                                                  0595 01385000
*               1 CMZP0013 STATIC LOCAL,/* PARAMETER LIST PASSED TO     01386000
*                                      ERROR FORMAT ROUTINE          */ 01387000
*                2 CMZL0013 FIXED(8) INIT(4),/* LEVEL PARAMETER    0595 01388000
*                                      (CLEV)                        */ 01389000
*                2 CMZE0013 FIXED(8) INIT(1),/* ERROR PARAMETER    0595 01390000
*                                      (CERR)                        */ 01391000
*                2 CMZA0013 CHAR(1) INIT('1'),/* ACTION PARAMETER  0595 01392000
*                                      (CACT)                        */ 01393000
*                2 CMZS0013 FIXED(8) INIT(0);/* LENGTH OF STRING   0595 01394000
*                                      PARAM                         */ 01395000
*             DCL                                                  0596 01396000
*               CMZR0013 REGISTER(1);/* ESTABLISH PARAMeter          */ 01397000
*             RESPECIFY                                            0597 01398000
*               CMZR0013 RSTD;      /* REGISTER FOR                  */ 01399000
*             CMZR0013=ADDR(CMZP0013);/* ADDRESS OF PARAMETER LIST   */ 01400000
         LA    CMZR0013,CMZP0013                                   0598 01401000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 01402000
         L     @15,@CV01287                                        0599 01403000
         BALR  @14,@15                                             0599 01404000
*             RESPECIFY                                            0600 01405000
*               CMZR0013 UNRSTD;                                   0600 01406000
*           END;                                                   0601 01407000
*           RTRRDBHD=OFF;           /* TURN OFF 'DYNAMIC' FLG IN     */ 01408000
*           RTRRDBHN=ON;            /* RDT AND SET THE 'NONE' ON     */ 01409000
         L     @08,MLCPTR3(,MLCAREG)                               0603 01410000
         OI    RTRRDBHN(@08),B'10000000'                           0603 01411000
         NI    RTRRDBHD(@08),B'10111111'                           0603 01412000
*         END;                                                     0604 01413000
*     ELSE                                                         0605 01414000
*       DO;                                                        0605 01415000
         B     @RC00587                                            0605 01416000
@RF00587 DS    0H                                                  0606 01417000
*         RTRBHSN1=PHRF34SP(1);     /* MOVE SET NAME TO              */ 01418000
         L     @08,MLCPTR3(,MLCAREG)                               0606 01419000
         MVC   RTRBHSN1(8,@08),PHRF34SP(PHR1REG)                   0606 01420000
*         RTRRDBHN=OFF;             /* RDT AND TURN OFF 'NONE' FLG   */ 01421000
*         RTRRDBHD=OFF;                                            0608 01422000
         NI    RTRRDBHN(@08),B'00111111'                           0608 01423000
*       END;                                                       0609 01424000
*   GPR15=0;                                                       0610 01425000
@RC00587 DS    0H                                                  0610 01426000
@RC00582 SLR   GPR15,GPR15                                         0610 01427000
*   GPR14=ADDR(MAINSUB);                                           0611 01428000
         LA    GPR14,MAINSUB                                       0611 01429000
*   END LINBHSET;                                                  0612 01430000
*                                                                  0612 01431000
@EL00019 DS    0H                                                  0612 01432000
@EF00019 DS    0H                                                  0612 01433000
@ER00019 BR    @14                                                 0612 01434000
*/********************************************************************/ 01435000
*/*                                                                  */ 01436000
*/* THIS ROUTINE WILL MOVE IF SPECIFIED, THE NUMBER OF TRANSMISSION  */ 01437000
*/* THE NCP WILL RECEIVE FROM OR SEND TO THIS STATION OR BOTH. IF NO */ 01438000
*/* NUMBER IS SPECIFIED A CHECK IS MADE TO SEE IF 'NO' WAS THE PARM. */ 01439000
*/* IF SO THERE IS NO LIMIT TRANSMISSIONS. IF NEITHER A COUNT OR     */ 01440000
*/* 'NO' WERE GIVEN, AN ERROR MSG IS PRINTED AND THE DEFAULT OF 'NO' */ 01441000
*/* IS TAKEN. IF A COUNT IS SPECIFIED IT MUST BE >0<256.             */ 01442000
*/*                                                                  */ 01443000
*/********************************************************************/ 01444000
*                                                                  0613 01445000
*LINXMTLM:                                                         0613 01446000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0613 01447000
LINXMTLM DS    0H                                                  0614 01448000
*   IF PHRF34SP(1)='000000NO'|PHRF34SL(1)=0 THEN                   0614 01449000
         CLC   PHRF34SP(8,PHR1REG),@CC01454                        0614 01450000
         BE    @RT00614                                            0614 01451000
         ICM   @08,3,PHRF34SL(PHR1REG)                             0614 01452000
         BNZ   @RF00614                                            0614 01453000
@RT00614 DS    0H                                                  0615 01454000
*     RTRTRMAX=0;                   /* IF 'NO' SET XMISSION CNT=0    */ 01455000
         L     @08,MLCPTR3(,MLCAREG)                               0615 01456000
         MVI   RTRTRMAX(@08),X'00'                                 0615 01457000
*   ELSE                                                           0616 01458000
*     DO;                           /* A NUM WAS SPECIFIED           */ 01459000
         B     @RC00614                                            0616 01460000
@RF00614 DS    0H                                                  0617 01461000
*       PACK(PHRF34SP(1),PHRF34SP(1));/* CONVERT IT TO BINARY        */ 01462000
         PACK  PHRF34SP(8,PHR1REG),PHRF34SP(8,PHR1REG)             0617 01463000
*       CVB(CVBNUM,PHRF34SP(1));    /* AND CK FOR VALID              */ 01464000
         CVB   @08,PHRF34SP(,PHR1REG)                              0618 01465000
         ST    @08,CVBNUM                                          0618 01466000
*       RTRTRMAX=CVBNUM2;           /* IF OK - STORE IT IN           */ 01467000
         L     @08,MLCPTR3(,MLCAREG)                               0619 01468000
         MVC   RTRTRMAX(1,@08),CVBNUM2                             0619 01469000
*     END;                                                         0620 01470000
*   GPR15=0;                        /* SET RTN CODE TO 0             */ 01471000
@RC00614 SLR   GPR15,GPR15                                         0621 01472000
*   GPR14=ADDR(MAINSUB);                                           0622 01473000
         LA    GPR14,MAINSUB                                       0622 01474000
*   END LINXMTLM;                                                  0623 01475000
*                                                                  0623 01476000
@EL00020 DS    0H                                                  0623 01477000
@EF00020 DS    0H                                                  0623 01478000
@ER00020 BR    @14                                                 0623 01479000
*/********************************************************************/ 01480000
*/*                                                                  */ 01481000
*/* THIS ROUTINE WILL PROCESS THE ITBMODE PARAMETER OF THE LINE      */ 01482000
*/* MACRO. IF EITHER SUBPARM IS INVALID AN ERROR MSG IS PUT OUT AND  */ 01483000
*/* THE DEFAULT IS TAKEN. IF THE SUBPARMS ARE VALID THE APPROPRIATE  */ 01484000
*/* BITS ARE TURNED ON IN THE TERMINAL MODEL IF OTHER THAN THE       */ 01485000
*/* DEFAULT VALUE IS SPECIFIED.                                      */ 01486000
*/*                                                                  */ 01487000
*/********************************************************************/ 01488000
*                                                                  0624 01489000
*LINITBMD:                                                         0624 01490000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0624 01491000
LINITBMD DS    0H                                                  0625 01492000
*   RTRRCIT1=OFF;                                                  0625 01493000
*   RTRRCIT2=OFF;                                                  0626 01494000
         L     @08,MLCPTR3(,MLCAREG)                               0626 01495000
         NI    RTRRCIT1(@08),B'00111111'                           0626 01496000
*   IF PHRF2VAL(1)=1                /* IF 1ST SUBPARM OK AND WAS     */ 01497000
*     THEN                          /* 'YES' TURN ON FLAG IN         */ 01498000
         CLC   PHRF2VAL(4,PHR1REG),@CF00039                        0627 01499000
         BNE   @RF00627                                            0627 01500000
*     RTRRCIT1=ON;                  /* TERM ENTRY. IF 'NO'           */ 01501000
         OI    RTRRCIT1(@08),B'10000000'                           0628 01502000
*   ELSE                                                           0629 01503000
*     ;                                                            0629 01504000
@RF00627 DS    0H                                                  0630 01505000
*   IF PHRF2NVL>1                   /* MORE THAN ONE SUBPARM         */ 01506000
*     THEN                          /* IF YES                        */ 01507000
         LA    @08,1                                               0630 01508000
         C     @08,PHRF2NVL(,PHR1REG)                              0630 01509000
         BNL   @RF00630                                            0630 01510000
*     IF PHRF2VAL(2)=1              /* IF 2ND SUBPARM OK AND WAS     */ 01511000
*       THEN                        /* 'YES' TURN ON FLAG IN ????    */ 01512000
         C     @08,PHRF2VAL+4(,PHR1REG)                            0631 01513000
         BNE   @RF00631                                            0631 01514000
*       RTRRCIT2=ON;                /* ENTRY. IF 'NO' LEAVE FLAG     */ 01515000
         L     @08,MLCPTR3(,MLCAREG)                               0632 01516000
         OI    RTRRCIT2(@08),B'01000000'                           0632 01517000
*     ELSE                                                         0633 01518000
*       ;                           /* OFF.                          */ 01519000
@RF00631 DS    0H                                                  0634 01520000
*   ELSE                                                           0634 01521000
*     ;                             /* ONLY ONE PARM                 */ 01522000
@RF00630 DS    0H                                                  0635 01523000
*   GPR15=0;                        /* SET RTN CODE TO 0             */ 01524000
         SLR   GPR15,GPR15                                         0635 01525000
*   GPR14=ADDR(MAINSUB);                                           0636 01526000
         LA    GPR14,MAINSUB                                       0636 01527000
*   END LINITBMD;                                                  0637 01528000
*                                                                  0637 01529000
@EL00021 DS    0H                                                  0637 01530000
@EF00021 DS    0H                                                  0637 01531000
@ER00021 BR    @14                                                 0637 01532000
*/********************************************************************/ 01533000
*/*                                                                  */ 01534000
*/*            SPEED PARAMETER SUBROUTINE                            */ 01535000
*/*                                                                  */ 01536000
*/* THIS ROUTINE PROCESSES THE SPEED KEYWORD.  IF THE SPEED IS LESS  */ 01537000
*/* THEN 2400, THE SCHEDULE LIMIT IS SET TO 2.  IF THE SPEED IS LESS */ 01538000
*/* THEN 50000, THE SCHEDULE LIMIT IST SET TO 3.  OTHERWISE IT IS    */ 01539000
*/* SET TO 4.                                                        */ 01540000
*/*                                                                  */ 01541000
*/********************************************************************/ 01542000
*                                                                  0638 01543000
*LINSPEED:                                                         0638 01544000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0638 01545000
LINSPEED DS    0H                                                  0639 01546000
*   PACK(PHRF34SP(1),PHRF34SP(1));                                 0639 01547000
         PACK  PHRF34SP(8,PHR1REG),PHRF34SP(8,PHR1REG)             0639 01548000
*   CVB(CVBNUM,PHRF34SP(1));                                       0640 01549000
         CVB   @08,PHRF34SP(,PHR1REG)                              0640 01550000
         ST    @08,CVBNUM                                          0640 01551000
*   IF CVBNUM<2400 THEN                                            0641 01552000
         CH    @08,@CH01461                                        0641 01553000
         BNL   @RF00641                                            0641 01554000
*     RLNRDSLM=2;                                                  0642 01555000
         L     @08,MLCPTR1(,MLCAREG)                               0642 01556000
         MVI   RLNRDSLM(@08),X'02'                                 0642 01557000
*   ELSE                                                           0643 01558000
*     DO;                                                          0643 01559000
         B     @RC00641                                            0643 01560000
@RF00641 DS    0H                                                  0644 01561000
*       IF CVBNUM<50000 THEN                                       0644 01562000
         L     @08,CVBNUM                                          0644 01563000
         C     @08,@CF01462                                        0644 01564000
         BNL   @RF00644                                            0644 01565000
*         RLNRDSLM=3;                                              0645 01566000
         L     @08,MLCPTR1(,MLCAREG)                               0645 01567000
         MVI   RLNRDSLM(@08),X'03'                                 0645 01568000
*       ELSE                                                       0646 01569000
*         RLNRDSLM=4;                                              0646 01570000
         B     @RC00644                                            0646 01571000
@RF00644 L     @08,MLCPTR1(,MLCAREG)                               0646 01572000
         MVI   RLNRDSLM(@08),X'04'                                 0646 01573000
*     END;                                                         0647 01574000
@RC00644 DS    0H                                                  0648 01575000
*   GPR15=0;                                                       0648 01576000
@RC00641 SLR   GPR15,GPR15                                         0648 01577000
*   END LINSPEED;                                                  0649 01578000
*                                                                  0649 01579000
@EL00022 DS    0H                                                  0649 01580000
@EF00022 DS    0H                                                  0649 01581000
@ER00022 BR    @14                                                 0649 01582000
*/********************************************************************/ 01583000
*/*                                                                  */ 01584000
*/*                FEATURE PARAMETER SUBROUTINE                      */ 01585000
*/*                                                                  */ 01586000
*/* THIS ROUTINE WILL TURN OFF ALL FEATURE FLAGS, THEN CALL          */ 01587000
*/* ADDBITS(ISTSDCC5) TO SET ANY SPECIFIED AS PER PHR DECODE OUTPUT. */ 01588000
*/*                                                                  */ 01589000
*/********************************************************************/ 01590000
*                                                                  0650 01591000
*LINFEATR:                                                         0650 01592000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0650 01593000
LINFEATR DS    0H                                                  0651 01594000
*   RTRREACR=OFF;                                                  0651 01595000
*   RTRREATT=OFF;                                                  0652 01596000
*   RTRREBRK=OFF;                                                  0653 01597000
*   RTRRECHK=OFF;                                                  0654 01598000
*   RTRRESCT=OFF;                                                  0655 01599000
*   RTRRESUP=OFF;                                                  0656 01600000
         L     @08,MLCPTR3(,MLCAREG)                               0656 01601000
         NI    RTRREACR(@08),B'01000001'                           0656 01602000
*   RTRRFXCT=OFF;                                                  0657 01603000
         NI    RTRRFXCT(@08),B'01111111'                           0657 01604000
*   RCLBATCH=OFF;                                                  0658 01605000
         L     @08,MLCPTR2(,MLCAREG)                               0658 01606000
         NI    RCLBATCH(@08),B'11011111'                           0658 01607000
*   CALL ISTSDCC5;                                                 0659 01608000
         L     @15,@CV01466                                        0659 01609000
         BALR  @14,@15                                             0659 01610000
*   GPR14=ADDR(MAINSUB);                                           0660 01611000
         LA    GPR14,MAINSUB                                       0660 01612000
*   END LINFEATR;                                                  0661 01613000
*                                                                  0661 01614000
@EL00023 DS    0H                                                  0661 01615000
