@RF00202 LA    @04,LUSWRNTB                                        0204 00082000
         ST    @04,KWDTBLAD                                        0204 00083000
*       RC=0;                                                      0205 00084000
*                                                                  0205 00085000
@RC00202 SLR   @04,@04                                             0205 00086000
         STH   @04,RC                                              0205 00087000
*/********************************************************************/ 00088000
*/* WHILE THE RETURN CODE = 0, THIS CODE PREPARES THE PHR PARAMETERS */ 00089000
*/* AND CALLS PHR.  UPON RETURN FROM PHR, THE RETURN CODE AND THE @  */ 00090000
*/* OF THE NEXT MACRO KEYWORD ARE SAVED.                             */ 00091000
*/********************************************************************/ 00092000
*                                                                  0206 00093000
*       DO WHILE(RC=0);                                            0206 00094000
         B     @DE00206                                            0206 00095000
@DL00206 DS    0H                                                  0207 00096000
*         CALL DOWHILE;                                            0207 00097000
         BAL   @14,DOWHILE                                         0207 00098000
*DOLOOP:                                                           0208 00099000
*         ;                                                        0208 00100000
*                                                                  0208 00101000
DOLOOP   DS    0H                                                  0209 00102000
*/********************************************************************/ 00103000
*/* AT THIS POINT A RETURN CODE OF 0 MEANS CONTINUE THE DO WHILE     */ 00104000
*/* LOOP.  OTHERWISE, THE LOOP IS DISCONTINUED WITH A RETURN CODE OF */ 00105000
*/* 8 OR 4.                                                          */ 00106000
*/********************************************************************/ 00107000
*                                                                  0209 00108000
*       END;                                                       0209 00109000
*                                                                  0209 00110000
@DE00206 LH    @04,RC                                              0209 00111000
         LTR   @04,@04                                             0209 00112000
         BZ    @DL00206                                            0209 00113000
*/********************************************************************/ 00114000
*/* AT THIS POINT IF RC=8, INDICATING END OF STRING, WE CALL AREA    */ 00115000
*/* HANDLER TO MOVE THE MODEL AREA INTO THE BUILD AREA.  UPON RETURN */ 00116000
*/* WE UPDATE THE CURRENT ENTRY POINTER TO THIS ENTRY. ALSO, ANY     */ 00117000
*/* COUNTS THAT NEED BE UPDATED, ARE DONE AT THIS POINT.             */ 00118000
*/********************************************************************/ 00119000
*                                                                  0210 00120000
*       IF RC=8 THEN                                               0210 00121000
         CH    @04,@CH00035                                        0210 00122000
         BNE   @RF00210                                            0210 00123000
*         DO;                                                      0211 00124000
*           IF RLURBISA='0'B        /* CHECK SPECIFIED VALUE OF    0212 00125000
