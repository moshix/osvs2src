         TITLE 'ISTSDCLU - LU MACRO HNDLR                              *00001000
                        '                                               00002000
ISTSDCLU CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCLU  78.005'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*/********************************************************************/ 00014000
*/* NOW DO LINKAGE                                                   */ 00015000
*/********************************************************************/ 00016000
*                                                                  0189 00017000
*   SAVCLLD=ADDR(RNCSAV1);          /* SAVE OUR SAVE ADDR IN CALLERS */ 00018000
         LA    @04,RNCSAV1(,RNCAREG)                               0189 00019000
         ST    @04,SAVCLLD(,SAVREG)                                0189 00020000
*   RNCSAV1(2)=SAVREG;              /* SAVE CALLERS SAVE ADDR IN OURS*/ 00021000
         ST    SAVREG,RNCSAV1+4(,RNCAREG)                          0190 00022000
*   SAVREG=ADDR(RNCSAV1);           /* SET UP OUR SAVE ADDR          */ 00023000
         LA    SAVREG,RNCSAV1(,RNCAREG)                            0191 00024000
*/********************************************************************/ 00025000
*/* BASE OUR DATA AREA ON THE AREA IN THE RNCA RNCDAT1 THIS IS DONE  */ 00026000
*/* USING THE SAME BASE AS IS USED FOR THE RNCA ITSELF BUT HAVING    */ 00027000
*/* A DISPLACEMENT FROM THE BEGINNING OF THE RNCA BASE EQAUL TO ITS  */ 00028000
*/* DISPLACEMENT IN THE RNCA                                         */ 00029000
*/********************************************************************/ 00030000
*                                                                  0192 00031000
*   GENERATE REFS(RNCDAT1,RNCSAV1,SAVREG) NOSETS                   0192 00032000
*       (USING @DATD-(RNCDAT1-RNCSAV1),SAVREG);                    0192 00033000
*                                                                  0192 00034000
         USING @DATD-(RNCDAT1-RNCSAV1),SAVREG                           00035000
*/********************************************************************/ 00036000
*/* SET UP TO CALL SIFT DEF TO GET THE LU MODEL SET UP REG 1 SET TO  */ 00037000
*/* LU ENTRY TYPE. PU MUST BE PROCESSED BEFORE LU.                   */ 00038000
*/********************************************************************/ 00039000
*                                                                  0193 00040000
*   RFY                                                            0193 00041000
*    (GPR0,                                                        0193 00042000
*     GPR1,                                                        0193 00043000
*     GPR14,                                                       0193 00044000
*     GPR15) RSTD;                                                 0193 00045000
*   IF RNCGPU='1'B                  /* PU FOOTPRINT          @D32CKCK*/ 00046000
*     THEN                                                         0194 00047000
         TM    RNCGPU(RNCAREG),B'00000001'                         0194 00048000
         BNO   @RF00194                                            0194 00049000
*     DO;                           /*                       @D32CKCK*/ 00050000
