*                                                                  0341 00444000
SUPANDB  DS    0H                                                  0342 00445000
*/********************************************************************/ 00446000
*/* LOOP WHILE THE NUMBER OF PARAMETERS HANDLED IS LESS THAN         */ 00447000
*/* THE COUNT OF SUBPARMS                                            */ 00448000
*/********************************************************************/ 00449000
*                                                                  0342 00450000
*CMLDW10:                                                          0342 00451000
*   DO COUNT=1 TO PHRF2NVL;                                        0342 00452000
CMLDW10  LA    COUNT,1                                             0342 00453000
         B     @DE00342                                            0342 00454000
@DL00342 DS    0H                                                  0343 00455000
*     DTERDTEA=0;                   /* SET RDTE ADDRESS              */ 00456000
         SLR   @14,@14                                             0343 00457000
         STCM  @14,15,DTERDTEA(ENTRYPTR)                           0343 00458000
*     DTELCTI=PHRF2VAL(COUNT);      /* SET LINE CNTRL TYP            */ 00459000
         LR    @14,COUNT                                           0344 00460000
         SLA   @14,2                                               0344 00461000
         L     @14,PHRF2VAL-4(@14,R2)                              0344 00462000
         STC   @14,DTELCTI(,ENTRYPTR)                              0344 00463000
*     ENTRYPTR=ENTRYPTR+LENGTH(ISTDTE);/* SET TO NXT ENT             */ 00464000
         AL    ENTRYPTR,@CF00700                                   0345 00465000
*   END CMLDW10;                                                   0346 00466000
         AL    COUNT,@CF00039                                      0346 00467000
@DE00342 C     COUNT,PHRF2NVL(,R2)                                 0346 00468000
         BNH   @DL00342                                            0346 00469000
*   DTELCTI=DTEND;                  /* SET END INDICATOR             */ 00470000
         MVI   DTELCTI(ENTRYPTR),X'FF'                             0347 00471000
*   MLDITSAV=R1;                    /* SAVE DITT POINTER             */ 00472000
*                                                                  0348 00473000
         LR    MLDITSAV,R1                                         0348 00474000
*   /*****************************************************************/ 00475000
*   /*                                                               */ 00476000
*   /* SEGMENT(SUPANDB)FINISH                                        */ 00477000
*   /*                                                               */ 00478000
*   /*****************************************************************/ 00479000
*                                                                  0349 00480000
*   DO;                                                            0349 00481000
*     GOTO SUPANDB#;                                               0350 00482000
