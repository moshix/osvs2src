@EL00002 DS    0H                                                  0285 00468000
@EF00002 DS    0H                                                  0285 00469000
@ER00002 BR    @14                                                 0285 00470000
*                                                                  0286 00471000
*/********************************************************************/ 00472000
*/* THIS ROUTINE PROCESSES THE ACR, CODE, AND LCTYPE PARAMETERS.     */ 00473000
*/*                                                                  */ 00474000
*/*                 LABELS FOR DEBUGGING                             */ 00475000
*/*        DCL  (NOTACR,NOTCODE,CHKBLANK,TABLNDX) LOCAL EXT LABEL    */ 00476000
*/********************************************************************/ 00477000
*                                                                  0286 00478000
*MTPARM:                                                           0286 00479000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0286 00480000
MTPARM   DS    0H                                                  0287 00481000
*   IF KEYWORD(1:3)='ACR'           /* WAS PARAMETER 'ACR'?          */ 00482000
*     THEN                                                         0287 00483000
         L     @10,MLCKWD(,MLCAREG)                                0287 00484000
         CLC   KEYWORD(3,@10),@CC01296                             0287 00485000
         BNE   @RF00287                                            0287 00486000
*     LCSTACR=PHRF2MSK(1);          /* YES-TURN ON ACR BIT           */ 00487000
         MVC   LCSTACR(1,LCSTREG),PHRF2MSK(PHRREG)                 0288 00488000
*   ELSE                            /* NO-CHECK FOR OTHERS           */ 00489000
*NOTACR:                                                           0289 00490000
*     DO;                                                          0289 00491000
         B     @RC00287                                            0289 00492000
@RF00287 DS    0H                                                  0289 00493000
NOTACR   DS    0H                                                  0290 00494000
*       IF KEYWORD(1:6)^='LCTYPE' THEN/* AT PRESENT THIS AN        0290 00495000
*                                      IMPOSSIBLE CONDITION. IF    0290 00496000
*                                      ADDITION PARAMETERS ARE ADDED    00497000
*                                      LATER, THEIR                  */ 00498000
         L     @10,MLCKWD(,MLCAREG)                                0290 00499000
         CLC   KEYWORD(6,@10),@CC01307                             0290 00500000
         BNE   @RT00290                                            0290 00501000
*         ;                         /* CHECKS SHOULD GO HERE.        */ 00502000
*       ELSE                        /* 'LCTYPE' PARAMETER            */ 00503000
*CHKBLANK:                                                         0292 00504000
*         DO;                                                      0292 00505000
CHKBLANK DS    0H                                                  0293 00506000
*           IF PHRF2VAL(1)=0        /* WAS PARAMETER BLANK?          */ 00507000
*             THEN                                                 0293 00508000
         L     @10,PHRF2VAL(,PHRREG)                               0293 00509000
         LTR   @10,@10                                             0293 00510000
         BZ    @RT00293                                            0293 00511000
*             ;                     /* YES-USE DEFAULT               */ 00512000
*           ELSE                                                   0295 00513000
*TABLNDX:                                                          0295 00514000
*             DO;                   /* NO-FILL IN LC INFO.           */ 00515000
TABLNDX  DS    0H                                                  0296 00516000
*/********************************************************************/ 00517000
*/*          TO FILL IN THE LINE CONTROL CODE AND ASSOCIATED         */ 00518000
*/*          TERMINAL TYPE, THE NEXT TWO INSTRUCTIONS USE THE        */ 00519000
*/*          OUTPUT FROM PHR IN A WAY THAT PHR DOESN'T EXPECT        */ 00520000
*/*          WHEN IT CREATES THE OUTPUT.  WHAT HAPPENS IS            */ 00521000
*/*          THIS.  SUBTABLE 'MTTYPTB2 CONSISTS OF A TERMINAL        */ 00522000
*/*          TYPE AND A ONE WORD FIELD WHICH PHR TREATS AS AN        */ 00523000
*/*          OFFSET, A FLAG BYTE, AND A ONE-BYTE BIT-PATTERN.        */ 00524000
*/*          THE ONE WORD FIELD REALLY CONSISTS OF AN                */ 00525000
*/*          UNUSED BYTE (ZEROES), A ONE BYTE LINE CONTROL           */ 00526000
*/*          CODE (CORRESPONDING TO THE TERMINAL TYPE), AND A        */ 00527000
*/*          TWO BYTE INDEX (ONLY THE SECOND BYTE IS NON-            */ 00528000
*/*          ZERO) TO THE ENTRY WITHIN MTTYPTB2 WHICH PHR            */ 00529000
*/*          USED TO CREATE THE WORD OF OUTPUT WHICH IS BEING        */ 00530000
*/*          EXPLAINED BY THIS SENTENCE.                             */ 00531000
*/*                                                                  */ 00532000
*/*          THE LINE CONTROL CODE CAN BE MOVED TO THE LCST          */ 00533000
*/*          AS IT IS BY REFERING TO AN OVERLAY TO                   */ 00534000
*/*          PHRF2VAL(1) WHICH SUBDIVIDES THE TWO BYTE               */ 00535000
*/*          OFFSET INTO TWO ONE BYTE FIELDS.  AS STATED             */ 00536000
*/*          BEFORE, THE LINE CONTROL CODE IS THE SECOND             */ 00537000
*/*          BYTE (NAMED 'WKCOD' IN THE OVERLAY).                    */ 00538000
*/*                                                                  */ 00539000
*/*          TO MOVE THE TERMINAL TYPE, THE PHRF2VAL(1) OVER-        */ 00540000
*/*          LAY IS USED TO REFERENCE AN INDEX (NAMED 'WKNDX'        */ 00541000
*/*          IN THE OVERLAY) TO THE ENTRY IN MTTYPTB2.               */ 00542000
*/*          ANOTHER OVERLAY, FOR THE ENTRY IN MTTYPTB2, IS          */ 00543000
*/*          THEN USED TO EXTRACT THE TERMINAL TYPE.                 */ 00544000
*/********************************************************************/ 00545000
*                                                                  0296 00546000
*               LCSTLCCD=WKCOD;     /* LINE CONTROL CODE             */ 00547000
         MVC   LCSTLCCD(1,LCSTREG),WKCOD(PHRREG)                   0296 00548000
*               LCSTLCTP=           /* TERMINAL TYPE                 */ 00549000
*                   TERMTYPE(WKNDX,1:5);                           0297 00550000
         LH    @10,WKNDX(,PHRREG)                                  0297 00551000
         MH    @10,@CH00032                                        0297 00552000
         LA    @08,TERMTYPE-12(@10)                                0297 00553000
         MVC   LCSTLCTP(5,LCSTREG),0(@08)                          0297 00554000
*             END TABLNDX;                                         0298 00555000
*         END CHKBLANK;                                            0299 00556000
@RT00293 DS    0H                                                  0300 00557000
*     END NOTACR;                                                  0300 00558000
@RT00290 DS    0H                                                  0301 00559000
*   GPR15=0;                        /* VALID RETURN CODE             */ 00560000
@RC00287 SLR   GPR15,GPR15                                         0301 00561000
*   GPR14=ADDR(MAINSUB);            /* RETURN TO MAINLINE            */ 00562000
         LA    GPR14,MAINSUB                                       0302 00563000
*   END MTPARM;                                                    0303 00564000
@EL00003 DS    0H                                                  0303 00565000
@EF00003 DS    0H                                                  0303 00566000
@ER00003 BR    @14                                                 0303 00567000
@PB00003 DS    0H                                                  0304 00568000
*                                                                  0304 00569000
*/********************************************************************/ 00570000
*/*                 PHR KEYWORD TABLE                                */ 00571000
