         TITLE 'ISTSDCMT  MTALCST MACRO PROCESSOR                      *00001000
                        '                                               00002000
ISTSDCMT CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCMT  75.172'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0179 00014000
*/********************************************************************/ 00015000
*/*                 ENTRY LINKAGE                                    */ 00016000
*/********************************************************************/ 00017000
*                                                                  0179 00018000
*   RNCSAV1(2)=SAVREG;              /* SET UP SAVE AREA PTRS         */ 00019000
         ST    SAVREG,RNCSAV1+4(,RNCAREG)                          0179 00020000
*   SAVREG=ADDR(RNCSAV1);           /* BY SYSDEF CONVENTIONS         */ 00021000
         LA    SAVREG,RNCSAV1(,RNCAREG)                            0180 00022000
*   MLCSAV3(3)=SAVREG;                                             0181 00023000
         ST    SAVREG,MLCSAV3+8(,MLCAREG)                          0181 00024000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP VIDSEQ DATA REG        */ 00025000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0182 00026000
*   GENERATE NOREFS NOSETS;                                        0183 00027000
*                                                                  0183 00028000
           USING @DATD,11                    @ABILITY FOR DATA          00029000
*/********************************************************************/ 00030000
*/* MTALCST STANDS FOR MULTIPLE TERMINAL ACCESS LINE CONTROL         */ 00031000
*/* SELECTION TABLE.  MTALCST MACRO BUILDS A DATA BLOCK WHICH        */ 00032000
*/* CONTAINS INFORMATION FOR EACH TYPE OF TERMINAL ALLOWED ON THE    */ 00033000
*/* SPECIFIC MTA LINE.  THE NEW BLOCK IS CHAINED TO THE HEAD OF THE  */ 00034000
*/* LCST CHAIN EVEN IF PREVIOUS BLOCKS EXIST. ALL LCST DATA BLOCKS   */ 00035000
*/* ARE OF ONE SIZE REGARDLESS OF NUMBER OF TERMINAL TYPES ALLOWED.  */ 00036000
*/********************************************************************/ 00037000
*                                                                  0184 00038000
*   DQSW='0'B                       /* THIS SWITCH IS SET TO ONE   0184 00039000
*                                      LATER IF AN LCST DATA BLOCK IS   00040000
*                                      QUEUED AND THEN FOUND TO BE 0184 00041000
*                                      INVALID, THUS REQUIR-         */ 00042000
*       ;                           /* ING A DEQUEUE                 */ 00043000
         NI    DQSW,B'01111111'                                    0184 00044000
*   SW='0'B                         /* THIS SWITCH IS USED FOR A 'DO    00045000
*                                      WHILE' IN THE                 */ 00046000
*       ;                           /* PARAMETER PROCESSOR           */ 00047000
         NI    SW,B'01111111'                                      0185 00048000
*/********************************************************************/ 00049000
*/* STORAGE FOR THE LCST DATA BLOCK IS OBTAINED VIA WORK AREA        */ 00050000
*/* HANDLER (ISTSDCC7). AT ENTRY TO ISTSDCC7 TO OBTAIN STORAGE, WORK */ 00051000
*/* AREA HANDLER REQUIRES THE DESIRED LENGTH IN REG0, AND REG1       */ 00052000
*/* SHOULD CONTAIN ZEROES.  UPON RETURN FROM WORK AREA HANDLER,      */ 00053000
*/* REG15 IS ZERO IF STORAGE WAS OBTAINED, NONZERO IF NOT. IF        */ 00054000
*/* STORAGE WAS OBTAINED REG1 HAS THE ACTUAL (VIRTUAL) ADDRESS, AND  */ 00055000
*/* REG0 HAS THE RELATIVE OFFSET FROM THE BEGINNING OF THE           */ 00056000
*/* TABLE/STRING BUILD AREA.  THIS MODULE DOES NOT USE THE           */ 00057000
*/* INFORMATION RETURNED IN REG0.                                    */ 00058000
*/********************************************************************/ 00059000
*                                                                  0186 00060000
*   GPR0=LENGTH(ISTLCST);           /* SIZE OF AREA REQUIRED         */ 00061000
         LA    GPR0,20                                             0186 00062000
*   GPR1=0                          /* ZERO OUT R1 FOR STORAGE       */ 00063000
*       ;                           /* REQUEST                       */ 00064000
         SLR   GPR1,GPR1                                           0187 00065000
*   CALL ISTSDCC7                   /* OBTAIN STORAGE FOR LCST       */ 00066000
*       ;                           /* DATA BLOCK                    */ 00067000
         L     @15,@CV01219                                        0188 00068000
         BALR  @14,@15                                             0188 00069000
*   IF GPR15^=0                     /* WAS STORAGE OBTAINED?         */ 00070000
*     THEN                          /* NO-ERROR CONDITION            */ 00071000
         LTR   GPR15,GPR15                                         0189 00072000
         BZ    @RF00189                                            0189 00073000
*NOSPACE:                                                          0190 00074000
*     RC=4;                         /* RETURN CODE-KILL BUILD        */ 00075000
NOSPACE  LA    RC,4                                                0190 00076000
*   ELSE                            /* STORAGE WAS OBTAINED.         */ 00077000
*HAVSPACE:                                                         0191 00078000
*     DO;                           /* CAN BUILD A DATA BLOCK        */ 00079000
         B     @RC00189                                            0191 00080000
@RF00189 DS    0H                                                  0191 00081000
HAVSPACE DS    0H                                                  0192 00082000
*/********************************************************************/ 00083000
*/* THE DATA BLOCK WILL BE QUEUED UP NOW AND WILL BE DEQUEUED LATER  */ 00084000
*/* IF UNUSABLE (SEE LABEL DEQUEUE).                                 */ 00085000
*/********************************************************************/ 00086000
*                                                                  0192 00087000
*       LCSTREG=GPR1;               /* @ OF NEW LCST DATA BLOK       */ 00088000
         LR    LCSTREG,GPR1                                        0192 00089000
*       ISTLCST=ISTLCST&&ISTLCST;   /* ZERO OUT DATA BLOCK           */ 00090000
         XC    ISTLCST(20,LCSTREG),ISTLCST(LCSTREG)                0193 00091000
*       LCSTFWDP=RNCLCST;           /* CHAIN UP NEW DATA BLOCK       */ 00092000
         L     @10,RNCLCST(,RNCAREG)                               0194 00093000
         ST    @10,LCSTFWDP(,LCSTREG)                              0194 00094000
*       RNCLCST=LCSTREG;                                           0195 00095000
*                                                                  0195 00096000
         ST    LCSTREG,RNCLCST(,RNCAREG)                           0195 00097000
*/********************************************************************/ 00098000
*/* START FILLING IN THE LCST WITH WHAT IS KNOWN NOW                 */ 00099000
*/********************************************************************/ 00100000
*                                                                  0196 00101000
*       LCSTLCTP='1050 ';           /* DEFAULT LINE CNTRL TYPE       */ 00102000
         MVC   LCSTLCTP(5,LCSTREG),@CC01239                        0196 00103000
*       LCSTLCCD='1C'X;             /* DEFAULT LINE CNTRL CODE     0197 00104000
*                                                            @DY04863*/ 00105000
         MVI   LCSTLCCD(LCSTREG),X'1C'                             0197 00106000
*       LCSTNAME=MLCNAME            /* NAME OF MTALCST MACRO. THIS 0198 00107000
*                                      WILL LATER BE RE- FERRED TO IN   00108000
*                                      THE LCST= PARAMETER ON A VTERM*/ 00109000
*           ;                       /* MACRO.                        */ 00110000
*                                                                  0198 00111000
         MVC   LCSTNAME(8,LCSTREG),MLCNAME(MLCAREG)                0198 00112000
*       /*************************************************************/ 00113000
*       /*                                                           */ 00114000
*       /* SEGMENT(PARAMS)REQUEST                                    */ 00115000
*       /*                                                           */ 00116000
*       /*************************************************************/ 00117000
*                                                                  0199 00118000
*       DO;                                                        0199 00119000
*         GOTO PARAMS;                                             0200 00120000
         B     PARAMS                                              0200 00121000
*PARAMS#:                                                          0201 00122000
*       END;                        /* PROCESS PARAMETERS            */ 00123000
PARAMS#  DS    0H                                                  0202 00124000
*       RC=0                        /* RETURN CODE IS VALID        0202 00125000
*                                      REGARDLESS OF PARAM-          */ 00126000
*           ;                       /* PROCESSING OR ERRORS          */ 00127000
         SLR   RC,RC                                               0202 00128000
*     END HAVSPACE;                                                0203 00129000
*                                                                  0203 00130000
*/********************************************************************/ 00131000
*/* ALL PROCESSING NECESSARY TO BUILD AND CHAIN THE NEW LCST DATA    */ 00132000
*/* BLOCK IS NOW DONE. IF THE NEW DATA BLOCK IS INVALID, SWITCH DQSW */ 00133000
*/* WILL BE SET. IF IT IS SET, THE INVALID BLOCK MUST BE DEQUEUED    */ 00134000
*/* FROM THE CHAIN. THE STORAGE WILL BE LEFT AS GARBAGE TO BE FREED  */ 00135000
*/* BY GENEND.                                                       */ 00136000
*/********************************************************************/ 00137000
*                                                                  0204 00138000
*   IF DQSW='1'B                    /* INVALID BLOCK TO DEQUE?       */ 00139000
*     THEN                                                         0204 00140000
@RC00189 TM    DQSW,B'10000000'                                    0204 00141000
         BNO   @RF00204                                            0204 00142000
*     RNCLCST=LCSTFWDP;             /* YES-DEQUEUE THE BLOCK         */ 00143000
         L     @10,LCSTFWDP(,LCSTREG)                              0205 00144000
         ST    @10,RNCLCST(,RNCAREG)                               0205 00145000
*   ELSE                                                           0206 00146000
*     ;                             /* NOTHIN TO DEQUEUE             */ 00147000
@RF00204 DS    0H                                                  0207 00148000
*/********************************************************************/ 00149000
*/*                 RETURN TO CALLER.                                */ 00150000
*/*                 RC=0 IS GOOD RETURN.  RC=4 KILLS THE BUILD.      */ 00151000
*/********************************************************************/ 00152000
*                                                                  0207 00153000
*RETRN:                                                            0207 00154000
*   SAVREG=RNCSAV1(2);              /* RESTORE CALLER'S REGS         */ 00155000
RETRN    L     SAVREG,RNCSAV1+4(,RNCAREG)                          0207 00156000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00157000
         LR    @15,RC                                              0208 00158000
         L     @14,12(,@13)                                        0208 00159000
         LM    @00,@12,20(@13)                                     0208 00160000
         BR    @14                                                 0208 00161000
*                                                                  0209 00162000
*   /*****************************************************************/ 00163000
*   /*                                                               */ 00164000
*   /* SEGMENT(PARAMS)START                                          */ 00165000
*   /*                                                               */ 00166000
*   /*****************************************************************/ 00167000
*                                                                  0209 00168000
*   DO;                                                            0209 00169000
*     GOTO PARAMS@;                                                0210 00170000
*PARAMS:                                                           0211 00171000
*   END;                                                           0211 00172000
*                                                                  0211 00173000
PARAMS   DS    0H                                                  0212 00174000
*/********************************************************************/ 00175000
*/* PROCESSING THE PARAMETERS IS TABLE DRIVEN.  A DO WHILE LOOP      */ 00176000
*/* CAUSES PHR (PARAMETER HANDLER ROUTINE, ISTINCPH) TO ANALYSE EACH */ 00177000
*/* PARAMETER IN TURN. THEN A SPECIFIC PARAMETER HANDLER SUBROUTINE  */ 00178000
*/* IS CHOSEN FROM A TABLE, AND THE PARAMETER IS PROCESSED           */ 00179000
*/* APPROPRIATELY. PROCESSING ENDS WHEN ALL PARAMETERS HAVE BEEN     */ 00180000
*/* PROCESSED.                                                       */ 00181000
*/********************************************************************/ 00182000
*                                                                  0212 00183000
*CHKPARMS:                                                         0212 00184000
*   DO WHILE(SW='0'B)               /* LOOK AT EACH PARAMETER        */ 00185000
*         ;                         /* IN TURN                       */ 00186000
CHKPARMS B     @DE00212                                            0212 00187000
@DL00212 DS    0H                                                  0213 00188000
*/********************************************************************/ 00189000
*/*          PHR REQUIRES THE FOLLOWING INPUT:                       */ 00190000
*/*               REG0 - @ OF PARAMETER TABLE                        */ 00191000
*/*               REG1 - @ OF FIRST/NEXT KEYWORD                     */ 00192000
*/*               REG2 - @ OF PARAMETER LIST CREATED BY PHR          */ 00193000
*/********************************************************************/ 00194000
*                                                                  0213 00195000
*     GPR0=ADDR(MTAKWTBL)           /* @ PARAMETER KEY WORD          */ 00196000
*         ;                         /* TABLE                         */ 00197000
         LA    GPR0,MTAKWTBL                                       0213 00198000
*     GPR1=MLCKWD;                  /* NEXT KEYWORD @                */ 00199000
         L     GPR1,MLCKWD(,MLCAREG)                               0214 00200000
*     PHRREG=ADDR(RNCPHR1);         /* @ OF PARAMETER LIST           */ 00201000
         LA    PHRREG,RNCPHR1(,RNCAREG)                            0215 00202000
*     CALL ISTINCPH;                /* LOOK FOR 1ST PARAMETER        */ 00203000
         L     @15,@CV01217                                        0216 00204000
         BALR  @14,@15                                             0216 00205000
*/********************************************************************/ 00206000
*/*          PHR OUTPUT VARIES ACCORDING TO THE FUNCTION             */ 00207000
*/*          USED AND THE KEYWORD TYPE.  THE OUTPUT EXPECTED         */ 00208000
*/*          AT THIS TIME IS AS FOLLOWS:                             */ 00209000
*/*               REG0 - @ OF PARAMETER HANDLING SUBROUTINE          */ 00210000
*/*               REG1 - @ OF THE NEXT KEYWORD                       */ 00211000
*/*               REG2 - @ OF DATA FOR PARAMETER HANDLER. THE        */ 00212000
*/*                      DATA IS DERIVED FROM THE SUBTABLES          */ 00213000
*/*                      SUPPLIED TO PHR FOR THE KEYWORD             */ 00214000
*/*               REG15- RETURN CODE FROM PHR                        */ 00215000
*/********************************************************************/ 00216000
*                                                                  0217 00217000
*     KWDPTR=GPR1;                  /* SAVE @ OF NEXT KEYWORD        */ 00218000
         LR    KWDPTR,GPR1                                         0217 00219000
*     IF GPR15=0                    /* VALID KEYWORD FOUND?          */ 00220000
*       THEN                        /* YES-HANDLE PARAMETER          */ 00221000
         LTR   GPR15,GPR15                                         0218 00222000
         BNZ   @RF00218                                            0218 00223000
*FOUND:                                                            0219 00224000
*       DO;                                                        0219 00225000
FOUND    DS    0H                                                  0220 00226000
*         GPR15=GPR0;               /* @ OF PARAMETER HANDLER      0220 00227000
*                                      REG2-ALREADY SET UP           */ 00228000
         LR    GPR15,GPR0                                          0220 00229000
*         CALL PARMHDLR;            /* HANDLE PARAMETER              */ 00230000
         BALR  @14,@15                                             0221 00231000
*MAINSUB:                                                          0222 00232000
*         ;                         /* RETURN @ FROM SUBROUTIN       */ 00233000
MAINSUB  DS    0H                                                  0223 00234000
*         MLCKWD=KWDPTR;            /* POINT TO NEXT KEYWORD         */ 00235000
         ST    KWDPTR,MLCKWD(,MLCAREG)                             0223 00236000
*       END FOUND;                  /* AND LOOK AT IT                */ 00237000
*     ELSE                          /* NOT VALID KEY WORD.           */ 00238000
*NOTFOUND:                                                         0225 00239000
*       DO;                         /* CHECK FOR ERROR               */ 00240000
         B     @RC00218                                            0225 00241000
@RF00218 DS    0H                                                  0225 00242000
NOTFOUND DS    0H                                                  0226 00243000
*         IF GPR15=4                /* UNEXPECTED KEYWORD?           */ 00244000
*           THEN                    /* YES-POSSIBLE ERROR            */ 00245000
         C     GPR15,@CF00051                                      0226 00246000
         BNE   @RF00226                                            0226 00247000
*XTRAPARM:                                                         0227 00248000
*           DO;                                                    0227 00249000
*                                                                  0227 00250000
XTRAPARM DS    0H                                                  0228 00251000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 00252000
*                                                                  0228 00253000
*             DO;                                                  0228 00254000
*               DCL                                                0229 00255000
*                 1 CMZP0005 STATIC LOCAL,/* PARAMETER LIST PASSED TO   00256000
*                                      ERROR FORMAT ROUTINE          */ 00257000
*                  2 CMZL0005 FIXED(8) INIT(4),/* LEVEL PARAMETER  0229 00258000
*                                      (CLEV)                        */ 00259000
*                  2 CMZE0005 FIXED(8) INIT(4),/* ERROR PARAMETER  0229 00260000
*                                      (CERR)                        */ 00261000
*                  2 CMZA0005 CHAR(1) INIT('1'),/* ACTION PARAMETER     00262000
*                                      (CACT)                        */ 00263000
*                  2 CMZS0005 FIXED(8) INIT(0);/* LENGTH OF STRING 0229 00264000
*                                      PARAM                         */ 00265000
*               DCL                                                0230 00266000
*                 CMZR0005 REGISTER(1);/* ESTABLISH PARAMeter        */ 00267000
*               RESPECIFY                                          0231 00268000
*                 CMZR0005 RSTD;    /* REGISTER FOR                  */ 00269000
*               CMZR0005=ADDR(CMZP0005);/* ADDRESS OF PARAMETER LIST */ 00270000
         LA    CMZR0005,CMZP0005                                   0232 00271000
*               CALL ISTSDCC2;      /* CALL ERROR FORMAT ROUTINE     */ 00272000
         L     @15,@CV01218                                        0233 00273000
         BALR  @14,@15                                             0233 00274000
*               RESPECIFY                                          0234 00275000
*                 CMZR0005 UNRSTD;                                 0234 00276000
*             END;                                                 0235 00277000
*             MLCKWD=KWDPTR;        /* POINT TO NEXT KEYWORD         */ 00278000
         ST    KWDPTR,MLCKWD(,MLCAREG)                             0236 00279000
*           END XTRAPARM;           /* LOOK FOR NEXT KEYWORD         */ 00280000
*         ELSE                                                     0238 00281000
*NOTXTRA:                                                          0238 00282000
*           DO;                                                    0238 00283000
         B     @RC00226                                            0238 00284000
@RF00226 DS    0H                                                  0238 00285000
NOTXTRA  DS    0H                                                  0239 00286000
*             IF GPR15=8            /* END OF KEYWORD STRING?        */ 00287000
*               THEN                /* YES-CHECK AND SET             */ 00288000
         C     GPR15,@CF00035                                      0239 00289000
         BNE   @RF00239                                            0239 00290000
*CLEANUP:                                                          0240 00291000
*               DO;                                                0240 00292000
*                                                                  0240 00293000
CLEANUP  DS    0H                                                  0241 00294000
*                 /***************************************************/ 00295000
*                 /*                                                 */ 00296000
*                 /* SEGMENT(SETDFLT)REQUEST                         */ 00297000
*                 /*                                                 */ 00298000
*                 /***************************************************/ 00299000
*                                                                  0241 00300000
*                 DO;                                              0241 00301000
*                   GOTO SETDFLT;                                  0242 00302000
         B     SETDFLT                                             0242 00303000
*SETDFLT#:                                                         0243 00304000
*                 END;              /* BLOCKS                        */ 00305000
SETDFLT# DS    0H                                                  0244 00306000
*               END CLEANUP;        /* LABEL 'GETOUT' IS NEXT        */ 00307000
*             ELSE                                                 0245 00308000
*BADSYNTX:                                                         0245 00309000
*               DO;                                                0245 00310000
*                                                                  0245 00311000
         B     @RC00239                                            0245 00312000
@RF00239 DS    0H                                                  0245 00313000
BADSYNTX DS    0H                                                  0246 00314000
*/* ERRFMT    LEVEL(PARM)  ERROR(SYN)  ACTION(IGNORE)                */ 00315000
*                                                                  0246 00316000
*                 DO;                                              0246 00317000
*                   DCL                                            0247 00318000
*                     1 CMZP0007 STATIC LOCAL,/* PARAMETER LIST    0247 00319000
*                                      PASSED TO ERROR FORMAT ROUTINE*/ 00320000
*                      2 CMZL0007 FIXED(8) INIT(4),/* LEVEL PARAMETER   00321000
*                                      (CLEV)                        */ 00322000
*                      2 CMZE0007 FIXED(8) INIT(6),/* ERROR PARAMETER   00323000
*                                      (CERR)                        */ 00324000
*                      2 CMZA0007 CHAR(1) INIT('1'),/* ACTION      0247 00325000
*                                      PARAMETER (CACT)              */ 00326000
*                      2 CMZS0007 FIXED(8) INIT(0);/* LENGTH OF    0247 00327000
*                                      STRING PARAM                  */ 00328000
*                   DCL                                            0248 00329000
*                     CMZR0007 REGISTER(1);/* ESTABLISH PARAMeter    */ 00330000
*                   RESPECIFY                                      0249 00331000
*                     CMZR0007 RSTD;/* REGISTER FOR                  */ 00332000
*                   CMZR0007=ADDR(CMZP0007);/* ADDRESS OF PARAMETER     00333000
*                                      LIST                          */ 00334000
         LA    CMZR0007,CMZP0007                                   0250 00335000
*                   CALL ISTSDCC2;  /* CALL ERROR FORMAT ROUTINE     */ 00336000
         L     @15,@CV01218                                        0251 00337000
         BALR  @14,@15                                             0251 00338000
*                   RESPECIFY                                      0252 00339000
*                     CMZR0007 UNRSTD;                             0252 00340000
*                 END;                                             0253 00341000
*SETTODQ:                                                          0254 00342000
*                 DQSW='1'B;        /* SET TO DEQUEUE DATA BLK       */ 00343000
SETTODQ  OI    DQSW,B'10000000'                                    0254 00344000
*               END BADSYNTX;                                      0255 00345000
*                                                                  0255 00346000
*/********************************************************************/ 00347000
*/*          WHETHER ERROR OR END OF PARAMETER LIST, NO MORE         */ 00348000
*/*          PARAMETERS ARE TO BE PROCESSED.                         */ 00349000
*/********************************************************************/ 00350000
*                                                                  0256 00351000
*GETOUT:                                                           0256 00352000
*             SW='1'B;              /* GET OUT OF LOOP               */ 00353000
@RC00239 DS    0H                                                  0256 00354000
GETOUT   OI    SW,B'10000000'                                      0256 00355000
*           END NOTXTRA;                                           0257 00356000
*       END NOTFOUND;                                              0258 00357000
@RC00226 DS    0H                                                  0259 00358000
*   END CHKPARMS;                                                  0259 00359000
*                                                                  0259 00360000
@RC00218 DS    0H                                                  0259 00361000
@DE00212 TM    SW,B'10000000'                                      0259 00362000
         BZ    @DL00212                                            0259 00363000
*   /*****************************************************************/ 00364000
*   /*                                                               */ 00365000
*   /* SEGMENT(PARAMS)FINISH                                         */ 00366000
*   /*                                                               */ 00367000
*   /*****************************************************************/ 00368000
*                                                                  0260 00369000
*   DO;                                                            0260 00370000
*     GOTO PARAMS#;                                                0261 00371000
         B     PARAMS#                                             0261 00372000
*PARAMS@:                                                          0262 00373000
*   END;                            /* RETURN TO CALLER              */ 00374000
PARAMS@  DS    0H                                                  0263 00375000
*                                                                  0263 00376000
*   /*****************************************************************/ 00377000
*   /*                                                               */ 00378000
*   /* SEGMENT(SETDFLT)START                                         */ 00379000
*   /*                                                               */ 00380000
*   /*****************************************************************/ 00381000
*                                                                  0263 00382000
*   DO;                                                            0263 00383000
*     GOTO SETDFLT@;                                               0264 00384000
         B     SETDFLT@                                            0264 00385000
*SETDFLT:                                                          0265 00386000
*   END;                                                           0265 00387000
*                                                                  0265 00388000
SETDFLT  DS    0H                                                  0266 00389000
*/********************************************************************/ 00390000
*/*          AT THIS POINT ALL THE PARAMETERS ON THE MTALCST         */ 00391000
*/*          MACRO HAVE BEEN PROCESSED VALIDLY.                      */ 00392000
*/*                                                                  */ 00393000
*/*          SOME VALUES TO BE ENTERED INTO THE LCST DATA            */ 00394000
*/*          BLOCKS HAVE DEFAULT VALUES FOR OTHER INFORMATION        */ 00395000
*/*          SPECIFIED.  THE VALUES MUST BE ENTERED NOW IF NO        */ 00396000
*/*          SPECIAL VALUE WAS SUPPLIED ON THE MACRO CALL.           */ 00397000
*/********************************************************************/ 00398000
*                                                                  0266 00399000
*   IF LCSTACR2='1'B                /* WAS ACR OPTION ON A           */ 00400000
*       &LCSTLCTP^='1050 '          /* NON-1050 DEVICE?              */ 00401000
*     THEN                          /* YES-ERR CONDITION. ACR        */ 00402000
         TM    LCSTACR2(LCSTREG),B'00100000'                       0266 00403000
         BNO   @RF00266                                            0266 00404000
         CLC   LCSTLCTP(5,LCSTREG),@CC01239                        0266 00405000
         BE    @RF00266                                            0266 00406000
*ACRINVAL:                                                         0267 00407000
*     DO;                           /* IS ONLY LEGAL ON 1050.        */ 00408000
ACRINVAL DS    0H                                                  0268 00409000
*/* ERRFMT    LEVEL(PARM)  ERROR(CONF)  ACTION(IGNORE)  STRING('ACR')*/ 00410000
*                                                                  0268 00411000
*       DO;                                                        0268 00412000
*         DCL                                                      0269 00413000
*           1 CMZP0010 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   00414000
*                                      FORMAT ROUTINE                */ 00415000
*            2 CMZL0010 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)   */ 00416000
*            2 CMZE0010 FIXED(8) INIT(2),/* ERROR PARAMETER (CERR)   */ 00417000
*            2 CMZA0010 CHAR(1) INIT('1'),/* ACTION PARAMETER (CACT) */ 00418000
*            2 CMZS0010 FIXED(8) INIT(3),/* LENGTH OF STRING PARAM   */ 00419000
*            2 CMZM0010 CHAR(3) INIT('ACR');/* PARAMETER STRING      */ 00420000
*         DCL                                                      0270 00421000
*           CMZR0010 REGISTER(1);   /* ESTABLISH PARAMeter           */ 00422000
*         RESPECIFY                                                0271 00423000
*           CMZR0010 RSTD;          /* REGISTER FOR                  */ 00424000
*         CMZR0010=ADDR(CMZP0010);  /* ADDRESS OF PARAMETER LIST     */ 00425000
         LA    CMZR0010,CMZP0010                                   0272 00426000
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */ 00427000
         L     @15,@CV01218                                        0273 00428000
         BALR  @14,@15                                             0273 00429000
*         RESPECIFY                                                0274 00430000
*           CMZR0010 UNRSTD;                                       0274 00431000
*       END;                                                       0275 00432000
*       DQSW='1'B;                  /* DEQUEUE DATE BLOCK SW         */ 00433000
         OI    DQSW,B'10000000'                                    0276 00434000
*     END ACRINVAL;                                                0277 00435000
*   ELSE                                                           0278 00436000
*     ;                             /* NO INVALID COMBINATION-       */ 00437000
*                                                                  0278 00438000
@RF00266 DS    0H                                                  0279 00439000
*   /*****************************************************************/ 00440000
*   /*                                                               */ 00441000
*   /* SEGMENT(SETDFLT)FINISH                                        */ 00442000
*   /*                                                               */ 00443000
*   /*****************************************************************/ 00444000
*                                                                  0279 00445000
*   DO;                                                            0279 00446000
*     GOTO SETDFLT#;                                               0280 00447000
         B     SETDFLT#                                            0280 00448000
*SETDFLT@:                                                         0281 00449000
*   END;                                                           0281 00450000
*                                                                  0281 00451000
*/*  RETURN TO CALLER                                                */ 00452000
*                                                                  0282 00453000
*                                                                  0282 00454000
*/********************************************************************/ 00455000
*/* THIS ROUTINE IS A NOP FOR PARMS WHICH ARE NOT USED BY SYSDEF.    */ 00456000
*/********************************************************************/ 00457000
*                                                                  0282 00458000
*MTNOP:                                                            0282 00459000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0282 00460000
         B     @PB00002                                            0282 00461000
MTNOP    DS    0H                                                  0283 00462000
*   GPR15=0;                        /* VALID RETURN CODE             */ 00463000
         SLR   GPR15,GPR15                                         0283 00464000
