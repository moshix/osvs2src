         TITLE 'ISTSDCP2 LCST PARAM HANDLER                            *00001000
                        '                                               00002000
ISTSDCP2 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCP2  75.172'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0170 00014000
*/********************************************************************/ 00015000
*/*            P R O C E S S I N G                                   */ 00016000
*/*                                                                  */ 00017000
*/*                       SAVE CALLER'S REGS                         */ 00018000
*/********************************************************************/ 00019000
*                                                                  0170 00020000
*   RNCSAV2(2)=GPR13;               /* BACKWARD PTR                  */ 00021000
         ST    GPR13,RNCSAV2+4(,RNCAREG)                           0170 00022000
*   GPR13=ADDR(RNCSAV2);            /* SET UP OUR SAVE REG           */ 00023000
         LA    GPR13,RNCSAV2(,RNCAREG)                             0171 00024000
*   RNCSAV1(3)=GPR13;               /* FORWARD PTR                   */ 00025000
         ST    GPR13,RNCSAV1+8(,RNCAREG)                           0172 00026000
*   DATDREG=ADDR(RNCDAT2);          /* SET UP OUR DATA REG           */ 00027000
         LA    DATDREG,RNCDAT2(,RNCAREG)                           0173 00028000
*   GENERATE NOREFS NOSETS;                                        0174 00029000
*                                                                  0174 00030000
           USING @DATD,11               ADDRESSABILITY FOR DATA         00031000
*/********************************************************************/ 00032000
*/*                      INITIALIZATION                              */ 00033000
*/********************************************************************/ 00034000
*                                                                  0175 00035000
*   SW=0;                           /* INIT LOOP CONTROL SW TO 0     */ 00036000
         SLR   @10,@10                                             0175 00037000
         LR    SW,@10                                              0175 00038000
*   LCSTREG=RNCLCST;                /* PTR TO LCST DATA BLK CHAIN    */ 00039000
         L     @09,RNCLCST(,RNCAREG)                               0176 00040000
         LR    LCSTREG,@09                                         0176 00041000
*   RTRREG=MLCPTR3;                 /* PTR TO TERMINAL MODEL ENTRY 0177 00042000
*                                      BEING BUILT                   */ 00043000
         L     RTRREG,MLCPTR3(,MLCAREG)                            0177 00044000
*   IF RNCLCST=0                    /* EMTPY LCST DATA BLK Q         */ 00045000
*     THEN                          /* SEND ERROR MSG                */ 00046000
         CR    @09,@10                                             0178 00047000
         BNE   @RF00178                                            0178 00048000
*     DO;                                                          0179 00049000
*                                                                  0179 00050000
*/* ERRFMT    LEVEL(VALUE)  ERROR(MISS)  ACTION(KILLBLD)             */ 00051000
*                                                                  0180 00052000
*       DO;                                                        0180 00053000
*         DCL                                                      0181 00054000
*           1 CMZP0003 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   00055000
*                                      FORMAT ROUTINE                */ 00056000
*            2 CMZL0003 FIXED(8) INIT(5),/* LEVEL PARAMETER (CLEV)   */ 00057000
*            2 CMZE0003 FIXED(8) INIT(8),/* ERROR PARAMETER (CERR)   */ 00058000
*            2 CMZA0003 CHAR(1) INIT('6'),/* ACTION PARAMETER (CACT) */ 00059000
*            2 CMZS0003 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 00060000
*         DCL                                                      0182 00061000
*           CMZR0003 REGISTER(1);   /* ESTABLISH PARAMeter           */ 00062000
*         RESPECIFY                                                0183 00063000
*           CMZR0003 RSTD;          /* REGISTER FOR                  */ 00064000
*         CMZR0003=ADDR(CMZP0003);  /* ADDRESS OF PARAMETER LIST     */ 00065000
         LA    CMZR0003,CMZP0003                                   0184 00066000
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */ 00067000
         L     @15,@CV01215                                        0185 00068000
         BALR  @14,@15                                             0185 00069000
*         RESPECIFY                                                0186 00070000
*           CMZR0003 UNRSTD;                                       0186 00071000
*       END;                                                       0187 00072000
*       RC=4;                                                      0188 00073000
         MVC   RC(2),@CH00040                                      0188 00074000
*     END;                                                         0189 00075000
*   ELSE                                                           0190 00076000
*                                                                  0190 00077000
*/********************************************************************/ 00078000
*/* LCST DATA BLOCK Q NOT EMPTY - SEARCH Q FOR A DATA BLOCK WHOSE    */ 00079000
*/* NAME MATCHES THAT SPECIFIED IN THE LCST PARAM,LCST=XXX           */ 00080000
*/********************************************************************/ 00081000
*                                                                  0190 00082000
*CP2LDOLP:                                                         0190 00083000
*     DO WHILE(SW=0);               /* LOOP TO SEARCH LCST CHAIN     */ 00084000
         B     @RC00178                                            0190 00085000
@RF00178 DS    0H                                                  0190 00086000
CP2LDOLP B     @DE00190                                            0190 00087000
@DL00190 DS    0H                                                  0191 00088000
*       IF PHRF34SP(1)=LCSTNAME     /* IF PARAM STRING=LCST NAME     */ 00089000
*/********************************************************************/ 00090000
*/* MATCHING MTALCST DATA BLK FOUND - MOVE PERTINENT TERMINAL INFORM-*/ 00091000
*/* ATION TO THE TERMINAL MODEL ENTRY BEING BUILT IN THE SIFT AND    */ 00092000
*/* DEFAULT TABLES.                                                  */ 00093000
*/********************************************************************/ 00094000
*                                                                  0191 00095000
*         THEN                                                     0191 00096000
         CLC   PHRF34SP(8,PHRREG),LCSTNAME(LCSTREG)                0191 00097000
         BNE   @RF00191                                            0191 00098000
*         DO;                                                      0192 00099000
*           RTRLCTI=LCSTLCCD;       /* MOVE LINE CONTROL CODE TO RTR */ 00100000
         MVC   RTRLCTI(1,RTRREG),LCSTLCCD(LCSTREG)                 0193 00101000
*           IF LCSTACR2='1'B        /* IF ACR2 IS ON                 */ 00102000
*             THEN                                                 0194 00103000
         TM    LCSTACR2(LCSTREG),B'00100000'                       0194 00104000
         BNO   @RF00194                                            0194 00105000
*             RTRREACR='1'B;        /* SET RTR ACR                   */ 00106000
         OI    RTRREACR(RTRREG),B'00100000'                        0195 00107000
*           ELSE                                                   0196 00108000
*             RTRREACR='0'B;        /* ELSE SET RTR ACR OFF          */ 00109000
         B     @RC00194                                            0196 00110000
@RF00194 NI    RTRREACR(RTRREG),B'11011111'                        0196 00111000
*           RC=0;                   /* DONE,SET RC=0 SUCCESSFUL      */ 00112000
@RC00194 SLR   @10,@10                                             0197 00113000
         STH   @10,RC                                              0197 00114000
*           SW=1;                   /* GET OUT OF SEARCH LOOP        */ 00115000
         LA    SW,1                                                0198 00116000
*         END;                                                     0199 00117000
*       ELSE                                                       0200 00118000
*                                                                  0200 00119000
*/********************************************************************/ 00120000
*/* BUMP TO NEXT LCST DATA BLK ON QUEUE AND CONTINUE SEARCHING FOR   */ 00121000
*/* MATCHING LCST DATA BLOCK.                                        */ 00122000
*/********************************************************************/ 00123000
*                                                                  0200 00124000
*         IF LCSTFWDP=0             /* END OF Q ?                    */ 00125000
*           THEN                                                   0200 00126000
         B     @RC00191                                            0200 00127000
@RF00191 ICM   @10,15,LCSTFWDP(LCSTREG)                            0200 00128000
         BNZ   @RF00200                                            0200 00129000
*           DO;                     /* YES,NO MATCH FOUND-ERROR      */ 00130000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(KILLBLD)             */ 00131000
*                                                                  0202 00132000
*             DO;                                                  0202 00133000
*               DCL                                                0203 00134000
*                 1 CMZP0004 STATIC LOCAL,/* PARAMETER LIST PASSED TO   00135000
*                                      ERROR FORMAT ROUTINE          */ 00136000
*                  2 CMZL0004 FIXED(8) INIT(4),/* LEVEL PARAMETER  0203 00137000
*                                      (CLEV)                        */ 00138000
*                  2 CMZE0004 FIXED(8) INIT(4),/* ERROR PARAMETER  0203 00139000
*                                      (CERR)                        */ 00140000
*                  2 CMZA0004 CHAR(1) INIT('6'),/* ACTION PARAMETER     00141000
*                                      (CACT)                        */ 00142000
*                  2 CMZS0004 FIXED(8) INIT(0);/* LENGTH OF STRING 0203 00143000
*                                      PARAM                         */ 00144000
*               DCL                                                0204 00145000
*                 CMZR0004 REGISTER(1);/* ESTABLISH PARAMeter        */ 00146000
*               RESPECIFY                                          0205 00147000
*                 CMZR0004 RSTD;    /* REGISTER FOR                  */ 00148000
*               CMZR0004=ADDR(CMZP0004);/* ADDRESS OF PARAMETER LIST */ 00149000
         LA    CMZR0004,CMZP0004                                   0206 00150000
*               CALL ISTSDCC2;      /* CALL ERROR FORMAT ROUTINE     */ 00151000
         L     @15,@CV01215                                        0207 00152000
         BALR  @14,@15                                             0207 00153000
*               RESPECIFY                                          0208 00154000
*                 CMZR0004 UNRSTD;                                 0208 00155000
*             END;                                                 0209 00156000
*             RC=4;                 /* SET RC=4 KILL BUILD           */ 00157000
         MVC   RC(2),@CH00040                                      0210 00158000
*             SW=1;                 /* GET OUT OF LOOP- RETURN       */ 00159000
         LA    SW,1                                                0211 00160000
*           END;                                                   0212 00161000
*         ELSE                                                     0213 00162000
*           LCSTREG=LCSTFWDP;       /* NOT END OF Q,BUMP LCST PTR TO    00163000
*                                      NEXT DATA BLOCK               */ 00164000
         B     @RC00200                                            0213 00165000
@RF00200 L     LCSTREG,LCSTFWDP(,LCSTREG)                          0213 00166000
*     END CP2LDOLP;                 /* END OF DO WHILE SRCH LOOP     */ 00167000
@RC00200 DS    0H                                                  0214 00168000
@RC00191 DS    0H                                                  0214 00169000
@DE00190 LTR   SW,SW                                               0214 00170000
         BZ    @DL00190                                            0214 00171000
*/********************************************************************/ 00172000
*/*                     RETURN TO CALLER                             */ 00173000
*/********************************************************************/ 00174000
*                                                                  0215 00175000
*   GPR13=RNCSAV2(2);               /* RESTORE CALLER'S REGS         */ 00176000
@RC00178 L     GPR13,RNCSAV2+4(,RNCAREG)                           0215 00177000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00178000
         LH    @15,RC                                              0216 00179000
         L     @14,12(,@13)                                        0216 00180000
         LM    @00,@12,20(@13)                                     0216 00181000
         BR    @14                                                 0216 00182000
*/* TPEPILOG                                                         */ 00183000
*                                                                  0217 00184000
*   GEN(PRINT OFF);                                                0217 00185000
         PRINT OFF                                                      00186000
*   GEN(PRINT ON);                                                 0218 00187000
*                                                                  0218 00188000
         PRINT ON                                                       00189000
*/* END OF TPEPILOG                                                  */ 00190000
*                                                                  0219 00191000
*   END ISTSDCP2;                                                  0219 00192000
@EL00001 DS    0H                                                  0219 00193000
@EF00001 DS    0H                                                  0219 00194000
@ER00001 LM    @14,@12,12(@13)                                     0219 00195000
         BR    @14                                                 0219 00196000
@DATA    DS    0H                                                       00197000
