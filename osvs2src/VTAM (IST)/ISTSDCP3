         TITLE ' ISTSDCP3 VIDSEQ PARAMETER PROCESSOR                   *00001000
                        '                                               00002000
ISTSDCP3 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCP3  75.130'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0177 00014000
*/********************************************************************/ 00015000
*/*                      ENTRY LINKAGE                               */ 00016000
*/********************************************************************/ 00017000
*                                                                  0177 00018000
*   RNCSAV2(2)=SAVREG;              /* SET UP SAVE AREA PTRS         */ 00019000
         ST    SAVREG,RNCSAV2+4(,RNCAREG)                          0177 00020000
*   SAVREG=ADDR(RNCSAV2);           /* BY SYSDEF CONVENTIONS         */ 00021000
         LA    SAVREG,RNCSAV2(,RNCAREG)                            0178 00022000
*   RNCSAV1(3)=SAVREG;                                             0179 00023000
         ST    SAVREG,RNCSAV1+8(,RNCAREG)                          0179 00024000
*   DATDREG=ADDR(RNCDAT2);          /* SET UP VIDSEQ'S DATA REG      */ 00025000
         LA    DATDREG,RNCDAT2(,RNCAREG)                           0180 00026000
*   GENERATE NOREFS NOSETS;                                        0181 00027000
*                                                                  0181 00028000
           USING @DATD,11                    @ABILITY FOR DATA          00029000
*/********************************************************************/ 00030000
*/*          THE VIDSEQ PARAMETER MAY EXIST ON EITHER A GROUP        */ 00031000
*/*          OR LINE MACRO (LINE SUPERCEEDING), BUT MAY BE           */ 00032000
*/*          VALIDLY SPECIFIED ONLY ONCE PER MACRO.  A VIDSEQ        */ 00033000
*/*          FOOTPRINT IS ZEROED BY THE GROUP AND LINE MACRO         */ 00034000
*/*          HANDLERS UPON ENTRY.  THE FIRST VIDSEQ PARAMETER        */ 00035000
*/*          ENCOUNTERED IS THE ONE USED (WHETHER IT IS VALID        */ 00036000
*/*          OR IN ERROR).  AN ADDITIONAL VIDSEQ PARAMETER           */ 00037000
*/*          FOR THE SAME MACRO WILL DETECTED BY THE FOOT-           */ 00038000
*/*          PRINT AND IGNORED.                                      */ 00039000
*/********************************************************************/ 00040000
*                                                                  0182 00041000
*   IF RNCGVSEQ='1'B                /* FIRST VIDSEQ ON MACRO?        */ 00042000
*     THEN                          /* NO-ERROR CONDITION            */ 00043000
         TM    RNCGVSEQ(RNCAREG),B'00000001'                       0182 00044000
         BNO   @RF00182                                            0182 00045000
*DUPARAM:                                                          0183 00046000
*     DO;                                                          0183 00047000
*                                                                  0183 00048000
DUPARAM  DS    0H                                                  0184 00049000
*/* ERRFMT    LEVEL(PARM)  ERROR(DUP)  ACTION(USEFIRST)              */ 00050000
*                                                                  0184 00051000
*       DO;                                                        0184 00052000
*         DCL                                                      0185 00053000
*           1 CMZP0003 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   00054000
*                                      FORMAT ROUTINE                */ 00055000
*            2 CMZL0003 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)   */ 00056000
*            2 CMZE0003 FIXED(8) INIT(3),/* ERROR PARAMETER (CERR)   */ 00057000
*            2 CMZA0003 CHAR(1) INIT('4'),/* ACTION PARAMETER (CACT) */ 00058000
*            2 CMZS0003 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 00059000
*         DCL                                                      0186 00060000
*           CMZR0003 REGISTER(1);   /* ESTABLISH PARAMeter           */ 00061000
*         RESPECIFY                                                0187 00062000
*           CMZR0003 RSTD;          /* REGISTER FOR                  */ 00063000
*         CMZR0003=ADDR(CMZP0003);  /* ADDRESS OF PARAMETER LIST     */ 00064000
         LA    CMZR0003,CMZP0003                                   0188 00065000
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */ 00066000
         L     @15,@CV01230                                        0189 00067000
         BALR  @14,@15                                             0189 00068000
*         RESPECIFY                                                0190 00069000
*           CMZR0003 UNRSTD;                                       0190 00070000
*       END;                                                       0191 00071000
*       RC=0;                       /* RETURN CODE-CONTINUE          */ 00072000
         SLR   @14,@14                                             0192 00073000
         STH   @14,RC                                              0192 00074000
*     END DUPARAM;                                                 0193 00075000
*   ELSE                                                           0194 00076000
*FRSTPARM:                                                         0194 00077000
*     DO;                           /* 1ST VIDSEQ PARAM              */ 00078000
         B     @RC00182                                            0194 00079000
@RF00182 DS    0H                                                  0194 00080000
FRSTPARM DS    0H                                                  0195 00081000
*       RNCGVSEQ='1'B;              /* FOOTPRINT FOR VIDSEQ ON       */ 00082000
         OI    RNCGVSEQ(RNCAREG),B'00000001'                       0195 00083000
*/********************************************************************/ 00084000
*/*          UPON RETURN FROM ISTINCPH (PHR) FUNCTION 4, SOME        */ 00085000
*/*          VALIDITY FLAGS MAY HAVE BEEN SET TO INDICATE            */ 00086000
*/*          THAT THE PARAMATER HAD AN INVALID STRUCTURE.  IF        */ 00087000
*/*          NO FLAGS ARE SET IN THE FUNCTION 4 OUTPUT AREA,         */ 00088000
*/*          THE PARAMETER IS CONSIDERED VALID, OTHERWISE THE        */ 00089000
*/*          BUILD IS KILLED.                                        */ 00090000
*/********************************************************************/ 00091000
*                                                                  0196 00092000
*       IF PHRF34VF(1)^=PHRVFAOK    /* INVALIDITY FLAGS SET?         */ 00093000
*         THEN                      /* YES-ERROR CONDITION           */ 00094000
         CLI   PHRF34VF(INDATA),X'00'                              0196 00095000
         BE    @RF00196                                            0196 00096000
*BADFORM:                                                          0197 00097000
*         DO;                                                      0197 00098000
*                                                                  0197 00099000
BADFORM  DS    0H                                                  0198 00100000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(KILLBLD)             */ 00101000
*                                                                  0198 00102000
*           DO;                                                    0198 00103000
*             DCL                                                  0199 00104000
*               1 CMZP0004 STATIC LOCAL,/* PARAMETER LIST PASSED TO     00105000
*                                      ERROR FORMAT ROUTINE          */ 00106000
*                2 CMZL0004 FIXED(8) INIT(4),/* LEVEL PARAMETER    0199 00107000
*                                      (CLEV)                        */ 00108000
*                2 CMZE0004 FIXED(8) INIT(4),/* ERROR PARAMETER    0199 00109000
*                                      (CERR)                        */ 00110000
*                2 CMZA0004 CHAR(1) INIT('6'),/* ACTION PARAMETER  0199 00111000
*                                      (CACT)                        */ 00112000
*                2 CMZS0004 FIXED(8) INIT(0);/* LENGTH OF STRING   0199 00113000
*                                      PARAM                         */ 00114000
*             DCL                                                  0200 00115000
*               CMZR0004 REGISTER(1);/* ESTABLISH PARAMeter          */ 00116000
*             RESPECIFY                                            0201 00117000
*               CMZR0004 RSTD;      /* REGISTER FOR                  */ 00118000
*             CMZR0004=ADDR(CMZP0004);/* ADDRESS OF PARAMETER LIST   */ 00119000
         LA    CMZR0004,CMZP0004                                   0202 00120000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 00121000
         L     @15,@CV01230                                        0203 00122000
         BALR  @14,@15                                             0203 00123000
*             RESPECIFY                                            0204 00124000
*               CMZR0004 UNRSTD;                                   0204 00125000
*           END;                                                   0205 00126000
*           RC=4;                   /* RETURN CODE-KILL BUILD        */ 00127000
         MVC   RC(2),@CH00051                                      0206 00128000
*         END BADFORM;                                             0207 00129000
*                                                                  0207 00130000
*/********************************************************************/ 00131000
*/*          THE VIDLIST MACRO (WHEN HANDLING THE VIDSEQ             */ 00132000
*/*          PARAM) SHOULD HAVE PREVIOUSLY BUILT A VIDHDR,           */ 00133000
*/*          VERIFICATION ID HEADER, WHICH CORRESPONDS TO THE        */ 00134000
*/*          NAME SUPPLIED ON THE VIDSEQ PARAMETER OF THE            */ 00135000
*/*          LINE OR GROUP MACRO.  THE VIDHDR CHAIN MUST NOW         */ 00136000
*/*          BE SEARCHED FOR A MATCH.  IF THERE IS NONE, THE         */ 00137000
*/*          BUILD IS KILLED.  OTHERWISE A DIVT IS BUILT FROM        */ 00138000
*/*          THE DIVT HEADER AND DIVT DATA BLOCKS.                   */ 00139000
*/*                                                                  */ 00140000
*/*          AN INTERNAL SWITCH, 'SW', IS USED (FOR STRUCTUR-        */ 00141000
*/*          ING PURPOSES ONLY) TO INDICATE WHETHER A MATCH          */ 00142000
*/*          WAS FOUND.                                              */ 00143000
*/********************************************************************/ 00144000
*                                                                  0208 00145000
*       ELSE                                                       0208 00146000
*WELLFORM:                                                         0208 00147000
*         DO;                       /* WELL STRUCTURED PARAM         */ 00148000
         B     @RC00196                                            0208 00149000
@RF00196 DS    0H                                                  0208 00150000
WELLFORM DS    0H                                                  0209 00151000
*           RFY                                                    0209 00152000
*             DHDVIREG RSTD;        /* VID HEADER            @ABILITY*/ 00153000
*           RFY                                                    0210 00154000
*             ISTDHDVI BASED(DHDVIREG);                            0210 00155000
*           DHDVIREG=RNCDIVT;       /* FIND 1ST VID HEADER           */ 00156000
         L     DHDVIREG,RNCDIVT(,RNCAREG)                          0211 00157000
*           SW='0'B;                /* 'MATCH NOT FOUND' YET         */ 00158000
         NI    SW,B'01111111'                                      0212 00159000
*SEARCH:                                                           0213 00160000
*           DO WHILE(DHDVIREG^=0    /* SEARCH CHAIN FOR MATCH        */ 00161000
*                 &SW='0'B);                                       0213 00162000
SEARCH   B     @DE00213                                            0213 00163000
@DL00213 DS    0H                                                  0214 00164000
*             IF DHDHNAME^=PHRF34SP(1)/* MATCH ON VIDLIST LABEL?     */ 00165000
*               THEN                /* NO-UPDATE TO NEXT VID         */ 00166000
         CLC   DHDHNAME(8,DHDVIREG),PHRF34SP(INDATA)               0214 00167000
         BE    @RF00214                                            0214 00168000
*NEXTHDR:                                                          0215 00169000
*               DHDVIREG=DHDFWPTR;  /* HEADER. IT MAY BE ZERO        */ 00170000
NEXTHDR  L     DHDVIREG,DHDFWPTR(,DHDVIREG)                        0215 00171000
*             ELSE                                                 0216 00172000
*MATCH:                                                            0216 00173000
*               DO;                 /* YES-PROCESS HEADER            */ 00174000
         B     @RC00214                                            0216 00175000
@RF00214 DS    0H                                                  0216 00176000
MATCH    DS    0H                                                  0217 00177000
*                 SW='1'B;          /* INDICATE HEADER FOUND         */ 00178000
*                                                                  0217 00179000
         OI    SW,B'10000000'                                      0217 00180000
*                 /***************************************************/ 00181000
*                 /*                                                 */ 00182000
*                 /* SEGMENT(BLDIVT)REQUEST                          */ 00183000
*                 /*                                                 */ 00184000
*                 /***************************************************/ 00185000
*                                                                  0218 00186000
*                 DO;                                              0218 00187000
*                   GOTO BLDIVT;                                   0219 00188000
         B     BLDIVT                                              0219 00189000
*BLDIVT#:                                                          0220 00190000
*                 END;              /* BUILD A DIVT FOR LABEL        */ 00191000
BLDIVT#  DS    0H                                                  0221 00192000
*               END MATCH;                                         0221 00193000
*           END SEARCH;                                            0222 00194000
@RC00214 DS    0H                                                  0222 00195000
@DE00213 LTR   DHDVIREG,DHDVIREG                                   0222 00196000
         BZ    @DC00213                                            0222 00197000
         TM    SW,B'10000000'                                      0222 00198000
         BZ    @DL00213                                            0222 00199000
@DC00213 DS    0H                                                  0223 00200000
*ENDSRCH:                                                          0223 00201000
*           IF SW='1'B              /* WAS DIVT MATCH FOUND          */ 00202000
*             THEN                                                 0223 00203000
ENDSRCH  TM    SW,B'10000000'                                      0223 00204000
         BO    @RT00223                                            0223 00205000
*             ;                     /* YES-EVERYTHING DONE           */ 00206000
*           ELSE                                                   0225 00207000
*NOTFOUND:                                                         0225 00208000
*             DO;                   /* NO-ERROR CONDITION            */ 00209000
NOTFOUND DS    0H                                                  0226 00210000
*/* ERRFMT    LEVEL(VALUE)  ERROR(MISS)  ACTION(KILLBLD)             */ 00211000
*                                                                  0226 00212000
*               DO;                                                0226 00213000
*                 DCL                                              0227 00214000
*                   1 CMZP0006 STATIC LOCAL,/* PARAMETER LIST PASSED    00215000
*                                      TO ERROR FORMAT ROUTINE       */ 00216000
*                    2 CMZL0006 FIXED(8) INIT(5),/* LEVEL PARAMETER     00217000
*                                      (CLEV)                        */ 00218000
*                    2 CMZE0006 FIXED(8) INIT(8),/* ERROR PARAMETER     00219000
*                                      (CERR)                        */ 00220000
*                    2 CMZA0006 CHAR(1) INIT('6'),/* ACTION PARAMETER   00221000
*                                      (CACT)                        */ 00222000
*                    2 CMZS0006 FIXED(8) INIT(0);/* LENGTH OF STRING    00223000
*                                      PARAM                         */ 00224000
*                 DCL                                              0228 00225000
*                   CMZR0006 REGISTER(1);/* ESTABLISH PARAMeter      */ 00226000
*                 RESPECIFY                                        0229 00227000
*                   CMZR0006 RSTD;  /* REGISTER FOR                  */ 00228000
*                 CMZR0006=ADDR(CMZP0006);/* ADDRESS OF PARAMETER  0230 00229000
*                                      LIST                          */ 00230000
         LA    CMZR0006,CMZP0006                                   0230 00231000
*                 CALL ISTSDCC2;    /* CALL ERROR FORMAT ROUTINE     */ 00232000
         L     @15,@CV01230                                        0231 00233000
         BALR  @14,@15                                             0231 00234000
*                 RESPECIFY                                        0232 00235000
*                   CMZR0006 UNRSTD;                               0232 00236000
*               END;                                               0233 00237000
*               RC=4;               /* RETURN CODE-KILL BUILD        */ 00238000
         MVC   RC(2),@CH00051                                      0234 00239000
*             END NOTFOUND;                                        0235 00240000
*         END WELLFORM;                                            0236 00241000
@RT00223 DS    0H                                                  0237 00242000
*     END FRSTPARM;                                                0237 00243000
*                                                                  0237 00244000
@RC00196 DS    0H                                                  0238 00245000
*/********************************************************************/ 00246000
*/*          RETURN TO CALLING MACRO HANDLER                         */ 00247000
*/*                                                                  */ 00248000
*/*          RC=0 IS GOOD RETURN.  RC=4 KILLS THE BUILD              */ 00249000
*/********************************************************************/ 00250000
*                                                                  0238 00251000
*RETRN:                                                            0238 00252000
*   SAVREG=RNCSAV2(2);              /* RESTORE CALLER'S REGS         */ 00253000
@RC00182 DS    0H                                                  0238 00254000
RETRN    L     SAVREG,RNCSAV2+4(,RNCAREG)                          0238 00255000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00256000
         LH    @15,RC                                              0239 00257000
         L     @14,12(,@13)                                        0239 00258000
         LM    @00,@12,20(@13)                                     0239 00259000
         BR    @14                                                 0239 00260000
*                                                                  0240 00261000
*   /*****************************************************************/ 00262000
*   /*                                                               */ 00263000
*   /* SEGMENT(BLDIVT)START                                          */ 00264000
*   /*                                                               */ 00265000
*   /*****************************************************************/ 00266000
*                                                                  0240 00267000
*   DO;                                                            0240 00268000
*     GOTO BLDIVT@;                                                0241 00269000
*BLDIVT:                                                           0242 00270000
*   END;                                                           0242 00271000
*                                                                  0242 00272000
BLDIVT   DS    0H                                                  0243 00273000
*/********************************************************************/ 00274000
*/*          THIS SEGMENT ATTEMPTS TO OBTAIN STORAGE FOR AND         */ 00275000
*/*          BUILDS A DIVT FROM THE DIVT HEADER AND THE DIVT         */ 00276000
*/*          CONTROL BLOCKS.  THEN A POINTER TO THE DIVT IS          */ 00277000
*/*          PLACED IN THE SIFT AND DEFAULT TABLE.                   */ 00278000
*/*                                                                  */ 00279000
*/*          A DIVT MAY ALREADY HAVE BEEN BUILT IF A LINE OR         */ 00280000
*/*          GROUP MACRO ENCOUNTERED PREVIOUSLY HAD SPECIFIED        */ 00281000
*/*          THE SAME VIDLIST MACRO VIA A VIDSEQ PARAMETER.          */ 00282000
*/*          IF SO, A POINTER TO THE REAL DIVT WILL BE IN THE        */ 00283000
*/*          VID HEADER.  IT IS STILL NECESSARY TO PLACE A           */ 00284000
*/*          POINTER TO THE DIVT IN THE SIFT AND DEFAULT             */ 00285000
*/*          TABLE.                                                  */ 00286000
*/********************************************************************/ 00287000
*                                                                  0243 00288000
*   IF DHDDHPTR^=0                  /* IS DIVT ALREADY BUILT?        */ 00289000
*     THEN                          /* YES-INDICATE VALID COND       */ 00290000
         SLR   @14,@14                                             0243 00291000
         C     @14,DHDDHPTR(,DHDVIREG)                             0243 00292000
         BE    @RF00243                                            0243 00293000
*     RC=0;                                                        0244 00294000
         STH   @14,RC                                              0244 00295000
*   ELSE                            /* NO-MUST BUILD A DIVT          */ 00296000
*TOBUILD:                                                          0245 00297000
*     DO;                                                          0245 00298000
*                                                                  0245 00299000
         B     @RC00243                                            0245 00300000
@RF00243 DS    0H                                                  0245 00301000
TOBUILD  DS    0H                                                  0246 00302000
*/********************************************************************/ 00303000
*/*          STORAGE FOR THE DIVT IS OBTAINED VIA AREA               */ 00304000
*/*          HANDLER (ISTSDCC1).  AT ENTRY TO ISTSDCC1 FOR A         */ 00305000
*/*          GETSTORE, AREA HANDLER REQUIRES THE DESIRED             */ 00306000
*/*          LENGTH IN REG0, AND REG1 SHOULD CONTAIN ZEROES.         */ 00307000
*/*          UPON RETURN FROM ISTSDCC1 TO GET STORAGE, REG15         */ 00308000
*/*          IS ZERO IF STORAGE WAS OBTAINED, NONZERO IF NOT.        */ 00309000
*/*          IF STORAGE WAS OBTAINED, REG1 HAS THE ACTUAL            */ 00310000
*/*          (VIRTUAL) ADDRESS AND REG0 HAS THE RELATIVE             */ 00311000
*/*          OFFSET FROM THE BEGINNING OF THE TABLE/STRING           */ 00312000
*/*          BUILD AREA.                                             */ 00313000
*/*                                                                  */ 00314000
*/*          THE SIZE OF A DIVT IS THE LENGTH OF THE HEADER          */ 00315000
*/*          PLUS AN AMOUNT FOR EACH ENTRY EQUAL TO 5 BYTES          */ 00316000
*/*          PLUS THE LENGTH OF THE BSCID.                           */ 00317000
*/********************************************************************/ 00318000
*                                                                  0246 00319000
*       GPR0=DHDNUME*(5+DHDIDLEN)   /* SIZE OF AREA REQUIRED         */ 00320000
*           +LENGTH(ISTDIVTH);      /* IN R0 FOR AREA HANDLER        */ 00321000
         LA    GPR0,5                                              0246 00322000
         SLR   @14,@14                                             0246 00323000
         ICM   @14,3,DHDIDLEN(DHDVIREG)                            0246 00324000
         ALR   GPR0,@14                                            0246 00325000
         LR    @09,GPR0                                            0246 00326000
         SLR   GPR0,GPR0                                           0246 00327000
         ICM   GPR0,3,DHDNUME(DHDVIREG)                            0246 00328000
         MR    @08,GPR0                                            0246 00329000
         AH    @09,@CH00035                                        0246 00330000
         LR    GPR0,@09                                            0246 00331000
*       GPR1=0                      /* ZERO OUT R1 FOR GET           */ 00332000
*           ;                       /* STORAGE REQUEST               */ 00333000
         SLR   GPR1,GPR1                                           0247 00334000
*       CALL ISTSDCC1;              /* OBTAIN STORAGE FOR DIVT       */ 00335000
         L     @15,@CV01229                                        0248 00336000
         BALR  @14,@15                                             0248 00337000
*       IF GPR15^=0                 /* WAS STORAGE OBTAINED?         */ 00338000
*         THEN                      /* NO-ERROR CONDITION            */ 00339000
         LTR   GPR15,GPR15                                         0249 00340000
         BZ    @RF00249                                            0249 00341000
*         RC=4;                     /* RETURN CODE-KILL BUILD        */ 00342000
         MVC   RC(2),@CH00051                                      0250 00343000
*       ELSE                        /* STORAGE WAS OBTAINED-         */ 00344000
*HAVSPACE:                                                         0251 00345000
*         DO;                       /* CAN BUILD DIVT NOW            */ 00346000
         B     @RC00249                                            0251 00347000
@RF00249 DS    0H                                                  0251 00348000
HAVSPACE DS    0H                                                  0252 00349000
*           RFY                                                    0252 00350000
*             ISTDIVTH BASED(GPR1); /*                       @ABILITY   00351000
*                                      TO DIVT                       */ 00352000
*           DHDDHPTR=GPR1           /* PLACE POINTER TO REAL         */ 00353000
*               ;                   /* DIVT IN VID HEADER            */ 00354000
         ST    GPR1,DHDDHPTR(,DHDVIREG)                            0253 00355000
*           DHDDVOFF=GPR0           /* OFFSET TO DIVT IN VID HEADER.    00356000
*                                      SAVE IT FOR                   */ 00357000
*               ;                   /* LATER USE.                    */ 00358000
         ST    GPR0,DHDDVOFF(,DHDVIREG)                            0254 00359000
*/********************************************************************/ 00360000
*/*          THE DIVT CONSISTS OF A HEADER AND ENTRIES IN ONE        */ 00361000
*/*          CONTIGUOUS STORAGE AREA.  THE INFORMATION FOR           */ 00362000
*/*          THE DIVT HEADER COMES FROM THE VID HEADER, WHICH        */ 00363000
*/*          WAS CREATED BY THE VIDLIST MACRO HANDLER.               */ 00364000
*/********************************************************************/ 00365000
*                                                                  0255 00366000
*           ISTDIVTH=DHDDIVTH       /* MOVE DIVT HEADER INFOR.       */ 00367000
*               ;                   /* TO DIVTH FROM VID HDR.        */ 00368000
         MVC   ISTDIVTH(8,GPR1),DHDDIVTH(DHDVIREG)                 0255 00369000
*           RFY                                                    0256 00370000
*             DVEBASE RSTD;         /* DIVT ENTRY            @ABILITY*/ 00371000
*           RFY                                                    0257 00372000
*             ISTDVE BASED(DVEBASE);                               0257 00373000
*           RFY                                                    0258 00374000
*             DDTBASE RSTD;         /* VID DATA BLOCK        @ABILITY*/ 00375000
*           RFY                                                    0259 00376000
*             ISTDDTVI BASED(DDTBASE);                             0259 00377000
*           DVEBASE=GPR1            /* LOCATE FIRST DIVT ENTRY       */ 00378000
*               +LENGTH(ISTDIVTH);                                 0260 00379000
         LA    DVEBASE,8                                           0260 00380000
         ALR   DVEBASE,GPR1                                        0260 00381000
*           DDTBASE=DHDDTPTR;       /* FIND 1ST VID DATA BLOCK       */ 00382000
         L     DDTBASE,DHDDTPTR(,DHDVIREG)                         0261 00383000
*/********************************************************************/ 00384000
*/*          EACH ENTRY IN THE DIVT IS COMPRISED OF INFOR-           */ 00385000
*/*          MATION WHICH IS CONTAINED IN A CORRESPONDING            */ 00386000
*/*          VID DATA BLOCK, WHICH WAS CREATED BY THE VIDLIST        */ 00387000
*/*          MACRO HANDLER.  THE NUMBER OF VID DATA BLOCKS TO        */ 00388000
*/*          BE CONSOLIDATED IS LOCATED IN THE VID HEADER.           */ 00389000
*/********************************************************************/ 00390000
*                                                                  0262 00391000
*CONSOLID:                                                         0262 00392000
*           DO COUNT=DHDNUME TO 1   /* ONE PASS THRU LOOP FOR        */ 00393000
*                 BY-1;             /* EACH VID DATA BLOCK           */ 00394000
CONSOLID SLR   @14,@14                                             0262 00395000
         ICM   @14,3,DHDNUME(DHDVIREG)                             0262 00396000
         STH   @14,COUNT                                           0262 00397000
         B     @DE00262                                            0262 00398000
@DL00262 DS    0H                                                  0263 00399000
*             DVEBSCAR(1:DHDIDLEN)= /* MOVE BSCID TO DIVT. THE       */ 00400000
*                 DDTBSCID(1:DHDIDLEN)/* LENGTH OF MAX BSCID IN DATA    00401000
*                                      BLOCKS FOR THIS DIVT IS     0263 00402000
*                                      LOCATED IN THE VID HEADER.  0263 00403000
*                                      NOTE- MAX VALUE FOR DHDIDLEN     00404000
*                                      WHICH THE CODE CAN HANDLE IS     00405000
*                                      255 NOTE-VIDLIST MACRO        */ 00406000
*                 ;                 /* HANDLER PADDED BLANKS         */ 00407000
         SLR   @10,@10                                             0263 00408000
         ICM   @10,3,DHDIDLEN(DHDVIREG)                            0263 00409000
         LR    @09,@10                                             0263 00410000
         BCTR  @09,0                                               0263 00411000
         EX    @09,@SM01308                                        0263 00412000
*             DVEBSCLN=DDTBSCLN     /* ACTUAL LENGH OF THE BSCID FOR    00413000
*                                      THIS ONE                      */ 00414000
*                 ;                 /* DIVT ENTRY (<= MAX)           */ 00415000
         MVC   DVEBSCLN(1,DVEBASE),DDTBSCLN(DDTBASE)               0264 00416000
*             DDTRDTPT=ADDR(DVERDTEA)/* ADDRESS OF POINTER TO        */ 00417000
*                 ;                 /* RDTE.                         */ 00418000
         ST    DVEBASE,DDTRDTPT(,DDTBASE)                          0265 00419000
*/********************************************************************/ 00420000
*/*                 THIS DIVT ENTRY IS NOW COMPLETE                  */ 00421000
*/********************************************************************/ 00422000
*                                                                  0266 00423000
*             DVEBASE=DVEBASE+DHDIDLEN/* UPDATE POINTER TO NEXT      */ 00424000
*                 +5;               /* DIVT ENTRY                    */ 00425000
         ALR   @10,DVEBASE                                         0266 00426000
         AH    @10,@CH00228                                        0266 00427000
         LR    DVEBASE,@10                                         0266 00428000
*             DDTBASE=DDTFWPTR      /* UPDATE POINTER TO NEXT        */ 00429000
*                 ;                 /* VID DATA BLOCK                */ 00430000
         L     DDTBASE,DDTFWPTR(,DDTBASE)                          0267 00431000
*           END CONSOLID;           /* HANDLE NEXT DIVT ENTRY        */ 00432000
         BCTR  @14,0                                               0268 00433000
         STH   @14,COUNT                                           0268 00434000
@DE00262 LTR   @14,@14                                             0268 00435000
         BP    @DL00262                                            0268 00436000
*/********************************************************************/ 00437000
*/*          THE DIVT IS NOW COMPLETE AND MAY BE REFERENCED          */ 00438000
*/*          IN THE SIFT AND DEFAULT TABLE.                          */ 00439000
*/********************************************************************/ 00440000
*                                                                  0269 00441000
*           RC=0;                   /* INDICATE VALID COND           */ 00442000
         SLR   @14,@14                                             0269 00443000
         STH   @14,RC                                              0269 00444000
*         END HAVSPACE;                                            0270 00445000
*     END TOBUILD;                                                 0271 00446000
@RC00249 DS    0H                                                  0272 00447000
*BUILT:                                                            0272 00448000
*   IF RC^=0                        /* VALID DIVT EXIST?             */ 00449000
*     THEN                                                         0272 00450000
@RC00243 DS    0H                                                  0272 00451000
BUILT    ICM   @14,3,RC                                            0272 00452000
         BNZ   @RT00272                                            0272 00453000
*     ;                             /* NO-RETURN TO MAINLINE         */ 00454000
*   ELSE                            /* YES-HOOK DIVT TO SIFT         */ 00455000
*SIFTIT:                                                           0274 00456000
*     DO;                           /* AND DEFAULT TABLE             */ 00457000
SIFTIT   DS    0H                                                  0275 00458000
*       RFY                                                        0275 00459000
*        (ISTRPRE,                                                 0275 00460000
*         ISTRPT,                                                  0275 00461000
*         ISTRLN) BASED(MLCPTR1);                                  0275 00462000
*                                                                  0275 00463000
*/********************************************************************/ 00464000
*/*          @ABILITY TO SIFT AND DEFAULT TABLE IS CORRECT           */ 00465000
*/*          WHETHER ENTRY IS FOR LINE OR GROUP MACRO.               */ 00466000
*/********************************************************************/ 00467000
*                                                                  0276 00468000
*       RPTDIVT=DHDDVOFF            /* RELATIVE OFFSET TO DIVT     0276 00469000
*                                      NOTE-REG0 NOT VALID IF DIVT 0276 00470000
*                                      ALREADY BUILT AT ENTRY. THUS,    00471000
*                                      USE VALUE IN VID HEADER. SAME    00472000
*                                      THING WITH REG1 ON THE        */ 00473000
*           ;                       /* NEXT INSTRUCTION.             */ 00474000
         L     @14,MLCPTR1(,MLCAREG)                               0276 00475000
         MVC   RPTDIVT(4,@14),DHDDVOFF(DHDVIREG)                   0276 00476000
*/********************************************************************/ 00477000
*/*          SINCE THE COMMUNICATION ID FIELD IN THE RDT.            */ 00478000
*/*          ENTRY PREFIX IS NOT USED PRIOR TO GENEND PROC-          */ 00479000
*/*          CESSING, IT MAY BE USED TO SAVE A POINTER TO THE        */ 00480000
*/*          DIVT.  GENEND WILL USE THE DIVT POINTER PRIOR TO        */ 00481000
*/*          FILLING IN THE COMMUNICATION ID FIELD.                  */ 00482000
*/********************************************************************/ 00483000
*                                                                  0277 00484000
*       RPRCOMID=DHDDHPTR;          /* SAVE DIVT @ IN RPR            */ 00485000
         MVC   RPRCOMID(4,@14),DHDDHPTR(DHDVIREG)                  0277 00486000
*       RLNIDV='1'B                 /* INDICATE THAT THIS PORT     0278 00487000
*                                      REQUIRES ID VERIFI-           */ 00488000
*           ;                       /* CATION                        */ 00489000
         OI    RLNIDV(@14),B'00001000'                             0278 00490000
*     END SIFTIT;                                                  0279 00491000
*                                                                  0279 00492000
*   /*****************************************************************/ 00493000
*   /*                                                               */ 00494000
*   /* SEGMENT(BLDIVT)FINISH                                         */ 00495000
*   /*                                                               */ 00496000
*   /*****************************************************************/ 00497000
*                                                                  0280 00498000
*   DO;                                                            0280 00499000
@RT00272 DS    0H                                                  0281 00500000
*     GOTO BLDIVT#;                                                0281 00501000
         B     BLDIVT#                                             0281 00502000
*BLDIVT@:                                                          0282 00503000
*   END;                            /* RETURN TO MAINLINE            */ 00504000
BLDIVT@  DS    0H                                                  0283 00505000
*/* TPEPILOG                                                         */ 00506000
*                                                                  0283 00507000
*   GEN(PRINT OFF);                                                0283 00508000
         PRINT OFF                                                      00509000
*   GEN(PRINT ON);                                                 0284 00510000
*                                                                  0284 00511000
         PRINT ON                                                       00512000
*/* END OF TPEPILOG                                                  */ 00513000
*                                                                  0285 00514000
*   END ISTSDCP3;                                                  0285 00515000
@EL00001 DS    0H                                                  0285 00516000
@EF00001 DS    0H                                                  0285 00517000
