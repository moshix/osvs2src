*     ISTPHROA BASED(PHR1REG);                                     0172 00030000
*   DEVCHPTR=MLCPTR4;               /* SET UP ISTDEVCH PTR           */ 00031000
         L     DEVCHPTR,MLCPTR4(,MLCAREG)                          0173 00032000
*   DEVTCODE=PHRF2VAL(1);           /* PICK UP DEVICE CODE           */ 00033000
         MVC   DEVTCODE(1,DEVCHPTR),PHRF2VAL+3(PHR1REG)            0174 00034000
*   IF DEVTCODE='F2'X THEN          /* IS DEVICE A 2740-2            */ 00035000
         CLI   DEVTCODE(DEVCHPTR),X'F2'                            0175 00036000
         BNE   @RF00175                                            0175 00037000
*     DO;                                                          0176 00038000
*       DEVTCODE=DEV2740;                                          0177 00039000
         MVI   DEVTCODE(DEVCHPTR),X'01'                            0177 00040000
*       DEVMCODE=DEVMOD2;                                          0178 00041000
         MVI   DEVMCODE(DEVCHPTR),X'01'                            0178 00042000
*     END;                                                         0179 00043000
*   ELSE                                                           0180 00044000
*     DO;                           /* SET RECORD MODE FOR 3270 TERMS*/ 00045000
         B     @RC00175                                            0180 00046000
@RF00175 DS    0H                                                  0181 00047000
*       IF DEVTCODE=DEV3275|DEVTCODE=DEV3277|DEVTCODE=DEV3284|DEVTCODE= 00048000
*           DEV3286 THEN                                           0181 00049000
         CLI   DEVTCODE(DEVCHPTR),X'1C'                            0181 00050000
         BE    @RT00181                                            0181 00051000
         CLI   DEVTCODE(DEVCHPTR),X'19'                            0181 00052000
         BE    @RT00181                                            0181 00053000
         CLI   DEVTCODE(DEVCHPTR),X'1A'                            0181 00054000
         BE    @RT00181                                            0181 00055000
