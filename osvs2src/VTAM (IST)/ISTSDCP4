*       ELSE                                                       0183 00062000
*         ;                                                        0183 00063000
@RF00181 DS    0H                                                  0184 00064000
*     END;                                                         0184 00065000
*   RC=0;                           /* SET SUCCESSFUL RETURN CODE    */ 00066000
@RC00175 SLR   RC,RC                                               0185 00067000
*   GPR13=RNCSAV2(2);                                              0186 00068000
         L     GPR13,RNCSAV2+4(,RNCAREG)                           0186 00069000
