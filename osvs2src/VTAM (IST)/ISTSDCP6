         TITLE 'ISTSDCP6-PACING AND VPACING PARAMETER HANDLER          *00001000
                        '                                               00002000
ISTSDCP6 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ISTSDCP6  75.130'                                 0001 00009000
ISTSDCP7 DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00170-ISTSDCP7)                                   00013000
         ENTRY ISTSDCP7                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @12,0                                               0001 00017000
@PSTART  DS    0H                                                  0001 00018000
         USING @PSTART,@12                                         0001 00019000
         AH    @15,4(,@15)                                         0001 00020000
         BR    @15                                                 0001 00021000
@EP00001 DS    0H                                                  0002 00022000
*                                                                  0166 00023000
*/********************************************************************/ 00024000
*/*     ISTSDCP6 - VPACING PARM ENTRY                                */ 00025000
*/********************************************************************/ 00026000
*                                                                  0167 00027000
*   LPACE=1;                                                       0167 00028000
         LA    LPACE,1                                             0167 00029000
*   CALL PACECOM;                                                  0168 00030000
         BAL   @14,PACECOM                                         0168 00031000
*   RETURN CODE(0);                                                0169 00032000
*                                                                  0169 00033000
         SLR   @15,@15                                             0169 00034000
         L     @14,12(,@13)                                        0169 00035000
         LM    @00,@12,20(@13)                                     0169 00036000
         BR    @14                                                 0169 00037000
*/********************************************************************/ 00038000
*/*     ISTSDCP7 - PACING PARM ENTRY                                 */ 00039000
*/********************************************************************/ 00040000
*                                                                  0170 00041000
*ISTSDCP7:                                                         0170 00042000
*   ENTRY;                                                         0170 00043000
@EP00170 DS    0H                                                  0171 00044000
*   LPACE=0;                                                       0171 00045000
         SLR   LPACE,LPACE                                         0171 00046000
*   CALL PACECOM;                                                  0172 00047000
         BAL   @14,PACECOM                                         0172 00048000
*   RETURN CODE(0);                                                0173 00049000
         SLR   @15,@15                                             0173 00050000
         L     @14,12(,@13)                                        0173 00051000
         LM    @00,@12,20(@13)                                     0173 00052000
         BR    @14                                                 0173 00053000
*                                                                  0174 00054000
*/********************************************************************/ 00055000
*/* COMMON PARM HANDLING PORTION FOR PACING AND VPACING. ORDER OF    */ 00056000
*/* CODE IS (N,M). IF NEITHER IS CODED USE DEFAULTS OF PACING=(0,0)  */ 00057000
*/* AND VPACING=(2,1). IF N IS CODED ALONE USE IT FOR BOTH. IF BOTH  */ 00058000
*/* ARE CODED AND N IS ZERO USE DEFAULTS AS PRESCRIBED ABOVE. IF N   */ 00059000
*/* IS NOT ZERO THEN M MUST BE LESS OR EQUAL TO N. IF SO USE M AS    */ 00060000
*/* CODED. IF NOT THEN DEFAULT M TO N FOR PACING AND VPACING=(2,1).  */ 00061000
*/********************************************************************/ 00062000
*                                                                  0174 00063000
*PACECOM:                                                          0174 00064000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0174 00065000
*                                                                  0174 00066000
PACECOM  DS    0H                                                  0175 00067000
*/********************************************************************/ 00068000
*/*                        ENTRY LINKAGE                             */ 00069000
*/********************************************************************/ 00070000
*                                                                  0175 00071000
*   SAVCLLD=ADDR(RNCSAV2);          /* SAVE SDCP6'S ADDR AT CALLER'S */ 00072000
         LA    @15,RNCSAV2(,RNCAPTR)                               0175 00073000
         ST    @15,SAVCLLD(,SAVREG)                                0175 00074000
*   RNCSAV2(2)=SAVREG;              /* SAVE CALLER'S SAVE ADDR AT  0176 00075000
*                                      SDCP6'S                       */ 00076000
         ST    SAVREG,RNCSAV2+4(,RNCAPTR)                          0176 00077000
*   SAVREG=ADDR(RNCSAV2);           /* SET UP ISTSDCP6'S SAVE ADDR   */ 00078000
         LA    SAVREG,RNCSAV2(,RNCAPTR)                            0177 00079000
*   GENERATE REFS(RNCDAT2,RNCSAV2,SAVREG) NOSETS;                  0178 00080000
     USING  @DATD-(RNCDAT2-RNCSAV2),SAVREG  ADDRESSABILITY FOR DATA     00081000
*   RETADR=GPR14;                                                  0179 00082000
         LR    RETADR,GPR14                                        0179 00083000
*   IF LPACE=0                      /* PACING?                       */ 00084000
*     THEN                                                         0180 00085000
         LTR   LPACE,LPACE                                         0180 00086000
         BNZ   @RF00180                                            0180 00087000
*     DO;                                                          0181 00088000
*       RLUNCPPN=0;                 /* DEFAULT TO (0,0) FOR PACING   */ 00089000
         L     @15,MLCPTR6(,MLCAPTR)                               0182 00090000
         MVI   RLUNCPPN(@15),X'00'                                 0182 00091000
*       RLUNCPPM=0;                                                0183 00092000
         MVI   RLUNCPPM(@15),X'00'                                 0183 00093000
*     END;                                                         0184 00094000
*   ELSE                                                           0185 00095000
*     DO;                                                          0185 00096000
         B     @RC00180                                            0185 00097000
@RF00180 DS    0H                                                  0186 00098000
*       IF RNCGLSBD='1'B            /* LOCAL SUBAREA VPACING DFLTS 0186 00099000
*                                      (1,1)                 @D32CKQH*/ 00100000
*         THEN                                                     0186 00101000
         TM    RNCGLSBD(RNCAPTR),B'00000001'                       0186 00102000
         BNO   @RF00186                                            0186 00103000
*         RLUPACEN=1;               /*                       @D32CKQH*/ 00104000
         L     @15,MLCPTR6(,MLCAPTR)                               0187 00105000
         MVI   RLUPACEN(@15),X'01'                                 0187 00106000
*       ELSE                                                       0188 00107000
*         RLUPACEN=2;               /* OTHER VPACING DFLTS (2,1)   0188 00108000
*                                                            @D32CKQH*/ 00109000
         B     @RC00186                                            0188 00110000
@RF00186 L     @15,MLCPTR6(,MLCAPTR)                               0188 00111000
         MVI   RLUPACEN(@15),X'02'                                 0188 00112000
*       RLUPACEM=1;                                                0189 00113000
@RC00186 L     @15,MLCPTR6(,MLCAPTR)                               0189 00114000
         MVI   RLUPACEM(@15),X'01'                                 0189 00115000
*     END;                                                         0190 00116000
*   IF PHRF8NSP<=1                  /* M VALUE NOT EXIST?            */ 00117000
*     THEN                                                         0191 00118000
@RC00180 L     @15,PHRF8NSP(,PHROAPTR)                             0191 00119000
         CH    @15,@CH00048                                        0191 00120000
         BH    @RF00191                                            0191 00121000
*     DO;                                                          0192 00122000
*       IF PHRF8DFT(1)='1'B         /* N DEFAULTED?                  */ 00123000
*         THEN                                                     0193 00124000
         TM    PHRF8DFT(PHROAPTR),B'10000000'                      0193 00125000
         BO    @RT00193                                            0193 00126000
*         ;                                                        0194 00127000
*       ELSE                                                       0195 00128000
