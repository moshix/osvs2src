*         REG1011=ADDR(MLCTPMSG);   /* SET RREG SYMBOL TO ADDRESS OF    00301000
*                                      PPARM OPERAND                 */ 00302000
         LA    REG1011,MLCTPMSG(,MLCAREG)                          0457 00303000
*       END;                        /* XREGSET                       */ 00304000
*       CMZID011=0;                                                0459 00305000
         SLR   @10,@10                                             0459 00306000
         ST    @10,CMZID011(,REG1011)                              0459 00307000
*       CMZRL011=0;                                                0460 00308000
         MVI   CMZRL011(REG1011),X'00'                             0460 00309000
