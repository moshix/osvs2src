         ST    @10,CMZSA011+16(,EMCR13)                            0467 00326000
*       DCL                                                        0468 00327000
*         REG15011 REG(15) PTR;                                    0468 00328000
*       RFY                                                        0469 00329000
*         REG15011 RSTD;                                           0469 00330000
*       DO;                         /* LISATCVT                    0470 00331000
*                                      CVTBASE(PAGEABLE,(REG15011))  */ 00332000
*         REG15011=ATCLCPTR;        /* SET UP ATCVT POINTER          */ 00333000
         L     REG15011,ATCLCPTR                                   0471 00334000
*       END;                        /* LISATCVT                      */ 00335000
*       DCL                                                        0473 00336000
*         CMTA011 BASED(REG15011) ENTRY VALUERANGE(*);             0473 00337000
*       REG15011=REG15011->ATCMSGP;                                0474 00338000
         L     REG15011,ATCMSGP(,REG15011)                         0474 00339000
*       CALL CMTA011;                                              0475 00340000
         BALR  @14,@15                                             0475 00341000
*       RESPECIFY                                                  0476 00342000
*        (REG1011,                                                 0476 00343000
*         REG15011) UNRESTRICTED;                                  0476 00344000
*       RFY                                                        0477 00345000
*        (EMCR13) UNRSTD;                                          0477 00346000
*     END;                          /*                       @D32CKQH*/ 00347000
*   GPR13=RNCSAV1(2);               /* RESTORE REG 13 BEFORE       0479 00348000
*                                      RETURNING             @D32CKQH*/ 00349000
@RT00449 L     GPR13,RNCSAV1+4(,RNCAREG)                           0479 00350000
*   RETURN CODE(RC);                                               0480 00351000
         LR    @15,RC                                              0480 00352000
         L     @14,12(,@13)                                        0480 00353000
         LM    @00,@12,20(@13)                                     0480 00354000
         BR    @14                                                 0480 00355000
*                                                                  0481 00356000
*/********************************************************************/ 00357000
*/* DOWHILE PROCEDURE TO CALL ISTINCPH TO INTERPRET PARAMETERS AND   */ 00358000
*/* THEN TO CALL THE INDIVIDUAL PARAMETER PROCESSING ROUTINES WITHIN */ 00359000
*/* THIS MODULE.                                                     */ 00360000
*/********************************************************************/ 00361000
*                                                                  0481 00362000
*DOWHILE:                                                          0481 00363000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0481 00364000
*                                                                  0481 00365000
DOWHILE  DS    0H                                                  0482 00366000
*/********************************************************************/ 00367000
*/* 1ST LEVEL INNER PROC CALLED BY THE OUTER PROC'S DO WHILE ROUTINE */ 00368000
*/********************************************************************/ 00369000
*                                                                  0482 00370000
*   GPR0=ADDR(PATKWTBL);                                           0482 00371000
         LA    GPR0,PATKWTBL                                       0482 00372000
*   PHRREG=ADDR(RNCPHR1);                                          0483 00373000
         LA    PHRREG,RNCPHR1(,RNCAREG)                            0483 00374000
*   CALL ISTINCPH;                  /* CALL PHR                      */ 00375000
         L     @15,@CV02193                                        0484 00376000
         BALR  @14,@15                                             0484 00377000
*   RC=GPR15;                                                      0485 00378000
         LR    RC,GPR15                                            0485 00379000
*   KWDPTR=GPR1;                                                   0486 00380000
         ST    GPR1,KWDPTR                                         0486 00381000
*   IF RC=0 THEN                                                   0487 00382000
         LTR   RC,RC                                               0487 00383000
         BNZ   @RF00487                                            0487 00384000
*     DO;                                                          0488 00385000
*                                                                  0488 00386000
*/********************************************************************/ 00387000
*/* IF THE RETURN CODE FROM PHR = 0, CALL THE PARAMETER HANDLER      */ 00388000
*/* WHOSE ADDRESS WAS RETURNED IN GPR0 BY PHR.                       */ 00389000
*/* NOTE: THE EXTERNAL PARAMETER HANDLERS WILL RETURN TO THIS        */ 00390000
*/*       ROUTINE VIA A RETURN.  ALL INTERNAL PARAMETER HANDLERS     */ 00391000
*/*       WILL RETURN TO THIS ROUTINE VIA A GOTO 'MAINSUB'.          */ 00392000
*/* NOTE: ALL PARAMETER HANDLERS MUST HAVE A 0 OR 4 RETURN CODE IN   */ 00393000
*/*       GPR15 TO INDICATE CONTINUE PROCESSING OR KILL THE BUILD.   */ 00394000
*/********************************************************************/ 00395000
*                                                                  0489 00396000
*       GPR15=GPR0;                                                0489 00397000
         LR    GPR15,GPR0                                          0489 00398000
*       CALL PARMHDLR;                                             0490 00399000
         BALR  @14,@15                                             0490 00400000
*MAINSUB:                                                          0491 00401000
*       ;                                                          0491 00402000
MAINSUB  DS    0H                                                  0492 00403000
*     END;                                                         0492 00404000
*   ELSE                                                           0493 00405000
*     DO;                                                          0493 00406000
*                                                                  0493 00407000
         B     @RC00487                                            0493 00408000
@RF00487 DS    0H                                                  0494 00409000
*/********************************************************************/ 00410000
*/* IF THE RETURN CODE FROM PHR = 4 INDICATING THE KEYWORD WAS NOT   */ 00411000
*/* FOUND IN THE PARAMETER TABLE, WRITE THE APPROPRIATE ERROR        */ 00412000
*/* MESSAGE, SET THE RETURN CODE = 0 AND CONTINUE THE DO WHILE LOOP. */ 00413000
*/********************************************************************/ 00414000
*                                                                  0494 00415000
*       IF RC=4 THEN                                               0494 00416000
         CH    RC,@CH00051                                         0494 00417000
         BNE   @RF00494                                            0494 00418000
*         DO;                                                      0495 00419000
*                                                                  0495 00420000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 00421000
*                                                                  0496 00422000
*           DO;                                                    0496 00423000
*             DCL                                                  0497 00424000
*               1 CMZP0020 STATIC LOCAL,/* PARAMETER LIST PASSED TO     00425000
*                                      ERROR FORMAT ROUTINE          */ 00426000
*                2 CMZL0020 FIXED(8) INIT(4),/* LEVEL PARAMETER    0497 00427000
*                                      (CLEV)                        */ 00428000
