         TITLE 'ISTSDCPA - SYSTEM DEFINITION PATH MACRO PROCESSOR      *00001000
                        '                                               00002000
ISTSDCPA CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCPA  75.148'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*   RNCSAV1(2)=GPR13;               /* BACKWARD POINT                */ 00014000
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0376 00015000
*   GPR13=ADDR(RNCSAV1);            /* SET UP OUR SAVE REG           */ 00016000
         LA    GPR13,RNCSAV1(,RNCAREG)                             0377 00017000
*   MLCSAV3(3)=GPR13;               /* FORWARD POINT                 */ 00018000
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0378 00019000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */ 00020000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0379 00021000
*   GENERATE NOREFS NOSETS;                                        0380 00022000
*                                                                  0380 00023000
           USING @DATD,11                 ADDRESSABILITY FOR DATA       00024000
*/********************************************************************/ 00025000
*/* DO INITIAL PROCESSING TO INSURE THAT A PATH CAN BE ADDED.        */ 00026000
*/*                                                                  */ 00027000
*/* THE CONDITIONS THAT MUST EXIST IN ORDER TO PROCESS THIS PATH ARE */ 00028000
*/*   1. THERE MUST HAVE BEEN A PU FOR THIS PATH.                    */ 00029000
*/*   2. THERE MUST EXIST A DIAL NO. AND GROUP NAME TABLE.           */ 00030000
*/*   3. THERE MUST EXIT A PATH TABLE FOR THE PRECEDING PU.          */ 00031000
*/*   4. THERE MUST BE ENOUGH ROOM IN THE PATH TABLE FOR AN ENTRY.   */ 00032000
*/********************************************************************/ 00033000
*                                                                  0381 00034000
*   DO;                             /* BLOCK                         */ 00035000
*     TPMSG=0;                      /* INITIALIZE ERROR MESSAGE TO NO*/ 00036000
         SLR   @10,@10                                             0382 00037000
         ST    @10,TPMSG                                           0382 00038000
*     IF RNCGPU='1'B                /* PREVIOUS PU EXIST?            */ 00039000
*       THEN                                                       0383 00040000
         TM    RNCGPU(RNCAREG),B'00000001'                         0383 00041000
         BO    @RT00383                                            0383 00042000
*       ;                                                          0384 00043000
*     ELSE                                                         0385 00044000
*       DO;                                                        0385 00045000
*                                                                  0385 00046000
*/* ERRFMT    LEVEL(MACRO)  ERROR(SEQ)  ACTION(KILLBLD)              */ 00047000
*                                                                  0386 00048000
*         DO;                                                      0386 00049000
*           DCL                                                    0387 00050000
*             1 CMZP0005 STATIC LOCAL,/* PARAMETER LIST PASSED TO  0387 00051000
*                                      ERROR FORMAT ROUTINE          */ 00052000
*              2 CMZL0005 FIXED(8) INIT(3),/* LEVEL PARAMETER (CLEV) */ 00053000
*              2 CMZE0005 FIXED(8) INIT(5),/* ERROR PARAMETER (CERR) */ 00054000
*              2 CMZA0005 CHAR(1) INIT('6'),/* ACTION PARAMETER    0387 00055000
*                                      (CACT)                        */ 00056000
*              2 CMZS0005 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM */ 00057000
*           DCL                                                    0388 00058000
*             CMZR0005 REGISTER(1); /* ESTABLISH PARAMeter           */ 00059000
*           RESPECIFY                                              0389 00060000
*             CMZR0005 RSTD;        /* REGISTER FOR                  */ 00061000
*           CMZR0005=ADDR(CMZP0005);/* ADDRESS OF PARAMETER LIST     */ 00062000
         LA    CMZR0005,CMZP0005                                   0390 00063000
*           CALL ISTSDCC2;          /* CALL ERROR FORMAT ROUTINE     */ 00064000
         L     @15,@CV02194                                        0391 00065000
         BALR  @14,@15                                             0391 00066000
*           RESPECIFY                                              0392 00067000
*             CMZR0005 UNRSTD;                                     0392 00068000
*         END;                                                     0393 00069000
*         RC=4;                                                    0394 00070000
*                                                                  0394 00071000
         LA    RC,4                                                0394 00072000
*/*   LEAVE(BLOCK)                                                   */ 00073000
*                                                                  0395 00074000
*         GOTO #BC00004;                                           0395 00075000
         B     #BC00004                                            0395 00076000
*       END;                                                       0396 00077000
*     IF RSWDIALT^=0&RSWGRUPT^=0    /* DIAL AND GROUP NAME TABS    0397 00078000
*                                      THERE?                        */ 00079000
*       THEN                                                       0397 00080000
@RT00383 SLR   @10,@10                                             0397 00081000
         L     @08,RNCSWLS(,RNCAREG)                               0397 00082000
         C     @10,RSWDIALT(,@08)                                  0397 00083000
         BE    @RF00397                                            0397 00084000
         C     @10,RSWGRUPT(,@08)                                  0397 00085000
         BNE   @RT00397                                            0397 00086000
*       ;                                                          0398 00087000
*     ELSE                                                         0399 00088000
*       DO;                                                        0399 00089000
@RF00397 DS    0H                                                  0400 00090000
*         TPMSG=ADDR(MSG142I);      /* SET UP TPMSG ID FOR ERROR     */ 00091000
         LA    @10,MSG142I                                         0400 00092000
         ST    @10,TPMSG                                           0400 00093000
*         MLCMACRO(1:1)='1';        /* ERROR CODE PUT IN TPMSG       */ 00094000
         MVI   MLCMACRO(MLCAREG),C'1'                              0401 00095000
*         RC=4;                                                    0402 00096000
*                                                                  0402 00097000
         LA    RC,4                                                0402 00098000
*/*   LEAVE(BLOCK)                                                   */ 00099000
*                                                                  0403 00100000
*         GOTO #BC00004;                                           0403 00101000
         B     #BC00004                                            0403 00102000
*       END;                                                       0404 00103000
*     IF RCCPATHT^=0                /* PATH TABLE EXIST?             */ 00104000
*       THEN                                                       0405 00105000
@RT00397 L     @10,RNCCC(,RNCAREG)                                 0405 00106000
         ICM   @08,15,RCCPATHT(@10)                                0405 00107000
         BNZ   @RT00405                                            0405 00108000
*       ;                                                          0406 00109000
*     ELSE                                                         0407 00110000
*       DO;                                                        0407 00111000
*         TPMSG=ADDR(MSG142I);                                     0408 00112000
         LA    @10,MSG142I                                         0408 00113000
         ST    @10,TPMSG                                           0408 00114000
*         MLCMACRO(1:1)='2';                                       0409 00115000
         MVI   MLCMACRO(MLCAREG),C'2'                              0409 00116000
*         RC=4;                                                    0410 00117000
*                                                                  0410 00118000
         LA    RC,4                                                0410 00119000
*/*   LEAVE(BLOCK)                                                   */ 00120000
*                                                                  0411 00121000
*         GOTO #BC00004;                                           0411 00122000
         B     #BC00004                                            0411 00123000
*       END;                                                       0412 00124000
*     IF((RCCPATHZ*LENGTH(ISTPAT))+4)<RNCPTLN/* ROOM FOR NEW PATH?   */ 00125000
*       THEN                                                       0413 00126000
@RT00405 L     @10,RNCCC(,RNCAREG)                                 0413 00127000
         LH    @08,RCCPATHZ(,@10)                                  0413 00128000
         SLA   @08,3                                               0413 00129000
         AH    @08,@CH00051                                        0413 00130000
         L     @10,RCCPATHT(,@10)                                  0413 00131000
         C     @08,RNCPTLN(,@10)                                   0413 00132000
         BL    @RT00413                                            0413 00133000
*       ;                                                          0414 00134000
*     ELSE                                                         0415 00135000
*       DO;                                                        0415 00136000
*         TPMSG=ADDR(MSG142I);                                     0416 00137000
         LA    @10,MSG142I                                         0416 00138000
         ST    @10,TPMSG                                           0416 00139000
*         MLCMACRO(1:1)='3';                                       0417 00140000
         MVI   MLCMACRO(MLCAREG),C'3'                              0417 00141000
*         RC=4;                                                    0418 00142000
*                                                                  0418 00143000
         LA    RC,4                                                0418 00144000
*/*   LEAVE(BLOCK)                                                   */ 00145000
*                                                                  0419 00146000
*         GOTO #BC00004;                                           0419 00147000
         B     #BC00004                                            0419 00148000
*       END;                                                       0420 00149000
*                                                                  0420 00150000
*/********************************************************************/ 00151000
*/* IF EVERYTHING IS OK SO FAR THEN PROCESSING FOR THIS PATH BEGINS. */ 00152000
*/* THE FIRST THING DONE IS TO PREPARE THE ENTRY BY ZEROING IT, THIS */ 00153000
*/* SETS ALL DEFAULTS OFF. THEN THE STANDARD DEFAULTS ARE SET FOR    */ 00154000
*/* USE AND REDIAL COUNT.                                            */ 00155000
*/********************************************************************/ 00156000
*                                                                  0421 00157000
*     PATHENT=((RCCPATHZ*LENGTH(ISTPAT))+4)+RCCPATHT;/* NXT FREE ENT */ 00158000
@RT00413 L     @10,RNCCC(,RNCAREG)                                 0421 00159000
         LH    PATHENT,RCCPATHZ(,@10)                              0421 00160000
         SLA   PATHENT,3                                           0421 00161000
         AH    PATHENT,@CH00051                                    0421 00162000
         AL    PATHENT,RCCPATHT(,@10)                              0421 00163000
*     ISTPAT=ISTPAT&&ISTPAT;        /* CLEAR ENTIRE ENTRY            */ 00164000
         XC    ISTPAT(8,PATHENT),ISTPAT(PATHENT)                   0422 00165000
*     PATUSEFG='1'B;                                               0423 00166000
         OI    PATUSEFG(PATHENT),B'10000000'                       0423 00167000
*     PATRDCNT=3;                                                  0424 00168000
*                                                                  0424 00169000
         MVI   PATRDCNT(PATHENT),X'03'                             0424 00170000
*/********************************************************************/ 00171000
*/* THE FOLLOWING PREPARES FOR AND THEN ENTERS A  DO-WHILE LOOP      */ 00172000
*/* THAT WILL CONTINUE TO CALL A ROUTINE CALLED                      */ 00173000
*/* DOWHILE UNTIL ALL THE PARAMETERS ON THE PATH MACRO HAVE          */ 00174000
*/* BEEN PROCESSED OR THERE IS SOME KIND OF ERROR ENCOUNTERED        */ 00175000
*/********************************************************************/ 00176000
*                                                                  0425 00177000
*     RC=0;                                                        0425 00178000
         SLR   RC,RC                                               0425 00179000
*     GPR1=MLCKWD;                                                 0426 00180000
         L     GPR1,MLCKWD(,MLCAREG)                               0426 00181000
*     KILLBDSW='0'B;                                               0427 00182000
         NI    KILLBDSW,B'01111111'                                0427 00183000
*     DO WHILE(RC=0);                                              0428 00184000
         B     @DE00428                                            0428 00185000
@DL00428 DS    0H                                                  0429 00186000
*       CALL DOWHILE;                                              0429 00187000
         BAL   @14,DOWHILE                                         0429 00188000
*DOLOOP:                            /* THIS LABEL IS A RETURN ADDR   */ 00189000
*     END;                                                         0430 00190000
*                                                                  0430 00191000
DOLOOP   DS    0H                                                  0430 00192000
@DE00428 SLR   @10,@10                                             0430 00193000
         CR    RC,@10                                              0430 00194000
         BE    @DL00428                                            0430 00195000
*/********************************************************************/ 00196000
*/* AT THIS POINT A RETURN CODE OF 8 MEANS END OF PARAMETERS.        */ 00197000
*/* A RETURN CODE OF 4 MEANS ERROR AND TO EITHER KILL THE BUILD      */ 00198000
*/* OR NOT DEPENDING ON HOW KILLBDSW IS SET .                        */ 00199000
*/* FOR A RETURN CODE OF 8 THE ENTRY HAS BEEN SUCCESSFULLY ADDED TO  */ 00200000
*/* THE PATH TABLE SO THE NUMBER OF ENTRIES IN THE PATH TABLE MUST   */ 00201000
*/* BE INCREMENTED BY 1 TO ACCOUNT FOR THE NEW ENTRY. IF THE COUNT   */ 00202000
*/* IS NOT UPDATED THE ENTRY, EVEN THOUGH SOME OF THE FIELDS MAY     */ 00203000
*/* ALREADY HAVE BEEN SET , WILL BE IGNORED.                         */ 00204000
*/********************************************************************/ 00205000
*                                                                  0431 00206000
*     IF RC=8 THEN                                                 0431 00207000
         CH    RC,@CH00035                                         0431 00208000
         BNE   @RF00431                                            0431 00209000
*       DO;                                                        0432 00210000
*         RCCPATHZ=RCCPATHZ+1;                                     0433 00211000
         L     @07,RNCCC(,RNCAREG)                                 0433 00212000
         LA    @04,1                                               0433 00213000
         AH    @04,RCCPATHZ(,@07)                                  0433 00214000
         STH   @04,RCCPATHZ(,@07)                                  0433 00215000
*         RC=0;                                                    0434 00216000
         LR    RC,@10                                              0434 00217000
*       END;                                                       0435 00218000
*                                                                  0435 00219000
*/********************************************************************/ 00220000
*/* IF THE RETURN  CODE IS NOT 8 THEN THE ENTRY WILL NOT BE ADDED.   */ 00221000
*/* IF THE COUNT OF ENTRIES IN THE DIAL NO. TABLE OR GROUP NAMES     */ 00222000
*/* TABLES HAVE BEEN UPDATED THEN THEY MUST BE DECREMENTED SINCE     */ 00223000
*/* THE ENTRY JUST MADE WILL NOT BE NEEDED.                          */ 00224000
*/********************************************************************/ 00225000
*                                                                  0436 00226000
*     ELSE                                                         0436 00227000
*       DO;                                                        0436 00228000
         B     @RC00431                                            0436 00229000
@RF00431 DS    0H                                                  0437 00230000
*         IF DNTINCR='1'B THEN                                     0437 00231000
         TM    DNTINCR,B'01000000'                                 0437 00232000
         BNO   @RF00437                                            0437 00233000
*           RSWDIALZ=RSWDIALZ-1;                                   0438 00234000
         L     @10,RNCSWLS(,RNCAREG)                               0438 00235000
         LH    @07,RSWDIALZ(,@10)                                  0438 00236000
         BCTR  @07,0                                               0438 00237000
         STH   @07,RSWDIALZ(,@10)                                  0438 00238000
*         ELSE                                                     0439 00239000
*           ;                                                      0439 00240000
@RF00437 DS    0H                                                  0440 00241000
*         IF GNTINCR='1'B THEN                                     0440 00242000
         TM    GNTINCR,B'00100000'                                 0440 00243000
         BNO   @RF00440                                            0440 00244000
*           RSWGRUPZ=RSWGRUPZ-1;                                   0441 00245000
         L     @10,RNCSWLS(,RNCAREG)                               0441 00246000
         LH    @07,RSWGRUPZ(,@10)                                  0441 00247000
         BCTR  @07,0                                               0441 00248000
         STH   @07,RSWGRUPZ(,@10)                                  0441 00249000
*         ELSE                                                     0442 00250000
*           ;                                                      0442 00251000
@RF00440 DS    0H                                                  0443 00252000
*         IF KILLBDSW='1'B THEN                                    0443 00253000
         TM    KILLBDSW,B'10000000'                                0443 00254000
         BO    @RT00443                                            0443 00255000
*           ;                                                      0444 00256000
*         ELSE                                                     0445 00257000
*           RC=0;                   /* IGNORE THIS MACRO             */ 00258000
         SLR   RC,RC                                               0445 00259000
*       END;                                                       0446 00260000
*                                                                  0446 00261000
@RT00443 DS    0H                                                  0447 00262000
*/********************************************************************/ 00263000
*/* THIS IS END OF BLOCK. ALL LEAVE STATEMENTS COME TO THIS POINT.   */ 00264000
*/* THIS IS ALSO THE RETURN POINT TO THE CALLING MODULE.             */ 00265000
*/********************************************************************/ 00266000
*                                                                  0447 00267000
*   END;                            /* END                           */ 00268000
@RC00431 DS    0H                                                  0448 00269000
*#BC00004:                                                         0448 00270000
*   ;                                                              0448 00271000
#BC00004 DS    0H                                                  0449 00272000
*   IF TPMSG=0 THEN                                                0449 00273000
         ICM   @10,15,TPMSG                                        0449 00274000
         BZ    @RT00449                                            0449 00275000
*     ;                                                            0450 00276000
*   ELSE                                                           0451 00277000
*                                                                  0451 00278000
*/*TPMSG MSGID((TPMSG))MSG((ADDR(MLCERBF1)))PLAREA(MLCTPMSG)OPTIONS(NCS 00279000
