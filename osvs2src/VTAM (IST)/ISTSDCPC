         TITLE 'ISTSDCPC-PCCU MACRO HNDLR                              *00001000
                        '                                               00002000
ISTSDCPC CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCPC  77.311'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*   RNCSAV1(2)=GPR13;               /* BACKWARD POINT                */ 00014000
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0178 00015000
*   GPR13=ADDR(RNCSAV1);            /* SET UP OUR SAVE REG           */ 00016000
         LA    GPR13,RNCSAV1(,RNCAREG)                             0179 00017000
*   MLCSAV3(3)=GPR13;               /* FORWARD POINT                 */ 00018000
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0180 00019000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */ 00020000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0181 00021000
*   GENERATE;                                                      0182 00022000
           USING @DATD,11                 ADDRESSABILITY FOR DATA       00023000
*   IF RNCGPCCU='0'B                /* CHECK PCCU MACRO FP           */ 00024000
*     THEN                                                         0183 00025000
         TM    RNCGPCCU(RNCAREG),B'10000000'                       0183 00026000
         BNZ   @RF00183                                            0183 00027000
*     DO;                           /* IF OFF CONTINUE, ELSE EXIT    */ 00028000
*/********************************************************************/ 00029000
*/* WHILE THE RETURN CODE = 0, THIS CODE PREPARES THE PHR PARAMETERS */ 00030000
*/* AND CALLS PHR.  UPON RETURN FROM PHR, THE RETURN CODE AND THE @  */ 00031000
*/* OF THE NEXT MACRO KEYWORD ARE SAVED.                             */ 00032000
*/********************************************************************/ 00033000
*                                                                  0185 00034000
*       RFY                                                        0185 00035000
*        (GPR0,                                                    0185 00036000
*         GPR1,                                                    0185 00037000
*         GPR14,                                                   0185 00038000
*         GPR15) RSTD;              /* RESTRICT REGS                 */ 00039000
*       GPR1=RPRENTRN;                                             0186 00040000
         LA    GPR1,1                                              0186 00041000
*       CALL ISTSDCRS;              /* CALL SIFTDEF                  */ 00042000
         L     @15,@CV01249                                        0187 00043000
         BALR  @14,@15                                             0187 00044000
*       ENTRYPTR=GPR1;                                             0188 00045000
         LR    ENTRYPTR,GPR1                                       0188 00046000
*       RFY                                                        0189 00047000
*        (ISTRDT,                                                  0189 00048000
*         ISTRRN,                                                  0189 00049000
*         ISTRPRE) BASED(ENTRYPTR); /*                       @X50BHMX*/ 00050000
*       RPRNAME=MLCCONFG;           /* NAME RDT SECTION BY         0190 00051000
*                                      CONFIGURATION NAME            */ 00052000
         MVC   RPRNAME(8,ENTRYPTR),MLCCONFG(MLCAREG)               0190 00053000
*       GPR1=MLCKWD;                                               0191 00054000
         L     GPR1,MLCKWD(,MLCAREG)                               0191 00055000
*       RC=0;                                                      0192 00056000
         SLR   @10,@10                                             0192 00057000
         STH   @10,RC                                              0192 00058000
*       DO WHILE(RC=0);                                            0193 00059000
         B     @DE00193                                            0193 00060000
@DL00193 DS    0H                                                  0194 00061000
*         CALL DOWHILE;                                            0194 00062000
         BAL   @14,DOWHILE                                         0194 00063000
*DOLOOP:                                                           0195 00064000
*         ;                                                        0195 00065000
*                                                                  0195 00066000
DOLOOP   DS    0H                                                  0196 00067000
*/********************************************************************/ 00068000
*/* AT THIS POINT A RETURN CODE OF 0 MEANS CONTINUE THE DO WHILE     */ 00069000
*/* LOOP.  OTHERWISE, THE LOOP IS DISCONTINUED WITH A RETURN CODE OF */ 00070000
*/* 8 OR 4.                                                          */ 00071000
*/********************************************************************/ 00072000
*                                                                  0196 00073000
*       END;                                                       0196 00074000
@DE00193 ICM   @10,3,RC                                            0196 00075000
         BZ    @DL00193                                            0196 00076000
*     END;                                                         0197 00077000
*   ELSE                                                           0198 00078000
*     DO;                           /* IF NOT IN SYNC                */ 00079000
         B     @RC00183                                            0198 00080000
@RF00183 DS    0H                                                  0199 00081000
*/* ERRFMT    LEVEL(MACRO)  ERROR(SEQ)  ACTION(KILLBLD)              */ 00082000
*                                                                  0199 00083000
*       DO;                                                        0199 00084000
*         DCL                                                      0200 00085000
*           1 CMZP0003 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   00086000
*                                      FORMAT ROUTINE                */ 00087000
*            2 CMZL0003 FIXED(8) INIT(3),/* LEVEL PARAMETER (CLEV)   */ 00088000
*            2 CMZE0003 FIXED(8) INIT(5),/* ERROR PARAMETER (CERR)   */ 00089000
*            2 CMZA0003 CHAR(1) INIT('6'),/* ACTION PARAMETER (CACT) */ 00090000
*            2 CMZS0003 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 00091000
*         DCL                                                      0201 00092000
*           CMZR0003 REGISTER(1);   /* ESTABLISH PARAMeter           */ 00093000
*         RESPECIFY                                                0202 00094000
*           CMZR0003 RSTD;          /* REGISTER FOR                  */ 00095000
*         CMZR0003=ADDR(CMZP0003);  /* ADDRESS OF PARAMETER LIST     */ 00096000
         LA    CMZR0003,CMZP0003                                   0203 00097000
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */ 00098000
         L     @15,@CV01247                                        0204 00099000
         BALR  @14,@15                                             0204 00100000
*         RESPECIFY                                                0205 00101000
*           CMZR0003 UNRSTD;                                       0205 00102000
*       END;                                                       0206 00103000
*       RC=4;                       /* SET KILL RTN CODE             */ 00104000
         MVC   RC(2),@CH00051                                      0207 00105000
*     END;                                                         0208 00106000
*                                                                  0208 00107000
*/********************************************************************/ 00108000
*/* AT THIS POINT IF RC=8, INDICATING END OF STRING,                 */ 00109000
*/* THE RTN CODE IS SET TO 0 TO TELL RNBUILD THAT NO ERRORS WERE     */ 00110000
*/* FOUND. IF THE RTN CODE IS 4 - WE GO BACK TO THE RNBUILD WITH THIS*/ 00111000
*/* CODE SO THAT THE BUILD WILL BE KILLED.                           */ 00112000
*/********************************************************************/ 00113000
*                                                                  0209 00114000
*   IF RC=8 THEN                                                   0209 00115000
@RC00183 CLC   RC(2),@CH00035                                      0209 00116000
         BNE   @RF00209                                            0209 00117000
*     DO;                           /*                       @X50BHMX*/ 00118000
*       RC=0;                                                      0211 00119000
         SLR   @10,@10                                             0211 00120000
         STH   @10,RC                                              0211 00121000
*       IF PWFLGTH^=0               /* CHECK FOR DATASET PASSWORD  0212 00122000
*                                      WITHOUT IDENTIFER     @XA15406*/ 00123000
*         THEN                                                     0212 00124000
         CLI   PWFLGTH(ENTRYPTR),0                                 0212 00125000
         BE    @RF00212                                            0212 00126000
*         IF RDTCRSDS^=BLANK8       /* IDENTIFER PRESENT?    @X50BHMX*/ 00127000
*           THEN                                                   0213 00128000
         CLC   RDTCRSDS(8,ENTRYPTR),@CC01242                       0213 00129000
         BNE   @RT00213                                            0213 00130000
*           ;                       /* YES - DO NOTHING      @X50BHMX*/ 00131000
*         ELSE                                                     0215 00132000
*           DO;                     /* NO - OUTPUT IGNORE MSG      0215 00133000
*                                                            @X50BHMX*/ 00134000
*/* ERRFMT    LEVEL(PARM)  ERROR(EXTRA)  ACTION(IGNORE)              */ 00135000
*                                                                  0216 00136000
*             DO;                                                  0216 00137000
*               DCL                                                0217 00138000
*                 1 CMZP0004 STATIC LOCAL,/* PARAMETER LIST PASSED TO   00139000
*                                      ERROR FORMAT ROUTINE          */ 00140000
*                  2 CMZL0004 FIXED(8) INIT(4),/* LEVEL PARAMETER  0217 00141000
*                                      (CLEV)                        */ 00142000
*                  2 CMZE0004 FIXED(8) INIT(7),/* ERROR PARAMETER  0217 00143000
*                                      (CERR)                        */ 00144000
*                  2 CMZA0004 CHAR(1) INIT('1'),/* ACTION PARAMETER     00145000
*                                      (CACT)                        */ 00146000
*                  2 CMZS0004 FIXED(8) INIT(0);/* LENGTH OF STRING 0217 00147000
*                                      PARAM                         */ 00148000
*               DCL                                                0218 00149000
*                 CMZR0004 REGISTER(1);/* ESTABLISH PARAMeter        */ 00150000
*               RESPECIFY                                          0219 00151000
*                 CMZR0004 RSTD;    /* REGISTER FOR                  */ 00152000
*               CMZR0004=ADDR(CMZP0004);/* ADDRESS OF PARAMETER LIST */ 00153000
         LA    CMZR0004,CMZP0004                                   0220 00154000
*               CALL ISTSDCC2;      /* CALL ERROR FORMAT ROUTINE     */ 00155000
         L     @15,@CV01247                                        0221 00156000
         BALR  @14,@15                                             0221 00157000
*               RESPECIFY                                          0222 00158000
*                 CMZR0004 UNRSTD;                                 0222 00159000
*             END;                  /*                       @X50BHMX*/ 00160000
*           END;                    /*                       @X50BHMX*/ 00161000
*     END;                          /*                       @X50BHMX*/ 00162000
*   ELSE                                                           0226 00163000
*     ;                                                            0226 00164000
@RF00209 DS    0H                                                  0227 00165000
*   RNCGPCCU='1'B;                  /* SET THIS MACROS FP            */ 00166000
@RC00209 OI    RNCGPCCU(RNCAREG),B'10000000'                       0227 00167000
*   GPR13=RNCSAV1(2);               /* RESTORE CALLERS REGS          */ 00168000
         L     GPR13,RNCSAV1+4(,RNCAREG)                           0228 00169000
*/********************************************************************/ 00170000
*/* AT THIS POINT RC CONTAINS EITHER A 0 OR A 4 INDICATING A GOOD    */ 00171000
*/* RETURN CODE OR AN ERROR RETURN CODE WHICH WILL KILL THE BUILD.   */ 00172000
*/********************************************************************/ 00173000
*                                                                  0229 00174000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00175000
         LH    @15,RC                                              0229 00176000
         L     @14,12(,@13)                                        0229 00177000
         LM    @00,@12,20(@13)                                     0229 00178000
         BR    @14                                                 0229 00179000
*DOWHILE:                                                          0230 00180000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0230 00181000
DOWHILE  DS    0H                                                  0231 00182000
*   GPR0=ADDR(PCUKWTBL);                                           0231 00183000
         LA    GPR0,PCUKWTBL                                       0231 00184000
*   PHR1REG=ADDR(RNCPHR1);                                         0232 00185000
         LA    PHR1REG,RNCPHR1(,RNCAREG)                           0232 00186000
*   RFY                                                            0233 00187000
*     ISTPHROA BASED(PHR1REG);                                     0233 00188000
*   CALL ISTINCPH;                  /* CALL PHR                      */ 00189000
         L     @15,@CV01246                                        0234 00190000
         BALR  @14,@15                                             0234 00191000
*   RC=GPR15;                                                      0235 00192000
         STH   GPR15,RC                                            0235 00193000
*   KWDPTR=GPR1;                                                   0236 00194000
         LR    KWDPTR,GPR1                                         0236 00195000
*   IF RC=0 THEN                                                   0237 00196000
         LH    @10,RC                                              0237 00197000
         LTR   @10,@10                                             0237 00198000
         BNZ   @RF00237                                            0237 00199000
*     DO;                                                          0238 00200000
*                                                                  0238 00201000
*/********************************************************************/ 00202000
*/* IF THE RETURN CODE FROM PHR = 0, CALL THE PARAMETER HANDLER      */ 00203000
*/* WHOSE ADDRESS WAS RETURNED IN GPR0 BY PHR.                       */ 00204000
*/* NOTE: THE EXTERNAL PARAMETER HANDLERS WILL RETURN TO THIS        */ 00205000
*/*       ROUTINE VIA A RETURN.  ALL INTERNAL PARAMETER HANDLERS     */ 00206000
*/*       WILL RETURN TO THIS ROUTINE VIA A GOTO 'MAINSUB'.          */ 00207000
*/* NOTE: ALL PARAMETER HANDLERS MUST HAVE A 0 OR 4 RETURN CODE IN   */ 00208000
*/*       GPR15 TO INDICATE CONTINUE PROCESSING OR KILL THE BUILD.   */ 00209000
*/********************************************************************/ 00210000
*                                                                  0239 00211000
*       GPR15=GPR0;                                                0239 00212000
         LR    GPR15,GPR0                                          0239 00213000
*       CALL PARMHDLR;                                             0240 00214000
         BALR  @14,@15                                             0240 00215000
*MAINSUB:                                                          0241 00216000
*       ;                                                          0241 00217000
MAINSUB  DS    0H                                                  0242 00218000
*       RC=GPR15;                                                  0242 00219000
         STH   GPR15,RC                                            0242 00220000
*     END;                                                         0243 00221000
*   ELSE                                                           0244 00222000
*     DO;                                                          0244 00223000
*                                                                  0244 00224000
         B     @RC00237                                            0244 00225000
@RF00237 DS    0H                                                  0245 00226000
*/********************************************************************/ 00227000
*/* IF THE RETURN CODE FROM PHR = 4 INDICATING THE KEYWORD WAS NOT   */ 00228000
*/* FOUND IN THE PARAMETER TABLE, WRITE THE APPROPRIATE ERROR        */ 00229000
*/* MESSAGE, SET THE RETURN CODE = 0 AND CONTINUE THE DO WHILE LOOP. */ 00230000
*/********************************************************************/ 00231000
*                                                                  0245 00232000
*       IF RC=4 THEN                                               0245 00233000
         CLC   RC(2),@CH00051                                      0245 00234000
         BNE   @RF00245                                            0245 00235000
*         DO;                                                      0246 00236000
*                                                                  0246 00237000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 00238000
*                                                                  0247 00239000
*           DO;                                                    0247 00240000
*             DCL                                                  0248 00241000
*               1 CMZP0005 STATIC LOCAL,/* PARAMETER LIST PASSED TO     00242000
*                                      ERROR FORMAT ROUTINE          */ 00243000
*                2 CMZL0005 FIXED(8) INIT(4),/* LEVEL PARAMETER    0248 00244000
*                                      (CLEV)                        */ 00245000
*                2 CMZE0005 FIXED(8) INIT(4),/* ERROR PARAMETER    0248 00246000
*                                      (CERR)                        */ 00247000
*                2 CMZA0005 CHAR(1) INIT('1'),/* ACTION PARAMETER  0248 00248000
*                                      (CACT)                        */ 00249000
*                2 CMZS0005 FIXED(8) INIT(0);/* LENGTH OF STRING   0248 00250000
*                                      PARAM                         */ 00251000
*             DCL                                                  0249 00252000
*               CMZR0005 REGISTER(1);/* ESTABLISH PARAMeter          */ 00253000
*             RESPECIFY                                            0250 00254000
*               CMZR0005 RSTD;      /* REGISTER FOR                  */ 00255000
*             CMZR0005=ADDR(CMZP0005);/* ADDRESS OF PARAMETER LIST   */ 00256000
         LA    CMZR0005,CMZP0005                                   0251 00257000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 00258000
         L     @15,@CV01247                                        0252 00259000
         BALR  @14,@15                                             0252 00260000
*             RESPECIFY                                            0253 00261000
*               CMZR0005 UNRSTD;                                   0253 00262000
*           END;                                                   0254 00263000
*           RC=0;                                                  0255 00264000
         SLR   @10,@10                                             0255 00265000
         STH   @10,RC                                              0255 00266000
*         END;                                                     0256 00267000
*       ELSE                                                       0257 00268000
*         DO;                                                      0257 00269000
*                                                                  0257 00270000
         B     @RC00245                                            0257 00271000
@RF00245 DS    0H                                                  0258 00272000
*/********************************************************************/ 00273000
*/* IF THE RETURN CODE FROM PHR = 8 OR 12 INDICATING AN END OF       */ 00274000
*/* STRING OR SYNTAX ERROR, THE FOLLOWING ROUTINE IS EXECUTED.       */ 00275000
*/********************************************************************/ 00276000
*                                                                  0258 00277000
*           CALL PCCUMAC1;                                         0258 00278000
         BAL   @14,PCCUMAC1                                        0258 00279000
*MAINSUB1:                                                         0259 00280000
*           ;                                                      0259 00281000
MAINSUB1 DS    0H                                                  0260 00282000
*         END;                                                     0260 00283000
*     END;                                                         0261 00284000
@RC00245 DS    0H                                                  0262 00285000
*   GPR1=KWDPTR;                                                   0262 00286000
@RC00237 LR    GPR1,KWDPTR                                         0262 00287000
*   MLCKWD=GPR1;                                                   0263 00288000
         ST    GPR1,MLCKWD(,MLCAREG)                               0263 00289000
*   GPR14=ADDR(DOLOOP);                                            0264 00290000
         LA    GPR14,DOLOOP                                        0264 00291000
*PCCUMAC1:                                                         0265 00292000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0265 00293000
*                                                                  0265 00294000
         B     @PB00003                                            0265 00295000
PCCUMAC1 DS    0H                                                  0266 00296000
*/********************************************************************/ 00297000
*/* THIS ROUTINE WILL PROCESS RETURN CODES 8 AND 12 FROM THE         */ 00298000
*/* PARAMETER HANDLER RTN. IF RC IS 8 THEN CERTAIN VALIDITY CHECKS   */ 00299000
*/* AND COMBINATION CHECKS ARE MADE. IF ANY ERRORS - A MESSAGE IS    */ 00300000
*/* PUT OUT. IF RC IS OTHER THAN 8 A SYNTAX OR FUNCTION ERROR        */ 00301000
*/* EXISTS. A MSG IS PRINTED AND THE BUILD IS KILLED.                */ 00302000
*/********************************************************************/ 00303000
*                                                                  0266 00304000
*   IF RC=8                         /* RC = 8 - PARM STRING EXHAUSTED*/ 00305000
*     THEN                                                         0266 00306000
         CLC   RC(2),@CH00035                                      0266 00307000
         BNE   @RF00266                                            0266 00308000
*     DO;                                                          0267 00309000
*       IF RRNRADMP=ON              /* CHECK FOR DUMPAUTO PARM       */ 00310000
*         THEN                      /* IF YES - CHECK TO SEE IF A PDS*/ 00311000
         TM    RRNRADMP(ENTRYPTR),B'00100000'                      0268 00312000
         BNO   @RF00268                                            0268 00313000
*         IF LFPDMPDS=ON            /* WAS SPECIFIED. IF IT WAS      */ 00314000
*           THEN                                                   0269 00315000
         TM    LFPDMPDS,B'01000000'                                0269 00316000
         BO    @RT00269                                            0269 00317000
*           ;                       /* RETURN TO CALLER              */ 00318000
*         ELSE                                                     0271 00319000
*           DO;                     /* IF NOT PUT OUT ERROR MSG      */ 00320000
*/* ERRFMT    LEVEL(PARM)  ERROR(CONF)  ACTION(TAKEDEF)  STRING('AUTODM 00321000
*')                                                                  */ 00322000
*                                                                  0272 00323000
*             DO;                                                  0272 00324000
*               DCL                                                0273 00325000
*                 1 CMZP0006 STATIC LOCAL,/* PARAMETER LIST PASSED TO   00326000
*                                      ERROR FORMAT ROUTINE          */ 00327000
*                  2 CMZL0006 FIXED(8) INIT(4),/* LEVEL PARAMETER  0273 00328000
*                                      (CLEV)                        */ 00329000
*                  2 CMZE0006 FIXED(8) INIT(2),/* ERROR PARAMETER  0273 00330000
*                                      (CERR)                        */ 00331000
*                  2 CMZA0006 CHAR(1) INIT('2'),/* ACTION PARAMETER     00332000
*                                      (CACT)                        */ 00333000
*                  2 CMZS0006 FIXED(8) INIT(7),/* LENGTH OF STRING 0273 00334000
*                                      PARAM                         */ 00335000
*                  2 CMZM0006 CHAR(7) INIT('AUTODMP');/* PARAMETER 0273 00336000
*                                      STRING                        */ 00337000
*               DCL                                                0274 00338000
*                 CMZR0006 REGISTER(1);/* ESTABLISH PARAMeter        */ 00339000
*               RESPECIFY                                          0275 00340000
*                 CMZR0006 RSTD;    /* REGISTER FOR                  */ 00341000
*               CMZR0006=ADDR(CMZP0006);/* ADDRESS OF PARAMETER LIST */ 00342000
         LA    CMZR0006,CMZP0006                                   0276 00343000
*               CALL ISTSDCC2;      /* CALL ERROR FORMAT ROUTINE     */ 00344000
         L     @15,@CV01247                                        0277 00345000
         BALR  @14,@15                                             0277 00346000
*               RESPECIFY                                          0278 00347000
*                 CMZR0006 UNRSTD;                                 0278 00348000
*             END;                                                 0279 00349000
*                                                                  0279 00350000
*/* ERRFMT    LEVEL(PARM)  ERROR(MISS)  ACTION(IGNORE)  STRING('DUMPDS' 00351000
*                                                                    */ 00352000
*                                                                  0280 00353000
*             DO;                                                  0280 00354000
*               DCL                                                0281 00355000
*                 1 CMZP0007 STATIC LOCAL,/* PARAMETER LIST PASSED TO   00356000
*                                      ERROR FORMAT ROUTINE          */ 00357000
*                  2 CMZL0007 FIXED(8) INIT(4),/* LEVEL PARAMETER  0281 00358000
*                                      (CLEV)                        */ 00359000
*                  2 CMZE0007 FIXED(8) INIT(8),/* ERROR PARAMETER  0281 00360000
*                                      (CERR)                        */ 00361000
*                  2 CMZA0007 CHAR(1) INIT('1'),/* ACTION PARAMETER     00362000
*                                      (CACT)                        */ 00363000
*                  2 CMZS0007 FIXED(8) INIT(6),/* LENGTH OF STRING 0281 00364000
*                                      PARAM                         */ 00365000
*                  2 CMZM0007 CHAR(6) INIT('DUMPDS');/* PARAMETER  0281 00366000
*                                      STRING                        */ 00367000
*               DCL                                                0282 00368000
*                 CMZR0007 REGISTER(1);/* ESTABLISH PARAMeter        */ 00369000
*               RESPECIFY                                          0283 00370000
*                 CMZR0007 RSTD;    /* REGISTER FOR                  */ 00371000
*               CMZR0007=ADDR(CMZP0007);/* ADDRESS OF PARAMETER LIST */ 00372000
         LA    CMZR0007,CMZP0007                                   0284 00373000
*               CALL ISTSDCC2;      /* CALL ERROR FORMAT ROUTINE     */ 00374000
         L     @15,@CV01247                                        0285 00375000
         BALR  @14,@15                                             0285 00376000
*               RESPECIFY                                          0286 00377000
*                 CMZR0007 UNRSTD;                                 0286 00378000
*             END;                                                 0287 00379000
*             RRNRADMP=OFF;         /* CLEAR AUTODMP REQUEST FLAG    */ 00380000
         NI    RRNRADMP(ENTRYPTR),B'11011111'                      0288 00381000
*           END;                                                   0289 00382000
*       ELSE                                                       0290 00383000
*         ;                         /* DUMPAUTO NOT SPECIFIED - RTN  */ 00384000
@RF00268 DS    0H                                                  0291 00385000
*     END;                                                         0291 00386000
*   ELSE                                                           0292 00387000
*     DO;                                                          0292 00388000
         B     @RC00266                                            0292 00389000
@RF00266 DS    0H                                                  0293 00390000
*       IF RC=12                    /* RTN CODE 12 - SYNTAX ERROR    */ 00391000
*         THEN                      /* PUT OUT MSG AND KILLBLD       */ 00392000
         CLC   RC(2),@CH00032                                      0293 00393000
         BNE   @RF00293                                            0293 00394000
*/* ERRFMT    LEVEL(PARM)  ERROR(SYN)  ACTION(KILLBLD)               */ 00395000
*                                                                  0294 00396000
*         DO;                                                      0294 00397000
*           DCL                                                    0295 00398000
*             1 CMZP0008 STATIC LOCAL,/* PARAMETER LIST PASSED TO  0295 00399000
*                                      ERROR FORMAT ROUTINE          */ 00400000
*              2 CMZL0008 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV) */ 00401000
*              2 CMZE0008 FIXED(8) INIT(6),/* ERROR PARAMETER (CERR) */ 00402000
*              2 CMZA0008 CHAR(1) INIT('6'),/* ACTION PARAMETER    0295 00403000
*                                      (CACT)                        */ 00404000
*              2 CMZS0008 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM */ 00405000
*           DCL                                                    0296 00406000
*             CMZR0008 REGISTER(1); /* ESTABLISH PARAMeter           */ 00407000
*           RESPECIFY                                              0297 00408000
*             CMZR0008 RSTD;        /* REGISTER FOR                  */ 00409000
*           CMZR0008=ADDR(CMZP0008);/* ADDRESS OF PARAMETER LIST     */ 00410000
         LA    CMZR0008,CMZP0008                                   0298 00411000
