@ER00002 BR    @14                                                 0532 00490000
@PB00002 DS    0H                                                  0533 00491000
*                                                                  0533 00492000
*/* SEGMENT(PUMOVE)START                                             */ 00493000
*                                                                  0533 00494000
*   DO;                                                            0533 00495000
*     GOTO PUMOVE@;                                                0534 00496000
         B     PUMOVE@                                             0534 00497000
*PUMOVE:                                                           0535 00498000
*   END;                                                           0535 00499000
*                                                                  0535 00500000
PUMOVE   DS    0H                                                  0536 00501000
*/********************************************************************/ 00502000
*/*                                                                  */ 00503000
*/*   PUMOVE SEGMENT:                                                */ 00504000
*/*   THIS SEGMENT CALLS ISTSDCC1 TO MOVE WHICH EVER ENTRY HAS BEEN  */ 00505000
*/*   BUILT BY ALL THE OTHER CODE(IF THE BUILD WORKED)               */ 00506000
*/*                                                                  */ 00507000
*/********************************************************************/ 00508000
*                                                                  0536 00509000
*   IF RNCGLSBD='1'B                /* IS THIS A LOCAL SUBAREA ENTRY?*/ 00510000
*     THEN                                                         0536 00511000
         TM    RNCGLSBD(RNCAREG),B'00000001'                       0536 00512000
         BNO   @RF00536                                            0536 00513000
*     DO;                           /* YES - PROCESS IT AS SUCH      */ 00514000
*       GPR0=0;                     /* SET UP TO MOVE DAN ENTRY      */ 00515000
         SLR   GPR0,GPR0                                           0538 00516000
*       GPR1=ENTRYPTR;              /* SET ADDR OF ENTRY             */ 00517000
         LR    GPR1,ENTRYPTR                                       0539 00518000
*       CALL ISTSDCC1;              /* MOVE ENTRY                    */ 00519000
         L     @15,@CV02910                                        0540 00520000
         BALR  @14,@15                                             0540 00521000
*       RC=GPR15;                   /* SAVE C1 RETURN CODE           */ 00522000
         STH   GPR15,RC                                            0541 00523000
*       IF RC=0                     /* NOW TEST IT                   */ 00524000
*         THEN                                                     0542 00525000
         LH    @11,RC                                              0542 00526000
         LTR   @11,@11                                             0542 00527000
         BNZ   @RF00542                                            0542 00528000
*         DO;                       /* YES - MORE PROCESSING         */ 00529000
*           RNCDA=GPR1;             /* SAVE PTR TO NEW LOCATION OF 0544 00530000
*                                      DAN ENTRY                     */ 00531000
         ST    GPR1,RNCDA(,RNCAREG)                                0544 00532000
*           GPR1=MLCPTR5;           /* SET REG 1 TO ADDR OF PU ENTRY    00533000
*                                      TO BE MOVED AFTER THE DAN   0545 00534000
*                                      ENTRY HAS BEEN MOVED          */ 00535000
         L     @11,MLCPTR5(,MLCAREG)                               0545 00536000
         LR    GPR1,@11                                            0545 00537000
*           IF RCCRBISA='1'B THEN                                  0546 00538000
