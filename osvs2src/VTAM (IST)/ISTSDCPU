*   IF RC=4                         /* CHECK FIRST MOVE AGAIN        */ 00556000
*     THEN                                                         0553 00557000
@RC00536 CLC   RC(2),@CH00052                                      0553 00558000
         BE    @RT00553                                            0553 00559000
*     ;                             /* YES IT FAILED - DO NOTHING  0554 00560000
*                                      MORE                          */ 00561000
*   ELSE                                                           0555 00562000
*     DO;                           /* FIRST MOVE OK OR NO MOVE -  0555 00563000
*                                      CONTINUE                      */ 00564000
*       GPR0=0;                     /* INDICATE MOVE ENTRY -> BY REG1*/ 00565000
