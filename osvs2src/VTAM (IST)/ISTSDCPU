*             DO;                   /* YES - OUTPUT IGNORE MSG       */ 02070000
*/* ERRFMT    LEVEL(VALUE)  ERROR(EXTRA)  ACTION(IGNORE)             */ 02071000
*                                                                  1018 02072000
*               DO;                                                1018 02073000
*                 DCL                                              1019 02074000
*                   1 CMZP0043 STATIC LOCAL,/* PARAMETER LIST PASSED    02075000
*                                      TO ERROR FORMAT ROUTINE       */ 02076000
*                    2 CMZL0043 FIXED(8) INIT(5),/* LEVEL PARAMETER     02077000
*                                      (CLEV)                        */ 02078000
*                    2 CMZE0043 FIXED(8) INIT(7),/* ERROR PARAMETER     02079000
*                                      (CERR)                        */ 02080000
*                    2 CMZA0043 CHAR(1) INIT('1'),/* ACTION PARAMETER   02081000
*                                      (CACT)                        */ 02082000
*                    2 CMZS0043 FIXED(8) INIT(0);/* LENGTH OF STRING    02083000
*                                      PARAM                         */ 02084000
*                 DCL                                              1020 02085000
*                   CMZR0043 REGISTER(1);/* ESTABLISH PARAMeter      */ 02086000
*                 RESPECIFY                                        1021 02087000
*                   CMZR0043 RSTD;  /* REGISTER FOR                  */ 02088000
*                 CMZR0043=ADDR(CMZP0043);/* ADDRESS OF PARAMETER  1022 02089000
*                                      LIST                          */ 02090000
         LA    CMZR0043,CMZP0043                                   1022 02091000
*                 CALL ISTSDCC2;    /* CALL ERROR FORMAT ROUTINE     */ 02092000
         L     @15,@CV02911                                        1023 02093000
         BALR  @14,@15                                             1023 02094000
*                 RESPECIFY                                        1024 02095000
*                   CMZR0043 UNRSTD;                               1024 02096000
*               END;                                               1025 02097000
*             END;                                                 1026 02098000
*           ELSE                                                   1027 02099000
*             ;                     /* NO - DO NOTHING               */ 02100000
@RF01016 DS    0H                                                  1028 02101000
*         END;                                                     1028 02102000
*       ELSE                                                       1029 02103000
*         ;                         /* NO - C4 FOUND AN ERROR - DO 1029 02104000
*                                      NOTHING                       */ 02105000
@RF01007 DS    0H                                                  1030 02106000
*     END;                                                         1030 02107000
*   ELSE                                                           1031 02108000
*     RC=4;                         /* ERROR - FORCE KILL MSG        */ 02109000
         B     @RC01001                                            1031 02110000
@RF01001 MVC   RC(2),@CH00052                                      1031 02111000
*   IF RC=0                         /* EVERTHING WENT OK?            */ 02112000
*     THEN                                                         1032 02113000
@RC01001 ICM   @11,3,RC                                            1032 02114000
         BZ    @RT01032                                            1032 02115000
*     ;                             /* YES - DO NOTHING              */ 02116000
*   ELSE                                                           1034 02117000
*     DO;                           /* NO - OUTPUT KILL MSG          */ 02118000
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(KILLBLD)            */ 02119000
*                                                                  1035 02120000
*       DO;                                                        1035 02121000
*         DCL                                                      1036 02122000
*           1 CMZP0044 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   02123000
*                                      FORMAT ROUTINE                */ 02124000
*            2 CMZL0044 FIXED(8) INIT(5),/* LEVEL PARAMETER (CLEV)   */ 02125000
*            2 CMZE0044 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR)   */ 02126000
*            2 CMZA0044 CHAR(1) INIT('6'),/* ACTION PARAMETER (CACT) */ 02127000
*            2 CMZS0044 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 02128000
*         DCL                                                      1037 02129000
*           CMZR0044 REGISTER(1);   /* ESTABLISH PARAMeter           */ 02130000
*         RESPECIFY                                                1038 02131000
*           CMZR0044 RSTD;          /* REGISTER FOR                  */ 02132000
*         CMZR0044=ADDR(CMZP0044);  /* ADDRESS OF PARAMETER LIST     */ 02133000
