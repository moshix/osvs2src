         B     PUPRSCN#                                            0510 00399000
*PUPRSCN@:                                                         0511 00400000
*   END;                                                           0511 00401000
*                                                                  0512 00402000
*/********************************************************************/ 00403000
*/*                                                                  */ 00404000
*/*   PUTYPE SUBROUTINE                                              */ 00405000
*/*                                                                  */ 00406000
*/*   THIS ROUTINE WILL CHECK FOR A PUTYPE KEYWORD IN CALLING MACRO  */ 00407000
*/*   (FUNCTION 02) AND SET PUTYPE DEFAULT IN ENTRY MODEL            */ 00408000
*/*   NOTE:  THIS PROCEDURE IS USED BY PUPRSCN.                      */ 00409000
*/*                                                                  */ 00410000
*/********************************************************************/ 00411000
*                                                                  0512 00412000
*PUPUTYPE:                                                         0512 00413000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0512 00414000
         B     @PB00002                                            0512 00415000
PUPUTYPE DS    0H                                                  0513 00416000
*   IF PHRF2VAL(1)=0                /* CHECK FOR SPECIFICATION =   0513 00417000
*                                      TYPE1                         */ 00418000
*     THEN                                                         0513 00419000
         ICM   @11,15,PHRF2VAL(PHR1REG)                            0513 00420000
         BNZ   @RF00513                                            0513 00421000
*     RCCTYPE=RCCPU01;              /* YES - SET TYPE1               */ 00422000
         L     @11,MLCPTR5(,MLCAREG)                               0514 00423000
         MVI   RCCTYPE(@11),X'01'                                  0514 00424000
*   ELSE                                                           0515 00425000
*     IF PHRF2VAL(1)=1              /* CHECK FOR SPECIFICATION =   0515 00426000
*                                      TYPE2                         */ 00427000
*       THEN                                                       0515 00428000
         B     @RC00513                                            0515 00429000
@RF00513 CLC   PHRF2VAL(4,PHR1REG),@CF00039                        0515 00430000
         BNE   @RF00515                                            0515 00431000
*       RCCTYPE=RCCPU02;            /* YES - SET IT                  */ 00432000
         L     @11,MLCPTR5(,MLCAREG)                               0516 00433000
         MVI   RCCTYPE(@11),X'02'                                  0516 00434000
*     ELSE                                                         0517 00435000
*       IF PHRF2VAL(1)=2            /* CHECK FOR SPECIFICATION =   0517 00436000
*                                      TYPE4                         */ 00437000
*         THEN                                                     0517 00438000
         B     @RC00515                                            0517 00439000
@RF00515 CLC   PHRF2VAL(4,PHR1REG),@CF00234                        0517 00440000
         BNE   @RF00517                                            0517 00441000
*         RCCTYPE=RCCPU04;          /* YES SET TYPE4                 */ 00442000
         L     @11,MLCPTR5(,MLCAREG)                               0518 00443000
         MVI   RCCTYPE(@11),X'04'                                  0518 00444000
*       ELSE                                                       0519 00445000
*         DO;                       /* NO - ISSUE ERROR MSG - TAKE 0519 00446000
*                                      DEFAULT                       */ 00447000
         B     @RC00517                                            0519 00448000
@RF00517 DS    0H                                                  0520 00449000
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(TAKEDEF)            */ 00450000
*                                                                  0520 00451000
*           DO;                                                    0520 00452000
*             DCL                                                  0521 00453000
*               1 CMZP0014 STATIC LOCAL,/* PARAMETER LIST PASSED TO     00454000
*                                      ERROR FORMAT ROUTINE          */ 00455000
*                2 CMZL0014 FIXED(8) INIT(5),/* LEVEL PARAMETER    0521 00456000
*                                      (CLEV)                        */ 00457000
*                2 CMZE0014 FIXED(8) INIT(4),/* ERROR PARAMETER    0521 00458000
