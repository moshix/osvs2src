*/*                                                                  */ 03171000
*/*   PUTYPE PARAMETER SUBROUTINE                                    */ 03172000
*/*                                                                  */ 03173000
*/*   THIS ROUTINE CHECKS TO SEE IF PUTYPE SPECIFIED IN CLUSTER      */ 03174000
*/*   MACRO AND IF SO, ISSUSES AN IGNORE MSG                         */ 03175000
*/*   (FUNCTION CODE = 07)                                           */ 03176000
*/*                                                                  */ 03177000
*/********************************************************************/ 03178000
*                                                                  1366 03179000
*PUTYPE:                                                           1366 03180000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1366 03181000
PUTYPE   DS    0H                                                  1367 03182000
*   IF RNCGCLPU='1'B                /* CHECK FOR CLUSTER MACRO     1367 03183000
*                                      HANDLER FOOTPRINT             */ 03184000
*     THEN                                                         1367 03185000
         TM    RNCGCLPU(RNCAREG),B'00001000'                       1367 03186000
         BNO   @RF01367                                            1367 03187000
*     DO;                           /* YES - PUT OUT IGNORE MSG      */ 03188000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 03189000
*                                                                  1369 03190000
*       DO;                                                        1369 03191000
*         DCL                                                      1370 03192000
*           1 CMZP0083 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   03193000
*                                      FORMAT ROUTINE                */ 03194000
*            2 CMZL0083 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)   */ 03195000
*            2 CMZE0083 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR)   */ 03196000
*            2 CMZA0083 CHAR(1) INIT('1'),/* ACTION PARAMETER (CACT) */ 03197000
*            2 CMZS0083 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 03198000
*         DCL                                                      1371 03199000
*           CMZR0083 REGISTER(1);   /* ESTABLISH PARAMeter           */ 03200000
*         RESPECIFY                                                1372 03201000
*           CMZR0083 RSTD;          /* REGISTER FOR                  */ 03202000
*         CMZR0083=ADDR(CMZP0083);  /* ADDRESS OF PARAMETER LIST     */ 03203000
         LA    CMZR0083,CMZP0083                                   1373 03204000
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */ 03205000
         L     @15,@CV02911                                        1374 03206000
