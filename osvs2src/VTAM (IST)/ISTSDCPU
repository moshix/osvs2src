*       CALL ISTSDCC4;              /* CONVERT CUA TO HEX            */ 01614000
         L     @15,@CV02912                                        0869 01615000
         BALR  @14,@15                                             0869 01616000
*       RC=GPR15;                   /* SAVE C4S RETURN CODE          */ 01617000
         STH   GPR15,RC                                            0870 01618000
*       IF RC=0                     /* NOW CHECK C4S RC              */ 01619000
*         THEN                                                     0871 01620000
         LH    @11,RC                                              0871 01621000
         LTR   @11,@11                                             0871 01622000
         BNZ   @RF00871                                            0871 01623000
*         DO;                       /* RC OK                         */ 01624000
*           IF PHRF34NS>1           /* MORE THAN ONE SPECIFIED?      */ 01625000
*             THEN                                                 0873 01626000
         L     @11,PHRF34NS(,PHR1REG)                              0873 01627000
         CH    @11,@CH00039                                        0873 01628000
         BNH   @RF00873                                            0873 01629000
*             DO;                   /* YES - OUTPUT IGNORE MSG       */ 01630000
*/* ERRFMT    LEVEL(VALUE)  ERROR(EXTRA)  ACTION(IGNORE)             */ 01631000
*                                                                  0875 01632000
*               DO;                                                0875 01633000
*                 DCL                                              0876 01634000
*                   1 CMZP0036 STATIC LOCAL,/* PARAMETER LIST PASSED    01635000
*                                      TO ERROR FORMAT ROUTINE       */ 01636000
*                    2 CMZL0036 FIXED(8) INIT(5),/* LEVEL PARAMETER     01637000
