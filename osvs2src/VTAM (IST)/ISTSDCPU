*         RESPECIFY                                                1375 03208000
*           CMZR0083 UNRSTD;                                       1375 03209000
*       END;                                                       1376 03210000
*     END;                                                         1377 03211000
*   ELSE                                                           1378 03212000
*     ;                             /* NO - DO NOTHING               */ 03213000
@RF01367 DS    0H                                                  1379 03214000
*   RC=0;                           /* FORCE ZERO RC                 */ 03215000
         SLR   @11,@11                                             1379 03216000
         STH   @11,RC                                              1379 03217000
*   GPR14=ADDR(MAINSUB);            /* SET RETURN ADDR               */ 03218000
         LA    GPR14,MAINSUB                                       1380 03219000
*   END PUTYPE;                                                    1381 03220000
