*/*                                                                  */ 03226000
*/*                   SSCPFM PARAMETER SUBROUTINE                    */ 03227000
*/*                                                                  */ 03228000
*/* THIS ROUTINE WILL CHECK THE VALIDITY OF THE SSCPFM VALUE         */ 03229000
*/*   SPECIFIES AND SET IT INTO THE LUSIFT ENTRY. (FUNCTION CODE =   */ 03230000
*/*   33)                                                            */ 03231000
*/*                                                                  */ 03232000
*/********************************************************************/ 03233000
*                                                                  1382 03234000
*PUSSCPFM:                                                         1382 03235000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1382 03236000
PUSSCPFM DS    0H                                                  1383 03237000
*   IF PHRF34SP(1)='USSSCS  '       /* WAS USSSCS SPECIFIED ON     1383 03238000
*                                      SSCPFM?                       */ 03239000
*     THEN                                                         1383 03240000
         CLC   PHRF34SP(8,PHR1REG),@CC03383                        1383 03241000
         BNE   @RF01383                                            1383 03242000
*     RLUSSCFM=RLUUSS;              /* YES - SET LU RDTE             */ 03243000
         L     @11,MLCPTR6(,MLCAREG)                               1384 03244000
         MVI   RLUSSCFM(@11),X'02'                                 1384 03245000
*   ELSE                                                           1385 03246000
*     IF PHRF34SP(1)='USS3270 '     /* NOT USSSCS - IS IT USS3270?   */ 03247000
*       THEN                                                       1385 03248000
         B     @RC01383                                            1385 03249000
@RF01383 CLC   PHRF34SP(8,PHR1REG),@CC03384                        1385 03250000
         BNE   @RF01385                                            1385 03251000
*       RLUSSCFM=RLUU3270;          /* YES - SET IT                  */ 03252000
         L     @11,MLCPTR6(,MLCAREG)                               1386 03253000
         MVI   RLUSSCFM(@11),X'04'                                 1386 03254000
*     ELSE                                                         1387 03255000
*       DO;                         /* NOT USSSCS OR USS3270 - IS IT    03256000
*                                      FSS?                          */ 03257000
         B     @RC01385                                            1387 03258000
@RF01385 DS    0H                                                  1388 03259000
*         IF PHRF34SP(1)='FSS     ' /* FSS?                          */ 03260000
*           THEN                                                   1388 03261000
         CLC   PHRF34SP(8,PHR1REG),@CC03385                        1388 03262000
         BE    @RT01388                                            1388 03263000
*           ;                       /* YES - DO NOTHING HERE         */ 03264000
*         ELSE                                                     1390 03265000
*           DO;                     /* NOT FSS - ISSUE TAKEDEF MSG   */ 03266000
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(TAKEDEF)            */ 03267000
*                                                                  1391 03268000
*             DO;                                                  1391 03269000
*               DCL                                                1392 03270000
*                 1 CMZP0084 STATIC LOCAL,/* PARAMETER LIST PASSED TO   03271000
*                                      ERROR FORMAT ROUTINE          */ 03272000
*                  2 CMZL0084 FIXED(8) INIT(5),/* LEVEL PARAMETER  1392 03273000
*                                      (CLEV)                        */ 03274000
*                  2 CMZE0084 FIXED(8) INIT(4),/* ERROR PARAMETER  1392 03275000
*                                      (CERR)                        */ 03276000
*                  2 CMZA0084 CHAR(1) INIT('2'),/* ACTION PARAMETER     03277000
*                                      (CACT)                        */ 03278000
*                  2 CMZS0084 FIXED(8) INIT(0);/* LENGTH OF STRING 1392 03279000
*                                      PARAM                         */ 03280000
*               DCL                                                1393 03281000
*                 CMZR0084 REGISTER(1);/* ESTABLISH PARAMeter        */ 03282000
*               RESPECIFY                                          1394 03283000
*                 CMZR0084 RSTD;    /* REGISTER FOR                  */ 03284000
*               CMZR0084=ADDR(CMZP0084);/* ADDRESS OF PARAMETER LIST */ 03285000
         LA    CMZR0084,CMZP0084                                   1395 03286000
