*   ELSE                                                           1408 03329000
*     DO;                           /* NO - ERROR - ISSUE MSG        */ 03330000
         B     @RC01406                                            1408 03331000
@RF01406 DS    0H                                                  1409 03332000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 03333000
*                                                                  1409 03334000
*       DO;                                                        1409 03335000
*         DCL                                                      1410 03336000
*           1 CMZP0085 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   03337000
*                                      FORMAT ROUTINE                */ 03338000
*            2 CMZL0085 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)   */ 03339000
*            2 CMZE0085 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR)   */ 03340000
*            2 CMZA0085 CHAR(1) INIT('1'),/* ACTION PARAMETER (CACT) */ 03341000
*            2 CMZS0085 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 03342000
*         DCL                                                      1411 03343000
*           CMZR0085 REGISTER(1);   /* ESTABLISH PARAMeter           */ 03344000
*         RESPECIFY                                                1412 03345000
*           CMZR0085 RSTD;          /* REGISTER FOR                  */ 03346000
