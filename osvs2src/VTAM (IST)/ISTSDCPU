         MVI   RCCTYPE(@11),X'02'                                  0447 00213000
*     ELSE                                                         0448 00214000
*       DO;                         /* NONE OF THE ABOVE-IMPLIES REAL   00215000
*                                      PU MACRO CALL                 */ 00216000
         B     @RC00446                                            0448 00217000
@RF00446 DS    0H                                                  0449 00218000
*         GPR1=MLCKWD;              /* POINT TO FIRST OF KEYWORDS IN    00219000
*                                      STRING                        */ 00220000
