@RT01032 OI    NUMFTPT,B'00001000'                                 1044 02143000
*   GPR14=ADDR(MAINSUB);            /* SET RETURN ADDR               */ 02144000
         LA    GPR14,MAINSUB                                       1045 02145000
*   END PUIDNUM;                                                   1046 02146000
@EL00012 DS    0H                                                  1046 02147000
@EF00012 DS    0H                                                  1046 02148000
@ER00012 BR    @14                                                 1046 02149000
*                                                                  1047 02150000
*/********************************************************************/ 02151000
*/*                                                                  */ 02152000
*/*   IRETRY PARAMETER SUBROUTINE                                    */ 02153000
*/*                                                                  */ 02154000
*/*   THIS ROUTINE WILL CHECK FOR A VALID IRETRY SPECIFICATION       */ 02155000
*/*   (FUNCTION 02) AND SETS THE BIT IN THE PU ENTRY                 */ 02156000
*/*                                                                  */ 02157000
*/********************************************************************/ 02158000
*                                                                  1047 02159000
*PUIRETRY:                                                         1047 02160000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1047 02161000
PUIRETRY DS    0H                                                  1048 02162000
*   IF PHRF2VAL(1)=1                /* WAS YES SPECIFIED?            */ 02163000
*     THEN                                                         1048 02164000
         CLC   PHRF2VAL(4,PHR1REG),@CF00039                        1048 02165000
         BNE   @RF01048                                            1048 02166000
*     RCCIRETR='1'B;                /* YES - SET PU ENTRY BIT        */ 02167000
         L     @11,MLCPTR5(,MLCAREG)                               1049 02168000
         OI    RCCIRETR(@11),B'00100000'                           1049 02169000
*   ELSE                                                           1050 02170000
*     DO;                           /* NO - VALIDITY CHECK         1050 02171000
*                                      SPECIFICATION                 */ 02172000
         B     @RC01048                                            1050 02173000
@RF01048 DS    0H                                                  1051 02174000
*       IF PHRF2VAL(1)=2|PHRF2VAL(1)=0/* NO OR DEFAULT SPECIFIED?    */ 02175000
*         THEN                                                     1051 02176000
         L     @11,PHRF2VAL(,PHR1REG)                              1051 02177000
         CH    @11,@CH00234                                        1051 02178000
         BE    @RT01051                                            1051 02179000
         LTR   @11,@11                                             1051 02180000
         BZ    @RT01051                                            1051 02181000
*         ;                         /* YES - DO NOTHING NOW          */ 02182000
*       ELSE                                                       1053 02183000
*         DO;                       /* NO - OUTPUT TAKEDEF MAG       */ 02184000
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(TAKEDEF)            */ 02185000
*                                                                  1054 02186000
*           DO;                                                    1054 02187000
*             DCL                                                  1055 02188000
*               1 CMZP0045 STATIC LOCAL,/* PARAMETER LIST PASSED TO     02189000
*                                      ERROR FORMAT ROUTINE          */ 02190000
*                2 CMZL0045 FIXED(8) INIT(5),/* LEVEL PARAMETER    1055 02191000
*                                      (CLEV)                        */ 02192000
*                2 CMZE0045 FIXED(8) INIT(4),/* ERROR PARAMETER    1055 02193000
*                                      (CERR)                        */ 02194000
*                2 CMZA0045 CHAR(1) INIT('2'),/* ACTION PARAMETER  1055 02195000
*                                      (CACT)                        */ 02196000
*                2 CMZS0045 FIXED(8) INIT(0);/* LENGTH OF STRING   1055 02197000
*                                      PARAM                         */ 02198000
*             DCL                                                  1056 02199000
*               CMZR0045 REGISTER(1);/* ESTABLISH PARAMeter          */ 02200000
*             RESPECIFY                                            1057 02201000
*               CMZR0045 RSTD;      /* REGISTER FOR                  */ 02202000
*             CMZR0045=ADDR(CMZP0045);/* ADDRESS OF PARAMETER LIST   */ 02203000
         LA    CMZR0045,CMZP0045                                   1058 02204000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 02205000
         L     @15,@CV02911                                        1059 02206000
         BALR  @14,@15                                             1059 02207000
*             RESPECIFY                                            1060 02208000
*               CMZR0045 UNRSTD;                                   1060 02209000
*           END;                                                   1061 02210000
*         END;                                                     1062 02211000
*       RCCIRETR='0'B;              /* ERROR OR DEFAULT SPECIFIED -     02212000
*                                      TAKE DEF                      */ 02213000
@RT01051 L     @11,MLCPTR5(,MLCAREG)                               1063 02214000
         NI    RCCIRETR(@11),B'11011111'                           1063 02215000
*     END;                                                         1064 02216000
*   RC=0;                           /* FORCE ZERO RC                 */ 02217000
@RC01048 SLR   @11,@11                                             1065 02218000
         STH   @11,RC                                              1065 02219000
*   GPR14=ADDR(MAINSUB);            /* SET RETURN ADDR               */ 02220000
         LA    GPR14,MAINSUB                                       1066 02221000
*   END PUIRETRY;                                                  1067 02222000
@EL00013 DS    0H                                                  1067 02223000
@EF00013 DS    0H                                                  1067 02224000
@ER00013 BR    @14                                                 1067 02225000
*                                                                  1068 02226000
*/********************************************************************/ 02227000
*/*                                                                  */ 02228000
*/*   ISTATUS PARAMETER SUBROUTINE                                   */ 02229000
*/*                                                                  */ 02230000
