*/********************************************************************/ 00827000
*/*                                                                  */ 00828000
*/*   PUVLCHK SEGMENT:                                               */ 00829000
*/*   THIS SEGMENT DOES THE VALIDITY CHECKING TO SEE IF ALL REQUIRED */ 00830000
*/*   VALUES/KEYWORDS WERE SPECIFIED ON THE PU MACRO                 */ 00831000
*/*                                                                  */ 00832000
*/********************************************************************/ 00833000
*                                                                  0640 00834000
*   IF RNCGRNBD='1'B                /* PU SPECIFIED IN RNBLD?        */ 00835000
*     THEN                                                         0640 00836000
         TM    RNCGRNBD(RNCAREG),B'00000100'                       0640 00837000
         BO    @RT00640                                            0640 00838000
*     ;                             /* YES - NO NOTHING - NCPGEN DID    00839000
*                                      ALL CHECKING                  */ 00840000
*   ELSE                                                           0642 00841000
*     IF RNCGLSBD='1'B              /* IS BUILD LOCAL SUBAREA?       */ 00842000
*       THEN                                                       0642 00843000
         TM    RNCGLSBD(RNCAREG),B'00000001'                       0642 00844000
         BNO   @RF00642                                            0642 00845000
*       DO;                         /* YES - CONTINUE                */ 00846000
*         IF CUAFTPT='1'B           /* CUADDR SPECIFIED?             */ 00847000
*           THEN                                                   0644 00848000
         TM    CUAFTPT,B'00100000'                                 0644 00849000
         BO    @RT00644                                            0644 00850000
*           ;                       /* YES - DO NOTHING              */ 00851000
*         ELSE                                                     0646 00852000
*           IF RCCRBISA='0'B        /* ISTATUS INACTIVE?             */ 00853000
*             THEN                                                 0646 00854000
         L     @11,MLCPTR5(,MLCAREG)                               0646 00855000
         TM    RCCRBISA(@11),B'10000000'                           0646 00856000
         BZ    @RT00646                                            0646 00857000
