         BNE   @RF01001                                            1001 02027000
*     DO;                           /* CONTINUE - IDNUM WAS SPECIFIED   02028000
*                                      OK                            */ 02029000
*       GPR0=5;                     /* SET LENGTH OF 5 FOR C4        */ 02030000
         LR    GPR0,@11                                            1003 02031000
*       GPR1=ADDR(PHRF34SP(1,4:8)); /* GET ADDR OF FIELD TO BE     1004 02032000
*                                      CONVERTED                     */ 02033000
         LA    GPR1,PHRF34SP+3(,PHR1REG)                           1004 02034000
*       CALL ISTSDCC4;              /* CONVERT IDNUM TO HEX          */ 02035000
         L     @15,@CV02912                                        1005 02036000
         BALR  @14,@15                                             1005 02037000
*       RC=GPR15;                   /* SAVE RC FROM C4               */ 02038000
         STH   GPR15,RC                                            1006 02039000
*       IF RC=0                     /* C4 PROCESS IDNUM OK           */ 02040000
*         THEN                                                     1007 02041000
         LH    @11,RC                                              1007 02042000
         LTR   @11,@11                                             1007 02043000
         BNZ   @RF01007                                            1007 02044000
*         DO;                       /* YES - CONTINUE                */ 02045000
*           RFY                                                    1009 02046000
*             WORKREG RSTD;                                        1009 02047000
*           WORKREG=PHRF34SP(1,4:6);/* GET IDNUM VALUE IN REG        */ 02048000
         SLR   WORKREG,WORKREG                                     1010 02049000
         ICM   WORKREG,7,PHRF34SP+3(PHR1REG)                       1010 02050000
*           STAIDWKA=''B;           /* CLEAR STATION ID WORK AREA    */ 02051000
         XC    STAIDWKA(8),STAIDWKA                                1011 02052000
*           GEN CODE;                                              1012 02053000
