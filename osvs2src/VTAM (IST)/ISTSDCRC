@RC00402 L     @14,12(,@13)                                        0451 00276000
         LM    @00,@12,20(@13)                                     0451 00277000
         BR    @14                                                 0451 00278000
*                                                                  0452 00279000
*/* SEGMENT(RDTADD)START                                             */ 00280000
*                                                                  0452 00281000
*   DO;                                                            0452 00282000
*     GOTO RDTADD@;                                                0453 00283000
*RDTADD:                                                           0454 00284000
*   END;                                                           0454 00285000
*                                                                  0454 00286000
RDTADD   DS    0H                                                  0455 00287000
*/********************************************************************/ 00288000
*/* PASS CONTROL TO MAIN SYSDEF PROCESSING WHICH IS IN ANOTHER TASK, */ 00289000
*/* NOT UNDER PSS. VPOST TO ISTSDCLM WHICH WILL CALL BACK AT RDTIO.  */ 00290000
*/* WE SAVE REGISTERS IN MLCSAV2 SO ISTSDCLM CAN GIVE THEM TO RDTIO. */ 00291000
*/* THIS TASK GOES INTO TPWAIT TO ALLOW OUR (AND EVERYONE ELSE'S)    */ 00292000
*/* PROCESSING TO GO FORWARD.                                        */ 00293000
*/********************************************************************/ 00294000
*                                                                  0455 00295000
*   LREG=ADDR(RDTIO);               /* SET RDTIO AS CALL ADDR FOR LM */ 00296000
         LA    LREG,RDTIO                                          0455 00297000
*   CONSDMLC=MLCAPTR;               /* LM NEEDS MLCA ADDR, SO PASS IT*/ 00298000
         ST    MLCAPTR,CONSDMLC(,CONFTPTR)                         0456 00299000
*   GEN SETS(MLCSAV2) REFS(REG11,REG8,MLCAPTR)                     0457 00300000
*       (STM   REG11,REG8,MLCSAV2(MLCAPTR));/* SAVE REGISTERS FOR LM */ 00301000
         STM   REG11,REG8,MLCSAV2(MLCAPTR)                              00302000
*   RFY                                                            0458 00303000
*     ISTRPH BASED(NCSPLRPH);       /*                       @ZM00680*/ 00304000
*   SAVREG=ADDR(RPHWORK);           /*                       @ZM00680*/ 00305000
         L     SAVREG,NCSPLRPH(,NCSPLPTR)                          0459 00306000
         LA    SAVREG,RPHWORK(,SAVREG)                             0459 00307000
*/*VSETLCK  (15,OBTAIN,LOCAL,(CONSDECB,ISTSDCRC(L2)))                */ 00308000
*                                                                  0460 00309000
*L1:                                                               0460 00310000
*   DO;                             /* DECLARE AND RESTRICT REGS   0460 00311000
*                                      2,10,11,12,13,14, AND 15      */ 00312000
L1       DS    0H                                                  0461 00313000
*     RFY                                                          0461 00314000
*      (GPR02P,                                                    0461 00315000
*       GPR10P,                                                    0461 00316000
*       GPR11P,                                                    0461 00317000
*       GPR12P,                                                    0461 00318000
*       GPR13P,                                                    0461 00319000
*       GPR14P,                                                    0461 00320000
*       GPR15P) RSTD;               /* SAVE REGS 2 AND 10 THRU 14    */ 00321000
*     GEN REFS(GPR02F,GPR10F,GPR11F,GPR12F,GPR14F,GPR13P);         0462 00322000
         STM   GPR10F,GPR12F,0(GPR13P)                                  00323000
