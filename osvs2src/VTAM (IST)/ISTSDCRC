         LR    RCSAVE,@14                                          0424 00187000
*       MLCNCSPL=NCSPLPTR;          /* SAVE ISTNCSPL ADDR FOR TPMSGS    00188000
*                                                            @D32CKQH*/ 00189000
         ST    NCSPLPTR,MLCNCSPL(,MLCAPTR)                         0425 00190000
*       IF NCSPLRPT=0               /* ADD OR DELETE AN RDT.         */ 00191000
*         THEN                      /* SEGMENT(RDTADD)REQUEST        */ 00192000
         C     @14,NCSPLRPT(,NCSPLPTR)                             0426 00193000
         BNE   @RF00426                                            0426 00194000
*         DO;                                                      0427 00195000
*           GOTO RDTADD;                                           0428 00196000
