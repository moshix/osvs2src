*                                      INVOKE OS/VS SETLOCK MACRO    */ 00331000
         LR    GPR10P,GPR13P                                       0464 00332000
*     GEN REFS(GPR13P,PSALITA,FLC) SETS(GPR12F);                   0465 00333000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        *00334000
               RELATED=(CONSDECB,ISTSDCRC(L2))                          00335000
*     GPR15P=GPR13P;                /* PUT RETURN CODE IN R 15 INVOKE   00336000
*                                      MODESET MACRO TO              */ 00337000
         LR    GPR15P,GPR13P                                       0466 00338000
*     DO;                           /* RESTORE PSW PROTECTION KEY    */ 00339000
*       GEN SETS(2);                                               0468 00340000
         MODESET KEYADDR=(2),WORKREG=2                                  00341000
*     END;                          /* RESTORE REGISTERS             */ 00342000
*     GEN REFS(GPR10F,GPR13P) SETS(GPR10F,GPR11F,GPR12F,GPR13F,GPR14F,  00343000
*         GPR02F);                                                 0470 00344000
