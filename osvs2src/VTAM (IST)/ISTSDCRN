*                 DCL                                              0506 00462000
*                   GPR15P PTR(31) REG(15);                        0506 00463000
*                 RFY                                              0507 00464000
*                  (GPR15P) RSTD;   /* PUT SUBPOOL PARM IN HIGH-   0507 00465000
*                                      ORDER BYTE OF REG 15          */ 00466000
*                 GPR15F=241;                                      0508 00467000
         LA    GPR15F,241                                          0508 00468000
*                 SLL(GPR15P,24);   /* COMBINE SUBPOOL AND SIZE IN 0509 00469000
