*                 RFY                                              0511 00474000
*                  (GPR15P) UNRSTD; /* DECLARE AND RESTRICT REG 1    */ 00475000
*                 DCL                                              0512 00476000
*                   GPR01F FIXED(31) REG(01);                      0512 00477000
*                 DCL                                              0513 00478000
*                   GPR01P PTR(31) REG(01);                        0513 00479000
*                 RFY                                              0514 00480000
*                  (GPR01P) RSTD;   /* INVOKE XREGSET MACRO TO CHECK    00481000
*                                      STORAGE PARM AND PUT ADDR IN     00482000
*                                      REGISTER 1                    */ 00483000
*/*XREGSET  PID('STORAGE')PPARM((CONRDBUF))RREG(1,GPR01P)            */ 00484000
*                                                                  0515 00485000
*                 DO;                                              0515 00486000
*                   GPR01P=CONRDBUF;/* SET RREG SYMBOL TO VALUE OF 0516 00487000
