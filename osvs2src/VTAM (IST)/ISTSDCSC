         TITLE 'ISTSDCSC   SYSCNTRL MACRO HNDLR                        *00001000
                        '                                               00002000
ISTSDCSC CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCSC  77.311'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*/********************************************************************/ 00014000
*/*                          START PROCESSING                        */ 00015000
*/********************************************************************/ 00016000
*                                                                  0173 00017000
*   RNCSAV1(2)=GPR13;               /* BACKWARD POINT                */ 00018000
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0173 00019000
*   GPR13=ADDR(RNCSAV1);            /* SET UP OUR SAVE REG           */ 00020000
         LA    GPR13,RNCSAV1(,RNCAREG)                             0174 00021000
*   MLCSAV3(3)=GPR13;               /* FORWARD POINT                 */ 00022000
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0175 00023000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */ 00024000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0176 00025000
*   GENERATE;                                                      0177 00026000
*                                                                  0177 00027000
           USING @DATD,11                 ADDRESSABILITY FOR DATA       00028000
*/********************************************************************/ 00029000
*/* WHILE THE RETURN CODE = 0, THIS CODE PREPARES THE PHR PARAMETERS */ 00030000
*/* AND CALLS PHR.  UPON RETURN FROM PHR, THE RETURN CODE AND THE @  */ 00031000
*/* OF THE NEXT MACRO KEYWORD ARE SAVED.                             */ 00032000
*/********************************************************************/ 00033000
*                                                                  0178 00034000
*   RFY                                                            0178 00035000
*    (GPR0,                                                        0178 00036000
*     GPR1,                                                        0178 00037000
*     GPR14,                                                       0178 00038000
*     GPR15) RSTD;                  /* RESTRICT REGS                 */ 00039000
*   RC=0;                                                          0179 00040000
         SLR   @10,@10                                             0179 00041000
         STH   @10,RC                                              0179 00042000
*   ENTRYPTR=MLCPTR0;               /*                       @XA19636*/ 00043000
         L     ENTRYPTR,MLCPTR0(,MLCAREG)                          0180 00044000
*   RFY                                                            0181 00045000
*     ISTRRN BASED(ENTRYPTR);                                      0181 00046000
*   GPR1=MLCKWD;                                                   0182 00047000
         L     GPR1,MLCKWD(,MLCAREG)                               0182 00048000
*   DO WHILE(RC=0);                                                0183 00049000
         B     @DE00183                                            0183 00050000
@DL00183 DS    0H                                                  0184 00051000
*     CALL DOWHILE;                                                0184 00052000
         BAL   @14,DOWHILE                                         0184 00053000
*DOLOOP:                                                           0185 00054000
*     ;                                                            0185 00055000
*                                                                  0185 00056000
DOLOOP   DS    0H                                                  0186 00057000
*/********************************************************************/ 00058000
*/* AT THIS POINT A RETURN CODE OF 0 MEANS CONTINUE THE DO WHILE     */ 00059000
*/* LOOP.  OTHERWISE, THE LOOP IS DISCONTINUED WITH A RETURN CODE OF */ 00060000
*/* 8 OR 4.                                                          */ 00061000
*/********************************************************************/ 00062000
*                                                                  0186 00063000
*   END;                                                           0186 00064000
*                                                                  0186 00065000
@DE00183 LH    @10,RC                                              0186 00066000
         SLR   @04,@04                                             0186 00067000
         CR    @10,@04                                             0186 00068000
         BE    @DL00183                                            0186 00069000
*/********************************************************************/ 00070000
*/* AT THIS POINT IF RC=8, INDICATING END OF STRING, WE RESET        */ 00071000
*/* THE RTN CODE TO 0 TO INFORM RNBUILD THAT NO ERRORS WERE FOUND    */ 00072000
*/* IF THE RETURN CODE WAS NOT 8 IT WILL BE 4. THIS WILL TELL        */ 00073000
*/* RNBUILD THAT ERRORS WERE FOUND AND TO KILL THE BUILD.            */ 00074000
*/********************************************************************/ 00075000
*                                                                  0187 00076000
*   IF RC=8 THEN                                                   0187 00077000
         C     @10,@CF00035                                        0187 00078000
         BNE   @RF00187                                            0187 00079000
*     RC=0;                                                        0188 00080000
         STH   @04,RC                                              0188 00081000
*   ELSE                                                           0189 00082000
*     ;                                                            0189 00083000
@RF00187 DS    0H                                                  0190 00084000
*   RNCGSYSC=ON;                    /* SET THIS MACROS FP            */ 00085000
         OI    RNCGSYSC(RNCAREG),B'00100000'                       0190 00086000
*   GPR13=RNCSAV1(2);               /* RESTORE CALLERS REGS          */ 00087000
         L     GPR13,RNCSAV1+4(,RNCAREG)                           0191 00088000
*/********************************************************************/ 00089000
*/* AT THIS POINT RC CONTAINS EITHER A 0 OR A 4 INDICATING A GOOD    */ 00090000
*/* RETURN CODE OR AN ERROR RETURN CODE WHICH WILL KILL THE BUILD.   */ 00091000
*/********************************************************************/ 00092000
*                                                                  0192 00093000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00094000
