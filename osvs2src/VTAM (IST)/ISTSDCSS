         TITLE 'ISTSDCSS - SYSTEM DEFINITION RDT SKELETON SEARCH       *00001000
                        '                                               00002000
ISTSDCSS CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCSS  78.206'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*   ;                                                              0358 00014000
*                                                                  0359 00015000
*/********************************************************************/ 00016000
*/* INITIALIZATION:  ESTABLISH THOSE CONTROL BLOCK ADDRESSABILITIES  */ 00017000
*/* AND REGISTER RESTRICTIONS WHICH WILL BE USED THROUGHOUT THE      */ 00018000
*/* MODULE.  ENVIRONMENTAL REGISTER 5 HAS MLCA ADDRESS, 3 (ATCVT)    */ 00019000
*/* & 4 (CONFT) ARE NOT NEEDED.  LINKAGE USES MLCSAV3/DAT3 FOR       */ 00020000
*/* SAVE/DATA AREAS.  INTERNAL RETURN CODE AND ADDRESS OF MESSAGE    */ 00021000
*/* ID ARE INITIALIZED TO ZERO.                                      */ 00022000
*/********************************************************************/ 00023000
*                                                                  0359 00024000
*   RFY                                                            0359 00025000
*    (MLCAPTR,                                                     0359 00026000
*     BASEREG) RSTD;                                               0359 00027000
*   RFY                                                            0360 00028000
*     RPHPTR UNRSTD;                                               0360 00029000
*   RFY                                                            0361 00030000
*     IHADCB BASED(ADRDCB);                                        0361 00031000
*   RFY                                                            0362 00032000
*     ISTMLCA BASED(MLCAPTR);                                      0362 00033000
*   RFY                                                            0363 00034000
*     ISTNCSPL BASED(MLCNCSPL);     /*                       @D32CKQH*/ 00035000
*   MLCSAV3(2)=BASEREG;                                            0364 00036000
         ST    BASEREG,MLCSAV3+4(,MLCAPTR)                         0364 00037000
*   SAVAREA(3)=ADDR(MLCSAV3);                                      0365 00038000
         LA    @04,MLCSAV3(,MLCAPTR)                               0365 00039000
         ST    @04,SAVAREA+8(,BASEREG)                             0365 00040000
*   BASEREG=ADDR(MLCSAV3);                                         0366 00041000
         LR    BASEREG,@04                                         0366 00042000
*   GEN REFS(MLCDAT3,ISTMLCA) NOSETS                               0367 00043000
*       (USING @DATD-(MLCDAT3-ISTMLCA),MLCAPTR    DATA ADDRESSABILITY); 00044000
         USING @DATD-(MLCDAT3-ISTMLCA),MLCAPTR    DATA ADDRESSABILITY   00045000
*   MSGIDADR=0;                                                    0368 00046000
         SLR   @04,@04                                             0368 00047000
         ST    @04,MSGIDADR                                        0368 00048000
*   SSRC=0;                                                        0369 00049000
*                                                                  0369 00050000
         ST    @04,SSRC                                            0369 00051000
*/********************************************************************/ 00052000
*/* PREPARE TO READ THE SKELETON:  A MODEL VTAMOBJ DCB IS IN THE     */ 00053000
*/* STATIC DATA AREA.  IT IS COPIED TO MLCDCB, ADDRESSED VIA ADRDCB, */ 00054000
*/* AND OPENED (VOPEN).  A FIND IS DONE USING THE MEMBER NAME IN     */ 00055000
*/* MLCCONFG. A RETURN OF 4 FROM FIND MEANS THE MEMBER WAS NOT FOUND,*/ 00056000
*/* AND THERE IS NO ERROR CONDITION.  ANY OTHER NON-ZERO RETURN      */ 00057000
*/* FROM VOPEN OR FIND IS AN ERROR, SO A MESSAGE IS ISSUED.  ANY     */ 00058000
*/* NON-ZERO RETURN FROM VOPEN/FIND MAKES THE INTERNAL CODE 4.       */ 00059000
*/********************************************************************/ 00060000
*                                                                  0370 00061000
*   RFY                                                            0370 00062000
*     ADRDCB RSTD;                                                 0370 00063000
*   ADRDCB=ADDR(MLCDCB);                                           0371 00064000
         LA    ADRDCB,MLCDCB(,MLCAPTR)                             0371 00065000
*   IHADCB(1:(ADDR(MODELEND)-ADDR(MODELDCB)))=MODELDCB(1:(ADDR(MODELEND 00066000
*       )-ADDR(MODELDCB)));                                        0372 00067000
         LA    @15,MODELEND                                        0372 00068000
         LA    @14,MODELDCB                                        0372 00069000
         SLR   @15,@14                                             0372 00070000
         BCTR  @15,0                                               0372 00071000
         EX    @15,@SM03317                                        0372 00072000
*   RFY                                                            0373 00073000
*     RC RSTD;                                                     0373 00074000
*                                                                  0374 00075000
*/*VTOPEN FILE((ADRDCB))                                             */ 00076000
*                                                                  0374 00077000
*   DO;                             /* DECLARE AND RESTRICT REG 2    */ 00078000
*     DCL                                                          0375 00079000
*       GPR02F FIXED(31) REG(02);                                  0375 00080000
*     DCL                                                          0376 00081000
*       GPR02P PTR(31) REG(02);                                    0376 00082000
*     RFY                                                          0377 00083000
*      (GPR02P) RSTD;               /* INVOKE XREGSET MACRO TO VERIFY   00084000
*                                      FILE OPERAND AND PUT IT IN  0377 00085000
*                                      REGISTER 2                    */ 00086000
*/*XREGSET  PID('FILE')PPARM((ADRDCB))RREG(2,GPR02P)                 */ 00087000
*                                                                  0378 00088000
*     DO;                                                          0378 00089000
*       GPR02P=ADRDCB;              /* SET RREG SYMBOL TO VALUE OF 0379 00090000
*                                      PPARM OPERAND                 */ 00091000
         LR    GPR02P,ADRDCB                                       0379 00092000
*     END;                          /* XREGSET                       */ 00093000
*     GEN REFS(GPR02P);                                            0381 00094000
         OPEN   ((2))                                                   00095000
*                                   /* RESTRICT REGISTER 15          */ 00096000
*     DCL                                                          0382 00097000
*       GPR15F FIXED(31) REG(15);                                  0382 00098000
*     DCL                                                          0383 00099000
*       GPR15P PTR(31) REG(15);                                    0383 00100000
*     RFY                                                          0384 00101000
*      (GPR15P) RSTD;                                              0384 00102000
*     GPR02F=GPR02F+40;             /* ADD OFFSET OF 40 TO DCBPTR TO    00103000
*                                      POINT TO IHADCS24 WHICH IS  0385 00104000
*                                      BASE FOR DCBOFLGS             */ 00105000
         AL    GPR02F,@CF00124                                     0385 00106000
*     IF GPR02P->DCBOFOPN='1'B THEN /* IF OPEN WAS SUCCESSFUL        */ 00107000
         TM    DCBOFOPN(GPR02P),B'00010000'                        0386 00108000
         BNO   @RF00386                                            0386 00109000
*       GPR15F=0;                   /* PUT 0 IN RETURN CODE REG      */ 00110000
         SLR   GPR15F,GPR15F                                       0387 00111000
*     ELSE                          /* IF OPEN WAS UNSUCCESSFUL      */ 00112000
*       GPR15F=4;                   /* PUT 4 IN RETURN CODE REG    0388 00113000
*                                      UNRESTRICT REG 15             */ 00114000
         B     @RC00386                                            0388 00115000
@RF00386 LA    GPR15F,4                                            0388 00116000
*     RFY                                                          0389 00117000
*      (GPR15P) UNRSTD;             /* UNRESTRICT REG 2              */ 00118000
@RC00386 DS    0H                                                  0390 00119000
*     RFY                                                          0390 00120000
*      (GPR02P) UNRSTD;                                            0390 00121000
*   END;                            /* VTOPEN OPEN SYS1.VTAMOBJ    0391 00122000
*                                                            @ZA29748*/ 00123000
*   IF RC^=0 THEN                                                  0392 00124000
         LTR   RC,RC                                               0392 00125000
         BZ    @RF00392                                            0392 00126000
*     DO;                                                          0393 00127000
*       MSGIDADR=ADDR('304I');                                     0394 00128000
         LA    @14,@CC02765                                        0394 00129000
         ST    @14,MSGIDADR                                        0394 00130000
*       SSRC=4;                                                    0395 00131000
         MVC   SSRC(4),@CF00040                                    0395 00132000
*     END;                                                         0396 00133000
*   ELSE                                                           0397 00134000
*                                                                  0397 00135000
*/********************************************************************/ 00136000
*/* OPEN SUCCESSFUL:                                                 */ 00137000
*/********************************************************************/ 00138000
*                                                                  0397 00139000
*     DO;                                                          0397 00140000
         B     @RC00392                                            0397 00141000
@RF00392 DS    0H                                                  0398 00142000
*       GEN REFS(ADRDCB,MLCCONFG) SETS(MLCDCB,RC,R0,R1,R14)        0398 00143000
*           (FIND (ADRDCB),MLCCONFG(,MLCAPTR),D);                  0398 00144000
         FIND (ADRDCB),MLCCONFG(,MLCAPTR),D                             00145000
*       IF RC^=0 THEN                                              0399 00146000
         LTR   RC,RC                                               0399 00147000
         BZ    @RF00399                                            0399 00148000
*         DO;                                                      0400 00149000
*           IF RC=4 THEN                                           0401 00150000
         C     RC,@CF00040                                         0401 00151000
         BE    @RT00401                                            0401 00152000
*             ;                                                    0402 00153000
*           ELSE                                                   0403 00154000
*             DO;                                                  0403 00155000
*               RFY                                                0404 00156000
*                 RC UNRSTD;                                       0404 00157000
*               MSGIDADR=ADDR('305I');                             0405 00158000
         LA    @15,@CC02766                                        0405 00159000
         ST    @15,MSGIDADR                                        0405 00160000
*             END;                                                 0406 00161000
*           SSRC=4;                                                0407 00162000
@RT00401 MVC   SSRC(4),@CF00040                                    0407 00163000
*         END;                                                     0408 00164000
*                                                                  0408 00165000
*/********************************************************************/ 00166000
*/* FINISH UP:  SEGMENT RDSKEL READS IN THE SKELETON.  WHETHER THE   */ 00167000
*/* FIND WAS SUCCESSFUL OR NOT, THE DCB MUST BE CLOSED (VCLOSE).     */ 00168000
*/* BEFORE EXIT, IF THE MESSAGE ID ADDRESS IS NON-ZERO, THE MESSAGE  */ 00169000
*/* IS ISSUED. IF THE SKELETON HAS BEEN READ (INTERNAL CODE 0),      */ 00170000
*/* ITS ADDRESS IS IN MLCSKEL. OTHERWISE MLCSKEL IS ZEROED.          */ 00171000
*/* THE INTERNAL CODE BECOMES THE EXIT RETURN CODE.                  */ 00172000
*/********************************************************************/ 00173000
*                                                                  0409 00174000
*       ELSE                        /* SEGMENT(RDSKEL)REQUEST        */ 00175000
*         DO;                                                      0409 00176000
         B     @RC00399                                            0409 00177000
@RF00399 DS    0H                                                  0410 00178000
*           GOTO RDSKEL;                                           0410 00179000
         B     RDSKEL                                              0410 00180000
*RDSKEL#:                                                          0411 00181000
*         END;                                                     0411 00182000
RDSKEL#  DS    0H                                                  0412 00183000
*                                                                  0412 00184000
*/*VTCLOSE FILE((ADRDCB))                                            */ 00185000
*                                                                  0412 00186000
*       DO;                         /* DECLARE AND RESTRICT REG 2    */ 00187000
@RC00399 DS    0H                                                  0413 00188000
*         RFY                                                      0413 00189000
*          (GPR02P) RSTD;           /* INVOKE XREGSET MACRO TO VERIFY   00190000
*                                      FILE OPERAND AND PUT IT IN  0413 00191000
*                                      REGISTER 2                    */ 00192000
*/*XREGSET  PID('FILE')PPARM((ADRDCB))RREG(2,GPR02P)                 */ 00193000
*                                                                  0414 00194000
*         DO;                                                      0414 00195000
*           GPR02P=ADRDCB;          /* SET RREG SYMBOL TO VALUE OF 0415 00196000
*                                      PPARM OPERAND                 */ 00197000
         LR    GPR02P,ADRDCB                                       0415 00198000
*         END;                      /* XREGSET                       */ 00199000
*         GEN REFS(GPR02P);                                        0417 00200000
         CLOSE  ((2))                                                   00201000
*                                   /* UNRESTRICT REGISTER 2         */ 00202000
*         RFY                                                      0418 00203000
*          (GPR02P) UNRSTD;                                        0418 00204000
*       END;                        /* VTCLOSE CLOSE SYS1.VTAMOBJ  0419 00205000
*                                                            @ZA29748*/ 00206000
*/********************************************************************/ 00207000
*/* END OF OPEN SUCCESSFUL:                                          */ 00208000
*/********************************************************************/ 00209000
*                                                                  0420 00210000
*     END;                                                         0420 00211000
*   IF MSGIDADR^=0 THEN                                            0421 00212000
*                                                                  0421 00213000
@RC00392 L     @15,MSGIDADR                                        0421 00214000
         SLR   @14,@14                                             0421 00215000
         CR    @15,@14                                             0421 00216000
         BE    @RF00421                                            0421 00217000
*/*TPMSG MSGID((MSGIDADR))MSG((ADDR(MLCCONFG)))PLAREA(MLCTPMSG)OPTIONS( 00218000
*CSPLSID)                                                            */ 00219000
*                                                                  0422 00220000
*     DO;                                                          0422 00221000
*       DCL                                                        0423 00222000
*         REG1015 REG(1) PTR;                                      0423 00223000
*       RFY                                                        0424 00224000
*        (REG1015) RSTD;                                           0424 00225000
*       RFY                                                        0425 00226000
*        (EMCR13) RSTD;                                            0425 00227000
*       DCL                                                        0426 00228000
*         1 * BASED(REG1015),                                      0426 00229000
*          2 CMZID015 PTR(31),                                     0426 00230000
*          2 *,                                                    0426 00231000
*           3 CMZRL015 PTR(8),                                     0426 00232000
*           3 CMZRA015 PTR(24) BDY(BYTE),                          0426 00233000
*          2 CMZEA015 PTR(31),                                     0426 00234000
*          2 CMZVP015 PTR(31);                                     0426 00235000
*                                                                  0426 00236000
*/*XREGSET  PID('PLAREA')PPARM(MLCTPMSG)RREG(,REG1015)               */ 00237000
*                                                                  0427 00238000
*       DO;                                                        0427 00239000
*         REG1015=ADDR(MLCTPMSG);   /* SET RREG SYMBOL TO ADDRESS OF    00240000
*                                      PPARM OPERAND                 */ 00241000
         LA    REG1015,MLCTPMSG(,MLCAPTR)                          0428 00242000
*       END;                        /* XREGSET                       */ 00243000
*       CMZID015=0;                                                0430 00244000
         ST    @14,CMZID015(,REG1015)                              0430 00245000
*       CMZRL015=0;                                                0431 00246000
         MVI   CMZRL015(REG1015),X'00'                             0431 00247000
*       CMZRA015=0;                                                0432 00248000
         STCM  @14,7,CMZRA015(REG1015)                             0432 00249000
*       CMZEA015=0;                                                0433 00250000
         ST    @14,CMZEA015(,REG1015)                              0433 00251000
*       CMZVP015=0;                                                0434 00252000
         ST    @14,CMZVP015(,REG1015)                              0434 00253000
*       CMZID015=MSGIDADR;                                         0435 00254000
         ST    @15,CMZID015(,REG1015)                              0435 00255000
*       CMZVP015=ADDR(MLCCONFG);                                   0436 00256000
         LA    @00,MLCCONFG(,MLCAPTR)                              0436 00257000
         ST    @00,CMZVP015(,REG1015)                              0436 00258000
*       DCL                                                        0437 00259000
*         CMZSA015(18) PTR(31) BASED(EMCR13);                      0437 00260000
*       CMZSA015(5)=ADDR(NCSPLSID);                                0438 00261000
         L     @15,MLCNCSPL(,MLCAPTR)                              0438 00262000
         LA    @00,NCSPLSID(,@15)                                  0438 00263000
         ST    @00,CMZSA015+16(,EMCR13)                            0438 00264000
*       DCL                                                        0439 00265000
*         REG15015 REG(15) PTR;                                    0439 00266000
*       RFY                                                        0440 00267000
*         REG15015 RSTD;                                           0440 00268000
*       DO;                         /* LISATCVT                    0441 00269000
*                                      CVTBASE(PAGEABLE,(REG15015))  */ 00270000
*         RFY                                                      0442 00271000
*           ISTPAB;                                                0442 00272000
*         RFY                                                      0443 00273000
*           ISTDYPAB;                                              0443 00274000
*         RFY                                                      0444 00275000
*           ISTATCVT;                                              0444 00276000
*         REG15015=ATCLCPTR;        /* SET UP ATCVT POINTER          */ 00277000
         L     REG15015,ATCLCPTR                                   0445 00278000
*       END;                        /* LISATCVT                      */ 00279000
*       DCL                                                        0447 00280000
*         CMTA015 BASED(REG15015) ENTRY VALUERANGE(*);             0447 00281000
*       REG15015=REG15015->ATCMSGP;                                0448 00282000
         L     REG15015,ATCMSGP(,REG15015)                         0448 00283000
*       CALL CMTA015;                                              0449 00284000
         BALR  @14,@15                                             0449 00285000
*       RESPECIFY                                                  0450 00286000
*        (REG1015,                                                 0450 00287000
*         REG15015) UNRESTRICTED;                                  0450 00288000
*       RFY                                                        0451 00289000
*        (EMCR13) UNRSTD;                                          0451 00290000
*     END;                          /*                       @D32CKQH*/ 00291000
*   IF SSRC^=0 THEN                                                0453 00292000
@RF00421 SLR   @15,@15                                             0453 00293000
         C     @15,SSRC                                            0453 00294000
         BE    @RF00453                                            0453 00295000
*     MLCSKEL=0;                                                   0454 00296000
         ST    @15,MLCSKEL(,MLCAPTR)                               0454 00297000
*   BASEREG=MLCSAV3(2);                                            0455 00298000
@RF00453 L     BASEREG,MLCSAV3+4(,MLCAPTR)                         0455 00299000
*   RETURN CODE(SSRC);                                             0456 00300000
         L     @15,SSRC                                            0456 00301000
         L     @14,12(,@13)                                        0456 00302000
         LM    @00,@12,20(@13)                                     0456 00303000
         BR    @14                                                 0456 00304000
*                                                                  0457 00305000
*/********************************************************************/ 00306000
*/*                           RDSKEL SEGMENT                         */ 00307000
*/*                                                                  */ 00308000
*/* RDSKEL USES TWO BUFFERS TO READ THE SKELETON.  GETSTOR SUPPLIES  */ 00309000
*/* BOTH.  THE FIRST IS THE SIZE OF ONE DATA BLOCK (DCBBLKSI) AND    */ 00310000
*/* IS USED TO READ THE BEGINNING OF THE RDT WHICH CONTAINS THE      */ 00311000
*/* TOTAL RDT LENGTH.  THE SECOND BUFFER, IF NEEDED, FOR THE         */ 00312000
*/* ENTIRE RDT, IS A MULTIPLE OF THE BLOCKSIZE.  THE RDT ADDRESS     */ 00313000
*/* IS PLACED IN MLCSKEL FOR POSTERITY.  ALL EXCESS BUFFER SPACE     */ 00314000
*/* IS RELEASED.                                                     */ 00315000
*/********************************************************************/ 00316000
*/* SEGMENT(RDSKEL)START                                             */ 00317000
*                                                                  0457 00318000
*   DO;                                                            0457 00319000
*     GOTO RDSKEL@;                                                0458 00320000
*RDSKEL:                                                           0459 00321000
*   END;                                                           0459 00322000
RDSKEL   DS    0H                                                  0460 00323000
*   RFY                                                            0460 00324000
*    (RPHPTR,                                                      0460 00325000
*     RC) RSTD;                                                    0460 00326000
*   RPHPTR=MLCRPH;                  /* OBTAIN THE FIRST BUFFER       */ 00327000
*                                                                  0461 00328000
         L     RPHPTR,MLCRPH(,MLCAPTR)                             0461 00329000
*   /*****************************************************************/ 00330000
*   /*                                                               */ 00331000
*   /* GETSTOR SIZE(DCBBLKSI)                                        */ 00332000
*   /*                                                               */ 00333000
*   /*****************************************************************/ 00334000
*                                                                  0462 00335000
*   DO;                                                            0462 00336000
*                                                                  0462 00337000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)                        */ 00338000
*                                                                  0463 00339000
*/* PROCESS ANCHOR BLOCK                                              * 00340000
*/*  REVISIONS                                         002 11/16/75  */ 00341000
*/*       APAR      OZ19540    DCL PABSSN ABNORMAL           @ZA19540*/ 00342000
*DCL  1  ISTPAB BDY(DWORD) BASED,                                  0463 00343000
*       2  PABWQCHN CHAR(8) BDY(DWORD), /* CPS SWAP FIELD FOR NEQ&CHN * 00344000
*        3  PABWEQA PTR(31) BDY(WORD), /* WORK ELEMENT QUEUE ADDRESS  * 00345000
*           4  PABWEQG BIT(1),         /* GATING BIT                  * 00346000
*           4  PABRSV01 BIT(7),        /* RESERVED                    * 00347000
*           4  PABWEQP PTR(24),        /* WKEL PTR                    * 00348000
*        3  PABCHAIN PTR(31) BDY(WORD), /* APS CHAIN                  * 00349000
*           4  PABCHNG BIT(1),         /* GATING BIT                  * 00350000
*           4  PABRESCH BIT(1),        /* RESCHEDULE PAB AT        0463 00351000
*                                         TPEXIT                      * 00352000
*           4  PABCDP BIT(1),          /* CLOSE DST/CHANGE IN PROGRESS* 00353000
*           4  PABUNCON BIT(1),        /* UNCONDITIONALLY RESCHEDULE    00354000
*                                         PAB AT TPEXIT               * 00355000
*           4  PABRESET BIT(1),        /* RESET ISSUED ON THIS PAB    * 00356000
*           4  PABRUNNG BIT(1),        /* PAB RUNNING                 * 00357000
*           4  PABDELAY BIT(1),        /* DELAY OPTION USED           * 00358000
*           4  PABRSV02 BIT(1),        /* RESERVED                    * 00359000
*           4  PABCHNGP PTR(24),       /* PSS CHAIN PTR               * 00360000
*        2  PABOFFST PTR(8),           /* OFFSET FROM CONTROL BLOCK   * 00361000
*        2  PABDVTA PTR(24),           /* DVT ADDRESS                 * 00362000
*        2  PABRPHFG PTR(31),          /* SCHED FLAGS AND RPH         * 00363000
*           3  PABFLAGS BIT(8),        /* SCHEDULING FLAGS            * 00364000
*              4  PABAPYP BIT(1),      /* APPLICATION IS A USER EXIT  * 00365000
*              4  PABPRIOR BIT(1),     /* IMMEDIATE PRIORITY          * 00366000
*              4  PABNORPR BIT(1),     /* NORMAL PRIORITY -           * 00367000
*                                      /* RESCHEDUALABLE              * 00368000
*              4  PABDYNAM BIT(1),     /* DYNAMIC                     * 00369000
*              4  PABSSN BIT(1) ABNL,  /* SYSTEM SERVICES    @ZA19540 * 00370000
*              4  PABERLCK BIT(1),     /* PAB ERROR LOCK FLAG         * 00371000
*                 5  PABERRLK BIT(1),  /* ERROR LOCK                  * 00372000
*                    6  PABERLOK BIT(1), /* ERROR LOCK                * 00373000
*/*    ABNORMAL TO ALLOW VARY COMPARE AND SWAP                        * 00374000
*              4  PABINHBT BIT(1) ABNL,/* 1=INHIBIT TRAFFIC,0=NO      * 00375000
*              4  PABNODQ BIT(1),      /* DO NOT DEQ WORK ELEMENT     * 00376000
*           3  PABRPHA PTR(24);        /* RPL HEADER ADDRESS          * 00377000
*DCL PABWEQG0 BIT(32) CONSTANT('7FFFFFFF'X),  /* TURN PABWEQG OFF     * 00378000
*    PABWEQG1 BIT(32) CONSTANT('80000000'X),  /* TURN PABWEQG ON      * 00379000
*    PABCHNG0 BIT(32) CONSTANT('7FFFFFFF'X),  /* TURN PABCHNG OFF     * 00380000
*    PABCHNG1 BIT(32) CONSTANT('80000000'X),  /* TURN PABCHNG ON      * 00381000
*    PABRESC0 BIT(32) CONSTANT('BFFFFFFF'X),  /* TURN PABRESCH OFF    * 00382000
*    PABRESC1 BIT(32) CONSTANT('40000000'X),  /* TURN PABRESCH ON     * 00383000
*    PABCDP0  BIT(32) CONSTANT ('DFFFFFFF'X), /* TURN PABCDP OFF      * 00384000
*    PABCDP1  BIT(32) CONSTANT ('20000000'X), /* TURN PABCDP ON       * 00385000
*    PABERLK0 BIT(32) CONSTANT('FBFFFFFF'X),  /* TURN PABERRLK ON     * 00386000
*    PABERLK1 BIT(32) CONSTANT('04000000'X),  /* TURN PABERRLK OFF    * 00387000
*    PABUNCO0 BIT(32) CONSTANT('EFFFFFFF'X),  /* TURN PABUNCON OFF    * 00388000
*    PABUNCO1 BIT(32) CONSTANT('10000000'X),  /* TURN PABUNCON ON     * 00389000
*    PABPRIR0 BIT(32) CONSTANT('BFFFFFFF'X),  /* TURN PAB PRIOR OFF   * 00390000
*    PABPRIR1 BIT(32) CONSTANT('40000000'X),  /* TURN PAB PRIOR ON    * 00391000
*    PABRST0  BIT(32) CONSTANT('F7FFFFFF'X),  /* TURN PABRESET OFF    * 00392000
*    PABRST1  BIT(32) CONSTANT('08000000'X),  /* TURN PABRESET ON     * 00393000
