*                   1 CMZA0043 FIXED(31) STATIC LOCAL,             0534 01094000
*                    2 * FIXED(8) INIT(64),/* OPTION FLAGS           */ 01095000
*                    2 * FIXED(8) INIT(96),/* STORAGE KEY            */ 01096000
*                    2 * FIXED(8) INIT(231),/* SUBPOOL NUMBER        */ 01097000
*                    2 * FIXED(8) INIT('00'X);/* REQUEST TYPE        */ 01098000
*                 GPR03F=CMZA0043;                                 0535 01099000
         L     GPR03F,CMZA0043                                     0535 01100000
*                 DO;               /* LISATCVT                    0536 01101000
*                                      CVTBASE(PAGEABLE,(GPR02P))    */ 01102000
*                   GPR02P=ATCLCPTR;/* SET UP ATCVT POINTER          */ 01103000
         L     GPR02P,ATCLCPTR                                     0537 01104000
*                 END;              /* LISATCVT                      */ 01105000
*                 CALL GPR02P->ATCGTSTR;/* CALL ISTORFSR             */ 01106000
         L     @15,ATCGTSTR(,GPR02P)                               0539 01107000
         BALR  @14,@15                                             0539 01108000
*                 RFY                                              0540 01109000
*                  (GPR00P,                                        0540 01110000
*                   GPR01P,                                        0540 01111000
*                   GPR02P,                                        0540 01112000
*                   GPR03P) UNRSTD;                                0540 01113000
*               END;                /* END OF GETSTOR                */ 01114000
*                                                                  0541 01115000
*               /*****************************************************/ 01116000
*               /*                                                   */ 01117000
*               /*                                           @ZA33078*/ 01118000
*               /*                                                   */ 01119000
*               /*****************************************************/ 01120000
*                                                                  0542 01121000
*               IF RC^=0 THEN       /* UNSUCCESSFUL GET      @ZA33078*/ 01122000
         LTR   RC,RC                                               0542 01123000
         BZ    @RF00542                                            0542 01124000
*                 SSRC=8;           /*                       @ZA33078*/ 01125000
         MVC   SSRC(4),@CF00042                                    0543 01126000
*               ELSE                /* GOOD GET              @ZA33078*/ 01127000
*                 DO;               /*                       @ZA33078*/ 01128000
