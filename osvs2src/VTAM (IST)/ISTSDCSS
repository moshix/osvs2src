*                   /*************************************************/ 01437000
*                   /*                                               */ 01438000
*                   /* FREESTOR SIZE(RLEN1)AREA(NEXTBUF)             */ 01439000
*                   /*                                               */ 01440000
*                   /*************************************************/ 01441000
*                                                                  0623 01442000
*                   DO;                                            0623 01443000
*                                                                  0623 01444000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)                        */ 01445000
*/* END OF ATBUILD                                                   */ 01446000
*                                                                  0624 01447000
*                     RFY                                          0624 01448000
*                      (GPR00P,                                    0624 01449000
*                       GPR01P,                                    0624 01450000
*                       GPR02P,                                    0624 01451000
*                       GPR03P) RSTD;                              0624 01452000
*                     RFY                                          0625 01453000
*                       ISTRPH BASED(GPR01P);                      0625 01454000
*                     GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,GPR06P, 0626 01455000
*                         GPR07P,GPR08P,GPR09P,GPR10P,GPR11P,GPR12P,    01456000
*                         GPR13P) SETS(RPHWORK);                   0626 01457000
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS      01458000
*                     GPR00F=RLEN1; /* SIZE IN REG0                  */ 01459000
         L     GPR00F,RLEN1                                        0627 01460000
*                     DCL                                          0628 01461000
*                       1 CMZA0098 FIXED(31) STATIC LOCAL,         0628 01462000
*                        2 * FIXED(8) INIT(64),/* OPTION FLAGS       */ 01463000
*                        2 * FIXED(8) INIT(96),/* STORAGE KEY        */ 01464000
*                        2 * FIXED(8) INIT(231),/* SUBPOOL NUMBER    */ 01465000
*                        2 * FIXED(8) INIT('01'X);/* REQUEST TYPE    */ 01466000
*                     GPR03F=CMZA0098;                             0629 01467000
         L     GPR03F,CMZA0098                                     0629 01468000
*                     RFY                                          0630 01469000
*                      (GPR04P) RSTD;                              0630 01470000
*                     GPR04P=NEXTBUF;/* PUT FREEMAIN ADDR IN REG4    */ 01471000
         LR    GPR04P,NEXTBUF                                      0631 01472000
*                     DO;           /* LISATCVT                    0632 01473000
*                                      CVTBASE(PAGEABLE,(GPR02P))    */ 01474000
*                       GPR02P=ATCLCPTR;/* SET UP ATCVT POINTER      */ 01475000
         L     GPR02P,ATCLCPTR                                     0633 01476000
*                     END;          /* LISATCVT                      */ 01477000
*                     CALL GPR02P->ATCGTSTR;/* CALL ISTORFSR         */ 01478000
         L     @15,ATCGTSTR(,GPR02P)                               0635 01479000
         BALR  @14,@15                                             0635 01480000
*                     RFY                                          0636 01481000
*                      (GPR00P,                                    0636 01482000
*                       GPR01P,                                    0636 01483000
*                       GPR02P,                                    0636 01484000
*                       GPR03P,                                    0636 01485000
*                       GPR04P) UNRSTD;                            0636 01486000
*                   END;            /* END OF FREESTOR               */ 01487000
*                                                                  0637 01488000
*                   /*************************************************/ 01489000
*                   /*                                               */ 01490000
*                   /*                                       @ZA33078*/ 01491000
*                   /*                                               */ 01492000
*                   /*************************************************/ 01493000
*                                                                  0638 01494000
*/********************************************************************/ 01495000
*/* END OF SECOND GETSTOR SUCCESSFUL:                                */ 01496000
*/********************************************************************/ 01497000
*                                                                  0638 01498000
*                 END;                                             0638 01499000
*                                                                  0638 01500000
*/********************************************************************/ 01501000
*/* END OF LONG SKELETON:                                            */ 01502000
*/********************************************************************/ 01503000
*                                                                  0639 01504000
*             END;                                                 0639 01505000
*                                                                  0639 01506000
@RC00572 DS    0H                                                  0640 01507000
*/********************************************************************/ 01508000
*/* END OF FIRST READ/CHECK SUCCESSFUL:                              */ 01509000
*/********************************************************************/ 01510000
*                                                                  0640 01511000
*         END;                                                     0640 01512000
@RC00526 DS    0H                                                  0641 01513000
*       RPHPTR=MLCRPH;              /* FREE THE 1ST BLOCK READ BUFFER*/ 01514000
*                                                                  0641 01515000
@RT00522 L     RPHPTR,MLCRPH(,MLCAPTR)                             0641 01516000
*       /*************************************************************/ 01517000
*       /*                                                           */ 01518000
*       /* FREESTOR SIZE(DCBBLKSI)AREA(RAREA)                        */ 01519000
*       /*                                                           */ 01520000
*       /*************************************************************/ 01521000
*                                                                  0642 01522000
*       DO;                                                        0642 01523000
*                                                                  0642 01524000
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT,ISTRPH)                        */ 01525000
*/* END OF ATBUILD                                                   */ 01526000
*                                                                  0643 01527000
*         RFY                                                      0643 01528000
*          (GPR00P,                                                0643 01529000
*           GPR01P,                                                0643 01530000
*           GPR02P,                                                0643 01531000
*           GPR03P) RSTD;                                          0643 01532000
*         RFY                                                      0644 01533000
*           ISTRPH BASED(GPR01P);                                  0644 01534000
*         GEN REFS(GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,    01535000
*             GPR09P,GPR10P,GPR11P,GPR12P,GPR13P) SETS(RPHWORK);   0645 01536000
         STM   GPR02P,GPR13P,RPHWORK(GPR01P)        SAVE REGISTERS      01537000
*         GPR00F=DCBBLKSI;          /* SIZE IN REG0                  */ 01538000
         LH    GPR00F,DCBBLKSI+52(,ADRDCB)                         0646 01539000
*         DCL                                                      0647 01540000
*           1 CMZA0117 FIXED(31) STATIC LOCAL,                     0647 01541000
*            2 * FIXED(8) INIT(64), /* OPTION FLAGS                  */ 01542000
*            2 * FIXED(8) INIT(96), /* STORAGE KEY                   */ 01543000
*            2 * FIXED(8) INIT(231),/* SUBPOOL NUMBER                */ 01544000
*            2 * FIXED(8) INIT('01'X);/* REQUEST TYPE                */ 01545000
*         GPR03F=CMZA0117;                                         0648 01546000
         L     GPR03F,CMZA0117                                     0648 01547000
*         RFY                                                      0649 01548000
*          (GPR04P) RSTD;                                          0649 01549000
*         GPR04P=RAREA;             /* PUT FREEMAIN ADDR IN REG4     */ 01550000
         LR    GPR04P,RAREA                                        0650 01551000
*         DO;                       /* LISATCVT                    0651 01552000
*                                      CVTBASE(PAGEABLE,(GPR02P))    */ 01553000
*           GPR02P=ATCLCPTR;        /* SET UP ATCVT POINTER          */ 01554000
         L     GPR02P,ATCLCPTR                                     0652 01555000
*         END;                      /* LISATCVT                      */ 01556000
*         CALL GPR02P->ATCGTSTR;    /* CALL ISTORFSR                 */ 01557000
         L     @15,ATCGTSTR(,GPR02P)                               0654 01558000
         BALR  @14,@15                                             0654 01559000
*         RFY                                                      0655 01560000
*          (GPR00P,                                                0655 01561000
*           GPR01P,                                                0655 01562000
*           GPR02P,                                                0655 01563000
*           GPR03P,                                                0655 01564000
*           GPR04P) UNRSTD;                                        0655 01565000
*       END;                        /* END OF FREESTOR               */ 01566000
*                                                                  0656 01567000
*       /*************************************************************/ 01568000
*       /*                                                           */ 01569000
*       /*                                                   @ZA33078*/ 01570000
*       /*                                                           */ 01571000
*       /*************************************************************/ 01572000
*                                                                  0657 01573000
*/********************************************************************/ 01574000
*/* END OF FIRST GETSTOR SUCCESSFUL:                                 */ 01575000
*/********************************************************************/ 01576000
*                                                                  0657 01577000
*     END;                                                         0657 01578000
*   RFY                                                            0658 01579000
*    (RPHPTR,                                                      0658 01580000
*     RC) UNRSTD;                                                  0658 01581000
*                                                                  0658 01582000
@RC00509 DS    0H                                                  0659 01583000
*/* SEGMENT(RDSKEL)FINISH                                            */ 01584000
*                                                                  0659 01585000
*   DO;                                                            0659 01586000
*     GOTO RDSKEL#;                                                0660 01587000
         B     RDSKEL#                                             0660 01588000
*RDSKEL@:                                                          0661 01589000
*   END;                                                           0661 01590000
RDSKEL@  DS    0H                                                  0662 01591000
*                                                                  0662 01592000
*/********************************************************************/ 01593000
*/*                           RDBDY SEGMENT                          */ 01594000
*/*                                                                  */ 01595000
*/* WHEN RDBDY IS CALLED, THE FIRST PHYSICAL DATA BLOCK IS ALREADY   */ 01596000
*/* IN THE SKELETON BUFFER, AND THESE POINTERS EXIST:                */ 01597000
*/*    NEXTBUF TO AREA IN SKELETON FOR NEXT READ,                    */ 01598000
*/*    RDDECB AND CKDECB TO TWO DECB'S,                              */ 01599000
*/*    SKELAD TO BEGINNING OF SKELETON.                              */ 01600000
*/* ONE RECORD IS READ. THEN REMAINING READS (IF ANY) ARE DONE IN A  */ 01601000
*/* LOOP WHICH OVERLAPS THE I/O.  WHETHER THE LOOP IS EXECUTED OR    */ 01602000
*/* NOT, ONE READ REMAINS AFTERWARD TO BE CHECKED.  AFTER THAT,      */ 01603000
*/* INPUT IS SET UP FOR RDSKEL'S FIRST FREESTOR - TO FREE THE EXTRA  */ 01604000
*/* SPACE IN THE SKELETON BUFFER.  IF AN ERROR HAS CAUSED THE        */ 01605000
*/* EODAD/SYNAD ROUTINE TO SET THE INTERNAL CODE TO 4, THE ENTIRE    */ 01606000
*/* SKELETON BUFFER MUST BE FREED.                                   */ 01607000
*/********************************************************************/ 01608000
*/* SEGMENT(RDBDY)START                                              */ 01609000
*                                                                  0662 01610000
*   DO;                                                            0662 01611000
*     GOTO RDBDY@;                                                 0663 01612000
         B     RDBDY@                                              0663 01613000
*RDBDY:                                                            0664 01614000
*   END;                                                           0664 01615000
RDBDY    DS    0H                                                  0665 01616000
*   GEN SETS(R0,R1,R14,R15) REFS(RDDECB,ADRDCB,NEXTBUF) SEQFLOW FLOWS(  01617000
*       CSSEODAD,CSSSYNAD)(READ  (RDDECB),SF,,(NEXTBUF),MF=E);     0665 01618000
         READ  (RDDECB),SF,,(NEXTBUF),MF=E                              01619000
*   SKELEND=SKELAD+RDTLEN;                                         0666 01620000
*                                                                  0666 01621000
         L     SKELEND,RDTLEN-1(,RAREA)                            0666 01622000
         LA    SKELEND,0(,SKELEND)                                 0666 01623000
         AL    SKELEND,SKELAD                                      0666 01624000
*/********************************************************************/ 01625000
*/* LOOP THROUGH READING THE SKELETON: AT THE BEGINNING OF EACH      */ 01626000
*/* PASS NEXTBUF POINTS TO AREA JUST READ AND IS UPDATED. RDDECB AND */ 01627000
*/* CKDECB POINT TO THE DECBS LAST READ AND CHECKED, RESPECTIVELY,   */ 01628000
*/* AND HAVE TO BE SWAPPED FOR THE READ AND CHECK ON THIS PASS.      */ 01629000
*/* LOOPING CONTINUES UNTIL NEXTBUF REACHES THE END OF THE SKELETON  */ 01630000
*/* OR AN ERROR OCCURS AND EODAD/SYNAD SETS THE INTERNAL CODE TO 4.  */ 01631000
*/********************************************************************/ 01632000
*                                                                  0667 01633000
*   NEXTBUF=NEXTBUF+DCBBLKSI;                                      0667 01634000
         AH    NEXTBUF,DCBBLKSI+52(,ADRDCB)                        0667 01635000
*   DO NEXTBUF=NEXTBUF BY DCBBLKSI WHILE NEXTBUF<SKELEND&SSRC=0;   0668 01636000
         B     @DE00668                                            0668 01637000
@DL00668 DS    0H                                                  0669 01638000
*     HOLDDECB=CKDECB;                                             0669 01639000
         LR    HOLDDECB,CKDECB                                     0669 01640000
*     CKDECB=RDDECB;                                               0670 01641000
         LR    CKDECB,RDDECB                                       0670 01642000
*     RDDECB=HOLDDECB;                                             0671 01643000
         LR    RDDECB,HOLDDECB                                     0671 01644000
*     GEN SETS(R0,R1,R14,15) REFS(RDDECB,CKDECB,ADRDCB,NEXTBUF) SEQFLOW 01645000
*         FLOWS(CSSEODAD,CSSSYNAD);                                0672 01646000
         READ  (RDDECB),SF,,(NEXTBUF),MF=E                              01647000
         CHECK (RDDECB)                                                 01648000
*   END;                                                           0673 01649000
*                                                                  0673 01650000
         AH    NEXTBUF,DCBBLKSI+52(,ADRDCB)                        0673 01651000
@DE00668 CR    NEXTBUF,SKELEND                                     0673 01652000
         BNL   @DC00668                                            0673 01653000
         L     @03,SSRC                                            0673 01654000
         LTR   @03,@03                                             0673 01655000
         BZ    @DL00668                                            0673 01656000
@DC00668 DS    0H                                                  0674 01657000
*/********************************************************************/ 01658000
*/* IF NO ERROR HAS OCCURRED, CHECK THE LAST READ.  THEN SET UP      */ 01659000
*/* INPUT TO FREE PART (ALL IF ERROR) OF SKELETON BUFFER NOT NEEDED. */ 01660000
*/********************************************************************/ 01661000
*                                                                  0674 01662000
*   IF SSRC=0 THEN                                                 0674 01663000
         L     @15,SSRC                                            0674 01664000
         LTR   @15,@15                                             0674 01665000
         BNZ   @RF00674                                            0674 01666000
*     GEN SETS(R0,R1,R14,R15) SEQFLOW FLOWS(CSSEODAD,CSSSYNAD) REFS(    01667000
*         RDDECB)(CHECK (RDDECB));                                 0675 01668000
         CHECK (RDDECB)                                                 01669000
*   IF SSRC=0 THEN                  /* NOTE: RLEN HAS BUFFER SIZE.   */ 01670000
@RF00674 L     @15,SSRC                                            0676 01671000
         LTR   @15,@15                                             0676 01672000
         BNZ   @RF00676                                            0676 01673000
*     DO;                           /* READS COMPLETE, SO USE EXCESS */ 01674000
*       RLEN=RLEN-RDTLEN;           /* LENGTH AND ADDRESS ABOVE      */ 01675000
         L     @15,RDTLEN-1(,RAREA)                                0678 01676000
         LA    @15,0(,@15)                                         0678 01677000
         L     @14,RLEN                                            0678 01678000
         SLR   @14,@15                                             0678 01679000
         ST    @14,RLEN                                            0678 01680000
*       NEXTBUF=SKELAD+RDTLEN;      /* AREA USED.                    */ 01681000
         AL    @15,SKELAD                                          0679 01682000
         LR    NEXTBUF,@15                                         0679 01683000
*     END;                                                         0680 01684000
*   ELSE                                                           0681 01685000
*     NEXTBUF=SKELAD;               /* INCOMPLETE, FREE WHOLE THING. */ 01686000
         B     @RC00676                                            0681 01687000
@RF00676 L     NEXTBUF,SKELAD                                      0681 01688000
*/* SEGMENT(RDBDY)FINISH                                             */ 01689000
*                                                                  0682 01690000
*   DO;                                                            0682 01691000
@RC00676 DS    0H                                                  0683 01692000
*     GOTO RDBDY#;                                                 0683 01693000
         B     RDBDY#                                              0683 01694000
*RDBDY@:                                                           0684 01695000
*   END;                                                           0684 01696000
*                                                                  0685 01697000
*/********************************************************************/ 01698000
*/* END OF DATA ROUTINE: END OF DATA SHOULD ONLY OCCUR ON PREMATURE  */ 01699000
*/* END BECAUSE RDSKEL AND RDBDY CONTROL THE NUMBER OF READS         */ 01700000
*/* PERFORMED. ERROR MESSAGE IS TO BE ISSUED. SET INTERNAL CODE TO 4.*/ 01701000
*/********************************************************************/ 01702000
*                                                                  0685 01703000
*CSSEODAD:                                                         0685 01704000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0685 01705000
         B     @PB00002                                            0685 01706000
CSSEODAD DS    0H                                                  0686 01707000
*   MSGIDADR=ADDR('306I');                                         0686 01708000
         LA    @15,@CC03298                                        0686 01709000
         ST    @15,MSGIDADR                                        0686 01710000
*   SSRC=4;                                                        0687 01711000
         MVC   SSRC(4),@CF00040                                    0687 01712000
*   END CSSEODAD;                                                  0688 01713000
*                                                                  0688 01714000
@EL00002 DS    0H                                                  0688 01715000
@EF00002 DS    0H                                                  0688 01716000
@ER00002 BR    @14                                                 0688 01717000
*/********************************************************************/ 01718000
*/* SYNCHRONOUS I/O ERROR ROUTINE.  ERROR MESSAGE IS TO BE ISSUED.   */ 01719000
*/* SET INTERNAL CODE TO 4.                                          */ 01720000
*/********************************************************************/ 01721000
*                                                                  0689 01722000
*CSSSYNAD:                                                         0689 01723000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0689 01724000
CSSSYNAD DS    0H                                                  0690 01725000
*   MSGIDADR=ADDR('305I');                                         0690 01726000
         LA    @15,@CC02766                                        0690 01727000
         ST    @15,MSGIDADR                                        0690 01728000
*   SSRC=4;                                                        0691 01729000
         MVC   SSRC(4),@CF00040                                    0691 01730000
*   END CSSSYNAD;                                                  0692 01731000
*                                                                  0692 01732000
@EL00003 DS    0H                                                  0692 01733000
@EF00003 DS    0H                                                  0692 01734000
@ER00003 BR    @14                                                 0692 01735000
@PB00003 DS    0H                                                  0693 01736000
*/* TPEPILOG                                                         */ 01737000
*                                                                  0693 01738000
*   GEN(PRINT OFF);                                                0693 01739000
         PRINT OFF                                                      01740000
*   DECLARE                                                        0694 01741000
*     EMCR0 PTR REG(0),                                            0694 01742000
