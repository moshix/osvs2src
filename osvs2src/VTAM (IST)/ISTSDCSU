         TITLE 'ISTSDCSU - SYSTEM DEFINITION RDT SKELETON UPDATE - OS/V*00001000
               VS       '                                               00002000
ISTSDCSU CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCSU  78.049'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                  0353 00014000
*/********************************************************************/ 00015000
*/* SKELETON UPDATE PROGRAM FUNCTION IS TO WRITE THE SKELETON RDT    */ 00016000
*/* FOR AN RN TO THE SYS1.VTAMOBJ DATA SET. THE RETURN CODE IS ZERO  */ 00017000
*/* FOR SUCCESS AND FOUR IF THE WRITE IS NOT COMPLETE. ON SUCCESS    */ 00018000
*/* THE TTR ADDRESS IS SAVED IN THE MLCA.                            */ 00019000
*/*                                                                  */ 00020000
*/* THIS MAIN SEGMENT IS A CONTROL WHICH CALLS THE OTHER SEGMENTS    */ 00021000
*/* WHERE THE UPDATE IS ACCOMPLISHED. THE DIVISION OF FUNCTION IS IN */ 00022000
*/* 3 PARTS.                                                         */ 00023000
*/*   1. INITIALIZATION OF WORKING STORAGE                           */ 00024000
*/*   2. WRITE SKELETON RDT TO SYS1.VTAMOBJ                          */ 00025000
*/*   3. UPDATE DIRECTORY ENTRY                                      */ 00026000
*/*                                                                  */ 00027000
*/*                        ENTRY LINKAGE                             */ 00028000
*/*                                                                  */ 00029000
*/********************************************************************/ 00030000
*                                                                  0353 00031000
*   MLCSAV3(2)=SAVREG;              /* SET UP SAVE AREA PTRS         */ 00032000
         ST    SAVREG,MLCSAV3+4(,MLCAPTR)                          0353 00033000
*   SAVREG=ADDR(MLCSAV3);           /* BY SYSDEF CONVENTIONS         */ 00034000
         LA    SAVREG,MLCSAV3(,MLCAPTR)                            0354 00035000
*   MLCSAV2(3)=SAVREG;                                             0355 00036000
         ST    SAVREG,MLCSAV2+8(,MLCAPTR)                          0355 00037000
*   GENERATE REFS(MLCDAT3,ISTMLCA);                                0356 00038000
*                                                                  0356 00039000
     USING @DATD-(MLCDAT3-ISTMLCA),MLCAPTR  ADDRESSABILITY FOR DATA     00040000
*   /*****************************************************************/ 00041000
*   /*                                                               */ 00042000
*   /* SEGMENT(INITWS)REQUEST                                        */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0357 00046000
*   DO;                                                            0357 00047000
*     GOTO INITWS;                                                 0358 00048000
         B     INITWS                                              0358 00049000
*INITWS#:                                                          0359 00050000
*   END;                            /* INITIALIZE WORK STORAGE       */ 00051000
INITWS#  DS    0H                                                  0360 00052000
*   IF RC=0 THEN                    /* INITWS EXECUTE OK?            */ 00053000
*                                                                  0360 00054000
         CLI   RC,0                                                0360 00055000
         BNE   @RF00360                                            0360 00056000
*     /***************************************************************/ 00057000
*     /*                                                             */ 00058000
*     /* ML1                                                         */ 00059000
*     /*                                                             */ 00060000
*     /***************************************************************/ 00061000
*                                                                  0361 00062000
*     DO;                                                          0361 00063000
*                                                                  0361 00064000
*       /*************************************************************/ 00065000
*       /*                                                           */ 00066000
*       /* SEGMENT(SKELWRT)REQUEST                                   */ 00067000
*       /*                                                           */ 00068000
*       /*************************************************************/ 00069000
*                                                                  0362 00070000
*       DO;                                                        0362 00071000
*         GOTO SKELWRT;                                            0363 00072000
         B     SKELWRT                                             0363 00073000
*SKELWRT#:                                                         0364 00074000
*       END;                        /* WRITE SKELETON RDT            */ 00075000
SKELWRT# DS    0H                                                  0365 00076000
*       IF RC=0 THEN                /* WRITE SKEL EXECUTE OK?        */ 00077000
*                                                                  0365 00078000
         CLI   RC,0                                                0365 00079000
         BNE   @RF00365                                            0365 00080000
*         /***********************************************************/ 00081000
*         /*                                                         */ 00082000
*         /* SEGMENT(DIRNTRY)REQUEST                                 */ 00083000
*         /*                                                         */ 00084000
*         /***********************************************************/ 00085000
*                                                                  0366 00086000
*         DO;                                                      0366 00087000
*           GOTO DIRNTRY;                                          0367 00088000
         B     DIRNTRY                                             0367 00089000
*DIRNTRY#:                                                         0368 00090000
*         END;                      /* UPDATE DIRECTORY              */ 00091000
*       ELSE                                                       0369 00092000
*         ;                                                        0369 00093000
@RF00365 DS    0H                                                  0370 00094000
*     END;                          /* ML1                           */ 00095000
*   ELSE                                                           0371 00096000
*     ;                                                            0371 00097000
@RF00360 DS    0H                                                  0372 00098000
*   IF DCBOFOPN=ON THEN             /* DCB OPEN?                     */ 00099000
@RC00360 TM    DCBOFOPN+40(RDCBAD),B'00010000'                     0372 00100000
         BNO   @RF00372                                            0372 00101000
*                                                                  0373 00102000
*/*VTCLOSE FILE((RDCBAD))                                            */ 00103000
*                                                                  0373 00104000
*     DO;                           /* DECLARE AND RESTRICT REG 2    */ 00105000
*       DCL                                                        0374 00106000
*         GPR02F FIXED(31) REG(02);                                0374 00107000
*       DCL                                                        0375 00108000
*         GPR02P PTR(31) REG(02);                                  0375 00109000
*       RFY                                                        0376 00110000
*        (GPR02P) RSTD;             /* INVOKE XREGSET MACRO TO VERIFY   00111000
*                                      FILE OPERAND AND PUT IT IN  0376 00112000
*                                      REGISTER 2                    */ 00113000
*/*XREGSET  PID('FILE')PPARM((RDCBAD))RREG(2,GPR02P)                 */ 00114000
*                                                                  0377 00115000
*       DO;                                                        0377 00116000
*         GPR02P=RDCBAD;            /* SET RREG SYMBOL TO VALUE OF 0378 00117000
*                                      PPARM OPERAND                 */ 00118000
         LR    GPR02P,RDCBAD                                       0378 00119000
*       END;                        /* XREGSET                       */ 00120000
*       GEN REFS(GPR02P);                                          0380 00121000
         CLOSE  ((2))                                                   00122000
*                                   /* UNRESTRICT REGISTER 2         */ 00123000
*       RFY                                                        0381 00124000
*        (GPR02P) UNRSTD;                                          0381 00125000
*     END;                          /* VTCLOSE CLOSE SYS1.VTAMOBJ  0382 00126000
*                                                            @ZA29748*/ 00127000
*   ELSE                                                           0383 00128000
*     ;                                                            0383 00129000
@RF00372 DS    0H                                                  0384 00130000
*   IF MSGIDADR=0 THEN                                             0384 00131000
         LTR   MSGIDADR,MSGIDADR                                   0384 00132000
         BZ    @RT00384                                            0384 00133000
*     ;                             /* ANY MSG TO PRINT?             */ 00134000
*   ELSE                                                           0386 00135000
*                                                                  0386 00136000
*/*TPMSG MSGID((MSGIDADR))MSG((ADDR(MLCCONFG)))PLAREA(MLCTPMSG)OPTIONS( 00137000
*CSPLSID)                                                            */ 00138000
*                                                                  0386 00139000
*     DO;                                                          0386 00140000
*       DCL                                                        0387 00141000
*         REG1011 REG(1) PTR;                                      0387 00142000
*       RFY                                                        0388 00143000
*        (REG1011) RSTD;                                           0388 00144000
*       RFY                                                        0389 00145000
*        (EMCR13) RSTD;                                            0389 00146000
*       DCL                                                        0390 00147000
*         1 * BASED(REG1011),                                      0390 00148000
*          2 CMZID011 PTR(31),                                     0390 00149000
*          2 *,                                                    0390 00150000
*           3 CMZRL011 PTR(8),                                     0390 00151000
*           3 CMZRA011 PTR(24) BDY(BYTE),                          0390 00152000
*          2 CMZEA011 PTR(31),                                     0390 00153000
*          2 CMZVP011 PTR(31);                                     0390 00154000
*                                                                  0390 00155000
*/*XREGSET  PID('PLAREA')PPARM(MLCTPMSG)RREG(,REG1011)               */ 00156000
*                                                                  0391 00157000
*       DO;                                                        0391 00158000
*         REG1011=ADDR(MLCTPMSG);   /* SET RREG SYMBOL TO ADDRESS OF    00159000
*                                      PPARM OPERAND                 */ 00160000
         LA    REG1011,MLCTPMSG(,MLCAPTR)                          0392 00161000
*       END;                        /* XREGSET                       */ 00162000
*       CMZID011=0;                                                0394 00163000
         SLR   @11,@11                                             0394 00164000
         ST    @11,CMZID011(,REG1011)                              0394 00165000
*       CMZRL011=0;                                                0395 00166000
         MVI   CMZRL011(REG1011),X'00'                             0395 00167000
*       CMZRA011=0;                                                0396 00168000
         STCM  @11,7,CMZRA011(REG1011)                             0396 00169000
*       CMZEA011=0;                                                0397 00170000
         ST    @11,CMZEA011(,REG1011)                              0397 00171000
*       CMZVP011=0;                                                0398 00172000
         ST    @11,CMZVP011(,REG1011)                              0398 00173000
*       CMZID011=MSGIDADR;                                         0399 00174000
         ST    MSGIDADR,CMZID011(,REG1011)                         0399 00175000
*       CMZVP011=ADDR(MLCCONFG);                                   0400 00176000
         LA    @00,MLCCONFG(,MLCAPTR)                              0400 00177000
         ST    @00,CMZVP011(,REG1011)                              0400 00178000
*       DCL                                                        0401 00179000
*         CMZSA011(18) PTR(31) BASED(EMCR13);                      0401 00180000
*       CMZSA011(5)=ADDR(NCSPLSID);                                0402 00181000
         L     @15,MLCNCSPL(,MLCAPTR)                              0402 00182000
         LA    @00,NCSPLSID(,@15)                                  0402 00183000
         ST    @00,CMZSA011+16(,EMCR13)                            0402 00184000
*       DCL                                                        0403 00185000
*         REG15011 REG(15) PTR;                                    0403 00186000
*       RFY                                                        0404 00187000
*         REG15011 RSTD;                                           0404 00188000
*       DO;                         /* LISATCVT                    0405 00189000
*                                      CVTBASE(PAGEABLE,(REG15011))  */ 00190000
*         RFY                                                      0406 00191000
*           ISTPAB;                                                0406 00192000
*         RFY                                                      0407 00193000
*           ISTDYPAB;                                              0407 00194000
*         RFY                                                      0408 00195000
*           ISTRPH;                                                0408 00196000
*         RFY                                                      0409 00197000
*           ISTATCVT;                                              0409 00198000
*         REG15011=ATCLCPTR;        /* SET UP ATCVT POINTER          */ 00199000
         L     REG15011,ATCLCPTR                                   0410 00200000
*       END;                        /* LISATCVT                      */ 00201000
*       DCL                                                        0412 00202000
*         CMTA011 BASED(REG15011) ENTRY VALUERANGE(*);             0412 00203000
*       REG15011=REG15011->ATCMSGP;                                0413 00204000
         L     REG15011,ATCMSGP(,REG15011)                         0413 00205000
*       CALL CMTA011;                                              0414 00206000
         BALR  @14,@15                                             0414 00207000
*       RESPECIFY                                                  0415 00208000
*        (REG1011,                                                 0415 00209000
*         REG15011) UNRESTRICTED;                                  0415 00210000
*       RFY                                                        0416 00211000
*        (EMCR13) UNRSTD;                                          0416 00212000
*     END;                          /*                       @D32CKQH*/ 00213000
*/********************************************************************/ 00214000
*/* RETURN TO CALLER                                                 */ 00215000
*/*    RC=0 IS SUCCESSFUL UPDATE                                     */ 00216000
*/*    RC=4 IS NO UPDATE OCCURRED                                    */ 00217000
*/********************************************************************/ 00218000
*                                                                  0418 00219000
*   SAVREG=MLCSAV3(2);              /* RESTORE CALLER REGS           */ 00220000
@RT00384 L     SAVREG,MLCSAV3+4(,MLCAPTR)                          0418 00221000
*   RETURN CODE(RC);                                               0419 00222000
         SLR   @15,@15                                             0419 00223000
         IC    @15,RC                                              0419 00224000
         L     @14,12(,@13)                                        0419 00225000
         LM    @00,@12,20(@13)                                     0419 00226000
         BR    @14                                                 0419 00227000
*                                                                  0420 00228000
*/********************************************************************/ 00229000
*/* INITIALIZATION OF WORKING STORAGE                                */ 00230000
*/*   THIS SEGMENT WILL SET UP THE DCB, OPEN SYS1.VTAMOBJ, ESTABLISH */ 00231000
*/*   A DECB, AND GET STORAGE FOR BUFFER SPACE.                      */ 00232000
*/********************************************************************/ 00233000
*                                                                  0420 00234000
*   /*****************************************************************/ 00235000
*   /*                                                               */ 00236000
*   /* SEGMENT(INITWS)START                                          */ 00237000
*   /*                                                               */ 00238000
*   /*****************************************************************/ 00239000
*                                                                  0420 00240000
*   DO;                                                            0420 00241000
*     GOTO INITWS@;                                                0421 00242000
*INITWS:                                                           0422 00243000
*   END;                                                           0422 00244000
INITWS   DS    0H                                                  0423 00245000
*   RDCBAD=ADDR(MLCDCB);            /* SET UP DCB PTR                */ 00246000
         LA    RDCBAD,MLCDCB(,MLCAPTR)                             0423 00247000
*   IHADCB(1:(ADDR(MODELEND)-ADDR(MODELDCB)))=/* DCB TO MLCA         */ 00248000
*       MODELDCB(1:(ADDR(MODELEND)-ADDR(MODELDCB)));               0424 00249000
         LA    @11,MODELEND                                        0424 00250000
         LA    @15,MODELDCB                                        0424 00251000
         SLR   @11,@15                                             0424 00252000
         BCTR  @11,0                                               0424 00253000
         EX    @11,@SM03286                                        0424 00254000
*   RC=0;                           /* INITIALIZE RC TO ZERO         */ 00255000
         MVI   RC,X'00'                                            0425 00256000
*   MSGIDADR=0;                     /* INITIALIZE TO ZERO            */ 00257000
         SLR   MSGIDADR,MSGIDADR                                   0426 00258000
*   RFY                                                            0427 00259000
*     GPR15 RSTD;                                                  0427 00260000
*                                                                  0428 00261000
*/*VTOPEN FILE((RDCBAD))OPTIONS(OUTPUT)                              */ 00262000
*                                                                  0428 00263000
*   DO;                             /* DECLARE AND RESTRICT REG 2    */ 00264000
*     RFY                                                          0429 00265000
*      (GPR02P) RSTD;               /* INVOKE XREGSET MACRO TO VERIFY   00266000
*                                      FILE OPERAND AND PUT IT IN  0429 00267000
*                                      REGISTER 2                    */ 00268000
*/*XREGSET  PID('FILE')PPARM((RDCBAD))RREG(2,GPR02P)                 */ 00269000
*                                                                  0430 00270000
*     DO;                                                          0430 00271000
*       GPR02P=RDCBAD;              /* SET RREG SYMBOL TO VALUE OF 0431 00272000
*                                      PPARM OPERAND                 */ 00273000
         LR    GPR02P,RDCBAD                                       0431 00274000
*     END;                          /* XREGSET                       */ 00275000
*     GEN REFS(GPR02P);                                            0433 00276000
         OPEN   ((2),(OUTPUT))                                          00277000
*                                   /* RESTRICT REGISTER 15          */ 00278000
*     DCL                                                          0434 00279000
