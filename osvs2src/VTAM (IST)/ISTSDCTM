*         RESPECIFY                                                0497 01041000
*           CMZR0015 UNRSTD;                                       0497 01042000
*       END;                                                       0498 01043000
*       RCPLGNAP=BLANK8;                                           0499 01044000
         L     @07,MLCPTR3(,MLCAREG)                               0499 01045000
         MVC   RCPLGNAP(8,@07),@CC01247                            0499 01046000
*     END;                                                         0500 01047000
*   GPR15=0;                                                       0501 01048000
@RC00488 SLR   GPR15,GPR15                                         0501 01049000
*   GPR14=ADDR(MAINSUB);                                           0502 01050000
         LA    GPR14,MAINSUB                                       0502 01051000
*   END TRMLGAPL;                                                  0503 01052000
@EL00010 DS    0H                                                  0503 01053000
@EF00010 DS    0H                                                  0503 01054000
@ER00010 BR    @14                                                 0503 01055000
*                                                                  0504 01056000
*/********************************************************************/ 01057000
*/*          LGTAB PARAMETER SUBROUTINE                              */ 01058000
*/********************************************************************/ 01059000
