*           CALL ISTSDCC2;          /* CALL ERROR FORMAT ROUTINE     */ 01959000
         L     @15,@CV01299                                        0779 01960000
         BALR  @14,@15                                             0779 01961000
*           RESPECIFY                                              0780 01962000
*             CMZR0023 UNRSTD;                                     0780 01963000
*         END;                      /*                       @XA06939*/ 01964000
*         RC=4;                     /*                       @XA06939*/ 01965000
         MVC   RC(2),@CH00052                                      0782 01966000
*       END;                        /*                       @XA06939*/ 01967000
*   ELSE                                                           0784 01968000
*     DO;                                                          0784 01969000
         B     @RC00741                                            0784 01970000
@RF00741 DS    0H                                                  0785 01971000
*       RNCDTYPE='00'X;             /* NOT COMP                      */ 01972000
         MVI   RNCDTYPE(RNCAREG),X'00'                             0785 01973000
*       GPR1=RPRENTTR;              /* ASK FOR TERM MOD              */ 01974000
         LA    GPR1,128                                            0786 01975000
*     END;                                                         0787 01976000
*                                                                  0787 01977000
*/* SEGMENT(TRMCOMP)FINISH                                           */ 01978000
*                                                                  0788 01979000
*   DO;                                                            0788 01980000
@RC00741 DS    0H                                                  0789 01981000
*     GOTO TRMCOMP#;                                               0789 01982000
         B     TRMCOMP#                                            0789 01983000
*TRMCOMP@:                                                         0790 01984000
*   END;                                                           0790 01985000
TRMCOMP@ DS    0H                                                  0791 01986000
*                                                                  0791 01987000
*/* SEGMENT(COUNTS)START                                             */ 01988000
*                                                                  0791 01989000
*   DO;                                                            0791 01990000
*     GOTO COUNTS@;                                                0792 01991000
         B     COUNTS@                                             0792 01992000
*COUNTS:                                                           0793 01993000
*   END;                                                           0793 01994000
*                                                                  0793 01995000
COUNTS   DS    0H                                                  0794 01996000
*/********************************************************************/ 01997000
*/* INCREMENT THE COUNT FOR ENTRIES HIGHER THAN THE PRESENT ONE      */ 01998000
*/* WHETHER THE ENTRY IS A COMP OR A TERMINAL                        */ 01999000
*/********************************************************************/ 02000000
*                                                                  0794 02001000
*   IF MLCMACRO='COMP    ' THEN     /* TERM OR COMP ENTRY            */ 02002000
         CLC   MLCMACRO(8,MLCAREG),@CC01457                        0794 02003000
         BNE   @RF00794                                            0794 02004000
*     DO;                                                          0795 02005000
*       RNCGCOMP='1'B;              /* GLOBAL COMP FOOTPRINT         */ 02006000
         OI    RNCGCOMP(RNCAREG),B'00010000'                       0796 02007000
*       RNCCM=GPR1;                 /* RNCA'S PTR TO PREVIOUS COMP   */ 02008000
         ST    GPR1,RNCCM(,RNCAREG)                                0797 02009000
*       RRNCTECM=RRNCTECM+1;        /* INCREMENT COMP COUNTS         */ 02010000
         L     @07,RNCRN(,RNCAREG)                                 0798 02011000
         LA    @04,1                                               0798 02012000
         LH    @03,RRNCTECM(,@07)                                  0798 02013000
         ALR   @03,@04                                             0798 02014000
         STH   @03,RRNCTECM(,@07)                                  0798 02015000
*       RGPCTECM=RGPCTECM+1;        /* IN RN, GP, LN, & TRM          */ 02016000
