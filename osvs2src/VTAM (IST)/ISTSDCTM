*       IF GPR0>0 THEN                                             0411 00759000
         CR    GPR0,@07                                            0411 00760000
         BNH   @RF00411                                            0411 00761000
*         DO;                                                      0412 00762000
*           CALL ISTSDCC1;          /* CALL AREA HANDLER TO GET RDT     00763000
*                                      EXTENTION FOR DIAL CHARACTERS */ 00764000
         L     @15,@CV01249                                        0413 00765000
         BALR  @14,@15                                             0413 00766000
*           IF GPR15=0 THEN                                        0414 00767000
         LTR   GPR15,GPR15                                         0414 00768000
         BNZ   @RF00414                                            0414 00769000
*             DO;                                                  0415 00770000
*               RTRDIAA=GPR0;       /* SAVE RELATIVE OFFSET TO DIAL     00771000
*                                      CHARACTERS                    */ 00772000
         L     @07,MLCPTR3(,MLCAREG)                               0416 00773000
