         OI    RTRRBISA(@07),B'01000000'                           0759 01895000
*                 TRMTEMP=ADDR(RNCTR->RPRDEVCH);                   0760 01896000
         LA    @04,RPRDEVCH(,@07)                                  0760 01897000
         ST    @04,TRMTEMP                                         0760 01898000
*                 TRMTEMP->DEVSUBND='1'B;/* DEVICE HAS SUBNODES      */ 01899000
         OI    DEVSUBND(@04),B'00010000'                           0761 01900000
*                 RNCTR->RPRAOPN='0'B;/* DUMMY TERMINAL CAN'T OPENED    01901000
*                                      BY APPL                       */ 01902000
         NI    RPRAOPN(@07),B'11011111'                            0762 01903000
*                 RNCTR->RTRCTECM=1;/* FIRST COMPONENT ENTRY         */ 01904000
         LA    @04,1                                               0763 01905000
         STH   @04,RTRCTECM(,@07)                                  0763 01906000
*                 RRNCTECM=RRNCTECM+1;/* UPDATE TOTAL COUNT OF     0764 01907000
*                                      COMPONENT ENTRIES FOR RN      */ 01908000
         L     @07,RNCRN(,RNCAREG)                                 0764 01909000
         LH    @03,RRNCTECM(,@07)                                  0764 01910000
         ALR   @03,@04                                             0764 01911000
         STH   @03,RRNCTECM(,@07)                                  0764 01912000
*                 RGPCTECM=RGPCTECM+1;/* UPDATE TOTAL COUNT OF     0765 01913000
*                                      COMPONENT ENTRIES FOR GROUP   */ 01914000
         L     @07,RNCGP(,RNCAREG)                                 0765 01915000
         LH    @03,RGPCTECM(,@07)                                  0765 01916000
         ALR   @03,@04                                             0765 01917000
         STH   @03,RGPCTECM(,@07)                                  0765 01918000
*                 RLNCTECM=RLNCTECM+1;/* UPDATE TOTAL COUNT OF     0766 01919000
*                                      COMPONENT ENTRIES FOR LINE    */ 01920000
         L     @07,RNCLN(,RNCAREG)                                 0766 01921000
         AH    @04,RLNCTECM(,@07)                                  0766 01922000
         STH   @04,RLNCTECM(,@07)                                  0766 01923000
*                 GPR1=RPRENTCM;    /* ASK SIFT/DEF FOR COMP         */ 01924000
         LA    GPR1,144                                            0767 01925000
*               END;                /* END OF GPR15=0 FROM C1        */ 01926000
*             ELSE                                                 0769 01927000
*               RC=4;               /* AREAHANDLER ALREADY PUT OUT 0769 01928000
*                                      ERROR MESSAGE                 */ 01929000
         B     @RC00749                                            0769 01930000
@RF00749 MVC   RC(2),@CH00052                                      0769 01931000
*           END;                    /* END FIRST COMPONENT           */ 01932000
*         ELSE                                                     0771 01933000
*           GPR1=RPRENTCM;          /* ASK SIFT/DEF FOR COMP         */ 01934000
         B     @RC00744                                            0771 01935000
@RF00744 LA    GPR1,144                                            0771 01936000
*       END;                        /* IF NOT 'COMP' MACRO           */ 01937000
*     ELSE                                                         0773 01938000
*       DO;                         /* DIALED COMP           @XA06939*/ 01939000
         B     @RC00742                                            0773 01940000
@RF00742 DS    0H                                                  0774 01941000
*/* ERRFMT    LEVEL(MACRO)  ERROR(CONF)  ACTION(KILLBLD)             */ 01942000
*                                                                  0774 01943000
*         DO;                                                      0774 01944000
*           DCL                                                    0775 01945000
*             1 CMZP0023 STATIC LOCAL,/* PARAMETER LIST PASSED TO  0775 01946000
*                                      ERROR FORMAT ROUTINE          */ 01947000
*              2 CMZL0023 FIXED(8) INIT(3),/* LEVEL PARAMETER (CLEV) */ 01948000
*              2 CMZE0023 FIXED(8) INIT(2),/* ERROR PARAMETER (CERR) */ 01949000
*              2 CMZA0023 CHAR(1) INIT('6'),/* ACTION PARAMETER    0775 01950000
