*                 IF RNCDTYPE='00'X /* WAS DEVICE TYPE SAVE AREA   0753 01877000
*                                      FILLED IN BY TRMDEVIC"        */ 01878000
*                   THEN                                           0753 01879000
         CLI   RNCDTYPE(RNCAREG),X'00'                             0753 01880000
         BE    @RT00753                                            0753 01881000
*                   ;                                              0754 01882000
*                 ELSE                                             0755 01883000
*                   DO;                                            0755 01884000
*                     TRMTEMP=ADDR(RNCCM->RPRDEVCH);               0756 01885000
         L     @07,RNCCM(,RNCAREG)                                 0756 01886000
         LA    @07,RPRDEVCH(,@07)                                  0756 01887000
         ST    @07,TRMTEMP                                         0756 01888000
*                     TRMTEMP->DEVTCODE=RNCDTYPE;/* COPY DEVICE TYPE    01889000
*                                      SAVE AREA                     */ 01890000
         MVC   DEVTCODE(1,@07),RNCDTYPE(RNCAREG)                   0757 01891000
*                   END;                                           0758 01892000
*                 RNCTR->RTRRBISA='1'B;/* ACTVE STATUS               */ 01893000
