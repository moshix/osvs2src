         BNE   @RF00370                                            0370 00633000
@RT00370 DS    0H                                                  0371 00634000
*     DO;                           /* IF 'NONE' SPECIFIED           */ 00635000
*       RTRRDBHN=ON;                /* TURN ON 'NONE' FLG IN RDT     */ 00636000
*       RTRRDBHD=OFF;                                              0373 00637000
         L     @07,MLCPTR3(,MLCAREG)                               0373 00638000
         OI    RTRRDBHN(@07),B'10000000'                           0373 00639000
         NI    RTRRDBHD(@07),B'10111111'                           0373 00640000
*     END;                                                         0374 00641000
*   ELSE                                                           0375 00642000
*     DO;                                                          0375 00643000
         B     @RC00370                                            0375 00644000
@RF00370 DS    0H                                                  0376 00645000
*       IF PHRF34SP(1,1:8)='DYNAMIC ' THEN                         0376 00646000
         CLC   PHRF34SP(8,PHR1REG),@CC01378                        0376 00647000
         BNE   @RF00376                                            0376 00648000
*         DO;                                                      0377 00649000
*                                                                  0377 00650000
*/********************************************************************/ 00651000
*/* IF DYNAMIC IS SPECIFIED THE SYSCNTRL MACRO MUST HAVE HAD THE     */ 00652000
*/* MODIFY BLOCK HANDLER SET ASSOCIATION (BHSASSC) SUBPARM LISTED    */ 00653000
*/* AS AN OPTION ENTRY.                                              */ 00654000
*/********************************************************************/ 00655000
*                                                                  0378 00656000
