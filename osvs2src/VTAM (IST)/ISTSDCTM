         CLI   PHRF34VF(PHR1REG),X'00'                             0505 01066000
         BNE   @RF00505                                            0505 01067000
*     RCPLGNCT=PHRF34SP(1);                                        0506 01068000
         L     @07,MLCPTR3(,MLCAREG)                               0506 01069000
         MVC   RCPLGNCT(8,@07),PHRF34SP(PHR1REG)                   0506 01070000
*   ELSE                                                           0507 01071000
*     DO;                           /* INVAL. TAKE DEFAULT           */ 01072000
         B     @RC00505                                            0507 01073000
@RF00505 DS    0H                                                  0508 01074000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 01075000
*                                                                  0508 01076000
*       DO;                                                        0508 01077000
*         DCL                                                      0509 01078000
*           1 CMZP0016 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   01079000
*                                      FORMAT ROUTINE                */ 01080000
*            2 CMZL0016 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)   */ 01081000
*            2 CMZE0016 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR)   */ 01082000
*            2 CMZA0016 CHAR(1) INIT('1'),/* ACTION PARAMETER (CACT) */ 01083000
*            2 CMZS0016 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 01084000
