*/********************************************************************/ 00806000
*/*                                                                  */ 00807000
*/*            DIRECTN PARAMETER SUBROUTINE                          */ 00808000
*/*                                                                  */ 00809000
*/********************************************************************/ 00810000
*                                                                  0427 00811000
*TRMDRCTN:                                                         0427 00812000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0427 00813000
TRMDRCTN DS    0H                                                  0428 00814000
*   IF PHRF2VAL(1)=DRCTNIN          /* INPUT ONLY DEVICE ?           */ 00815000
*     THEN                                                         0428 00816000
         CLC   PHRF2VAL(4,PHR1REG),@CF00039                        0428 00817000
         BNE   @RF00428                                            0428 00818000
*     DO;                                                          0429 00819000
*       DEVINPUT=ON;                /* DEVICE FLAGS FOR              */ 00820000
*       DEVOTPUT=OFF;               /* INPUT BUT NOT OUTPUT          */ 00821000
         L     @07,MLCPTR4(,MLCAREG)                               0431 00822000
         OI    DEVINPUT(@07),B'10000000'                           0431 00823000
         NI    DEVOTPUT(@07),B'10111111'                           0431 00824000
*     END;                                                         0432 00825000
*   ELSE                                                           0433 00826000
*     IF PHRF2VAL(1)=DRCTNOUT       /* OUTPUT ONLY DEVICE ?          */ 00827000
*       THEN                                                       0433 00828000
         B     @RC00428                                            0433 00829000
@RF00428 CLC   PHRF2VAL(4,PHR1REG),@CF00180                        0433 00830000
         BNE   @RF00433                                            0433 00831000
*       DO;                                                        0434 00832000
*         DEVINPUT=OFF;             /* DEVICE FLAGS FOR              */ 00833000
*         DEVOTPUT=ON;              /* OUTPUT BUT NOT INPUT          */ 00834000
         L     @07,MLCPTR4(,MLCAREG)                               0436 00835000
         OI    DEVOTPUT(@07),B'01000000'                           0436 00836000
         NI    DEVINPUT(@07),B'01111111'                           0436 00837000
*       END;                                                       0437 00838000
*     ELSE                                                         0438 00839000
*       DO;                         /* MUST BE INPUT AND OUTPUT      */ 00840000
         B     @RC00433                                            0438 00841000
@RF00433 DS    0H                                                  0439 00842000
*         DEVINPUT=ON;              /* SET DEVICE FLAGS FOR BOTH     */ 00843000
*         DEVOTPUT=ON;                                             0440 00844000
         L     @07,MLCPTR4(,MLCAREG)                               0440 00845000
         OI    DEVINPUT(@07),B'11000000'                           0440 00846000
*       END;                                                       0441 00847000
*   GPR14=ADDR(MAINSUB);                                           0442 00848000
@RC00433 DS    0H                                                  0442 00849000
@RC00428 LA    GPR14,MAINSUB                                       0442 00850000
*   GPR15=0;                        /* RETURN CODE                   */ 00851000
         SLR   GPR15,GPR15                                         0443 00852000
*   END TRMDRCTN;                                                  0444 00853000
@EL00007 DS    0H                                                  0444 00854000
@EF00007 DS    0H                                                  0444 00855000
@ER00007 BR    @14                                                 0444 00856000
*                                                                  0445 00857000
*/********************************************************************/ 00858000
*/*        ITBMODE PARAMETER SUBROUTINE                              */ 00859000
*/********************************************************************/ 00860000
*                                                                  0445 00861000
*TRMITBMD:                                                         0445 00862000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0445 00863000
*                                                                  0445 00864000
TRMITBMD DS    0H                                                  0446 00865000
*/********************************************************************/ 00866000
*/* THIS ROUTINE WILL PROCESS THE ITBMODE PARAMETER OF THE TERMINAL  */ 00867000
*/* MACRO. IF EITHER SUBPARM IS INVALID AN ERROR MSG IS PUT OUT AND  */ 00868000
*/* THE DEFAULT IS TAKEN. IF THE SUBPARMS ARE VALID THE APPROPRIATE  */ 00869000
*/* BITS ARE TURNED ON IN THE TERMINAL  MODEL IF OTHER THAN THE      */ 00870000
*/* DEFAULT VALUE IS SPECIFIED.                                      */ 00871000
*/********************************************************************/ 00872000
*                                                                  0446 00873000
*   RTRRCIT1=OFF;                   /* DEFAULT OF NO EIB CHARACTER   */ 00874000
*   RTRRCIT2=OFF;                   /* AFTER ITB                     */ 00875000
         L     @07,MLCPTR3(,MLCAREG)                               0447 00876000
         NI    RTRRCIT1(@07),B'00111111'                           0447 00877000
*   IF PHRF2VAL(1)=1                /* IF 1ST SUBPARM OK AND WAS     */ 00878000
*     THEN                          /* 'YES' SET RECEIVE FLAG IN     */ 00879000
         CLC   PHRF2VAL(4,PHR1REG),@CF00039                        0448 00880000
         BNE   @RF00448                                            0448 00881000
*     RTRRCIT1=ON;                  /* TERM ENTRY. IF 'NO'           */ 00882000
         OI    RTRRCIT1(@07),B'10000000'                           0449 00883000
*   ELSE                                                           0450 00884000
*     ;                             /* LEAVE FLAG OFF                */ 00885000
@RF00448 DS    0H                                                  0451 00886000
*   IF PHRF2NVL>1                   /* MORE THAN ONE PARM            */ 00887000
*     THEN                          /* PROCESS SECOND                */ 00888000
         LA    @07,1                                               0451 00889000
         C     @07,PHRF2NVL(,PHR1REG)                              0451 00890000
         BNL   @RF00451                                            0451 00891000
*     IF PHRF2VAL(2)=1              /* IF 2ND SUBPARM OK AND WAS     */ 00892000
*       THEN                        /* 'YES' SET SEND FLAG IN TERM   */ 00893000
         C     @07,PHRF2VAL+4(,PHR1REG)                            0452 00894000
         BNE   @RF00452                                            0452 00895000
*       RTRRCIT2=ON;                /* ENTRY. IF 'NO' LEAVE FLAG     */ 00896000
         L     @07,MLCPTR3(,MLCAREG)                               0453 00897000
         OI    RTRRCIT2(@07),B'01000000'                           0453 00898000
*     ELSE                                                         0454 00899000
*       ;                           /* OFF.                          */ 00900000
@RF00452 DS    0H                                                  0455 00901000
*   ELSE                                                           0455 00902000
*     ;                             /* ONLY ONE PARM SPECIFIED       */ 00903000
@RF00451 DS    0H                                                  0456 00904000
*   GPR15=0;                        /* SET RTN CODE TO 0             */ 00905000
         SLR   GPR15,GPR15                                         0456 00906000
*   GPR14=ADDR(MAINSUB);                                           0457 00907000
         LA    GPR14,MAINSUB                                       0457 00908000
*   END TRMITBMD;                                                  0458 00909000
@EL00008 DS    0H                                                  0458 00910000
@EF00008 DS    0H                                                  0458 00911000
@ER00008 BR    @14                                                 0458 00912000
*                                                                  0459 00913000
*/********************************************************************/ 00914000
*/*          BUFLIM PARAMETER SUBROUTINE                             */ 00915000
*/********************************************************************/ 00916000
*                                                                  0459 00917000
*TRMBFLIM:                                                         0459 00918000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0459 00919000
TRMBFLIM DS    0H                                                  0460 00920000
*   IF PHRF7NSP=0                   /* IF NO PARMS SPECIFIED         */ 00921000
*     THEN                                                         0460 00922000
         L     @07,PHRF7NSP(,PHR1REG)                              0460 00923000
         LTR   @07,@07                                             0460 00924000
         BZ    @RT00460                                            0460 00925000
*     ;                                                            0461 00926000
*   ELSE                                                           0462 00927000
*     DO;                                                          0462 00928000
*       IF PHRF7NUM(1)=0            /* BUFFER LIMIT OF 0 ?           */ 00929000
*         THEN                                                     0463 00930000
*                                                                  0463 00931000
         CLI   PHRF7NUM(PHR1REG),0                                 0463 00932000
         BNE   @RF00463                                            0463 00933000
*/* ERRFMT    LEVEL(VALUE)  ERROR(INVAL)  ACTION(TAKEDEF)            */ 00934000
*                                                                  0464 00935000
*         DO;                                                      0464 00936000
*           DCL                                                    0465 00937000
*             1 CMZP0013 STATIC LOCAL,/* PARAMETER LIST PASSED TO  0465 00938000
*                                      ERROR FORMAT ROUTINE          */ 00939000
*              2 CMZL0013 FIXED(8) INIT(5),/* LEVEL PARAMETER (CLEV) */ 00940000
*              2 CMZE0013 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR) */ 00941000
*              2 CMZA0013 CHAR(1) INIT('2'),/* ACTION PARAMETER    0465 00942000
*                                      (CACT)                        */ 00943000
*              2 CMZS0013 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM */ 00944000
*           DCL                                                    0466 00945000
*             CMZR0013 REGISTER(1); /* ESTABLISH PARAMeter           */ 00946000
*           RESPECIFY                                              0467 00947000
*             CMZR0013 RSTD;        /* REGISTER FOR                  */ 00948000
*           CMZR0013=ADDR(CMZP0013);/* ADDRESS OF PARAMETER LIST     */ 00949000
         LA    CMZR0013,CMZP0013                                   0468 00950000
*           CALL ISTSDCC2;          /* CALL ERROR FORMAT ROUTINE     */ 00951000
         L     @15,@CV01299                                        0469 00952000
         BALR  @14,@15                                             0469 00953000
*           RESPECIFY                                              0470 00954000
*             CMZR0013 UNRSTD;                                     0470 00955000
*         END;                      /* YES                           */ 00956000
*       ELSE                                                       0472 00957000
*         RCPBFLIM=PHRF7NUM(1);     /* NO. USE VALID VALUE           */ 00958000
         B     @RC00463                                            0472 00959000
@RF00463 L     @07,MLCPTR3(,MLCAREG)                               0472 00960000
         MVC   RCPBFLIM(1,@07),PHRF7NUM(PHR1REG)                   0472 00961000
*       IF PHRF7NSP>1               /* ONLY ONE BUFFER LIMIT ALLOWED */ 00962000
*         THEN                                                     0473 00963000
*                                                                  0473 00964000
@RC00463 L     @07,PHRF7NSP(,PHR1REG)                              0473 00965000
         C     @07,@CF00039                                        0473 00966000
         BNH   @RF00473                                            0473 00967000
*/* ERRFMT    LEVEL(VALUE)  ERROR(EXTRA)  ACTION(IGNORE)             */ 00968000
*                                                                  0474 00969000
*         DO;                                                      0474 00970000
*           DCL                                                    0475 00971000
*             1 CMZP0014 STATIC LOCAL,/* PARAMETER LIST PASSED TO  0475 00972000
*                                      ERROR FORMAT ROUTINE          */ 00973000
*              2 CMZL0014 FIXED(8) INIT(5),/* LEVEL PARAMETER (CLEV) */ 00974000
*              2 CMZE0014 FIXED(8) INIT(7),/* ERROR PARAMETER (CERR) */ 00975000
*              2 CMZA0014 CHAR(1) INIT('1'),/* ACTION PARAMETER    0475 00976000
*                                      (CACT)                        */ 00977000
*              2 CMZS0014 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM */ 00978000
*           DCL                                                    0476 00979000
*             CMZR0014 REGISTER(1); /* ESTABLISH PARAMeter           */ 00980000
*           RESPECIFY                                              0477 00981000
*             CMZR0014 RSTD;        /* REGISTER FOR                  */ 00982000
*           CMZR0014=ADDR(CMZP0014);/* ADDRESS OF PARAMETER LIST     */ 00983000
         LA    CMZR0014,CMZP0014                                   0478 00984000
*           CALL ISTSDCC2;          /* CALL ERROR FORMAT ROUTINE     */ 00985000
         L     @15,@CV01299                                        0479 00986000
         BALR  @14,@15                                             0479 00987000
*           RESPECIFY                                              0480 00988000
*             CMZR0014 UNRSTD;                                     0480 00989000
*         END;                                                     0481 00990000
*       ELSE                                                       0482 00991000
*         ;                                                        0482 00992000
@RF00473 DS    0H                                                  0483 00993000
*     END;                                                         0483 00994000
*   GPR15=0;                                                       0484 00995000
@RT00460 SLR   GPR15,GPR15                                         0484 00996000
*   GPR14=ADDR(MAINSUB);                                           0485 00997000
         LA    GPR14,MAINSUB                                       0485 00998000
*   END TRMBFLIM;                                                  0486 00999000
@EL00009 DS    0H                                                  0486 01000000
@EF00009 DS    0H                                                  0486 01001000
@ER00009 BR    @14                                                 0486 01002000
*                                                                  0487 01003000
*/********************************************************************/ 01004000
*/*          LGAPL PARAMETER SUBROUTINE                              */ 01005000
*/********************************************************************/ 01006000
*                                                                  0487 01007000
*TRMLGAPL:                                                         0487 01008000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0487 01009000
TRMLGAPL DS    0H                                                  0488 01010000
*   IF PHRF34VF(1)=PHRVFAOK         /* IF PARM VALID                 */ 01011000
*     THEN                          /* SAVE APPLIC NAME              */ 01012000
         CLI   PHRF34VF(PHR1REG),X'00'                             0488 01013000
         BNE   @RF00488                                            0488 01014000
*     RCPLGNAP=PHRF34SP(1);         /* APPL FOR SIM LOGON            */ 01015000
         L     @07,MLCPTR3(,MLCAREG)                               0489 01016000
         MVC   RCPLGNAP(8,@07),PHRF34SP(PHR1REG)                   0489 01017000
*   ELSE                                                           0490 01018000
*     DO;                           /* INVAL. TAKE DEFAULT           */ 01019000
         B     @RC00488                                            0490 01020000
@RF00488 DS    0H                                                  0491 01021000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 01022000
*                                                                  0491 01023000
*       DO;                                                        0491 01024000
*         DCL                                                      0492 01025000
*           1 CMZP0015 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   01026000
*                                      FORMAT ROUTINE                */ 01027000
*            2 CMZL0015 FIXED(8) INIT(4),/* LEVEL PARAMETER (CLEV)   */ 01028000
*            2 CMZE0015 FIXED(8) INIT(4),/* ERROR PARAMETER (CERR)   */ 01029000
*            2 CMZA0015 CHAR(1) INIT('1'),/* ACTION PARAMETER (CACT) */ 01030000
*            2 CMZS0015 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 01031000
*         DCL                                                      0493 01032000
*           CMZR0015 REGISTER(1);   /* ESTABLISH PARAMeter           */ 01033000
*         RESPECIFY                                                0494 01034000
*           CMZR0015 RSTD;          /* REGISTER FOR                  */ 01035000
*         CMZR0015=ADDR(CMZP0015);  /* ADDRESS OF PARAMETER LIST     */ 01036000
         LA    CMZR0015,CMZP0015                                   0495 01037000
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */ 01038000
