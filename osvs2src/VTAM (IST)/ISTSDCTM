         TITLE 'TERM MACRO HNDLR-ISTSDCTM                              *00001000
                        '                                               00002000
ISTSDCTM CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCTM  78.100'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  LA    @12,4095(,@10)                                      0001 00012000
         USING @PSTART,@10                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
*   RNCSAV1(2)=GPR13;               /* BACKWARD POINT                */ 00015000
         ST    GPR13,RNCSAV1+4(,RNCAREG)                           0199 00016000
*   GPR13=ADDR(RNCSAV1);            /* SET UP OUR SAVE REG           */ 00017000
         LA    GPR13,RNCSAV1(,RNCAREG)                             0200 00018000
*   MLCSAV3(3)=GPR13;               /* FORWARD POINT                 */ 00019000
         ST    GPR13,MLCSAV3+8(,MLCAREG)                           0201 00020000
*   DATDREG=ADDR(RNCDAT1);          /* SET UP OUR DATA REG           */ 00021000
         LA    DATDREG,RNCDAT1(,RNCAREG)                           0202 00022000
*   GENERATE;                                                      0203 00023000
*                                                                  0203 00024000
           USING @DATD,11                 ADDRESSABILITY FOR DATA       00025000
*/********************************************************************/ 00026000
*/* DIAL SUPPORT                                                     */ 00027000
*/********************************************************************/ 00028000
*                                                                  0204 00029000
*   IF MLCMACRO='VTERM   '          /* IF DIAL SUPPORT MACR0 (VTERM) */ 00030000
*       &RNCGCTRM='0'B THEN         /* WAS CTERM SPECIFIED ON      0204 00031000
*                                      PREVIOUS TERM MACRO           */ 00032000
         CLC   MLCMACRO(8,MLCAREG),@CC01290                        0204 00033000
         BNE   @RF00204                                            0204 00034000
         TM    RNCGCTRM(RNCAREG),B'00000100'                       0204 00035000
         BNZ   @RF00204                                            0204 00036000
*     DO;                           /* NO. ERROR CONDITION           */ 00037000
*/* ERRFMT    LEVEL(MACRO)  ERROR(SEQ)  ACTION(KILLBLD)              */ 00038000
*                                                                  0206 00039000
*       DO;                                                        0206 00040000
*         DCL                                                      0207 00041000
*           1 CMZP0004 STATIC LOCAL,/* PARAMETER LIST PASSED TO ERROR   00042000
*                                      FORMAT ROUTINE                */ 00043000
*            2 CMZL0004 FIXED(8) INIT(3),/* LEVEL PARAMETER (CLEV)   */ 00044000
*            2 CMZE0004 FIXED(8) INIT(5),/* ERROR PARAMETER (CERR)   */ 00045000
*            2 CMZA0004 CHAR(1) INIT('6'),/* ACTION PARAMETER (CACT) */ 00046000
*            2 CMZS0004 FIXED(8) INIT(0);/* LENGTH OF STRING PARAM   */ 00047000
*         DCL                                                      0208 00048000
*           CMZR0004 REGISTER(1);   /* ESTABLISH PARAMeter           */ 00049000
*         RESPECIFY                                                0209 00050000
*           CMZR0004 RSTD;          /* REGISTER FOR                  */ 00051000
*         CMZR0004=ADDR(CMZP0004);  /* ADDRESS OF PARAMETER LIST     */ 00052000
         LA    CMZR0004,CMZP0004                                   0210 00053000
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */ 00054000
         L     @15,@CV01299                                        0211 00055000
         BALR  @14,@15                                             0211 00056000
*         RESPECIFY                                                0212 00057000
*           CMZR0004 UNRSTD;                                       0212 00058000
*       END;                                                       0213 00059000
*       RC=4;                       /* SET ERROR RETURN              */ 00060000
         MVC   RC(2),@CH00052                                      0214 00061000
*     END;                                                         0215 00062000
*   ELSE                                                           0216 00063000
*     DO;                           /* PROPER ORDER OR NOT VTERM     */ 00064000
         B     @RC00204                                            0216 00065000
@RF00204 DS    0H                                                  0217 00066000
*/********************************************************************/ 00067000
*/* WHILE THE RETURN CODE = 0, THIS CODE PREPARES THE PHR PARAMETERS */ 00068000
*/* AND CALLS PHR.  UPON RETURN FROM PHR, THE RETURN CODE AND THE @  */ 00069000
*/* OF THE NEXT MACRO KEYWORD ARE SAVED.                             */ 00070000
*/********************************************************************/ 00071000
*                                                                  0217 00072000
*       RC=0;                                                      0217 00073000
*                                                                  0217 00074000
         SLR   @07,@07                                             0217 00075000
         STH   @07,RC                                              0217 00076000
*       /*************************************************************/ 00077000
*       /*                                                           */ 00078000
*       /* SEGMENT(TRMCOMP)REQUEST                                   */ 00079000
*       /*                                                           */ 00080000
*       /*************************************************************/ 00081000
*                                                                  0218 00082000
*       DO;                                                        0218 00083000
*         GOTO TRMCOMP;                                            0219 00084000
         B     TRMCOMP                                             0219 00085000
*TRMCOMP#:                                                         0220 00086000
*       END;                        /* PREPARE TO CALL SIFTDEF     0220 00087000
*                                      (ISTSDCRS) FOR COMP. MACRO OR    00088000
*                                      ANY OTHER.                    */ 00089000
TRMCOMP# DS    0H                                                  0221 00090000
*       IF RC=0 THEN                /* CONTINUE PROCESSING ?         */ 00091000
         LH    @07,RC                                              0221 00092000
         LTR   @07,@07                                             0221 00093000
         BNZ   @RF00221                                            0221 00094000
*         DO;                       /* YES. NOTE: GPR1 HAS @ OF TERM    00095000
*                                      OR COMP SIFT MODEL            */ 00096000
*CALLRS:                                                           0223 00097000
*           CALL ISTSDCRS;          /* CALL SIFTDEF                  */ 00098000
CALLRS   L     @15,@CV01253                                        0223 00099000
         BALR  @14,@15                                             0223 00100000
*           GPR1=MLCKWD;            /* FOR PHR IN FIRST PASS THRU  0224 00101000
*                                      DOWHILE                       */ 00102000
         L     GPR1,MLCKWD(,MLCAREG)                               0224 00103000
*/********************************************************************/ 00104000
*/* DIAL SUPPORT                                                     */ 00105000
*/********************************************************************/ 00106000
*                                                                  0225 00107000
*           IF MLCMACRO='VTERM   ' THEN                            0225 00108000
         CLC   MLCMACRO(8,MLCAREG),@CC01290                        0225 00109000
         BNE   @RF00225                                            0225 00110000
*             DO;                                                  0226 00111000
*               KWTBLAD=ADDR(VTRKWTBL);/* SET UP 'VTERM' K.W. TABLE  */ 00112000
         LA    @07,VTRKWTBL                                        0227 00113000
         ST    @07,KWTBLAD                                         0227 00114000
*               RTRVTERM=ON;        /* FLAG ENTRY AS VTERM           */ 00115000
         L     @07,MLCPTR3(,MLCAREG)                               0228 00116000
         OI    RTRVTERM(@07),B'00000100'                           0228 00117000
*             END;                                                 0229 00118000
*           ELSE                                                   0230 00119000
*             DO;                                                  0230 00120000
         B     @RC00225                                            0230 00121000
@RF00225 DS    0H                                                  0231 00122000
*               KWTBLAD=ADDR(TRMKWTBL);/* SAME K.W. TABLE FOR 'TERM'    00123000
*                                      AND 'COMP'                    */ 00124000
         LA    @07,TRMKWTBL                                        0231 00125000
         ST    @07,KWTBLAD                                         0231 00126000
*             END;                  /* 'TERM' OR COMP                */ 00127000
*/********************************************************************/ 00128000
*/* RC IS STILL ZERO FROM RETURN FROM 'TERMCOMP'                     */ 00129000
*/********************************************************************/ 00130000
*                                                                  0233 00131000
*           DO WHILE(RC=0);         /* SCAN THE MACRO AND HANDLE     */ 00132000
@RC00225 B     @DE00233                                            0233 00133000
@DL00233 DS    0H                                                  0234 00134000
*             CALL DOWHILE;         /* EACH KEYWORD IN TURN          */ 00135000
         BAL   @14,DOWHILE                                         0234 00136000
*DOLOOP:                                                           0235 00137000
*             ;                                                    0235 00138000
*                                                                  0235 00139000
DOLOOP   DS    0H                                                  0236 00140000
*/********************************************************************/ 00141000
*/* AT THIS POINT A RETURN CODE OF 0 MEANS CONTINUE THE DO WHILE     */ 00142000
*/* LOOP.  OTHERWISE, THE LOOP IS DISCONTINUED WITH A RETURN CODE OF */ 00143000
*/* 8 (NORMAL END OF STRING) OR 4 (SIGNIFICANT ERROR).               */ 00144000
*/********************************************************************/ 00145000
*                                                                  0236 00146000
*           END;                                                   0236 00147000
@DE00233 LH    @07,RC                                              0236 00148000
         LTR   @07,@07                                             0236 00149000
         BZ    @DL00233                                            0236 00150000
*         END;                      /* END DO IF RC = 0 FROM TRMCOMP */ 00151000
*       ELSE                                                       0238 00152000
*         ;                         /* RETURN CODE FROM TRMCOMP NOT     00153000
*                                      EQ 0 (ITS 4-KILL THE BUILD    */ 00154000
@RF00221 DS    0H                                                  0239 00155000
*     END;                          /* END ELSE FOR NO CTERM BEFORE     00156000
*                                      VTERM ERROR CONDITION         */ 00157000
*/********************************************************************/ 00158000
*/* AT THIS POINT IF RC=8, INDICATING END OF STRING, WE CALL AREA    */ 00159000
*/* HANDLER TO MOVE THE MODEL AREA INTO THE BUILD AREA.  UPON RETURN */ 00160000
*/* WE UPDATE THE CURRENT ENTRY POINTER TO THIS ENTRY. ALSO, ANY     */ 00161000
*/* COUNTS THAT NEED BE UPDATED, ARE DONE AT THIS POINT.             */ 00162000
*/********************************************************************/ 00163000
*                                                                  0240 00164000
*MAINRC:                                                           0240 00165000
*   IF RC=8 THEN                                                   0240 00166000
@RC00204 DS    0H                                                  0240 00167000
MAINRC   CLC   RC(2),@CH00036                                      0240 00168000
         BNE   @RF00240                                            0240 00169000
*     DO;                                                          0241 00170000
*                                                                  0241 00171000
*       /*************************************************************/ 00172000
*       /*                                                           */ 00173000
*       /* SEGMENT(POLDEVT)REQUEST                                   */ 00174000
*       /*                                                           */ 00175000
*       /*************************************************************/ 00176000
*                                                                  0242 00177000
*       DO;                                                        0242 00178000
*         GOTO POLDEVT;                                            0243 00179000
         B     POLDEVT                                             0243 00180000
*POLDEVT#:                                                         0244 00181000
*       END;                        /* CHECK POLIMIT & DEVICE        */ 00182000
POLDEVT# DS    0H                                                  0245 00183000
*       IF RC=8 THEN                                               0245 00184000
         CLC   RC(2),@CH00036                                      0245 00185000
         BNE   @RF00245                                            0245 00186000
*         DO;                       /* COMBINATION OK. CONTINUE      */ 00187000
*/********************************************************************/ 00188000
*/* DIAL                                                             */ 00189000
*/********************************************************************/ 00190000
*                                                                  0247 00191000
*           CALL CTRMUTRM;          /* CALL SUB TO CALL AREA HANDLER    00192000
*                                      (ISTSDCC1)                    */ 00193000
         BAL   @14,CTRMUTRM                                        0247 00194000
*TRMRET1:                                                          0248 00195000
*           ;                       /* ****NOTE THIS LABEL USED AS A    00196000
*                                      RETURN POINT BY CTRMUTRM      */ 00197000
TRMRET1  DS    0H                                                  0249 00198000
*           RC=GPR15;                                              0249 00199000
*                                                                  0249 00200000
         STH   GPR15,RC                                            0249 00201000
*/********************************************************************/ 00202000
*/* AT THIS TIME THE VALID RETURN CODE HAS CHANGED FROM 8 TO ZERO    */ 00203000
*/********************************************************************/ 00204000
*                                                                  0250 00205000
*           IF RC=0 THEN                                           0250 00206000
         LH    @07,RC                                              0250 00207000
         LTR   @07,@07                                             0250 00208000
         BNZ   @RF00250                                            0250 00209000
*             DO;                   /* CONTINUE THE BUILD            */ 00210000
*/********************************************************************/ 00211000
*/* CTRMUTRM GOT AN ENTRY IN ALL CASES EXCEPT 'CTERM' NO 'UTERM'.    */ 00212000
*/* IF ENTRY WAS GOTTEN (GETSW=ON) MODEL WAS MOVED INTO BUILD AREA:  */ 00213000
*/* PROCESS IT.  IN NO ENTRY (GETSW=OFF) NOTHING TO PROCESS.         */ 00214000
*/********************************************************************/ 00215000
*                                                                  0252 00216000
*               IF GETSW='0'B THEN                                 0252 00217000
         TM    GETSW,B'01000000'                                   0252 00218000
         BZ    @RT00252                                            0252 00219000
*                 ;                                                0253 00220000
*               ELSE                                               0254 00221000
*                 DO;                                              0254 00222000
*                   CALL RESETYPE;  /* SET TERMINAL CHARACTERISTICS     00223000
*                                      IN DEVCHCK                    */ 00224000
         BAL   @14,RESETYPE                                        0255 00225000
*RESRET01:                                                         0256 00226000
*                   ;                                              0256 00227000
RESRET01 DS    0H                                                  0257 00228000
*                   IF DEVTCODE=DEV2980/* SPECIAL FOR 2980           */ 00229000
*                     THEN                                         0257 00230000
         L     @07,MLCPTR4(,MLCAREG)                               0257 00231000
         CLI   DEVTCODE(@07),X'18'                                 0257 00232000
         BNE   @RF00257                                            0257 00233000
*                     DO;                                          0258 00234000
*                                                                  0258 00235000
*                       /*********************************************/ 00236000
*                       /*                                           */ 00237000
*                       /* SEGMENT(T2980)REQUEST                     */ 00238000
*                       /*                                           */ 00239000
*                       /*********************************************/ 00240000
*                                                                  0259 00241000
*                       DO;                                        0259 00242000
*                         GOTO T2980;                              0260 00243000
         B     T2980                                               0260 00244000
*T2980#:                                                           0261 00245000
*                       END;                                       0261 00246000
*                                                                  0261 00247000
T2980#   DS    0H                                                  0262 00248000
*/*  BUILD SECOND ENTRY                                              */ 00249000
*                                                                  0262 00250000
*                     END;                                         0262 00251000
*                   ELSE                                           0263 00252000
*                     ;             /* NOT 2980                      */ 00253000
@RF00257 DS    0H                                                  0264 00254000
*                   IF RC=0 THEN                                   0264 00255000
         LH    @07,RC                                              0264 00256000
         LTR   @07,@07                                             0264 00257000
         BNZ   @RF00264                                            0264 00258000
*                     DO;           /* IF GOOD RETURN FROM T2980     */ 00259000
*/********************************************************************/ 00260000
*/* UPDATE APPROPRIATE COUNTS FOR TERMINAL                           */ 00261000
*/********************************************************************/ 00262000
*                                                                  0266 00263000
*                       /*********************************************/ 00264000
*                       /*                                           */ 00265000
*                       /* SEGMENT(COUNTS)REQUEST                    */ 00266000
*                       /*                                           */ 00267000
*                       /*********************************************/ 00268000
*                                                                  0266 00269000
*                       DO;                                        0266 00270000
*                         GOTO COUNTS;                             0267 00271000
         B     COUNTS                                              0267 00272000
*COUNTS#:                                                          0268 00273000
*                       END;                                       0268 00274000
COUNTS#  DS    0H                                                  0269 00275000
*                     END;          /* END VALID RETURN FROM T2980   */ 00276000
*                   ELSE                                           0270 00277000
*                     ;                                            0270 00278000
@RF00264 DS    0H                                                  0271 00279000
*                 END;              /* IF GETSW ^= 0                 */ 00280000
*             END;                  /* END VALID RETURN FROM CTRMUTRM*/ 00281000
*           ELSE                                                   0273 00282000
*             ;                     /* INVALID RETURN FROM CTRMUTRM  */ 00283000
@RF00250 DS    0H                                                  0274 00284000
*         END;                      /* END VALID RETURN FROM POLDEVT */ 00285000
*       ELSE                                                       0275 00286000
*         ;                         /* INVALID RETURN FROM POLDEVT   */ 00287000
@RF00245 DS    0H                                                  0276 00288000
*     END;                          /* END VALID RETURN FROM DOWHILE */ 00289000
*   ELSE                                                           0277 00290000
*     ;                             /* INVALID RETURN FROM DOWHILE   */ 00291000
@RF00240 DS    0H                                                  0278 00292000
*   GPR13=RNCSAV1(2);               /* RESTORE CALLERS REGS          */ 00293000
@RC00240 L     GPR13,RNCSAV1+4(,RNCAREG)                           0278 00294000
*/********************************************************************/ 00295000
*/* AT THIS POINT RC CONTAINS EITHER A 0 OR A 4 INDICATING A GOOD    */ 00296000
*/* RETURN CODE OR AN ERROR RETURN CODE WHICH WILL KILL THE BUILD.   */ 00297000
*/********************************************************************/ 00298000
*                                                                  0279 00299000
*MAINRET:                                                          0279 00300000
*   RETURN CODE(RC);                /* RETURN TO CALLER              */ 00301000
MAINRET  LH    @15,RC                                              0279 00302000
         L     @14,12(,@13)                                        0279 00303000
         LM    @00,@12,20(@13)                                     0279 00304000
         BR    @14                                                 0279 00305000
*DOWHILE:                                                          0280 00306000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0280 00307000
DOWHILE  DS    0H                                                  0281 00308000
*   GPR0=KWTBLAD;                   /* @ OF APPROPRIATE K.W. TABLE   */ 00309000
         L     GPR0,KWTBLAD                                        0281 00310000
*   PHR1REG=ADDR(RNCPHR1);          /* @ PHR OUTPUT AREA             */ 00311000
         LA    PHR1REG,RNCPHR1(,RNCAREG)                           0282 00312000
*CALLPHR:                                                          0283 00313000
*   CALL ISTINCPH;                  /* CALL PHR TO LOCATE KEYWORD    */ 00314000
CALLPHR  L     @15,@CV01248                                        0283 00315000
         BALR  @14,@15                                             0283 00316000
*   RC=GPR15;                                                      0284 00317000
         STH   GPR15,RC                                            0284 00318000
*   KWDPTR=GPR1;                    /* SAVE PTR TO NEXT KEYWORD      */ 00319000
         ST    GPR1,KWDPTR                                         0285 00320000
*   IF RC=0                         /* WAS KEYWORD FOUND ?           */ 00321000
*     THEN                                                         0286 00322000
         LH    @07,RC                                              0286 00323000
         LTR   @07,@07                                             0286 00324000
         BNZ   @RF00286                                            0286 00325000
*     DO;                                                          0287 00326000
*                                                                  0287 00327000
*/********************************************************************/ 00328000
*/* IF THE RETURN CODE FROM PHR = 0, CALL THE PARAMETER HANDLER      */ 00329000
*/* WHOSE ADDRESS WAS RETURNED IN GPR0 BY PHR.                       */ 00330000
*/* NOTE: THE EXTERNAL PARAMETER HANDLERS WILL RETURN TO THIS        */ 00331000
*/*       ROUTINE VIA A RETURN.  ALL INTERNAL PARAMETER HANDLERS     */ 00332000
*/*       WILL RETURN TO THIS ROUTINE VIA A GOTO 'MAINSUB'.          */ 00333000
*/* NOTE: ALL PARAMETER HANDLERS MUST HAVE A 0 OR 4 RETURN CODE IN   */ 00334000
*/*       GPR15 TO INDICATE CONTINUE PROCESSING OR KILL THE BUILD.   */ 00335000
*/********************************************************************/ 00336000
*                                                                  0288 00337000
*       GPR15=GPR0;                                                0288 00338000
         LR    GPR15,GPR0                                          0288 00339000
*CALLPARM:                                                         0289 00340000
*       CALL PARMHDLR;                                             0289 00341000
CALLPARM BALR  @14,@15                                             0289 00342000
*MAINSUB:                                                          0290 00343000
*       ;                                                          0290 00344000
MAINSUB  DS    0H                                                  0291 00345000
*       RC=GPR15;                                                  0291 00346000
         STH   GPR15,RC                                            0291 00347000
*     END;                                                         0292 00348000
*   ELSE                                                           0293 00349000
*     DO;                                                          0293 00350000
*                                                                  0293 00351000
         B     @RC00286                                            0293 00352000
@RF00286 DS    0H                                                  0294 00353000
*/********************************************************************/ 00354000
*/* IF THE RETURN CODE FROM PHR = 4 INDICATING THE KEYWORD WAS NOT   */ 00355000
*/* FOUND IN THE PARAMETER TABLE, WRITE THE APPROPRIATE ERROR        */ 00356000
*/* MESSAGE, SET THE RETURN CODE = 0 AND CONTINUE THE DO WHILE LOOP. */ 00357000
*/********************************************************************/ 00358000
*                                                                  0294 00359000
*       IF RC=4 THEN                                               0294 00360000
         CLC   RC(2),@CH00052                                      0294 00361000
         BNE   @RF00294                                            0294 00362000
*         DO;                                                      0295 00363000
*ERR03:                                                            0296 00364000
*                                                                  0296 00365000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)              */ 00366000
*                                                                  0296 00367000
*           DO;                                                    0296 00368000
ERR03    DS    0H                                                  0297 00369000
*             DCL                                                  0297 00370000
*               1 CMZP0009 STATIC LOCAL,/* PARAMETER LIST PASSED TO     00371000
*                                      ERROR FORMAT ROUTINE          */ 00372000
*                2 CMZL0009 FIXED(8) INIT(4),/* LEVEL PARAMETER    0297 00373000
*                                      (CLEV)                        */ 00374000
*                2 CMZE0009 FIXED(8) INIT(4),/* ERROR PARAMETER    0297 00375000
*                                      (CERR)                        */ 00376000
*                2 CMZA0009 CHAR(1) INIT('1'),/* ACTION PARAMETER  0297 00377000
*                                      (CACT)                        */ 00378000
*                2 CMZS0009 FIXED(8) INIT(0);/* LENGTH OF STRING   0297 00379000
*                                      PARAM                         */ 00380000
*             DCL                                                  0298 00381000
*               CMZR0009 REGISTER(1);/* ESTABLISH PARAMeter          */ 00382000
*             RESPECIFY                                            0299 00383000
*               CMZR0009 RSTD;      /* REGISTER FOR                  */ 00384000
*             CMZR0009=ADDR(CMZP0009);/* ADDRESS OF PARAMETER LIST   */ 00385000
         LA    CMZR0009,CMZP0009                                   0300 00386000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 00387000
         L     @15,@CV01299                                        0301 00388000
         BALR  @14,@15                                             0301 00389000
*             RESPECIFY                                            0302 00390000
*               CMZR0009 UNRSTD;                                   0302 00391000
*           END;                                                   0303 00392000
*           RC=0;                                                  0304 00393000
         SLR   @07,@07                                             0304 00394000
         STH   @07,RC                                              0304 00395000
*         END;                                                     0305 00396000
*       ELSE                                                       0306 00397000
*         DO;                                                      0306 00398000
*                                                                  0306 00399000
         B     @RC00294                                            0306 00400000
@RF00294 DS    0H                                                  0307 00401000
*/********************************************************************/ 00402000
*/* IF THE RETURN CODE FROM PHR = 8, 12, OR 16 INDICATING AN END OF  */ 00403000
*/* STRING OR SYNTAX OR OTHER ERROR, THE FOLLOWING ROUTINE IS        */ 00404000
*/* EXECUTED. FOR AN ERROR CONDITION, IT PUTS OUT AN ERROR MSG WITH  */ 00405000
*/* A RETURN CODE.  FOR THE END OF STRING CONDITION, DEVICE          */ 00406000
*/* DEPENDENT (MOSTLY) BITS ARE SET IN THE LINE ENTRY.               */ 00407000
*/********************************************************************/ 00408000
*                                                                  0307 00409000
*CALLRC4:                                                          0307 00410000
*           CALL CTMRCGT4;                                         0307 00411000
CALLRC4  BAL   @14,CTMRCGT4                                        0307 00412000
*CTMRET01:                                                         0308 00413000
*           ;                                                      0308 00414000
CTMRET01 DS    0H                                                  0309 00415000
*         END;                                                     0309 00416000
*     END;                                                         0310 00417000
@RC00294 DS    0H                                                  0311 00418000
*   GPR1=KWDPTR;                    /* RESTORE PTR TO NEXT KEYWORD   */ 00419000
@RC00286 L     GPR1,KWDPTR                                         0311 00420000
*   MLCKWD=GPR1;                    /* IT IS NOW CURRENT K.W.        */ 00421000
         ST    GPR1,MLCKWD(,MLCAREG)                               0312 00422000
*   GPR14=ADDR(DOLOOP);                                            0313 00423000
         LA    GPR14,DOLOOP                                        0313 00424000
*CTMRCGT4:                                                         0314 00425000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0314 00426000
         B     @PB00003                                            0314 00427000
CTMRCGT4 DS    0H                                                  0315 00428000
*   IF RC=8                         /* PARM STRING EXHAUSTED         */ 00429000
*     THEN                                                         0315 00430000
         CLC   RC(2),@CH00036                                      0315 00431000
         BNE   @RF00315                                            0315 00432000
*     DO;                                                          0316 00433000
*                                                                  0316 00434000
*/********************************************************************/ 00435000
*/* DIAL                                                             */ 00436000
*/********************************************************************/ 00437000
*                                                                  0317 00438000
*       IF MLCMACRO='VTERM   ' THEN                                0317 00439000
         CLC   MLCMACRO(8,MLCAREG),@CC01290                        0317 00440000
         BNE   @RF00317                                            0317 00441000
*         DEVTCODE=DEVMTA;          /* ALL VTERMS ARE MTA    @DM12891*/ 00442000
         L     @07,MLCPTR4(,MLCAREG)                               0318 00443000
         MVI   DEVTCODE(@07),X'28'                                 0318 00444000
*       ELSE                                                       0319 00445000
*         DO;                       /* IF TERM, COMP, UTERM, ETC.    */ 00446000
         B     @RC00317                                            0319 00447000
@RF00317 DS    0H                                                  0320 00448000
*/********************************************************************/ 00449000
*/* ALL START/STOP LINES HAVE THE SAME LINE CONTROL DICIPLINE FOR    */ 00450000
*/* ALL DEVICES. CHECK DEVICES FOR START/STOP TYPE AND SET LC        */ 00451000
*/* DICIPLINE AS INDICATED.                                          */ 00452000
*/********************************************************************/ 00453000
*                                                                  0320 00454000
*           IF DEVTCODE=DEV3284|DEVTCODE=DEV3286 THEN              0320 00455000
         L     @07,MLCPTR4(,MLCAREG)                               0320 00456000
