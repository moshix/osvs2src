         LA    CMZR0016,CMZP0016                                   0512 01090000
*         CALL ISTSDCC2;            /* CALL ERROR FORMAT ROUTINE     */ 01091000
         L     @15,@CV01299                                        0513 01092000
         BALR  @14,@15                                             0513 01093000
*         RESPECIFY                                                0514 01094000
*           CMZR0016 UNRSTD;                                       0514 01095000
*       END;                                                       0515 01096000
*       RCPLGNCT=BLANK8;                                           0516 01097000
         L     @07,MLCPTR3(,MLCAREG)                               0516 01098000
         MVC   RCPLGNCT(8,@07),@CC01247                            0516 01099000
*     END;                                                         0517 01100000
*   GPR15=0;                                                       0518 01101000
@RC00505 SLR   GPR15,GPR15                                         0518 01102000
*   GPR14=ADDR(MAINSUB);                                           0519 01103000
         LA    GPR14,MAINSUB                                       0519 01104000
*   END TRMLGTAB;                                                  0520 01105000
@EL00011 DS    0H                                                  0520 01106000
@EF00011 DS    0H                                                  0520 01107000
@ER00011 BR    @14                                                 0520 01108000
*                                                                  0521 01109000
*/********************************************************************/ 01110000
*/*           POLL PARAMETER SUBROUTINE                              */ 01111000
*/********************************************************************/ 01112000
*                                                                  0521 01113000
*TRMPOLL:                                                          0521 01114000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0521 01115000
*                                                                  0521 01116000
TRMPOLL  DS    0H                                                  0522 01117000
*/********************************************************************/ 01118000
*/* THIS ROUTINE WILL MOVE THE POLLING CHARACTERS TO THE RDT         */ 01119000
*/* EXTENSION AREA. THE LENGTH AND POINTER TO THESE CHARACTERS ARE   */ 01120000
*/* FOUND IN THE VTAM PARAMETER HANDLER FUNCTION ONE OUTPUT AREA     */ 01121000
*/********************************************************************/ 01122000
*                                                                  0522 01123000
*   GPR0=PHRF1SLN;                  /* SET UP PARMS FOR CONVERSION 0522 01124000
*                                      RTN                           */ 01125000
         L     GPR0,PHRF1SLN(,PHR1REG)                             0522 01126000
*   GPR1=PHRF1SAD;                  /* GPR0=LENGTH OF STRING - GPR1 =   01127000
*                                      PTR                           */ 01128000
         L     GPR1,PHRF1SAD(,PHR1REG)                             0523 01129000
*   CALL ISTSDCC4;                  /* TO STRING.                    */ 01130000
         L     @15,@CV01252                                        0524 01131000
         BALR  @14,@15                                             0524 01132000
*   GPR1=0;                         /* SET UP PARMS FOR AREA HANDLER    01133000
*                                      RTN.                          */ 01134000
         SLR   GPR1,GPR1                                           0525 01135000
*   GPR0=PHRF1SLN/2;                /* HEX SIZE IS EBCDIC/2          */ 01136000
         L     @08,PHRF1SLN(,PHR1REG)                              0526 01137000
         SRDA  @08,32                                              0526 01138000
         D     @08,@CF00180                                        0526 01139000
         LR    GPR0,@09                                            0526 01140000
*   RTRCTPOL=GPR0;                  /* NUMB OF CHARS IN POLL STRING  */ 01141000
         L     @07,MLCPTR3(,MLCAREG)                               0527 01142000
         STH   GPR0,RTRCTPOL(,@07)                                 0527 01143000
*   CALL ISTSDCC1;                  /* CHARS IN BYTES.               */ 01144000
         L     @15,@CV01249                                        0528 01145000
         BALR  @14,@15                                             0528 01146000
*   IF GPR15=0                      /* IF STORAGE WAS AVAIL. FOR   0529 01147000
*                                      POLLING                       */ 01148000
*     THEN                                                         0529 01149000
         SLR   @07,@07                                             0529 01150000
         CR    GPR15,@07                                           0529 01151000
         BNE   @RF00529                                            0529 01152000
*     DO;                           /* CHARS MOVE THEM TO THE RDT  0530 01153000
*                                      EXT.                          */ 01154000
*       POLLEXT(1:RTRCTPOL)=POLLCHAR(1:RTRCTPOL);                  0531 01155000
         L     @04,MLCPTR3(,MLCAREG)                               0531 01156000
         LH    @03,RTRCTPOL(,@04)                                  0531 01157000
         BCTR  @03,0                                               0531 01158000
         L     @09,PHRF1SAD(,PHR1REG)                              0531 01159000
         EX    @03,@SM01905                                        0531 01160000
*       RTRPOLA=GPR0;               /* SAVE RELATIVE OFFSET OF POLL     01161000
*                                      CHARS                         */ 01162000
         ST    GPR0,RTRPOLA(,@04)                                  0532 01163000
*       LCLFPOLL=ON;                /* SET POLL FOOTPRINT    @DM08373*/ 01164000
         OI    LCLFPOLL,B'00001000'                                0533 01165000
*       GPR15=0;                                                   0534 01166000
         LR    GPR15,@07                                           0534 01167000
*     END;                                                         0535 01168000
*   ELSE                                                           0536 01169000
*     DO;                                                          0536 01170000
         B     @RC00529                                            0536 01171000
@RF00529 DS    0H                                                  0537 01172000
*       GPR15=4;                    /* MSG ALREADY PUT OUT BY AREA 0537 01173000
*                                      HANDLER                       */ 01174000
         LA    GPR15,4                                             0537 01175000
*     END;                                                         0538 01176000
*   GPR14=ADDR(MAINSUB);                                           0539 01177000
@RC00529 LA    GPR14,MAINSUB                                       0539 01178000
*   END TRMPOLL;                                                   0540 01179000
@EL00012 DS    0H                                                  0540 01180000
@EF00012 DS    0H                                                  0540 01181000
@ER00012 BR    @14                                                 0540 01182000
*                                                                  0541 01183000
*/********************************************************************/ 01184000
*/*        XMITLIM PARAMETER SUBROUTINE                              */ 01185000
*/********************************************************************/ 01186000
*                                                                  0541 01187000
*TRMXMTLM:                                                         0541 01188000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0541 01189000
*                                                                  0541 01190000
TRMXMTLM DS    0H                                                  0542 01191000
*/********************************************************************/ 01192000
*/* THIS ROUTINE WILL MOVE IF SPECIFIED, THE NUMBER OF TRANSMISSIONS */ 01193000
*/* THE NCP WILL RECEIVE FROM OR SEND TO THIS STATION OR BOTH. IF NO */ 01194000
*/* NUMBER IS SPECIFIED A CHECK IS MADE TO SEE IF 'NO' WAS THE PARM. */ 01195000
*/* IF SO THERE IS NO LIMIT ON TRANSMISSIONS. IF NEITHER A COUNT OR  */ 01196000
*                                                                  0542 01197000
*   IF PHRF34SP(1)='000000NO'|PHRF34SL(1)=0 THEN/* TO SEE IF PARM WAS   01198000
*                                      'NO'                          */ 01199000
         CLC   PHRF34SP(8,PHR1REG),@CC01436                        0542 01200000
         BE    @RT00542                                            0542 01201000
         LH    @07,PHRF34SL(,PHR1REG)                              0542 01202000
         LTR   @07,@07                                             0542 01203000
         BNZ   @RF00542                                            0542 01204000
@RT00542 DS    0H                                                  0543 01205000
*     RTRTRMAX=0;                   /* IF 'NO' SET XMISSION CNT=0    */ 01206000
         L     @07,MLCPTR3(,MLCAREG)                               0543 01207000
         MVI   RTRTRMAX(@07),X'00'                                 0543 01208000
*   ELSE                                                           0544 01209000
*     DO;                           /* A NUM WAS SPECIFIED           */ 01210000
         B     @RC00542                                            0544 01211000
@RF00542 DS    0H                                                  0545 01212000
*       PACK(PHRF34SP(1),PHRF34SP(1));/* CONVERT IT TO BINARY        */ 01213000
         PACK  PHRF34SP(8,PHR1REG),PHRF34SP(8,PHR1REG)             0545 01214000
*       CVB(CVBNUM,PHRF34SP(1));    /* AND CK FOR VALID              */ 01215000
         CVB   @07,PHRF34SP(,PHR1REG)                              0546 01216000
         ST    @07,CVBNUM                                          0546 01217000
*       RTRTRMAX=CVBNUM2;           /* IF OK - STORE IT IN           */ 01218000
         L     @07,MLCPTR3(,MLCAREG)                               0547 01219000
         MVC   RTRTRMAX(1,@07),CVBNUM2                             0547 01220000
*     END;                                                         0548 01221000
*   GPR15=0;                        /* SET RTN CODE TO 0             */ 01222000
@RC00542 SLR   GPR15,GPR15                                         0549 01223000
*   GPR14=ADDR(MAINSUB);                                           0550 01224000
         LA    GPR14,MAINSUB                                       0550 01225000
*   END TRMXMTLM;                                                  0551 01226000
@EL00013 DS    0H                                                  0551 01227000
@EF00013 DS    0H                                                  0551 01228000
@ER00013 BR    @14                                                 0551 01229000
*TRMFEATR:                                                         0552 01230000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0552 01231000
*                                                                  0552 01232000
TRMFEATR DS    0H                                                  0553 01233000
*/********************************************************************/ 01234000
*/* TURN OFF PREVIOUSLY NCP SPECIFIED FEATURES BEFORE SETTING TRM    */ 01235000
*/* INDICATED NCP FEATURES.                                          */ 01236000
*/********************************************************************/ 01237000
*                                                                  0553 01238000
*   RTRREACR='0'B;                  /* ACCELERATED CARRIAGE RETURN   */ 01239000
*   RTRREATT='0'B;                  /* TERMINAL INTERRUPT (ATTENTION)*/ 01240000
*   RTRREBRK='0'B;                  /* TRANSMIT INTERRUPT (REVRSE  0555 01241000
*                                      BREAK)                        */ 01242000
*   RTRRECHK='0'B;                  /* CHECKING                      */ 01243000
*   RTRRESCT='0'B;                  /* STATION CONTROL               */ 01244000
