         TITLE 'ISTSDCVG - SWITCHED/LOCAL SUBAREA SKELETON BUILD       *00001000
                        '                                               00002000
ISTSDCVG CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSDCVG  75.168'                                 0001 00008000
         DROP  @15                                                      00009000
