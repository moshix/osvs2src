*     END;                                                         0413 00185000
@RC00406 DS    0H                                                  0414 00186000
*   SAVREG=RNCSAV1(2);                                             0414 00187000
@RC00368 L     SAVREG,RNCSAV1+4(,RNCAPTR)                          0414 00188000
*   RETURN CODE(RC);                                               0415 00189000
         LH    @15,RC                                              0415 00190000
         L     @14,12(,@13)                                        0415 00191000
         LM    @00,@12,20(@13)                                     0415 00192000
         BR    @14                                                 0415 00193000
*                                                                  0416 00194000
*/********************************************************************/ 00195000
*/*                       VGROOM ROUTINE                             */ 00196000
*/* THIS ROUTINE GETS A BLOCK OF CONTIGUOUS STORAGE TO PUT THE RDT   */ 00197000
*/* TOGETHER. CALCULATION OF SIZE IS AS FOLLOWS:                     */ 00198000
*/*   LOCAL SUBAREA - ADD UP LENGTH USED IN EACH BUILD AREA BLOCK.   */ 00199000
*/*   SWITCHED SUBAREA - GO THROUGTH EACH ENTRY WITHIN THE BLOCKS    */ 00200000
*/*                      AND ADD UP ENTRY LENGTH PLUS THE LENGTH OF  */ 00201000
*/*                      ANY TABLES THE ENTRIES NEED.                */ 00202000
*/********************************************************************/ 00203000
*/* SEGMENT(VGROOM)START                                             */ 00204000
*                                                                  0416 00205000
*   DO;                                                            0416 00206000
*     GOTO VGROOM@;                                                0417 00207000
*VGROOM:                                                           0418 00208000
*   END;                                                           0418 00209000
VGROOM   DS    0H                                                  0419 00210000
*   BLOCKPTR=RNCFENT;               /* FIRST BLOCK                   */ 00211000
         L     BLOCKPTR,RNCFENT(,RNCAPTR)                          0419 00212000
*   ENTRYPTR=ADDR(BLDAREA);         /* FIRST ENTRY                   */ 00213000
         LA    ENTRYPTR,BLDAREA(,BLOCKPTR)                         0420 00214000
*   SKELSIZ=0;                      /* START AT ZERO                 */ 00215000
         SLR   @14,@14                                             0421 00216000
         ST    @14,SKELSIZ                                         0421 00217000
*   IF RNCGSWBD='1'B                /* SWITCHED SUBAREA BUILD?       */ 00218000
*     THEN                                                         0422 00219000
         TM    RNCGSWBD(RNCAPTR),B'00000010'                       0422 00220000
         BNO   @RF00422                                            0422 00221000
*     DO;                                                          0423 00222000
*       COUNT=0;                    /* START AT ZERO                 */ 00223000
         LR    COUNT,@14                                           0424 00224000
*       IF RSWDIALT=0               /* NO DNT?                       */ 00225000
*         THEN                                                     0425 00226000
         C     @14,RSWDIALT(,ENTRYPTR)                             0425 00227000
         BE    @RT00425                                            0425 00228000
*         ;                                                        0426 00229000
*       ELSE                                                       0427 00230000
*         SKELSIZ=SKELSIZ+(RSWDIALZ*LENGTH(ISTDNT));               0427 00231000
         LH    @14,RSWDIALZ(,ENTRYPTR)                             0427 00232000
         MH    @14,@CH02290                                        0427 00233000
         AL    @14,SKELSIZ                                         0427 00234000
         ST    @14,SKELSIZ                                         0427 00235000
*       IF RSWGRUPT=0               /* NO GNT?                       */ 00236000
*         THEN                                                     0428 00237000
