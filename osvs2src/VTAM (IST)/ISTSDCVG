         LTR   @14,@14                                             0428 00239000
         BZ    @RT00428                                            0428 00240000
*         ;                                                        0429 00241000
*       ELSE                                                       0430 00242000
*         SKELSIZ=SKELSIZ+(RSWGRUPZ*LENGTH(ISTGNT));               0430 00243000
         LH    @14,RSWGRUPZ(,ENTRYPTR)                             0430 00244000
         SLA   @14,3                                               0430 00245000
         AL    @14,SKELSIZ                                         0430 00246000
         ST    @14,SKELSIZ                                         0430 00247000
*       DO WHILE(BLOCKPTR^=0);                                     0431 00248000
@RT00428 B     @DE00431                                            0431 00249000
@DL00431 DS    0H                                                  0432 00250000
*         SKELSIZ=SKELSIZ+RPRELEN;  /* ADD IN ENTRY LENGTH           */ 00251000
         LH    @14,RPRELEN(,ENTRYPTR)                              0432 00252000
         AL    @14,SKELSIZ                                         0432 00253000
         ST    @14,SKELSIZ                                         0432 00254000
*         IF RPRENTRY=RPRENTCC      /* PU ENTRY?                     */ 00255000
*             &RCCPATHT^=0          /* AND PATH TABLE?               */ 00256000
*           THEN                                                   0433 00257000
         CLI   RPRENTRY(ENTRYPTR),X'71'                            0433 00258000
         BNE   @RF00433                                            0433 00259000
         L     @03,RCCPATHT(,ENTRYPTR)                             0433 00260000
         LTR   @03,@03                                             0433 00261000
         BZ    @RF00433                                            0433 00262000
*           SKELSIZ=SKELSIZ+(RCCPATHZ*LENGTH(ISTPAT));             0434 00263000
         LH    @03,RCCPATHZ(,ENTRYPTR)                             0434 00264000
         SLA   @03,3                                               0434 00265000
         ALR   @14,@03                                             0434 00266000
         ST    @14,SKELSIZ                                         0434 00267000
*         ELSE                                                     0435 00268000
*           ;                                                      0435 00269000
@RF00433 DS    0H                                                  0436 00270000
*         COUNT=COUNT+RPRELEN;                                     0436 00271000
         AH    COUNT,RPRELEN(,ENTRYPTR)                            0436 00272000
*         IF COUNT>=BLDARLNG        /* BLOCK FINISHED?               */ 00273000
*           THEN                                                   0437 00274000
