*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0236 00149000
*                                                                  0236 00150000
CVIPDATA DS    0H                                                  0237 00151000
*/********************************************************************/ 00152000
*/*  ROUTINE NAME : CVIPDATA -- VIDDATA BLOCK BUILD                  */ 00153000
*/*                                                                  */ 00154000
*/*  FUNCTION : THIS ROUTINE SEARCHES THE VIDLIST PARAMETER LIST     */ 00155000
*/*             FOR THE VIDSEQ KEYWORD PARAMETER. ONCE FOUND,THE     */ 00156000
*/*             VIDSEQ PARAMETER HANDLER (CVIPVSEQ) IS CALLED TO     */ 00157000
*/*             ANALYZE THE PARAMETER AND BUILD VIDDATA BLOCK.       */ 00158000
*/*                                                                  */ 00159000
*/*  INPUT : R3 - PTR TO VID HEADER BLOCK                            */ 00160000
*/*          R5 - PTR TO MLCA                                        */ 00161000
*/*          R6 - PTR TO RNCA                                        */ 00162000
*/*                                                                  */ 00163000
*/*  OUTPUT : BUILDSW SWITCH - ON : SUCCESSFUL VIDDATA BUILD         */ 00164000
*/*                            OFF : ERROR IN PARAMETER LIST OF      */ 00165000
*/*                                  VIDLIST,VIDHDR SHOULD BE DEQUEUD*/ 00166000
*/*                                                                  */ 00167000
*/*  ROUTINES CALLED : ISTINCPH -PHRFCN1,  PHR FUNCTION 1 (EXTERNAL) */ 00168000
*/*                    CVIPVSEQ -VIDSEQ PARM HANDLER (INTERNAL)      */ 00169000
*/*                                                                  */ 00170000
*/*  MACRO USED : ERRFMT                                             */ 00171000
*/*                                                                  */ 00172000
*/********************************************************************/ 00173000
*/*                         INITIALIZE SWITCHES                      */ 00174000
*/********************************************************************/ 00175000
*                                                                  0237 00176000
*   VIDSEQSW=OFF;                   /* VIDSEQ PARM NOT FOUND YET     */ 00177000
*   LOOPSW=ON;                      /* INIT LOOP SW TO SEARCH FOR  0238 00178000
*                                      VIDSEQ PARM                   */ 00179000
         OI    LOOPSW,B'00100000'                                  0238 00180000
         NI    VIDSEQSW,B'10111111'                                0238 00181000
*   NXTKYWRD=MLCKWD;                /* OBTAIN CURRENT KEYWORD PTR    */ 00182000
         L     @10,MLCKWD(,MLCAREG)                                0239 00183000
         ST    @10,NXTKYWRD                                        0239 00184000
*   DRETSAVE=GPR14;                 /* SAVE RETURN @ TO CALLER       */ 00185000
         ST    GPR14,DRETSAVE                                      0240 00186000
*/********************************************************************/ 00187000
*/* CALL PHR FUNCTION 1 TO REMOVE OUTMOST PARENTHESIS IF VIDSEQ      */ 00188000
*/* KEYWORD IS FOUND.                                                */ 00189000
*/********************************************************************/ 00190000
*                                                                  0241 00191000
*CVILLOOP:                                                         0241 00192000
*   MLCKWD=NXTKYWRD;                /* UPDATE MLCKWD TO NEXT PTR     */ 00193000
CVILLOOP L     @10,NXTKYWRD                                        0241 00194000
         ST    @10,MLCKWD(,MLCAREG)                                0241 00195000
*   GPR0=ADDR(VIDKWTBL);            /* PTR TO KEYWORD TABLE          */ 00196000
         LA    GPR0,VIDKWTBL                                       0242 00197000
*   GPR1=NXTKYWRD;                  /* PTR TO CURRENT KEYWORD        */ 00198000
         LR    GPR1,@10                                            0243 00199000
*   PHRREG=ADDR(RNCPHR1);           /* PTR TO PHR OUTPUT AREA        */ 00200000
         LA    PHRREG,RNCPHR1(,RNCAREG)                            0244 00201000
*   CALL ISTINCPH;                  /* CALL PHR ROUTINE -FN1         */ 00202000
         L     @15,@CV01222                                        0245 00203000
         BALR  @14,@15                                             0245 00204000
*   NXTKYWRD=GPR1;                  /* SAVE UPDATED STRING PTR       */ 00205000
         ST    GPR1,NXTKYWRD                                       0246 00206000
*/********************************************************************/ 00207000
*/* ANALYZE RETURN CODES FROM PHR                                    */ 00208000
*/* RC=0, SUCCESS                                                    */ 00209000
*/********************************************************************/ 00210000
*                                                                  0247 00211000
*   IF GPR15=0                      /* RC=0,VIDSEQ LOCATED           */ 00212000
*     THEN                                                         0247 00213000
         LTR   GPR15,GPR15                                         0247 00214000
         BNZ   @RF00247                                            0247 00215000
*     DO;                                                          0248 00216000
*       GPR15=GPR0;                 /* R15 PTR TO PARM HANDLER       */ 00217000
         LR    GPR15,GPR0                                          0249 00218000
*       CALL PARMHDLR;              /* CALL VIDSEQ PARM HANDLER-     */ 00219000
         BALR  @14,@15                                             0250 00220000
*     END;                          /* NORMAL CASE                   */ 00221000
*/********************************************************************/ 00222000
*/* RC=4,ARG NOT FOUND                                               */ 00223000
*/********************************************************************/ 00224000
*                                                                  0252 00225000
