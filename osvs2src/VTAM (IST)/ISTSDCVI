@RC00364 L     GPR14,SRETSAVE                                      0407 00817000
*   RETURN;                                                        0408 00818000
@EL00004 DS    0H                                                  0408 00819000
@EF00004 DS    0H                                                  0408 00820000
@ER00004 BR    @14                                                 0408 00821000
*CVIPANLZ:                                                         0409 00822000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0409 00823000
*                                                                  0409 00824000
CVIPANLZ DS    0H                                                  0410 00825000
*/********************************************************************/ 00826000
*/*                                                                  */ 00827000
*/*  ROUTINE NAME : CVIPANLZ -- SUBPARM ANALYZER                     */ 00828000
*/*                                                                  */ 00829000
*/*  FUNCTION : THIS ROUTINE CHECKS VALIDITY OF BSCID AND TERM NAME  */ 00830000
*/*             AND CALLS EBCHEX TO TRANSLATE BSCID TO HEX REPRESENT-*/ 00831000
*/*             ATION. IF VALID,SKELETON VIDDATA BLOCK IS BUILT.     */ 00832000
*/*                                                                  */ 00833000
*/*  INPUT : R2 -  PTR TO PHR OUTPUT AREA                            */ 00834000
*/*          R3 -  PTR TO VIDHDR                                     */ 00835000
*/*          R4 -  PTR TO VIDDATA BLOCK TO BE BUILT                  */ 00836000
*/*          R5 -  PTR TO MLCA                                       */ 00837000
*/*          R6 -  PTR TO RNCA                                       */ 00838000
*/*          BUILDSW=OFF,SET BY CVIPSUBP ROUTINE                     */ 00839000
*/*                                                                  */ 00840000
*/*  OUTPUT: BUILDSW =ON ,SUCCESSFUL BUILD                           */ 00841000
*/*                   OFF,INVALID BSCID OR TERM NAME                 */ 00842000
*/*                                                                  */ 00843000
*/*  ROUTINES CALLED : ISTSDCC4  -EBCHEX     (EXTERNAL)              */ 00844000
*/*                                                                  */ 00845000
*/*  MACRO USED:  ERRFMT                                             */ 00846000
*/********************************************************************/ 00847000
*                                                                  0410 00848000
*   ARETSAVE=GPR14;                 /* SAVE CALLER'S RET ADDR        */ 00849000
         LR    ARETSAVE,GPR14                                      0410 00850000
*/********************************************************************/ 00851000
*/* BSCID IS PUNCHED ON CARDS IN HEX REPRESENTATION OF ITS EBCDIC    */ 00852000
*/* CHARS WHICH IS AGAIN TRANSLATED INTO EBCDIC CODE BY THE TIME IT  */ 00853000
*/* RESIDES IN CORE -EXAMPLE: BSCID IS 'A'. IT IS PUNCHED AS 'C1' ON */ 00854000
*/* CARD. IN CORE,IT IS REPRESENTED AS X'C3F1'.                      */ 00855000
*/********************************************************************/ 00856000
*/* CHECK VALIDITY OF BSCID                                          */ 00857000
*/********************************************************************/ 00858000
*                                                                  0411 00859000
*   IF PHRF5SL(1)//2=0              /* IS BSCID LEN AN EVEN NO.      */ 00860000
*       &PHRF5SL(1)>1               /* AND >1 AND ^>40               */ 00861000
*       &PHRF5SL(1)^>40             /* AND ALL CHARS ALPHANUM ?      */ 00862000
*       &(PHRF5VF(1)=PHRVFAOK|PHRF5VF(1)=PHRVFNA1)/* YES- CONTINUE 0411 00863000
*                                      PROCESSING                    */ 00864000
*     THEN                                                         0411 00865000
*                                                                  0411 00866000
         LH    @10,PHRF5SL(,PHRREG)                                0411 00867000
         LR    @08,@10                                             0411 00868000
         SRDA  @08,32                                              0411 00869000
         D     @08,@CF00096                                        0411 00870000
         LTR   @08,@08                                             0411 00871000
         BNZ   @RF00411                                            0411 00872000
         C     @10,@CF00042                                        0411 00873000
         BNH   @RF00411                                            0411 00874000
         C     @10,@CF00061                                        0411 00875000
         BH    @RF00411                                            0411 00876000
         CLI   PHRF5VF(PHRREG),X'00'                               0411 00877000
         BE    @RT00411                                            0411 00878000
         CLI   PHRF5VF(PHRREG),X'10'                               0411 00879000
         BNE   @RF00411                                            0411 00880000
@RT00411 DS    0H                                                  0412 00881000
*/********************************************************************/ 00882000
*/* CALL EBCHEX TO TRANSLATE BSCID BACK TO HEX REPRESENTATION OF ITS */ 00883000
*/* EBCDIC CHARS,EX. TRANSLATE X'C3F1' TO X'C1' WHICH IS HEX OF C'A' */ 00884000
*/********************************************************************/ 00885000
*                                                                  0412 00886000
*CVILVAID:                                                         0412 00887000
*     DO;                                                          0412 00888000
CVILVAID DS    0H                                                  0413 00889000
*       GPR1=PHRF5SPT(1);           /* PTR TO SUBPARAM               */ 00890000
         L     GPR1,PHRF5SPT(,PHRREG)                              0413 00891000
*       GPR0=PHRF5SL(1);            /* LENGTH OF SUBPARAM            */ 00892000
         LH    GPR0,PHRF5SL(,PHRREG)                               0414 00893000
*       CALL ISTSDCC4;              /* CALL EBCHEX                   */ 00894000
         L     @15,@CV01224                                        0415 00895000
         BALR  @14,@15                                             0415 00896000
*       IF GPR15=0                  /* RC=0? SUCCESSFUL              */ 00897000
*         THEN                      /* YES BUILD SKEL VIDDATA        */ 00898000
         LTR   GPR15,GPR15                                         0416 00899000
         BNZ   @RF00416                                            0416 00900000
*CVILBULD:                                                         0417 00901000
*         DO;                                                      0417 00902000
*                                                                  0417 00903000
CVILBULD DS    0H                                                  0418 00904000
*/********************************************************************/ 00905000
*/* MOVE BSCID TO VIDDATA -R0 HAS LEN,R1 POINTS TO TRANSLATED BSCID  */ 00906000
*/********************************************************************/ 00907000
*                                                                  0418 00908000
*           DDTBSCLN=GPR0;          /* MOVE LEN OF BSCID TO VIDAT    */ 00909000
         STC   GPR0,DDTBSCLN(,DDTVIREG)                            0418 00910000
*           DDTBSCID=TRANID(1:GPR0);/* MOVE BSCID TO VIDDATA TRANID     00911000
*                                      IS PTRED BY R1                */ 00912000
         MVI   DDTBSCID+1(DDTVIREG),C' '                           0419 00913000
         MVC   DDTBSCID+2(21,DDTVIREG),DDTBSCID+1(DDTVIREG)        0419 00914000
         LR    @10,GPR0                                            0419 00915000
         BCTR  @10,0                                               0419 00916000
         EX    @10,@SM01398                                        0419 00917000
*           IF DDTBSCLN>DHDIDLEN    /* L'BSCID > MAX L'BSCID         */ 00918000
*             THEN                                                 0420 00919000
         SLR   @10,@10                                             0420 00920000
         IC    @10,DDTBSCLN(,DDTVIREG)                             0420 00921000
         LH    @09,DHDIDLEN(,DHDVIREG)                             0420 00922000
         N     @09,@CF01400                                        0420 00923000
         CR    @10,@09                                             0420 00924000
         BNH   @RF00420                                            0420 00925000
*             DHDIDLEN=DDTBSCLN;    /* REPLACE MAX LEN               */ 00926000
         STH   @10,DHDIDLEN(,DHDVIREG)                             0421 00927000
*           ELSE                                                   0422 00928000
*             ;                                                    0422 00929000
*                                                                  0422 00930000
@RF00420 DS    0H                                                  0423 00931000
*/********************************************************************/ 00932000
*/* CHECK VALIDITY OF TERM NAME                                      */ 00933000
*/********************************************************************/ 00934000
*                                                                  0423 00935000
*           IF PHRF5SL(2)>0         /* LEN OF TERM NAME >0           */ 00936000
*               &PHRF5SL(2)^>8      /* AND ^>8 AND IS A              */ 00937000
*               &PHRF5VF(2)=PHRVFAOK/* VALID SYMBOLIC NAME           */ 00938000
*/********************************************************************/ 00939000
*/* MOVE TERM NAME TO VIDDATA                                        */ 00940000
*/********************************************************************/ 00941000
*                                                                  0423 00942000
*             THEN                                                 0423 00943000
         LH    @10,PHRF5SL+8(,PHRREG)                              0423 00944000
         LTR   @10,@10                                             0423 00945000
         BNP   @RF00423                                            0423 00946000
         C     @10,@CF00048                                        0423 00947000
         BH    @RF00423                                            0423 00948000
         CLI   PHRF5VF+8(PHRREG),X'00'                             0423 00949000
         BNE   @RF00423                                            0423 00950000
*             DO;                   /* YES,MOVE                      */ 00951000
*               BUILDSW=ON;         /* SET BUILSW ON=SUCCESS         */ 00952000
         OI    BUILDSW,B'10000000'                                 0425 00953000
*               DDTTRNM=TERMNAME(1:PHRF5SL(2));/* TERMN TO VID       */ 00954000
         MVI   DDTTRNM+1(DDTVIREG),C' '                            0426 00955000
         MVC   DDTTRNM+2(6,DDTVIREG),DDTTRNM+1(DDTVIREG)           0426 00956000
         BCTR  @10,0                                               0426 00957000
         L     @09,PHRF5SPT+8(,PHRREG)                             0426 00958000
         EX    @10,@SM01401                                        0426 00959000
*               DHDNUME=DHDNUME+1;  /* INCREMENT NO. OF VIDDATA    0427 00960000
*                                      ENTRIES                       */ 00961000
         LA    @10,1                                               0427 00962000
         LH    @09,DHDNUME(,DHDVIREG)                              0427 00963000
         N     @09,@CF01400                                        0427 00964000
         ALR   @10,@09                                             0427 00965000
         STH   @10,DHDNUME(,DHDVIREG)                              0427 00966000
*             END;                                                 0428 00967000
*           ELSE                                                   0429 00968000
*             ;                     /* INVALID TERM NAME             */ 00969000
@RF00423 DS    0H                                                  0430 00970000
*         END CVILBULD;             /* END BUILD OF SKEL VIDDATA     */ 00971000
*       ELSE                                                       0431 00972000
*         ;                         /* RC FROM EBCHEX NOT 0          */ 00973000
@RF00416 DS    0H                                                  0432 00974000
*     END CVILVAID;                 /* END OF VALID BSCID LEG        */ 00975000
*   ELSE                                                           0433 00976000
*     ;                             /* INVALID BSCID LEG             */ 00977000
@RF00411 DS    0H                                                  0434 00978000
*   GPR14=ARETSAVE;                 /* RESTORE RETURN REG            */ 00979000
         LR    GPR14,ARETSAVE                                      0434 00980000
*   END CVIPANLZ;                   /* RETURN TO CALLER              */ 00981000
@EL00005 DS    0H                                                  0435 00982000
@EF00005 DS    0H                                                  0435 00983000
@ER00005 BR    @14                                                 0435 00984000
*CVIPQDAT:                                                         0436 00985000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0436 00986000
*                                                                  0436 00987000
CVIPQDAT DS    0H                                                  0437 00988000
*/********************************************************************/ 00989000
*/*                                                                  */ 00990000
*/*  ROUTINE NAME : CVIPQDAT -- QUEUE DATA BLOCK                     */ 00991000
*/*                                                                  */ 00992000
*/*  FUNCTION : THIS ROUTINE QUEUES THE SKELETON VIDDATA BLOCK TO    */ 00993000
*/*             THE VIDHDR IN ASCENDING ORDER OF BSCID.              */ 00994000
*/*                                                                  */ 00995000
*/*  INPUT : R3 - PTR TO VIDHDR                                      */ 00996000
*/*          R4 - PTR TO VIDDATA BLOCK TO BE QUEUED                  */ 00997000
*/*          R5 - PTR TO MLCA                                        */ 00998000
*/*          R6 - PTR TO RNCA                                        */ 00999000
*/*                                                                  */ 01000000
*/*  OUTPUT: BUILDSW= ON,SUCCESSFUL Q                                */ 01001000
*/*                   OFF,INVALID PARAM,DUPLICATE BSCID              */ 01002000
*/*                                                                  */ 01003000
*/*  REGISTERS USED RSTD: R7 -LSTPTR ,PTR TO LOC OF LSTFWPTR         */ 01004000
*/*                       R8 -NXTPTR ,PTR TO Q ELEMENT BEING SCANNED */ 01005000
*/*                                                                  */ 01006000
*/*  MACRO USED: ERRFMT                                              */ 01007000
*/*                                                                  */ 01008000
*/********************************************************************/ 01009000
*                                                                  0437 01010000
*   QRETSAVE=GPR14;                 /* SAVE RETURN ADDR              */ 01011000
         ST    GPR14,QRETSAVE                                      0437 01012000
*   RFY                                                            0438 01013000
*     LSTPTR RSTD;                  /* RESPECIFY REGS AS RESTRICT    */ 01014000
*   RFY                                                            0439 01015000
*     NXTPTR RSTD;                                                 0439 01016000
*                                                                  0439 01017000
*/********************************************************************/ 01018000
*/* QUEUE VIDDATA BLOCK TO VIDHDR IN ASCENDING ORDER OF BSCID        */ 01019000
*/********************************************************************/ 01020000
*                                                                  0440 01021000
*   LSTPTR=ADDR(DHDDTPTR);          /* LOC OF VIDDATA PTR IN HDR     */ 01022000
         LA    LSTPTR,DHDDTPTR(,DHDVIREG)                          0440 01023000
*   NXTPTR=DHDDTPTR;                /* PTR TO FIRST VIDDATA BLK      */ 01024000
         L     NXTPTR,DHDDTPTR(,DHDVIREG)                          0441 01025000
*/********************************************************************/ 01026000
*/* SEARCH LOOP                                                      */ 01027000
*/********************************************************************/ 01028000
*                                                                  0442 01029000
*CVILSRQU:                                                         0442 01030000
*   LOOPSRCH=OFF;                   /* INIT SEARCH LOOP SWITCH       */ 01031000
CVILSRQU NI    LOOPSRCH,B'11111011'                                0442 01032000
*   IF LSTFWPTR=0                   /* END OF CHAIN ?                */ 01033000
*     THEN                                                         0443 01034000
         L     @10,LSTFWPTR(,LSTPTR)                               0443 01035000
         LTR   @10,@10                                             0443 01036000
         BNZ   @RF00443                                            0443 01037000
*     LSTFWPTR=DDTVIREG;            /* YES,Q DATA BLOCK TO END OF  0444 01038000
*                                      CHAIN                         */ 01039000
         ST    DDTVIREG,LSTFWPTR(,LSTPTR)                          0444 01040000
*   ELSE                                                           0445 01041000
*     IF DDTVIREG->DDTBSCID<NXTPTR->DDTBSCID                       0445 01042000
*                                                                  0445 01043000
*/********************************************************************/ 01044000
*/*             IF BSCID OF BLOCK TO BE ADDED IS LESS THAN THAT OF   */ 01045000
*/*             Q ELEMENT,THEN Q IT IN FRONT OF Q ELEMENT            */ 01046000
*/********************************************************************/ 01047000
*                                                                  0445 01048000
*       THEN                                                       0445 01049000
         B     @RC00443                                            0445 01050000
@RF00443 CLC   DDTBSCID(23,DDTVIREG),DDTBSCID(NXTPTR)              0445 01051000
         BNL   @RF00445                                            0445 01052000
*       DO;                                                        0446 01053000
*         LSTFWPTR=DDTVIREG;        /* LAST FWD PTR=ADDR(NEW BLK)    */ 01054000
         ST    DDTVIREG,LSTFWPTR(,LSTPTR)                          0447 01055000
*         DDTVIREG->DDTFWPTR=NXTPTR;/* FWD PTR OF NEW BLOCK          */ 01056000
         ST    NXTPTR,DDTFWPTR(,DDTVIREG)                          0448 01057000
*       END;                        /* PTR TO NEXT Q ELEM            */ 01058000
*     ELSE                                                         0450 01059000
*       IF DDTVIREG->DDTBSCID=NXTPTR->DDTBSCID                     0450 01060000
*                                                                  0450 01061000
*/********************************************************************/ 01062000
*/* IF BSCID OF NEW ELEM = BSCID OF Q ELEM - ERROR CONDITION         */ 01063000
*/********************************************************************/ 01064000
*                                                                  0450 01065000
*         THEN                                                     0450 01066000
         B     @RC00445                                            0450 01067000
@RF00445 CLC   DDTBSCID(23,DDTVIREG),DDTBSCID(NXTPTR)              0450 01068000
         BNE   @RF00450                                            0450 01069000
*         DO;                                                      0451 01070000
*                                                                  0451 01071000
*/* ERRFMT    LEVEL(PARM)  ERROR(INVAL)  ACTION(IGNORE)  STRING('vidseq 01072000
*)                                                                   */ 01073000
*                                                                  0452 01074000
*           DO;                                                    0452 01075000
*             DCL                                                  0453 01076000
*               1 CMZP0017 STATIC LOCAL,/* PARAMETER LIST PASSED TO     01077000
*                                      ERROR FORMAT ROUTINE          */ 01078000
*                2 CMZL0017 FIXED(8) INIT(4),/* LEVEL PARAMETER    0453 01079000
*                                      (CLEV)                        */ 01080000
*                2 CMZE0017 FIXED(8) INIT(4),/* ERROR PARAMETER    0453 01081000
*                                      (CERR)                        */ 01082000
*                2 CMZA0017 CHAR(1) INIT('1'),/* ACTION PARAMETER  0453 01083000
*                                      (CACT)                        */ 01084000
*                2 CMZS0017 FIXED(8) INIT(6),/* LENGTH OF STRING   0453 01085000
*                                      PARAM                         */ 01086000
*                2 CMZM0017 CHAR(6) INIT('vidseq');/* PARAMETER    0453 01087000
*                                      STRING                        */ 01088000
*             DCL                                                  0454 01089000
*               CMZR0017 REGISTER(1);/* ESTABLISH PARAMeter          */ 01090000
*             RESPECIFY                                            0455 01091000
*               CMZR0017 RSTD;      /* REGISTER FOR                  */ 01092000
*             CMZR0017=ADDR(CMZP0017);/* ADDRESS OF PARAMETER LIST   */ 01093000
         LA    CMZR0017,CMZP0017                                   0456 01094000
*             CALL ISTSDCC2;        /* CALL ERROR FORMAT ROUTINE     */ 01095000
         L     @15,@CV01277                                        0457 01096000
         BALR  @14,@15                                             0457 01097000
*             RESPECIFY                                            0458 01098000
*               CMZR0017 UNRSTD;                                   0458 01099000
*           END;                                                   0459 01100000
*           BUILDSW=OFF;                                           0460 01101000
         NI    BUILDSW,B'01111111'                                 0460 01102000
*         END;                                                     0461 01103000
*                                                                  0461 01104000
*/********************************************************************/ 01105000
*/* BSCID OF NEW ELEM IS GREATER THAN Q ELEM,BUMP TO NEXT Q ELEM     */ 01106000
*/********************************************************************/ 01107000
*                                                                  0462 01108000
*       ELSE                                                       0462 01109000
*         DO;                                                      0462 01110000
