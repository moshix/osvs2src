*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0362 00628000
*                                                                  0362 00629000
CVIPSUBP DS    0H                                                  0363 00630000
*/********************************************************************/ 00631000
*/*                                                                  */ 00632000
*/*  ROUTINE NAME : CVIPSUBP -- SUBPARM HANDLER                      */ 00633000
*/*                                                                  */ 00634000
*/*  FUNCTION : THIS ROUTINE GETS THE STORAGE BLOCK FOR VIDDATA      */ 00635000
*/*             IT THEN CALLS CVIPANLZ -SUBPARM ANAALYZER ROUTINE    */ 00636000
*/*             TO CHECK VALIDITY OF BSCID AND TERMNAME AND BUILD    */ 00637000
*/*             THE SKELETON VIDDATA.                                */ 00638000
*/*                                                                  */ 00639000
*/*  INPUT : R2 - PTR TO PHR OUTPUT AREA                             */ 00640000
*/*          R3 - PTR TO VIDHDR                                      */ 00641000
*/*          R5 - PTR TO MLCA                                        */ 00642000
*/*          R6 - PTR TO RNCA                                        */ 00643000
*/*                                                                  */ 00644000
*/*  OUTPUT: BUILDSW =ON =SUCCESSFUL BUILD                           */ 00645000
*/*                   OFF : ERROR IN PARAM                           */ 00646000
*/*                                                                  */ 00647000
*/*  ROUTINES CALLED: ISTSDCC7 - WORK AREA HANDLER (EXTERNAL)        */ 00648000
*/*                   CVIPANLZ - SUBPARM ANALYZER  (INTERNAL)        */ 00649000
*/*                   CVIPQDAT - QUEUE DATA BLOCK  (INTERNAL)        */ 00650000
*/*  MACRO USED : ERRFMT                                             */ 00651000
*/********************************************************************/ 00652000
*                                                                  0363 00653000
*   SRETSAVE=GPR14;                 /* SAVE CALLER'S RET ADDR        */ 00654000
         ST    GPR14,SRETSAVE                                      0363 00655000
*   IF VALIDBR=ON                   /* IS THIS ROUTINE ENTERED       */ 00656000
*     THEN                          /* BY A CALL FROM VIDSEQ PARM    */ 00657000
         TM    VALIDBR,B'00001000'                                 0364 00658000
         BNO   @RF00364                                            0364 00659000
*CVILVLBR:                                                         0365 00660000
*     DO;                           /* HANDLER ? YES,CONTINUE        */ 00661000
CVILVLBR DS    0H                                                  0366 00662000
*       BUILDSW=OFF;                /* INIT BUILDSW TO OFF           */ 00663000
         NI    BUILDSW,B'01111111'                                 0366 00664000
*/********************************************************************/ 00665000
*/* IF NO. OF SUB-PARAMS IS TWO,IT IS CORRECT,SO CALL WORK AREA      */ 00666000
*/* HANDLER TO GET STORAGE FOR VIDDATA BLOCK                         */ 00667000
*/********************************************************************/ 00668000
*                                                                  0367 00669000
