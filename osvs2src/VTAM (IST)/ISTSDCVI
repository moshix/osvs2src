*         CMZR0011 RSTD;            /* REGISTER FOR                  */ 00461000
*       CMZR0011=ADDR(CMZP0011);    /* ADDRESS OF PARAMETER LIST     */ 00462000
         LA    CMZR0011,CMZP0011                                   0308 00463000
*       CALL ISTSDCC2;              /* CALL ERROR FORMAT ROUTINE     */ 00464000
         L     @15,@CV01277                                        0309 00465000
         BALR  @14,@15                                             0309 00466000
*       RESPECIFY                                                  0310 00467000
*         CMZR0011 UNRSTD;                                         0310 00468000
*     END;                                                         0311 00469000
*   ELSE                                                           0312 00470000
*     DO;                                                          0312 00471000
*                                                                  0312 00472000
         B     @RC00303                                            0312 00473000
@RF00303 DS    0H                                                  0313 00474000
*/********************************************************************/ 00475000
*/* NORMAL CASE - VIDSEQ KYWRD LOCATED FIRST TIME                    */ 00476000
*/********************************************************************/ 00477000
*                                                                  0313 00478000
*       VIDSEQSW=ON;                /* SET VIDSEQ TO FOUND           */ 00479000
         OI    VIDSEQSW,B'01000000'                                0313 00480000
*       IF PHRF1SLN>253             /* STRING EXCEED MAX LENGTH      */ 00481000
