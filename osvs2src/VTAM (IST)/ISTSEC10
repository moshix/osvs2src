*                     DO;           /* VALIDITY CHECK OK - SET UP       00271000
*                                      PROPER RU FOR RESPONSE        */ 00272000
*                                                               06S0438 00273000
*/********************************************************************/ 00274000
*/*                                                                  */ 00275000
*/*        SRU RESPONSE ACTION CODES ARE SET ACCORDING TO THE        */ 00276000
*/*        FOLLOWING TABLE:                                          */ 00277000
*/*                                                                  */ 00278000
*/*   ACTION   | SRUSPAC   SRUPSAC   SRUSPSN   SRUPSSN               */ 00279000
*/*   ---------|----------------------------------------             */ 00280000
*/*   RPLIRSET | SRRQRAC      -         0         -                  */ 00281000
*/*   RPLIPOS  | SRTSPAC      -      RPLIBSQV     -                  */ 00282000
*/*   RPLINEG  | SRTSNAC      -      RPLIBSQV     -                  */ 00283000
*/*   RPLIINV  | SRINVAC      -         0         -                  */ 00284000
*/*            |                                                     */ 00285000
*/*   RPLORSET |    -      SRRQRAC      -         0                  */ 00286000
*/*   RPLOPOS  |    -      SRTSPAC      -      RPLOBSQV              */ 00287000
*/*   RPLONEG  |    -      SRTSNAC      -      RPLOBSQV              */ 00288000
*/*   RPLOINV  |    -      SRINVAC      -         0                  */ 00289000
*/*                                                                  */ 00290000
*/********************************************************************/ 00291000
*                                                               06S0438 00292000
*                       IF RPLIRSET=ON THEN                     06S0438 00293000
         L     @10,PTRRPL                                       06S0438 00294000
         TM    RPLIRSET+60(@10),B'00100000'                     06S0438 00295000
         BNO   @RF00438                                         06S0438 00296000
*                         DO;                                   06S0439 00297000
*                           SRUSPAC=SRRQRAC;                    06S0440 00298000
         L     @10,PTRLCPB                                      06S0440 00299000
         LA    @10,DATA+3(,@10)                                 06S0440 00300000
         NI    SRUSPAC(@10),B'00111111'                         06S0440 00301000
*                           SRUSPSN=0;                          06S0441 00302000
         SLR   @00,@00                                          06S0441 00303000
         STCM  @00,3,SRUSPSN(@10)                               06S0441 00304000
*                         END;                                  06S0442 00305000
*                       ELSE                                    06S0443 00306000
*                         IF RPLIPOS=ON THEN                    06S0443 00307000
         B     @RC00438                                         06S0443 00308000
@RF00438 LA    @10,60                                           06S0443 00309000
         AL    @10,PTRRPL                                       06S0443 00310000
         TM    RPLIPOS(@10),B'00001000'                         06S0443 00311000
         BNO   @RF00443                                         06S0443 00312000
*                           DO;                                 06S0444 00313000
*                             SRUSPAC=SRTSPAC;                  06S0445 00314000
         L     @15,PTRLCPB                                      06S0445 00315000
         LA    @15,DATA+3(,@15)                                 06S0445 00316000
         NI    SRUSPAC(@15),B'01111111'                         06S0445 00317000
         OI    SRUSPAC(@15),B'01000000'                         06S0445 00318000
*                             SRUSPSN=RPLIBSQV;                 06S0446 00319000
         MVC   SRUSPSN(2,@15),RPLIBSQV(@10)                     06S0446 00320000
*                           END;                                06S0447 00321000
*                         ELSE                                  06S0448 00322000
*                           IF RPLINEG=ON THEN                  06S0448 00323000
         B     @RC00443                                         06S0448 00324000
@RF00443 LA    @10,60                                           06S0448 00325000
         AL    @10,PTRRPL                                       06S0448 00326000
         TM    RPLINEG(@10),B'00000100'                         06S0448 00327000
         BNO   @RF00448                                         06S0448 00328000
*                             DO;                               06S0449 00329000
*                               SRUSPAC=SRTSNAC;                06S0450 00330000
         L     @15,PTRLCPB                                      06S0450 00331000
         LA    @15,DATA+3(,@15)                                 06S0450 00332000
         OI    SRUSPAC(@15),B'11000000'                         06S0450 00333000
*                               SRUSPSN=RPLIBSQV;               06S0451 00334000
         MVC   SRUSPSN(2,@15),RPLIBSQV(@10)                     06S0451 00335000
*                             END;                              06S0452 00336000
*                           ELSE                                06S0453 00337000
*                             IF RPLIINV=ON THEN                06S0453 00338000
         B     @RC00448                                         06S0453 00339000
@RF00448 L     @10,PTRRPL                                       06S0453 00340000
         TM    RPLIINV+60(@10),B'00000010'                      06S0453 00341000
         BNO   @RF00453                                         06S0453 00342000
*                               DO;                             06S0454 00343000
