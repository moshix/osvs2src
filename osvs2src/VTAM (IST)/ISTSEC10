         L     @10,PTRLCPB                                      06S0455 00345000
         LA    @10,DATA+3(,@10)                                 06S0455 00346000
         NI    SRUSPAC(@10),B'10111111'                         06S0455 00347000
         OI    SRUSPAC(@10),B'10000000'                         06S0455 00348000
*                                 SRUSPSN=0;                    06S0456 00349000
         SLR   @00,@00                                          06S0456 00350000
         STCM  @00,3,SRUSPSN(@10)                               06S0456 00351000
*                               END;                            06S0457 00352000
*                       IF RPLORSET=ON THEN                     06S0458 00353000
@RF00453 DS    0H                                               06S0458 00354000
@RC00448 DS    0H                                               06S0458 00355000
@RC00443 DS    0H                                               06S0458 00356000
@RC00438 L     @10,PTRRPL                                       06S0458 00357000
         TM    RPLORSET+60(@10),B'00100000'                     06S0458 00358000
         BNO   @RF00458                                         06S0458 00359000
