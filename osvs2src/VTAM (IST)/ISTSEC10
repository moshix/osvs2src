*                           SRUPSAC=SRRQRAC;                    06S0460 00361000
         L     @10,PTRLCPB                                      06S0460 00362000
         LA    @10,DATA+3(,@10)                                 06S0460 00363000
         NI    SRUPSAC(@10),B'11001111'                         06S0460 00364000
*                           SRUPSSN=0;                          06S0461 00365000
