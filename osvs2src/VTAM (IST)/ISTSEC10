SVTM     TITLE 'ISTSEC10 - SESSION CONTROL OUTBOUND PROCESSOR          *00001000
                        '                                               00002000
* IBM OPERATING SYSTEM/VS2                                              00003000
* VTAM - 5742-SC-VTM                                                    00004000
ISTSEC10 CSECT ,                                                01S0002 00005000
@MAINENT BALR  @15,0                                            01S0002 00006000
         USING *,@15                                            01S0002 00007000
         B     @PROLOG                                          01S0002 00008000
         DC    AL1(16)                                          01S0002 00009000
         DC    C'ISTSEC10  75.130'                              01S0002 00010000
         DROP  @15                                                      00011000
@PROLOG  BALR  @11,0                                            01S0002 00012000
@PSTART  DS    0H                                               01S0002 00013000
         USING @PSTART,@11                                      01S0002 00014000
*   CRRSMAS=''B;                    /* CLEAR MODULE ACTIVE SWITCHES  */ 00015000
         L     @10,RPHCRR(,RPHPTR)                              01S0376 00016000
         LA    @15,12                                           01S0376 00017000
         AL    @15,CRACRR(,@10)                                 01S0376 00018000
         MVI   CRRSMAS(@15),X'00'                               01S0376 00019000
*   CRRMSC10=ON;                    /* TURN ON MODULE ACTIVE SWITCH  */ 00020000
         OI    CRRMSC10(@15),B'01000000'                        01S0377 00021000
*   CRRASC10=ON;                    /* AND AUDIT TRAIL SWITCH        */ 00022000
         OI    CRRASC10(@15),B'01000000'                        01S0378 00023000
*   RFY                                                         01S0379 00024000
*     R13 RSTD;                                                 01S0379 00025000
*   R13=ADDR(CRRSAREA);             /* SAVE AREA ADDRESS             */ 00026000
         LR    R13,@15                                          01S0380 00027000
*   RFY                                                         01S0381 00028000
*     CRRSAREA BASED(R13);                                      01S0381 00029000
*   GENERATE REFS(DATABASE) NOSETS(USING  @DATD,DATABASE);/* @DL29HDA*/ 00030000
         USING  @DATD,DATABASE                                          00031000
*   DATABASE=ADDR(CRRSAVE1);        /* CMPLR WORK ADDR       @DL29HDA*/ 00032000
         LA    DATABASE,CRRSAVE1(,R13)                          01S0383 00033000
*   PTRRPL=RPHWEA;                  /* GET RPL ADDRESS               */ 00034000
         L     @10,RPHWEA(,RPHPTR)                              01S0384 00035000
         ST    @10,PTRRPL                                       01S0384 00036000
*   RFY                                                         01S0385 00037000
*     IFGRPL BASED(PTRRPL);                                     01S0385 00038000
*   PTRFMCB=CRRFMCB;                /* GET FMCB ADDRESS              */ 00039000
         L     @00,CRRFMCB(,R13)                                01S0386 00040000
         ST    @00,PTRFMCB                                      01S0386 00041000
*   RFY                                                         01S0387 00042000
*     ISTFMCB BASED(PTRFMCB);                                   01S0387 00043000
*   PTRLCPB=CRRLCPB;                /* POINT TO LCPB CORE    @DL29HDA*/ 00044000
         L     @15,CRRLCPB(,R13)                                02S0388 00045000
         ST    @15,PTRLCPB                                      02S0388 00046000
*   RFY                                                         02S0389 00047000
*    (ISTDCLCP,                                                 02S0389 00048000
*     ISTLCPB) BASED(PTRLCPB);                                  02S0389 00049000
*   LCPPABA=ADDR(RPHMAJCB->ACDSIP); /* POINT TO SESSION CONTROL 02S0390 00050000
*                                      INBOUND PAB                   */ 00051000
         L     @14,RPHMAJCB-1(,RPHPTR)                          02S0390 00052000
         LA    @14,0(,@14)                                      02S0390 00053000
         LA    @14,ACDSIP(,@14)                                 02S0390 00054000
         ST    @14,LCPPABA(,@15)                                02S0390 00055000
*   DCLRPLA=PTRRPL;                 /* POINT LCPB TO RPL             */ 00056000
         ST    @10,DCLRPLA(,@15)                                02S0391 00057000
*   LCPNXLCW=ADDR(DCLLCCW2);        /* POINT TO 1ST LCCW             */ 00058000
         LA    @14,DCLLCCW2(,@15)                               02S0392 00059000
         ST    @14,LCPNXLCW(,@15)                               02S0392 00060000
*   LCPSAF=FMCSAF;                  /* PUT SOURCE ADDRESS INTO LCPB  */ 00061000
         LR    @09,@00                                          02S0393 00062000
         MVC   LCPSAF(2,@15),FMCSAF(@09)                        02S0393 00063000
*   LCPDAF=FMCDNCBA->NCBDAF;        /* ALSO DESTINATION ADDRESS      */ 00064000
         LR    @09,@00                                          02S0394 00065000
         L     @09,FMCDNCBA(,@09)                               02S0394 00066000
         MVC   LCPDAF(2,@15),NCBDAF(@09)                        02S0394 00067000
*   LCPASYN=ON;                     /* THIS IS ASYNCHRONOUS LCPB     */ 00068000
         OI    LCPASYN(@15),B'01000000'                         02S0395 00069000
*   PTRLCCW=ADDR(DCLLCCW2);         /* POINT TO LCCW                 */ 00070000
         ST    @14,PTRLCCW                                      02S0396 00071000
*   RFY                                                         02S0397 00072000
*     ISTLCCW BASED(PTRLCCW);                                   02S0397 00073000
*   IF RPLCLEAR=ON THEN             /* IF CLEAR WAS REQUESTED,       */ 00074000
         TM    RPLCLEAR+16(@10),B'01000000'                     02S0398 00075000
         BNO   @RF00398                                         02S0398 00076000
*     DO;                           /* SET UP CLEAR INFO     @DL29HDA*/ 00077000
*       LCCWOP=LCLEAR;              /* SET OP CODE TO CLEAR          */ 00078000
         MVI   LCCWOP(@14),X'F2'                                02S0400 00079000
*       LCPMNRCD=LCPMNAP;           /* PURGE MINOR RET CODE  @DL29HDA*/ 00080000
         MVI   LCPMNRCD(@15),X'0A'                              02S0401 00081000
*     END;                          /* END OF CLEAR SETUP    @DL29HDA*/ 00082000
*   ELSE                            /* CLEAR WAS NOT REQUESTED       */ 00083000
*     LCCWOP=LSEND;                 /* SET OP CODE TO SEND           */ 00084000
         B     @RC00398                                         02S0403 00085000
@RF00398 L     @10,PTRLCCW                                      02S0403 00086000
         MVI   LCCWOP(@10),X'F1'                                02S0403 00087000
*   IF RPLSTSN=ON THEN              /* IF SET&TEST WAS REQUESTED,    */ 00088000
@RC00398 LA    @10,16                                           02S0404 00089000
         AL    @10,PTRRPL                                       02S0404 00090000
         TM    RPLSTSN(@10),B'00100000'                         02S0404 00091000
         BNO   @RF00404                                         02S0404 00092000
*     DO;                           /* PROCESS THE SET&TEST REQUEST  */ 00093000
*       LCCWCNT=9;                  /* WILL PROBABLY SEND 9 BYTES    */ 00094000
         L     @15,PTRLCCW                                      03S0406 00095000
         MVC   LCCWCNT(2,@15),@CH01250                          03S0406 00096000
*       LCCWNTVC=ON;                /* VALIDITY CHECK IS NOT    03S0407 00097000
*                                      NECESSARY             @DL29HDA*/ 00098000
         OI    LCCWNTVC(@15),B'00010000'                        03S0407 00099000
*       LCCWDATA=ADDR(DATA);        /* POINT TO DATA AREA            */ 00100000
         L     @14,PTRLCPB                                      03S0408 00101000
         LA    @00,DATA(,@14)                                   03S0408 00102000
         ST    @00,LCCWDATA(,@15)                               03S0408 00103000
*       DATA(1:3)=RHBYTES;          /* PUT IN STANDARD RH BYTES      */ 00104000
         MVC   DATA(3,@14),@CB03292                             03S0409 00105000
*       DATA(4)=SRSTSN;             /* RU CONTROL BYTE FOR STSN      */ 00106000
         MVI   DATA+3(@14),X'A2'                                03S0410 00107000
*       IF RPLSRESP=ON THEN         /* IF THIS IS A RESPONSE,        */ 00108000
         TM    RPLSRESP(@10),B'10000000'                        03S0411 00109000
         BNO   @RF00411                                         03S0411 00110000
*         DO;                       /* CHECK FOR VALID RESPONSE      */ 00111000
*           LCCWLSI=ON;             /* INDICATE SYNCHRONOUS OPERATION*/ 00112000
         OI    LCCWLSI(@15),B'00000100'                         04S0413 00113000
*           RHQS=ON;                /* INDICATE RESPONSE             */ 00114000
         LA    @15,DATA(,@14)                                   04S0414 00115000
         OI    RHQS(@15),B'10000000'                            04S0414 00116000
*           IF RPLEX=ON THEN        /* IF THIS IS AN EXCEPTIONAL        00117000
*                                      RESPONSE,                     */ 00118000
         TM    RPLEX(@10),B'00000100'                           04S0415 00119000
         BNO   @RF00415                                         04S0415 00120000
*             DO;                   /* MUST CHECK FOR A SET FROM THE    00121000
*                                      LU                            */ 00122000
*               IF FMCIBSAC=SET|FMCOBSAC=SET THEN/* IF EITHER   04S0417 00123000
*                                      SEQUENCE NUMBER WAS SET       */ 00124000
         L     @10,PTRFMCB                                      04S0417 00125000
         TM    FMCIBSAC(@10),B'01000000'                        04S0417 00126000
         BNO   @GL00001                                         04S0417 00127000
         TM    FMCIBSAC(@10),B'10000000'                        04S0417 00128000
         BZ    @RT00417                                         04S0417 00129000
@GL00001 L     @10,PTRFMCB                                      04S0417 00130000
         TM    FMCOBSAC(@10),B'00010000'                        04S0417 00131000
         BNO   @RF00417                                         04S0417 00132000
         TM    FMCOBSAC(@10),B'00100000'                        04S0417 00133000
         BNZ   @RF00417                                         04S0417 00134000
@RT00417 DS    0H                                               04S0418 00135000
*                 DO;               /* THEN PROCESS EXCEPTIONAL 04S0418 00136000
*                                      RESPONSE                      */ 00137000
*                   DATASENS=0;     /* CLEAR ALL SENSE BYTES         */ 00138000
         L     @10,PTRLCPB                                      04S0419 00139000
         SLR   @00,@00                                          04S0419 00140000
         STCM  @00,15,DATASENS(@10)                             04S0419 00141000
*                   SRUSNS1=SRUFIE; /* FUNCTION INTERPRETOR ERROR       00142000
*                                                            @DL29HDA*/ 00143000
         LA    @15,DATA+3(,@10)                                 04S0420 00144000
         MVI   SRUSNS1(@15),X'10'                               04S0420 00145000
*                   SRUSNS2=SRUFNS; /* FUNCTION NOT SUPPORTED   04S0421 00146000
*                                                            @DL29HDA*/ 00147000
         MVI   SRUSNS2(@15),X'04'                               04S0421 00148000
*                   DATA(8)=SRSTSN; /* STSN COMMAND                  */ 00149000
         MVI   DATA+7(@10),X'A2'                                04S0422 00150000
*                   RHEXCEPT=ON;    /* EXCEPTIONAL RESPONSE IN RH,      00151000
*                                      TOO                           */ 00152000
         LA    @10,DATA(,@10)                                   04S0423 00153000
         OI    RHEXCEPT(@10),B'00010000'                        04S0423 00154000
*                   RHSENSE=ON;     /* SENSE BYTES PRESENT           */ 00155000
         OI    RHSENSE(@10),B'00000100'                         04S0424 00156000
*                   LCCWCNT=8;      /* SENDING ONLY 8 BYTES          */ 00157000
         L     @10,PTRLCCW                                      04S0425 00158000
         MVC   LCCWCNT(2,@10),@CH00037                          04S0425 00159000
*                   FMCAPRO=OFF;    /* APPLICATION HAS RESPONDED     */ 00160000
         L     @10,PTRFMCB                                      04S0426 00161000
         NI    FMCAPRO(@10),B'11111011'                         04S0426 00162000
*                 END;                                          04S0427 00163000
*               ELSE                /* NEITHER SEQUENCE NUMBER WAS      00164000
*                                      SET                           */ 00165000
*                 DO;               /* EXCEPTIONAL RESPONSE NOT 04S0428 00166000
*                                      ALLOWED                       */ 00167000
         B     @RC00417                                         04S0428 00168000
@RF00417 DS    0H                                               04S0429 00169000
*                   CRRRTNCD=CRRLOGIC;/* ERROR                       */ 00170000
         MVI   CRRRTNCD(R13),X'08'                              04S0429 00171000
*                   CRRFDB2=CRRACINV;/* INVALID ACTION REQUESTED     */ 00172000
         MVI   CRRFDB2(R13),X'69'                               04S0430 00173000
*                 END;                                          04S0431 00174000
*             END;                                              04S0432 00175000
*           ELSE                    /* NOT AN EXCEPTIONAL RESPONSE   */ 00176000
*             DO;                   /* VALIDATE ACTION CODES AND        00177000
*                                      FINISH DATA FIELD             */ 00178000
         B     @RC00415                                         04S0433 00179000
@RF00415 DS    0H                                               04S0434 00180000
*               IF(RPLORSET=ON|RPLOPOS=ON|RPLONEG=ON|RPLOINV=ON|RPLOIGN 00181000
*                   =ON)&           /* IF ANY OUTBOUND RESPONSE 05S0434 00182000
*                                      ACTION CODE AND               */ 00183000
*                   (RPLIRSET=ON|RPLIPOS=ON|RPLINEG=ON|RPLIINV=ON|      00184000
*                   RPLIIGN=ON) THEN/* ANY INBOUND RESPONSE ACTION      00185000
*                                      CODE WAS SET,                 */ 00186000
         LA    @10,60                                           05S0434 00187000
         AL    @10,PTRRPL                                       05S0434 00188000
         TM    RPLORSET(@10),B'00111110'                        05S0434 00189000
         BZ    @RF00434                                         05S0434 00190000
         TM    RPLIRSET(@10),B'00111110'                        05S0434 00191000
         BZ    @RF00434                                         05S0434 00192000
*                 DO;               /* CHECK FOR PROPER RESPONSE TO     00193000
*                                      LOGICAL UNIT REQUEST          */ 00194000
*                   IF((FMCOBSAC=SET&(RPLOPOS=ON|RPLORSET=ON))|/*       00195000
*                                      LU=SET, APPL=POS,RESET        */ 00196000
*                       (FMCOBSAC=SETTST&RPLOIGN=OFF)|/* LU=SET&TEST,   00197000
*                                      APPL=ANY BUT IGNORE           */ 00198000
*                       (FMCOBSAC=IGNORE&RPLOIGN=ON)|/* LU=IGNORE,      00199000
*                                      APPL=IGNORE                   */ 00200000
*                       (FMCOBSAC=INVALID&(RPLOPOS=OFF&RPLOIGN=OFF)))   00201000
*                                   /* LU=INVALID, APPL=ANY BUT 05S0436 00202000
*                                      POS,IGNORE                    */ 00203000
*                       &           /* THAT WAS OUTBOUND, NOW FOR       00204000
*                                      INBOUND SEQUENCE NUMBER       */ 00205000
*                       ((FMCIBSAC=SET&(RPLIPOS=ON|RPLIRSET=ON))|/*     00206000
*                                      LU=SET, APPL=POS,RESET        */ 00207000
*                       (FMCIBSAC=SETTST&RPLIIGN=OFF)|/* LU=SET&TEST,   00208000
*                                      APPL=ANY BUT IGNORE           */ 00209000
*                       (FMCIBSAC=IGNORE&RPLIIGN=ON)|/* LU=IGNORE,      00210000
*                                      APPL=IGNORE                   */ 00211000
*                       (FMCIBSAC=INVALID&(RPLIPOS=OFF&RPLIIGN=OFF)))   00212000
*                     THEN          /* LU=INVALID, APPL=ANY BUT 05S0436 00213000
*                                      POS,IGNORE                    */ 00214000
         L     @15,PTRFMCB                                      05S0436 00215000
         TM    FMCOBSAC(@15),B'00010000'                        05S0436 00216000
         BNO   @GL00006                                         05S0436 00217000
         TM    FMCOBSAC(@15),B'00100000'                        05S0436 00218000
         BNZ   @GL00006                                         05S0436 00219000
         TM    RPLOPOS(@10),B'00101000'                         05S0436 00220000
         BNZ   @GL00003                                         05S0436 00221000
@GL00006 L     @10,PTRFMCB                                      05S0436 00222000
         TM    FMCOBSAC(@10),B'00110000'                        05S0436 00223000
         BNO   @GL00005                                         05S0436 00224000
         L     @10,PTRRPL                                       05S0436 00225000
         TM    RPLOIGN+60(@10),B'00010000'                      05S0436 00226000
         BZ    @GL00003                                         05S0436 00227000
@GL00005 L     @10,PTRFMCB                                      05S0436 00228000
         TM    FMCOBSAC(@10),B'00110000'                        05S0436 00229000
         BNZ   @GL00004                                         05S0436 00230000
         L     @10,PTRRPL                                       05S0436 00231000
         TM    RPLOIGN+60(@10),B'00010000'                      05S0436 00232000
         BO    @GL00003                                         05S0436 00233000
@GL00004 L     @10,PTRFMCB                                      05S0436 00234000
         TM    FMCOBSAC(@10),B'00100000'                        05S0436 00235000
         BNO   @RF00436                                         05S0436 00236000
