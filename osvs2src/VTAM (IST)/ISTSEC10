*     END;                                                      02S0550 00654000
*   IF CRRRTNCD=0 THEN              /* IF NO ERROR OCCURRED,         */ 00655000
@RC00404 CLI   CRRRTNCD(R13),0                                  01S0551 00656000
         BNE   @RF00551                                         01S0551 00657000
*     DO;                           /* QUEUE LCPB TO ATOP            */ 00658000
*       IF RPLCLEAR=ON THEN         /* IF CLEAR REQUEST, MUST QUEUE     00659000
*                                      WITH RESET                    */ 00660000
