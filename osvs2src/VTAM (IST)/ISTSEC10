@RC00458 L     @10,PTRFMCB                                      05S0478 00421000
         NI    FMCAPRO(@10),B'11111011'                         05S0478 00422000
*                     END;                                      05S0479 00423000
*                   ELSE            /* NOT A PROPER RESPONSE FOR THE    00424000
*                                      LOGICAL UNIT REQUEST          */ 00425000
*                     DO;                                       05S0480 00426000
         B     @RC00436                                         05S0480 00427000
@RF00436 DS    0H                                               05S0481 00428000
*                       CRRRTNCD=CRRLOGIC;/* ERROR                   */ 00429000
         MVI   CRRRTNCD(R13),X'08'                              05S0481 00430000
