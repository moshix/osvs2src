@ER00002 BR    @14                                                 0532 01580000
@PB00002 DS    0H                                                  0533 01581000
*GEN(EJECT);                                                       0533 01582000
         EJECT                                                          01583000
*/********************************************************************/ 01584000
*/* ROUTINE NAME: C01MVDTA - MOVE LOGON DATA                         */ 01585000
*/*                                                                  */ 01586000
*/* FUNCTION: CALLED BY CLGNDTA1 TO MOVE LOGON DATA FROM BUFFER      */ 01587000
*/*           AREA POINTED AT BY ICEDATA TO USER AREA SPECIFIED      */ 01588000
*/*           IN RPLAREA.                                            */ 01589000
*/*                                                                  */ 01590000
*/* INPUT:  DATASIZE (R5) - SIZE OF LOGON DATA TO BE MOVED           */ 01591000
*/*         BFHDRPTR(R11) - POINTER TO BUFFER CONTAINING LOGON DATA  */ 01592000
*/*         CREGE   (R14) - NSI ADDR IN CALLING ROUTINE (CLGNDTA1)   */ 01593000
*/*                                                                  */ 01594000
*/* OUTPUT: CREGE   (R14) - NSI ADDR IN CALLING ROUTINE (CLGNDTA1)   */ 01595000
*/*         DATASIZE (R5) - LENGTH OF LOGON DATA MOVED               */ 01596000
*/*         CREG9    (R9) - SET TO 0 TO INDICATE SUCCESS             */ 01597000
*/*                                                                  */ 01598000
*/* ROUTINES CALLED: CTLNSRCH - NONE                                 */ 01599000
*/*                                                                  */ 01600000
*/* MACROS USED: NONE                                                */ 01601000
*/********************************************************************/ 01602000
*                                                                  0534 01603000
*C01MVDTA: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0534 01604000
         B     @PB00003                                            0534 01605000
C01MVDTA DS    0H                                                  0535 01606000
*OCWR14SV(4) = CREGE;                      /* SAVE RETURN ADDRESS  */   01607000
         L     @12,RPHWEA(,PTRRPH)                                 0535 01608000
         ST    CREGE,OCWR14SV+12(,@12)                             0535 01609000
*RFY CREG9 UNRSTD;       /* ALLOW USE IN THIS SUBROUTINE     @D32CKTQ*/ 01610000
*RFY (DATASIZE,LDATAPTR,CAREA,TEMPCNT) RSTD;                       0537 01611000
*LDATAPTR =ADDR(LGPLOGNP);   /* GET ADDRESS OF LOGON DATA    @D32CKTQ*/ 01612000
         LA    LDATAPTR,LGPLOGNP(,BFHDRPTR)                        0538 01613000
*CAREA=RPLAREA;              /* GET ADDRESS OF USER SUPPLIED AREA */    01614000
         L     @12,OCWRPL(,@12)                                    0539 01615000
         L     CAREA,RPLAREA(,@12)                                 0539 01616000
*CLGNT = DATASIZE;                     /* GET LOGON DATA SIZE */   0540 01617000
         LR    CLGNT,DATASIZE                                      0540 01618000
*RFY CREG8 RSTD;                   /* SAVE FOR USE AS SAVEREG@DM10724*/ 01619000
*CREG8=DATASIZE;     /* WE WILL NEED THIS VALUE TO PUT IN R0 @DM10724*/ 01620000
         LR    CREG8,DATASIZE                                      0542 01621000
*IF DATASIZE < BFHSIZE-LENGTH(LGPSESSP)-3 THEN             /*@D32CKTQ*/ 01622000
         LH    @12,BFHSIZE(,BFHDRPTR)                              0543 01623000
         SL    @12,@CF03790                                        0543 01624000
         CR    DATASIZE,@12                                        0543 01625000
         BNL   @RF00543                                            0543 01626000
*                        /* IF LOGON DATA IN ONE BUFFER THEN @D32CKTQ*/ 01627000
*  MVCL (CAREA,LDATAPTR);             /* MOVE DATA TO USER AREA */ 0544 01628000
         MVCL  CAREA,LDATAPTR                                      0544 01629000
*ELSE DO;                    /* LOGON DATA IN MORE THAN ONE BUFFER */   01630000
         B     @RC00543                                            0545 01631000
@RF00543 DS    0H                                                  0546 01632000
*  DATASIZE = BFHSIZE-LENGTH(LGPSESSP)-4;                  /*@D32CKTQ*/ 01633000
         LH    DATASIZE,BFHSIZE(,BFHDRPTR)                         0546 01634000
         SL    DATASIZE,@CF02956                                   0546 01635000
*                     /*LENGTH OF LOGON DATA IN FIRST BUFFER @D32CKTQ*/ 01636000
*  CLGNT=DATASIZE;             /* LENGTH OF USER AREA TO BE FILLED */   01637000
         LR    CLGNT,DATASIZE                                      0547 01638000
*  MVCL (CAREA,LDATAPTR);     /* MOVE LOGON DATA FROM FIRST BUFFER */   01639000
         MVCL  CAREA,LDATAPTR                                      0548 01640000
*/* SET WORK REG TO LNGTH OF LOGON DATA REMAINING IN BUFFERS:@D32CKTQ*/ 01641000
*  TEMPCNT = BINUSEL-(BFHSIZE-LENGTH(LGPSESSP)-4);         /*@D32CKTQ*/ 01642000
         LH    TEMPCNT,BFHSIZE(,BFHDRPTR)                          0549 01643000
         SL    TEMPCNT,@CF02956                                    0549 01644000
         LCR   TEMPCNT,TEMPCNT                                     0549 01645000
         SLR   @12,@12                                             0549 01646000
         IC    @12,BINUSEL+12(,BFHDRPTR)                           0549 01647000
         ALR   TEMPCNT,@12                                         0549 01648000
*  BFHDRPTR = BFHNXCHN;                /* GET NEXT BUFFER ADDRESS */    01649000
         L     BFHDRPTR,BFHNXCHN(,BFHDRPTR)                        0550 01650000
*  DO WHILE BFHDRPTR ^= 0; /* DO UNTIL DATA IN LAST BUFFER IS MOVED */  01651000
         B     @DE00551                                            0551 01652000
@DL00551 DS    0H                                                  0552 01653000
*    LDATAPTR=BFHDRPTR + LENGTH(ISTBFHDR);  /* INCREMENT POINTER BY     01654000
*                                         HEADER LENGTH */         0552 01655000
         LA    LDATAPTR,8                                          0552 01656000
         ALR   LDATAPTR,BFHDRPTR                                   0552 01657000
*    IF TEMPCNT >= BFHSIZE THEN          /* BUFFER COMPLETELY FILLED */ 01658000
         LH    @12,BFHSIZE(,BFHDRPTR)                              0553 01659000
         CR    TEMPCNT,@12                                         0553 01660000
         BL    @RF00553                                            0553 01661000
*      DATASIZE=BFHSIZE;         /* SET LENGTH OF DATA TO BE MOVED */   01662000
         LR    DATASIZE,@12                                        0554 01663000
*    ELSE                     /* BUFFER NOT COMPLETELY FILLED */   0555 01664000
*      DATASIZE=TEMPCNT;      /* SET LENGTH OF DATA TO BE MOVED */ 0555 01665000
         B     @RC00553                                            0555 01666000
@RF00553 LR    DATASIZE,TEMPCNT                                    0555 01667000
*    CLGNT=DATASIZE;         /* SET LENGTH OF DATA TO BE RECEIVED */    01668000
@RC00553 LR    CLGNT,DATASIZE                                      0556 01669000
*    TEMPCNT=TEMPCNT - DATASIZE;  /*DECREMENT SIZE OF DATA REMAINING */ 01670000
         SLR   TEMPCNT,DATASIZE                                    0557 01671000
*    MVCL (CAREA,LDATAPTR);  /* MOVE LOGON DATA TO USER AREA @DM10725*/ 01672000
         MVCL  CAREA,LDATAPTR                                      0558 01673000
*    BFHDRPTR=BFHNXCHN;        /*  GET NEXT BUFFER ADDRESS */      0559 01674000
         L     BFHDRPTR,BFHNXCHN(,BFHDRPTR)                        0559 01675000
*   END;                                  /* END OF DO WHILE */    0560 01676000
@DE00551 LTR   BFHDRPTR,BFHDRPTR                                   0560 01677000
         BNZ   @DL00551                                            0560 01678000
*  END;                                   /* END CHAINED BUFFER AREA */ 01679000
* CREGE = OCWR14SV(4);                    /* RESTORE RETURN ADDRESS*/   01680000
@RC00543 L     @12,RPHWEA(,PTRRPH)                                 0562 01681000
         L     CREGE,OCWR14SV+12(,@12)                             0562 01682000
* RFY CREG9 RSTD;       /* WAS USED AS WORK REG IN THIS SUBR @D32CKTQ*/ 01683000
* CREG9=GOODCODE;              /* SET EVERYTHING OK RETURN CODE */ 0564 01684000
         SLR   CREG9,CREG9                                         0564 01685000
* DATASIZE=CREG8;       /* REESTABLISH LENGTH OF LOGON  DATA @DM10725*/ 01686000
