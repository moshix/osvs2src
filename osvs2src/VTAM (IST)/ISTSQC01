*          3  LCPZTRV4 PTR(31);        /* RESERVED FOR RESTORE LCP   */ 00257000
*DCL LCTYPE BIT(8) CONSTANT('04'X);                                0255 00258000
*DCL PLCPTYPE BIT(8) CONSTANT('28'X);  /* PURGE LCPB TYPE ID         */ 00259000
*DCL LCPCLACB BIT(8) CONSTANT('04'X);  /* LCPB FROM CLOSE ACB        */ 00260000
*DCL LSRSTYPE BIT(8) CONSTANT('FF'X);  /* RESET SUBTYPE              */ 00261000
*DCL LCPETYPE BIT(8) CONSTANT('40'X);  /* EXTENDED LCPB TYPE ID      */ 00262000
*/*                                                                  */ 00263000
*/*  MINOR RETURN CODES                                              */ 00264000
*/*                                                                  */ 00265000
*DCL LCMCRSTR BIT(8) CONSTANT('F7'X),  /* MINOR RC FOR RESETSR       */ 00266000
