*/*VSETLCK  (CRETC,RELEASE,LOCAL,(LOCAL,ISTSQC01(C01OBT)))*/       0505 01505000
*C01REL:         DO;                                               0505 01506000
C01REL   DS    0H                                                  0506 01507000
*                                        /*DECLARE AND RESTRICT REGS */ 01508000
*                                        /*   2,10,11,12,13,14, AND 15  01509000
**/                                                                0506 01510000
*                RFY (GPR02P,GPR10P,GPR11P,GPR12P,GPR13P,GPR14P,GPR15P) 01511000
*RSTD;                                                             0506 01512000
*                                        /*SAVE REGS 2 AND 10 THRU 14*/ 01513000
*        GEN REFS(GPR02F,GPR10F,GPR11F,GPR12F,GPR14F,GPR13P);      0507 01514000
*                                                                  0507 01515000
         STM   GPR10F,GPR12F,0(GPR13P)                                  01516000
         ST    GPR14F,12(GPR13P)                                        01517000
         ST    GPR02F,16(GPR13P)                                        01518000
*                                        /*INVOKE MODESET TO CHANGE  */ 01519000
*                                        /*PSW PROTECTION KEY TO 0   */ 01520000
*        GEN SETS(2);                                              0508 01521000
*                                                                  0508 01522000
         MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2                      01523000
*                  GPR10P=GPR13P;        /*PUT SAVE AREA ADDR IN R 10*/ 01524000
         LR    GPR10P,GPR13P                                       0509 01525000
*                                        /*INVOKE OS/VS SETLOCK MACRO*/ 01526000
*                  GEN REFS(GPR13P,PSALITA,FLC) SETS(GPR12F);      0510 01527000
         SETLOCK RELEASE,TYPE=LOCAL,                                   *01528000
               RELATED=(LOCAL,ISTSQC01(C01OBT))                         01529000
*                  GPR15P=GPR13P;        /*PUT RETURN CODE IN R 15 */   01530000
         LR    GPR15P,GPR13P                                       0511 01531000
*                                        /*INVOKE MODESET MACRO TO   */ 01532000
*                    DO;                 /*RESTORE PSW PROTECTION KEY*/ 01533000
*        GEN SETS(2);                                              0513 01534000
         MODESET KEYADDR=(2),WORKREG=2                                  01535000
*                    END;                /* RESTORE REGISTERS        */ 01536000
*                  GEN REFS(GPR10F,GPR13P) SETS(GPR10F,GPR11F,     0515 01537000
*                  GPR12F,GPR13F,GPR14F,GPR02F);                   0515 01538000
         LR    GPR13F,GPR10F                                            01539000
         LM    GPR10F,GPR12F,0(GPR13P)                                  01540000
         L     GPR14F,12(GPR13P)                                        01541000
         L     GPR02F,16(GPR13P)                                        01542000
*                  IF GPR15F=4 THEN GPR15F=0;                      0516 01543000
