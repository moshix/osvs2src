         TITLE 'ISTSQC01 - LOGON DATA                                  *00001000
                        '                                               00002000
ISTSQC01 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSQC01  75.130'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @01,@12,24(@13)                                     0001 00011000
         BALR  @03,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@03                                         0001 00014000
*   CREG1=CREG1;                                                   0237 00015000
*                                                                  0237 00016000
*/********************************************************************/ 00017000
*/* ROUTINE NAME: ISTSQC01 - INQUIRE: TYPE = LOGON DATA (MAINLINE)   */ 00018000
*/*                                                                  */ 00019000
*/* FUNCTION: PERFORMS HOUSEKEEPING. TESTS TO SEE IF REQUEST IS FOR  */ 00020000
*/*           A VALID RDTE TYPE (LOCAL OR REMOTE TERMINAL, LOGICAL   */ 00021000
*/*           UNIT, OR COMPONENT) AND IF A LOGON ICE (INACTIVE       */ 00022000
*/*           EXISTS.  IF THE ICE BELONGS TO THE APPLICATION THAT    */ 00023000
*/*           IS REQUESTING THE DATA, CLGNDTA1 IS CALLED TO PROCESS  */ 00024000
*/*           THE ICE.                                               */ 00025000
*/*                                                                  */ 00026000
*/* INPUT:  CREG1   (R1) - POINTER TO RPL HEADER (ISTRPH)            */ 00027000
*/*         CREGD  (R13) - POINTER TO O/C SAVE AREA                  */ 00028000
*/*         CREGE  (R14) - NSI ADDRESS IN CALLING ROUTINE            */ 00029000
*/*                                                                  */ 00030000
*/* OUTPUT: CRETC  (R15) - RETURN CODE (4=UNSUCCESSFUL)              */ 00031000
*/*         CREGE  (R14) - NSI ADDRESS IN CALLING ROUTINE            */ 00032000
*/*                                                                  */ 00033000
*/* ROUTINES CALLED: CLGNDTA1 - TO CHECK LOGON DATA AND USER-        */ 00034000
*/*                             SUPPLIED BUFFER AREA                 */ 00035000
*/*                                                                  */ 00036000
*/* MACROS USED: CRRMAINT - TO ESTABLISH COMPONENT RECOVERY RECORD   */ 00037000
*/********************************************************************/ 00038000
*                                                                  0238 00039000
*                                                                  0238 00040000
*   DO;                                                            0238 00041000
*/***  CRRMAINT  PROCNM(ISTSQC01)COMP(NS)FUNC(SUB)  ***/           0239 00042000
*   DCL CMZK0005 PTR(31) REG(1);                                   0239 00043000
*   RFY (CMZK0005) RSTD;                                           0240 00044000
*                                                                  0240 00045000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTNSCRR)  */                  0241 00046000
*/* LOGICAL CHANNEL COMMAND WORD                                      * 00047000
*/* ADD A DECLARE                                      003 10/18/74   * 00048000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0241 00049000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 00050000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 00051000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 00052000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 00053000
*              4  * BIT(1),            /* RESERVED                    * 00054000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0241 00055000
*                                         ERASE OR READ CONT          * 00056000
*              4  LCCWCTL BIT(4),      /* CONTROL TYPE                * 00057000
*                 5  LCCWMODE BIT(2),  /* MODE TYPE                   * 00058000
*                 5  LCCWTYPE BIT(2),  /* OPERATION TYPE              * 00059000
*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00060000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00061000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00062000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00063000
*                                         RECORD                      * 00064000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00065000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00066000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00067000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00068000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00069000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00070000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00071000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00072000
*/* LOGICAL COMMAND CODES */                                       0242 00073000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00074000
*     LXEC     BIT(8) CONSTANT('4B'X), /* RESET AT END OF COMMAND     * 00075000
*     LXO      BIT(8) CONSTANT('5B'X), /* RESET ORDERLY               * 00076000
*     LCCWPRGE BIT(8) CONSTANT('6B'X), /* PURGE                       * 00077000
*     LWH      BIT(8) CONSTANT('0F'X), /* WRITE HEADER INDI. 1ST BLOCK* 00078000
*     LWRVI    BIT(8) CONSTANT('1F'X), /* WRITE RVI                   * 00079000
*     LWNA     BIT(8) CONSTANT('2F'X), /* WRITE NEGATIVE ACK          * 00080000
*     LWAA     BIT(8) CONSTANT('3F'X), /* WRITE ALTERNATE ACK         * 00081000
*     LWPLG    BIT(8) CONSTANT('5F'X), /* WRITE ACK LEADING GRAPHICS  * 00082000
*     LWNLG    BIT(8) CONSTANT('6F'X), /* WRITE NACK LEADING GRAPHICS * 00083000
*     LSMDE    BIT(8) CONSTANT('4F'X), /* SET MODE                    * 00084000
*     LIFBM    BIT(8) CONSTANT('7F'X), /* INDIC 1ST BLOCK OF MESSAGE  * 00085000
*     LTEST    BIT(8) CONSTANT('FE'X), /* TEST LCCW                   * 00086000
*     LPRMPT   BIT(8) CONSTANT('13'X), /* PREEMPT LCCW                * 00087000
*     LRSTR    BIT(8) CONSTANT('43'X), /* RESTORE LCCW                * 00088000
*     LEXEC    BIT(8) CONSTANT('FF'X), /* SPECIAL CONTROL OPERATION   * 00089000
*     LREAD    BIT(2) CONSTANT('10'B),  /* READ TYPE LCCW             * 00090000
*     LWRITE   BIT(2) CONSTANT('01'B),  /* WRITE TYPE LCCW            * 00091000
*     LCNTRL   BIT(2) CONSTANT('11'B),  /* CONTROL TYPE LCCW          * 00092000
*     LRESET   BIT(4) CONSTANT('1011'B),/* RESET COMMANDS             * 00093000
*     LTIC     BIT(8) CONSTANT('08'X),  /* LOGICAL TIC                * 00094000
*     LWR      BIT(6) CONSTANT('000001'B),  /* WRITE CONVERSATIONAL   * 00095000
*     LWB      BIT(6) CONSTANT('000101'B),  /* WRITE BLOCK            * 00096000
*     LWM      BIT(6) CONSTANT('001001'B),  /* WRITE MESSAGE          * 00097000
*     LWT      BIT(6) CONSTANT('001101'B),  /* WRITE TRANSMISSION     * 00098000
*     LEWM     BIT(6) CONSTANT('011001'B),  /* ERASE/WRITE MESSAGE    * 00099000
*     LEWT     BIT(6) CONSTANT('011101'B),  /* ERASE/WRITE TRNSMSSN   * 00100000
*     LEAU     BIT(6) CONSTANT('010001'B),  /* ERASE ALL UNPROTECTED  * 00101000
*     LRB      BIT(6) CONSTANT('000110'B),  /* READ BLOCK             * 00102000
*     LRM      BIT(6) CONSTANT('001010'B),  /* READ MESSAGE           * 00103000
*     LRT      BIT(6) CONSTANT('001110'B),  /* READ TRANSMISSION      * 00104000
*     LRMOD    BIT(6) CONSTANT('010010'B),  /* READ MODIFIED          * 00105000
*     LRBUF    BIT(6) CONSTANT('010110'B),  /* READ BUFFER            * 00106000
*     LRCONT   BIT(6) CONSTANT('011010'B),  /* READ CONTINUOUS        * 00107000
*     LRPEP    BIT(6) CONSTANT('011110'B),  /* READ PERPETUAL         * 00108000
*     LCPYM    BIT(6) CONSTANT('101001'B),  /* COPY MESSAGE           * 00109000
*     LCPYT    BIT(6) CONSTANT('101101'B),  /* COPY TRANSMISSION      * 00110000
*     LD       BIT(8) CONSTANT('23'X),  /* DISCONNECT                 * 00111000
*     LDEC     BIT(8) CONSTANT('33'X),  /* DISCONNECT WITH END OF CALL* 00112000
*     LEOC     BIT(8) CONSTANT('53'X),  /* END OF CALL COMMAND        * 00113000
*     LXEL     BIT(8) CONSTANT('0B'X),  /* RESET ERROR LOCK           * 00114000
*     LXC      BIT(8) CONSTANT('1B'X),  /* RESET CONDITIONAL          * 00115000
*     LXI      BIT(8) CONSTANT('2B'X),  /* RESET IMMEDIATE            * 00116000
*     LC       BIT(8) CONSTANT('03'X),  /* CONTACT                    * 00117000
*     LWPA     BIT(8) CONSTANT('FD'X); /* WRITE POSITIVE ACK          * 00118000
*DCL  LMT      BIT(2) CONSTANT('11'B), /* TRANSMISSION MODE           * 00119000
*     LMS      BIT(2) CONSTANT('00'B), /* SPECIAL MODE LIKE CONV, EAU * 00120000
*     LMB      BIT(2) CONSTANT('01'B), /* BLOCK MODE                  * 00121000
*     LMM      BIT(2) CONSTANT('10'B); /* MSG OR CONTINUOUS MODE      * 00122000
*/* FOLLOWING EQUATES FOR  - PORT SOLICITOR (DIAL)                    * 00123000
*/* PORT SOLICITOR COMMAND CODES                                      * 00124000
*DCL PSOL    BIT(8) CONSTANT('FE'X), /* SPECIAL PORT SOLICITOR     0244 00125000
*                                       COMMAND                       * 00126000
*    STCTL    BIT(8) CONSTANT('01'X), /* START CONTROL                * 00127000
*    RESTCTL  BIT(8) CONSTANT('02'X), /* RESET & CONTROL - REDRIVE    * 00128000
*    STIPUT   BIT(8) CONSTANT('03'X); /* START INPUT                  * 00129000
*/*   FOLLOWING EQUATES FOR SDLC SUPPORT                              * 00130000
*DCL  LCCHOLD  BIT(8) CONSTANT('01'X),  /* HOLD                       * 00131000
*     LCCRHOLD BIT(8) CONSTANT('02'X),  /* RELEASE HOLD               * 00132000
*     LSEND    BIT(8) CONSTANT ('F1'X), /* SEND                       * 00133000
*     LCLEAR   BIT(8) CONSTANT ('F2'X), /* CLEAR                      * 00134000
*     LCRUD    BIT(8) CONSTANT ('F3'X), /* CLEAR, UNBIND              * 00135000
*     LCRUDNFY BIT(8) CONSTANT ('F4'X), /* CLEAR, UNBIND, NOTIFY      * 00136000
*     LNFY     BIT(8) CONSTANT ('F5'X), /* NOTIFY                     * 00137000
*     LRELCB   BIT(8) CONSTANT ('F6'X), /* RELEASE CB                 * 00138000
*     LRESTSR  BIT(8) CONSTANT ('F7'X), /* RESETSR                    * 00139000
*     LCLRNFY  BIT(8) CONSTANT('F8'X),  /* CLEAR/NOTIFY               * 00140000
*     LCHPUR   BIT(8) CONSTANT('F9'X);  /* PURGE                      * 00141000
*DCL  LCCWSIZE FIXED(8) CONSTANT(8);    /* LCCW SIZE                  * 00142000
*DCL  LRDS     BIT(8) CONSTANT('FF'X);  /* RECORD DEV STATS COMMAND   * 00143000
*DCL  LQUIS    BIT(8) CONSTANT('FE'X);  /* QUIESCE COMMAND            * 00144000
*/*              LOGICAL CHANNEL PROGRAM BLOCK                       */ 00145000
*/*                                                                  */ 00146000
*/*  REVISIONS                                         006 03/24/75  */ 00147000
*/*                                                                  */ 00148000
*DCL  1  ISTLCPB BDY(WORD) BASED(LCPBPTR),                         0249 00149000
*        2 LCPTYPE CHAR(1),            /* TYPE CODE                  */ 00150000
*        2 LCPLNGTH  PTR(8),           /* LENGTH IN BYTES            */ 00151000
*        2 LCPREQTG CHAR(2),           /* LCPB REQ. TAG FIELD        */ 00152000
*          3 LCPFLAGS BIT(8),          /* FLAG BYTE                  */ 00153000
*            4 LCPZRT00 BIT(1),        /* LOGICAL ERROR FLAG         */ 00154000
*            4 LCPZRT01 BIT(1),        /* CONTROL BLOCK ERROR        */ 00155000
*            4 LCPZF001 BIT(1),        /* LCP START PROCESS FLAG     */ 00156000
*              5 LCPZRT02 BIT(1),      /* I/O INITIATION DISABLED    */ 00157000
*                6 LCPZRTL2 BIT(1),    /* END OF SCHEDULABLE BLOCK   */ 00158000
*            4 LCPZF002 BIT(1),        /* LCP IN PROCESSING FLAG     */ 00159000
*              5 LCPZRT03 BIT(1),      /* GO LOOK AT FMCB LINE STATE */ 00160000
*                                      /* FIELD (REQ. TAG OVERLAY)   */ 00161000
*                6 LCPZRTL3 BIT(1),    /* WRITE/READ REQUEST         */ 00162000
*            4 LCPZF003 BIT(1),        /* LCP PROCESSING COMPLETE FLAG* 00163000
*              5 LCPZRT04 BIT(1),      /* STPR SET IN BDU FLAG       */ 00164000
*                                      /* (REQ. TAG OVERLAY)         */ 00165000
*                6 LCPZRTL4 BIT(1),    /* FEEDBACK REQUESTED         */ 00166000
*            4 LCPZF004 BIT(1),        /* SINGLE LCCW/LCP FLAG       */ 00167000
*              5 LCPZRT05 BIT(1),      /* FEEDBACK TO BE ROUTED TO   */ 00168000
*                                      /* ICNCB FMCB (REQ. TAG       */ 00169000
*                                      /* OVERLAY)                   */ 00170000
*                6 LCPZRTL5 BIT(1),    /* DECREMENT SCHED COUNT ON DEQ* 00171000
*            4 LCPZRT06 BIT(1),        /* CRITICAL TEXT BLOCK        */ 00172000
*                                      /* FLAG (REQ. TAG OVERLAY)    */ 00173000
*              5 LCPZRTL6 BIT(1),      /* WRITE REQUEST              */ 00174000
*            4 LCPZRT07 BIT(1),        /* BUFFER TRACE ACTIVE        */ 00175000
*                                      /* FLAG (REQ. TAG OVERLAY)    */ 00176000
*              5 LCPZRTL7 BIT(1),      /* SCHEDULING FLAG            */ 00177000
*          3 LCPMNRCD CHAR(1),         /* LCP MINOR REJECTION CODE   */ 00178000
*            4 LCPZRT08 BIT(1),        /* I/O TRACE ACTIVE FLAG      */ 00179000
*                                      /* (REQ. TAG OVERLAY)         */ 00180000
*              5 LCPZRTL8 BIT(1),      /* DEVICE END NOTIFICATION    */ 00181000
*            4 LCPZRT09 BIT(1),        /* DIALOGUE SCHED DISABLED    */ 00182000
*              5 LCPZRTL9 BIT(1),      /* CRITICAL TEXT FLAG         */ 00183000
*            4 LCPZRT0A BIT(1),        /* SIMULATED CONTACT FLAG     */ 00184000
*            4 LCPZRT0B BIT(1),        /* SIMULATED DISCONNECT FLAG  */ 00185000
*        2 LCPCHAIN PTR(31),           /* APS CHAIN                  */ 00186000
*          3 LCPCHAI1 PTR(8),                                      0249 00187000
*            4 LCPCHNG BIT(1),         /* GATE BIT                   */ 00188000
*          3 LCPCHAI2 PTR(24),         /* ADDRESS FIELD              */ 00189000
*        2 LCPRUCNT PTR(8),            /* NUMBER OF RU FOR THIS LCPB */ 00190000
*          3 LCPZMIRC CHAR(1),         /* MINOR RETURN CODE          */ 00191000
*        2 LCPFLAG2 CHAR(1),           /* SECOND FLAG BYTE           */ 00192000
*          3 LCPPRFLG BIT(1),          /* PACING REQUIRED            */ 00193000
*          3 LCPASYN  BIT(1),          /* ASYNCHRONOUS LCPB          */ 00194000
*          3 LCPRSTRT BIT(1),          /* RESTART LCPB               */ 00195000
*          3 LCPTLBPI BIT(1),          /* TPIOS BLOCK LCCW           */ 00196000
*            4 LCPZRIMM BIT(1),        /* RESET IMMED REQUIRED ON ERR*/ 00197000
*          3 LCPFID0  BIT(1),          /* FID0 TH INDICATOR FOR RECORD* 00198000
*            4 LCPEXRES BIT(1),        /* EXCEP. RESPONSE SPEC. 3270R*/ 00199000
*          3 LCPDSTAT BIT(1),          /* DIALOGUE STATE BIT, 3270R  */ 00200000
*                                      /* 0=REQ. REJECT,1=STATE ERROR*/ 00201000
*          3 LCPFREE  BIT(1),          /* DEALLOCATE LCPB 3270R      */ 00202000
*          3 LCPATOP  BIT(1),          /* ATOP LCPB  3270R           */ 00203000
*        2 LCPSEQ   FIXED(16),         /* OUTBOUND SEQUENCE NUMBER   */ 00204000
*          3 LCPRRCCT PTR(8),          /* COUNT OF RESPONSES RECEIVED*/ 00205000
*          3 LCPRRCNT PTR(8),          /* COUNT OF RESPONSES REQUESTED* 00206000
*        2 LCPLCCW1 PTR(31),           /* ADDR FIRST LCCW FOR FID1   */ 00207000
*        2 LCPCID CHAR(4) BDY(WORD),   /* COMMUNICATION ID           */ 00208000
*          3 LCPSAF BIT(16),           /* SOURCE IDENTIFICATION      */ 00209000
*          3 LCPDAF BIT(16),           /* DESTINATION IDENTIFICATION */ 00210000
*        2 LCPNXLCW PTR(31),           /* ADDR NEXT LCCW             */ 00211000
*        2 LCPFDBA PTR(31),            /* BTU ADDRESS                */ 00212000
*          3 LCPFDB1 PTR(8),                                       0249 00213000
*          3 LCPFDB2 PTR(24),          /* ADDRESS FIELD              */ 00214000
*        2 LCPTLBAD PTR(31),           /* ADDRESS OF TLB             */ 00215000
*        2 LCPFOSN  BIT(16),           /* SEQUENCE NUMBER FIRST RU   */ 00216000
*        2 LCPFSNCH FIXED(15),         /* 1ST SEQ NUMBER IN RU CHAIN */ 00217000
*        2 LCPSTYPE FIXED(8),          /* LCPB SUB-TYPE X'FF' = RESET,* 00218000
*                                      /* X'00' = NON-RESET          */ 00219000
*        2 LCPFLG1 CHAR(1),            /* FLAGS                      */ 00220000
*          3 LCPRESET BIT(1),          /* A TPQUE WITH RESET QUEUED  */ 00221000
*                                      /* THIS ELEMENT               */ 00222000
*          3 LCPINTER BIT(1),          /* INTERNAL LCPB              */ 00223000
*          3 LCPDIRPT BIT(1),          /* NOT SEND W/POST=SCHED=0    */ 00224000
*          3 LCPREDRV BIT(1),          /* LCPB IS FOR REDRIVE        */ 00225000
*          3 LCPSTLB  BIT(1),          /* TLB NOT TO BE FREED        */ 00226000
*          3 LCPTRANS BIT(1),          /* DATA INCLUDES CONTROL CHARS*/ 00227000
*          3 LCPRSV78 BIT(2),          /* RESERVED                   */ 00228000
*        2 LCPLCCWC BIT(8),            /* COUNT OF LCCWS             */ 00229000
*        2 LCPRSV77 CHAR(1),           /* RESERVED                   */ 00230000
*        2 LCPFLCCW PTR(31),           /* ADDR OF FIRST LCCW         */ 00231000
*          3 LCPUBFSB PTR(31),         /* UNBIND FSB ADDRESS         */ 00232000
*        2 LCPCLREP PTR(31),           /* PTR TO CLEAR LCPB IN UCB   */ 00233000
*/*  NOTE:  THE FOLLOWING FIELD - LCPLCCW MUST BE THE LAST FIELD     */ 00234000
*/*         IN THIS CONTROL BLOCK.  THE CONTROL BLOCK ISTDCLCP HAS   */ 00235000
*/*         FIELDS IN IT WHICH MUST BE CONTIGUOUS WITH LCPLCCW       */ 00236000
*        2 LCPLCCW  CHAR(LENGTH(ISTLCCW)) BDY(WORD), /* FIRST LCCW   */ 00237000
*          3 LCPRSTQH PTR(31),         /* RESET LCPB QUEUE HEADER    */ 00238000
*          3 LCPPABA  PTR(31),         /* PAB POINTER                */ 00239000
*            4 LCPRPHA  PTR(31),       /* RPH POINTER                */ 00240000
*              5 LCPFTYPE BIT(1);      /* FEEDBACK TYPE              */ 00241000
*/*     CONSTANTS FOR LCPTYPE                                         * 00242000
*DCL  LCRPHFT  BIT(1) CONSTANT('1'B);                              0250 00243000
*DCL  LCPABFT  BIT(1) CONSTANT('0'B);                              0251 00244000
*DCL LCPBFIRM BIT(8) CONSTANT('10'X);  /* LU FAILED, CLSDST, SCHED LOST 00245000
*                                         TERM, OPNDST MAY BE TRIED   * 00246000
*DCL LCPBTERM BIT(8) CONSTANT('11'X);  /* TERM RECEIVED,CLSDST,    0253 00247000
*                                         SCHED LOST TERM             * 00248000
*/*                                                                   * 00249000
*                                                                  0254 00250000
*DCL 1 LCPZTIE BDY(WORD) DEFINED(LCPCID), /* TOLTEP INTERFACE      0254 00251000
*                                         ELEMENT OVERLAY            */ 00252000
*       2  LCPZFMCA PTR(31),           /* ACTIVE FMCB ADDRESS        */ 00253000
