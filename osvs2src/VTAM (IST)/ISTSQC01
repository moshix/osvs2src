*UNRSTD;         END;  /*C01REL:VSETLCK */            CREGD = CREGD - 1 01551000
*;                           /*@XM08755*/                          0519 01552000
         SL    CREGD,@CF01001                                      0521 01553000
*              END;                                                0522 01554000
*              END;                                                0523 01555000
*           ELSE                                                   0524 01556000
*               CREG9 = SIZECODE;        /* USER AREA NOT BIG ENOUGH */ 01557000
         B     @RC00393                                            0524 01558000
@RF00393 LA    CREG9,43                                            0524 01559000
