*          3  LCPZTRV4 PTR(31);        /* RESERVED FOR RESTORE LCP   */ 00280000
*DCL LCTYPE BIT(8) CONSTANT('04'X);                                0260 00281000
*DCL PLCPTYPE BIT(8) CONSTANT('28'X);  /* PURGE LCPB TYPE ID         */ 00282000
*DCL LCPCLACB BIT(8) CONSTANT('04'X);  /* LCPB FROM CLOSE ACB        */ 00283000
*DCL LSRSTYPE BIT(8) CONSTANT('FF'X);  /* RESET SUBTYPE              */ 00284000
*DCL LCPETYPE BIT(8) CONSTANT('40'X);  /* EXTENDED LCPB TYPE ID      */ 00285000
*/*                                                                  */ 00286000
*/*  MINOR RETURN CODES                                              */ 00287000
*/*                                                                  */ 00288000
*DCL LCMCRSTR BIT(8) CONSTANT('F7'X),  /* MINOR RC FOR RESETSR       */ 00289000
*    LCPMNEW  BIT(8) CONSTANT('02'X),  /* EARLY WARNING,             */ 00290000
*                                      /* RECOVERY IN PROGRESS       */ 00291000
*    LCPMNPF  BIT(8) CONSTANT('03'X),  /* PERM. I/O FAILURE          */ 00292000
*    LCPMNAS  BIT(8) CONSTANT('04'X),  /* AUTO NETWORK SHUTDOWN      */ 00293000
*    LCPMNCL  BIT(8) CONSTANT('06'X),  /* CLOSE/CLSDST OCCURED       */ 00294000
*    LCPMNVD  BIT(8) CONSTANT('07'X),  /* VARY DEACTIVATE            */ 00295000
*    LCPMNTF  BIT(8) CONSTANT('10'X),  /* TEMP RECORD DEVICE FAILURE */ 00296000
*    LCPMNSF  BIT(8) CONSTANT('01'X),  /* TEMP BASIC FAILURE /       */ 00297000
*    LCPMNTS  BIT(8) CONSTANT('11'X),  /* TERMINATE SELF             */ 00298000
*    LCPMNAP  BIT(8) CONSTANT('0A'X),  /* APPL. ISSUED CLEAR         */ 00299000
*    LCPMNPR  BIT(8) CONSTANT('0B'X),  /* PREEMPT                    */ 00300000
*    LCPMNRE  BIT(8) CONSTANT('0C'X);  /* RESTORE                    */ 00301000
*/* VS1, VS2 TPIOS SAVEAREA FORMAT */                              0266 00302000
*DCL  1  ISTZSAVE BDY(WORD) BASED,                                 0266 00303000
*        2   ZSAZSVBC PTR(31),         /* SAVEAREA BACK CHAIN        */ 00304000
*            3   ZSAZSVMS CHAR(1),     /* MODULE TRACE CODE          */ 00305000
*                4 ZSAZACTV BIT(1),    /* ACTIVE SAVE AREA FLAG      */ 00306000
