*        GEN REFS(GPR02F,GPR10F,GPR11F,GPR12F,GPR14F,GPR13P);      0500 01495000
*                                                                  0500 01496000
         STM   GPR10F,GPR12F,0(GPR13P)                                  01497000
         ST    GPR14F,12(GPR13P)                                        01498000
         ST    GPR02F,16(GPR13P)                                        01499000
*                                        /*INVOKE MODESET TO CHANGE  */ 01500000
*                                        /*PSW PROTECTION KEY TO 0   */ 01501000
*        GEN SETS(2);                                              0501 01502000
*                                                                  0501 01503000
         MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2                      01504000
*                GPR10P=GPR13P;          /*PUT SAVE AREA ADDR IN R 10*/ 01505000
         LR    GPR10P,GPR13P                                       0502 01506000
*                                        /*INVOKE OS/VS SETLOCK MACRO*/ 01507000
*                GEN REFS(GPR13P,PSALITA,FLC) SETS(GPR12F);        0503 01508000
         SETLOCK RELEASE,TYPE=LOCAL,                                   *01509000
               RELATED=(LOCAL,ISTSQC02(C02OBT1))                        01510000
*                GPR15P=GPR13P;          /*PUT RETURN CODE IN R 15 */   01511000
         LR    GPR15P,GPR13P                                       0504 01512000
*                                        /*INVOKE MODESET MACRO TO   */ 01513000
*                  DO;                   /*RESTORE PSW PROTECTION KEY*/ 01514000
*        GEN SETS(2);                                              0506 01515000
         MODESET KEYADDR=(2),WORKREG=2                                  01516000
*                  END;                  /* RESTORE REGISTERS        */ 01517000
*                GEN REFS(GPR10F,GPR13P) SETS(GPR10F,GPR11F,       0508 01518000
*                GPR12F,GPR13F,GPR14F,GPR02F);                     0508 01519000
         LR    GPR13F,GPR10F                                            01520000
         LM    GPR10F,GPR12F,0(GPR13P)                                  01521000
         L     GPR14F,12(GPR13P)                                        01522000
         L     GPR02F,16(GPR13P)                                        01523000
*                IF GPR15F=4 THEN GPR15F=0;                        0509 01524000
         C     GPR15F,@CF00053                                     0509 01525000
         BNE   @RF00509                                            0509 01526000
         SLR   GPR15F,GPR15F                                       0510 01527000
*                CRETC=GPR15F;           /*PUT RETURN CODE IN RCREG*/   01528000
*                                                                  0511 01529000
@RF00509 DS    0H                                                  0512 01530000
*              RFY (GPR02P,GPR10P,GPR11P,GPR12P,GPR13P,GPR14P,GPR15P) U 01531000
*RSTD;         END;  /*VSETLCK */                                  0512 01532000
*       CREGD=CREGD-12;            /* RESET SAVEAREA PTR     @XM08755*/ 01533000
         SL    CREGD,@CF01001                                      0514 01534000
*        END;                      /* END OF SUCCESSFUL VSETLCK */ 0515 01535000
*       END;                       /* END OF ENOUGH AREA */        0516 01536000
*      ELSE                       /* USER WANTS SIZE */            0517 01537000
*        CREG9 = SIZECODE;         /* SET RETURN CODE = 2B */      0517 01538000
         B     @RC00372                                            0517 01539000
@RF00372 LA    CREG9,43                                            0517 01540000
*   END;                                                           0518 01541000
@RC00372 DS    0H                                                  0519 01542000
*CSIZE = LENGTH(ISTDEVCH);         /*SET SIZE IN REG 0 */          0519 01543000
@RC00369 LA    CSIZE,8                                             0519 01544000
*CRETC = CREG9;                    /* PUT RETURN CODE IN R15 */    0520 01545000
         LR    CRETC,CREG9                                         0520 01546000
*CREGD = SAVR13;                  /* RESTORE REG 13 */             0521 01547000
*                                                                  0521 01548000
         LR    CREGD,SAVR13                                        0521 01549000
*/* TPEPILOG   */                                                  0522 01550000
* GEN (PRINT OFF);                                                 0522 01551000
*                                                                  0522 01552000
         PRINT OFF                                                      01553000
* GEN (PRINT ON);                                                  0523 01554000
         PRINT ON                                                       01555000
*/* END OF TPEPILOG */                                             0524 01556000
