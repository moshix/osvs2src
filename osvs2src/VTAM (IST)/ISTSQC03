         TITLE 'ISTSQC03 - INQUIRE: COUNTS                             *00001000
                        '                                               00002000
ISTSQC03 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSQC03  75.130'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @01,@12,24(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  DS    0H                                                  0001 00013000
         USING @PSTART,@12                                         0001 00014000
*C03R1 = C03R1;                                                    0233 00015000
*/********************************************************************/ 00016000
*/* ROUTINE NAME: ISTSQC03 - INQUIRE: COUNTS (MAINLINE)              */ 00017000
*/*                                                                  */ 00018000
*/* FUNCTION: CALLED BY ISTSQC00 TO OBTAIN COUNTS OF ACTIVE AND      */ 00019000
*/*           INACTIVE CONNECTIONS.  SAVES AND RESTORES REGISTERS,   */ 00020000
*/*           VALIDATES USER-SUPPLIED BUFFER AREA, OBTAINS COUNT OF  */ 00021000
*/*           ACTIVE ELEMENTS FROM RDTE AND THEN COUNTS INACTIVE     */ 00022000
*/*           ELEMENTS IN THE QUEUE.  MOVES COUNTS TO USER-SUPPLIED  */ 00023000
*/*           BUFFER AREA.                                           */ 00024000
*/*                                                                  */ 00025000
*/* INPUT:  C09R1   (R1) - POINTER TO RPL HEADER (ISTRPH)            */ 00026000
*/*         C09R13 (R13) - POINTER TO O/C SAVE AREA                  */ 00027000
*/*         C09R14 (R14) - POINTER TO NSI IN ISTSQC00                */ 00028000
*/*                                                                  */ 00029000
*/* OUTPUT: BUFFER POINTED AT BY RPLAREA CONTAINS COUNTS OF ACTIVE   */ 00030000
*/*                AND INACTIVE CONNECTIONS TO THE APPLICATION       */ 00031000
*/*         C09R0   (R0) - SIZE OF NEEDED OR USED AREA               */ 00032000
*/*         C09R15 (R15) - RETURN CODE (0-SUCCESSFUL, X'17'-INPUT    */ 00033000
*/*                        AREA NOT VALID,  X'2B'-INPUT AREA TOO     */ 00034000
*/*                        SMALL TO HOLD DATA)                       */ 00035000
*/*         C09R14 (R14) - POINTER TO NSI IN ISTSQC00                */ 00036000
*/*                                                                  */ 00037000
*/* ROUTINES CALLED: NONE                                            */ 00038000
*/*                                                                  */ 00039000
*/* MACROS USED: VSETLCK  - TO OBTAIN AND RELEASE LOCAL MEMORY LOCK  */ 00040000
*/*              VALCHEK  - TO VALIDATE USER-SPECIFIED BUFFER AREA   */ 00041000
*/*              CRRMAINT - TO ESTABLISH COMPONENT RECOVERY RECORD   */ 00042000
*/********************************************************************/ 00043000
*                                                                  0234 00044000
*                                                                  0234 00045000
*    DO;                                                           0234 00046000
*/***  CRRMAINT  PROCNM(ISTSQC03)COMP(NS)FUNC(SUB)  ***/           0235 00047000
*    DCL CMZK0004 PTR(31) REG(1);                                  0235 00048000
*    RFY (CMZK0004) RSTD;                                          0236 00049000
*                                                                  0236 00050000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTNSCRR)  */                  0237 00051000
*/* LOGICAL CHANNEL COMMAND WORD                                      * 00052000
*/* ADD A DECLARE                                      003 10/18/74   * 00053000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0237 00054000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 00055000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 00056000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 00057000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 00058000
*              4  * BIT(1),            /* RESERVED                    * 00059000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0237 00060000
*                                         ERASE OR READ CONT          * 00061000
*              4  LCCWCTL BIT(4),      /* CONTROL TYPE                * 00062000
*                 5  LCCWMODE BIT(2),  /* MODE TYPE                   * 00063000
*                 5  LCCWTYPE BIT(2),  /* OPERATION TYPE              * 00064000
*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00065000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00066000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00067000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00068000
*                                         RECORD                      * 00069000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00070000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00071000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00072000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00073000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00074000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00075000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00076000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00077000
*/* LOGICAL COMMAND CODES */                                       0238 00078000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00079000
*     LXEC     BIT(8) CONSTANT('4B'X), /* RESET AT END OF COMMAND     * 00080000
*     LXO      BIT(8) CONSTANT('5B'X), /* RESET ORDERLY               * 00081000
*     LCCWPRGE BIT(8) CONSTANT('6B'X), /* PURGE                       * 00082000
*     LWH      BIT(8) CONSTANT('0F'X), /* WRITE HEADER INDI. 1ST BLOCK* 00083000
*     LWRVI    BIT(8) CONSTANT('1F'X), /* WRITE RVI                   * 00084000
*     LWNA     BIT(8) CONSTANT('2F'X), /* WRITE NEGATIVE ACK          * 00085000
*     LWAA     BIT(8) CONSTANT('3F'X), /* WRITE ALTERNATE ACK         * 00086000
*     LWPLG    BIT(8) CONSTANT('5F'X), /* WRITE ACK LEADING GRAPHICS  * 00087000
*     LWNLG    BIT(8) CONSTANT('6F'X), /* WRITE NACK LEADING GRAPHICS * 00088000
*     LSMDE    BIT(8) CONSTANT('4F'X), /* SET MODE                    * 00089000
*     LIFBM    BIT(8) CONSTANT('7F'X), /* INDIC 1ST BLOCK OF MESSAGE  * 00090000
*     LTEST    BIT(8) CONSTANT('FE'X), /* TEST LCCW                   * 00091000
*     LPRMPT   BIT(8) CONSTANT('13'X), /* PREEMPT LCCW                * 00092000
*     LRSTR    BIT(8) CONSTANT('43'X), /* RESTORE LCCW                * 00093000
*     LEXEC    BIT(8) CONSTANT('FF'X), /* SPECIAL CONTROL OPERATION   * 00094000
*     LREAD    BIT(2) CONSTANT('10'B),  /* READ TYPE LCCW             * 00095000
*     LWRITE   BIT(2) CONSTANT('01'B),  /* WRITE TYPE LCCW            * 00096000
*     LCNTRL   BIT(2) CONSTANT('11'B),  /* CONTROL TYPE LCCW          * 00097000
*     LRESET   BIT(4) CONSTANT('1011'B),/* RESET COMMANDS             * 00098000
*     LTIC     BIT(8) CONSTANT('08'X),  /* LOGICAL TIC                * 00099000
*     LWR      BIT(6) CONSTANT('000001'B),  /* WRITE CONVERSATIONAL   * 00100000
*     LWB      BIT(6) CONSTANT('000101'B),  /* WRITE BLOCK            * 00101000
*     LWM      BIT(6) CONSTANT('001001'B),  /* WRITE MESSAGE          * 00102000
*     LWT      BIT(6) CONSTANT('001101'B),  /* WRITE TRANSMISSION     * 00103000
*     LEWM     BIT(6) CONSTANT('011001'B),  /* ERASE/WRITE MESSAGE    * 00104000
*     LEWT     BIT(6) CONSTANT('011101'B),  /* ERASE/WRITE TRNSMSSN   * 00105000
*     LEAU     BIT(6) CONSTANT('010001'B),  /* ERASE ALL UNPROTECTED  * 00106000
*     LRB      BIT(6) CONSTANT('000110'B),  /* READ BLOCK             * 00107000
*     LRM      BIT(6) CONSTANT('001010'B),  /* READ MESSAGE           * 00108000
*     LRT      BIT(6) CONSTANT('001110'B),  /* READ TRANSMISSION      * 00109000
*     LRMOD    BIT(6) CONSTANT('010010'B),  /* READ MODIFIED          * 00110000
*     LRBUF    BIT(6) CONSTANT('010110'B),  /* READ BUFFER            * 00111000
*     LRCONT   BIT(6) CONSTANT('011010'B),  /* READ CONTINUOUS        * 00112000
*     LRPEP    BIT(6) CONSTANT('011110'B),  /* READ PERPETUAL         * 00113000
*     LCPYM    BIT(6) CONSTANT('101001'B),  /* COPY MESSAGE           * 00114000
*     LCPYT    BIT(6) CONSTANT('101101'B),  /* COPY TRANSMISSION      * 00115000
*     LD       BIT(8) CONSTANT('23'X),  /* DISCONNECT                 * 00116000
*     LDEC     BIT(8) CONSTANT('33'X),  /* DISCONNECT WITH END OF CALL* 00117000
*     LEOC     BIT(8) CONSTANT('53'X),  /* END OF CALL COMMAND        * 00118000
*     LXEL     BIT(8) CONSTANT('0B'X),  /* RESET ERROR LOCK           * 00119000
*     LXC      BIT(8) CONSTANT('1B'X),  /* RESET CONDITIONAL          * 00120000
*     LXI      BIT(8) CONSTANT('2B'X),  /* RESET IMMEDIATE            * 00121000
*     LC       BIT(8) CONSTANT('03'X),  /* CONTACT                    * 00122000
*     LWPA     BIT(8) CONSTANT('FD'X); /* WRITE POSITIVE ACK          * 00123000
*DCL  LMT      BIT(2) CONSTANT('11'B), /* TRANSMISSION MODE           * 00124000
*     LMS      BIT(2) CONSTANT('00'B), /* SPECIAL MODE LIKE CONV, EAU * 00125000
*     LMB      BIT(2) CONSTANT('01'B), /* BLOCK MODE                  * 00126000
*     LMM      BIT(2) CONSTANT('10'B); /* MSG OR CONTINUOUS MODE      * 00127000
*/* FOLLOWING EQUATES FOR  - PORT SOLICITOR (DIAL)                    * 00128000
*/* PORT SOLICITOR COMMAND CODES                                      * 00129000
*DCL PSOL    BIT(8) CONSTANT('FE'X), /* SPECIAL PORT SOLICITOR     0240 00130000
*                                       COMMAND                       * 00131000
*    STCTL    BIT(8) CONSTANT('01'X), /* START CONTROL                * 00132000
*    RESTCTL  BIT(8) CONSTANT('02'X), /* RESET & CONTROL - REDRIVE    * 00133000
*    STIPUT   BIT(8) CONSTANT('03'X); /* START INPUT                  * 00134000
*/*   FOLLOWING EQUATES FOR SDLC SUPPORT                              * 00135000
*DCL  LCCHOLD  BIT(8) CONSTANT('01'X),  /* HOLD                       * 00136000
*     LCCRHOLD BIT(8) CONSTANT('02'X),  /* RELEASE HOLD               * 00137000
*     LSEND    BIT(8) CONSTANT ('F1'X), /* SEND                       * 00138000
*     LCLEAR   BIT(8) CONSTANT ('F2'X), /* CLEAR                      * 00139000
*     LCRUD    BIT(8) CONSTANT ('F3'X), /* CLEAR, UNBIND              * 00140000
*     LCRUDNFY BIT(8) CONSTANT ('F4'X), /* CLEAR, UNBIND, NOTIFY      * 00141000
*     LNFY     BIT(8) CONSTANT ('F5'X), /* NOTIFY                     * 00142000
*     LRELCB   BIT(8) CONSTANT ('F6'X), /* RELEASE CB                 * 00143000
*     LRESTSR  BIT(8) CONSTANT ('F7'X), /* RESETSR                    * 00144000
*     LCLRNFY  BIT(8) CONSTANT('F8'X),  /* CLEAR/NOTIFY               * 00145000
*     LCHPUR   BIT(8) CONSTANT('F9'X);  /* PURGE                      * 00146000
*DCL  LCCWSIZE FIXED(8) CONSTANT(8);    /* LCCW SIZE                  * 00147000
*DCL  LRDS     BIT(8) CONSTANT('FF'X);  /* RECORD DEV STATS COMMAND   * 00148000
*DCL  LQUIS    BIT(8) CONSTANT('FE'X);  /* QUIESCE COMMAND            * 00149000
*/*              LOGICAL CHANNEL PROGRAM BLOCK                       */ 00150000
*/*                                                                  */ 00151000
*/*  REVISIONS                                         006 03/24/75  */ 00152000
*/*                                                                  */ 00153000
*DCL  1  ISTLCPB BDY(WORD) BASED(LCPBPTR),                         0245 00154000
*        2 LCPTYPE CHAR(1),            /* TYPE CODE                  */ 00155000
*        2 LCPLNGTH  PTR(8),           /* LENGTH IN BYTES            */ 00156000
*        2 LCPREQTG CHAR(2),           /* LCPB REQ. TAG FIELD        */ 00157000
*          3 LCPFLAGS BIT(8),          /* FLAG BYTE                  */ 00158000
*            4 LCPZRT00 BIT(1),        /* LOGICAL ERROR FLAG         */ 00159000
*            4 LCPZRT01 BIT(1),        /* CONTROL BLOCK ERROR        */ 00160000
*            4 LCPZF001 BIT(1),        /* LCP START PROCESS FLAG     */ 00161000
*              5 LCPZRT02 BIT(1),      /* I/O INITIATION DISABLED    */ 00162000
*                6 LCPZRTL2 BIT(1),    /* END OF SCHEDULABLE BLOCK   */ 00163000
*            4 LCPZF002 BIT(1),        /* LCP IN PROCESSING FLAG     */ 00164000
*              5 LCPZRT03 BIT(1),      /* GO LOOK AT FMCB LINE STATE */ 00165000
*                                      /* FIELD (REQ. TAG OVERLAY)   */ 00166000
*                6 LCPZRTL3 BIT(1),    /* WRITE/READ REQUEST         */ 00167000
*            4 LCPZF003 BIT(1),        /* LCP PROCESSING COMPLETE FLAG* 00168000
*              5 LCPZRT04 BIT(1),      /* STPR SET IN BDU FLAG       */ 00169000
*                                      /* (REQ. TAG OVERLAY)         */ 00170000
*                6 LCPZRTL4 BIT(1),    /* FEEDBACK REQUESTED         */ 00171000
*            4 LCPZF004 BIT(1),        /* SINGLE LCCW/LCP FLAG       */ 00172000
*              5 LCPZRT05 BIT(1),      /* FEEDBACK TO BE ROUTED TO   */ 00173000
*                                      /* ICNCB FMCB (REQ. TAG       */ 00174000
*                                      /* OVERLAY)                   */ 00175000
*                6 LCPZRTL5 BIT(1),    /* DECREMENT SCHED COUNT ON DEQ* 00176000
*            4 LCPZRT06 BIT(1),        /* CRITICAL TEXT BLOCK        */ 00177000
*                                      /* FLAG (REQ. TAG OVERLAY)    */ 00178000
*              5 LCPZRTL6 BIT(1),      /* WRITE REQUEST              */ 00179000
*            4 LCPZRT07 BIT(1),        /* BUFFER TRACE ACTIVE        */ 00180000
*                                      /* FLAG (REQ. TAG OVERLAY)    */ 00181000
*              5 LCPZRTL7 BIT(1),      /* SCHEDULING FLAG            */ 00182000
*          3 LCPMNRCD CHAR(1),         /* LCP MINOR REJECTION CODE   */ 00183000
*            4 LCPZRT08 BIT(1),        /* I/O TRACE ACTIVE FLAG      */ 00184000
*                                      /* (REQ. TAG OVERLAY)         */ 00185000
*              5 LCPZRTL8 BIT(1),      /* DEVICE END NOTIFICATION    */ 00186000
*            4 LCPZRT09 BIT(1),        /* DIALOGUE SCHED DISABLED    */ 00187000
*              5 LCPZRTL9 BIT(1),      /* CRITICAL TEXT FLAG         */ 00188000
*            4 LCPZRT0A BIT(1),        /* SIMULATED CONTACT FLAG     */ 00189000
*            4 LCPZRT0B BIT(1),        /* SIMULATED DISCONNECT FLAG  */ 00190000
*        2 LCPCHAIN PTR(31),           /* APS CHAIN                  */ 00191000
*          3 LCPCHAI1 PTR(8),                                      0245 00192000
*            4 LCPCHNG BIT(1),         /* GATE BIT                   */ 00193000
*          3 LCPCHAI2 PTR(24),         /* ADDRESS FIELD              */ 00194000
*        2 LCPRUCNT PTR(8),            /* NUMBER OF RU FOR THIS LCPB */ 00195000
*          3 LCPZMIRC CHAR(1),         /* MINOR RETURN CODE          */ 00196000
*        2 LCPFLAG2 CHAR(1),           /* SECOND FLAG BYTE           */ 00197000
*          3 LCPPRFLG BIT(1),          /* PACING REQUIRED            */ 00198000
*          3 LCPASYN  BIT(1),          /* ASYNCHRONOUS LCPB          */ 00199000
*          3 LCPRSTRT BIT(1),          /* RESTART LCPB               */ 00200000
*          3 LCPTLBPI BIT(1),          /* TPIOS BLOCK LCCW           */ 00201000
*            4 LCPZRIMM BIT(1),        /* RESET IMMED REQUIRED ON ERR*/ 00202000
*          3 LCPFID0  BIT(1),          /* FID0 TH INDICATOR FOR RECORD* 00203000
*            4 LCPEXRES BIT(1),        /* EXCEP. RESPONSE SPEC. 3270R*/ 00204000
*          3 LCPDSTAT BIT(1),          /* DIALOGUE STATE BIT, 3270R  */ 00205000
*                                      /* 0=REQ. REJECT,1=STATE ERROR*/ 00206000
*          3 LCPFREE  BIT(1),          /* DEALLOCATE LCPB 3270R      */ 00207000
*          3 LCPATOP  BIT(1),          /* ATOP LCPB  3270R           */ 00208000
*        2 LCPSEQ   FIXED(16),         /* OUTBOUND SEQUENCE NUMBER   */ 00209000
*          3 LCPRRCCT PTR(8),          /* COUNT OF RESPONSES RECEIVED*/ 00210000
*          3 LCPRRCNT PTR(8),          /* COUNT OF RESPONSES REQUESTED* 00211000
*        2 LCPLCCW1 PTR(31),           /* ADDR FIRST LCCW FOR FID1   */ 00212000
*        2 LCPCID CHAR(4) BDY(WORD),   /* COMMUNICATION ID           */ 00213000
*          3 LCPSAF BIT(16),           /* SOURCE IDENTIFICATION      */ 00214000
*          3 LCPDAF BIT(16),           /* DESTINATION IDENTIFICATION */ 00215000
*        2 LCPNXLCW PTR(31),           /* ADDR NEXT LCCW             */ 00216000
*        2 LCPFDBA PTR(31),            /* BTU ADDRESS                */ 00217000
*          3 LCPFDB1 PTR(8),                                       0245 00218000
*          3 LCPFDB2 PTR(24),          /* ADDRESS FIELD              */ 00219000
*        2 LCPTLBAD PTR(31),           /* ADDRESS OF TLB             */ 00220000
*        2 LCPFOSN  BIT(16),           /* SEQUENCE NUMBER FIRST RU   */ 00221000
*        2 LCPFSNCH FIXED(15),         /* 1ST SEQ NUMBER IN RU CHAIN */ 00222000
*        2 LCPSTYPE FIXED(8),          /* LCPB SUB-TYPE X'FF' = RESET,* 00223000
*                                      /* X'00' = NON-RESET          */ 00224000
*        2 LCPFLG1 CHAR(1),            /* FLAGS                      */ 00225000
*          3 LCPRESET BIT(1),          /* A TPQUE WITH RESET QUEUED  */ 00226000
*                                      /* THIS ELEMENT               */ 00227000
*          3 LCPINTER BIT(1),          /* INTERNAL LCPB              */ 00228000
*          3 LCPDIRPT BIT(1),          /* NOT SEND W/POST=SCHED=0    */ 00229000
*          3 LCPREDRV BIT(1),          /* LCPB IS FOR REDRIVE        */ 00230000
*          3 LCPSTLB  BIT(1),          /* TLB NOT TO BE FREED        */ 00231000
*          3 LCPTRANS BIT(1),          /* DATA INCLUDES CONTROL CHARS*/ 00232000
*          3 LCPRSV78 BIT(2),          /* RESERVED                   */ 00233000
*        2 LCPLCCWC BIT(8),            /* COUNT OF LCCWS             */ 00234000
*        2 LCPRSV77 CHAR(1),           /* RESERVED                   */ 00235000
*        2 LCPFLCCW PTR(31),           /* ADDR OF FIRST LCCW         */ 00236000
*          3 LCPUBFSB PTR(31),         /* UNBIND FSB ADDRESS         */ 00237000
*        2 LCPCLREP PTR(31),           /* PTR TO CLEAR LCPB IN UCB   */ 00238000
*/*  NOTE:  THE FOLLOWING FIELD - LCPLCCW MUST BE THE LAST FIELD     */ 00239000
*/*         IN THIS CONTROL BLOCK.  THE CONTROL BLOCK ISTDCLCP HAS   */ 00240000
*/*         FIELDS IN IT WHICH MUST BE CONTIGUOUS WITH LCPLCCW       */ 00241000
*        2 LCPLCCW  CHAR(LENGTH(ISTLCCW)) BDY(WORD), /* FIRST LCCW   */ 00242000
*          3 LCPRSTQH PTR(31),         /* RESET LCPB QUEUE HEADER    */ 00243000
*          3 LCPPABA  PTR(31),         /* PAB POINTER                */ 00244000
*            4 LCPRPHA  PTR(31),       /* RPH POINTER                */ 00245000
*              5 LCPFTYPE BIT(1);      /* FEEDBACK TYPE              */ 00246000
*/*     CONSTANTS FOR LCPTYPE                                         * 00247000
*DCL  LCRPHFT  BIT(1) CONSTANT('1'B);                              0246 00248000
*DCL  LCPABFT  BIT(1) CONSTANT('0'B);                              0247 00249000
*DCL LCPBFIRM BIT(8) CONSTANT('10'X);  /* LU FAILED, CLSDST, SCHED LOST 00250000
*                                         TERM, OPNDST MAY BE TRIED   * 00251000
*DCL LCPBTERM BIT(8) CONSTANT('11'X);  /* TERM RECEIVED,CLSDST,    0249 00252000
*                                         SCHED LOST TERM             * 00253000
*/*                                                                   * 00254000
*                                                                  0250 00255000
*DCL 1 LCPZTIE BDY(WORD) DEFINED(LCPCID), /* TOLTEP INTERFACE      0250 00256000
*                                         ELEMENT OVERLAY            */ 00257000
*       2  LCPZFMCA PTR(31),           /* ACTIVE FMCB ADDRESS        */ 00258000
