*     LRESTSR  BIT(8) CONSTANT ('F7'X), /* RESETSR                    * 00139000
*     LCLRNFY  BIT(8) CONSTANT('F8'X),  /* CLEAR/NOTIFY               * 00140000
*     LCHPUR   BIT(8) CONSTANT('F9'X);  /* PURGE                      * 00141000
*DCL  LCCWSIZE FIXED(8) CONSTANT(8);    /* LCCW SIZE                  * 00142000
*DCL  LRDS     BIT(8) CONSTANT('FF'X);  /* RECORD DEV STATS COMMAND   * 00143000
*DCL  LQUIS    BIT(8) CONSTANT('FE'X);  /* QUIESCE COMMAND            * 00144000
*/*              LOGICAL CHANNEL PROGRAM BLOCK                       */ 00145000
*/*                                                                  */ 00146000
*/*  REVISIONS                                         007 05/19/75  */ 00147000
*/*                                                                  */ 00148000
*DCL  1  ISTLCPB BDY(WORD) BASED(LCPBPTR),                         0284 00149000
*        2 LCPTYPE CHAR(1),            /* TYPE CODE                  */ 00150000
*        2 LCPLNGTH  PTR(8),           /* LENGTH IN BYTES            */ 00151000
*        2 LCPREQTG CHAR(2),           /* LCPB REQ. TAG FIELD        */ 00152000
*          3 LCPFLAGS BIT(8),          /* FLAG BYTE                  */ 00153000
*            4 LCPZRT00 BIT(1),        /* LOGICAL ERROR FLAG         */ 00154000
*            4 LCPZRT01 BIT(1),        /* CONTROL BLOCK ERROR        */ 00155000
*            4 LCPZF001 BIT(1),        /* LCP START PROCESS FLAG     */ 00156000
*              5 LCPZRT02 BIT(1),      /* I/O INITIATION DISABLED    */ 00157000
*                6 LCPZRTL2 BIT(1),    /* END OF SCHEDULABLE BLOCK   */ 00158000
*            4 LCPZF002 BIT(1),        /* LCP IN PROCESSING FLAG     */ 00159000
