*        2 LCPFOSN  BIT(16),           /* SEQUENCE NUMBER FIRST RU   */ 00216000
*        2 LCPFSNCH FIXED(16),         /* 1ST SEQ NUMBER IN RU CHAIN */ 00217000
*        2 LCPSTYPE FIXED(8),          /* LCPB SUB-TYPE X'FF' = RESET,* 00218000
*                                      /* X'00' = NON-RESET          */ 00219000
*        2 LCPFLG1 CHAR(1),            /* FLAGS                      */ 00220000
*          3 LCPRESET BIT(1),          /* A TPQUE WITH RESET QUEUED  */ 00221000
*                                      /* THIS ELEMENT               */ 00222000
*          3 LCPINTER BIT(1),          /* INTERNAL LCPB              */ 00223000
*          3 LCPDIRPT BIT(1),          /* NOT SEND W/POST=SCHED=0    */ 00224000
*          3 LCPREDRV BIT(1),          /* LCPB IS FOR REDRIVE        */ 00225000
*          3 LCPSTLB  BIT(1),          /* TLB NOT TO BE FREED        */ 00226000
*          3 LCPTRANS BIT(1),          /* DATA INCLUDES CONTROL CHARS*/ 00227000
*          3 LCPRSV78 BIT(2),          /* RESERVED                   */ 00228000
