*/* ROUTINE NAME: C04NIBIN - NIB INITIALIZATION              @XM08904*/ 02606000
*/*                                                                  */ 02607000
*/* FUNCTION:  CALLED THROUGHOUT ISTSQC04 TO INITIALIZE      @XM08904*/ 02608000
*/*            FIELDS IN NIBS TO BE PROVIDED TO USER.        @XM08904*/ 02609000
*/*                                                                  */ 02610000
*/* INPUT:  C04RDTE9 (R9) - POINTER TO RDTE CONTAINING       @XM08904*/ 02611000
*/*                         INFORMATION USED IN INITIALIZING @XM08904*/ 02612000
*/*                         NIB                              @XM08904*/ 02613000
*/*         NIBPTR  (R6)  - POINTER TO NIB                   @XM08904*/ 02614000
*/*                                                                  */ 02615000
*/* OUTPUT:  NIB INITIALIZED WITH DEVICE NAME, CHARACTERISTICS,      */ 02616000
*/*          CONTROL BLOCK ID AND LENGTH, AND NIBLAST BIT    @XM08904*/ 02617000
*/*          SET ON.                                         @XM08904*/ 02618000
*/*                                                                  */ 02619000
*/* ROUTINES CALLED:  NONE                                   @XM08904*/ 02620000
*/*                                                                  */ 02621000
*/* MACROS USED:  NONE                                       @XM08904*/ 02622000
*/*                                                                  */ 02623000
*/********************************************************************/ 02624000
*                                                                  0888 02625000
*C04NIBIN:                                                 /*@XM08904*/ 02626000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                       /*@XM08904*/ 02627000
C04NIBIN DS    0H                                                  0889 02628000
*   RFY (NIBPTR,C04RDTE9,C04R10) RSTD;                     /*@XM08371*/ 02629000
*   RFY ISTRPRE BASED(C04RDTE9);    /* USE PARM REG FOR BASE @XM08904*/ 02630000
*   NIBSYM = RPRNAME;                                      /*@XM08904*/ 02631000
         LR    @15,NIBPTR                                          0891 02632000
         MVC   NIBSYM(8,@15),RPRNAME(C04RDTE9)                     0891 02633000
*   NIBDEVCH = RPRDEVCH;                                   /*@XM08904*/ 02634000
         LR    @15,NIBPTR                                          0892 02635000
         MVC   NIBDEVCH(8,@15),RPRDEVCH(C04RDTE9)                  0892 02636000
*   NIBLEN = C04NIBL;                                      /*@XM08904*/ 02637000
         L     @15,C04NIBL                                         0893 02638000
         LR    @12,NIBPTR                                          0893 02639000
         STC   @15,NIBLEN(,@12)                                    0893 02640000
*   NIBID = NIBIDD;                                        /*@XM08904*/ 02641000
