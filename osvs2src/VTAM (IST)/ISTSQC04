*                     NIBLAST=C04OFF;                              0681 01919000
         LR    @15,NIBPTR                                          0681 01920000
         NI    NIBLAST(@15),B'01111111'                            0681 01921000
*                   END;                                           0682 01922000
*             END;                                                 0683 01923000
@RC00673 DS    0H                                                  0683 01924000
@RC00671 DS    0H                                                  0684 01925000
*           RFY                                                    0684 01926000
*             C04SIZE RSTD;                                        0684 01927000
@RC00665 DS    0H                                                  0685 01928000
*           C04SIZE=C04SZE;         /* RESTORE NEEDED SIZE           */ 01929000
         L     @15,RPHWEA(,PTRRPH)                                 0685 01930000
         L     C04SIZE,C04SZE+552(,@15)                            0685 01931000
*         END;                                                     0686 01932000
*     END;                                                         0687 01933000
@RC00613 DS    0H                                                  0688 01934000
*   C04R14=C04SAVE2;                /* RESTORE RETURN REG            */ 01935000
@RT00609 L     @12,RPHWEA(,PTRRPH)                                 0688 01936000
         L     C04R14,C04SAVE2+552(,@12)                           0688 01937000
*   END C04SWL;                                                    0689 01938000
@EL00003 DS    0H                                                  0689 01939000
@EF00003 DS    0H                                                  0689 01940000
@ER00003 BR    @14                                                 0689 01941000
*/********************************************************************/ 01942000
*/* ROUTINE NAME: C04DITT - DIAL-IN-TYPE-TABLE (DITT) PROCESSOR      */ 01943000
*/*                                                                  */ 01944000
*/* FUNCTION: CALLED BY C04SWL TO PROVIDE TERMINAL INFORMATION       */ 01945000
*/*           FROM TERMINALS IN DIAL-IN-TYPE TABLE (IE. MTA/SS)      */ 01946000
*/*                                                                  */ 01947000
*/* INPUT:  NIBPTR   (R0) - ADDRESS OF USER AREA                     */ 01948000
*/*         RDTPTR   (R6) - ADDRESS OF MTA/SS PORT ENTRY             */ 01949000
*/*         C04COUNT (R4) - NUMBER OF TERMINALS                      */ 01950000
*/*         C04R14  (R14) - POINTER TO NSI IN C04SWL                 */ 01951000
*/*                                                                  */ 01952000
*/* OUTPUT: C04COUNT (R4) - CONTAINS ZERO                            */ 01953000
*/*         C04RC8   (R8) - CONTAINS ZERO                            */ 01954000
*/*         C04R14  (R14) - POINTER TO NSI IN C04SWL                 */ 01955000
*/*         TERMINAL NAME AND DEVICE CHARACTERISTICS MOVED TO        */ 01956000
*/*           USER-DESIGNATED AREA FOR EACH TERMINAL                 */ 01957000
*/*                                                                  */ 01958000
*/* ROUTINES CALLED: C04NIBIN - TO INITIALIZE NIBS           @XM08904*/ 01959000
*/*                                                                  */ 01960000
*/* MACROS USED: NONE                                                */ 01961000
*/********************************************************************/ 01962000
*                                                                  0690 01963000
*                                                                  0690 01964000
*C04DITT:                                                          0690 01965000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0690 01966000
C04DITT  DS    0H                                                  0691 01967000
*   C04SAVE3=C04R14;                                       /*@XM08904*/ 01968000
         L     @15,RPHWEA(,PTRRPH)                                 0691 01969000
         ST    C04R14,C04SAVE3+552(,@15)                           0691 01970000
*   TABPTR=RPTDITT;                 /* GET ADDRESS OF FIRST ENTRY  0692 01971000
*                                      @DM6299                       */ 01972000
         L     TABPTR,RPTDITT(,RDTPTR)                             0692 01973000
*   DO WHILE DTELCTI^=DITTEND;      /* SEARCH TO LAST ENTRY @DM7262  */ 01974000
         B     @DE00693                                            0693 01975000
@DL00693 DS    0H                                                  0694 01976000
*     RFY C04RDTE9 RSTD;            /* FOR C04NIBIN          @XM08904*/ 01977000
*     C04RDTE9=DTERDTEA;            /* RDTE PTR FROM DITT    @XM08904*/ 01978000
         ICM   C04RDTE9,15,DTERDTEA(TABPTR)                        0695 01979000
*     CALL C04NIBIN;                /* INITIALIZE NIBS       @XM08904*/ 01980000
         BAL   @14,C04NIBIN                                        0696 01981000
*     RFY C04RDTE9 UNRSTD;          /* RELESE REG9           @XM08904*/ 01982000
*     TABPTR=TABPTR+LENGTH(ISTDTE); /* INCR. TO NEXT ENTRY           */ 01983000
         AL    TABPTR,@CF00771                                     0698 01984000
*     NIBPTR=NIBPTR+LENGTH(ISTNIB); /* UPDATE TO NEXT NIB            */ 01985000
         AL    NIBPTR,@CF01184                                     0699 01986000
*   END;                                                           0700 01987000
*                                                                  0700 01988000
@DE00693 CLI   DTELCTI(TABPTR),X'FF'                               0700 01989000
         BNE   @DL00693                                            0700 01990000
*/* MOVE LAST ENTRY IN TABLE                                         */ 01991000
*                                                                  0701 01992000
*   RFY C04RDTE9 RSTD;              /* FOR C04NIBIN          @XM08904*/ 01993000
*   C04RDTE9=DTERDTEA;              /* RDTE PTR FROM DITT    @XM08904*/ 01994000
         ICM   C04RDTE9,15,DTERDTEA(TABPTR)                        0702 01995000
*   CALL C04NIBIN;                  /* INITIALIZE NIBS       @XM08904*/ 01996000
         BAL   @14,C04NIBIN                                        0703 01997000
*   RFY C04RDTE9 UNRSTD;            /* RELESE REG9           @XM08904*/ 01998000
*   IF RPTUTERM^=C04ZERO THEN                                      0705 01999000
         L     @15,RPTUTERM(,RDTPTR)                               0705 02000000
         LTR   @15,@15                                             0705 02001000
         BZ    @RF00705                                            0705 02002000
*     DO;                                                          0706 02003000
*       RFY C04RDTE9 RSTD;          /* FOR C04NIBIN          @XM08904*/ 02004000
*       C04RDTE9=RPTUTERM;          /* POINT TO UTERM RDTE   @XM08904*/ 02005000
         LR    C04RDTE9,@15                                        0708 02006000
*       CALL C04NIBIN;              /* INITIALIZE UTERM NIB  @XM08904*/ 02007000
         BAL   @14,C04NIBIN                                        0709 02008000
*       RFY C04RDTE9 UNRSTD;        /* RELEASE REG 9         @XM08904*/ 02009000
*     END;                                                         0711 02010000
*   ELSE                                                           0712 02011000
*     ;                                                            0712 02012000
@RF00705 DS    0H                                                  0713 02013000
*   NIBLAST=C04OFF;                 /* FLAG NIB AS LAST IN CHAIN     */ 02014000
         LR    @15,NIBPTR                                          0713 02015000
         NI    NIBLAST(@15),B'01111111'                            0713 02016000
*   RFY                                                            0714 02017000
*     C04COUNT RSTD;                                               0714 02018000
*   C04COUNT=C04ZERO;               /* ZERO COUNT                    */ 02019000
         SLR   C04COUNT,C04COUNT                                   0715 02020000
*   C04RC8=C04ZERO;                 /* GOOD RETURN                   */ 02021000
         SLR   C04RC8,C04RC8                                       0716 02022000
*   END C04DITT;                                                   0717 02023000
@EL00004 DS    0H                                                  0717 02024000
@EF00004 DS    0H                                                  0717 02025000
@ER00004 BR    @14                                                 0717 02026000
*/********************************************************************/ 02027000
*/* ROUTINE NAME: C04DIVT - DIAL-IN VERIFICATION                     */ 02028000
*/*                                                                  */ 02029000
*/* FUNCTION: CALLED BY C04SWL TO PROVIDE TERMINAL INFORMATION FROM  */ 02030000
*/*           TERMINALS IN DIAL-IN-VERIFICATION (I.E., BSC) TABLE    */ 02031000
*/*                                                                  */ 02032000
*/* INPUT:  TABPTR   (R5) - ADDR OF DIVTH (HEADER ENTRY)             */ 02033000
*/*         NIBPTR   (R0) - ADDR OF FIRST NIB IN USER AREA           */ 02034000
*/*         OWCDRDTE - ADDR OF BYSNC PORT ENTRY                      */ 02035000
*/*         C04R14  (R14) - NSI ADDR IN CALLING ROUTINE (C04SWL)     */ 02036000
*/*                                                                  */ 02037000
*/* OUTPUT: C04COUNT (R4) - SET TO ZERO                              */ 02038000
*/*         C04R14  (R14) - NSI ADDR IN CALLING ROUTINE (C04SWL)     */ 02039000
*/*         C04RC8  (R8)  - SET TO ZERO                              */ 02040000
*/*         TERMINAL NAME AND DEVICE CHARACTERISTICS MOVED TO USER-  */ 02041000
*/*           DESIGNATED AREA FOR EACH DIVT ENTRY                    */ 02042000
*/*                                                                  */ 02043000
*/* ROUTINES CALLED: C04NIBIN - TO INITIALIZE NIBS           @XM08904*/ 02044000
*/*                                                                  */ 02045000
*/* MACROS USED: NONE                                                */ 02046000
*/********************************************************************/ 02047000
*                                                                  0718 02048000
*C04DIVT:                                                          0718 02049000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0718 02050000
C04DIVT  DS    0H                                                  0719 02051000
*   C04SAVE3=C04R14;                                       /*@XM08904*/ 02052000
         L     @15,RPHWEA(,PTRRPH)                                 0719 02053000
         ST    C04R14,C04SAVE3+552(,@15)                           0719 02054000
*   C04COUNT=DIVNUME;               /* RESET COUNT TO AGREE WITH     */ 02055000
         LH    C04COUNT,DIVNUME(,TABPTR)                           0720 02056000
*                                   /* TABLE                         */ 02057000
*   RFY C04R10 RSTD;                /* FOR DOWHILE           @XM08371*/ 02058000
*   C04R10=DIVIDLEN;                /* GET BSCID FLD LENGTH  @XM08371*/ 02059000
         LH    C04R10,DIVIDLEN(,TABPTR)                            0722 02060000
*   TABPTR=TABPTR+LENGTH(ISTDIVTH); /* GET ADDR OF FIRST ENTRY       */ 02061000
         AL    TABPTR,@CF00059                                     0723 02062000
*   DO WHILE C04COUNT>C04ZERO;      /* SEARCH DIVT TABLE             */ 02063000
         B     @DE00724                                            0724 02064000
@DL00724 DS    0H                                                  0725 02065000
*     RFY C04RDTE9 RSTD;            /* FOR C04NIBIN          @XM08904*/ 02066000
*     C04RDTE9=DTERDTEA;            /* RDTE PTR FROM DITT    @XM08904*/ 02067000
         ICM   C04RDTE9,15,DTERDTEA(TABPTR)                        0726 02068000
*     CALL C04NIBIN;                /* INITIALIZE NIBS       @XM08904*/ 02069000
         BAL   @14,C04NIBIN                                        0727 02070000
*     RFY C04RDTE9 UNRSTD;          /* RELESE REG9           @XM08904*/ 02071000
*     NIBPTR=NIBPTR+LENGTH(ISTNIB); /* GET NEXT NIB                  */ 02072000
         AL    NIBPTR,@CF01184                                     0729 02073000
*     TABPTR=TABPTR+                /* GET NEXT DVE- INCREMENT BY LEN*/ 02074000
*         C04FIVE+C04R10;           /* OF PTR+LEN FLD+ID FLD @XM08371*/ 02075000
         LA    @15,5                                               0730 02076000
         ALR   @15,TABPTR                                          0730 02077000
         ALR   @15,C04R10                                          0730 02078000
         LR    TABPTR,@15                                          0730 02079000
*     C04COUNT=C04COUNT-C04ONE;     /* DECR. COUNT                   */ 02080000
         BCTR  C04COUNT,0                                          0731 02081000
*   END;                                                           0732 02082000
@DE00724 SLR   @15,@15                                             0732 02083000
         CR    C04COUNT,@15                                        0732 02084000
         BH    @DL00724                                            0732 02085000
*   RFY C04R10 UNRSTD;              /*                       @XM08371*/ 02086000
*   IF RPTUTERM^=C04ZERO            /* IF UTERM EXIST                */ 02087000
*     THEN                                                         0734 02088000
         L     @12,RPTUTERM(,RDTPTR)                               0734 02089000
         CR    @12,@15                                             0734 02090000
         BE    @RF00734                                            0734 02091000
*     DO;                           /* MOVE UTERM INFO.              */ 02092000
*       RFY C04RDTE9 RSTD;          /* FOR C04NIBIN          @XM08904*/ 02093000
*       C04RDTE9=RPTUTERM;          /* POINT TO UTERM RDTE   @XM08904*/ 02094000
         LR    C04RDTE9,@12                                        0737 02095000
*       CALL C04NIBIN;              /* INITIALIZE UTERM NIB  @XM08904*/ 02096000
         BAL   @14,C04NIBIN                                        0738 02097000
*       RFY C04RDTE9 UNRSTD;        /* RELEASE REG 9         @XM08904*/ 02098000
*     END;                                                         0740 02099000
*   ELSE                                                           0741 02100000
*     NIBPTR=NIBPTR-LENGTH(ISTNIB); /* DECR. UNNEEDED NIB            */ 02101000
         B     @RC00734                                            0741 02102000
