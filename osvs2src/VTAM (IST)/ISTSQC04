         ICM   GPR04F,15,SAV40021(GPR13P)                          0659 01864000
*              GPR06F=SAV60021;          /*           SIX,          */  01865000
         ICM   GPR06F,15,SAV60021(GPR13P)                          0660 01866000
*              GPR14F=SAVE0021;          /*    AND FOURTEEN         */  01867000
*                                                                  0661 01868000
         ICM   GPR14F,15,SAVE0021(GPR13P)                          0661 01869000
*            RFY (GPR01P,GPR02P,GPR04P,GPR06P,GPR13P,GPR14P,GPR15P) UNR 01870000
*TD;                                     /*UNRESTRICT REGISTERS    */   01871000
*            END;  /* VALCHEK */                                   0663 01872000
*           C04R13=C04R13-C04TWEL;                                 0664 01873000
         SL    C04R13,@CF01010                                     0664 01874000
*           IF C04R15^=C04ZERO THEN    /* VALCHEK FAILED             */ 01875000
         LTR   C04R15,C04R15                                       0665 01876000
         BZ    @RF00665                                            0665 01877000
*             C04RC8=C04VC;          /* SET INVALID RETURN CODE      */ 01878000
         LA    C04RC8,23                                           0666 01879000
*           ELSE                                                   0667 01880000
*             DO;                                                  0667 01881000
         B     @RC00665                                            0667 01882000
@RF00665 DS    0H                                                  0668 01883000
*           RFY C04R15 UNRSTD;      /* FREE REG 15           @DM12421*/ 01884000
*               RFY                                                0669 01885000
*                 NIBPTR RSTD;                                     0669 01886000
*               NIBPTR=RPLAREA;     /* GET USER AREAS ADDR           */ 01887000
         L     @15,RPHWEA(,PTRRPH)                                 0670 01888000
         L     @15,OCWRPL(,@15)                                    0670 01889000
         L     NIBPTR,RPLAREA(,@15)                                0670 01890000
*               IF RLNTTV=C04ON     /* IF DIAL-IN-TYPE               */ 01891000
*                 THEN                                             0671 01892000
         TM    RLNTTV(RDTPTR),B'00001000'                          0671 01893000
         BNO   @RF00671                                            0671 01894000
*                 CALL C04DITT;     /* EXIST (MTA/SS) GOTO           */ 01895000
         BAL   @14,C04DITT                                         0672 01896000
*                                   /* DIAL-IN SUBRTN                */ 01897000
*               ELSE                                               0673 01898000
*                 IF RLNIDV=C04ON   /* IF DIAL-IN-VERIFICATN         */ 01899000
*                   THEN                                           0673 01900000
         B     @RC00671                                            0673 01901000
@RF00671 TM    RLNIDV(RDTPTR),B'00001000'                          0673 01902000
         BNO   @RF00673                                            0673 01903000
*                   CALL C04DIVT;   /* (BSC) EXIST GOTO              */ 01904000
         BAL   @14,C04DIVT                                         0674 01905000
*                                   /* VERIFICATION SUBRTN           */ 01906000
*                 ELSE                                             0675 01907000
*                   DO;             /* MOVE UTERM INFORMATION        */ 01908000
         B     @RC00673                                            0675 01909000
@RF00673 DS    0H                                                  0676 01910000
*                     RFY C04RDTE9 RSTD;  /* FOR C04NIBIN    @XM08904*/ 01911000
*                     C04RDTE9=RPTUTERM;  /* UTERM RDTE      @XM08904*/ 01912000
         L     C04RDTE9,RPTUTERM(,RDTPTR)                          0677 01913000
*                     CALL C04NIBIN;  /* INITIALIZE THE NIB  @XM08904*/ 01914000
