*                                        /*UNRESTRICT REGISTERS    */   01508000
*        END;  /* VALCHEK */                                       0543 01509000
*       C04R13=C04R13-12;           /* RESET SAVE ADDRESS            */ 01510000
         SL    C04R13,@CF01010                                     0544 01511000
*       IF C04R15^=C04ZERO          /* IF VALIDITY CHECK NOT OK      */ 01512000
*         THEN                                                     0545 01513000
         LTR   C04R15,C04R15                                       0545 01514000
         BZ    @RF00545                                            0545 01515000
*         C04RC8=C04VC;             /* THEN SET INVALID RETURN CODE  */ 01516000
         LA    C04RC8,23                                           0546 01517000
*       ELSE                                                       0547 01518000
*         DO;                                                      0547 01519000
         B     @RC00545                                            0547 01520000
@RF00545 DS    0H                                                  0548 01521000
*       RFY C04R15 UNRSTD;          /* FREE REG 15           @DM12421*/ 01522000
*           RFY                                                    0549 01523000
*             NIBPTR RSTD;                                         0549 01524000
*           NIBPTR=RPLAREA;         /* INITIALIZE NIB BASE POINTER  */  01525000
         L     @15,RPHWEA(,PTRRPH)                                 0550 01526000
         L     @15,OCWRPL(,@15)                                    0550 01527000
         L     NIBPTR,RPLAREA(,@15)                                0550 01528000
*           DO WHILE C04COUNT>C04ZERO;  /* TO FIND RDTE             */  01529000
         B     @DE00551                                            0551 01530000
@DL00551 DS    0H                                                  0552 01531000
*             IF RPRENTRY=RPRENTTR|RPRENTRY=RPRENTCM|RPRENTRY=RPRENTUT| 01532000
