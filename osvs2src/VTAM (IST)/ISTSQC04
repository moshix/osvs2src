         NI    NIBLAST(@15),B'01111111'                            0742 02106000
*   RFY                                                            0743 02107000
*    (TABPTR,                                                      0743 02108000
*     NIBPTR)UNRSTD;                                               0743 02109000
*   C04RC8=C04ZERO;                 /* SET GOOD RETURN               */ 02110000
         SLR   C04RC8,C04RC8                                       0744 02111000
*   END C04DIVT;                                                   0745 02112000
@EL00005 DS    0H                                                  0745 02113000
@EF00005 DS    0H                                                  0745 02114000
@ER00005 BR    @14                                                 0745 02115000
*/********************************************************************/ 02116000
*/* ROUTINE NAME: C04DLOUT - DIAL OUT PROCESSOR                      */ 02117000
*/*                                                                  */ 02118000
*/* FUNCTION: CALLED BY CTERMN1 TO GET COUNT OF TERMINALS ON         */ 02119000
*/*           LCDO LINE GROUP (DIAL OUT OR DIAL IN-OUT LINES)        */ 02120000
*/*                                                                  */ 02121000
*/* INPUT:  RDTPTR   (R6) - POINTER TO DIAL-OUT RDT ENTRY            */ 02122000
*/*         C04COUNT (R4) - CONTAINS A ZERO                          */ 02123000
*/*         C04R14  (R14) - POINTER TO NSI IN CTERMN1                */ 02124000
*/*                                                                  */ 02125000
*/* OUTPUT: C06RC8  (R15) - CONTAINS A RETURN CODE, AS FOLLOWS:      */ 02126000
*/*                         0   USER AREA HAS DATA                   */ 02127000
*/*                      X'17'  AREA NOT VALID                       */ 02128000
*/*                      X'2B'  NOT ENOUGH AREA                      */ 02129000
*/*                      X'2C'  DATA NOT AVAILABLE                   */ 02130000
*/*         C04R14  (R14) - POINTER TO NSI IN CTERMN1                */ 02131000
*/*         TERMINAL NAME AND DEVICE CHARACTERISTICS MOVED TO USER-  */ 02132000
*/*           DESIGNATED AREA FOR EACH DIAL-OUT TERMINAL IN TABLE    */ 02133000
*/*                                                                  */ 02134000
*/* ROUTINES CALLED: C04NIBIN - TO INITIALIZE NIBS           @XM08904*/ 02135000
*/*                                                                  */ 02136000
*/* MACROS USED: VALCHEK - TO VALIDATE USER SUPPLIED AREA            */ 02137000
*/********************************************************************/ 02138000
*                                                                  0746 02139000
*C04DLOUT:                                                         0746 02140000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0746 02141000
C04DLOUT DS    0H                                                  0747 02142000
*   C04SAVE2=C04R14;                /* SAVE RETURN ADDR              */ 02143000
         L     @15,RPHWEA(,PTRRPH)                                 0747 02144000
         ST    C04R14,C04SAVE2+552(,@15)                           0747 02145000
*   C04COUNT=RDSCTTRM;              /* COUNT OF ENTRIES IN DIAL      */ 02146000
         LH    C04COUNT,RDSCTTRM(,RDTPTR)                          0748 02147000
*   RFY                             /* OUT TABLE                     */ 02148000
*     C04SIZE RSTD;                                                0749 02149000
*   C04SIZE=C04COUNT*C04NIBL;                                      0750 02150000
         LR    @11,C04COUNT                                        0750 02151000
         M     @10,C04NIBL                                         0750 02152000
         LR    C04SIZE,@11                                         0750 02153000
*   IF RPLBUFL<C04SIZE              /* ENOUGH AREA IN BUFFER TO      */ 02154000
*     THEN                          /*   BUILD NIBS?                 */ 02155000
         L     @15,OCWRPL(,@15)                                    0751 02156000
         C     C04SIZE,RPLBUFL(,@15)                               0751 02157000
         BNH   @RF00751                                            0751 02158000
*     DO;                                                          0752 02159000
*       C04COUNT=C04ZERO;                                          0753 02160000
         SLR   C04COUNT,C04COUNT                                   0753 02161000
*       C04RC8=C042B;               /* NO - 2B RC AND RETURN         */ 02162000
         LA    C04RC8,43                                           0754 02163000
*     END;                                                         0755 02164000
*   ELSE                                                           0756 02165000
*     DO;                                                          0756 02166000
         B     @RC00751                                            0756 02167000
@RF00751 DS    0H                                                  0757 02168000
*       C04SZE=C04SIZE;             /* SAVE NEEDED SIZE              */ 02169000
         L     @15,RPHWEA(,PTRRPH)                                 0757 02170000
         ST    C04SIZE,C04SZE+552(,@15)                            0757 02171000
*       RFY                                                        0758 02172000
*         C04SIZE UNRSTD;                                          0758 02173000
*       C04R13=C04R13+C04TWEL;      /* 6 WORD SAVE AREA              */ 02174000
         AL    C04R13,@CF01010                                     0759 02175000
*       C04R1=PTRRPH;               /* RESTORE RPH POINTER TO REG 1  */ 02176000
         LR    C04R1,PTRRPH                                        0760 02177000
*       RFY C04R15 RSTD;            /* RESTRICT ACROSS MACRO @DM12421*/ 02178000
*                                                                  0761 02179000
*/*VALCHEK  (C04R15,(C04R13),(RPLAREA),UA,RPLBUFL)  RANGE*/        0762 02180000
*        DO;                                                       0762 02181000
*                                                                  0762 02182000
*        RFY (GPR01P,GPR02P,GPR04P,GPR06P,GPR13P,GPR14P,GPR15P) RSTD;   02183000
*                                        /*DECLARE AND RESTRICT REGS*/  02184000
*                                        /*  1, 2, 4, 6, 13,14, AND 15  02185000
*/         C04R15=0;                    /*SET RETURN CODE TO NO ERROR*/ 02186000
         SLR   C04R15,C04R15                                       0764 02187000
*                                        /*INVOKE XREGSET MACRO TO */   02188000
*                                        /*VERIFY SAVEAREA OPERAND */   02189000
*                                        /*AND PUT IT IN REG 13    */   02190000
*                                                                  0765 02191000
*/*XREGSET  PID('SAVEAREA')PPARM((C04R13))RREG(13,GPR13P)*/        0765 02192000
*        DO;                                                       0765 02193000
*          GPR13P=C04R13;                /*SET RREG SYMBOL TO VALUE*/   02194000
*                                        /*OF PPARM OPERAND        */   02195000
*        END;  /* XREGSET */                                       0767 02196000
*          DCL 1 SAV0028 CHAR(20) BASED(GPR13P),                   0768 02197000
*                                        /*ESTABLISH SAVE AREA MAP   */ 02198000
*                2 SAV10028 CHAR(4),     /*TO SAVE REGS 1,2,4,6, AND 14 02199000
*/               2 SAV20028 CHAR(4),                               0768 02200000
*                2 SAV40028 CHAR(4),                               0768 02201000
*                2 SAV60028 CHAR(4),                               0768 02202000
*                2 SAVE0028 CHAR(4);                               0768 02203000
*          SAV10028=GPR01F;              /* SAVE REGS ONE,     */  0769 02204000
         STCM  GPR01F,15,SAV10028(GPR13P)                          0769 02205000
*          SAV20028=GPR02F;              /*           TWO,     */  0770 02206000
         STCM  GPR02F,15,SAV20028(GPR13P)                          0770 02207000
*          SAV40028=GPR04F;              /*          FOUR,     */  0771 02208000
         STCM  GPR04F,15,SAV40028(GPR13P)                          0771 02209000
*          SAV60028=GPR06F;              /*          SIX,      */  0772 02210000
         STCM  GPR06F,15,SAV60028(GPR13P)                          0772 02211000
*          SAVE0028=GPR14F;              /*     AND FOURTEEN   */  0773 02212000
         STCM  GPR14F,15,SAVE0028(GPR13P)                          0773 02213000
*          GPR04P=GPR01P->RPHTSKID;      /*PUT ADDR OF PROCESS SCHED-*/ 02214000
         L     GPR04P,RPHTSKID(,GPR01P)                            0774 02215000
*                                        /*ULING TABLE IN REG 4      */ 02216000
*                                                                  0775 02217000
*/* ATBUILD (ISTPST)  */                                           0775 02218000
*/* END OF ATBUILD */                    /*INCLUDE PROCESS SCHEDULING * 02219000
*                                        /*   TABLE (ISTPST)         */ 02220000
*          GPR15P=GPR04P->PSTTCBA;       /*PUT TASK CONTROL BLOCK    */ 02221000
         L     GPR15P,PSTTCBA(,GPR04P)                             0775 02222000
*                                        /*ADDRESS IN REGISTER 15    */ 02223000
*          GPR04F=SAV40028;              /*RESTORE REG 4*/         0776 02224000
         ICM   @12,15,SAV40028(GPR13P)                             0776 02225000
