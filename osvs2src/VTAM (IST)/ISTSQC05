*     LEOC     BIT(8) CONSTANT('53'X),  /* END OF CALL COMMAND        * 00133000
*     LXEL     BIT(8) CONSTANT('0B'X),  /* RESET ERROR LOCK           * 00134000
*     LXC      BIT(8) CONSTANT('1B'X),  /* RESET CONDITIONAL          * 00135000
*     LXI      BIT(8) CONSTANT('2B'X),  /* RESET IMMEDIATE            * 00136000
*     LC       BIT(8) CONSTANT('03'X),  /* CONTACT                    * 00137000
*     LWPA     BIT(8) CONSTANT('FD'X); /* WRITE POSITIVE ACK          * 00138000
*DCL  LMT      BIT(2) CONSTANT('11'B), /* TRANSMISSION MODE           * 00139000
*     LMS      BIT(2) CONSTANT('00'B), /* SPECIAL MODE LIKE CONV, EAU * 00140000
*     LMB      BIT(2) CONSTANT('01'B), /* BLOCK MODE                  * 00141000
