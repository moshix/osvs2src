*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00080000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00081000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00082000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00083000
*                                         RECORD                      * 00084000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00085000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00086000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00087000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00088000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00089000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00090000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00091000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00092000
*/* LOGICAL COMMAND CODES */                                       0217 00093000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00094000
