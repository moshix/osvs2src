*                 RDTPTR=RDTPTR+RPRELEN; /* NEXT ENTRY       @D32CKTQ*/ 03127000
         AH    RDTPTR,RPRELEN(,RDTPTR)                             1158 03128000
*                 IF RPRENTRY=RPRENTLN /* IS IT A LINE?      @D32CKTQ*/ 03129000
