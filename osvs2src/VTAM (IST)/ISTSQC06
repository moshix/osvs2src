*                   RNXERCT=C06R2-> /* MOVE TEMP ERROR COUNTS TO     */ 03081000
*                            ICNZX001;  /*   USER AREA        @DM7253*/ 03082000
         MVC   RNXERCT(1,@14),ICNZX001(C06R2)                      1143 03083000
*                 END;              /* END OK VALCHECK               */ 03084000
*                                                                  1144 03085000
*            /********************************************************/ 03086000
*            /*  UNLOCK DAF                                          */ 03087000
