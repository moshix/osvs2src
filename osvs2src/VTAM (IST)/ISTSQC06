         TITLE 'INQUIRE DISPLAY-ISTSQC06                               *00001000
                        '                                               00002000
ISTSQC06 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTSQC06  77.334'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  ST    @14,12(,@13)                                        0001 00010000
         STM   @01,@12,24(@13)                                     0001 00011000
         BALR  @12,0                                               0001 00012000
@PSTART  LA    @09,4095(,@12)                                      0001 00013000
         USING @PSTART,@12                                         0001 00014000
         USING @PSTART+4095,@09                                    0001 00015000
*   C06R1=C06R1;                                                   0469 00016000
*                                                                  0469 00017000
*/********************************************************************/ 00018000
*/* ROUTINE NAME: ISTSQC06 - INQUIRE: TYPE = DISPLAY STATUS          */ 00019000
*/*                                                                  */ 00020000
*/* FUNCTION: PERFORMS HOUSEKEEPING. TESTS TO SEE IF REQUEST IS FOR  */ 00021000
*/*           OPERATOR CONTROL, IF NEEDED SIZE CALCULATION IS THE    */ 00022000
*/*           ONLY REQUIREMENT, OR IF MOVEMENT OF THE RDTE AND       */ 00023000
*/*           RELATED DATA IS ALSO NECESSARY. RELEASES LOCAL MEMORY  */ 00024000
*/*           LOCKS (IF REQUIRED) AFTER SUBROUTINE CALLS.            */ 00025000
*/*                                                                  */ 00026000
*/* INPUT:  C06R1   (R1) - POINTER TO RPL HEADER (ISTRPH)            */ 00027000
*/*         C06R13 (R13) - POINTER TO O/C SAVE AREA                  */ 00028000
*/*                                                                  */ 00029000
*/* OUTPUT: C06R0   (R0) - SIZE OF NEEDED | USED AREA (FOR RDTE AND  */ 00030000
*/*                        RELATED DATA)                             */ 00031000
*/*         C06R15 (R15) - RETURN CODE (0=SUCCESSFUL, 4=UNSUCCESSFUL,*/ 00032000
*/*                        X'2B' = CALCULATION OF SIZE ONLY)         */ 00033000
*/*                                                                  */ 00034000
*/* ROUTINES CALLED: C06VALA  - TO VALIDATE USER-SUPPLIED AREA AND   */ 00035000
*/*                              SET LOCAL MEMORY LOCKS              */ 00036000
*/*                  C06DISPA - TO BE ROUTED TO PROPER SUBROUTINE    */ 00037000
*/*                             TO PROCESS TYPE OF RDTE INQUIRY      */ 00038000
*/*                                                                  */ 00039000
*/* MACROS USED: CRRMAINT - TO ESTABLISH COMPONENT RECOVERY RECORD   */ 00040000
*/*              VSETLCK  - TO RELEASE LOCAL MEMORY LOCK             */ 00041000
*/********************************************************************/ 00042000
*                                                                  0470 00043000
*   RFY                                                            0470 00044000
*    (PTRRPH,                                                      0470 00045000
*     SAREG,                                                       0470 00046000
*     C06R0,                                                       0470 00047000
*     C06R13)RSTD;                                         /*@D32CKTQ*/ 00048000
*   PTRRPH=C06R1;                   /* SAVE POINTER TO RPH   @D32CKTQ*/ 00049000
*                                                                  0471 00050000
         LR    PTRRPH,C06R1                                        0471 00051000
*    DO;                                                           0472 00052000
*/***  CRRMAINT  PROCNM(ISTSQC06)FUNC(SUB)COMP(NS)  ***/           0473 00053000
*    DCL CMZK0010 PTR(31) REG(1);                                  0473 00054000
*    RFY (CMZK0010) RSTD;                                          0474 00055000
*                                                                  0474 00056000
*/* ATBUILD (ISTRPH,ISTCRA,ISTPFCRR,ISTNSCRR)  */                  0475 00057000
*/* LOGICAL CHANNEL COMMAND WORD                                      * 00058000
*/* ADD A DECLARE                                      003 10/18/74   * 00059000
*DCL  1  ISTLCCW BDY(WORD) BASED,                                  0475 00060000
*        2  LCCWOP    CHAR(1),         /* OPERATION CODE -CONTROL     * 00061000
*           3  LCCWSI BIT(1),          /* SESS INIT / START DIALOG FLG* 00062000
*           3  LCCWSD BIT(1),          /* SESS DISCON/END DIALOG FLG  * 00063000
*           3  LCCWRWOP BIT(6),        /* READ/WRITE OPERATION CODE   * 00064000
*              4  * BIT(1),            /* RESERVED                    * 00065000
*              4  LCCWSOP BIT(1),      /* SPECIAL OPERATION LIKE   0475 00066000
*                                         ERASE OR READ CONT          * 00067000
*              4  LCCWCTL BIT(4),      /* CONTROL TYPE                * 00068000
*                 5  LCCWMODE BIT(2),  /* MODE TYPE                   * 00069000
*                 5  LCCWTYPE BIT(2),  /* OPERATION TYPE              * 00070000
*        2  LCCWFLAG  CHAR(1),         /* FLAG BYTE                   * 00071000
*           3  LCCWCD BIT(1),          /* CHAIN DATA                  * 00072000
*           3  LCCWCC BIT(1),          /* CHAIN COMMAND               * 00073000
*           3  LCCWCKPT BIT(1),        /* SUPPRESS 3705 CHECK POINT     00074000
*                                         RECORD                      * 00075000
*           3  LCCWNTVC BIT(1),        /* DO NOT PERFORM VALCHEK      * 00076000
*           3  LCCWIDAT BIT(1),        /* LCCWDATA CONTAINS DATA      * 00077000
*           3  LCCWLSI BIT(1),         /* LCCW SYNCH INDICATOR        * 00078000
*              4  LCCWPER BIT(1),      /* POST=RESP                   * 00079000
*           3  LCCWRFI BIT(1),         /* REQUEST FEEDBACK INDICATOR  * 00080000
*           3  LCCWACI BIT(1),         /* ADDITION COMMAND INDICATOR  * 00081000
*        2  LCCWCNT FIXED(16),         /* DATA COUNT                  * 00082000
*        2  LCCWDATA PTR(31);          /* DATA ADDRESS OR IMMED. DATA * 00083000
*/* LOGICAL COMMAND CODES */                                       0476 00084000
*DCL  LXDQ     BIT(8) CONSTANT('3B'X), /* RESET DEVICE QUEUE          * 00085000
*     LXEC     BIT(8) CONSTANT('4B'X), /* RESET AT END OF COMMAND     * 00086000
*     LXO      BIT(8) CONSTANT('5B'X), /* RESET ORDERLY               * 00087000
*     LCCWPRGE BIT(8) CONSTANT('6B'X), /* PURGE                       * 00088000
*     LWH      BIT(8) CONSTANT('0F'X), /* WRITE HEADER INDI. 1ST BLOCK* 00089000
*     LWRVI    BIT(8) CONSTANT('1F'X), /* WRITE RVI                   * 00090000
*     LWNA     BIT(8) CONSTANT('2F'X), /* WRITE NEGATIVE ACK          * 00091000
*     LWAA     BIT(8) CONSTANT('3F'X), /* WRITE ALTERNATE ACK         * 00092000
*     LWPLG    BIT(8) CONSTANT('5F'X), /* WRITE ACK LEADING GRAPHICS  * 00093000
*     LWNLG    BIT(8) CONSTANT('6F'X), /* WRITE NACK LEADING GRAPHICS * 00094000
*     LSMDE    BIT(8) CONSTANT('4F'X), /* SET MODE                    * 00095000
*     LIFBM    BIT(8) CONSTANT('7F'X), /* INDIC 1ST BLOCK OF MESSAGE  * 00096000
*     LTEST    BIT(8) CONSTANT('FE'X), /* TEST LCCW                   * 00097000
*     LPRMPT   BIT(8) CONSTANT('13'X), /* PREEMPT LCCW                * 00098000
*     LRSTR    BIT(8) CONSTANT('43'X), /* RESTORE LCCW                * 00099000
*     LEXEC    BIT(8) CONSTANT('FF'X), /* SPECIAL CONTROL OPERATION   * 00100000
*     LREAD    BIT(2) CONSTANT('10'B),  /* READ TYPE LCCW             * 00101000
*     LWRITE   BIT(2) CONSTANT('01'B),  /* WRITE TYPE LCCW            * 00102000
*     LCNTRL   BIT(2) CONSTANT('11'B),  /* CONTROL TYPE LCCW          * 00103000
*     LRESET   BIT(4) CONSTANT('1011'B),/* RESET COMMANDS             * 00104000
*     LTIC     BIT(8) CONSTANT('08'X),  /* LOGICAL TIC                * 00105000
*     LWR      BIT(6) CONSTANT('000001'B),  /* WRITE CONVERSATIONAL   * 00106000
*     LWB      BIT(6) CONSTANT('000101'B),  /* WRITE BLOCK            * 00107000
*     LWM      BIT(6) CONSTANT('001001'B),  /* WRITE MESSAGE          * 00108000
*     LWT      BIT(6) CONSTANT('001101'B),  /* WRITE TRANSMISSION     * 00109000
*     LEWM     BIT(6) CONSTANT('011001'B),  /* ERASE/WRITE MESSAGE    * 00110000
*     LEWT     BIT(6) CONSTANT('011101'B),  /* ERASE/WRITE TRNSMSSN   * 00111000
*     LEAU     BIT(6) CONSTANT('010001'B),  /* ERASE ALL UNPROTECTED  * 00112000
*     LRB      BIT(6) CONSTANT('000110'B),  /* READ BLOCK             * 00113000
*     LRM      BIT(6) CONSTANT('001010'B),  /* READ MESSAGE           * 00114000
*     LRT      BIT(6) CONSTANT('001110'B),  /* READ TRANSMISSION      * 00115000
*     LRMOD    BIT(6) CONSTANT('010010'B),  /* READ MODIFIED          * 00116000
*     LRBUF    BIT(6) CONSTANT('010110'B),  /* READ BUFFER            * 00117000
*     LRCONT   BIT(6) CONSTANT('011010'B),  /* READ CONTINUOUS        * 00118000
*     LRPEP    BIT(6) CONSTANT('011110'B),  /* READ PERPETUAL         * 00119000
*     LCPYM    BIT(6) CONSTANT('101001'B),  /* COPY MESSAGE           * 00120000
*     LCPYT    BIT(6) CONSTANT('101101'B),  /* COPY TRANSMISSION      * 00121000
*     LD       BIT(8) CONSTANT('23'X),  /* DISCONNECT                 * 00122000
*     LDEC     BIT(8) CONSTANT('33'X),  /* DISCONNECT WITH END OF CALL* 00123000
*     LEOC     BIT(8) CONSTANT('53'X),  /* END OF CALL COMMAND        * 00124000
*     LXEL     BIT(8) CONSTANT('0B'X),  /* RESET ERROR LOCK           * 00125000
*     LXC      BIT(8) CONSTANT('1B'X),  /* RESET CONDITIONAL          * 00126000
*     LXI      BIT(8) CONSTANT('2B'X),  /* RESET IMMEDIATE            * 00127000
*     LC       BIT(8) CONSTANT('03'X),  /* CONTACT                    * 00128000
*     LWPA     BIT(8) CONSTANT('FD'X); /* WRITE POSITIVE ACK          * 00129000
*DCL  LMT      BIT(2) CONSTANT('11'B), /* TRANSMISSION MODE           * 00130000
*     LMS      BIT(2) CONSTANT('00'B), /* SPECIAL MODE LIKE CONV, EAU * 00131000
*     LMB      BIT(2) CONSTANT('01'B), /* BLOCK MODE                  * 00132000
*     LMM      BIT(2) CONSTANT('10'B); /* MSG OR CONTINUOUS MODE      * 00133000
*/* FOLLOWING EQUATES FOR  - PORT SOLICITOR (DIAL)                    * 00134000
*/* PORT SOLICITOR COMMAND CODES                                      * 00135000
*DCL PSOL    BIT(8) CONSTANT('FE'X), /* SPECIAL PORT SOLICITOR     0478 00136000
*                                       COMMAND                       * 00137000
*    STCTL    BIT(8) CONSTANT('01'X), /* START CONTROL                * 00138000
*    RESTCTL  BIT(8) CONSTANT('02'X), /* RESET & CONTROL - REDRIVE    * 00139000
*    STIPUT   BIT(8) CONSTANT('03'X); /* START INPUT                  * 00140000
*/*   FOLLOWING EQUATES FOR SDLC SUPPORT                              * 00141000
*DCL  LCCHOLD  BIT(8) CONSTANT('01'X),  /* HOLD                       * 00142000
*     LCCRHOLD BIT(8) CONSTANT('02'X),  /* RELEASE HOLD               * 00143000
*     LSEND    BIT(8) CONSTANT ('F1'X), /* SEND                       * 00144000
*     LCLEAR   BIT(8) CONSTANT ('F2'X), /* CLEAR                      * 00145000
*     LCRUD    BIT(8) CONSTANT ('F3'X), /* CLEAR, UNBIND              * 00146000
*     LCRUDNFY BIT(8) CONSTANT ('F4'X), /* CLEAR, UNBIND, NOTIFY      * 00147000
*     LNFY     BIT(8) CONSTANT ('F5'X), /* NOTIFY                     * 00148000
*     LRELCB   BIT(8) CONSTANT ('F6'X), /* RELEASE CB                 * 00149000
*     LRESTSR  BIT(8) CONSTANT ('F7'X), /* RESETSR                    * 00150000
*     LCLRNFY  BIT(8) CONSTANT('F8'X),  /* CLEAR/NOTIFY               * 00151000
*     LCHPUR   BIT(8) CONSTANT('F9'X);  /* PURGE                      * 00152000
*DCL  LCCWSIZE FIXED(8) CONSTANT(8);    /* LCCW SIZE                  * 00153000
*DCL  LRDS     BIT(8) CONSTANT('FF'X);  /* RECORD DEV STATS COMMAND   * 00154000
*DCL  LQUIS    BIT(8) CONSTANT('FE'X);  /* QUIESCE COMMAND            * 00155000
*/*              LOGICAL CHANNEL PROGRAM BLOCK                       */ 00156000
*/*                                                                  */ 00157000
*/*  REVISIONS                                         007 05/19/75  */ 00158000
*/*                                                                  */ 00159000
*DCL  1  ISTLCPB BDY(WORD) BASED(LCPBPTR),                         0483 00160000
*        2 LCPTYPE CHAR(1),            /* TYPE CODE                  */ 00161000
*        2 LCPLNGTH  PTR(8),           /* LENGTH IN BYTES            */ 00162000
*        2 LCPREQTG CHAR(2),           /* LCPB REQ. TAG FIELD        */ 00163000
*          3 LCPFLAGS BIT(8),          /* FLAG BYTE                  */ 00164000
*            4 LCPZRT00 BIT(1),        /* LOGICAL ERROR FLAG         */ 00165000
*            4 LCPZRT01 BIT(1),        /* CONTROL BLOCK ERROR        */ 00166000
