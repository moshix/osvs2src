@RT00691 DS    0H                                                  0692 01434000
         BAL   @14,CCCSTAT1                                        0692 01435000
*           /*********************************************************/ 01436000
*           /* OTHERWISE, RDTE TYPE IS NOT SUPPORTED FOR DISPLAY     */ 01437000
*           /*********************************************************/ 01438000
*           ELSE                                                   0693 01439000
*             RC=FOUR;              /* SET INVALID RDTE RETURN CODE  */ 01440000
         B     @RC00691                                            0693 01441000
@RF00691 LA    RC,4                                                0693 01442000
*   RFY                                                            0694 01443000
*     C06R14 RSTD;                                                 0694 01444000
@RC00691 DS    0H                                                  0694 01445000
@RC00689 DS    0H                                                  0694 01446000
@RC00687 DS    0H                                                  0694 01447000
@RC00685 DS    0H                                                  0694 01448000
@RC00683 DS    0H                                                  0694 01449000
@RC00681 DS    0H                                                  0694 01450000
@RC00677 DS    0H                                                  0695 01451000
*   C06R14=RETDISPA;                /* RESTORE RETURN ADDRESS        */ 01452000
         L     @06,RPHWEA(,PTRRPH)                                 0695 01453000
         L     C06R14,RETDISPA+552(,@06)                           0695 01454000
*   END C06DISPA;                                                  0696 01455000
@EL00002 DS    0H                                                  0696 01456000
@EF00002 DS    0H                                                  0696 01457000
@ER00002 BR    @14                                                 0696 01458000
*                                                                  0697 01459000
*/********************************************************************/ 01460000
*/* ROUTINE NAME: CTRMSTA1 - TERMINAL/LOGICAL UNIT/COMPONENT STATUS  */ 01461000
*/*                                                                  */ 01462000
*/* FUNCTION: CALLED BY C06DISPA TO OBTAIN DATA FOR REMOTE TERMINAL, */ 01463000
*/*           COMPONENT, UTERM, OR LOGICAL UNIT DISPLAY      @D31CKTQ*/ 01464000
*/*           INQUIRIES. FINDS                                       */ 01465000
*/*           REQUIRED SIZE OF BUFFER TO CONTAIN RDTE, TESTS TO SEE  */ 01466000
*/*           THAT USER-SUPPLIED BUFFER IS LARGE ENOUGH, AND--IF IT  */ 01467000
*/*           IS--MOVES RDTE INTO IT.  CALLS CSETSTLU TO     @X50BHTQ*/ 01468000
*/*           DETERMINE OUTSTANDING I/O.                     @X50BHTQ*/ 01469000
*/*           CALLS CTLNSRCH TO LOCATE LINE                  @X50BHTQ*/ 01470000
*/*           NAME AND CTRMSTA2 (IN LINE) TO CHECK FOR AN APPLICA-   */ 01471000
*/*           TION HAVING THIS TERMINAL OR LOGICAL UNIT CONNECTED    */ 01472000
*/*           TO IT.  CTRMSTA2 MOVES THE APPLICATION NAME TO THE     */ 01473000
*/*           AREA. IF THE TERMINAL OR LOGICAL UNIT IS NOT CONNECTED */ 01474000
*/*           TO AN APPLICATION, THE FIELD IS FILLED WITH BLANKS.    */ 01475000
*/*                                                                  */ 01476000
*/* INPUT:  RDTPTR   (R4) - POINTER TO TERMINAL, COMPONENT, UTERM, OR*/ 01477000
*/*                         LOGICAL UNIT RDTE                        */ 01478000
*/*         REGDATA  (R5) - POINTER TO RETURN REG SAVE AREA          */ 01479000
*/*         RPLPTR   (R7) - POINTER TO USER AREA CONTAINING BUFFER   */ 01480000
*/*         RC       (R8) - RETURN CODE                              */ 01481000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (C06DISPA)   */ 01482000
*/*                                                                  */ 01483000
*/* OUTPUT: RC       (R8) - RETURN CODE (TO X'2B' IF USER SUPPLIED   */ 01484000
*/*                         BUFFER AREA IS INADEQUATE, X'0B' @X50BHTQ*/ 01485000
*/*                         IF CALL TO CSETSTLU RESULTS      @X50BHTQ*/ 01486000
*/*                         IN VSETLCK FAILURE)              @X50BHTQ*/ 01487000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (C06DISPA)   */ 01488000
*/*         NEEDSIZE      - SIZE OF BUFFER AREA REQUIRED     @D32CKTQ*/ 01489000
*/*         IF RC=0, RDTE AND APPLICATION NAME (OR BLANKS) MOVED     */ 01490000
*/*                  TO USER-SUPPLIED BUFFER, TERMLCL (FLAG) SET OFF */ 01491000
*/*                                                                  */ 01492000
*/* ROUTINES CALLED: CTLNSRCH - TO LOCATE LINE AND/OR GROUP NAME     */ 01493000
*/*                                                                  */ 01494000
*/* MACROS USED: NONE                                                */ 01495000
*/********************************************************************/ 01496000
*                                                                  0697 01497000
*CTRMSTA1:                                                         0697 01498000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0697 01499000
CTRMSTA1 DS    0H                                                  0698 01500000
*   RETTRA1=C06R14;                 /* SAVE RETURN ADDRESS           */ 01501000
         LA    @06,552                                             0698 01502000
         AL    @06,RPHWEA(,PTRRPH)                                 0698 01503000
         ST    C06R14,RETTRA1(,@06)                                0698 01504000
*   RFY                                                            0699 01505000
*     C06R14 UNRSTD;                                               0699 01506000
*   NEEDSIZE=LENGTH(ISTTERM);       /* EST. SIZE REQUIRED FOR STATUS */ 01507000
         MVC   NEEDSIZE(4,@06),@CF06013                            0700 01508000
*   IF C06CALC=ON2B                 /* CALCULATION-ONLY REQUIRED     */ 01509000
*     THEN                          /* BYPASS MOVEMENT OF RDTE AND   */ 01510000
         CH    C06CALC,@CH04855                                    0701 01511000
         BE    @RT00701                                            0701 01512000
*      ;                            /*    RELATED DATA TO USER AREA  */ 01513000
*   ELSE                                                           0703 01514000
*     IF RPLBUFL<NEEDSIZE           /* SPECIFIED SIZE IS TOO SMALL   */ 01515000
*       THEN                        /* SET CALCULATION-ONLY SWITCH   */ 01516000
         L     @14,RPHWEA(,PTRRPH)                                 0703 01517000
         L     @06,OCWRPL(,@14)                                    0703 01518000
         L     @06,RPLBUFL(,@06)                                   0703 01519000
         C     @06,NEEDSIZE+552(,@14)                              0703 01520000
         BNL   @RF00703                                            0703 01521000
*       RC=TWENTYB;                 /*   ON AND RETURN CODE = X'2B'  */ 01522000
         LA    RC,43                                               0704 01523000
*     ELSE                                                         0705 01524000
*       DO;                                                        0705 01525000
*                                                                  0705 01526000
         B     @RC00703                                            0705 01527000
@RF00703 DS    0H                                                  0706 01528000
*         /***********************************************************/ 01529000
*         /*    MOVE RDTE AND RELATED DATA TO USER-SUPPLIED AREA     */ 01530000
*         /***********************************************************/ 01531000
*    /*   TERMRDT=ISTRTR;              MOVE IN TERMINAL RDTE @ZA25608*/ 01532000
*         IF RPRENTRY=RPRENTLU      /* A LU ?                @ZA25608*/ 01533000
*         THEN TERMRDT=ISTRLU;      /* MOVE IN LU RDTE       @ZA25608*/ 01534000
         CLI   RPRENTRY(RDTPTR),X'81'                              0706 01535000
         BNE   @RF00706                                            0706 01536000
         L     @14,RPHWEA(,PTRRPH)                                 0707 01537000
         L     @14,OCWRPL(,@14)                                    0707 01538000
         L     @14,RPLAREA(,@14)                                   0707 01539000
         MVI   TERMRDT+176(@14),C' '                               0707 01540000
         MVC   TERMRDT+177(35,@14),TERMRDT+176(@14)                0707 01541000
         MVC   TERMRDT(176,@14),ISTRLU(RDTPTR)                     0707 01542000
*         ELSE TERMRDT=ISTRTR;      /* MOVE IN TERM RDTE     @ZA25608*/ 01543000
         B     @RC00706                                            0708 01544000
@RF00706 L     @14,RPHWEA(,PTRRPH)                                 0708 01545000
         L     @14,OCWRPL(,@14)                                    0708 01546000
         L     @14,RPLAREA(,@14)                                   0708 01547000
         MVC   TERMRDT(212,@14),ISTRTR(RDTPTR)                     0708 01548000
*         TERMLCL=OFF;              /* INDICATE REMOTE TERMINAL      */ 01549000
@RC00706 L     @14,RPHWEA(,PTRRPH)                                 0709 01550000
         L     @14,OCWRPL(,@14)                                    0709 01551000
         L     @14,RPLAREA(,@14)                                   0709 01552000
         NI    TERMLCL(@14),B'01111111'                            0709 01553000
*         IF RPRONLIN=ON            /* ACTIVE                @X50BHTQ*/ 01554000
*           &RPRENTRY=RPRENTLU      /* LOGICAL UNIT          @X50BHTQ*/ 01555000
*           THEN                    /* PROVIDE INDICATION OF @X50BHTQ*/ 01556000
         TM    RPRONLIN(RDTPTR),B'00001000'                        0710 01557000
         BNO   @RF00710                                            0710 01558000
         CLI   RPRENTRY(RDTPTR),X'81'                              0710 01559000
         BNE   @RF00710                                            0710 01560000
*           DO;                     /* CLEAR, UNBIND, OR BIND@X50BHTQ*/ 01561000
*             RFY C06R15 RSTD;      /* CSETSTLU RETURN       @X50BHTQ*/ 01562000
*             CALL CSETSTLU;        /* CALL SUBROUTINE TO    @X50BHTQ*/ 01563000
         BAL   @14,CSETSTLU                                        0713 01564000
*             IF RC=ZERO            /* EXAMINE FMCB          @X50BHTQ*/ 01565000
*               THEN                /* NO VSETLCK FAILURE    @X50BHTQ*/ 01566000
         LTR   RC,RC                                               0714 01567000
         BNZ   @RF00714                                            0714 01568000
*               DO;                                        /*@X50BHTQ*/ 01569000
*                 IF C06R15=FOUR    /* BIND IN PROGRESS      @X50BHTQ*/ 01570000
*                   THEN TERMBIP=ON;  /* TELL ISTCFCDP       @X50BHTQ*/ 01571000
         CH    C06R15,@CH00055                                     0716 01572000
         BNE   @RF00716                                            0716 01573000
         L     @14,RPHWEA(,PTRRPH)                                 0717 01574000
         L     @14,OCWRPL(,@14)                                    0717 01575000
         L     @14,RPLAREA(,@14)                                   0717 01576000
         OI    TERMBIP(@14),B'00000010'                            0717 01577000
*                   ELSE                                   /*@X50BHTQ*/ 01578000
*                    IF C06R15=8 /* CLEAR OR UNBIND          @X50BHTQ*/ 01579000
*                       THEN TERMCUIP=ON; /* IN PROGRESS     @X50BHTQ*/ 01580000
         B     @RC00716                                            0718 01581000
@RF00716 CH    C06R15,@CH00061                                     0718 01582000
