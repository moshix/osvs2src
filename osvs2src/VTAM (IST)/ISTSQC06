*            4 LCPZRT08 BIT(1),        /* I/O TRACE ACTIVE FLAG      */ 00190000
*                                      /* (REQ. TAG OVERLAY)         */ 00191000
*              5 LCPZRTL8 BIT(1),      /* DEVICE END NOTIFICATION    */ 00192000
*            4 LCPZRT09 BIT(1),        /* DIALOGUE SCHED DISABLED    */ 00193000
*              5 LCPZRTL9 BIT(1),      /* CRITICAL TEXT FLAG         */ 00194000
*            4 LCPZRT0A BIT(1),        /* SIMULATED CONTACT FLAG     */ 00195000
*            4 LCPZRT0B BIT(1),        /* SIMULATED DISCONNECT FLAG  */ 00196000
