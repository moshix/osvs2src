         BR    @14                                                 0671 01279000
*                                                                  0672 01280000
*/********************************************************************/ 01281000
*/* ROUTINE NAME: C06DISPA - DISPATCHER                              */ 01282000
*/*                                                                  */ 01283000
*/* FUNCTION: CALLED BY MAIN-LINE TO DISPATCH APPROPRIATE SUB-       */ 01284000
*/*           ROUTINE (SEE 'ROUTINES CALLED', BELOW) DEPENDENT       */ 01285000
*/*           UPON RDT ENTRY TYPE.                                   */ 01286000
*/*                                                                  */ 01287000
*/* INPUT:  PTRRPH   (R6) - POINTER TO RPH                   @D32CKTQ*/ 01288000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (MAIN LINE)  */ 01289000
*/*                                                                  */ 01290000
*/* OUTPUT: RC       (R8) - RETURN CODE (SET FOR ERROR ONLY)         */ 01291000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (MAIN LINE)  */ 01292000
*/*                                                                  */ 01293000
*/* ROUTINES CALLED: CLCSTAT1 - TO GET LOCAL DEVICE STATUS           */ 01294000
*/*                  CTRMSTA1 - TO GET REMOTE TERMINAL, LOGICAL      */ 01295000
*/*                             UNIT, OR COMPONENT STATUS            */ 01296000
*/*                  CLNSTAT1 - TO GET LINE STATUS                   */ 01297000
*/*                  CRNSTAT1 - TO GET COMMUNICATION CONTROLLER      */ 01298000
*/*                             (3705) STATUS                        */ 01299000
*/*                  CAPSTAT1 - TO GET APPLICATION STATUS            */ 01300000
*/*                  CCCSTAT1 - TO GET CLUSTER CONTROLLER OR         */ 01301000
*/*                             COMPONENT CLUSTER STATUS             */ 01302000
*/*                  CPASTAT1 - TO PATH INFO FOR PHYS UNIT   @D32CKTQ*/ 01303000
*/*                                                                  */ 01304000
*/* MACROS USED: NONE                                                */ 01305000
*/********************************************************************/ 01306000
*                                                                  0672 01307000
*C06DISPA:                                                         0672 01308000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0672 01309000
C06DISPA DS    0H                                                  0673 01310000
*   RFY                                                            0673 01311000
*    (RDTPTR,                                                      0673 01312000
*     C06R14)RSTD;                                                 0673 01313000
*   RETDISPA=C06R14;                /* SAVE RETURN ADDRESS           */ 01314000
         L     @06,RPHWEA(,PTRRPH)                                 0674 01315000
         ST    C06R14,RETDISPA+552(,@06)                           0674 01316000
*   RFY                                                            0675 01317000
*     C06R14 UNRSTD;                                               0675 01318000
*   RDTPTR=OCWDRDTE;                /* ESTABLISH BASE FOR RDT ENTRY  */ 01319000
*                                                                  0676 01320000
         L     RDTPTR,OCWDRDTE(,@06)                               0676 01321000
*   /*****************************************************************/ 01322000
*   /* IS REQUEST FOR DISPLAY PATHS?                         @D32CKTQ*/ 01323000
*   /*****************************************************************/ 01324000
*                                                                  0677 01325000
*IF RPLVTUSE=ON                     /* CFCDP SETS FOR PATHS  @D32CKTQ*/ 01326000
* THEN                              /* CAN ONLY DISPLAY      @D32CKTQ*/ 01327000
         L     @14,OCWRPL(,@06)                                    0677 01328000
         TM    RPLVTUSE+16(@14),B'01000000'                        0677 01329000
         BNO   @RF00677                                            0677 01330000
*  IF RPRENTRY=RPRENTCC             /* PATHS FOR PU          @D32CKTQ*/ 01331000
*     &RPRHDTYP=RPRENTSW            /* IN SWITCHED SUBAREA   @D32CKTQ*/ 01332000
*   THEN                                                           0678 01333000
         CLI   RPRENTRY(RDTPTR),X'71'                              0678 01334000
         BNE   @RF00678                                            0678 01335000
         CLI   RPRHDTYP(RDTPTR),X'04'                              0678 01336000
         BNE   @RF00678                                            0678 01337000
*     CALL CPASTAT1;                /* PROCESSING SUBROUTINE @D32CKTQ*/ 01338000
         BAL   @14,CPASTAT1                                        0679 01339000
*   ELSE                                                           0680 01340000
*     RC=FOUR;                      /* INVALID NODE TYPE     @D32CKTQ*/ 01341000
*                                                                  0680 01342000
         B     @RC00678                                            0680 01343000
@RF00678 LA    RC,4                                                0680 01344000
*   /*****************************************************************/ 01345000
*   /* RDTE THAT OF A LOCAL TERMINAL?                                */ 01346000
*   /*****************************************************************/ 01347000
*   ELSE                                                           0681 01348000
*   IF RPRENTRY=RPRENTTR&           /* IF ENTRY IS A TERMINAL AND    */ 01349000
*       RPRNSNEA->RPRENTRY          /* NEXT SCHEDULABLE NODE ENTRY   */ 01350000
*       =RPRENTLC                   /* IS A LOCAL TERMINAL           */ 01351000
*     THEN                                                         0681 01352000
         B     @RC00677                                            0681 01353000
