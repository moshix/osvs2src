*          3 LCPCHAI1 PTR(8),                                      0483 00198000
*            4 LCPCHNG BIT(1),         /* GATE BIT                   */ 00199000
*          3 LCPCHAI2 PTR(24),         /* ADDRESS FIELD              */ 00200000
*        2 LCPRUCNT PTR(8),            /* NUMBER OF RU FOR THIS LCPB */ 00201000
*          3 LCPZMIRC CHAR(1),         /* MINOR RETURN CODE          */ 00202000
*        2 LCPFLAG2 CHAR(1),           /* SECOND FLAG BYTE           */ 00203000
*          3 LCPPRFLG BIT(1),          /* PACING REQUIRED            */ 00204000
*          3 LCPASYN  BIT(1),          /* ASYNCHRONOUS LCPB          */ 00205000
*          3 LCPRSTRT BIT(1),          /* RESTART LCPB               */ 00206000
*          3 LCPTLBPI BIT(1),          /* TPIOS BLOCK LCCW           */ 00207000
*            4 LCPZRIMM BIT(1),        /* RESET IMMED REQUIRED ON ERR*/ 00208000
*          3 LCPFID0  BIT(1),          /* FID0 TH INDICATOR FOR RECORD* 00209000
*            4 LCPEXRES BIT(1),        /* EXCEP. RESPONSE SPEC. 3270R*/ 00210000
*          3 LCPDSTAT BIT(1),          /* DIALOGUE STATE BIT, 3270R  */ 00211000
*                                      /* 0=REQ. REJECT,1=STATE ERROR*/ 00212000
*          3 LCPFREE  BIT(1),          /* DEALLOCATE LCPB 3270R      */ 00213000
*          3 LCPATOP  BIT(1),          /* ATOP LCPB  3270R           */ 00214000
*        2 LCPSEQ   FIXED(16),         /* OUTBOUND SEQUENCE NUMBER   */ 00215000
*          3 LCPRRCCT PTR(8),          /* COUNT OF RESPONSES RECEIVED*/ 00216000
*          3 LCPRRCNT PTR(8),          /* COUNT OF RESPONSES REQUESTED* 00217000
*        2 LCPLCCW1 PTR(31),           /* ADDR FIRST LCCW FOR FID1   */ 00218000
*        2 LCPCID CHAR(4) BDY(WORD),   /* COMMUNICATION ID           */ 00219000
*          3 LCPSAF BIT(16),           /* SOURCE IDENTIFICATION      */ 00220000
*          3 LCPDAF BIT(16),           /* DESTINATION IDENTIFICATION */ 00221000
*        2 LCPNXLCW PTR(31),           /* ADDR NEXT LCCW             */ 00222000
*        2 LCPFDBA PTR(31),            /* BTU ADDRESS                */ 00223000
*          3 LCPFDB1 PTR(8),                                       0483 00224000
*          3 LCPFDB2 PTR(24),          /* ADDRESS FIELD              */ 00225000
*        2 LCPTLBAD PTR(31),           /* ADDRESS OF TLB             */ 00226000
*        2 LCPFOSN  BIT(16),           /* SEQUENCE NUMBER FIRST RU   */ 00227000
*        2 LCPFSNCH FIXED(16),         /* 1ST SEQ NUMBER IN RU CHAIN */ 00228000
*        2 LCPSTYPE FIXED(8),          /* LCPB SUB-TYPE X'FF' = RESET,* 00229000
*                                      /* X'00' = NON-RESET          */ 00230000
*        2 LCPFLG1 CHAR(1),            /* FLAGS                      */ 00231000
*          3 LCPRESET BIT(1),          /* A TPQUE WITH RESET QUEUED  */ 00232000
*                                      /* THIS ELEMENT               */ 00233000
*          3 LCPINTER BIT(1),          /* INTERNAL LCPB              */ 00234000
*          3 LCPDIRPT BIT(1),          /* NOT SEND W/POST=SCHED=0    */ 00235000
*          3 LCPREDRV BIT(1),          /* LCPB IS FOR REDRIVE        */ 00236000
*          3 LCPSTLB  BIT(1),          /* TLB NOT TO BE FREED        */ 00237000
*          3 LCPTRANS BIT(1),          /* DATA INCLUDES CONTROL CHARS*/ 00238000
*          3 LCPRSV78 BIT(2),          /* RESERVED                   */ 00239000
*        2 LCPLCCWC BIT(8),            /* COUNT OF LCCWS             */ 00240000
*        2 LCPRSV77 CHAR(1),           /* RESERVED                   */ 00241000
*        2 LCPFLCCW PTR(31),           /* ADDR OF FIRST LCCW         */ 00242000
*          3 LCPUBFSB PTR(31),         /* UNBIND FSB ADDRESS         */ 00243000
*        2 LCPCLREP PTR(31),           /* PTR TO CLEAR LCPB IN UCB   */ 00244000
*/*  NOTE:  THE FOLLOWING FIELD - LCPLCCW MUST BE THE LAST FIELD     */ 00245000
*/*         IN THIS CONTROL BLOCK.  THE CONTROL BLOCK ISTDCLCP HAS   */ 00246000
*/*         FIELDS IN IT WHICH MUST BE CONTIGUOUS WITH LCPLCCW       */ 00247000
*        2 LCPLCCW  CHAR(LENGTH(ISTLCCW)) BDY(WORD), /* FIRST LCCW   */ 00248000
