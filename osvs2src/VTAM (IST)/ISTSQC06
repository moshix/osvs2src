         L     @14,RPHWEA(,PTRRPH)                                 0664 01249000
         L     C06R0,NEEDSIZE+552(,@14)                            0664 01250000
*         END;                      /* END USER AREA NOT SPECIFIED   */ 01251000
*     END;                          /* END REQUEST ACB=OPERATOR ACB  */ 01252000
*                                                                  0666 01253000
*   /*****************************************************************/ 01254000
*   /* ACB NOT FOR OPERATOR CONTROL                                  */ 01255000
*   /*****************************************************************/ 01256000
*                                                                  0667 01257000
*   ELSE                                                           0667 01258000
*     RC=FOUR;                      /* SET RETURN CODE = 4           */ 01259000
*                                                                  0667 01260000
         B     @RC00605                                            0667 01261000
@RF00605 LA    RC,4                                                0667 01262000
*   /*****************************************************************/ 01263000
*   /*      RETURN TO ISTSQC00                                       */ 01264000
*   /*****************************************************************/ 01265000
*                                                                  0668 01266000
*   C06R15=RC;                      /* LOAD RETURN CODE              */ 01267000
@RC00605 LR    C06R15,RC                                           0668 01268000
*   C06R13=C06R13->SAVBACK;         /* POINT TO CALLERS SAVE AREA    */ 01269000
