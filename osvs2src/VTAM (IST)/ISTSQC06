*                   DO;                  /*RESTORE PSW PROTECTION KEY*/ 01206000
*        GEN SETS(2);                                              0649 01207000
         MODESET KEYADDR=(2),WORKREG=2                                  01208000
*                   END;                 /* RESTORE REGISTERS        */ 01209000
*                 GEN REFS(GPR10F,GPR13P) SETS(GPR10F,GPR11F,      0651 01210000
*                 GPR12F,GPR13F,GPR14F,GPR02F);                    0651 01211000
         LR    GPR13F,GPR10F                                            01212000
         LM    GPR10F,GPR12F,0(GPR13P)                                  01213000
         L     GPR14F,12(GPR13P)                                        01214000
         L     GPR02F,16(GPR13P)                                        01215000
*                 IF GPR15F=4 THEN GPR15F=0;                       0652 01216000
         CH    GPR15F,@CH00055                                     0652 01217000
         BNE   @RF00652                                            0652 01218000
         SLR   GPR15F,GPR15F                                       0653 01219000
*                 C06R15=GPR15F;         /*PUT RETURN CODE IN RCREG*/   01220000
*                                                                  0654 01221000
