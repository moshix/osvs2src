*/* ROUTINE NAME: CLNSTAT1 - LINE STATUS                             */ 01931000
*/*                                                                  */ 01932000
*/* FUNCTION: CALLED BY C06DISPA TO DETERMINE INFORMATION FOR LINE-  */ 01933000
*/*           TYPE DISPLAY INQUIRIES.  CALCULATES MAXIMUM REQUIRED  */  01934000
*/*           SIZE OF USER AREA TO CONTAIN RDTE AND RELATED DATA,    */ 01935000
*/*           TESTS TO SEE THAT USER SUPPLIED AREA IS LARGE ENOUGH,  */ 01936000
*/*           AND--IF IT IS--MOVES RDTE TO IT.  CALLS CLGPSRCH TO    */ 01937000
*/*           LOCATE RN NAME AND TO                          @D32CKTQ*/ 01938000
*/*           LOCATE GROUP NAME AND CLNSTAT3  TO LOCATE      @X50BHTQ*/ 01939000
*/*           TERMINAL NAME IF LINE IS SWITCHED OR CLNSTAT5 TO       */ 01940000
*/*           LOCATE TERMINAL NAMES IF LINE IS LEASED.  CLNSTAT3     */ 01941000
*/*           MOVES TERMINAL NAME INTO USER AREA IF TERMINAL IS      */ 01942000
*/*           DIALED IN, BLANKS IF ONE IS NOT DIALED IN.             */ 01943000
*/*                                                                  */ 01944000
*/* INPUT:  RDTPTR   (R4) - POINTER TO LINE RDTE                     */ 01945000
*/*         RC       (R8) - RETURN CODE                              */ 01946000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (C06DISPA)   */ 01947000
*/*                                                                  */ 01948000
*/* OUTPUT: RC       (R8) - RETURN CODE (TO X'2B' IF USER SUPPLIED   */ 01949000
*/*                         BUFFER AREA IS INADEQUATE, TO    @X50BHTQ*/ 01950000
*/*                         X'0B' IF VSETLCK FAILS).         @X50BHTQ*/ 01951000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (C06DISPA)   */ 01952000
*/*         NEEDSIZE      - SIZE OF BUFFER AREA REQUIRED     @D32CKTQ*/ 01953000
*/*         IF RC=0, RDTE AND  TERMINAL NAME                 @X50BHTQ*/ 01954000
*/*                  (OR BLANKS) MOVED TO USER-SUPPLIED BUFFER,      */ 01955000
*/*                  ALONG WITH TERMINAL STATUS FLAGS        @X50BHTQ*/ 01956000
*/*                                                                  */ 01957000
*/* ROUTINES CALLED: CLGPSRCH - TO LOCATE GROUP NAME OF WHICH LINE   */ 01958000
*/*                             IS A PART                            */ 01959000
*/*                  CLNSTAT5 - TO LOCATE TERMINAL NAMES ON LEASED   */ 01960000
*/*                             LINE                                 */ 01961000
*/*                                                                  */ 01962000
*/* MACROS USED: NONE                                                */ 01963000
*/********************************************************************/ 01964000
*                                                                  0814 01965000
*CLNSTAT1:                                                         0814 01966000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0814 01967000
CLNSTAT1 DS    0H                                                  0815 01968000
*   RFY                                                            0815 01969000
*      (LINEPTR,                                                   0815 01970000
*     COUNT)RSTD;                                          /*@D32CKTQ*/ 01971000
*   RETLNA1=C06R14;                 /* SAVE RETURN ADDRESS           */ 01972000
         LA    @10,552                                             0816 01973000
         AL    @10,RPHWEA(,PTRRPH)                                 0816 01974000
         ST    C06R14,RETLNA1(,@10)                                0816 01975000
*   RFY                                                            0817 01976000
*     C06R14 UNRSTD;                                               0817 01977000
*   NEEDSIZE=LENGTH(ISTLINE);       /* ESTIMATE SIZE FOR STATUS      */ 01978000
         MVC   NEEDSIZE(4,@10),@CF06014                            0818 01979000
*   /*****************************************************************/ 01980000
*   /* NOTE THAT FOR SWITCHED SDLC LINES IT IS NECESSARY TO  @DM14008*/ 01981000
*   /* OBTAIN THE COUNT OF SUBNODES BY COUNTING PUX AND LUX  @DM14008*/ 01982000
*   /* ENTRIES                                               @DM14008*/ 01983000
*   /*****************************************************************/ 01984000
*IF RLNRATRN=ON&                    /* SDLC LINK             @DM14008*/ 01985000
*   DEVCSWL=ON                      /* SWITCHED              @DM14008*/ 01986000
*  THEN DO;                                                /*@DM14008*/ 01987000
         TM    RLNRATRN(RDTPTR),B'00000010'                        0819 01988000
         BNO   @RF00819                                            0819 01989000
         TM    DEVCSWL+44(RDTPTR),B'00010000'                      0819 01990000
         BNO   @RF00819                                            0819 01991000
*    COUNT=1;                       /* 1 FOR PHYS UNIT       @DM14008*/ 01992000
         LA    COUNT,1                                             0821 01993000
*    RDTPTR=RDTPTR+RPRELEN;         /* AFTER LINE IS PUX     @DM14008*/ 01994000
         AH    RDTPTR,RPRELEN(,RDTPTR)                             0822 01995000
*    RDTPTR=RPXFLUX;                /* POINT TO 1ST LUX      @DM14008*/ 01996000
         L     RDTPTR,RPXFLUX(,RDTPTR)                             0823 01997000
*    DO WHILE RDTPTR^=ZERO;         /* EXHAUST LUX CHAIN     @DM14008*/ 01998000
         B     @DE00824                                            0824 01999000
@DL00824 DS    0H                                                  0825 02000000
*      COUNT=COUNT+1;               /* BUMP COUNT            @DM14008*/ 02001000
         AH    COUNT,@CH00063                                      0825 02002000
*      RDTPTR=RLXNLUX;              /*POINT TO NEXT IN CHAIN @DM14008*/ 02003000
         L     RDTPTR,RLXNLUX(,RDTPTR)                             0826 02004000
*      END;                         /* END DO-WHILE GROUP    @DM14008*/ 02005000
@DE00824 LTR   RDTPTR,RDTPTR                                       0827 02006000
         BNZ   @DL00824                                            0827 02007000
*    RDTPTR=OCWDRDTE;               /* GET LINE RDTE BACK    @DM14008*/ 02008000
         L     @14,RPHWEA(,PTRRPH)                                 0828 02009000
         L     RDTPTR,OCWDRDTE(,@14)                               0828 02010000
*    END;                           /* END SW SDLC LINK      @DM14008*/ 02011000
*ELSE                               /* NOT SW SDLC LINK:     @DM14008*/ 02012000
*COUNT = RLNCTETR+                  /* COUNT OF TERMINALS PLUS       */ 02013000
*        RLNCTECL+                  /* COUNT OF CLUSTER CONTROLLERS  */ 02014000
*        RLNCTECM+                  /* PLUS COUNT OF COMPONENTS PLUS */ 02015000
*         (2*RLNCTEIN);             /* TWICE THE COUNT OF INTERMEDIATE  02016000
*                                                           NODES    */ 02017000
         B     @RC00819                                            0830 02018000
@RF00819 LH    COUNT,RLNCTETR(,RDTPTR)                             0830 02019000
         AH    COUNT,RLNCTECL(,RDTPTR)                             0830 02020000
         AH    COUNT,RLNCTECM(,RDTPTR)                             0830 02021000
         LH    @14,RLNCTEIN(,RDTPTR)                               0830 02022000
         ALR   @14,@14                                             0830 02023000
         ALR   COUNT,@14                                           0830 02024000
*IF COUNT > 1                       /* SIZE OF DSECT (ISTLINE) MUST  */ 02025000
*      THEN NEEDSIZE =              /*     BE INCREMENTED            */ 02026000
@RC00819 CH    COUNT,@CH00063                                      0831 02027000
         BNH   @RF00831                                            0831 02028000
*          NEEDSIZE+(9*(COUNT-1));  /* TOTAL SIZE ESTIMATE           */ 02029000
         LA    @14,552                                             0832 02030000
         AL    @14,RPHWEA(,PTRRPH)                                 0832 02031000
         LR    @10,COUNT                                           0832 02032000
         BCTR  @10,0                                               0832 02033000
         MH    @10,@CH00033                                        0832 02034000
         AL    @10,NEEDSIZE(,@14)                                  0832 02035000
         ST    @10,NEEDSIZE(,@14)                                  0832 02036000
*   ELSE                            /* DSECT SIZE IS SUFFICIENT      */ 02037000
*     ;                                                            0833 02038000
@RF00831 DS    0H                                                  0834 02039000
*   IF C06CALC=ON2B                 /* CALCULATION-ONLY REQUIRED     */ 02040000
*     THEN                          /* BYPASS MOVEMENT OF RDTE AND   */ 02041000
         CH    C06CALC,@CH04855                                    0834 02042000
         BE    @RT00834                                            0834 02043000
*      ;                            /*    RELATED DATA TO USER AREA  */ 02044000
*   ELSE                                                           0836 02045000
*     IF RPLBUFL<NEEDSIZE           /* SPECIFIED SIZE IS TOO SMALL   */ 02046000
*       THEN                        /* SET CALCULATION-ONLY SWITCH   */ 02047000
         L     @14,RPHWEA(,PTRRPH)                                 0836 02048000
         L     @10,OCWRPL(,@14)                                    0836 02049000
         L     @10,RPLBUFL(,@10)                                   0836 02050000
         C     @10,NEEDSIZE+552(,@14)                              0836 02051000
         BNL   @RF00836                                            0836 02052000
*       RC=TWENTYB;                 /*   ON AND RETURN CODE = X'2B'  */ 02053000
         LA    RC,43                                               0837 02054000
*      ELSE                                                        0838 02055000
*         DO;                                                      0838 02056000
*                                                                  0838 02057000
         B     @RC00836                                            0838 02058000
@RF00836 DS    0H                                                  0839 02059000
*         /***********************************************************/ 02060000
*         /*    MOVE RDTE AND RELATED DATA TO USER-SUPPLIED AREA     */ 02061000
*         /***********************************************************/ 02062000
*                                                                  0839 02063000
*           LINEPTR=RPLAREA;        /* ESTABLISH BASE FOR USER AREA  */ 02064000
         L     @14,RPHWEA(,PTRRPH)                                 0839 02065000
         L     @14,OCWRPL(,@14)                                    0839 02066000
         L     LINEPTR,RPLAREA(,@14)                               0839 02067000
*           IF RLNPORT=ON           /* LINE ENTRY IS FOR A PORT      */ 02068000
*             THEN                  /* MOVE PORT RDTE TO USER-       */ 02069000
         TM    RLNPORT(RDTPTR),B'00010000'                         0840 02070000
         BNO   @RF00840                                            0840 02071000
*             LINERDT=ISTRPT;       /*   SPECIFIED AREA      @D32CKTQ*/ 02072000
         MVC   LINERDT(160,LINEPTR),ISTRPT(RDTPTR)                 0841 02073000
*           ELSE                    /* ENTRY IS FOR A LEASED LINE    */ 02074000
*             LINERDT=ISTRLN;       /* MOVE LEASED LINE RDTE         */ 02075000
*                                                                  0842 02076000
         B     @RC00840                                            0842 02077000
@RF00840 MVI   LINERDT+116(LINEPTR),C' '                           0842 02078000
         MVC   LINERDT+117(43,LINEPTR),LINERDT+116(LINEPTR)        0842 02079000
         MVC   LINERDT(116,LINEPTR),ISTRLN(RDTPTR)                 0842 02080000
*         /***********************************************************/ 02081000
*         /*   CHECK PRESENCE OF POSSIBLE GROUP OR DIALSET   @DM09571*/ 02082000
*         /***********************************************************/ 02083000
*                                                                  0843 02084000
*           CALL CLGPSRCH;          /* CALL SUBROUTINE               */ 02085000
@RC00840 BAL   @14,CLGPSRCH                                        0843 02086000
*           NEEDSIZE=LENGTH(ISTLINE);/* RESET NEEDED SIZE            */ 02087000
         L     @14,RPHWEA(,PTRRPH)                                 0844 02088000
         MVC   NEEDSIZE+552(4,@14),@CF06014                        0844 02089000
*           IF DEVCSWL^=ON          /* IF NOT SWITCHED       @DM13677*/ 02090000
*             THEN                                                 0845 02091000
         TM    DEVCSWL+44(RDTPTR),B'00010000'                      0845 02092000
         BO    @RF00845                                            0845 02093000
*             CALL CLNSTAT5;        /* CALL SUBRTN FOR LEASED LINES  */ 02094000
         BAL   @14,CLNSTAT5                                        0846 02095000
*           ELSE                                                   0847 02096000
*             CALL CLNSTAT3;        /* LOCATE SUBNODE NAMES  @DM14008*/ 02097000
         B     @RC00845                                            0847 02098000
@RF00845 BAL   @14,CLNSTAT3                                        0847 02099000
*         END;                                                     0848 02100000
@RC00845 DS    0H                                                  0849 02101000
*   RFY                                                            0849 02102000
*     C06R14 RSTD;                                                 0849 02103000
@RC00836 DS    0H                                                  0849 02104000
@RT00834 DS    0H                                                  0850 02105000
*   C06R14=RETLNA1;                 /* RESTORE RETURN ADDRESS        */ 02106000
         L     @10,RPHWEA(,PTRRPH)                                 0850 02107000
         L     C06R14,RETLNA1+552(,@10)                            0850 02108000
*   END CLNSTAT1;                                                  0851 02109000
@EL00005 DS    0H                                                  0851 02110000
@EF00005 DS    0H                                                  0851 02111000
@ER00005 BR    @14                                                 0851 02112000
*                                                                  0852 02113000
*/********************************************************************/ 02114000
*/* ROUTINE NAME: CLGPSRCH - LOCATE GROUP NAME                       */ 02115000
*/*                                                                  */ 02116000
*/* FUNCTION: CALLED BY CLNSTAT1 TO LOCATE NAME OF GROUP (OR DIALSET)*/ 02117000
*/*           TO WHICH LINE BELONGS (IF ANY) AND MOVE IT TO          */ 02118000
*/*           USER-SUPPLIED BUFFER. IF THERE IS NO GROUP NAME, THE   */ 02119000
*/*           CORRESPONDING FIELD IS FILLED WITH BLANKS.             */ 02120000
*/*           ALSO PUTS 370X NAME IN USER BUFFER.            @D32CKTQ*/ 02121000
*/*                                                                  */ 02122000
*/* INPUT:  LINEPTR  (R1) - POINTER TO USER-SUPPLIED BUFFER          */ 02123000
*/*         RDTPTR   (R4) - POINTER TO LINE RDT              @D32CKTQ*/ 02124000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (CLNSTAT1)   */ 02125000
*/*                                                                  */ 02126000
*/* OUTPUT: GROUP NAME OR BLANKS MOVED INTO USER BUFFER              */ 02127000
*/*         RDTPTR   (R4) - POINTER TO LINE RDT                      */ 02128000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (CLNSTAT1)   */ 02129000
*/*                                                                  */ 02130000
*/* ROUTINES CALLED: NONE                                            */ 02131000
*/*                                                                  */ 02132000
*/* MACROS USED: NONE                                                */ 02133000
*/********************************************************************/ 02134000
*                                                                  0852 02135000
*CLGPSRCH:                                                         0852 02136000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0852 02137000
*                                                                  0852 02138000
CLGPSRCH DS    0H                                                  0853 02139000
*   /*****************************************************************/ 02140000
*   /*                                                               */ 02141000
*   /* SEARCH FOR GROUP, DIALSET OR COMMUNICATIONS CONTROLLER (370X) */ 02142000
*   /* RDTE.                                                         */ 02143000
*   /*                                                               */ 02144000
*   /*****************************************************************/ 02145000
*                                                                  0853 02146000
*   DO WHILE RPRENTRY^=RPRENTGP     /* STOP ON GROUP RDTE OR         */ 02147000
*           &RPRENTRY^=RPRENTDS     /* LCDO RDTE OR          @DM09571*/ 02148000
*           &RPRENTRY^=RPRENTRN;    /* 370X RDTE                     */ 02149000
         B     @DE00853                                            0853 02150000
@DL00853 DS    0H                                                  0854 02151000
*     RDTPTR=RPRNSNEA;              /* BACK-UP ONE ENTRY             */ 02152000
         L     RDTPTR,RPRNSNEA(,RDTPTR)                            0854 02153000
*   END;                                                           0855 02154000
@DE00853 CLI   RPRENTRY(RDTPTR),X'30'                              0855 02155000
         BE    @DC00853                                            0855 02156000
         CLI   RPRENTRY(RDTPTR),X'20'                              0855 02157000
         BE    @DC00853                                            0855 02158000
         CLI   RPRENTRY(RDTPTR),X'01'                              0855 02159000
         BNE   @DL00853                                            0855 02160000
@DC00853 DS    0H                                                  0856 02161000
*   IF RPRENTRY=RPRENTGP            /* IF LINE IS PART OF A GROUP    */ 02162000
*     |RPRENTRY=RPRENTDS            /* OR LCDO               @DM09571*/ 02163000
*       THEN DO;                    /* THEN MOVE GROUP NAME TO USER  */ 02164000
         CLI   RPRENTRY(RDTPTR),X'30'                              0856 02165000
         BE    @RT00856                                            0856 02166000
         CLI   RPRENTRY(RDTPTR),X'20'                              0856 02167000
         BNE   @RF00856                                            0856 02168000
@RT00856 DS    0H                                                  0857 02169000
*              LINEGN=RPRNAME;      /*    PROVIDED AREA              */ 02170000
         MVC   LINEGN(8,LINEPTR),RPRNAME(RDTPTR)                   0858 02171000
*              DO WHILE RPRENTRY^=RPRENTRN; /* SEEK RN RDTE  @D32CKTQ*/ 02172000
         B     @DE00859                                            0859 02173000
@DL00859 DS    0H                                                  0860 02174000
*                RDTPTR=RPRNSNEA;   /* LOOK UP NSNEA CHAIN   @D32CKTQ*/ 02175000
         L     RDTPTR,RPRNSNEA(,RDTPTR)                            0860 02176000
*                END;                                      /*@D32CKTQ*/ 02177000
@DE00859 CLI   RPRENTRY(RDTPTR),X'01'                              0861 02178000
         BNE   @DL00859                                            0861 02179000
*              LINE370X=RPRNAME;    /* MOVE IN RN NAME       @D32CKTQ*/ 02180000
         MVC   LINE370X(8,LINEPTR),RPRNAME(RDTPTR)                 0862 02181000
*           END;                                                   0863 02182000
*    ELSE DO;                       /* IF NOT, MOVE BLANKS TO GROUP  */ 02183000
         B     @RC00856                                            0864 02184000
@RF00856 DS    0H                                                  0865 02185000
*           LINEGN='        ';      /*   NAME FIELD IN USER AREA     */ 02186000
         MVC   LINEGN(8,LINEPTR),@CC04841                          0865 02187000
*           LINE370X=RPRNAME;       /* WE FOUND RN W/O GROUP @D32CKTQ*/ 02188000
         MVC   LINE370X(8,LINEPTR),RPRNAME(RDTPTR)                 0866 02189000
*        END;                                                      0867 02190000
*   RDTPTR=OCWDRDTE;                /* RESTORE POINTER TO LINE RDTE  */ 02191000
@RC00856 L     @10,RPHWEA(,PTRRPH)                                 0868 02192000
         L     RDTPTR,OCWDRDTE(,@10)                               0868 02193000
*   END CLGPSRCH;                   /* END GROUP SEARCH              */ 02194000
@EL00006 DS    0H                                                  0869 02195000
@EF00006 DS    0H                                                  0869 02196000
@ER00006 BR    @14                                                 0869 02197000
*/********************************************************************/ 02198000
*/* ROUTINE NAME: CLNSTAT3 - LOCATE SUBNODE NAME             @DM14008*/ 02199000
*/*                          (SWITCHED LINE)                 @DM14008*/ 02200000
*/*                                                          @DM14008*/ 02201000
*/* FUNCTION: CALLED BY CLNSTAT1 TO LOCATE NAME OF DIALED-IN @DM14008*/ 02202000
*/*          TERMINAL (IF LINE IS A PORT) OR PHYSICAL UNIT   @DM14008*/ 02203000
*/*          WITH ITS SUBORDINATE LOGICAL UNITS (IF LINE IS  @DM14008*/ 02204000
*/*          A SWITCHED SDLC LINK).  FOR EACH LU PROVIDES AN @DM14008*/ 02205000
*/*          INDICATION IF A CLEAR, UNBIND, OR BIND IS IN    @DM14008*/ 02206000
*/*          PROGRESS.                                       @DM14008*/ 02207000
*/*                                                          @DM14008*/ 02208000
*/* INPUT:  LINEPTR  (R1) - POINTER TO THE USER BUFFER       @DM14008*/ 02209000
*/*         RDTPTR   (R4) - POINTER TO THE RDT               @DM14008*/ 02210000
*/*         NEEDSIZE      - SIZE OF LINE RDT                 @DM14008*/ 02211000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE      @DM14008*/ 02212000
*/*                                                          @DM14008*/ 02213000
*/* OUTPUT:  TERMINAL, PHYS UNIT, LOGICAL UNIT NAME(S) MOVED @DM14008*/ 02214000
*/*          TO USER BUFFER ALONG WITH SETTING OF NODE TYPE  @DM14008*/ 02215000
*/*          FLAG, STATUS FLAG, AND INDICATION IF BIND,      @DM14008*/ 02216000
*/*          UNBIND, OR CLEAR IS IN PROGRESS AGAINST AN LU.  @DM14008*/ 02217000
*/*          RDTPTR    (R4) - POINTER TO LINE RDTE           @DM14008*/ 02218000
*/*          NEEDSIZE       - SIZE OF LINE RDTE PLUS DATA    @DM14008*/ 02219000
*/*                           ADDED HERE                     @DM14008*/ 02220000
*/*          RC        (R8) - X'0B' IF VSETLCK FAILED IN     @DM14008*/ 02221000
*/*                           CSETSTLU                       @DM14008*/ 02222000
*/*          C06R14   (R14) - NSI ADDR                       @DM14008*/ 02223000
*/*                                                          @DM14008*/ 02224000
*/* ROUTINES CALLED:  CSETSTLU - EXAMINE FMCB FOR BIND,      @DM14008*/ 02225000
*/*                              UNBIND, OR CLEAR IN PROGRESS@DM14008*/ 02226000
*/*                                                          @DM14008*/ 02227000
*/* MACROS USED:  VSETLCK - TO RELEASE LOCAL MEMORY LOCK     @DM14008*/ 02228000
*/*                         BEFORE ISSUING CIDCTL            @DM14008*/ 02229000
*/*               CIDCTL  - TO OBTAIN PTR TO PU RDTE         @DM14008*/ 02230000
*/********************************************************************/ 02231000
*                                                                  0870 02232000
*CLNSTAT3: PROC OPTIONS(NOSAVEAREA,DONTSAVE);              /*@DM14008*/ 02233000
CLNSTAT3 DS    0H                                                  0871 02234000
*RETAPA3=C06R14;                    /* SAVE NSI ADDR         @DM14008*/ 02235000
         L     @10,RPHWEA(,PTRRPH)                                 0871 02236000
         ST    C06R14,RETAPA3+552(,@10)                            0871 02237000
*RFY C06R14 UNRSTD;                                        /*@DM14008*/ 02238000
*LINETN='        ';                 /* INIT WITH BLANKS      @DM14008*/ 02239000
         MVC   LINETN(8,LINEPTR),@CC04841                          0873 02240000
*LINESW=ON;                         /* LINE IS SWITCHED      @DM14008*/ 02241000
         OI    LINESW(LINEPTR),B'00001000'                         0874 02242000
*IF RLNRATRN=ON                     /* IF SDLC               @DM14008*/ 02243000
*  THEN DO;                                                /*@DM14008*/ 02244000
         TM    RLNRATRN(RDTPTR),B'00000010'                        0875 02245000
         BNO   @RF00875                                            0875 02246000
*    IF RLNINUSE=OFF                /* SEE IF ITS IN USE     @DM14008*/ 02247000
*    THEN;                          /* NOTHING TO GET IF NOT @DM14008*/ 02248000
         TM    RLNINUSE(RDTPTR),B'01000000'                        0877 02249000
         BZ    @RT00877                                            0877 02250000
*    ELSE                           /* OTHERWISE LOOK AT     @DM14008*/ 02251000
*    DO;                            /* SWITCHED SUBAREA      @DM14008*/ 02252000
*/* LOCKING REQUIREMENT:                                     @DM14008*/ 02253000
*      C06R13=C06R13+TWELVE;        /* BUMP SAVEAREA PTR     @DM14008*/ 02254000
         AH    C06R13,@CH00032                                     0880 02255000
*      C06REL6:                                                    0881 02256000
*/*VSETLCK  (C06R15,RELEASE,LOCAL,(LOCAL,ISTSQC06(C06OBT)))*/      0881 02257000
* DO;                                                              0881 02258000
C06REL6  DS    0H                                                  0882 02259000
*                                        /*DECLARE AND RESTRICT REGS */ 02260000
*                                        /*   2,10,11,12,13,14, AND 15  02261000
**/                                                                0882 02262000
* RFY (GPR02P,GPR10P,GPR11P,GPR12P,GPR13P,GPR14P,GPR15P) RSTD;     0882 02263000
*                                        /*SAVE REGS 2 AND 10 THRU 14*/ 02264000
*        GEN REFS(GPR02F,GPR10F,GPR11F,GPR12F,GPR14F,GPR13P);      0883 02265000
*                                                                  0883 02266000
         STM   GPR10F,GPR12F,0(GPR13P)                                  02267000
         ST    GPR14F,12(GPR13P)                                        02268000
         ST    GPR02F,16(GPR13P)                                        02269000
*                                        /*INVOKE MODESET TO CHANGE  */ 02270000
*                                        /*PSW PROTECTION KEY TO 0   */ 02271000
*        GEN SETS(2);                                              0884 02272000
*                                                                  0884 02273000
         MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2                      02274000
*   GPR10P=GPR13P;                       /*PUT SAVE AREA ADDR IN R 10*/ 02275000
         LR    GPR10P,GPR13P                                       0885 02276000
*                                        /*INVOKE OS/VS SETLOCK MACRO*/ 02277000
*   GEN REFS(GPR13P,PSALITA,FLC) SETS(GPR12F);                     0886 02278000
         SETLOCK RELEASE,TYPE=LOCAL,                                   *02279000
               RELATED=(LOCAL,ISTSQC06(C06OBT))                         02280000
*   GPR15P=GPR13P;                       /*PUT RETURN CODE IN R 15 */   02281000
         LR    GPR15P,GPR13P                                       0887 02282000
*                                        /*INVOKE MODESET MACRO TO   */ 02283000
*     DO;                                /*RESTORE PSW PROTECTION KEY*/ 02284000
*        GEN SETS(2);                                              0889 02285000
         MODESET KEYADDR=(2),WORKREG=2                                  02286000
*     END;                               /* RESTORE REGISTERS        */ 02287000
*   GEN REFS(GPR10F,GPR13P) SETS(GPR10F,GPR11F,                    0891 02288000
*   GPR12F,GPR13F,GPR14F,GPR02F);                                  0891 02289000
         LR    GPR13F,GPR10F                                            02290000
         LM    GPR10F,GPR12F,0(GPR13P)                                  02291000
         L     GPR14F,12(GPR13P)                                        02292000
         L     GPR02F,16(GPR13P)                                        02293000
*   IF GPR15F=4 THEN GPR15F=0;                                     0892 02294000
