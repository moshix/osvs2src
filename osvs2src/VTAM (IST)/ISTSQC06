         BNE   @RF00681                                            0681 01358000
*     CALL CLCSTAT1;                /* GO TO LOCAL STATUS ROUTINE    */ 01359000
*                                                                  0682 01360000
         BAL   @14,CLCSTAT1                                        0682 01361000
*   /*****************************************************************/ 01362000
*   /* RDTE THAT OF A REMOTE TERMINAL, LOGICAL UNIT, COMPONENT, OR   */ 01363000
*   /*            UTERM?                                     @D31CKTQ*/ 01364000
*   /*****************************************************************/ 01365000
*   ELSE                                                           0683 01366000
*     IF RPRENTRY=RPRENTTR|         /* IF ENTRY IS A TERMINAL OR     */ 01367000
*        RPRENTRY=RPRENTUT|         /* ENTRY IS A UTERM OR   @D31CKTQ*/ 01368000
*                RPRENTRY=RPRENTLU|    /* ENTRY IS A LOGICAL UNIT OR */ 01369000
*                   RPRENTRY=RPRENTCM  /* ENTRY IS A COMPONENT       */ 01370000
*       THEN                                                       0683 01371000
         B     @RC00681                                            0683 01372000
@RF00681 CLI   RPRENTRY(RDTPTR),X'80'                              0683 01373000
         BE    @RT00683                                            0683 01374000
         CLI   RPRENTRY(RDTPTR),X'75'                              0683 01375000
         BE    @RT00683                                            0683 01376000
         CLI   RPRENTRY(RDTPTR),X'81'                              0683 01377000
         BE    @RT00683                                            0683 01378000
         CLI   RPRENTRY(RDTPTR),X'90'                              0683 01379000
         BNE   @RF00683                                            0683 01380000
@RT00683 DS    0H                                                  0684 01381000
*       CALL CTRMSTA1;              /* GO TO TERMINAL ROUTINE        */ 01382000
*                                                                  0684 01383000
         BAL   @14,CTRMSTA1                                        0684 01384000
*     /***************************************************************/ 01385000
*     /* RDTE THAT OF A LINE?                                        */ 01386000
*     /***************************************************************/ 01387000
*     ELSE                                                         0685 01388000
*       IF RPRENTRY=RPRENTLN        /* IF ENTRY IS A LINE            */ 01389000
*         THEN                                                     0685 01390000
         B     @RC00683                                            0685 01391000
@RF00683 CLI   RPRENTRY(RDTPTR),X'50'                              0685 01392000
         BNE   @RF00685                                            0685 01393000
*         CALL CLNSTAT1;            /* GO TO LINE ROUTINE            */ 01394000
*                                                                  0686 01395000
         BAL   @14,CLNSTAT1                                        0686 01396000
*       /*************************************************************/ 01397000
*       /* RDTE THAT OF A COMMUNICATION CONTROLLER?                  */ 01398000
*       /*************************************************************/ 01399000
*       ELSE                                                       0687 01400000
*         IF RPRENTRY=RPRENTRN      /* IF ENTRY IS THE 370X          */ 01401000
*           THEN                                                   0687 01402000
         B     @RC00685                                            0687 01403000
@RF00685 CLI   RPRENTRY(RDTPTR),X'01'                              0687 01404000
         BNE   @RF00687                                            0687 01405000
*           CALL CRNSTAT1;          /* GO TO COMMUN. CONT. ROUTINE   */ 01406000
*                                                                  0688 01407000
         BAL   @14,CRNSTAT1                                        0688 01408000
*         /***********************************************************/ 01409000
*         /* RDTE THAT OF AN APPLICATION?                            */ 01410000
*         /***********************************************************/ 01411000
*         ELSE                                                     0689 01412000
*           IF RPRENTRY=RPRENTAP    /* IF ENTRY IS AN APPLICATION    */ 01413000
*             THEN                                                 0689 01414000
         B     @RC00687                                            0689 01415000
@RF00687 CLI   RPRENTRY(RDTPTR),X'55'                              0689 01416000
         BNE   @RF00689                                            0689 01417000
*             CALL CAPSTAT1;        /* GO TO APPLICATION ROUTINE     */ 01418000
*                                                                  0690 01419000
         BAL   @14,CAPSTAT1                                        0690 01420000
*         /***********************************************************/ 01421000
*         /* RDTE THAT OF A CLUSTER CONTROLLER OR COMPONENT CLUSTER? */ 01422000
*         /***********************************************************/ 01423000
*          ELSE                                                    0691 01424000
*            IF RPRENTRY=RPRENTCL|   /* IF ENTRY IS A CLUSTER OR     */ 01425000
