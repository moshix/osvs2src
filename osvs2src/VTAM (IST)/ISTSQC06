         L     @14,RPLAREA(,@14)                                   0719 01586000
         OI    TERMCUIP(@14),B'00000001'                           0719 01587000
*                       ELSE;                              /*@X50BHTQ*/ 01588000
@RF00718 DS    0H                                                  0721 01589000
*                 END;              /* END RC=ZERO GROUP     @X50BHTQ*/ 01590000
*               ELSE;                                      /*@X50BHTQ*/ 01591000
@RF00714 DS    0H                                                  0723 01592000
*             END;                  /* END ACTIVE LU GROUP   @X50BHTQ*/ 01593000
*           ELSE;                                          /*@X50BHTQ*/ 01594000
@RF00710 DS    0H                                                  0725 01595000
*         IF RC=ZERO                /* CONTINUE IF NO        @X50BHTQ*/ 01596000
*           THEN                    /* VSETLCK FAILURE       @X50BHTQ*/ 01597000
@RC00710 LTR   RC,RC                                               0725 01598000
         BNZ   @RF00725                                            0725 01599000
*           DO;                                            /*@X50BHTQ*/ 01600000
*           /* WANT TO MAKE EXECUTION OF THIS CODE           @XA10138*/ 01601000
*           /* INDEPENDENT OF ACTIVE STATUS.                 @XA10138*/ 01602000
*             IF RPRENTRY=RPRENTLU& /* IF LOGICAL UNIT       @XA10138*/ 01603000
