*               RFY (GPR02P,GPR10P,GPR11P,GPR12P,GPR13P,GPR14P,GPR15P)  01223000
*NRSTD;         END;  /*C06REL2:VSETLCK */                         0655 01224000
*                C06R13=C06R13-TWELVE;  /* UNBUMP PTR        @MX08755*/ 01225000
         SH    C06R13,@CH00032                                     0657 01226000
*                END;                                      /*@XM08755*/ 01227000
*               ELSE                /* LOCAL MEMORY LOCK WAS NOT SET */ 01228000
*                 ;                 /* NO NEED TO RELEASE IT         */ 01229000
@RF00638 DS    0H                                                  0660 01230000
*         END;                      /* END USER AREA SPECIFIED       */ 01231000
*                                                                  0660 01232000
*       /*************************************************************/ 01233000
*       /* USER AREA NOT PROVIDED FOR RDTE AND RELATED DATA          */ 01234000
*       /*************************************************************/ 01235000
*                                                                  0661 01236000
*       ELSE                                                       0661 01237000
*         DO;                       /* USER AREA NOT SPECIFIED       */ 01238000
         B     @RC00607                                            0661 01239000
@RF00607 DS    0H                                                  0662 01240000
*           C06CALC=ON2B;           /*  REQUIRED AREA CALCULATION ONLY  01241000
