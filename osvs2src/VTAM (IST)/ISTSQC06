*/********************************************************************/ 03167000
*/* ROUTINE NAME: CAPSTAT1 - APPLICATION STATUS                      */ 03168000
*/*                                                                  */ 03169000
*/* FUNCTION: CALLED BY C06DISPA TO OBTAIN INFORMATION FOR APPLICA-  */ 03170000
*/*           TION-TYPE DISPLAY INQUIRIES.  DETERMINES SIZE OF       */ 03171000
*/*           BUFFER REQUIRED TO CONTAIN RDTE AND TESTS TO SEE IF    */ 03172000
*/*           USER-SUPPLIED AREA IS ADEQUATE.  IF IT IS, THE RDTE IS */ 03173000
*/*           MOVED TO THE USER BUFFER AND CAPSTAT2 IS CALLED TO     */ 03174000
*/*           OBTAIN THE ICES AND ACES ASSOCIATED WITH THE           */ 03175000
*/*           APPLICATION.                                           */ 03176000
*/*                                                                  */ 03177000
*/* INPUT:  RDTPTR   (R4) - POINTER TO APPLICATION RDT       @D32CKTQ*/ 03178000
*/*         RPLPTR   (R7) - POINTER TO USER AREA CONTAINING BUFFER   */ 03179000
*/*         RC       (R8) - RETURN CODE                              */ 03180000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (C06DISPA)   */ 03181000
*/*                                                                  */ 03182000
*/* OUTPUT: RC       (R8) - RETURN CODE (TO X'2B' IF USER SUPPLIED   */ 03183000
*/*                         BUFFER AREA IS INADEQUATE, X'0B' @X50BHTQ*/ 03184000
*/*                         IF CSETSTLU VSETLCK FAILS        @X50BHTQ*/ 03185000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (C06DISPA)   */ 03186000
*/*         NEEDSIZE      - SIZE OF BUFFER AREA REQUIRED     @D32CKTQ*/ 03187000
*/*         IF RC=0, RDTE MOVED TO USER-SUPPLIED BUFFER              */ 03188000
*/*                                                                  */ 03189000
*/* ROUTINES CALLED: CAPSTAT2 - TO GET ACTIVE CONNECTION ELEMENTS    */ 03190000
*/*                             (ACES) AND INACTIVE CONNECTION       */ 03191000
*/*                             ELEMENTS (ICES)                      */ 03192000
*/*                                                                  */ 03193000
*/* MACROS USED: NONE                                                */ 03194000
*/********************************************************************/ 03195000
*                                                                  1174 03196000
*CAPSTAT1:                                                         1174 03197000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             1174 03198000
CAPSTAT1 DS    0H                                                  1175 03199000
*   RFY                                                            1175 03200000
*    (APPLPTR,                                                     1175 03201000
*     AREALEFT)RSTD;                                               1175 03202000
*   RETAPA1=C06R14;                 /* SAVE RETURN ADDRESS           */ 03203000
         L     @10,RPHWEA(,PTRRPH)                                 1176 03204000
         ST    C06R14,RETAPA1+552(,@10)                            1176 03205000
*   RFY                                                            1177 03206000
*     C06R14 UNRSTD;                                               1177 03207000
*   IF C06CALC=ON2B                 /* CALCULATION-ONLY REQUIRED     */ 03208000
*     THEN                          /* BYPASS MOVEMENT OF RDTE AND   */ 03209000
         CH    C06CALC,@CH04855                                    1178 03210000
         BE    @RT01178                                            1178 03211000
*      ;                            /*    RELATED DATA TO USER AREA  */ 03212000
*   ELSE                                                           1180 03213000
*     DO;                           /* NO                            */ 03214000
*       APPLPTR=RPLAREA;            /* ESTABLISH BASE FOR USER AREA  */ 03215000
         L     @14,RPHWEA(,PTRRPH)                                 1181 03216000
         L     @14,OCWRPL(,@14)                                    1181 03217000
         L     APPLPTR,RPLAREA(,@14)                               1181 03218000
*       AREALEFT=RPLBUFL;           /* START WITH GIVEN SIZE AND     */ 03219000
         L     AREALEFT,RPLBUFL(,@14)                              1182 03220000
*       AREALEFT=AREALEFT-LENGTH(ISTRAP);/* SUBTRACT LENGTH OF RDTE  */ 03221000
         SH    AREALEFT,@CH06016                                   1183 03222000
*       IF AREALEFT>=ZERO THEN      /* IF SIZE OF USER AREA ADEQUATE */ 03223000
         LTR   AREALEFT,AREALEFT                                   1184 03224000
         BM    @RF01184                                            1184 03225000
*         APPLRDT=ISTRAP;           /* MOVE APPLICATION RDTE TO AREA */ 03226000
         MVC   APPLRDT(120,APPLPTR),ISTRAP(RDTPTR)                 1185 03227000
*       ELSE                        /* SET CALCULATION-ONLY SWITCH   */ 03228000
*           RC=TWENTYB;             /*   ON AND RETURN CODE = X'2B'  */ 03229000
         B     @RC01184                                            1186 03230000
@RF01184 LA    RC,43                                               1186 03231000
*     END;                                                         1187 03232000
@RC01184 DS    0H                                                  1188 03233000
*   NEEDSIZE=LENGTH(ISTRAP);        /* USED AREA IS RDTE LENGTH      */ 03234000
@RT01178 L     @14,RPHWEA(,PTRRPH)                                 1188 03235000
         MVC   NEEDSIZE+552(4,@14),@CF06016                        1188 03236000
*   CALL CAPSTAT2;                  /* CALL SUBROUTINE TO FINISH     */ 03237000
         BAL   @14,CAPSTAT2                                        1189 03238000
*   RFY                                                            1190 03239000
*     C06R14 RSTD;                                                 1190 03240000
*   C06R14=RETAPA1;                 /* RESTORE RETURN ADDRESS        */ 03241000
         L     @10,RPHWEA(,PTRRPH)                                 1191 03242000
         L     C06R14,RETAPA1+552(,@10)                            1191 03243000
*   END CAPSTAT1;                                                  1192 03244000
@EL00010 DS    0H                                                  1192 03245000
@EF00010 DS    0H                                                  1192 03246000
@ER00010 BR    @14                                                 1192 03247000
*                                                                  1193 03248000
*/********************************************************************/ 03249000
*/* ROUTINE NAME: CAPSTAT2 - GET ACES AND ICES                       */ 03250000
*/*                                                                  */ 03251000
*/* FUNCTION: CALLED BY CAPSTAT1 TO FIND TERMINALS THAT HAVE ASKED   */ 03252000
*/*           FOR THIS APPLICATION OR THAT HAVE BEEN REQUESTED OR    */ 03253000
*/*           ACQUIRED BY IT. AS EACH ACE OR ICE IS FOUND, CAPSTAT3  */ 03254000
*/*           IS CALLED TO MOVE THE TERMINAL NAME TO THE USER AREA.  */ 03255000
*/*                                                                  */ 03256000
*/* INPUT:  RDTPTR   (R4) - POINTER TO THE APPLICATION RDT           */ 03257000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (CAPSTAT1)   */ 03258000
*/*                                                                  */ 03259000
*/* OUTPUT: RDTPTR   (R4) - POINTER TO APPLICATION RDT               */ 03260000
*/*         C06R14  (R14) - NSI ADDR IN CALLING ROUTINE (CLNSTAT1)   */ 03261000
*/*                                                                  */ 03262000
*/* ROUTINES CALLED: CAPSTAT3 - TO GET TERMINAL NAME ASSOCIATED      */ 03263000
*/*                             EACH ICE AND ACE                     */ 03264000
*/*                                                                  */ 03265000
*/* MACROS CALLED: NONE                                              */ 03266000
*/********************************************************************/ 03267000
*                                                                  1193 03268000
*CAPSTAT2:                                                         1193 03269000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             1193 03270000
CAPSTAT2 DS    0H                                                  1194 03271000
*   RFY                                                            1194 03272000
*    (ICEPTR,                                                      1194 03273000
*     C06ICEF)RSTD;                                                1194 03274000
*   RETAPA2=C06R14;                 /* SAVE RETURN ADDRESS           */ 03275000
         L     @06,RPHWEA(,PTRRPH)                                 1195 03276000
         ST    C06R14,RETAPA2+552(,@06)                            1195 03277000
*   RFY                                                            1196 03278000
*     C06R14 UNRSTD;                                               1196 03279000
*                                                                  1196 03280000
*   /*****************************************************************/ 03281000
*   /*                                                               */ 03282000
*   /* SEARCH IS FOR ICES, I.E., TERMINALS THAT HAVE REQUESTED THIS  */ 03283000
*   /* APPLICATION                                                   */ 03284000
*   /*                                                               */ 03285000
*   /*****************************************************************/ 03286000
*   C06ICEF=ONREG;                  /* INDICATE SEARCH IS FOR ICES   */ 03287000
         LA    C06ICEF,1                                           1197 03288000
*   ICEPTR=RAPICELG;                /* ESTABLISH ICE BASE FROM RDTE  */ 03289000
         L     ICEPTR,RAPICELG(,RDTPTR)                            1198 03290000
*   DO WHILE ICEPTR^=               /* SEARCH WHILE AN ICE EXISTS -  */ 03291000
*         ADDR(RAPICELG)&           /* NOT EQ TO ADDR OF FIRST ICE   */ 03292000
*         (RC=ZERO|RC=TWENTYB);     /* MAKE SURE VSETLCK OK  @X50BHTQ*/ 03293000
         B     @DE01199                                            1199 03294000
@DL01199 DS    0H                                                  1200 03295000
*     RDTPTR=ICEOBJ;                /* GET TERM RDTE ADDR FROM ICE   */ 03296000
         L     RDTPTR,ICEOBJ(,ICEPTR)                              1200 03297000
*     CALL CAPSTAT3;                /* CALL SUBRTN TO GET TERM  NAME */ 03298000
         BAL   @14,CAPSTAT3                                        1201 03299000
*     RDTPTR=OCWDRDTE;              /* RESTOR APPL RDTE BASE @D32CKTQ*/ 03300000
         L     @14,RPHWEA(,PTRRPH)                                 1202 03301000
         L     RDTPTR,OCWDRDTE(,@14)                               1202 03302000
*     ICEPTR=ICESUBJQ;              /* UPDATE TO NEXT ICE FOR TERM   */ 03303000
         L     ICEPTR,ICESUBJQ(,ICEPTR)                            1203 03304000
*   END;                                                           1204 03305000
@DE01199 LA    @14,RAPICELG(,RDTPTR)                               1204 03306000
         CR    ICEPTR,@14                                          1204 03307000
         BE    @DC01199                                            1204 03308000
         LTR   RC,RC                                               1204 03309000
         BZ    @DL01199                                            1204 03310000
         CH    RC,@CH04855                                         1204 03311000
         BE    @DL01199                                            1204 03312000
@DC01199 DS    0H                                                  1205 03313000
*   C06ICEF=OFFREG;                 /* INDICATE ICE SEARCH COMPLETE  */ 03314000
         SLR   C06ICEF,C06ICEF                                     1205 03315000
*   RFY                                                            1206 03316000
*     ICEPTR UNRSTD;                                               1206 03317000
*                                                                  1206 03318000
*   /*****************************************************************/ 03319000
*   /*                                                               */ 03320000
*   /* SEARCH FOR ACES I.E. TERMINALS THAT HAVE BEEN REQUESTED OR    */ 03321000
*   /* ACQUIRED BY THIS APPLICATION                                  */ 03322000
*   /*                                                               */ 03323000
*   /*****************************************************************/ 03324000
*   RFY                                                            1207 03325000
*     ACEPTR RSTD;                                                 1207 03326000
*   ACEPTR=OCWDRDTE->RAPACEAP;      /* ESTABLISH ACE BASE FROM RDTE  */ 03327000
         L     @14,RPHWEA(,PTRRPH)                                 1208 03328000
         L     @14,OCWDRDTE(,@14)                                  1208 03329000
         L     ACEPTR,RAPACEAP(,@14)                               1208 03330000
*   DO WHILE ACEPTR^=               /* SEARCH WHILE ACES EXIST -     */ 03331000
*         ADDR(OCWDRDTE->RAPACEAP)& /* NOT EQ TO ADDR OF FIRST ACE   */ 03332000
*         (RC=ZERO|RC=TWENTYB);     /* MAKE SURE VSETLCK OK  @X50BHTQ*/ 03333000
         B     @DE01209                                            1209 03334000
@DL01209 DS    0H                                                  1210 03335000
*     RDTPTR=ACEOBJ;                /* GET TERM RDTE ADDR FROM ACE   */ 03336000
         L     RDTPTR,ACEOBJ(,ACEPTR)                              1210 03337000
*     CALL CAPSTAT3;                /* CALL SUBRTN. TO GET TERM NAME */ 03338000
         BAL   @14,CAPSTAT3                                        1211 03339000
*     ACEPTR=ACESUBJQ;              /* GET NEXT ACE IN QUEUE         */ 03340000
         L     ACEPTR,ACESUBJQ(,ACEPTR)                            1212 03341000
*   END;                                                           1213 03342000
@DE01209 L     @14,RPHWEA(,PTRRPH)                                 1213 03343000
         L     @14,OCWDRDTE(,@14)                                  1213 03344000
         LA    @14,RAPACEAP(,@14)                                  1213 03345000
         CR    ACEPTR,@14                                          1213 03346000
         BE    @DC01209                                            1213 03347000
         LTR   RC,RC                                               1213 03348000
         BZ    @DL01209                                            1213 03349000
         CH    RC,@CH04855                                         1213 03350000
         BE    @DL01209                                            1213 03351000
@DC01209 DS    0H                                                  1214 03352000
*   RDTPTR=OCWDRDTE;                /* RESTORE POINTER TO APPL RDTE  */ 03353000
         L     @14,RPHWEA(,PTRRPH)                                 1214 03354000
         L     RDTPTR,OCWDRDTE(,@14)                               1214 03355000
*   RFY                                                            1215 03356000
*     C06R14 RSTD;                                                 1215 03357000
*   C06R14=RETAPA2;                 /* RESTORE RETURN ADDRESS        */ 03358000
         L     @06,RPHWEA(,PTRRPH)                                 1216 03359000
         L     C06R14,RETAPA2+552(,@06)                            1216 03360000
*   END CAPSTAT2;                                                  1217 03361000
@EL00011 DS    0H                                                  1217 03362000
@EF00011 DS    0H                                                  1217 03363000
@ER00011 BR    @14                                                 1217 03364000
*                                                                  1218 03365000
*/********************************************************************/ 03366000
*/* ROUTINE NAME: CAPSTAT3 - MOVE TERMINAL NAME                      */ 03367000
*/*                                                                  */ 03368000
*/* FUNCTION: CALLED BY CAPSTAT2 TO MOVE THE TERMINAL NAME ASSOCI-   */ 03369000
*/*           ATED WITH EACH ICE AND ACE TO THE USER-SPECIFIED AREA. */ 03370000
*/*           THE FLAG ASSOCIATED WITH EACH NAME IS SET OFF (LOGON   */ 03371000
*/*           PENDING) FOR EACH ICE AND ON (ACTIVE CONNECTION) FOR   */ 03372000
*/*           EACH ACE.  THE NEEDED SIZE IS INCREMENTED BY THE       */ 03373000
*/*           LENGTH OF THE NAME AND FLAG FIELD WITH EACH CALL.      */ 03374000
*/*           ADDITIONALLY, FOR EACH TERMINAL WHICH IS AN    @X50BHTQ*/ 03375000
*/*           ACTIVE LOGICAL UNIT,                           @X50BHTQ*/ 03376000
*/*           AN INDICATION IS PROVIDED IF BIND, UNBIND,     @X50BHTQ*/ 03377000
*/*           OR CLEAR IS IN PROGRESS.                       @X50BHTQ*/ 03378000
*/*                                                                  */ 03379000
*/* INPUT:  APPLPTR  (R1) - POINTER TO THE USER SPECIFIED BUFFER     */ 03380000
*/*         C06ICEF  (R2) - ICE FLAG (1=ICE, 0=ACE)                  */ 03381000
*/*         RDTPTR   (R4) - POINTER TO TERMINAL RDTE                 */ 03382000
*/*         RC       (R8) - RETURN CODE REGISTER                     */ 03383000
*/*         NEEDSIZE      - SIZE OF BUFFER FILLED TO THIS POINT      */ 03384000
*/*         AREALEFT (R11)- AREA REMAINING IN USER BUFFER            */ 03385000
*/*         C06R14   (R14)- NSI ADDR IN CALLING ROUTINE (CLNSTAT1)   */ 03386000
*/*         ICEPTR   (R10)- POINTER TO THE INACTIVE CONNECTION       */ 03387000
*/*                         ELEMENT (ICE)                            */ 03388000
*/*         ACEPTR   (R10)- POINTER TO THE ACTIVE CONNECTION         */ 03389000
*/*                         ELEMENT (ACE)                            */ 03390000
*/*                                                                  */ 03391000
*/* OUTPUT: TERMINAL NAME MOVED TO USER BUFFER, ACTIVE BIT SET       */ 03392000
*/*         ON (FOR ACE) OR OFF (FOR ICE)                            */ 03393000
*/*         ADDITIONALLY, FOR EACH TERMINAL WHICH IS AN LU   @X50BHTQ*/ 03394000
*/*         ACTIVELY CONNECTED, AN INDICATION IS PROVIDED    @X50BHTQ*/ 03395000
*/*         IF BIND, UNBIND, OR CLEAR IS IN PROGRESS.        @X50BHTQ*/ 03396000
*/*         APPLPTR  (R1) - POINTER TO THE USER SPECIFIED BUFFER     */ 03397000
*/*                         INCREMENTED BY LENGTH OF ADDED DATA      */ 03398000
*/*         RDTPTR   (R4) - POINTER TO TERMINAL RDT                  */ 03399000
*/*         RC       (R8) - RETURN CODE (TO X'2B' IF USER SUPPLIED   */ 03400000
*/*                         BUFFER AREA IS INADEQUATE)               */ 03401000
*/*         NEEDSIZE      - SIZE OF APPLICATION RDT+DATA ADDED HERE  */ 03402000
*/*         AREALEFT (R11)- SIZE OF USER AREA REMAINING UNFILLED     */ 03403000
*/*         C06R14   (R14)- NSI ADDR IN CALLING ROUTINE (CLNSTAT1)   */ 03404000
*/*                                                                  */ 03405000
*/* ROUTINES CALLED: NONE                                            */ 03406000
*/*                                                                  */ 03407000
*/* MACROS USED: NONE                                                */ 03408000
*/********************************************************************/ 03409000
*                                                                  1218 03410000
*CAPSTAT3:                                                         1218 03411000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             1218 03412000
CAPSTAT3 DS    0H                                                  1219 03413000
*   RETAPA3=C06R14;                 /* SAVE RETURN ADDRESS   @X50BHTQ*/ 03414000
         L     @06,RPHWEA(,PTRRPH)                                 1219 03415000
         ST    C06R14,RETAPA3+552(,@06)                            1219 03416000
*   RFY C06R14 UNRSTD;                                     /*@X50BHTQ*/ 03417000
*   IF C06CALC=ON2B                 /* CALCULATION-ONLY REQUIRED     */ 03418000
*     THEN                          /* BYPASS MOVEMENT OF RDTE AND   */ 03419000
         CH    C06CALC,@CH04855                                    1221 03420000
         BE    @RT01221                                            1221 03421000
*      ;                            /*    RELATED DATA TO USER AREA  */ 03422000
*   ELSE                                                           1223 03423000
*     DO;                                                          1223 03424000
*       AREALEFT=AREALEFT-          /* SUBTRACT NAME & FLAG LENGTH   */ 03425000
*           (LENGTH(APPLTN)         /*     FROM AREA REMAINING IN    */ 03426000
*            +LENGTH(APPLTCON));    /*     USER-SUPPLIED BUFFFER     */ 03427000
         SH    AREALEFT,@CH00033                                   1224 03428000
*       IF AREALEFT>=ZERO           /* ENOUGH AREA REMAINS           */ 03429000
*         THEN                                                     1225 03430000
         LTR   AREALEFT,AREALEFT                                   1225 03431000
         BM    @RF01225                                            1225 03432000
*         DO;                                                      1226 03433000
*                                                                  1226 03434000
*         /***********************************************************/ 03435000
*         /*    MOVE TERMINAL NAME AND RELATED DATA TO USER AREA     */ 03436000
*         /***********************************************************/ 03437000
*           APPLTN=RPRNAME;         /* MOVE TERMINAL NAME TO AREA    */ 03438000
         MVC   APPLTN(8,APPLPTR),RPRNAME(RDTPTR)                   1227 03439000
*           IF C06ICEF=ONREG        /* IF THIS IS AN ICE             */ 03440000
*             THEN                                                 1228 03441000
         CH    C06ICEF,@CH00063                                    1228 03442000
         BNE   @RF01228                                            1228 03443000
*             APPLTCON=OFF;         /*  SET LOGON PENDING FLAG       */ 03444000
         NI    APPLTCON(APPLPTR),B'01111111'                       1229 03445000
*           ELSE DO;                 /* IT IS AN ACE         @X50BHTQ*/ 03446000
         B     @RC01228                                            1230 03447000
@RF01228 DS    0H                                                  1231 03448000
*             APPLTCON=ON;          /* SET CONNECTED FLAG            */ 03449000
         OI    APPLTCON(APPLPTR),B'10000000'                       1231 03450000
*             IF RPRONLIN=ON        /* FOR ACTIVE            @X50BHTQ*/ 03451000
*               &RPRENTRY=RPRENTLU  /* LOGICAL UNITS, CHECK  @X50BHTQ*/ 03452000
*             THEN                  /* FMCB FOR CLEAR,       @X50BHTQ*/ 03453000
         TM    RPRONLIN(RDTPTR),B'00001000'                        1232 03454000
         BNO   @RF01232                                            1232 03455000
         CLI   RPRENTRY(RDTPTR),X'81'                              1232 03456000
         BNE   @RF01232                                            1232 03457000
*             DO;                   /* UNBIND, BIND I.P.     @X50BHTQ*/ 03458000
*               CALL CSETSTLU;      /* CALL SUBR TO DO IT    @X50BHTQ*/ 03459000
         BAL   @14,CSETSTLU                                        1234 03460000
*               IF RC=ZERO          /* NO VSETLCK FAILURE    @X50BHTQ*/ 03461000
*                 THEN DO;                                 /*@X50BHTQ*/ 03462000
         LTR   RC,RC                                               1235 03463000
         BNZ   @RF01235                                            1235 03464000
*                   IF C06R15=FOUR  /* BIND IN PROGRESS      @X50BHTQ*/ 03465000
*                     THEN APPBIP=ON;      /* TELL ISTCFCDP  @X50BHTQ*/ 03466000
         CH    C06R15,@CH00055                                     1237 03467000
         BNE   @RF01237                                            1237 03468000
         OI    APPBIP(APPLPTR),B'00000010'                         1238 03469000
*                     ELSE;                                /*@X50BHTQ*/ 03470000
@RF01237 DS    0H                                                  1240 03471000
*                   IF C06R15=8     /* CLEAR OR UNBIND I.P.  @X50BHTQ*/ 03472000
*                     THEN APPCUIP=ON;     /* TELL ISTCFCDP  @X50BHTQ*/ 03473000
         CH    C06R15,@CH00061                                     1240 03474000
         BNE   @RF01240                                            1240 03475000
         OI    APPCUIP(APPLPTR),B'00000001'                        1241 03476000
*                     ELSE;                                /*@X50BHTQ*/ 03477000
@RF01240 DS    0H                                                  1243 03478000
*                   END;            /* END RC=0 LEG          @X50BHTQ*/ 03479000
*                 ELSE;                                    /*@X50BHTQ*/ 03480000
@RF01235 DS    0H                                                  1245 03481000
*               END;                /* END ACTIVE LU LEG     @X50BHTQ*/ 03482000
*             ELSE;                                        /*@X50BHTQ*/ 03483000
@RF01232 DS    0H                                                  1247 03484000
*             END;                  /* END ACT CONNECT LEG   @X50BHTQ*/ 03485000
*           APPLPTR=APPLPTR+        /* UPDATE USER AREA POINTER      */ 03486000
*               LENGTH(APPLTN)+LENGTH(APPLTCON);                   1248 03487000
@RC01228 AH    APPLPTR,@CH00033                                    1248 03488000
*         END;                                                     1249 03489000
*       ELSE                        /* AREA IS LESS THAN ZERO        */ 03490000
*           RC=TWENTYB;            /* SET CALCULATION-ONLY SWITCH  1250 03491000
*                                      ON AND RETURN CODE = X'2B'    */ 03492000
         B     @RC01225                                            1250 03493000
@RF01225 LA    RC,43                                               1250 03494000
*     END;                                                         1251 03495000
@RC01225 DS    0H                                                  1252 03496000
*   NEEDSIZE=NEEDSIZE+LENGTH(APPLTN)/* CALCULATE USED SIZE           */ 03497000
*       +LENGTH(APPLTCON);                                         1252 03498000
@RT01221 LA    @14,552                                             1252 03499000
         AL    @14,RPHWEA(,PTRRPH)                                 1252 03500000
         LA    @06,9                                               1252 03501000
