*RFY CSIZE UNRSTD;                                                 0562 01568000
*GEN(EJECT);                                                       0563 01569000
         EJECT                                                          01570000
*/********************************************************************/ 01571000
*/* ROUTINE NAME: GTSPRMSB - GET SESSION PARAMETERS                  */ 01572000
*/*                                                                  */ 01573000
*/* FUNCTION:   VERIFIES THAT APPLICATION-SUPPLIED AREA IS LARGE     */ 01574000
*/*             ENOUGH TO HOLD SESSION PARAMETERS.  SETS UP CALL TO  */ 01575000
*/*             SYSTEM SUBROUTINE ISTINCSH, WHICH FINDS THE SESSION  */ 01576000
*/*             PARAMETERS FOR THE GIVEN NODE AND GIVEN MODE.        */ 01577000
*/*                                                                  */ 01578000
*/* INPUT:   MODEREG  (R5) - POINTER TO LOGON MODE NAME              */ 01579000
*/*          CREGD   (R13) - POINTER TO SAVE AREA                    */ 01580000
*/*          CREGE   (R14) - POINTER TO NSI IN CALLING SUBROUTINE    */ 01581000
*/*                          (MAINLINE OR CLGNDTAB)                  */ 01582000
*/*                                                                  */ 01583000
*/* OUTPUT:  CSIZE  (R0) - AREA LENGTH REQUIRED/USED                 */ 01584000
*/*          CREG9  (R9) - RETURN CODE:                              */ 01585000
*/*                        0 - SESSION PARAMETERS SUCCESSFULLY PLACED*/ 01586000
*/*                            IN LOCATION SPECIFIED BY RPLAREA      */ 01587000
*/*                    X'0B' - VSETLCK FAILURE                       */ 01588000
*/*                    X'2B' - INSUFFICIENT USER STORAGE             */ 01589000
*/*                    X'2E' - ISTINCSH FAILURE                      */ 01590000
*/*                                                                  */ 01591000
*/* ROUTINES CALLED: ISTINCSH - TO OBTAIN SESSION PARAMETERS         */ 01592000
*/*                             ASSOCIATED WITH SPECIFIED RDTE AND   */ 01593000
*/*                             LOGMODE.                             */ 01594000
*/*                                                                  */ 01595000
*/* MACROS USED: VSETLCK - TO SET LOCAL MEMORY LOCK                  */ 01596000
*/*                                                                  */ 01597000
*/********************************************************************/ 01598000
*                                                                  0564 01599000
*GTSPRMSB: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0564 01600000
         B     @PB00003                                            0564 01601000
GTSPRMSB DS    0H                                                  0565 01602000
*OCWR14SV(4)=CREGE;                /* SAVE RETURN POINT              */ 01603000
         L     @12,RPHWEA(,PTRRPH)                                 0565 01604000
         ST    CREGE,OCWR14SV+12(,@12)                             0565 01605000
*                                  /* (NOTE THAT GTSPRMSB AND        */ 01606000
*                                  /* C0BMVDTA USE THE SAME OCW SLOT;*/ 01607000
*                                  /* THIS IS ACCEPTABLE BECAUSE     */ 01608000
*                                  /* THEY DONT CONTEND SIMULTANEOUS-*/ 01609000
*                                  /* LY FOR IT.)                    */ 01610000
*IF RPLBUFL>=BINDL                 /* SEE IF USER AREA BIG ENOUGH    */ 01611000
*   THEN                           /* IF SO, PROVIDE THA DATA        */ 01612000
         L     @08,OCWRPL(,@12)                                    0566 01613000
         L     @06,RPLBUFL(,@08)                                   0566 01614000
         C     @06,BINDL                                           0566 01615000
         BL    @RF00566                                            0566 01616000
*   DO;                            /* SET UP ISTFNDSP CALL           */ 01617000
*      FNDLU=OCWDRDTE;             /* RDTE POINTER                   */ 01618000
         LA    @06,176                                             0568 01619000
