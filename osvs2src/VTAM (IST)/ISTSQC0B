*  BFHDRPTR = BFHNXCHN;                /* GET NEXT BUFFER ADDRESS */    01839000
         L     BFHDRPTR,BFHNXCHN(,BFHDRPTR)                        0641 01840000
*  DO WHILE BFHDRPTR ^= 0; /* DO UNTIL DATA IN LAST BUFFER IS MOVED */  01841000
         B     @DE00642                                            0642 01842000
