*            CREG9=SIZECODE;       /* RPLBUFL NOT BIG ENOUGH         */ 01524000
         B     @RC00495                                            0546 01525000
@RF00495 LA    CREG9,43                                            0546 01526000
*      RFY CSIZE RSTD;                                             0547 01527000
@RC00495 DS    0H                                                  0548 01528000
*      CSIZE=DATASIZE;             /* PUT LENGTH OF LOGN PARMS IN R0 */ 01529000
         LR    CSIZE,DATASIZE                                      0548 01530000
*      RFY DATASIZE UNRSTD;        /* RESTRICT AGAIN IN C0BMVDTA     */ 01531000
*      END;                        /* END OF NON-ZERO ICEDATA PROCESS*/ 01532000
*   ELSE                           /* NEED TO CALL GTSPRMSB          */ 01533000
*   DO;                                                            0551 01534000
         B     @RC00491                                            0551 01535000
