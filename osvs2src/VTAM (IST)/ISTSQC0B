         LH    DATASIZE,BFHSIZE(,BFHDRPTR)                         0637 01827000
         SL    DATASIZE,@CF00053                                   0637 01828000
*  CLGNT=DATASIZE;             /* LENGTH OF USER AREA TO BE FILLED */   01829000
         LR    CLGNT,DATASIZE                                      0638 01830000
*  MVCL (CAREA,LDATAPTR);     /* MOVE LOGON DATA FROM FIRST BUFFER */   01831000
         MVCL  CAREA,LDATAPTR                                      0639 01832000
*/* SET WORK REG TO LNGTH OF LOGON PARMS REMAINING IN BUFFERS:       */ 01833000
*  TEMPCNT = LGPDTLEN-(BFHSIZE-4);                                 0640 01834000
         LH    TEMPCNT,BFHSIZE(,BFHDRPTR)                          0640 01835000
