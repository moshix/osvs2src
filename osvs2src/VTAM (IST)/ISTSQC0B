         L     @12,OCWDRDTE(,@12)                                  0568 01621000
         ST    @12,FNDLU(,@06)                                     0568 01622000
*      FNDAREA=RPLAREA;            /* SESSPARMS GO TO USER AREA      */ 01623000
         L     @12,RPLAREA(,@08)                                   0569 01624000
         ST    @12,FNDAREA(,@06)                                   0569 01625000
*      FNDMODE=MODEREG;            /* MODE NAME                      */ 01626000
         ST    MODEREG,FNDMODE(,@06)                               0570 01627000
*      RFY MODEREG UNRSTD;         /* UNNECESSARY NOW                */ 01628000
*      CREGD=CREGD+12;             /* ADJUST SAVEAREA POINTER        */ 01629000
         AL    CREGD,@CF01001                                      0572 01630000
*/*VSETLCK  (CRETC,OBTAIN,LOCAL,(LOCAL,ISTSQC0B(C0BREL2)))*/       0573 01631000
*C0BOBT2:  DO;                                                     0573 01632000
