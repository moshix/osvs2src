*    LCPMNTS  BIT(8) CONSTANT('11'X),  /* TERMINATE SELF             */ 00281000
*    LCPMNAP  BIT(8) CONSTANT('0A'X),  /* APPL. ISSUED CLEAR         */ 00282000
*    LCPMNPR  BIT(8) CONSTANT('0B'X),  /* PREEMPT                    */ 00283000
*    LCPMNRE  BIT(8) CONSTANT('0C'X);  /* RESTORE                    */ 00284000
*/* VS1, VS2 TPIOS SAVEAREA FORMAT */                              0308 00285000
*DCL  1  ISTZSAVE BDY(WORD) BASED,                                 0308 00286000
*        2   ZSAZSVBC PTR(31),         /* SAVEAREA BACK CHAIN        */ 00287000
*            3   ZSAZSVMS CHAR(1),     /* MODULE TRACE CODE          */ 00288000
*                4 ZSAZACTV BIT(1),    /* ACTIVE SAVE AREA FLAG      */ 00289000
*        2   ZSAZSVFC PTR(31),         /* SAVEAREA FORWARD CHAIN     */ 00290000
*        2   ZSAZSVRR PTR(31),         /* RETURN REGISTER            */ 00291000
*        2   ZSAZSVVR (6) PTR(31),    /* VARIABLE REGS (7-12) SVEAREA*/ 00292000
*        2   ZSAZTRAC CHAR(4);        /* EBCDIC NAME OF OWNER        */ 00293000
*/*                                                                0309 00294000
*THE FOLLOWING EQUATES SHOULD READ ''FIXED(8) CONSTANT(LENGTH(X))'',    00295000
*WHERE X IS THE NAME OF THE VARIABLE CONTROL BLOCK OR FIELD OF WHICH    00296000
*LENGTH IS THE VALUE BEING EQUATED, SHOULD THE PLS COMPILER BE     0309 00297000
*BE ABLE TO TAKE IT.                                               0309 00298000
*                                                                    */ 00299000
*DCL ZSAZSVVA FIXED(8) CONSTANT(8);  /* DISPLACEMENT TO VARIABLE REGS*/ 00300000
*DCL ZSAZSVSZ FIXED(8) CONSTANT(40); /* LENGTH OF ISTZSAVE           */ 00301000
*/* PREFIX FOR CRR                                                   */ 00302000
*/*                                                                  */ 00303000
*/* RELEASE 1 - JUNE 28, 1973                                        */ 00304000
*/*                                                                  */ 00305000
*DCL 1 ISTPFCRR BASED BDY(WORD),        /* PREFIX FOR CRR            */ 00306000
