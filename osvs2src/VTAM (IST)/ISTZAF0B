         B     SEG0031#                                            0866 01263000
*SEG0031@:                                                         0867 01264000
*   END;                                                           0867 01265000
SEG0031@ DS    0H                                                  0868 01266000
*   GEN NOSETS NOREFS NODEFS(EJECT);                               0868 01267000
         EJECT                                                          01268000
*                                                                  0869 01269000
*/********************************************************************/ 01270000
*/*                                                                  */ 01271000
*/* SEGMENT-                                                         */ 01272000
*/*      SEG0040                                                     */ 01273000
*/*                                                                  */ 01274000
*/* FUNCTION-                                                        */ 01275000
*/*      THIS ROUTINE FINDS THE NEXT PROCESSABLE ELEMENT ON THE      */ 01276000
*/*      PAB QUEUE. (FMCPAB3)  IF NO WORK ELEMENT IS FOUND,          */ 01277000
*/*      SWITCHES ARE SET FOR MAIN LINE EXIT.  IF AN ELEMENT IS      */ 01278000
*/*      FOUND, IMMEDIATE SCHEDULING IS CHECKED. (UNLESS THIS IS     */ 01279000
*/*      THE FIRST TIME THROUGH THE ROUTINE, THEN IMMEDIATE          */ 01280000
*/*      SCHEDULING WILL HAVE ALREADY BEEN CHECKED) IF IMMEDIATE     */ 01281000
*/*      SCHEDULING IS REQUIRED, THE LCCW/BTU TRANSLATION CONTROLLER */ 01282000
*/*      IS CALLED TO PROCESS THE LCCW.                              */ 01283000
*/*                                                                  */ 01284000
*/********************************************************************/ 01285000
*                                                                  0869 01286000
*   /*****************************************************************/ 01287000
*   /*                                                               */ 01288000
*   /* SEGMENT(SEG0040)START                                         */ 01289000
*   /*                                                               */ 01290000
*   /*****************************************************************/ 01291000
*                                                                  0869 01292000
*   DO;                                                            0869 01293000
*     GOTO SEG0040@;                                               0870 01294000
         B     SEG0040@                                            0870 01295000
*SEG0040:                                                          0871 01296000
*   END;                                                           0871 01297000
*                                                                  0871 01298000
SEG0040  DS    0H                                                  0872 01299000
*/* TEST OF FIRST TIME SWITCH AND LOAD FROM RPHWEA WERE      @XL03HDP*/ 01300000
*/* REMOVED SINCE RPHWEA COULD HAVE BEEN DESTROYED BY TPFEL. @XL03HDP*/ 01301000
*/* TPFEL (,FMCPAB3) RETPTR(PTRLCP)                                  */ 01302000
*                                                                  0872 01303000
*   DO;                             /* BEGIN TPFEL CODE GENERATION   */ 01304000
*     RFY                                                          0873 01305000
*      (GPR00P,                                                    0873 01306000
*       GPR01P,                                                    0873 01307000
*       GPR14P,                                                    0873 01308000
*       GPR15P) RSTD;                                              0873 01309000
*     GPR15P=ADDR(FMCPAB3);         /* ADDRESS QUEUE POINTER         */ 01310000
         LA    GPR15P,FMCPAB3(,PTRFMC)                             0874 01311000
*     GPR14P=GPR15P->PABWEQA|FMZCA064;                             0875 01312000
         L     @11,@CF02365                                        0875 01313000
         L     GPR14P,PABWEQA(,GPR15P)                             0875 01314000
         OR    GPR14P,@11                                          0875 01315000
*     IF GPR14P=FMZCA064 THEN                                      0876 01316000
         CLR   GPR14P,@11                                          0876 01317000
         BE    @RT00876                                            0876 01318000
*       ;                                                          0877 01319000
*     ELSE                                                         0878 01320000
*       DO;                                                        0878 01321000
*         GPR00P=FMZCA064;          /* INITIALIZE QUIT FLAG          */ 01322000
         L     GPR00P,@CF02365                                     0879 01323000
*         DO WHILE(GPR00P^=GPR14P->WKECHAIN);                      0880 01324000
         B     @DE00880                                            0880 01325000
@DL00880 DS    0H                                                  0881 01326000
*           GPR14P=GPR14P->WKECHAIN;/* SEARCH FOR END OF CHAIN       */ 01327000
         L     GPR14P,WKECHAIN(,GPR14P)                            0881 01328000
*         END;                                                     0882 01329000
@DE00880 C     GPR00P,WKECHAIN(,GPR14P)                            0882 01330000
         BNE   @DL00880                                            0882 01331000
*       END;                                                       0883 01332000
*     GPR14P=GPR14P&FMZCB064;                                      0884 01333000
@RT00876 N     GPR14P,@CF02460                                     0884 01334000
*     PTRLCP=GPR14P;                                               0885 01335000
         LR    PTRLCP,GPR14P                                       0885 01336000
*     RFY                                                          0886 01337000
*      (GPR00P,                                                    0886 01338000
*       GPR01P,                                                    0886 01339000
*       GPR14P,                                                    0886 01340000
*       GPR15P) UNRSTD;                                            0886 01341000
*   END;                            /* END TPFEL GENERATION LOCATE 0887 01342000
*                                      ELEMENT FROM FMCPAB3          */ 01343000
*   RPHWEA=PTRLCP;                  /* SET ELEMENT ADDRESS IN RPH    */ 01344000
         ST    PTRLCP,RPHWEA(,PTRRPH)                              0888 01345000
*   IF PTRLCP^=0 THEN               /* IF ELEMENT LOCATED            */ 01346000
         LTR   PTRLCP,PTRLCP                                       0889 01347000
         BZ    @RF00889                                            0889 01348000
*     DO;                           /* PROCESS IT                    */ 01349000
*       IF ZCROB1='1'B THEN                                        0891 01350000
         TM    ZCROB1(ZCRREG),B'00010000'                          0891 01351000
         BO    @RT00891                                            0891 01352000
*         ;                         /* IF FIRST TIME THROUGH         */ 01353000
*       ELSE                        /* OTHERWISE                     */ 01354000
*         CALL AF0B0015;            /* TEST FOR IMMEDIATE SCHED.     */ 01355000
         BAL   @14,AF0B0015                                        0893 01356000
*       IF ZCRENDSH='1'B THEN                                      0894 01357000
@RT00891 TM    ZCRENDSH(ZCRREG),B'10000000'                        0894 01358000
         BO    @RT00894                                            0894 01359000
*         ;                         /* IF EXIT IS REQUIRED           */ 01360000
*       ELSE                        /* OTHERWISE                     */ 01361000
*         IF ZCRSHEDI='1'B THEN                                    0896 01362000
         TM    ZCRSHEDI(ZCRREG),B'01000000'                        0896 01363000
         BO    @RT00896                                            0896 01364000
*           ;                       /* IF IMMEDIATE SCHEDULING       */ 01365000
*         ELSE                      /* NOT IMMEDIATE SCHEDULING      */ 01366000
*           IF FMCZF002='1'B THEN   /* IF I/O TO BE SUSPENDED        */ 01367000
         TM    FMCZF002(PTRFMC),B'01000000'                        0898 01368000
         BNO   @RF00898                                            0898 01369000
*             DO;                   /* SET EXIT SWITCHES             */ 01370000
*               ZCRENDSH='1'B;      /* EXIT SWITCH 1                 */ 01371000
*               ZCRENDPR='1'B;      /* EXIT SWITCH 2                 */ 01372000
         OI    ZCRENDSH(ZCRREG),B'10100000'                        0901 01373000
*             END;                  /* EXIT WILL COMMENCE            */ 01374000
*           ELSE                    /* I/O NOT TO BE SUSPENDED       */ 01375000
*             DO;                   /* INVOKE TRANSLATION CONTROL    */ 01376000
         B     @RC00898                                            0903 01377000
@RF00898 DS    0H                                                  0904 01378000
*               CALL ISTZBF0B;      /* CALL LCCW/BTU TRANS. CTL      */ 01379000
         SLR   @11,@11                                             0904 01380000
         ICM   @11,7,PABDVTA+96(PTRFMC)                            0904 01381000
         L     @15,ZLBZBF0B(,@11)                                  0904 01382000
         BALR  @14,@15                                             0904 01383000
*               ZCROB1='0'B;        /* SET OFF FIRST TIME SWITCH     */ 01384000
         NI    ZCROB1(ZCRREG),B'11101111'                          0905 01385000
*             END;                  /* LCCW TRANSLATED               */ 01386000
*     END;                          /* END OF WORK ELEMENT PROCESS   */ 01387000
*   ELSE                            /* NO WORK ELEMENT LOCATED       */ 01388000
*     DO;                           /* SET UP FOR EXIT               */ 01389000
         B     @RC00889                                            0908 01390000
@RF00889 DS    0H                                                  0909 01391000
*       IF PTRDNC->NCBTYPE=DNTYPE THEN/* IF DNCB ADDRESSED   @XM18385*/ 01392000
         CLI   NCBTYPE(PTRDNC),X'05'                               0909 01393000
         BNE   @RF00909                                            0909 01394000
*         IF DNCSESS='0'B|          /* IF NOT IN SESSION OR          */ 01395000
*             DNCDISC='1'B THEN     /* DISCONNECT PENDING            */ 01396000
*                                                                  0910 01397000
         TM    DNCDISC(PTRDNC),B'00001000'                         0910 01398000
         BNZ   @RT00910                                            0910 01399000
         TM    DNCSESS(PTRDNC),B'01000000'                         0910 01400000
         BO    @RF00910                                            0910 01401000
@RT00910 DS    0H                                                  0911 01402000
*           /*********************************************************/ 01403000
*           /*                                                       */ 01404000
*           /* SEGMENT(SEG0020)REQUEST                               */ 01405000
*           /*                                                       */ 01406000
*           /*********************************************************/ 01407000
*                                                                  0911 01408000
*           DO;                                                    0911 01409000
*             GOTO SEG0020;                                        0912 01410000
         B     SEG0020                                             0912 01411000
*SEG0020#:                                                         0913 01412000
*           END;                    /* PURGE FREE LSCB QUEUE         */ 01413000
*         ELSE                                                     0914 01414000
*           ;                       /* OTHERWISE DON'T               */ 01415000
@RF00910 DS    0H                                                  0915 01416000
*       ELSE                                                       0915 01417000
*         ;                         /* NO DNCB ADDR          @XM18385*/ 01418000
@RF00909 DS    0H                                                  0916 01419000
*       ZCRENDSH='1'B;              /* SET EXIT SWITCH 1             */ 01420000
@RC00909 DS    0H                                                  0917 01421000
*       ZCRENDPR='1'B;              /* SET EXIT SWITCH 2             */ 01422000
         OI    ZCRENDSH(ZCRREG),B'10100000'                        0917 01423000
*       IF FMCZF002='1'B THEN                                      0918 01424000
         TM    FMCZF002(PTRFMC),B'01000000'                        0918 01425000
         BO    @RT00918                                            0918 01426000
*         ;                         /* IF I/O SCHEDULING SUSPENDED   */ 01427000
*       ELSE                        /* OTHERWISE                     */ 01428000
*         ZCREXTYP='1'B;            /* SET SCHEDULE EXIT             */ 01429000
         OI    ZCREXTYP(ZCRREG),B'01000000'                        0920 01430000
*     END;                          /* END EXIT PROCESSING           */ 01431000
*                                                                  0921 01432000
@RT00918 DS    0H                                                  0922 01433000
*   /*****************************************************************/ 01434000
*   /*                                                               */ 01435000
*   /* SEGMENT(SEG0040)FINISH                                        */ 01436000
*   /*                                                               */ 01437000
*   /*****************************************************************/ 01438000
*                                                                  0922 01439000
*   DO;                                                            0922 01440000
@RC00889 DS    0H                                                  0923 01441000
*     GOTO SEG0040#;                                               0923 01442000
         B     SEG0040#                                            0923 01443000
*SEG0040@:                                                         0924 01444000
*   END;                                                           0924 01445000
SEG0040@ DS    0H                                                  0925 01446000
*   GEN NOSETS NODEFS NOREFS(EJECT);                               0925 01447000
         EJECT                                                          01448000
*                                                                  0926 01449000
*/********************************************************************/ 01450000
*/*                                                                  */ 01451000
*/* SUBROUTINE-                                                      */ 01452000
*/*      AF0B0015                                                    */ 01453000
*/*                                                                  */ 01454000
*/* FUNCTION-                                                        */ 01455000
*/*      TESTS FOR IMMEDIATE SCHEDULING. IMMEDIATE SCHEDULING OCCURS */ 01456000
*/*      WHEN A SPECIAL LCCW IS FOUND, SUCH AS RESET, TEST,          */ 01457000
*/*      OR CONTROL.  IF IMMEDIATE SCHEDULING IS                     */ 01458000
*/*      RECOGNISED, THE BTU/LCCW TRANSLATION CONTROLLER IS CALLED   */ 01459000
*/*      TO PROCESS THE LCCW.                                        */ 01460000
*/*                                                                  */ 01461000
*/********************************************************************/ 01462000
*                                                                  0926 01463000
*AF0B0015:                                                         0926 01464000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0926 01465000
         B     @PB00002                                            0926 01466000
AF0B0015 DS    0H                                                  0927 01467000
*   RFY                                                            0927 01468000
*     LNKREG RSTD;                  /* TO SAVE RETURN ADDRESS        */ 01469000
*   ZCRSVF1=LNKREG;                 /* SAVE RETURN ADDRESS           */ 01470000
         ST    LNKREG,ZCRSVF1(,ZCRREG)                             0928 01471000
*   RFY                                                            0929 01472000
*     LNKREG UNRSTD;                /* SAVED                         */ 01473000
*   ZCRSHEDI='0'B;                  /* INITIALIZE IMMED SCHED SW     */ 01474000
         NI    ZCRSHEDI(ZCRREG),B'10111111'                        0930 01475000
*   IF ZCROB1='1'B THEN             /* IF FIRST TIME THROUGH         */ 01476000
         TM    ZCROB1(ZCRREG),B'00010000'                          0931 01477000
         BNO   @RF00931                                            0931 01478000
*     DO;                           /* FIRST TIME THROUGH    @ZA08916*/ 01479000
*/* TPFEL (,FMCPAB3)                                                 */ 01480000
*                                                                  0933 01481000
*       DO;                         /* BEGIN TPFEL CODE GENERATION   */ 01482000
*         RFY                                                      0934 01483000
*          (GPR00P,                                                0934 01484000
*           GPR01P,                                                0934 01485000
*           GPR14P,                                                0934 01486000
*           GPR15P) RSTD;                                          0934 01487000
*         GPR15P=ADDR(FMCPAB3);     /* ADDRESS QUEUE POINTER         */ 01488000
         LA    GPR15P,FMCPAB3(,PTRFMC)                             0935 01489000
*         GPR14P=GPR15P->PABWEQA|FMZCA064;                         0936 01490000
         L     @11,@CF02365                                        0936 01491000
         L     GPR14P,PABWEQA(,GPR15P)                             0936 01492000
         OR    GPR14P,@11                                          0936 01493000
*         IF GPR14P=FMZCA064 THEN                                  0937 01494000
         CLR   GPR14P,@11                                          0937 01495000
         BE    @RT00937                                            0937 01496000
*           ;                                                      0938 01497000
*         ELSE                                                     0939 01498000
*           DO;                                                    0939 01499000
*             GPR00P=FMZCA064;      /* INITIALIZE QUIT FLAG          */ 01500000
         L     GPR00P,@CF02365                                     0940 01501000
*             DO WHILE(GPR00P^=GPR14P->WKECHAIN);                  0941 01502000
         B     @DE00941                                            0941 01503000
@DL00941 DS    0H                                                  0942 01504000
*               GPR14P=GPR14P->WKECHAIN;/* SEARCH FOR END OF CHAIN   */ 01505000
         L     GPR14P,WKECHAIN(,GPR14P)                            0942 01506000
*             END;                                                 0943 01507000
@DE00941 C     GPR00P,WKECHAIN(,GPR14P)                            0943 01508000
         BNE   @DL00941                                            0943 01509000
*           END;                                                   0944 01510000
*         GPR14P=GPR14P&FMZCB064;                                  0945 01511000
@RT00937 N     GPR14P,@CF02460                                     0945 01512000
*         GPR01P->RPHWEA=GPR14P;                                   0946 01513000
         ST    GPR14P,RPHWEA(,GPR01P)                              0946 01514000
*         RFY                                                      0947 01515000
*          (GPR00P,                                                0947 01516000
*           GPR01P,                                                0947 01517000
*           GPR14P,                                                0947 01518000
*           GPR15P) UNRSTD;                                        0947 01519000
*       END;                        /* END TPFEL GENERATION GET LCPB    01520000
*                                      FROM TOP              @ZA08916*/ 01521000
*       PTRLCP=RPHWEA;              /* GET LCPB FROM RPH             */ 01522000
         L     PTRLCP,RPHWEA(,PTRRPH)                              0949 01523000
*     END;                          /* FIRST TIME THROUGH    @ZA08916*/ 01524000
*   ELSE                                                           0951 01525000
*     ;                             /* OTHERWISE ALREADY GOTTEN      */ 01526000
@RF00931 DS    0H                                                  0952 01527000
*   IF PTRLCP^=0 THEN               /* IF WORK ELEMENT EXISTS        */ 01528000
         LTR   PTRLCP,PTRLCP                                       0952 01529000
         BZ    @RF00952                                            0952 01530000
*     DO;                           /* PROCESS WORK ELEMENT          */ 01531000
*       RFY                                                        0954 01532000
*         ISTLCCW BASED(LCPNXLCW);  /* COVER LCCW                    */ 01533000
*       IF LCCWOP=LTIC THEN         /* IF LCCW IS A TIC              */ 01534000
         L     @14,LCPNXLCW(,PTRLCP)                               0955 01535000
         CLI   LCCWOP(@14),X'08'                                   0955 01536000
         BNE   @RF00955                                            0955 01537000
*         DO;                                                      0956 01538000
*           LCPNXLCW=LCCWDATA;      /* ADDRESS NEXT LCCW             */ 01539000
         L     @14,LCCWDATA(,@14)                                  0957 01540000
         ST    @14,LCPNXLCW(,PTRLCP)                               0957 01541000
*           LCPLCCWC=LCPLCCWC+1;    /* BUMP COUNT OF LCCWS@XL03HDP   */ 01542000
         LA    @11,1                                               0958 01543000
         SLR   @00,@00                                             0958 01544000
