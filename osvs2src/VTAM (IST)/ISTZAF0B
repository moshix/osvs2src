@RF00831 LA    WKREG8,FMCPAB4(,PTRFMC)                             0833 01162000
*/*  TPQUE (ADDR(ZIBFSB),WKREG8->PABWEQA) NONE                       */ 01163000
*                                                                  0834 01164000
*           DO;                                                    0834 01165000
@RC00831 DS    0H                                                  0835 01166000
*             RFY                                                  0835 01167000
*              (GPR00P,                                            0835 01168000
*               GPR01P,                                            0835 01169000
*               GPR02P,                                            0835 01170000
*               GPR14P,                                            0835 01171000
*               GPR15P) RSTD;                                      0835 01172000
*             GEN REFS(GPR01P) SETS(RPHWORK)                       0836 01173000
*                 (STM GPR02P,GPR08P,RPHWORK(GPR01P));             0836 01174000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              01175000
*             GPR15P=ADDR(ZIBFSB);  /* GET WORK ELEM POINTER         */ 01176000
         LA    GPR15P,ZIBFSB(,PTRZOB)                              0837 01177000
*             GPR14P=ADDR(WKREG8->PABWEQA);/* ADDRESS PAB/QUEUE    0838 01178000
*                                      POINTER                       */ 01179000
         LR    GPR14P,WKREG8                                       0838 01180000
*             GPR15P=GPR15P|PABWEQG1;                              0839 01181000
         O     GPR15P,@CF02365                                     0839 01182000
*             GPR02P=GPR14P->PABWEQA;/* INITIALIZE WITH QUEUE HEADER */ 01183000
         L     GPR02P,PABWEQA(,GPR14P)                             0840 01184000
*CMZA0075:                                                         0841 01185000
*             ;                     /* CS FAIL TARGET                */ 01186000
CMZA0075 DS    0H                                                  0842 01187000
*             GPR00P=GPR02P;        /* SAVE QUEUE CONTENTS           */ 01188000
         LR    GPR00P,GPR02P                                       0842 01189000
*             GPR15P->PABCHAIN=GPR02P|PABWEQG1;/* FORWARD CHAIN WORK    01190000
*                                      ELEMENT                       */ 01191000
         LR    @11,GPR02P                                          0843 01192000
         O     @11,@CF02365                                        0843 01193000
         ST    @11,PABCHAIN(,GPR15P)                               0843 01194000
*             CS(GPR02P,GPR15P,GPR14P->PABWEQA);/* CHECK FOR END OF     01195000
*                                      QUEUE                         */ 01196000
         CS    GPR02P,@15,PABWEQA(GPR14P)                          0844 01197000
*             BC(7,CMZA0075);                                      0845 01198000
         BC    7,CMZA0075                                          0845 01199000
*             GEN REFS(RPHWORK,GPR01P) NOSETS                      0846 01200000
