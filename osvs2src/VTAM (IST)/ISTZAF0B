         B     SEG0031@                                            0820 01109000
*SEG0031:                                                          0821 01110000
*   END;                                                           0821 01111000
SEG0031  DS    0H                                                  0822 01112000
*   RCDREG=GOIO;                    /* SET I/O RTRN CODE     @XM03HDP*/ 01113000
         SLR   @11,@11                                             0822 01114000
         LR    RCDREG,@11                                          0822 01115000
*   IF PTRLCP^=0&                   /* IS THERE AN LCPB      @X50BHXF*/ 01116000
*       (LCPZRT00='1'B|             /* IF ANY ERROR          @XM03HDP*/ 01117000
*       LCPZRT01='1'B|              /* DECTECTED OR          @XM03HDP*/ 01118000
*       LCPZRT02='1'B|              /* IF I/O SCHEDULING DISABLED    */ 01119000
*       PTRZOB=0) THEN              /* OR NO BUFFER          @XL03HDP*/ 01120000
