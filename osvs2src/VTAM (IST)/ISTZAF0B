*             RFY                                                  0847 01203000
*              (GPR00P,                                            0847 01204000
*               GPR01P,                                            0847 01205000
*               GPR02P,                                            0847 01206000
*               GPR14P,                                            0847 01207000
*               GPR15P) UNRSTD;                                    0847 01208000
*           END;                    /* *** END OF TPQUE GENERATION 0848 01209000
*                                      QUEUE FSB TO PROPER TIP     0848 01210000
