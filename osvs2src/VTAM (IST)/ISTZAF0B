         BNZ   @RT00823                                            0823 01124000
         CR    PTRZOB,@11                                          0823 01125000
         BNE   @RF00823                                            0823 01126000
@RT00823 DS    0H                                                  0824 01127000
*     DO;                           /*                       @XL03HDP*/ 01128000
*       RFY                                                        0825 01129000
*         WKREG8 RSTD;              /* HOLD WORK REGISTER    @XL03HDP*/ 01130000
*       RFY                                                        0826 01131000
*         ISTFSB BASED(ADDR(ZIBFSB));/* @ POSSIBLE FSB@XL03HDP       */ 01132000
*       IF PTRZOB=0 THEN            /* IF NO BUFFER          @XL03HDP*/ 01133000
         LTR   PTRZOB,PTRZOB                                       0827 01134000
         BNZ   @RF00827                                            0827 01135000
*         WKREG8=ADDR(DNCZC001);    /* PREPARE FOR SCHEDULING OF DNCB   01136000
*                                      TIP                   @XL03HDP*/ 01137000
         LA    WKREG8,DNCZC001(,PTRDNC)                            0828 01138000
*       ELSE                        /* THERES A BUFFER       @XL03HDP*/ 01139000
*/* SINCE THERE IS A BUFFER AND NO I/O IS TO BE DONE, THIS MEANS THAT*/ 01140000
*/* A FSB HAS BEEN BUILT WHICH MUST BE QUEUED TO THE INBOUND PAB.    */ 01141000
*                                                                  0829 01142000
*         DO;                       /* SO....                @XL03HDP*/ 01143000
         B     @RC00827                                            0829 01144000
@RF00827 DS    0H                                                  0830 01145000
*           RFY                                                    0830 01146000
*             ISTBTU BASED(ADDR(ZIBBTU));/* @ TO ZIB         @XL03HDP*/ 01147000
*/* IF THE FSB IS FOR AN INTERNAL CONTACT, IT WILL BE QUEUED TO THE  */ 01148000
*/* DNCB TIP. OTHERWISE THE FSB IS QUEUED TO THE FMCB TIP.           */ 01149000
*                                                                  0831 01150000
*           IF BDUCOMND=BCMDC THEN  /* IS FSB FOR CONTACT?@XL03HDP   */ 01151000
