*           IF LCCWOP=LTIC THEN     /* IF NEXT LCCW IS A TIC         */ 01548000
         CLI   LCCWOP(@14),X'08'                                   0959 01549000
         BNE   @RF00959                                            0959 01550000
*             DO;                   /* THIS IS AN INVALID STRING     */ 01551000
*               CALL ISTZBFX0;                                     0961 01552000
         SLR   @11,@11                                             0961 01553000
         ICM   @11,7,PABDVTA+96(PTRFMC)                            0961 01554000
         L     @15,ZLBZBFX0(,@11)                                  0961 01555000
         BALR  @14,@15                                             0961 01556000
*               ZCRENDPR='1'B;      /* CALL INVALID LCCW AND EXIT    */ 01557000
         OI    ZCRENDPR(ZCRREG),B'00100000'                        0962 01558000
*             END;                                                 0963 01559000
*           ELSE                                                   0964 01560000
*             ;                     /* NEXT LCCW NOT TIC             */ 01561000
@RF00959 DS    0H                                                  0965 01562000
*         END;                      /* END 1ST LCCW TIC              */ 01563000
*       ELSE                                                       0966 01564000
*         ;                         /* 1ST LCCW NOT A TIC            */ 01565000
@RF00955 DS    0H                                                  0967 01566000
*       IF ZCRENDPR='1'B THEN                                      0967 01567000
         TM    ZCRENDPR(ZCRREG),B'00100000'                        0967 01568000
         BO    @RT00967                                            0967 01569000
*         ;                         /* EXIT FLG ON?          @XL03HDP*/ 01570000
*       ELSE                        /* NO                    @XL03HDP*/ 01571000
*         IF LCCWOP=LEXEC           /* CMND CODE=CONTROL     @XL03HCQ*/ 01572000
*/*  OR  RESET COMND WITH NEW LCPB AND PATH IS SICK OR  RESET - IN -    01573000
*    PROGRESS IS OFF...IF SO THE RESET WILL BE PROCESSED.    @XM03HDP*/ 01574000
*                                                                  0969 01575000
*             |(LCCWCTL=LRESET&LCPSTYPE^=LSRSTYPE/*          @XL03HDP*/ 01576000
*             &(FMCCONF='1'B|FMCZF023='0'B|LCPINTER='1'B)) THEN/*  0969 01577000
*                                      INTERNAL RESET?       @ZA13704*/ 01578000
         L     @14,LCPNXLCW(,PTRLCP)                               0969 01579000
         CLI   LCCWOP(@14),X'FF'                                   0969 01580000
         BE    @RT00969                                            0969 01581000
         TM    LCCWCTL(@14),B'00001011'                            0969 01582000
         BNO   @RF00969                                            0969 01583000
