         EJECT                                                          00713000
*                                                                  0830 00714000
*/********************************************************************/ 00715000
*/*                                                                  */ 00716000
*/* SEGMENT-                                                         */ 00717000
*/*      SEG00BA                                                     */ 00718000
*/*                                                                  */ 00719000
*/* FUNCTION-                                                        */ 00720000
*/*      PROCESS BIND REQUEST.                                       */ 00721000
*/*      THE BIND FUNCTIONAL PROCESSOR DOES NOTHING IF A CLEAR       */ 00722000
*/*      IS IN PROGRESS.                                             */ 00723000
*/*      OTHERWISE, THE FMCB BHSET IS SET TO 0, AND THE      @X50BHFZ*/ 00724000
*/*      DNCB MODE AND BHSET ARE SET TO MATCH THE FMCB MODE  @X50BHFZ*/ 00725000
*/*      AND BHSET.  I/O IS DONE TO INSURE THAT THE NCP MODE @X50BHFZ*/ 00726000
*/*      AND BHSET MATCH THE DNCB MODE AND BHSET.            @X50BHFZ*/ 00727000
*/*                                                                  */ 00728000
*/********************************************************************/ 00729000
*                                                                  0830 00730000
*   /*****************************************************************/ 00731000
*   /*                                                               */ 00732000
*   /* SEGMENT(SEG00BA)START                                         */ 00733000
*   /*                                                               */ 00734000
*   /*****************************************************************/ 00735000
*                                                                  0830 00736000
*   DO;                                                            0830 00737000
*     GOTO SEG00BA@;                                               0831 00738000
         B     SEG00BA@                                            0831 00739000
*SEG00BA:                                                          0832 00740000
*   END;                            /* AF1R00BA: SUBROUTINE          */ 00741000
SEG00BA  DS    0H                                                  0833 00742000
*   IF FMCCIPI='1'B THEN                                           0833 00743000
         TM    FMCCIPI(PTRFMC),B'01000000'                         0833 00744000
         BO    @RT00833                                            0833 00745000
*     ;                             /* EXIT IF CLEAR IN PROGRESS     */ 00746000
*   ELSE                                                           0835 00747000
*     DO;                           /* CLEAR NOT IN PROGRESS         */ 00748000
*       CALL SUB000G;               /* DEQUEUE CURRENT ATOP ELEM     */ 00749000
         BAL   @14,SUB000G                                         0836 00750000
*       CALL SUB000H;               /* QUEUE BIND LCPB TO FMCB     0837 00751000
*                                      ASYNCHRONOUS WAIT QUEUE       */ 00752000
         BAL   @14,SUB000H                                         0837 00753000
*       PTRG15=0;                   /* INITIALIZE RETURN FLAG        */ 00754000
         SLR   PTRG15,PTRG15                                       0838 00755000
*       DNCZF002='1'B;              /* INITIALIZE DNCB DIALOG      0839 00756000
*                                      SCHEDULE FLAG                 */ 00757000
         OI    DNCZF002(PTRDNC),B'00000010'                        0839 00758000
*       IF PTRICN->NCBTYPE=LDTYPE THEN                             0840 00759000
         CLI   NCBTYPE(PTRICN),X'07'                               0840 00760000
         BE    @RT00840                                            0840 00761000
*         ;                         /* IF LOCAL, NO NEED TO MATCH  0841 00762000
*                                      FMCB AND DNCB MODE/BHSET      */ 00763000
*       ELSE                                                       0842 00764000
*         DO;                       /* PROCESS 370X DNCB/FMCB MODE 0842 00765000
*                                      AND BHSET MATCHING            */ 00766000
*           FMCBHSET=0;             /* FORCE BHSET DESELECTION     0843 00767000
*                                                            @X50BHFZ*/ 00768000
         MVI   FMCBHSET(PTRFMC),X'00'                              0843 00769000
*           IF DNCMODE^=ISTPROCD(2:4) THEN                         0844 00770000
         CLC   DNCMODE(3,PTRDNC),ISTPROCD+49(PTRFMC)               0844 00771000
         BE    @RF00844                                            0844 00772000
*             DO;                   /* IF MODES DON'T MATCH          */ 00773000
*               PTRG00=0;           /* INDICATE MODE PROCESSING      */ 00774000
         SLR   PTRG00,PTRG00                                       0846 00775000
*               CALL SUB0BAA;       /* BUILD SET MODE PIU            */ 00776000
         BAL   @14,SUB0BAA                                         0847 00777000
*             END;                  /* END MODES DON'T MATCH         */ 00778000
*           ELSE                                                   0849 00779000
*             ;                     /* MODES MATCH                   */ 00780000
@RF00844 DS    0H                                                  0850 00781000
*           IF DNCBHSET^=FMCBHSET THEN                             0850 00782000
         CLC   DNCBHSET(1,PTRDNC),FMCBHSET(PTRFMC)                 0850 00783000
         BE    @RF00850                                            0850 00784000
*             DO;                   /* IF BHSETS DON'T MATCH         */ 00785000
*               PTRG00=1;           /* INDICATE BHSET PROCESSING     */ 00786000
         LA    PTRG00,1                                            0852 00787000
*               CALL SUB0BAA;       /* BUILD SET BHSET ASSOC. PIU    */ 00788000
         BAL   @14,SUB0BAA                                         0853 00789000
*             END;                  /* END BHSETS DON'T MATCH        */ 00790000
*           ELSE                                                   0855 00791000
*             ;                     /* BHSETS MATCH                  */ 00792000
@RF00850 DS    0H                                                  0856 00793000
*         END;                      /* END MODE/BHSET MATCHING       */ 00794000
*       IF PTRG15=0 THEN            /* IF NO I/O REQUIRED            */ 00795000
