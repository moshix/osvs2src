*                                                                  0889 01158000
*   DO;                                                            0889 01159000
@RC00883 DS    0H                                                  0889 01160000
@RT00881 DS    0H                                                  0890 01161000
*     GOTO PROCRST#;                                               0890 01162000
         B     PROCRST#                                            0890 01163000
*PROCRST@:                                                         0891 01164000
*   END;                            /* END OF SEGMENT                */ 01165000
PROCRST@ DS    0H                                                  0892 01166000
*   GEN NOSETS NODEFS NOREFS(EJECT);                               0892 01167000
         EJECT                                                          01168000
*                                                                  0893 01169000
*/********************************************************************/ 01170000
*/*                                                                  */ 01171000
*/* SEGMENT-                                                         */ 01172000
*/*      ERRPROC                                                     */ 01173000
*/*                                                                  */ 01174000
*/* FUNCTION-                                                        */ 01175000
*/*      SETS ERROR LOCK.                                            */ 01176000
*/*      DETERMINES HOW MANY REAL CCW'S CAN BE PROCESS, AND GOES     */ 01177000
*/*      AHEAD AND INITIATES THEM.                                   */ 01178000
*/*                                                                  */ 01179000
*/********************************************************************/ 01180000
*                                                                  0893 01181000
*   /*****************************************************************/ 01182000
*   /*                                                               */ 01183000
*   /* SEGMENT(ERRPROC)START                                         */ 01184000
*   /*                                                               */ 01185000
*   /*****************************************************************/ 01186000
*                                                                  0893 01187000
*   DO;                                                            0893 01188000
*     GOTO ERRPROC@;                                               0894 01189000
         B     ERRPROC@                                            0894 01190000
*ERRPROC:                                                          0895 01191000
*   END;                            /* ERRPROC: SUBROUTINE           */ 01192000
ERRPROC  DS    0H                                                  0896 01193000
*   ZCRTSW2='0'B;                   /* INDICATE NO RELEASE           */ 01194000
         L     @14,RPHCRR(,PTRRPH)                                 0896 01195000
         L     @14,CRACRR(,@14)                                    0896 01196000
         NI    ZCRTSW2(@14),B'10111111'                            0896 01197000
*   PTRZLB=ZCRZLB;                  /* ADDRESS BUFFER                */ 01198000
         L     PTRZLB,ZCRZLB(,@14)                                 0897 01199000
*   IF(LCPFDBA&MASKWORD)=0          /* IF BUFF NOT CHAINED           */ 01200000
*       |LCPFDBA->ZLBLCCWA=LCPNXLCW THEN                           0898 01201000
         L     @14,LCPFDBA(,PTRLCP)                                0898 01202000
         LR    @11,@14                                             0898 01203000
         N     @11,@CF04341                                        0898 01204000
         LTR   @11,@11                                             0898 01205000
         BZ    @RT00898                                            0898 01206000
         CLC   ZLBLCCWA(4,@14),LCPNXLCW(PTRLCP)                    0898 01207000
         BE    @RT00898                                            0898 01208000
*     ;                             /* | FIRST BUFFER IN ERROR       */ 01209000
*   ELSE                            /* NOT FIRST BUFFER IN ERROR     */ 01210000
*     DO;                           /* LEVEL 1 - NOT FIRST BUFFER    */ 01211000
*       IF ZCRZLB->ZLBLCCWA=LCPNXLCW THEN/* IF BUFFER OBTAINED FOR 0901 01212000
*                                      LCCW IN ERROR                 */ 01213000
         L     @14,RPHCRR(,PTRRPH)                                 0901 01214000
         L     @14,CRACRR(,@14)                                    0901 01215000
         L     @11,ZCRZLB(,@14)                                    0901 01216000
         CLC   ZLBLCCWA(4,@11),LCPNXLCW(PTRLCP)                    0901 01217000
         BNE   @RF00901                                            0901 01218000
*         DO;                       /* LEVEL 2 - BUFF FOR ERR LCCW   */ 01219000
*           ZCRTSW2='1'B;           /* INDICATE RELEASE BUFFER       */ 01220000
         OI    ZCRTSW2(@14),B'01000000'                            0903 01221000
*           PTRZLB=ZCRPVZB;         /* ADDRESS PREVIOUS BUFFER       */ 01222000
         L     PTRZLB,ZCRPVZB(,@14)                                0904 01223000
*           PTRG15=ZLBLSTST;        /* ADDRESS LAST DATA CHAINED     */ 01224000
         L     PTRG15,ZLBLSTST(,PTRZLB)                            0905 01225000
*           ZCRBFPTR=PTRG15;        /* SET POINTER IN ZCR            */ 01226000
         ST    PTRG15,ZCRBFPTR(,@14)                               0906 01227000
*           PTRG15->FDBFDBA=0;      /* ZERO POINTER TO NEXT BUFFER   */ 01228000
         SLR   @14,@14                                             0907 01229000
         ST    @14,FDBFDBA(,PTRG15)                                0907 01230000
*           IF PTRG15->ZLBCPCF2=TIC THEN/* IF CCW1 TO BE MODIFIED    */ 01231000
         CLI   ZLBCPCF2(PTRG15),X'08'                              0908 01232000
         BNE   @RF00908                                            0908 01233000
*             ZLBCPFL1=ZLBSLI;      /* SET SLI ONLY IN FIRST.        */ 01234000
         MVI   ZLBCPFL1(PTRZLB),X'20'                              0909 01235000
*           ELSE                    /* IF CCW2 TO BE MODIFIED        */ 01236000
*             ZLBCPFL2=ZLBSLI;      /* SET SLI TO PREVENT TIC        */ 01237000
         B     @RC00908                                            0910 01238000
@RF00908 MVI   ZLBCPFL2(PTRZLB),X'20'                              0910 01239000
*         END;                      /* LEVEL 2 - BUFF FOR ERR LCCW   */ 01240000
*       ELSE                                                       0912 01241000
*         ;                         /* IF BUFF NOT OBTAINED          */ 01242000
@RF00901 DS    0H                                                  0913 01243000
*       ZLBRT02='1'B;               /* SET END OF BLOCK FLAG         */ 01244000
@RC00901 DS    0H                                                  0914 01245000
*       ZLBRT05='1'B;               /* SET DECREMENT SCHED CNT       */ 01246000
*       ZLBRT07='1'B;               /* SET SCHEDULE FLAG             */ 01247000
         OI    ZLBRT02(PTRZLB),B'00100101'                         0915 01248000
*       IF FMCZX003=0 THEN          /* IF SCHEDULE COUNT ZERO        */ 01249000
         CLI   FMCZX003(PTRFMC),0                                  0916 01250000
         BNE   @RF00916                                            0916 01251000
*         CALL GETLSCB;             /* ALLOCATE LSCB + INIT I/O      */ 01252000
         BAL   @14,GETLSCB                                         0917 01253000
*       ELSE                        /* IF SCHED COUNT NON-ZERO       */ 01254000
*         DO;                       /* LEVEL 2 - NONZERO COUNT       */ 01255000
         B     @RC00916                                            0918 01256000
@RF00916 DS    0H                                                  0919 01257000
*           PTRZLB=LCPFDBA;         /* ADDRESS BEGINNING OF CHAN P   */ 01258000
         L     PTRZLB,LCPFDBA(,PTRLCP)                             0919 01259000
*           DO WHILE(FMCZX003^=0);  /* ADDRESS APPROPRIATE ZLBUF     */ 01260000
         B     @DE00920                                            0920 01261000
@DL00920 DS    0H                                                  0921 01262000
*             PTRG15=ZLBLSTST;      /* ADDR LAST DATA CHAINED LCCW   */ 01263000
         L     PTRG15,ZLBLSTST(,PTRZLB)                            0921 01264000
*             IF PTRG15->ZLBRT02='1'B THEN/* IF END OF BLOCK         */ 01265000
         TM    ZLBRT02(PTRG15),B'00100000'                         0922 01266000
         BNO   @RF00922                                            0922 01267000
*               FMCZX003=FMCZX003-1;/* DECREMENT I/O SCHED COUNT     */ 01268000
         SLR   @14,@14                                             0923 01269000
         IC    @14,FMCZX003(,PTRFMC)                               0923 01270000
         BCTR  @14,0                                               0923 01271000
         STC   @14,FMCZX003(,PTRFMC)                               0923 01272000
*             ELSE                                                 0924 01273000
*               ;                   /* OTHERWISE DON'T               */ 01274000
@RF00922 DS    0H                                                  0925 01275000
*             PTRZLB=PTRG15->FDBFDBA;/* ADDRESS NEXT BUFFER          */ 01276000
         L     PTRZLB,FDBFDBA(,PTRG15)                             0925 01277000
*           END;                    /* APPROPRIATE ADDRESS SET       */ 01278000
@DE00920 CLI   FMCZX003(PTRFMC),0                                  0926 01279000
         BNE   @DL00920                                            0926 01280000
*           IF PTRZLB=0 THEN        /* IF BUFFER NOT CHAINED         */ 01281000
         LTR   PTRZLB,PTRZLB                                       0927 01282000
         BNZ   @RF00927                                            0927 01283000
*             CALL DOIO;            /* CALL SHIPOUT TO SEND I/O      */ 01284000
         BAL   @14,DOIO                                            0928 01285000
*           ELSE                    /* IF BUFFER CHAINED             */ 01286000
*             CALL GETLSCB;         /* ALLOCATE LSCB + INIT I/O      */ 01287000
         B     @RC00927                                            0929 01288000
@RF00927 BAL   @14,GETLSCB                                         0929 01289000
*         END;                      /* LEVEL 2 - NONZERO COUNT       */ 01290000
@RC00927 DS    0H                                                  0931 01291000
*       IF ZCRTSW2='1'B THEN        /* IF BUFFER TO BE FREED         */ 01292000
@RC00916 L     @14,RPHCRR(,PTRRPH)                                 0931 01293000
         L     @14,CRACRR(,@14)                                    0931 01294000
         TM    ZCRTSW2(@14),B'01000000'                            0931 01295000
         BNO   @RF00931                                            0931 01296000
*         LCPFDBA=ZCRZLB;           /* ADDRESS OF DOOMED BUFFER      */ 01297000
         L     @14,ZCRZLB(,@14)                                    0932 01298000
         ST    @14,LCPFDBA(,PTRLCP)                                0932 01299000
*       ELSE                                                       0933 01300000
*         ;                         /* NO BUFFER TO FREE             */ 01301000
@RF00931 DS    0H                                                  0934 01302000
*     END;                          /* LEVEL 1 - NOT FIRST BUFFER    */ 01303000
*   CALL ATCZPSVT->ZPSZBFZ0;        /* CALL ERROR RECOVERY           */ 01304000
@RT00898 L     @11,ATCZPSVT(,PTRATC)                               0935 01305000
         L     @15,ZPSZBFZ0(,@11)                                  0935 01306000
         BALR  @14,@15                                             0935 01307000
*   ZCRSLCP='0'B;                   /* CLEAR PROCESS SWITCH          */ 01308000
*   ZCREXTYP='0'B;                  /* SET EXIT TYPE = NORMAL        */ 01309000
*                                                                  0937 01310000
         L     @14,RPHCRR(,PTRRPH)                                 0937 01311000
         L     @14,CRACRR(,@14)                                    0937 01312000
         NI    ZCRSLCP(@14),B'00111111'                            0937 01313000
*   /*****************************************************************/ 01314000
*   /*                                                               */ 01315000
*   /* SEGMENT(ERRPROC)FINISH                                        */ 01316000
*   /*                                                               */ 01317000
*   /*****************************************************************/ 01318000
*                                                                  0938 01319000
*   DO;                                                            0938 01320000
*     GOTO ERRPROC#;                                               0939 01321000
         B     ERRPROC#                                            0939 01322000
*ERRPROC@:                                                         0940 01323000
*   END;                            /* END OF SEGMENT                */ 01324000
ERRPROC@ DS    0H                                                  0941 01325000
*/* TPEPILOG                                                         */ 01326000
*                                                                  0941 01327000
*   GEN(PRINT OFF);                                                0941 01328000
         PRINT OFF                                                      01329000
*   GEN(PRINT ON);                                                 0942 01330000
*                                                                  0942 01331000
         PRINT ON                                                       01332000
*/* END OF TPEPILOG                                                  */ 01333000
*                                                                  0943 01334000
*   END ISTZBF0A                    /* END OF COMPILATION            */ 01335000
*                                                                  0943 01336000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 01337000
*/*%INCLUDE SYSLIB  (IECDIOSB)                                       */ 01338000
