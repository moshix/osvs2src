         OI    LCCWLSI(@14),B'00000100'                            0910 01447000
*       ELSE                                                       0911 01448000
*         ;                         /* LCCW SYNCH NOT SPECIFIED      */ 01449000
@RF00909 DS    0H                                                  0912 01450000
*                                                                  0912 01451000
*/*VMDESET  (15,ZERO)                                                */ 01452000
*                                                                  0912 01453000
*       DO;                         /* INVOKE MODESET MACRO          */ 01454000
*         GEN;                                                     0913 01455000
         MODESET EXTKEY=ZERO,WORKREG=2                                  01456000
*         RFY                                                      0914 01457000
*          (GPR15P) RSTD;                                          0914 01458000
*         GPR15F=0;                                                0915 01459000
         SLR   @11,@11                                             0915 01460000
         LR    GPR15F,@11                                          0915 01461000
*         RFY                                                      0916 01462000
*          (GPR15P) UNRSTD;         /* PUT RETURN CODE IN RCREG      */ 01463000
*       END;                        /* VMDESET TRY TO GET TO KEY 0   */ 01464000
*       IF R15=0 THEN               /* IF WE MADE IT                 */ 01465000
         CR    R15,@11                                             0918 01466000
         BNE   @RF00918                                            0918 01467000
*         DO;                                                      0919 01468000
*           IF LCCWNTVC^='1'B THEN  /* GET LOCAL LOCK AND DO @ZA08949*/ 01469000
