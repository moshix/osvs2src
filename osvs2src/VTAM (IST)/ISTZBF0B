*                   END;            /* *** END OF TPQUE GENERATION   */ 00813000
*                                                                  0749 00814000
*               /*****************************************************/ 00815000
*               /*                                                   */ 00816000
*               /* PUT NEW ELEMENT ON SCHEDULE Q                     */ 00817000
*               /*                                                   */ 00818000
*               /*****************************************************/ 00819000
*                                                                  0750 00820000
*               ZOBLNG2=ZOBLNG1;    /* SAVE BUFFER LENGTH IN BUFFER  */ 00821000
