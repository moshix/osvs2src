         TM    LCCWNTVC(@14),B'00010000'                           0920 01471000
         BO    @RF00920                                            0920 01472000
*             DO;                   /* VALCHECK ONLY IF THIS IS NOT     01473000
*                                      AUTHORIZED PATH       @ZA08949*/ 01474000
*               WORK=R13;           /* SAVE TPIOS SAVE AREA ADDRESS  */ 01475000
         LR    WORK,R13                                            0922 01476000
*               R13=ADDR(CRASVC);   /* MACRO SAVE AREA ADDRESS       */ 01477000
         L     R13,RPHCRR(,RPHREG)                                 0923 01478000
         LA    R13,CRASVC(,R13)                                    0923 01479000
*BF0B45LK:                                                         0924 01480000
*                                                                  0924 01481000
*/*VSETLCK  (15,OBTAIN,LOCAL,(LOCAL,ISTZBF0B(BF0B45UL)))             */ 01482000
