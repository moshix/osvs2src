*                 ;                 /* END BRACKET NO SPECIFIED    1057 01942000
*                                                            @ZA08949*/ 01943000
@RF01055 DS    0H                                                  1058 01944000
*               IF RHEXCEPT='1'B THEN/* EXCEPTION RESPONSE ? @ZA08949*/ 01945000
         TM    RHEXCEPT+4(R10),B'00010000'                         1058 01946000
         BNO   @RF01058                                            1058 01947000
*                 ZCREXRES='1'B;    /* FLAG THE ZCR          @ZA08949*/ 01948000
         OI    ZCREXRES(ZCRREG),B'00001000'                        1059 01949000
*               ELSE                                               1060 01950000
*                 ;                 /* NO EXCEPTION RESPONSE REQUEST    01951000
*                                                            @ZA08949*/ 01952000
@RF01058 DS    0H                                                  1061 01953000
*               LCCWDATA=LCCWDATA+1;/* POINT TO NEXT DATA BYTE, I.E.    01954000
*                                      COMMAND IS NOT DATA   @ZA08949*/ 01955000
         L     @11,LCPNXLCW(,LCPBREG)                              1061 01956000
         LA    @15,1                                               1061 01957000
         AL    @15,LCCWDATA(,@11)                                  1061 01958000
         ST    @15,LCCWDATA(,@11)                                  1061 01959000
*               LCCWCNT=LCCWCNT-1;  /* ADJUST COUNT ACCORDINGLY    1062 01960000
*                                                            @ZA08949*/ 01961000
         LH    @15,LCCWCNT(,@11)                                   1062 01962000
         N     @15,@CF04731                                        1062 01963000
         BCTR  @15,0                                               1062 01964000
         STH   @15,LCCWCNT(,@11)                                   1062 01965000
*               RFY                                                1063 01966000
*                 R15 RSTD;         /*                       @ZA08949*/ 01967000
*             END;                  /* END AUTHORIZED PATH   @ZA08949*/ 01968000
*/*VMDESET  (15,VTAM)                                                */ 01969000
*                                                                  1065 01970000
*           DO;                     /* INVOKE MODESET MACRO          */ 01971000
@RC00920 DS    0H                                                  1066 01972000
*             GEN;                                                 1066 01973000
         MODESET EXTKEY=VTAM,WORKREG=2                                  01974000
*             RFY                                                  1067 01975000
*              (GPR15P) RSTD;                                      1067 01976000
*             GPR15F=0;                                            1068 01977000
         SLR   GPR15F,GPR15F                                       1068 01978000
*             RFY                                                  1069 01979000
*              (GPR15P) UNRSTD;     /* PUT RETURN CODE IN RCREG      */ 01980000
*           END;                    /* VMDESET GET BACK INTO VTAM KEY*/ 01981000
*         END;                      /* END OK VMDESET LEG            */ 01982000
*       ELSE                        /* BAD VMDESET                   */ 01983000
*         DO;                                                      1072 01984000
         B     @RC00918                                            1072 01985000
@RF00918 DS    0H                                                  1073 01986000
*           LCPZRT00='1'B;          /* LOGIC ERROR                   */ 01987000
         OI    LCPZRT00(LCPBREG),B'10000000'                       1073 01988000
*           LCPMNRCD=FRMVTAME;      /* VTAM ERROR                    */ 01989000
         MVI   LCPMNRCD(LCPBREG),X'A0'                             1074 01990000
*         END;                      /* END BAD VMDESET PATH          */ 01991000
*     END;                          /* END 2ND LCCW VALID PATH       */ 01992000
@RC00918 DS    0H                                                  1077 01993000
*/* SEGMENT(BF0B045)FINISH                                           */ 01994000
*                                                                  1077 01995000
*   DO;                                                            1077 01996000
@RC00904 DS    0H                                                  1078 01997000
*     GOTO BF0B045#;                                               1078 01998000
         B     BF0B045#                                            1078 01999000
*BF0B045@:                                                         1079 02000000
*   END;                                                           1079 02001000
*                                                                  1079 02002000
BF0B045@ DS    0H                                                  1080 02003000
*/* ZERORU - THIS SEGMENT CHECKS IF RU INDICATES                     */ 02004000
*/*          THAT A DISCONNECT SHOULD BE SENT OUT.                   */ 02005000
*/*          ERROR IS SET ON ANY OTHER RH.                           */ 02006000
*/* SEGMENT(ZERORU)START                                             */ 02007000
*                                                                  1080 02008000
*   DO;                                                            1080 02009000
*     GOTO ZERORU@;                                                1081 02010000
         B     ZERORU@                                             1081 02011000
*ZERORU:                                                           1082 02012000
*   END;                                                           1082 02013000
ZERORU   DS    0H                                                  1083 02014000
*   IF RHBB='0'B&                   /* NOT BEGIN BRACKIT BUT @ZA28604*/ 02015000
*       RHEB='1'B THEN              /* END BRACKET           @ZA28604*/ 02016000
         TM    RHEB+4(R10),B'01000000'                             1083 02017000
         BNO   @RF01083                                            1083 02018000
         TM    RHBB+4(R10),B'10000000'                             1083 02019000
         BNZ   @RF01083                                            1083 02020000
*     DO;                                                          1084 02021000
*       ISTLCCW=''B;                /* ZERO LCCW ADDRESS     @ZA28604*/ 02022000
         L     @14,LCPNXLCW(,LCPBREG)                              1085 02023000
         XC    ISTLCCW(8,@14),ISTLCCW(@14)                         1085 02024000
*       LCCWOP=LD;                  /* CMND = DISCONNECT     @ZA28604*/ 02025000
         MVI   LCCWOP(@14),X'23'                                   1086 02026000
*       FMCZF002='1'B;              /* SET I/O SCHD SUSPENDED      1087 02027000
*                                                            @ZA28604*/ 02028000
         OI    FMCZF002(FMCBREG),B'01000000'                       1087 02029000
*     END;                                                         1088 02030000
*   ELSE                            /* NOT VALID RH          @ZA28604*/ 02031000
*     DO;                                                          1089 02032000
         B     @RC01083                                            1089 02033000
@RF01083 DS    0H                                                  1090 02034000
*       LCPZRT01='1'B;              /* CONTROL BLOCK ERROR   @ZA28604*/ 02035000
         OI    LCPZRT01(LCPBREG),B'01000000'                       1090 02036000
*       LCPMNRCD=FRMINDTR;          /* INVALID DATA          @ZA28604*/ 02037000
         MVI   LCPMNRCD(LCPBREG),X'17'                             1091 02038000
*     END;                                                         1092 02039000
*                                                                  1092 02040000
*/* SEGMENT(ZERORU)FINISH                                            */ 02041000
*                                                                  1093 02042000
*   DO;                                                            1093 02043000
@RC01083 DS    0H                                                  1094 02044000
*     GOTO ZERORU#;                                                1094 02045000
         B     ZERORU#                                             1094 02046000
