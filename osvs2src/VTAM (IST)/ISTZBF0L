         L     @14,CRACRR(,@14)                                    1058 01680000
         TM    ZCREXRES(@14),B'00001000'                           1058 01681000
         BNO   @RF01058                                            1058 01682000
*     LSCEXRES='1'B;                /* SET EXCEPTION RESPONSE        */ 01683000
         OI    LSCEXRES(PTRLSC),B'00001000'                        1059 01684000
*   ELSE                                                           1060 01685000
*     ;                             /* OTHERWISE BEST NOT TO         */ 01686000
@RF01058 DS    0H                                                  1061 01687000
*   RPHWEA=0;                       /* FLAG END OF LCPB PROCESSING   */ 01688000
         SLR   @14,@14                                             1061 01689000
         ST    @14,RPHWEA(,PTRRPH)                                 1061 01690000
*   RFY                                                            1062 01691000
*     ISTLSCB;                      /* NO LONGER REQUIRED            */ 01692000
