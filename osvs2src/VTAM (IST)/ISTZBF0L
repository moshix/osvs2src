*     DO;                           /* LCCW COUNT NON-ZERO           */ 00628000
*                                                                  0741 00629000
         B     @RC00737                                            0741 00630000
@RF00737 DS    0H                                                  0742 00631000
*       /*************************************************************/ 00632000
*       /*                                                           */ 00633000
*       /* SEGMENT(SEGVAL)REQUEST                                    */ 00634000
*       /*                                                           */ 00635000
*       /*************************************************************/ 00636000
*                                                                  0742 00637000
*       DO;                                                        0742 00638000
*         GOTO SEGVAL;                                             0743 00639000
         B     SEGVAL                                              0743 00640000
*SEGVAL#:                                                          0744 00641000
*       END;                        /* VALIDATE AND MOVE 1ST DATA  0744 00642000
*                                      BYTE TO ZCR                   */ 00643000
SEGVAL#  DS    0H                                                  0745 00644000
*       IF LCPZRT00='0'B            /* IF NO VALIDATION ERROR        */ 00645000
*           &LCPZRT01='0'B THEN                                    0745 00646000
         TM    LCPZRT00(PTRLCP),B'11000000'                        0745 00647000
         BNZ   @RF00745                                            0745 00648000
*         DO;                                                      0746 00649000
*           LCPNXLCW->LCCWOP=SNATRT(ZCROPCDE+1);/* TRANSLATE DATA  0747 00650000
*                                      BYTE TO BASIC LCCW OPERATION     00651000
*                                      CODE                          */ 00652000
