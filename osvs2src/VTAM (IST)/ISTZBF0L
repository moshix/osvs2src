*                   GOTO SEG0069;                                  0682 00406000
         B     SEG0069                                             0682 00407000
*SEG0069#:                                                         0683 00408000
*                 END;              /* TRANSFORM LCCW OP CODE        */ 00409000
*               ELSE                                               0684 00410000
*                 ;                 /* IF ERROR DETECTED             */ 00411000
@RF00680 DS    0H                                                  0685 00412000
*               RFY                                                0685 00413000
*                 PTRRH UNRSTD;                                    0685 00414000
@RC00680 DS    0H                                                  0686 00415000
*             END;                  /* END SEND LCCW PROCESSOR       */ 00416000
*           ELSE                                                   0687 00417000
*             ;                     /* IF NOT SEND LCCW              */ 00418000
@RF00673 DS    0H                                                  0688 00419000
*         END;                      /* LEVEL 3 - BUFFER GOTTEN       */ 00420000
*       ELSE                        /* BUFFER NOT OBTAINED           */ 00421000
*         DO;                       /* PROCESS VTAM ERROR            */ 00422000
         B     @RC00659                                            0689 00423000
@RF00659 DS    0H                                                  0690 00424000
*           LCPZRT00='1'B;          /* INDICATE LOGIC ERROR          */ 00425000
         OI    LCPZRT00(PTRLCP),B'10000000'                        0690 00426000
*           LCPMNRCD=FRMVTAME;      /* INDICATE VTAM ERROR           */ 00427000
         MVI   LCPMNRCD(PTRLCP),X'A0'                              0691 00428000
*         END;                      /* END BUFFER NOT OBTAINED ERR   */ 00429000
*     END;                          /* LEVEL 1 - NO ERROR            */ 00430000
@RC00659 DS    0H                                                  0694 00431000
*   IF LCPZRT00='0'B                /* IF NO ERROR                   */ 00432000
*       &LCPZRT01='0'B THEN                                        0694 00433000
@RT00653 TM    LCPZRT00(PTRLCP),B'11000000'                        0694 00434000
         BNZ   @RF00694                                            0694 00435000
*     PTRG15=ATCZPSVT->ZPSZTRTT->ISTZTRTT(LCPNXLCW->LCCWOP+1);/* SET    00436000
*                                      LCCW PROCESS VECTOR   @XM06212*/ 00437000
