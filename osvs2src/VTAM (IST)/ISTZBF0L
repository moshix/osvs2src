         IC    @00,SNATRT(@08)                                     0747 00659000
         STC   @00,LCCWOP(,@14)                                    0747 00660000
*           IF RHBB='1'B THEN       /* IF BEG SEQUENCE               */ 00661000
         TM    RHBB(PTRRH),B'10000000'                             0748 00662000
         BNO   @RF00748                                            0748 00663000
*             LCPNXLCW->LCCWSI='1'B;/* THEN SESSION INITIATION       */ 00664000
         OI    LCCWSI(@14),B'10000000'                             0749 00665000
*           ELSE                                                   0750 00666000
*             ;                     /* NOT BEGIN SEQUENCE            */ 00667000
@RF00748 DS    0H                                                  0751 00668000
*           IF RHEB='1'B THEN       /* IF END SEQUENCE               */ 00669000
         TM    RHEB(PTRRH),B'01000000'                             0751 00670000
         BNO   @RF00751                                            0751 00671000
*             LCPNXLCW->LCCWSD='1'B;/* THEN SESSION DISCONNECT       */ 00672000
         L     @14,LCPNXLCW(,PTRLCP)                               0752 00673000
         OI    LCCWSD(@14),B'01000000'                             0752 00674000
*           ELSE                                                   0753 00675000
*             ;                     /* NOT END SEQUENCE              */ 00676000
@RF00751 DS    0H                                                  0754 00677000
*           IF RHEXCEPT='1'B THEN   /* IF EXCEPTION RESPONSE         */ 00678000
         TM    RHEXCEPT(PTRRH),B'00010000'                         0754 00679000
         BNO   @RF00754                                            0754 00680000
*             ZCREXRES='1'B;        /* SET ZCR EXCEPTION RESPONSE    */ 00681000
         L     @14,RPHCRR(,PTRRPH)                                 0755 00682000
         L     @14,CRACRR(,@14)                                    0755 00683000
         OI    ZCREXRES(@14),B'00001000'                           0755 00684000
*           ELSE                                                   0756 00685000
*             ;                     /* NOT EXCEPTION RESPONSE        */ 00686000
@RF00754 DS    0H                                                  0757 00687000
*           LCPNXLCW->LCCWDATA=LCPNXLCW->LCCWDATA+1;/* INCREMENT DATA   00688000
*                                      ADDRESS                       */ 00689000
         L     @14,LCPNXLCW(,PTRLCP)                               0757 00690000
         LA    @00,1                                               0757 00691000
         AL    @00,LCCWDATA(,@14)                                  0757 00692000
         ST    @00,LCCWDATA(,@14)                                  0757 00693000
*           LCPNXLCW->LCCWCNT=LCPNXLCW->LCCWCNT-1;/* DECREMENT DATA     00694000
*                                      COUNT                         */ 00695000
         LH    @00,LCCWCNT(,@14)                                   0758 00696000
         N     @00,@CF04884                                        0758 00697000
         BCTR  @00,0                                               0758 00698000
         STH   @00,LCCWCNT(,@14)                                   0758 00699000
*         END;                      /* END VALIDATION OK             */ 00700000
*       ELSE                                                       0760 00701000
*         ;                         /* NOT VALIDATED OK              */ 00702000
@RF00745 DS    0H                                                  0761 00703000
*     END;                          /* END LCCW COUNT NON-ZERO       */ 00704000
*                                                                  0761 00705000
*   /*****************************************************************/ 00706000
*   /*                                                               */ 00707000
*   /* SEGMENT(SEG0069)FINISH                                        */ 00708000
*   /*                                                               */ 00709000
*   /*****************************************************************/ 00710000
*                                                                  0762 00711000
*   DO;                                                            0762 00712000
@RC00737 DS    0H                                                  0763 00713000
*     GOTO SEG0069#;                                               0763 00714000
         B     SEG0069#                                            0763 00715000
*SEG0069@:                                                         0764 00716000
*   END;                            /* END OF SUBROUTINE             */ 00717000
SEG0069@ DS    0H                                                  0765 00718000
*                                                                  0765 00719000
*/********************************************************************/ 00720000
*/*                                                                  */ 00721000
*/* SEGMENT-                                                         */ 00722000
*/*      SEGVAL                                                      */ 00723000
*/*                                                                  */ 00724000
*/* FUNCTION-                                                        */ 00725000
*/*      VALIDITY CHECK 1ST DDATA BYTE OF 2ND SEND LCCW.             */ 00726000
*/*      THE DATA BYTE CONTAINS THE 3270 FUNCTION CODE.              */ 00727000
*/*                                                                  */ 00728000
*/********************************************************************/ 00729000
*                                                                  0765 00730000
*   /*****************************************************************/ 00731000
*   /*                                                               */ 00732000
*   /* SEGMENT(SEGVAL)START                                          */ 00733000
*   /*                                                               */ 00734000
*   /*****************************************************************/ 00735000
*                                                                  0765 00736000
*   DO;                                                            0765 00737000
*     GOTO SEGVAL@;                                                0766 00738000
         B     SEGVAL@                                             0766 00739000
*SEGVAL:                                                           0767 00740000
*   END;                            /* VALIDATE: SUBROUTINE          */ 00741000
SEGVAL   DS    0H                                                  0768 00742000
*   ZCRSVWK1=PTRG13;                /* SAVE SAVE AREA POINTER        */ 00743000
         L     @14,RPHCRR(,PTRRPH)                                 0768 00744000
         L     @09,CRACRR(,@14)                                    0768 00745000
         ST    PTRG13,ZCRSVWK1(,@09)                               0768 00746000
*   ZCRSVWK2=PTRRH;                 /* COMPILER NEEDS A REGISTER     */ 00747000
         ST    PTRRH,ZCRSVWK2(,@09)                                0769 00748000
*   RFY                                                            0770 00749000
*     PTRRH UNRSTD;                 /* RELEASE FOR COMPILER          */ 00750000
*   PTRG13=ADDR(CRASVC);            /* ADDRESS A SAVE AREA VMDESET 0771 00751000
*                                      REMOVED               @DC80401*/ 00752000
         LA    PTRG13,CRASVC(,@14)                                 0771 00753000
*                                                                  0772 00754000
*/*VMDESET  (15,ZERO)                                                */ 00755000
*                                                                  0772 00756000
*   DO;                             /* INVOKE MODESET MACRO          */ 00757000
*     GEN;                                                         0773 00758000
         MODESET EXTKEY=ZERO,WORKREG=2                                  00759000
*     RFY                                                          0774 00760000
*      (GPR15P) RSTD;                                              0774 00761000
*     GPR15F=0;                                                    0775 00762000
         SLR   GPR15F,GPR15F                                       0775 00763000
*     RFY                                                          0776 00764000
*      (GPR15P) UNRSTD;             /* PUT RETURN CODE IN RCREG      */ 00765000
*   END;                            /* VMDESET SET ZERO KEY - MVM  0777 00766000
*                                                            @X50BHWJ*/ 00767000
*   IF LCPNXLCW->LCCWNTVC^='1'B THEN/* IF THE LCCW DOES NOT INDICATE    00768000
*                                      AUTHORIZED PATH, THEN @ZA09611*/ 00769000
         L     @14,LCPNXLCW(,PTRLCP)                               0778 00770000
         TM    LCCWNTVC(@14),B'00010000'                           0778 00771000
         BO    @RF00778                                            0778 00772000
*     DO;                           /* SETLOCK & VALCHECK    @ZA09611*/ 00773000
*/*VSETLCK  (15,OBTAIN,LOCAL,(IEAVGM00,ISTZBF0L(UNLOCK)))            */ 00774000
*                                                                  0780 00775000
*LOCKER:                                                           0780 00776000
*       DO;                         /* DECLARE AND RESTRICT REGS   0780 00777000
*                                      2,10,11,12,13,14, AND 15      */ 00778000
LOCKER   DS    0H                                                  0781 00779000
*         RFY                                                      0781 00780000
*          (GPR02P,                                                0781 00781000
*           GPR10P,                                                0781 00782000
*           GPR11P,                                                0781 00783000
*           GPR14P,                                                0781 00784000
*           GPR15P) RSTD;           /* SAVE REGS 2 AND 10 THRU 14    */ 00785000
*         GEN REFS(GPR02F,GPR10F,GPR11F,GPR12F,GPR14F,GPR13P);     0782 00786000
         STM   GPR10F,GPR12F,0(GPR13P)                                  00787000
         ST    GPR14F,12(GPR13P)                                        00788000
         ST    GPR02F,16(GPR13P)                                        00789000
*                                   /* INVOKE MODESET TO CHANGE PSW     00790000
*                                      PROTECTION KEY TO 0           */ 00791000
*         GEN SETS(2);                                             0783 00792000
         MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2                      00793000
*         GPR10P=GPR13P;            /* PUT SAVE AREA ADDR IN R 10  0784 00794000
*                                      INVOKE OS/VS SETLOCK MACRO    */ 00795000
         LR    GPR10P,GPR13P                                       0784 00796000
*         GEN REFS(GPR13P,PSALITA,FLC) SETS(GPR12F);               0785 00797000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        *00798000
               RELATED=(IEAVGM00,ISTZBF0L(UNLOCK))                      00799000
*         GPR15P=GPR13P;            /* PUT RETURN CODE IN R 15 INVOKE   00800000
*                                      MODESET MACRO TO              */ 00801000
         LR    GPR15P,GPR13P                                       0786 00802000
*         DO;                       /* RESTORE PSW PROTECTION KEY    */ 00803000
*           GEN SETS(2);                                           0788 00804000
         MODESET KEYADDR=(2),WORKREG=2                                  00805000
*         END;                      /* RESTORE REGISTERS             */ 00806000
*         GEN REFS(GPR10F,GPR13P) SETS(GPR10F,GPR11F,GPR12F,GPR13F,     00807000
