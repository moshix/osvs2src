*/* SEGMENT-                                                         */ 01639000
*/*      PROCSYN                                                     */ 01640000
*/*                                                                  */ 01641000
*/* FUNCTION-                                                        */ 01642000
*/*      GET AN EXTRA LSCB WHEN LCCW SYNCH IS REQUIRED.  THE LSCB    */ 01643000
*/*      IS INITIALIZED TO INDICATE IT IS A SYNCH LSCB.              */ 01644000
*/*                                                                  */ 01645000
*/********************************************************************/ 01646000
*                                                                  1045 01647000
*   /*****************************************************************/ 01648000
*   /*                                                               */ 01649000
*   /* SEGMENT(PROCSYN)START                                         */ 01650000
*   /*                                                               */ 01651000
*   /*****************************************************************/ 01652000
*                                                                  1045 01653000
*   DO;                                                            1045 01654000
*     GOTO PROCSYN@;                                               1046 01655000
         B     PROCSYN@                                            1046 01656000
*PROCSYN:                                                          1047 01657000
*   END;                            /* PROCSYN: SUBROUTINE           */ 01658000
PROCSYN  DS    0H                                                  1048 01659000
*   RFY                                                            1048 01660000
*     PTRLSC RSTD;                  /* RETURNED FROM SUBROUTINE      */ 01661000
*   RFY                                                            1049 01662000
*     ISTLSCB BASED(PTRLSC);        /* RETURNED FROM SUBROUTINE      */ 01663000
*   ZLBRT02='1'B;                   /* SET EOB                       */ 01664000
*   ZLBRT04='1'B;                   /* SET FEEDBACK REQ'D            */ 01665000
*   ZLBRT05='1'B;                   /* SET DECREMENT SCHED COUNT     */ 01666000
*   ZLBRT07='1'B;                   /* SET SCHEDULING FLAG           */ 01667000
         OI    ZLBRT02(PTRZLB),B'00101101'                         1053 01668000
*   ZLBELI='1'B;                    /* END OF LCPB                   */ 01669000
         OI    ZLBELI(PTRZLB),B'10000000'                          1054 01670000
*   CALL GETLSCB;                   /* GET LSCB & PUT IT ON WAITQ    */ 01671000
         BAL   @14,GETLSCB                                         1055 01672000
*   LSCLCCWA=0;                     /* CLEAR LCCW POINTER            */ 01673000
         SLR   @14,@14                                             1056 01674000
         ST    @14,LSCLCCWA(,PTRLSC)                               1056 01675000
*   LSCLCPBA=0;                     /* CLEAR LCPB POINTER            */ 01676000
         ST    @14,LSCLCPBA(,PTRLSC)                               1057 01677000
*   IF ZCREXRES='1'B THEN           /* IF EXCEPTION RESPONSE         */ 01678000
