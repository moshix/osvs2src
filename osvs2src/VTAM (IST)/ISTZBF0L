         BZ    @RT00707                                            0707 00501000
         TM    FMCZFD06(PTRFMC),B'10000000'                        0707 00502000
         BNO   @RF00707                                            0707 00503000
@RT00707 DS    0H                                                  0708 00504000
*       DO;                         /* | DISCONNECT PENDING          */ 00505000
*         FMCZX001=FMCZX001+1;      /* INCREMENT SESSION COUNT       */ 00506000
         LA    @14,1                                               0709 00507000
         SLR   @00,@00                                             0709 00508000
         IC    @00,FMCZX001(,PTRFMC)                               0709 00509000
         ALR   @14,@00                                             0709 00510000
         STC   @14,FMCZX001(,PTRFMC)                               0709 00511000
*         FMCZFC06='1'B;            /* FLAG CONTACT OUTSTANDING      */ 00512000
         OI    FMCZFC06(PTRFMC),B'00000001'                        0710 00513000
*         ZLBRT0A='1'B;             /* SET CONTACT IN REQUEST TAG    */ 00514000
         OI    ZLBRT0A(PTRZLB),B'00100000'                         0711 00515000
*         FMCZF002='1'B;            /* FLAG I/O SCHEDULING    SUSPEND*/ 00516000
         OI    FMCZF002(PTRFMC),B'01000000'                        0712 00517000
*       END;                        /* END SET UP CONTACT            */ 00518000
*     ELSE                                                         0714 00519000
*       DO;                         /* NOT SET UP CONTACT            */ 00520000
         B     @RC00707                                            0714 00521000
@RF00707 DS    0H                                                  0715 00522000
*         LCPZRT00='1'B;            /* FLAG AS LOGICAL ERROR         */ 00523000
         OI    LCPZRT00(PTRLCP),B'10000000'                        0715 00524000
*         LCPMNRCD=FRMCDITS;        /* CONFLICTING DIALOG SEQUENCE   */ 00525000
         MVI   LCPMNRCD(PTRLCP),X'A6'                              0716 00526000
*         FMCZF002='0'B;            /* ALLOW I/O SCHEDULING          */ 00527000
         NI    FMCZF002(PTRFMC),B'10111111'                        0717 00528000
*         ZCRBRIS='1'B;             /* SET BRACKET REJECT SWITCH     */ 00529000
         L     @14,RPHCRR(,PTRRPH)                                 0718 00530000
         L     @14,CRACRR(,@14)                                    0718 00531000
         OI    ZCRBRIS(@14),B'00000010'                            0718 00532000
*       END;                        /* END NOT CONTACT               */ 00533000
*   ELSE                            /* NOT START DIALOG              */ 00534000
*     IF(FMCZFA06='1'B              /* IF IN DIALOG                  */ 00535000
*         |FMCZFC06='1'B)           /* | CONTACT PENDING             */ 00536000
*         &FMCZFD06='0'B THEN                                      0720 00537000
         B     @RC00706                                            0720 00538000
@RF00706 TM    FMCZFA06(PTRFMC),B'00000101'                        0720 00539000
         BZ    @RF00720                                            0720 00540000
         TM    FMCZFD06(PTRFMC),B'10000000'                        0720 00541000
         BZ    @RT00720                                            0720 00542000
*       ;                           /* & DISCONNECT NOT PENDING      */ 00543000
*     ELSE                                                         0722 00544000
*       DO;                         /* OTHERWISE IS ERROR            */ 00545000
@RF00720 DS    0H                                                  0723 00546000
*         LCPZRT00='1'B;            /* FLAG AS LOGICAL ERROR         */ 00547000
         OI    LCPZRT00(PTRLCP),B'10000000'                        0723 00548000
*         LCPMNRCD=FRMCDITS;        /* CONFLICTING DIALOG SEQUENCE   */ 00549000
         MVI   LCPMNRCD(PTRLCP),X'A6'                              0724 00550000
*       END;                        /* END DIALOG ERROR              */ 00551000
*                                                                  0725 00552000
*   /*****************************************************************/ 00553000
*   /*                                                               */ 00554000
*   /* SEGMENT(SEGCON)FINISH                                         */ 00555000
*   /*                                                               */ 00556000
*   /*****************************************************************/ 00557000
*                                                                  0726 00558000
*   DO;                                                            0726 00559000
@RT00720 DS    0H                                                  0726 00560000
@RC00706 DS    0H                                                  0727 00561000
*     GOTO SEGCON#;                                                0727 00562000
         B     SEGCON#                                             0727 00563000
*SEGCON@:                                                          0728 00564000
*   END;                            /* END OF SEGMENT                */ 00565000
SEGCON@  DS    0H                                                  0729 00566000
*                                                                  0729 00567000
*/********************************************************************/ 00568000
*/*                                                                  */ 00569000
*/* SEGMENT-                                                         */ 00570000
*/*      SEG0069                                                     */ 00571000
*/*                                                                  */ 00572000
*/* FUNCTION-                                                        */ 00573000
*/*      TRANSFORM THE LCCW OP CODE.                                 */ 00574000
*/*      THE LCCW POINTS TO A DATA STREAM, THE FIRST CHARACTER OF    */ 00575000
*/*      WHICH IS A COMMAND CODE FOR A 3270.                         */ 00576000
*/*      THIS ROUTING USES THE COMMAND CODE TO DETERMINE WHAT LCCW   */ 00577000
*/*      SHOULD REALLY BE USED SO THAT THE COMMAND CODE CAN BE       */ 00578000
*/*      BUILT AGAIN IN A DIFFERENT FORMAT. NO KIDDING.              */ 00579000
*/*                                                                  */ 00580000
*/********************************************************************/ 00581000
*                                                                  0729 00582000
*   /*****************************************************************/ 00583000
*   /*                                                               */ 00584000
*   /* SEGMENT(SEG0069)START                                         */ 00585000
*   /*                                                               */ 00586000
*   /*****************************************************************/ 00587000
*                                                                  0729 00588000
*   DO;                                                            0729 00589000
*     GOTO SEG0069@;                                               0730 00590000
         B     SEG0069@                                            0730 00591000
*SEG0069:                                                          0731 00592000
*   END;                            /* BF0L0069: SUBROUTINE          */ 00593000
SEG0069  DS    0H                                                  0732 00594000
*   PTRLCC=LCPNXLCW;                /* ADDRESS LCCW                  */ 00595000
         L     @14,LCPNXLCW(,PTRLCP)                               0732 00596000
         LR    PTRLCC,@14                                          0732 00597000
*   LCPNXLCW=LCPNXLCW+LENGTH(ISTLCCW);/* INCREMENT LCCW POINTER      */ 00598000
         AL    @14,@CF00071                                        0733 00599000
         ST    @14,LCPNXLCW(,PTRLCP)                               0733 00600000
*   IF LCCWLSI='1'B THEN            /* IF 1ST LCCW HAS SYNCH         */ 00601000
         TM    LCCWLSI(PTRLCC),B'00000100'                         0734 00602000
         BNO   @RF00734                                            0734 00603000
*     LCPNXLCW->LCCWLSI='1'B;       /* NOW SO DOES SECOND            */ 00604000
         OI    LCCWLSI(@14),B'00000100'                            0735 00605000
*   ELSE                                                           0736 00606000
*     ;                             /* 1ST LCCW DIDN'T HAVE SYNCH    */ 00607000
@RF00734 DS    0H                                                  0737 00608000
*   IF LCPNXLCW->LCCWCNT=0 THEN     /* IF LCCW HAS 0 CNT     @ZA28604*/ 00609000
*                                                                  0737 00610000
         L     @14,LCPNXLCW(,PTRLCP)                               0737 00611000
         LH    @14,LCCWCNT(,@14)                                   0737 00612000
         N     @14,@CF04884                                        0737 00613000
         LTR   @14,@14                                             0737 00614000
         BNZ   @RF00737                                            0737 00615000
*     /***************************************************************/ 00616000
*     /*                                                             */ 00617000
*     /* SEGMENT(ZERORU)REQUEST                                      */ 00618000
*     /*                                                             */ 00619000
*     /***************************************************************/ 00620000
*                                                                  0738 00621000
*     DO;                                                          0738 00622000
*       GOTO ZERORU;                                               0739 00623000
         B     ZERORU                                              0739 00624000
*ZERORU#:                                                          0740 00625000
*     END;                          /* PROCESS ZERO LNGTH    @ZA28604*/ 00626000
