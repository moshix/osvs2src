         IC    PTRG15,ISTZTRTT(@14,@11)                            0695 00444000
*   ELSE                            /* IF ERROR DECTECTED            */ 00445000
*     PTRG15=0;                     /* SET EXIT RETURN CODE          */ 00446000
         B     @RC00694                                            0696 00447000
@RF00694 SLR   PTRG15,PTRG15                                       0696 00448000
*   RFY                                                            0697 00449000
*     PTRG14 RSTD;                  /* TO RESTORE RETURN ADDRESS     */ 00450000
@RC00694 DS    0H                                                  0698 00451000
*   PTRG14=ZCRSVF6;                 /* RESTORE RETURN ADDRESS        */ 00452000
