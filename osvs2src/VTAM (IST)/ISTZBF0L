         L     PTRG14,ZCRSVF6(,@11)                                0698 00455000
*   RFY                                                            0699 00456000
*     PTRG14 UNRSTD;                /* RESTORED                      */ 00457000
*   RETURN;                         /* END OF SUBROUTINE             */ 00458000
@EL00002 DS    0H                                                  0700 00459000
@EF00002 DS    0H                                                  0700 00460000
@ER00002 BR    @14                                                 0700 00461000
*                                                                  0701 00462000
*/********************************************************************/ 00463000
*/*                                                                  */ 00464000
*/* SEGMENT-                                                         */ 00465000
*/*      SEGCON                                                      */ 00466000
*/*                                                                  */ 00467000
*/* FUNCTION-                                                        */ 00468000
*/*      SETS UP CONTACT, IF REQUIRED.                               */ 00469000
*/*      VARIOUS FIELDS ARE SET UP TO SIMULATE CONTACT PROCESSING.   */ 00470000
*/*                                                                  */ 00471000
*/********************************************************************/ 00472000
*                                                                  0701 00473000
*   /*****************************************************************/ 00474000
*   /*                                                               */ 00475000
*   /* SEGMENT(SEGCON)START                                          */ 00476000
*   /*                                                               */ 00477000
*   /*****************************************************************/ 00478000
*                                                                  0701 00479000
*   DO;                                                            0701 00480000
*     GOTO SEGCON@;                                                0702 00481000
*SEGCON:                                                           0703 00482000
*   END;                            /* CONTACT: SUBROUTINE           */ 00483000
SEGCON   DS    0H                                                  0704 00484000
*/*                                                                  */ 00485000
*/*      COVER FOR ISTRH ESTABLISHED IN CHKLCCW SUBROUTINE.          */ 00486000
*/*      USAGE OF PTRRH IS NOT RELEASED UNTIL END OF SEGMENT SEGVAL. */ 00487000
*/*                                                                  */ 00488000
*                                                                  0704 00489000
*   RFY                                                            0704 00490000
*     PTRRH RSTD;                   /* REQUEST/RESPONSE HEADER       */ 00491000
*   RFY                                                            0705 00492000
*     ISTRH BASED(PTRRH);           /* ESTABLISH COVER               */ 00493000
*   IF RHBB='1'B THEN               /* IF START DIALOG               */ 00494000
         TM    RHBB(PTRRH),B'10000000'                             0706 00495000
         BNO   @RF00706                                            0706 00496000
*     IF(FMCZFA06='0'B              /* IF NOT IN DIALOG              */ 00497000
*         &FMCZFC06='0'B)           /* & CONTACT NOT PENDING         */ 00498000
*         |FMCZFD06='1'B THEN                                      0707 00499000
