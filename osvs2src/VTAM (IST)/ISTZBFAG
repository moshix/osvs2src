         B     BCAG010@                                            0524 00315000
*BCAG010:                                                          0525 00316000
*   END;                                                           0525 00317000
*                                                                  0525 00318000
BCAG010  DS    0H                                                  0526 00319000
*/*                                                                  */ 00320000
*/* THIS  ROUTINE VALIDATES THE LCCW,   SETS THE APPROPRIATE VALUE   */ 00321000
*/* IN LPCB IF AN ERROR IS FOUND,AND RETURNS                         */ 00322000
*/*                                                                  */ 00323000
*/*                                                                  */ 00324000
*                                                                  0526 00325000
*   IF LCCWCD='1'B                  /* DATA CHAIN FLAG ON ?          */ 00326000
*     THEN                          /* YES                           */ 00327000
         L     @11,LCPNXLCW(,LCPBREG)                              0526 00328000
         TM    LCCWCD(@11),B'10000000'                             0526 00329000
         BNO   @RF00526                                            0526 00330000
*     DO;                                                          0527 00331000
*       LCPZRT01='1'B;              /* SET LOGICAL ERROR FLAG        */ 00332000
         OI    LCPZRT01(LCPBREG),B'01000000'                       0528 00333000
*       LCPMNRCD=FRMINVCF;          /* SET MINOR RETURN CODE         */ 00334000
         MVI   LCPMNRCD(LCPBREG),X'81'                             0529 00335000
*     END;                                                         0530 00336000
*   ELSE                            /* NO, CONTINUE                  */ 00337000
*     IF LCCWSD='1'B                /* END DIALOG?                   */ 00338000
*       THEN                                                       0531 00339000
         B     @RC00526                                            0531 00340000
@RF00526 L     @11,LCPNXLCW(,LCPBREG)                              0531 00341000
         TM    LCCWSD(@11),B'01000000'                             0531 00342000
         BNO   @RF00531                                            0531 00343000
*       IF LCCWRWOP=LRT             /* READ TRANSMISSION ?           */ 00344000
*         THEN                                                     0532 00345000
         TM    LCCWRWOP(@11),B'00001110'                           0532 00346000
         BNO   @RF00532                                            0532 00347000
         TM    LCCWRWOP(@11),B'00110001'                           0532 00348000
         BZ    @RT00532                                            0532 00349000
*         ;                         /* YES, OK                       */ 00350000
*       ELSE                        /* NO, SET ERROR CODES           */ 00351000
*         DO;                                                      0534 00352000
@RF00532 DS    0H                                                  0535 00353000
*           LCPZRT00='1'B;          /* ERROR FLAG                    */ 00354000
         OI    LCPZRT00(LCPBREG),B'10000000'                       0535 00355000
*           LCPMNRCD=FRMCDITS;      /* SET MINOR RC                  */ 00356000
         MVI   LCPMNRCD(LCPBREG),X'A6'                             0536 00357000
*         END;                                                     0537 00358000
*     ELSE                          /* NO, END DIALOG NOT ON         */ 00359000
*       IF LCCWRWOP=LRCONT          /* READ CONTINUOUS ?             */ 00360000
*         THEN                      /* YES, CONTINUE                 */ 00361000
         B     @RC00531                                            0538 00362000
@RF00531 L     @11,LCPNXLCW(,LCPBREG)                              0538 00363000
         TM    LCCWRWOP(@11),B'00011010'                           0538 00364000
         BNO   @RF00538                                            0538 00365000
         TM    LCCWRWOP(@11),B'00100101'                           0538 00366000
         BNZ   @RF00538                                            0538 00367000
*         IF LCCWCC='1'B            /* COMMAND CHAINED?              */ 00368000
*           THEN                    /* YES, SET ERROR CODES          */ 00369000
         TM    LCCWCC(@11),B'01000000'                             0539 00370000
         BNO   @RF00539                                            0539 00371000
*           DO;                                                    0540 00372000
*             LCPZRT01='1'B;        /* LOGICAL ERROR                 */ 00373000
         OI    LCPZRT01(LCPBREG),B'01000000'                       0541 00374000
*             LCPMNRCD=FRMINVCF;    /* MINOR RC                      */ 00375000
         MVI   LCPMNRCD(LCPBREG),X'81'                             0542 00376000
*           END;                                                   0543 00377000
*         ELSE                      /* NOT COMMAND CHAINED           */ 00378000
*           IF PROSETM4=PROSBLCK    /* BLOCK MODE?           @XM03HDP*/ 00379000
*             THEN                  /* YES, SET ERROR CODES          */ 00380000
         B     @RC00539                                            0544 00381000
@RF00539 CLI   PROSETM4+48(FMCBREG),X'01'                          0544 00382000
         BNE   @RF00544                                            0544 00383000
*             DO;                                                  0545 00384000
*               LCPZRT00='1'B;                                     0546 00385000
         OI    LCPZRT00(LCPBREG),B'10000000'                       0546 00386000
*               LCPMNRCD=FRMINVL;                                  0547 00387000
         MVI   LCPMNRCD(LCPBREG),X'A3'                             0547 00388000
*             END;                                                 0548 00389000
*           ELSE                                                   0549 00390000
*             ;                                                    0549 00391000
@RF00544 DS    0H                                                  0550 00392000
*       ELSE                                                       0550 00393000
*         ;                                                        0550 00394000
*                                                                  0550 00395000
@RF00538 DS    0H                                                  0551 00396000
*/* SEGMENT(BCAG010)FINISH                                           */ 00397000
*                                                                  0551 00398000
*   DO;                                                            0551 00399000
@RC00538 DS    0H                                                  0551 00400000
@RC00531 DS    0H                                                  0551 00401000
@RC00526 DS    0H                                                  0552 00402000
*     GOTO BCAG010#;                                               0552 00403000
         B     BCAG010#                                            0552 00404000
*BCAG010@:                                                         0553 00405000
*   END;                                                           0553 00406000
BCAG010@ DS    0H                                                  0554 00407000
*   GEN(EJECT);                                                    0554 00408000
         EJECT                                                          00409000
*                                                                  0555 00410000
*/*  SEGMENT(BCAG020)START                                           */ 00411000
*                                                                  0555 00412000
*   DO;                                                            0555 00413000
*     GOTO BCAG020@;                                               0556 00414000
         B     BCAG020@                                            0556 00415000
*BCAG020:                                                          0557 00416000
*   END;                                                           0557 00417000
*                                                                  0557 00418000
BCAG020  DS    0H                                                  0558 00419000
*/*                                                                  */ 00420000
*/* THIS MODULE SETS THE  APPROPRIATE COMMAND CODE IN THE BDU.       */ 00421000
*/*                                                                  */ 00422000
*                                                                  0558 00423000
*   BDUREG=ADDR(BTUBDU);            /* ADDRESS BDU                   */ 00424000
         LA    BDUREG,BTUBDU(,BTUREG)                              0558 00425000
