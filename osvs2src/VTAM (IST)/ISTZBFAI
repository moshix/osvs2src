@PB00002 DS    0H                                                  0526 00435000
*   GEN NODEFS NOREFS NOSETS;                                      0526 00436000
*                                                                  0526 00437000
 TITLE 'ISTZBFAI - READ PROCESSOR FOR GENERAL POLL DEVICE - PROCBM'     00438000
*/********************************************************************/ 00439000
*/*                                                                  */ 00440000
*/* PROCBM: THIS ROUTINE COMPLETES PROCESSING OF READ MODIFIED AND   */ 00441000
*/*         READ BUFFER COMMANDS.                                    */ 00442000
*/*                                                                  */ 00443000
*/********************************************************************/ 00444000
*                                                                  0527 00445000
*   /*****************************************************************/ 00446000
*   /*                                                               */ 00447000
*   /* SEGMENT(PROCBM)START                                          */ 00448000
*   /*                                                               */ 00449000
*   /*****************************************************************/ 00450000
*                                                                  0527 00451000
*   DO;                                                            0527 00452000
*     GOTO PROCBM@;                                                0528 00453000
         B     PROCBM@                                             0528 00454000
*PROCBM:                                                           0529 00455000
*   END;                                                           0529 00456000
PROCBM   DS    0H                                                  0530 00457000
*   IF DEVTCODE=DEV3275|DEVTCODE=DEV3277|DEVTCODE=DEV3284|DEVTCODE=     00458000
*       DEV3286 THEN                /* IS DEVICE A 3270?             */ 00459000
         LA    @14,36                                              0530 00460000
         ALR   @14,DNCBREG                                         0530 00461000
         CLI   DEVTCODE(@14),X'1C'                                 0530 00462000
         BE    @RT00530                                            0530 00463000
         CLI   DEVTCODE(@14),X'19'                                 0530 00464000
         BE    @RT00530                                            0530 00465000
         CLI   DEVTCODE(@14),X'1A'                                 0530 00466000
         BE    @RT00530                                            0530 00467000
         CLI   DEVTCODE(@14),X'1B'                                 0530 00468000
         BNE   @RF00530                                            0530 00469000
@RT00530 DS    0H                                                  0531 00470000
*FAIDO100:                                                         0531 00471000
*     DO;                           /* YES,                          */ 00472000
FAIDO100 DS    0H                                                  0532 00473000
*       FDBDATAA=ADDR(ZOBBTU);      /* SET BUFFER DATA ADDRESS AS BTU   00474000
*                                      ADDRESS                       */ 00475000
         LA    @14,ZOBBTU(,ZOBPTR)                                 0532 00476000
         ST    @14,FDBDATAA(,ZOBPTR)                               0532 00477000
*       BTUDCF=12;                  /* INITIALIZE COUNT      @XL03HAH*/ 00478000
         LA    @14,12                                              0533 00479000
         STH   @14,BTUDCF+28(,ZOBPTR)                              0533 00480000
*       FDBDATCT=BTUDCF+LENGTH(ISTTH);/* SET BFR DATA CNT    @XM08930*/ 00481000
         AL    @14,@CF02602                                        0534 00482000
         STH   @14,FDBDATCT(,ZOBPTR)                               0534 00483000
*       BDUCOMND=BCMDW;             /* BDU COMMAND IS WRITE          */ 00484000
         MVI   BDUCOMND+42(ZOBPTR),X'02'                           0535 00485000
*FAIIF010:                                                         0536 00486000
*       IF(FMCZFA06=ON&FMCZFD06=ON)|(FMCZFA06=OFF&FMCZFB06=OFF&FMCZFC06 00487000
*           =OFF) THEN              /* IS EITHER (DEVICE IN SESSION     00488000
*                                      AND DISCONNECT OUTSTANDING) OR   00489000
*                                      (DEVICE NOT IN SESSION, INVITE   00490000
*                                      NOT OUTSTANDING, AND CONTACT     00491000
*                                      NOT OUTSTANDING)?             */ 00492000
FAIIF010 TM    FMCZFA06(FMCBREG),B'00000100'                       0536 00493000
         BNO   @GL00006                                            0536 00494000
         TM    FMCZFD06(FMCBREG),B'10000000'                       0536 00495000
         BO    @RT00536                                            0536 00496000
@GL00006 TM    FMCZFA06(FMCBREG),B'00000111'                       0536 00497000
         BNZ   @RF00536                                            0536 00498000
@RT00536 DS    0H                                                  0537 00499000
*FAIDO110:                                                         0537 00500000
*         DO;                       /* YES,                          */ 00501000
FAIDO110 DS    0H                                                  0538 00502000
*           BDUMODIF=BMODWCT;       /* SET BDU MODIFIER TO WRITE   0538 00503000
*                                      TRANSMISSION WITH CONTACT     */ 00504000
         LA    @14,42                                              0538 00505000
         ALR   @14,ZOBPTR                                          0538 00506000
         MVI   BDUMODIF(@14),X'0A'                                 0538 00507000
*           BDURTOE=ON;             /* SET BIT TO RETURN TEXT ON   0539 00508000
*                                      ERROR                 @X50BHXF*/ 00509000
         OI    BDURTOE(@14),B'00000010'                            0539 00510000
*           FMCZFC06=ON;            /* SET ON CONTACT OUTSTANDING    */ 00511000
         OI    FMCZFC06(FMCBREG),B'00000001'                       0540 00512000
*           FMCZX001=FMCZX001+1;    /* INCREMENT SESSION COUNT       */ 00513000
         LA    @14,1                                               0541 00514000
         SLR   @00,@00                                             0541 00515000
         IC    @00,FMCZX001(,FMCBREG)                              0541 00516000
         ALR   @14,@00                                             0541 00517000
         STC   @14,FMCZX001(,FMCBREG)                              0541 00518000
*           FMCZF002=ON;            /* SET I/O SCHED SUSP    @XM09540*/ 00519000
         OI    FMCZF002(FMCBREG),B'01000000'                       0542 00520000
*         END FAIDO110;                                            0543 00521000
*       ELSE                        /* NO,(FOR FAIIF010)             */ 00522000
*         BDUMODIF=BMODWT;          /* SET MODIFIER AS WRITE       0544 00523000
*                                      TRANSMISSION                  */ 00524000
         B     @RC00536                                            0544 00525000
@RF00536 MVI   BDUMODIF+42(ZOBPTR),X'02'                           0544 00526000
*       IF LCCWSD=ON THEN           /* IS END DIALOG SWITCH ON?      */ 00527000
@RC00536 L     @14,LSCLCCWA(,LSCBPTR)                              0545 00528000
         TM    LCCWSD(@14),B'01000000'                             0545 00529000
         BNO   @RF00545                                            0545 00530000
*FAIDO120:                                                         0546 00531000
*         DO;                                                      0546 00532000
FAIDO120 DS    0H                                                  0547 00533000
*           BDUMODIF=BDUMODIF|BMODWD;/* SET ON DISCONNECT IN MODIFIER*/ 00534000
         OI    BDUMODIF+42(ZOBPTR),X'03'                           0547 00535000
*           FMCZFD06=ON;            /* SET ON DISCONN OUTSTANDING    */ 00536000
         OI    FMCZFD06(FMCBREG),B'10000000'                       0548 00537000
*           DNCDISC=ON;             /* SET ON DISCONN OUTSTANDING    */ 00538000
         OI    DNCDISC(DNCBREG),B'00001000'                        0549 00539000
*         END FAIDO120;                                            0550 00540000
*       ELSE                        /* NO,(NOT END DIALOG)           */ 00541000
*FAIDO130:                                                         0551 00542000
*         DO;                                                      0551 00543000
         B     @RC00545                                            0551 00544000
@RF00545 DS    0H                                                  0551 00545000
FAIDO130 DS    0H                                                  0552 00546000
*           FMCZFD06=OFF;           /* SET OFF DISCONN OUTSTANDING   */ 00547000
         NI    FMCZFD06(FMCBREG),B'01111111'                       0552 00548000
*           DNCDISC=OFF;            /* SET OFF DISCONN OUTSTANDING   */ 00549000
         NI    DNCDISC(DNCBREG),B'11110111'                        0553 00550000
*         END FAIDO130;                                            0554 00551000
*       BTUFID=THSET;               /* INIT TH BYTES 1&2     @XL03HAH*/ 00552000
