*       BTUDEST=NCBDAF;             /* SET DEST ADDR FIELD IN BTU    */ 00561000
         MVC   BTUDEST(2,@14),NCBDAF(DNCBREG)                      0558 00562000
*/* SET BTU FLAGS AND REQUEST TAG SEQUENCE, ADJUST FMCB REQTAG SEQ   */ 00563000
*                                                                  0559 00564000
*       IF FMCZF007=ON THEN         /* BUFFER TRACE ACTIVE FLAG      */ 00565000
         TM    FMCZF007(FMCBREG),B'00000100'                       0559 00566000
         BNO   @RF00559                                            0559 00567000
*         BTUZRT07=ON;                                             0560 00568000
         OI    BTUZRT07(@14),B'00000001'                           0560 00569000
