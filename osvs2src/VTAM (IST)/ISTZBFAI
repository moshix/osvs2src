 TITLE 'ISTZBFAI - READ PROCESSOR FOR GENERAL POLL DEVICE - PROCMT'     00333000
*/********************************************************************/ 00334000
*/*                                                                  */ 00335000
*/* PROCMT: THIS ROUTINE COMPLETES PROCESSING OF READ MESSAGE AND    */ 00336000
*/*         READ TRANSMISSION COMMANDS.                              */ 00337000
*/*                                                                  */ 00338000
*/*         NOTE: THE RETURN ADDRESS IS SAVED IN WORKREG2            */ 00339000
*/*                                                                  */ 00340000
*/********************************************************************/ 00341000
*                                                                  0492 00342000
*PROCMT:                                                           0492 00343000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0492 00344000
         B     @PB00002                                            0492 00345000
PROCMT   DS    0H                                                  0493 00346000
*   RFY                                                            0493 00347000
*    (WORKREG2,                                                    0493 00348000
*     LNKREG) RSTD;                                                0493 00349000
*   WORKREG2=LNKREG;                /* SAVE RETURN ADDRESS           */ 00350000
         LR    WORKREG2,LNKREG                                     0494 00351000
*   RFY                                                            0495 00352000
*     LNKREG UNRSTD;                                               0495 00353000
*   IF FMCZFA06=ON THEN             /* IS DEVICE IN SESSION?         */ 00354000
         TM    FMCZFA06(FMCBREG),B'00000100'                       0496 00355000
         BO    @RT00496                                            0496 00356000
*     ;                             /* YES, (NULL)                   */ 00357000
*   ELSE                            /* NO,                           */ 00358000
*FAIDO070:                                                         0498 00359000
*     DO;                                                          0498 00360000
FAIDO070 DS    0H                                                  0499 00361000
*       FMCZFB06=ON;                /* SET ON INVITE OUTSTANDING FLAG*/ 00362000
         OI    FMCZFB06(FMCBREG),B'00000010'                       0499 00363000
*       FMCZX001=FMCZX001+1;        /* INCREMENT APPLICATION SESSION    00364000
*                                      COUNT                         */ 00365000
         LA    @08,1                                               0500 00366000
         SLR   @00,@00                                             0500 00367000
         IC    @00,FMCZX001(,FMCBREG)                              0500 00368000
         ALR   @08,@00                                             0500 00369000
         STC   @08,FMCZX001(,FMCBREG)                              0500 00370000
*     END FAIDO070;                                                0501 00371000
*   RFY                                                            0502 00372000
*     WORKREG RSTD;                                                0502 00373000
@RT00496 DS    0H                                                  0503 00374000
*   WORKREG=LSCBPTR;                /* SAVE WAITQ ELEMENT ADDRESS    */ 00375000
         LR    WORKREG,LSCBPTR                                     0503 00376000
*   RLBFPTR=ZOBPTR;                 /* GET CURRENT BUFFER ADDRESS    */ 00377000
         LR    RLBFPTR,ZOBPTR                                      0504 00378000
*   ZCRPCOD1='00'X;                 /* CLEAR UTILITY PARAM FLAGS     */ 00379000
         L     @15,RPHCRR(,RPHREG)                                 0505 00380000
         L     @15,CRACRR(,@15)                                    0505 00381000
         MVI   ZCRPCOD1(@15),X'00'                                 0505 00382000
*   ZCRPBUF=ON;                     /* SET ON FLAG FOR RELEASE BUFFER   00383000
*                                      REQUEST                       */ 00384000
         OI    ZCRPBUF(@15),B'00000100'                            0506 00385000
*   CALL ZPSZDFB0;                  /* CALL BUFFER DEALLOCATION      */ 00386000
         L     @14,ATCZPSVT(,VTCVTREG)                             0507 00387000
         L     @15,ZPSZDFB0(,@14)                                  0507 00388000
         BALR  @14,@15                                             0507 00389000
*   RFY                                                            0508 00390000
*     RCDREG RSTD;                                                 0508 00391000
*   LSCBPTR=WORKREG;                /* RESTORE WAITQ ELEM ADDRESS    */ 00392000
         LR    LSCBPTR,WORKREG                                     0509 00393000
*   RFY                                                            0510 00394000
*     WORKREG UNRSTD;                                              0510 00395000
*   IF RCDREG=GOODRC THEN           /* WAS BUFFER DEALLOCATION     0511 00396000
*                                      SUCCESSFUL?                   */ 00397000
         C     RCDREG,@CF00583                                     0511 00398000
         BNE   @RF00511                                            0511 00399000
*FAIDO080:                                                         0512 00400000
*     DO;                           /* YES,                          */ 00401000
FAIDO080 DS    0H                                                  0513 00402000
*       LSCFDBA=EMPTY;              /* NULLIFY LSCB BUFFER PTR       */ 00403000
         SLR   @14,@14                                             0513 00404000
         ST    @14,LSCFDBA(,LSCBPTR)                               0513 00405000
*       LSCREQTG=ZERO;              /* SET GENERAL POLL REQ TAG      */ 00406000
         STH   @14,LSCREQTG(,LSCBPTR)                              0514 00407000
*       FMCZF004=ON;                /* SET ON READ OUTSTANDING FL    */ 00408000
*       FMCZF001=ON;                /* SET ON BTU ALLOC REQD FLAG    */ 00409000
         OI    FMCZF004(FMCBREG),B'10010000'                       0516 00410000
*     END FAIDO080;                                                0517 00411000
*   ELSE                            /* NO, (NOT SUCCESSUL RC)        */ 00412000
*FAIDO090:                                                         0518 00413000
*     DO;                                                          0518 00414000
         B     @RC00511                                            0518 00415000
@RF00511 DS    0H                                                  0518 00416000
FAIDO090 DS    0H                                                  0519 00417000
*       LSCZRT00=ON;                /* SET ON LOGICAL ERROR FLAG     */ 00418000
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0519 00419000
*       LSCZMNRC=FRMVTAME;          /* SET MINOR RC AS VTAM ERROR    */ 00420000
         MVI   LSCZMNRC(LSCBPTR),X'A0'                             0520 00421000
*     END FAIDO090;                                                0521 00422000
*   RFY                                                            0522 00423000
