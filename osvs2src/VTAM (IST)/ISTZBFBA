         L     @14,CRACRR(,@14)                                    0683 00448000
         TM    ZCRTIC(@14),B'10000000'                             0683 00449000
         BNZ   @RF00683                                            0683 00450000
*         DO;                       /* FIRST CCW IN CHANNEL PGM      */ 00451000
*           ZLBCPAD2=ZLBCPAD1;      /* MOVE ADDRESS TO 2ND CCW       */ 00452000
         MVC   ZLBCPAD2(3,PTRZLB),ZLBCPAD1(PTRZLB)                 0685 00453000
*           ZLBCPTC2=FDBDATCT;      /* MOVE DATA COUNT TO 2ND CCW    */ 00454000
         LH    @14,FDBDATCT(,PTRZLB)                               0686 00455000
         STH   @14,ZLBCPTC2(,PTRZLB)                               0686 00456000
*           ZLBCCW1=ZLBSEL;         /* SET 1ST CCW TO SELECT         */ 00457000
