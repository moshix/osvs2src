*           PTRCCW=PTRCCW+LENGTH(ISTCCW);/* ADDRESS NEXT CCW         */ 00459000
         AL    PTRCCW,@CF00072                                     0688 00460000
*         END;                      /* END FIRST CCW IN CHAN PGM     */ 00461000
*       ELSE                        /* NOT FIRST CCW IN CHAN PGM     */ 00462000
*         CCWCOUNT=FDBDATCT;        /* SET UP DATA COUNT             */ 00463000
         B     @RC00683                                            0690 00464000
@RF00683 LH    @14,FDBDATCT(,PTRZLB)                               0690 00465000
         STH   @14,CCWCOUNT(,PTRCCW)                               0690 00466000
*       CCWCODE=WRITE;              /* SET CCW OPERATION CODE        */ 00467000
@RC00683 MVI   CCWCODE(PTRCCW),X'01'                               0691 00468000
*     END;                          /* END NON ERASE WRITE           */ 00469000
*   IF LCCWMODE=LMS THEN            /* IF WRITE CONVERSATIONAL       */ 00470000
