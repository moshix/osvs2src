*/********************************************************************/ 00396000
*/*                                                                  */ 00397000
*/* SUBROUTINE-                                                      */ 00398000
*/*      FORMCCW                                                     */ 00399000
*/*                                                                  */ 00400000
*/* FUNCTION-                                                        */ 00401000
*/*      FORMAT A WRITE CCW LIST FOR LOCAL BUFFERS.                  */ 00402000
*/*                                                                  */ 00403000
*/********************************************************************/ 00404000
*                                                                  0668 00405000
*FORMCCW:                                                          0668 00406000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0668 00407000
         B     @PB00002                                            0668 00408000
FORMCCW  DS    0H                                                  0669 00409000
*   RFY                                                            0669 00410000
*     PTRG14 RSTD;                  /* TO SAVE RETURN REG            */ 00411000
*   ZCRSVWK1=PTRG14;                /* SAVE RETURN REGISTER          */ 00412000
         L     @09,RPHCRR(,PTRRPH)                                 0670 00413000
         L     @09,CRACRR(,@09)                                    0670 00414000
