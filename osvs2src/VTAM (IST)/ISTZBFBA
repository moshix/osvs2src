*       CCWCOUNT=FDBDATCT;          /* SET CCW DATA COUNT            */ 00438000
         LH    @14,FDBDATCT(,PTRZLB)                               0680 00439000
         STH   @14,CCWCOUNT(,PTRCCW)                               0680 00440000
*     END;                          /* END ERASE WRITE               */ 00441000
*   ELSE                            /* NOT ERASE WRITE               */ 00442000
*     DO;                           /* PROCESS NON ERASE WRITE       */ 00443000
         B     @RC00676                                            0682 00444000
@RF00676 DS    0H                                                  0683 00445000
*       IF ZCRTIC='0'B THEN         /* IF 1ST CCW IN CHAN PGM        */ 00446000
