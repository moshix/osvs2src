         TM    LCCWMODE(@14),B'00001100'                           0693 00472000
         BNZ   @RF00693                                            0693 00473000
*     DO;                           /* SET WRITE CONV. FLAGS         */ 00474000
*       ZLBRT02='1'B;               /* INDICATE EOB                  */ 00475000
*       ZLBRT03='1'B;               /* INDICATE WRITE/READ           */ 00476000
*       ZLBRT05='1'B;               /* INDICATE DECREMENT SCHED      */ 00477000
