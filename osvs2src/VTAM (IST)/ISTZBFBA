*   ZLBCPAD1=PTRG15;                /* SET REAL ADDRESS IN CCW       */ 00425000
         STCM  PTRG15,7,ZLBCPAD1(PTRZLB)                           0675 00426000
*   IF LCCWSOP='1'B THEN            /* IF ERASE WRITE                */ 00427000
         L     @14,LCPNXLCW(,PTRLCP)                               0676 00428000
         TM    LCCWSOP(@14),B'00010000'                            0676 00429000
         BNO   @RF00676                                            0676 00430000
*     DO;                           /* PROCESS ERASE WRITE           */ 00431000
*       LDNZARM=LDNZRM;             /* SET MAX DATA FOR ATTN READ    */ 00432000
