         BNE   @RF00700                                            0700 00488000
@RT00700 DS    0H                                                  0701 00489000
*         DO;                       /* PROCESS 3270 PRINTER DEVICE   */ 00490000
*           LCPZRT00='1'B;          /* FLAG LOGICAL ERROR            */ 00491000
         OI    LCPZRT00(PTRLCP),B'10000000'                        0702 00492000
*           LCPMNRCD=FRMUSELE;      /* FLAG USER ERROR               */ 00493000
         MVI   LCPMNRCD(PTRLCP),X'A3'                              0703 00494000
*         END;                      /* END 3270 PRINTER              */ 00495000
*       ELSE                                                       0705 00496000
*         ;                         /* NOT 3270 PRINTER              */ 00497000
@RF00700 DS    0H                                                  0706 00498000
*     END;                          /* END WRITE CONVERSATIONAL      */ 00499000
*   ELSE                            /* NOT WRITE CONVERSATIONAL      */ 00500000
