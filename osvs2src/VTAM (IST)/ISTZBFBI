         CS    GPR02P,@15,PABWEQA(GPR14P)                          0673 00969000
*             BC(7,CMZA0027);                                      0674 00970000
         BC    7,CMZA0027                                          0674 00971000
*             GEN REFS(RPHWORK,GPR01P) NOSETS                      0675 00972000
*                 (LM    GPR02P,GPR08P,RPHWORK(GPR01P));           0675 00973000
         LM    GPR02P,GPR08P,RPHWORK(GPR01P)                            00974000
*             RFY                                                  0676 00975000
*              (GPR00P,                                            0676 00976000
*               GPR01P,                                            0676 00977000
*               GPR02P,                                            0676 00978000
*               GPR14P,                                            0676 00979000
*               GPR15P) UNRSTD;                                    0676 00980000
*           END;                    /* *** END OF TPQUE GENERATION 0677 00981000
*                                      QUEUE LSCB TO SCHEDQ WITH NO     00982000
*                                      SCHEDULING                    */ 00983000
*           CALL ADDR(FMCPAB3)->PABDVTA->ZLBZBFIB;/* GOTO DISCONN  0678 00984000
*                                      LCCW PROCESSOR                */ 00985000
         L     @14,PABDVTA+95(,FMCBREG)                            0678 00986000
