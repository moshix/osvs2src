*           LSCBPTR=WORKREG;        /* RESTORE               @CURRENT   00989000
*                                      SCHED BLK                     */ 00990000
         LR    LSCBPTR,WORKREG                                     0679 00991000
*           ZOBPTR=LSCFDBA;         /* RESTORE                @BUFFER*/ 00992000
         L     ZOBPTR,LSCFDBA(,LSCBPTR)                            0680 00993000
*         END;                                                     0681 00994000
*       ELSE                        /* LSCB/BUFF ALLOCATN ERROR      */ 00995000
*         DO;                                                      0682 00996000
         B     @RC00654                                            0682 00997000
@RF00654 DS    0H                                                  0683 00998000
*           LSCZRT00=ON;            /* SET LOGICAL ERR FLAG          */ 00999000
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0683 01000000
*           LSCZMNRC=FRMVTAME;      /* MINOR RC= VTAM ERROR          */ 01001000
         MVI   LSCZMNRC(LSCBPTR),X'A0'                             0684 01002000
