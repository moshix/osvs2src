         B     BTUPTR@                                             0637 00855000
*BTUPTR:                                                           0638 00856000
*   END;                                                           0638 00857000
*                                                                  0638 00858000
BTUPTR   DS    0H                                                  0639 00859000
*/********************************************************************/ 00860000
*/* BTUPTR: THIS ROUTINE SETS THE WRITE/READ FLAG IN THE BDU MODIFIER*/ 00861000
*/*         AND THE RESPONSE SUPPRESSION FLAG IN THE BDU.            */ 00862000
*/*         IF THE'DIALOGUE TO BE ENDED'FLAG IS SET, IT CALLS        */ 00863000
*/*         ISTZDFA0 FOR ALLOCATION OF AN LSCB WITH BUFFER, QUEUES   */ 00864000
*/*         THE LSCB TO THE SCHED Q AND CALLS ISTZBFIB- DISCONNECT   */ 00865000
*/*         LCCW PROCESSOR.                                          */ 00866000
*/*         THE CHANGE ACTIVITY FOR THIS ROUTINE IS          @XL03HCH*/ 00867000
*/********************************************************************/ 00868000
*                                                                  0639 00869000
*   ZCRWRPTR=ON;                    /* ON WRITE TO PRINTER     SWITCH*/ 00870000
         L     @14,RPHCRR(,RPHREG)                                 0639 00871000
         L     @14,CRACRR(,@14)                                    0639 00872000
         OI    ZCRWRPTR(@14),B'00000010'                           0639 00873000
*   BDUMODIF=BDUMODIF&('FF'X-BMODWD);/* OFF DISCONNECT AND WR MSG  0640 00874000
*                                      FLGS -INVALID WITH WRITE/READ */ 00875000
         LA    @14,42                                              0640 00876000
         ALR   @14,ZOBPTR                                          0640 00877000
         SLR   @00,@00                                             0640 00878000
         IC    @00,BDUMODIF(,@14)                                  0640 00879000
         N     @00,@CF03082                                        0640 00880000
         STC   @00,BDUMODIF(,@14)                                  0640 00881000
*   BDUMODIF=BDUMODIF|BMODWR;       /* TURN ON WRITE-READ FLAG       */ 00882000
         OI    BDUMODIF(@14),X'06'                                 0641 00883000
*   BDUSTPR=ON;                     /* SET RESPONSE SUPPRESSN IN BDU */ 00884000
         OI    BDUSTPR(@14),B'00000100'                            0642 00885000
*   IF LCCWSD=ON THEN               /* DIALOG TO BE ENDED?           */ 00886000
         TM    LCCWSD(LCCWPTR),B'01000000'                         0643 00887000
         BNO   @RF00643                                            0643 00888000
*BFBIDIEN:                                                         0644 00889000
*     DO;                           /* YES....                       */ 00890000
BFBIDIEN DS    0H                                                  0645 00891000
*       FMCZF002=ON;                /* SET I/O SCHED TO BE     SUSPND*/ 00892000
         OI    FMCZF002(FMCBREG),B'01000000'                       0645 00893000
*       FMCZF021=ON;                /* SET DISCONN IN PROCESS        */ 00894000
         OI    FMCZF021(FMCBREG),B'00010000'                       0646 00895000
*       FMCZFD06=OFF;               /* CLEAR DISCONNECT OUTSTANDING     00896000
*                                      IN FMCB               @XA07962*/ 00897000
         NI    FMCZFD06(FMCBREG),B'01111111'                       0647 00898000
*       DNCDISC=OFF;                /* AND IN DNCB           @XA07962*/ 00899000
         NI    DNCDISC(DNCBREG),B'11110111'                        0648 00900000
*       LSCZRIMM=ON;                /* SET ON RESET IMMED REQ ON   0649 00901000
*                                      ERROR                 @XA07962*/ 00902000
         OI    LSCZRIMM(LSCBPTR),B'00010000'                       0649 00903000
*       ZCRPCOD1=0;                 /* ZERO PARM FLAG                */ 00904000
         L     @14,RPHCRR(,RPHREG)                                 0650 00905000
         L     @14,CRACRR(,@14)                                    0650 00906000
         MVI   ZCRPCOD1(@14),X'00'                                 0650 00907000
*       ZCRPLSBF=ON;                /* PARM =GET LSCB&BUFF FOR     0651 00908000
*                                      DISCONNECT                    */ 00909000
         OI    ZCRPLSBF(@14),B'00010000'                           0651 00910000
*       CALL ATCZPSVT->ZPSZDFA0;    /* GO GET LSCB/BUFF              */ 00911000
         L     @14,ATCZPSVT(,VTCVTREG)                             0652 00912000
         L     @15,ZPSZDFA0(,@14)                                  0652 00913000
         BALR  @14,@15                                             0652 00914000
*       RFY                                                        0653 00915000
*         RCDREG RSTD;                                             0653 00916000
*       IF RCDREG=0 THEN            /* LSCB OBTAINED ?               */ 00917000
         SLR   @14,@14                                             0654 00918000
         CR    RCDREG,@14                                          0654 00919000
         BNE   @RF00654                                            0654 00920000
*         DO;                       /* YES....                       */ 00921000
*           RFY                                                    0656 00922000
*             RCDREG UNRSTD;                                       0656 00923000
*           RFY                                                    0657 00924000
*             WORKREG RSTD;                                        0657 00925000
*           WORKREG=LSCBPTR;        /* SAVE @ CURRENT SCHED BLK      */ 00926000
         LR    WORKREG,LSCBPTR                                     0658 00927000
*           LSCBPTR=FBISRPRM;       /* INIT NEW LSCB PTR     @XL03HEK*/ 00928000
         L     LSCBPTR,FBISRPRM(,RPHREG)                           0659 00929000
*           LSCLCPBA=ZERO;          /* INIT LCPB TO ZERO     @XA07962*/ 00930000
         ST    @14,LSCLCPBA(,LSCBPTR)                              0660 00931000
*           LSCLCCWA=ZERO;          /* SET LCCW IN DISC TO ZERO    0661 00932000
*                                                            @XA07962*/ 00933000
         ST    @14,LSCLCCWA(,LSCBPTR)                              0661 00934000
*           ZOBPTR=LSCFDBA;         /* INIT ZOB REG                  */ 00935000
         L     ZOBPTR,LSCFDBA(,LSCBPTR)                            0662 00936000
*/*  TPQUE (LSCBPTR,FMCSHEDQ)                                        */ 00937000
*                                                                  0663 00938000
*           DO;                                                    0663 00939000
*             RFY                                                  0664 00940000
*              (GPR00P,                                            0664 00941000
*               GPR01P,                                            0664 00942000
*               GPR02P,                                            0664 00943000
*               GPR14P,                                            0664 00944000
*               GPR15P) RSTD;                                      0664 00945000
*             GEN REFS(GPR01P) SETS(RPHWORK)                       0665 00946000
*                 (STM GPR02P,GPR08P,RPHWORK(GPR01P));             0665 00947000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00948000
*             GPR15P=LSCBPTR;       /* GET WORK ELEM POINTER         */ 00949000
         LR    GPR15P,LSCBPTR                                      0666 00950000
*             GPR14P=ADDR(FMCSHEDQ);/* ADDRESS PAB/QUEUE POINTER     */ 00951000
         LA    GPR14P,FMCSHEDQ(,FMCBREG)                           0667 00952000
*             GPR15P=GPR15P|PABWEQG1;                              0668 00953000
         O     GPR15P,@CF00202                                     0668 00954000
*             GPR02P=GPR14P->PABWEQA;/* INITIALIZE WITH QUEUE HEADER */ 00955000
         L     GPR02P,PABWEQA(,GPR14P)                             0669 00956000
*CMZA0027:                                                         0670 00957000
*             ;                     /* CS FAIL TARGET                */ 00958000
CMZA0027 DS    0H                                                  0671 00959000
*             GPR00P=GPR02P;        /* SAVE QUEUE CONTENTS           */ 00960000
         LR    GPR00P,GPR02P                                       0671 00961000
