         STC   @14,FMCZX003(,FMCBREG)                              0617 00796000
*           ELSE                                                   0618 00797000
*             DO;                                                  0618 00798000
         B     @RC00616                                            0618 00799000
@RF00616 DS    0H                                                  0619 00800000
*               FMCZX004=FMCZX004+1;/* INCRMT RESP SUPPRS CNT        */ 00801000
         LA    @14,1                                               0619 00802000
         SLR   @00,@00                                             0619 00803000
         IC    @00,FMCZX004(,FMCBREG)                              0619 00804000
         ALR   @14,@00                                             0619 00805000
         STC   @14,FMCZX004(,FMCBREG)                              0619 00806000
*               IF FMCZX004<DNCLICP THEN/* IF SUPPR CNT < SCHEDULE 0620 00807000
*                                      LIMIT                         */ 00808000
         CLC   FMCZX004(1,FMCBREG),DNCLICP(DNCBREG)                0620 00809000
