*             DO;                                                  0588 00791000
*               BTUZRT04=ON;        /* SET SUPPRESION IN BTU         */ 00792000
         OI    BTUZRT04+28(ZOBPTR),B'00001000'                     0589 00793000
*               BDUSTPR=ON;         /* ON RESPONSE SUPPRES IN BDU    */ 00794000
         OI    BDUSTPR+42(ZOBPTR),B'00000100'                      0590 00795000
*               LSCZRT04=ON;        /* SET SUPPRESSION IN SCHED Q  0591 00796000
*                                      ELEMENT                       */ 00797000
         OI    LSCZRT04(LSCBPTR),B'00001000'                       0591 00798000
*             END;                                                 0592 00799000
