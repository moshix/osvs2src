*               GPR08F FIXED(31) REG(08);                          0632 00936000
*             DCL                                                  0633 00937000
*               GPR08P PTR(31) REG(08);                            0633 00938000
*             GEN REFS(GPR01P) SETS(RPHWORK)                       0634 00939000
*                 (STM GPR02P,GPR08P,RPHWORK(GPR01P));             0634 00940000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00941000
*             GPR15P=LSCBPTR;       /* GET WORK ELEM POINTER         */ 00942000
         LR    GPR15P,LSCBPTR                                      0635 00943000
*             GPR14P=ADDR(FMCSHEDQ);/* ADDRESS PAB/QUEUE POINTER     */ 00944000
