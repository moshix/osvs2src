*     GOTO BTUPTR@;                                                0600 00833000
         B     BTUPTR@                                             0600 00834000
*BTUPTR:                                                           0601 00835000
*   END;                                                           0601 00836000
*                                                                  0601 00837000
BTUPTR   DS    0H                                                  0602 00838000
*/********************************************************************/ 00839000
*/* BTUPTR: THIS ROUTINE SETS THE WRITE/READ FLAG IN THE BDU MODIFIER*/ 00840000
*/*         AND THE RESPONSE SUPPRESSION FLAG IN THE BDU.            */ 00841000
*/*         IF THE'DIALOGUE TO BE ENDED'FLAG IS SET, IT CALLS        */ 00842000
*/*         ISTZDFA0 FOR ALLOCATION OF AN LSCB WITH BUFFER, QUEUES   */ 00843000
*/*         THE LSCB TO THE SCHED Q AND CALLS ISTZBFIB- DISCONNECT   */ 00844000
*/*         LCCW PROCESSOR.                                          */ 00845000
*/*         THE CHANGE ACTIVITY FOR THIS ROUTINE IS          @XL03HCH*/ 00846000
*/********************************************************************/ 00847000
*                                                                  0602 00848000
*   ZCRWRPTR=ON;                    /* ON WRITE TO PRINTER     SWITCH*/ 00849000
         L     @14,RPHCRR(,RPHREG)                                 0602 00850000
         L     @14,CRACRR(,@14)                                    0602 00851000
         OI    ZCRWRPTR(@14),B'00000010'                           0602 00852000
*   BDUMODIF=BDUMODIF&('FF'X-BMODWD);/* OFF DISCONNECT AND WR MSG  0603 00853000
*                                      FLGS -INVALID WITH WRITE/READ */ 00854000
         LA    @14,42                                              0603 00855000
         ALR   @14,ZOBPTR                                          0603 00856000
         SLR   @09,@09                                             0603 00857000
         IC    @09,BDUMODIF(,@14)                                  0603 00858000
         N     @09,@CF03064                                        0603 00859000
         STC   @09,BDUMODIF(,@14)                                  0603 00860000
*   BDUMODIF=BDUMODIF|BMODWR;       /* TURN ON WRITE-READ FLAG       */ 00861000
         OI    BDUMODIF(@14),X'06'                                 0604 00862000
*   BDUSTPR=ON;                     /* SET RESPONSE SUPPRESSN IN BDU */ 00863000
         OI    BDUSTPR(@14),B'00000100'                            0605 00864000
*   IF LCCWSD=ON THEN               /* DIALOG TO BE ENDED?           */ 00865000
         L     @14,LCPNXLCW(,LCPBPTR)                              0606 00866000
         TM    LCCWSD(@14),B'01000000'                             0606 00867000
         BNO   @RF00606                                            0606 00868000
*BFBRDIEN:                                                         0607 00869000
*     DO;                           /* YES....                       */ 00870000
BFBRDIEN DS    0H                                                  0608 00871000
*       FMCZF002=ON;                /* SET I/O SCHED TO BE     SUSPND*/ 00872000
         OI    FMCZF002(FMCBREG),B'01000000'                       0608 00873000
*       FMCZF021=ON;                /* SET DISCONN IN PROCESS        */ 00874000
         OI    FMCZF021(FMCBREG),B'00010000'                       0609 00875000
*       FMCZFD06=OFF;               /* CLEAR DISCONN OUTSTANDING IN     00876000
*                                      FMCB                  @XA07962*/ 00877000
         NI    FMCZFD06(FMCBREG),B'01111111'                       0610 00878000
*       DNCDISC=OFF;                /* AND IN DNCB           @XA07962*/ 00879000
         NI    DNCDISC(DNCBREG),B'11110111'                        0611 00880000
*       LSCZRIMM=ON;                /* SET ON RESET IMM REQ ON ERR 0612 00881000
*                                                            @XA07962*/ 00882000
         OI    LSCZRIMM(LSCBPTR),B'00010000'                       0612 00883000
*       ZCRPCOD1=0;                 /* ZERO PARM FLAG                */ 00884000
         L     @14,RPHCRR(,RPHREG)                                 0613 00885000
         L     @14,CRACRR(,@14)                                    0613 00886000
         MVI   ZCRPCOD1(@14),X'00'                                 0613 00887000
*       ZCRPLSBF=ON;                /* PARM =GET LSCB&BUFF FOR     0614 00888000
*                                      DISCONNECT                    */ 00889000
         OI    ZCRPLSBF(@14),B'00010000'                           0614 00890000
*       CALL ATCZPSVT->ZPSZDFA0->XITPOINT;/* GO GET LSCB/BUFF        */ 00891000
         L     @09,ATCZPSVT(,VTCVTREG)                             0615 00892000
         L     @15,ZPSZDFA0(,@09)                                  0615 00893000
         BALR  @14,@15                                             0615 00894000
*       RFY                                                        0616 00895000
*         RCDREG RSTD;                                             0616 00896000
*       IF RCDREG=0 THEN            /* LSCB OBTAINED ?               */ 00897000
         SLR   @14,@14                                             0617 00898000
         CR    RCDREG,@14                                          0617 00899000
         BNE   @RF00617                                            0617 00900000
*         DO;                       /* YES....                       */ 00901000
*           RFY                                                    0619 00902000
*             RCDREG UNRSTD;                                       0619 00903000
*           RFY                                                    0620 00904000
*             WORKREG RSTD;                                        0620 00905000
*           WORKREG=LSCBPTR;        /* SAVE @ CURRENT SCHED BLK      */ 00906000
         LR    WORKREG,LSCBPTR                                     0621 00907000
*           LSCBPTR=RPHSRPRM;       /* INIT PTR TO NEW LSCB          */ 00908000
         L     LSCBPTR,RPHSRPRM(,RPHREG)                           0622 00909000
*           LSCLCPBA=ZERO;          /* INIT @LCPB IN LSCB FOR DISC.     00910000
*                                                            @XA07962*/ 00911000
         ST    @14,LSCLCPBA(,LSCBPTR)                              0623 00912000
*           LSCLCCWA=ZERO;          /* SET @LCCW IN DISC. LSCB TO  0624 00913000
*                                      ZERO                  @XA07962*/ 00914000
         ST    @14,LSCLCCWA(,LSCBPTR)                              0624 00915000
*           ZOBPTR=LSCFDBA;         /* INIT ZOB REG                  */ 00916000
         L     ZOBPTR,LSCFDBA(,LSCBPTR)                            0625 00917000
*/*  TPQUE (LSCBPTR,FMCSHEDQ)                                        */ 00918000
*                                                                  0626 00919000
*           DO;                                                    0626 00920000
*             DCL                                                  0627 00921000
*               GPR00F FIXED(31) REG(00);                          0627 00922000
*             DCL                                                  0628 00923000
*               GPR00P PTR(31) REG(00);                            0628 00924000
*             DCL                                                  0629 00925000
*               GPR02F FIXED(31) REG(02);                          0629 00926000
*             DCL                                                  0630 00927000
*               GPR02P PTR(31) REG(02);                            0630 00928000
*             RFY                                                  0631 00929000
*              (GPR00P,                                            0631 00930000
*               GPR01P,                                            0631 00931000
*               GPR02P,                                            0631 00932000
*               GPR14P,                                            0631 00933000
*               GPR15P) RSTD;                                      0631 00934000
