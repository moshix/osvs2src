*/*                                                                  */ 00219000
*/* TITLE-                                                           */ 00220000
*/*      BFCB0010 - LCCW VALIDATION PROCESSOR                        */ 00221000
*/*                                                                  */ 00222000
*/* FUNCTION-                                                        */ 00223000
*/*      THE LCCW IS CHECKED FOR COMMAND CHAINING. IF IT IS          */ 00224000
*/*      NOT COMMAND CHAINED, NO FURTHER CHECKS ARE MADE.            */ 00225000
*/*      IF IT IS COMMAND CHAINED, THE FOLLOWING ERROR CHECKS ARE    */ 00226000
*/*      MADE:                                                       */ 00227000
*/*      1. END DIALOG FLAG ON IN LCCW.                              */ 00228000
*/*      2. NEXT LCCW THAT IS NOT A TIC IS NOT A WRITE LCCW.         */ 00229000
*/*                                                                  */ 00230000
*/********************************************************************/ 00231000
*/* SEGMENT(BFCB010)START                                            */ 00232000
*                                                                  0421 00233000
*   DO;                                                            0421 00234000
*     GOTO BFCB010@;                                               0422 00235000
         B     BFCB010@                                            0422 00236000
*BFCB010:                                                          0423 00237000
*   END;                                                           0423 00238000
BFCB010  DS    0H                                                  0424 00239000
*   PTRLCC=LCPNXLCW;                /* ADDRESS LCCW                  */ 00240000
         L     PTRLCC,LCPNXLCW(,PTRLCP)                            0424 00241000
*   IF DNCZF006='0'B&DNCPSOL='1'B THEN/* SWITCHED CALL-IN LINE WITH     00242000
*                                                            @XL03HDP*/ 00243000
         TM    DNCPSOL(PTRDNC),B'00001000'                         0425 00244000
         BNO   @RF00425                                            0425 00245000
         TM    DNCZF006(PTRDNC),B'00100000'                        0425 00246000
         BNZ   @RF00425                                            0425 00247000
*     DO;                           /* NO CONNECTION MADE ?          */ 00248000
*       LCPZRT00='1'B;              /* SET ERROR FLAG        @XL03HEN*/ 00249000
         OI    LCPZRT00(PTRLCP),B'10000000'                        0427 00250000
*       LCPMNRCD=FRMNORD;           /* SETERROR CODE         @XL03HEN*/ 00251000
         MVI   LCPMNRCD(PTRLCP),X'AC'                              0428 00252000
*     END;                                                         0429 00253000
*   ELSE                                                           0430 00254000
*     IF LCCWCC='1'B THEN           /* IF LCCW COMMAND CHAINED       */ 00255000
         B     @RC00425                                            0430 00256000
@RF00425 TM    LCCWCC(PTRLCC),B'01000000'                          0430 00257000
         BNO   @RF00430                                            0430 00258000
*       IF LCCWSD='1'B THEN         /* IF END DIALOG FLAG ON         */ 00259000
         TM    LCCWSD(PTRLCC),B'01000000'                          0431 00260000
         BNO   @RF00431                                            0431 00261000
*         DO;                       /* ERROR - COMMAND CHAIN + SD    */ 00262000
*           LCPZRT00='1'B;          /* SET ERROR FLAG                */ 00263000
         OI    LCPZRT00(PTRLCP),B'10000000'                        0433 00264000
*           LCPMNRCD=FRMCDITS;      /* INVALID INITIATION            */ 00265000
         MVI   LCPMNRCD(PTRLCP),X'A6'                              0434 00266000
*         END;                                                     0435 00267000
*       ELSE                        /* END DIALOG FLAG OFF           */ 00268000
*         DO;                       /* CHECK OUT NEXT LCCW           */ 00269000
         B     @RC00431                                            0436 00270000
@RF00431 DS    0H                                                  0437 00271000
*           PTRLCC=PTRLCC+          /* ADDRESS NEXT LCCW             */ 00272000
*               LENGTH(ISTLCCW);                                   0437 00273000
         AL    PTRLCC,@CF00038                                     0437 00274000
*           DO WHILE(LCCWOP=LTIC);  /* FLUSH OUT ANY TICS            */ 00275000
         B     @DE00438                                            0438 00276000
@DL00438 DS    0H                                                  0439 00277000
*             PTRLCC=LCCWDATA;      /* ADDRESS NEXT LCCW             */ 00278000
         L     PTRLCC,LCCWDATA(,PTRLCC)                            0439 00279000
*             LCPLCCWC=LCPLCCWC+1;  /* UPDATE LCCW/LCP COUNT @XL03HEN*/ 00280000
         LA    @14,1                                               0440 00281000
         SLR   @09,@09                                             0440 00282000
         IC    @09,LCPLCCWC(,PTRLCP)                               0440 00283000
         ALR   @14,@09                                             0440 00284000
         STC   @14,LCPLCCWC(,PTRLCP)                               0440 00285000
*           END;                                                   0441 00286000
@DE00438 CLI   LCCWOP(PTRLCC),X'08'                                0441 00287000
         BE    @DL00438                                            0441 00288000
*           IF LCCWRWOP=LWB         /* IF WRITE LCCW COMMAND         */ 00289000
*               |LCCWRWOP=LWM|LCCWRWOP=LWT THEN/* THEN NOT AN ERROR  */ 00290000
         TM    LCCWRWOP(PTRLCC),B'00000101'                        0442 00291000
         BNO   @GL00002                                            0442 00292000
         TM    LCCWRWOP(PTRLCC),B'00111010'                        0442 00293000
         BZ    @RT00442                                            0442 00294000
@GL00002 TM    LCCWRWOP(PTRLCC),B'00001001'                        0442 00295000
         BNO   @GL00001                                            0442 00296000
         TM    LCCWRWOP(PTRLCC),B'00110110'                        0442 00297000
         BZ    @RT00442                                            0442 00298000
@GL00001 TM    LCCWRWOP(PTRLCC),B'00001101'                        0442 00299000
         BNO   @RF00442                                            0442 00300000
         TM    LCCWRWOP(PTRLCC),B'00110010'                        0442 00301000
         BNZ   @RF00442                                            0442 00302000
@RT00442 DS    0H                                                  0443 00303000
*             PTRLCC=LCPNXLCW;      /* RE-ADDRESS WRITE SOH LCCW     */ 00304000
         L     PTRLCC,LCPNXLCW(,PTRLCP)                            0443 00305000
*           ELSE                    /* NEXT LCCW NOT A WRITE         */ 00306000
*             DO;                   /* SET ERROR CODE                */ 00307000
         B     @RC00442                                            0444 00308000
@RF00442 DS    0H                                                  0445 00309000
*               LCPZRT01='1'B;      /* SET CONTROL BLCOK ERROR       */ 00310000
         OI    LCPZRT01(PTRLCP),B'01000000'                        0445 00311000
*               LCPMNRCD=FRMINVCF;  /* INVALID CHAINING FIELD        */ 00312000
         MVI   LCPMNRCD(PTRLCP),X'81'                              0446 00313000
*             END;                                                 0447 00314000
*         END;                      /* END CHECK OUT NEXT LCCW       */ 00315000
*     ELSE                                                         0449 00316000
*       ;                           /* NOT COMMAND CHAINED           */ 00317000
@RF00430 DS    0H                                                  0450 00318000
*/* SEGMENT(BFCB010)FINISH                                           */ 00319000
*                                                                  0450 00320000
*   DO;                                                            0450 00321000
@RC00430 DS    0H                                                  0450 00322000
