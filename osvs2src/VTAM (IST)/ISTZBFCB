*   GEN NOSETS NOREFS NODEFS(EJECT);                               0453 00329000
         EJECT                                                          00330000
*                                                                  0454 00331000
*/********************************************************************/ 00332000
*/*                                                                  */ 00333000
*/* TITLE-                                                           */ 00334000
*/*      BFCB0020 - ANALYZE SESSION STATUS                           */ 00335000
*/*                                                                  */ 00336000
*/* FUNCTION-                                                        */ 00337000
*/*      CHECKS FOR IN SESSION OR CONTACT OR INVITE PENDING.  IF     */ 00338000
*/*      NONE OF THESE CONDITIONS ARE MET, THE CONTACT MODIFIER      */ 00339000
*/*      IS SET ON IN THE BDU, CONTACT PENDING IS FLAGGED, AND       */ 00340000
*/*      THE SESSION COUNT IS INCREMENTED.  IT ANY OF THE SESSION    */ 00341000
*/*      FLAGS ARE SET, A CHECK IS MADE FOR DISCONNECT PENDING.      */ 00342000
*/*      IF A DISCONNECT IS PENDING, A SESSION IS INITIATED AS       */ 00343000
*/*      BEFORE.  IF DISCONNECT IS NOT PENDING AND THE START DIALOG  */ 00344000
*/*      FLAG IS SET IN THE LCCW, AN ERROR CONDITION IS INDICATED.   */ 00345000
*/*                                                                  */ 00346000
*/********************************************************************/ 00347000
*/* SEGMENT(BFCB020)START                                            */ 00348000
*                                                                  0454 00349000
*   DO;                                                            0454 00350000
*     GOTO BFCB020@;                                               0455 00351000
         B     BFCB020@                                            0455 00352000
*BFCB020:                                                          0456 00353000
*   END;                                                           0456 00354000
BFCB020  DS    0H                                                  0457 00355000
*   RFY                                                            0457 00356000
*     SCEDPARM RSTD;                                               0457 00357000
*   RFY                                                            0458 00358000
*     RCDREG RSTD;                                                 0458 00359000
*   IF FMCZFA06='1'B                /* IF ALREADY IN SESSION         */ 00360000
*       |FMCZFB06='1'B              /* OR SESSION PENDING            */ 00361000
*       |FMCZFC06='1'B THEN                                        0459 00362000
         TM    FMCZFA06(PTRFMC),B'00000111'                        0459 00363000
         BZ    @RF00459                                            0459 00364000
*     IF FMCZFD06='1'B THEN         /* THEN IF DISCONNECT PENDING    */ 00365000
         TM    FMCZFD06(PTRFMC),B'10000000'                        0460 00366000
         BNO   @RF00460                                            0460 00367000
*       DO;                         /* INITIATE SESSION              */ 00368000
*         BDUMODIF=BMODWC;          /* SET WRITE CONTACT MODIFIER    */ 00369000
         MVI   BDUMODIF+42(PTRZOB),X'08'                           0462 00370000
*         FMCZFC06='1'B;            /* SET CONTACT OUTSTANDING       */ 00371000
         OI    FMCZFC06(PTRFMC),B'00000001'                        0463 00372000
*         DNCCONT='1'B;             /* SET CONTACT OUTSTANDING       */ 00373000
         OI    DNCCONT(PTRDNC),B'00010000'                         0464 00374000
*         FMCZX001=FMCZX001+1;      /* INCREMENT SESSION COUNT       */ 00375000
         LA    @14,1                                               0465 00376000
         SLR   @09,@09                                             0465 00377000
         IC    @09,FMCZX001(,PTRFMC)                               0465 00378000
         ALR   @14,@09                                             0465 00379000
         STC   @14,FMCZX001(,PTRFMC)                               0465 00380000
*       END;                        /* SESSION INITIATED             */ 00381000
*     ELSE                          /* DISCONNECT IS NOT PENDING     */ 00382000
*       IF LCCWSI='1'B THEN         /* IF START DIALOG FLAG ON       */ 00383000
         B     @RC00460                                            0467 00384000
@RF00460 TM    LCCWSI(PTRLCC),B'10000000'                          0467 00385000
         BNO   @RF00467                                            0467 00386000
*         DO;                       /* THEN SET ERROR CODE           */ 00387000
*           LCPZRT00='1'B;          /* SET LOGICAL ERROR FLAG        */ 00388000
         OI    LCPZRT00(PTRLCP),B'10000000'                        0469 00389000
*           LCPMNRCD=FRMCDITS;      /* SET INVALID INITIATION FLG    */ 00390000
         MVI   LCPMNRCD(PTRLCP),X'A6'                              0470 00391000
*         END;                                                     0471 00392000
*       ELSE                                                       0472 00393000
*         ;                         /* OTHERWISE DON'T               */ 00394000
@RF00467 DS    0H                                                  0473 00395000
*   ELSE                            /* NO SESSION PENDING            */ 00396000
*     DO;                                                          0473 00397000
         B     @RC00459                                            0473 00398000
@RF00459 DS    0H                                                  0474 00399000
*       SCEDPARM=8;                                                0474 00400000
         LA    SCEDPARM,8                                          0474 00401000
*       CALL ISTZBFNB;              /*                       @XL03HEN   00402000
*                                      CALL SESSION SCHEDULER        */ 00403000
         L     @09,PABDVTA+95(,PTRFMC)                             0475 00404000
         LA    @09,0(,@09)                                         0475 00405000
         L     @15,ZLBZBFNB(,@09)                                  0475 00406000
         BALR  @14,@15                                             0475 00407000
*       IF RCDREG=0 THEN            /* CONTINUE FOR NON-ERROR        */ 00408000
         LTR   RCDREG,RCDREG                                       0476 00409000
         BNZ   @RF00476                                            0476 00410000
*         DO;                       /* RETURN                @XL03HEN*/ 00411000
*           BDUMODIF=BMODWC;        /* WRITE CONTACT         @XL03HEN*/ 00412000
         LA    @14,42                                              0478 00413000
         ALR   @14,PTRZOB                                          0478 00414000
         MVI   BDUMODIF(@14),X'08'                                 0478 00415000
*           BDURTOE='1'B;           /* RETN TEXT ON ERROR    @X50BHXF*/ 00416000
         OI    BDURTOE(@14),B'00000010'                            0479 00417000
*           FMCZFC06='1'B;          /* CONTACT OUTSTANDNG@XL03HEN FOR   00418000
*                                      FUNCTION                      */ 00419000
         OI    FMCZFC06(PTRFMC),B'00000001'                        0480 00420000
