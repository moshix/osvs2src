 TITLE 'ISTZBFDR-COPY LCCW PROCESSOR FOR 3270 S/R BSC DEVICE-FORMBTU'   00305000
*/********************************************************************/ 00306000
*/*                                                                  */ 00307000
*/* FORMBTU: THIS ROUTINE COMPLETES FORMATTING OF THE BTU BEING      */ 00308000
*/*          BUILT. ALSO FMCB AND DNCB SESSION STATUS FLAGS ARE SET  */ 00309000
*/*          TO REFLECT ANY CONTACT OR DISCONNECT OUTSTANDING STATUS */ 00310000
*/*                                                                  */ 00311000
*/********************************************************************/ 00312000
*                                                                  0469 00313000
*   /*****************************************************************/ 00314000
*   /*                                                               */ 00315000
*   /* SEGMENT(FORMBTU)START                                         */ 00316000
*   /*                                                               */ 00317000
*   /*****************************************************************/ 00318000
*                                                                  0469 00319000
*   DO;                                                            0469 00320000
*     GOTO FORMBTU@;                                               0470 00321000
         B     FORMBTU@                                            0470 00322000
*FORMBTU:                                                          0471 00323000
*   END;                                                           0471 00324000
FORMBTU  DS    0H                                                  0472 00325000
*FDIIF010:                                                         0472 00326000
*   IF(FMCZFA06=ON|FMCZFB06=ON|FMCZFC06=ON)&FMCZFD06=OFF THEN/* IS 0472 00327000
*                                      (DEVICE IN SESSION, INVITE  0472 00328000
*                                      OUTSTANDING, OR CONTACT     0472 00329000
*                                      OUTSTANDING) AND DISCONNECT 0472 00330000
*                                      NOT OUTSTANDING?              */ 00331000
FDIIF010 TM    FMCZFA06(FMCBREG),B'00000111'                       0472 00332000
         BZ    @RF00472                                            0472 00333000
         TM    FMCZFD06(FMCBREG),B'10000000'                       0472 00334000
         BNZ   @RF00472                                            0472 00335000
*     IF LCCWSI=ON THEN                                            0473 00336000
         TM    LCCWSI(LCCWPTR),B'10000000'                         0473 00337000
         BNO   @RF00473                                            0473 00338000
*       DO;                         /* IS START DIALOG FLAG ON?      */ 00339000
*         ZCRBRIS=ON;               /* BRACKET REJECT, IN SESSION    */ 00340000
         L     @08,RPHCRR(,RPHREG)                                 0475 00341000
         L     @08,CRACRR(,@08)                                    0475 00342000
         OI    ZCRBRIS(@08),B'00000010'                            0475 00343000
*         LSCZRT00=ON;              /* SET ON LOGICAL ERROR FLAG     */ 00344000
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0476 00345000
*         LSCZMNRC=FRMCDITS;        /* SET MINOR RC: CONFLICTING   0477 00346000
*                                      DIALOG INIT/TERM SEQUENCE     */ 00347000
         MVI   LSCZMNRC(LSCBPTR),X'A6'                             0477 00348000
*       END;                                                       0478 00349000
*     ELSE                                                         0479 00350000
*       ;                           /* NO,(LCCWSI=OFF) (NULL)        */ 00351000
@RF00473 DS    0H                                                  0480 00352000
*   ELSE                            /* NO, (SEE FDIIF010)            */ 00353000
*     IF LCCWSI=ON THEN                                            0480 00354000
         B     @RC00472                                            0480 00355000
@RF00472 TM    LCCWSI(LCCWPTR),B'10000000'                         0480 00356000
         BNO   @RF00480                                            0480 00357000
*       DO;                         /* IF START DIALOG               */ 00358000
*         BDUMODIF=BDUMODIF|BMODWC; /* SET WRT WITH CONTACT MODIF    */ 00359000
         OI    BDUMODIF+42(ZOBPTR),X'08'                           0482 00360000
*         FMCZFC06=ON;              /* SET ON CONTACT OUTSTANDING    */ 00361000
         OI    FMCZFC06(FMCBREG),B'00000001'                       0483 00362000
*         FMCZX001=FMCZX001+1;      /* INCREMENT APPL SESS COUNT     */ 00363000
         LA    @08,1                                               0484 00364000
         SLR   @00,@00                                             0484 00365000
         IC    @00,FMCZX001(,FMCBREG)                              0484 00366000
         ALR   @08,@00                                             0484 00367000
         STC   @08,FMCZX001(,FMCBREG)                              0484 00368000
*         FMCZF002=ON;              /* SUSPEND I/O SCHEDULING        */ 00369000
         OI    FMCZF002(FMCBREG),B'01000000'                       0485 00370000
*       END;                                                       0486 00371000
*     ELSE                                                         0487 00372000
*       DO;                         /* NOT START DIALOG              */ 00373000
         B     @RC00480                                            0487 00374000
@RF00480 DS    0H                                                  0488 00375000
*         LSCZRT00='1'B;            /* INDICATE LOGICAL ERROR        */ 00376000
         OI    LSCZRT00(LSCBPTR),B'10000000'                       0488 00377000
*         LSCZMNRC=FRMCDITS;        /* CODE=CONFLICTING INIT/TERM    */ 00378000
         MVI   LSCZMNRC(LSCBPTR),X'A6'                             0489 00379000
*       END;                        /* END NOT START DIALOG          */ 00380000
*   IF LSCZRT00=ON THEN             /* LOGICAL ERROR FLAG SET ON?    */ 00381000
@RC00480 DS    0H                                                  0491 00382000
@RC00472 TM    LSCZRT00(LSCBPTR),B'10000000'                       0491 00383000
         BO    @RT00491                                            0491 00384000
*     ;                             /* YES, SKIP FURTHER PROCESSG    */ 00385000
*   ELSE                                                           0493 00386000
*     DO;                           /* NO,                           */ 00387000
*       IF LCCWSD=ON THEN                                          0494 00388000
         TM    LCCWSD(LCCWPTR),B'01000000'                         0494 00389000
         BNO   @RF00494                                            0494 00390000
*         DO;                       /* IS END DIALOG FLAG ON?        */ 00391000
*           BDUMODIF=BDUMODIF|BMODWD;/* SET DISCONNECT MODIFR        */ 00392000
         OI    BDUMODIF+42(ZOBPTR),X'03'                           0496 00393000
*           FMCZF002='1'B;          /* SET I/O SCHEDULING SUSPENDED     00394000
