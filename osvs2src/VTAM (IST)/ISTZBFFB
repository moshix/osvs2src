*   FSBSPHSE=NCPPHSE3;              /* SET PHASE 3                   */ 01202000
         OI    FSBSPHSE(@15),B'01100000'                           0873 01203000
*   FSBSCODE=NCPOK;                 /* SET NCP STATUS TO OK          */ 01204000
         NI    FSBSCODE(@15),B'11100000'                           0874 01205000
*   MVN(FSBSEQ(1:1),REQTAG);        /* ASSIGN REQTG SEQ NUMBER     0875 01206000
*                                                            @XM9006K*/ 01207000
