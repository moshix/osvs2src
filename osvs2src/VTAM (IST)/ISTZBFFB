*                                                                  0733 00760000
BFFB040  DS    0H                                                  0734 00761000
*/*                                                                  */ 00762000
*/* A RESET CONDITIONAL BDU IS GENERATED AND QUEUED TO               */ 00763000
*/* THE HEAD OF THE SCHEDULE QUEUE IF THERE ARE ELEMENTS ON THE      */ 00764000
*/* WAIT QUEUE  AND THE 370X IS OK.  OTHERWISE, THE RESET            */ 00765000
*/* CLEANUP PHASES I AND III ARE CALLED TO PURGE THE WAIT QUEUE.     */ 00766000
*/* THE SCHEDULE QUEUE IS LOCKED, AND A FSB IS GENERATED AND QUEUED  */ 00767000
*/* TO THE TIP.                                                      */ 00768000
*/*                                                                  */ 00769000
*                                                                  0734 00770000
*   IF DNCZF001=ON THEN             /* DEVICE ERROR LOCK SET?        */ 00771000
         TM    DNCZF001(DNCBREG),B'10000000'                       0734 00772000
         BNO   @RF00734                                            0734 00773000
*     DO;                                                          0735 00774000
*       LCPZRT00=ON;                /* SET ERROR FLAG                */ 00775000
         OI    LCPZRT00(LCPBREG),B'10000000'                       0736 00776000
*       LCPMNRCD=FRMRCDNP;          /* ERROR CODE=RESET CONDITIONAL     00777000
*                                      REJECTED-ERROR LOCK SET     0737 00778000
*                                                            @XL03HCH*/ 00779000
         MVI   LCPMNRCD(LCPBREG),X'29'                             0737 00780000
*       ZCRRCDSV=4;                 /* ERROR RETURN                  */ 00781000
         MVI   ZCRRCDSV(ZCRREG),X'04'                              0738 00782000
*     END;                                                         0739 00783000
*   ELSE                                                           0740 00784000
*     DO;                                                          0740 00785000
         B     @RC00734                                            0740 00786000
@RF00734 DS    0H                                                  0741 00787000
*       FMCZX006=FMCZX006+1;        /* INCREMENT RESET CT    @XL03HEK*/ 00788000
         LA    @15,1                                               0741 00789000
         SLR   @14,@14                                             0741 00790000
         IC    @14,FMCZX006(,FMCBREG)                              0741 00791000
         ALR   @15,@14                                             0741 00792000
         STC   @15,FMCZX006(,FMCBREG)                              0741 00793000
*       IF(FMCLCPB2=0)|ZCRTSW1=ON THEN/* WAIT QUEUE EMPTY OR 370X ILL   00794000
*                                                            @XL03HEK*/ 00795000
