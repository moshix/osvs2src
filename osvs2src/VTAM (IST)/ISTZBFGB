         LA    @14,4                                               0482 00402000
         AL    @14,LCCWDATA(,LCCWREG)                              0482 00403000
         ST    @14,LCCWDATA(,LCCWREG)                              0482 00404000
*   CALL ISTZDFC0;                  /* CALL MOVE LCCW RTN            */ 00405000
         L     @14,ATCZPSVT(,ATCVTREG)                             0483 00406000
         L     @15,ZPSZDFC0(,@14)                                  0483 00407000
         BALR  @14,@15                                             0483 00408000
*   LCCWDATA=LCCWDATA-LENGTH(ISTESCDA);/* RESTORE LCCW DATA PTR    0484 00409000
*                                                             @X03HDP*/ 00410000
