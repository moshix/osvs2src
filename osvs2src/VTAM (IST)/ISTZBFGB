         ST    @14,LCCWDATA(,LCCWREG)                              0484 00413000
*   IF RTNCODE=0 THEN                                              0485 00414000
         LTR   RTNCODE,RTNCODE                                     0485 00415000
         BNZ   @RF00485                                            0485 00416000
*     DO;                                                          0486 00417000
*                                                                  0486 00418000
*/***** LOOP THRU ZOBUF COUNTING ALL DATA  ***************************/ 00419000
*                                                                  0487 00420000
*       RFY                                                        0487 00421000
*        (COUNT,                                                   0487 00422000
*         COUNTPTR) RSTD;           /* RESTRICT REGS 0 & 14          */ 00423000
*       COUNTPTR=ZOBUFREG;          /* INIT FDBPTR                   */ 00424000
         LR    COUNTPTR,ZOBUFREG                                   0488 00425000
*       COUNT=FDBDATCT;             /* INIT COUNT            @XM08285*/ 00426000
         LH    COUNT,FDBDATCT(,ZOBUFREG)                           0489 00427000
*       DO WHILE(COUNTPTR->FDBFDBA^=0);                            0490 00428000
         B     @DE00490                                            0490 00429000
@DL00490 DS    0H                                                  0491 00430000
*         COUNTPTR=COUNTPTR->FDBFDBA;/* GET NEXT FDB                 */ 00431000
         L     COUNTPTR,FDBFDBA(,COUNTPTR)                         0491 00432000
*         COUNT=COUNT+COUNTPTR->FDBDATCT;                          0492 00433000
*                                                                  0492 00434000
         AH    COUNT,FDBDATCT(,COUNTPTR)                           0492 00435000
*         /***********************************************************/ 00436000
*         /*                                                         */ 00437000
*         /* UPDATE COUNT                                            */ 00438000
*         /*                                                         */ 00439000
*         /***********************************************************/ 00440000
*                                                                  0493 00441000
*       END;                                                       0493 00442000
*                                                                  0493 00443000
@DE00490 SLR   @11,@11                                             0493 00444000
         C     @11,FDBFDBA(,COUNTPTR)                              0493 00445000
         BNE   @DL00490                                            0493 00446000
*/* BTUDCF MUST CONTAIN LENGTH OF BTU (MINUS TH PORTION) +         0494 00447000
*    ACCUMULATED DATA COUNTS FROM FDBS, IF ANY.                      */ 00448000
*                                                                  0494 00449000
*       BTUDCF=COUNT-(ADDR(BTURH)-ADDR(BTUFID));                   0494 00450000
*                                                                  0494 00451000
         LA    @10,28                                              0494 00452000
         ALR   @10,ZOBUFREG                                        0494 00453000
         LA    @11,BTURH(,@10)                                     0494 00454000
         LCR   @11,@11                                             0494 00455000
         ALR   @11,@10                                             0494 00456000
         ALR   @11,COUNT                                           0494 00457000
         STH   @11,BTUDCF(,@10)                                    0494 00458000
*       /*************************************************************/ 00459000
*       /*                                                           */ 00460000
*       /* PUT TOTAL COUNT IN BTU                            @XM08285*/ 00461000
*       /*                                                           */ 00462000
*       /*************************************************************/ 00463000
*                                                                  0495 00464000
*       RTNCODE=0;                  /* RC = NO ERRORS                */ 00465000
         SLR   @11,@11                                             0495 00466000
         LR    RTNCODE,@11                                         0495 00467000
*     END;                                                         0496 00468000
*   ELSE                            /* MOVE LCCW ERROR               */ 00469000
*     DO;                                                          0497 00470000
         B     @RC00485                                            0497 00471000
@RF00485 DS    0H                                                  0498 00472000
*       IF RTNCODE=8 THEN           /* IF LOGIC ERROR                */ 00473000
         CH    RTNCODE,@CH00034                                    0498 00474000
         BNE   @RF00498                                            0498 00475000
*         DO;                                                      0499 00476000
*           LCPZRT00=ON;            /* SET LOGIC ERR FLAG IN       0500 00477000
*                                      LCPB/LSCB                     */ 00478000
         OI    LCPZRT00(LCPBREG),B'10000000'                       0500 00479000
*           LCPMNRCD=FRMVTAME;      /* MINOR RC= VTAM ERROR          */ 00480000
