*     EMCR13 PTR REG(13),                                          0511 00522000
*     EMCR14 PTR REG(14),                                          0511 00523000
*     EMCR15 PTR REG(15);                                          0511 00524000
*   DECLARE                                                        0512 00525000
*     ISTSDCCA ENTRY BASED(ATCCDADD) VALRG(*),/* CIDCTL ADD ROUTINE  */ 00526000
*     ISTSDCCD ENTRY BASED(ATCCDDEL) VALRG(*),/* CIDCTL DELETE     0512 00527000
*                                      ROUTINE                       */ 00528000
