         BNZ   @RF00397                                            0397 00410000
         CR    CNTLCCW,@15                                         0397 00411000
         BNH   @RF00397                                            0397 00412000
*         DO;                                                      0398 00413000
*           LCPZRT01=ON;                                           0399 00414000
         OI    LCPZRT01(LCPBPTR),B'01000000'                       0399 00415000
*           LCPMNRCD=FRMINVCF;                                     0400 00416000
         MVI   LCPMNRCD(LCPBPTR),X'81'                             0400 00417000
*           ZCRENDAN=ON;                                           0401 00418000
         OI    ZCRENDAN(ZCRPTR),B'00000010'                        0401 00419000
*         END;                                                     0402 00420000
*       ELSE                                                       0403 00421000
*                                                                  0403 00422000
*         /***********************************************************/ 00423000
*         /*                                                         */ 00424000
*         /* INCREMENT TO NEXT LCCW. IF THIS IS A TIC LCCW, USE LCCW */ 00425000
*         /* DATA ADDRESS FOR NEXT LCCW ADDRESS.                     */ 00426000
*         /*                                                         */ 00427000
*         /***********************************************************/ 00428000
*                                                                  0403 00429000
*         IF LCCWOP=LTIC THEN       /* A TIC LCCW                    */ 00430000
         B     @RC00397                                            0403 00431000
@RF00397 CLI   LCCWOP(CURLCCW),X'08'                               0403 00432000
         BNE   @RF00403                                            0403 00433000
*           DO;                                                    0404 00434000
*             CURLCCW=LCCWDATA;     /* NEXT LCCW ADDRESS = CURRENT 0405 00435000
*                                      LCCW DATA ADDRESS             */ 00436000
         L     CURLCCW,LCCWDATA(,CURLCCW)                          0405 00437000
*             IF LCCWOP=LTIC THEN   /* TIC TO A TIC ?        @XL03HEN*/ 00438000
         CLI   LCCWOP(CURLCCW),X'08'                               0406 00439000
         BNE   @RF00406                                            0406 00440000
*               DO;                 /*                       @XL03HEN*/ 00441000
*                 LCPZRT01=ON;      /* ERROR FLAG            @XL03HEN*/ 00442000
         OI    LCPZRT01(LCPBPTR),B'01000000'                       0408 00443000
*                 LCPMNRCD=FRMINVCF;/* ERROR CODE            @XL03HEN*/ 00444000
         MVI   LCPMNRCD(LCPBPTR),X'81'                             0409 00445000
*                 ZCRENDAN=ON;      /* EXIT SWITCH           @XL03HEN*/ 00446000
         OI    ZCRENDAN(ZCRPTR),B'00000010'                        0410 00447000
*               END;                /*                       @XL03HEN*/ 00448000
*             ELSE                                                 0412 00449000
*               ;                   /*                       @XL03HEN*/ 00450000
@RF00406 DS    0H                                                  0413 00451000
*           END;                    /*                       @XL03HEN*/ 00452000
*         ELSE                      /* NOT A TIC -                   */ 00453000
*           CURLCCW=CURLCCW+LENGTH(ISTLCCW);/* POINT TO NEXT LCCW    */ 00454000
         B     @RC00403                                            0414 00455000
@RF00403 AH    CURLCCW,@CH00057                                    0414 00456000
*     END;                                                         0415 00457000
@RC00403 DS    0H                                                  0415 00458000
@RC00397 DS    0H                                                  0416 00459000
*   RETNREG=ZCRSVF3;                /* RESTORE RETURN REGISTER       */ 00460000
@RC00389 L     RETNREG,ZCRSVF3(,ZCRPTR)                            0416 00461000
*   END BFMB0010;                   /* END - INTERNAL PROCEDURE      */ 00462000
@EL00002 DS    0H                                                  0417 00463000
@EF00002 DS    0H                                                  0417 00464000
@ER00002 BR    @14                                                 0417 00465000
