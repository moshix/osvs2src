*               ;                   /* IF TLB NOT PRESENT            */ 00884000
@RF00700 DS    0H                                                  0761 00885000
*             GO TO CHECKFLG;       /* CONTINUE PROCESSING           */ 00886000
         B     CHECKFLG                                            0761 00887000
*           END;                    /* END FSB PRESENT               */ 00888000
*         ELSE                                                     0763 00889000
*           DO;                     /* IF FSB IS NOT PRESENT         */ 00890000
@RF00698 DS    0H                                                  0764 00891000
*             LCPRUCNT=LCPMNRCD;    /* SAVE CLEAR REASON CODE      0764 00892000
*                                                            @XM19747*/ 00893000
