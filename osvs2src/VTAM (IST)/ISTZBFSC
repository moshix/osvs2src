@RF00770 DS    0H                                                  0776 00928000
*                 GO TO GETFSB;     /* CONTINUE PROCESSING           */ 00929000
         B     GETFSB                                              0776 00930000
*               END;                /* END NOT IN SESSION            */ 00931000
*             ELSE                                                 0778 00932000
*               DO;                 /* IF BOUND OR BIND UP           */ 00933000
@RF00768 DS    0H                                                  0779 00934000
*                 IF FMCSNBI=ON THEN/* IF SESSION NOT BOUND          */ 00935000
         TM    FMCSNBI(FMCBPTR),B'10000000'                        0779 00936000
         BNO   @RF00779                                            0779 00937000
*                   IF LCCWOP=LCLEAR/* IF CLEAR              @XM10462*/ 00938000
*                       |LCCWOP=LCRUD/* | CLEAR UNBIND               */ 00939000
*                       |LCCWOP=LCRUDNFY/* | CLEAR UNBIND NOTIFY     */ 00940000
*                       |LCCWOP=LNFY/* | NOTIFY                      */ 00941000
*                       |LCCWOP=LCLRNFY THEN                       0780 00942000
         L     @09,LCCWPTR(,ZCRRPTR)                               0780 00943000
         CLI   LCCWOP(@09),X'F2'                                   0780 00944000
         BE    @RT00780                                            0780 00945000
         CLI   LCCWOP(@09),X'F3'                                   0780 00946000
         BE    @RT00780                                            0780 00947000
         CLI   LCCWOP(@09),X'F4'                                   0780 00948000
         BE    @RT00780                                            0780 00949000
         CLI   LCCWOP(@09),X'F5'                                   0780 00950000
         BE    @RT00780                                            0780 00951000
         CLI   LCCWOP(@09),X'F8'                                   0780 00952000
         BNE   @RF00780                                            0780 00953000
@RT00780 DS    0H                                                  0781 00954000
*                     ;             /* | CLEAR NOTIFY                */ 00955000
*                   ELSE            /* NONE OF THE ABOVE             */ 00956000
*                     GO TO GETFSB; /* CONTINUE FROM GETFSB          */ 00957000
*                 IF(FMCCIPI=ON     /* IF CLEAR IN PROGRESS          */ 00958000
*                     |FMCUIPI=ON)  /* | UNBIND IN PROGRESS          */ 00959000
*                     &LCCWOP=LCLEAR THEN/* & OPERATION CODE = CLEAR */ 00960000
@RF00779 TM    FMCCIPI(FMCBPTR),B'01100000'                        0783 00961000
         BZ    @RF00783                                            0783 00962000
         L     @09,LCCWPTR(,ZCRRPTR)                               0783 00963000
         CLI   LCCWOP(@09),X'F2'                                   0783 00964000
         BE    @RT00783                                            0783 00965000
*                   GO TO GETFSB;   /* CONTINUE FROM GETFSB          */ 00966000
*                 ELSE              /* OTHERWISE                     */ 00967000
*                   CALL ISTZCLR;   /* PROCESS OTHER POSSIBIITIES    */ 00968000
@RF00783 BAL   @14,ISTZCLR                                         0785 00969000
*                 GO TO CHECKFLG;   /* CONTINUE IN ANY CASE          */ 00970000
         B     CHECKFLG                                            0786 00971000
*               END;                /* END BOUND OR BIND UP          */ 00972000
*GETFSB:                                                           0788 00973000
*             ;                     /* BRANCH POINT                  */ 00974000
GETFSB   DS    0H                                                  0789 00975000
*             ZCRPCOD1=0;           /* ZDFA0 PARAMETER               */ 00976000
         MVI   ZCRPCOD1(ZCRRPTR),X'00'                             0789 00977000
*             ZCRPFSB=ON;           /* ZDFA0 PARAMETER               */ 00978000
         OI    ZCRPFSB(ZCRRPTR),B'00100000'                        0790 00979000
*             CALL ISTZDFA0;        /* BUFFER ALLOCATION UTILITY     */ 00980000
         L     @09,ATCZPSVT(,ATCVTPTR)                             0791 00981000
         L     @15,ZPSZDFA0(,@09)                                  0791 00982000
         BALR  @14,@15                                             0791 00983000
*             FSBPTR=RPHSRPRM;      /* ADDRESS ALLOCATED FSB         */ 00984000
         L     @09,RPHSRPRM(,RPHPTR)                               0792 00985000
         ST    @09,FSBPTR(,ZCRRPTR)                                0792 00986000
*             FSBMJRCD=FRLOGICE;    /* MAJOR=LOGIC ERROR             */ 00987000
         MVI   FSBMJRCD(@09),X'08'                                 0793 00988000
*             FSBMNRCD=FRMUSELE;    /* MINOR=USER LOGIC ERROR        */ 00989000
         MVI   FSBMNRCD(@09),X'A3'                                 0794 00990000
*             FSBLCPB=LCPBPTR;      /* SET ADDRESS OF LCPB IN FSB    */ 00991000
         L     @07,LCPBPTR(,ZCRRPTR)                               0795 00992000
         ST    @07,FSBLCPB(,@09)                                   0795 00993000
*             FSBLCCWA=LCPNXLCW;    /* ADDRESS FAILING LCCW          */ 00994000
         L     @07,LCPNXLCW(,@07)                                  0796 00995000
         ST    @07,FSBLCCWA(,@09)                                  0796 00996000
*             ZCRRJLCP=ON;          /* FLAG LCPB REJECTED            */ 00997000
         OI    ZCRRJLCP(ZCRRPTR),B'00100000'                       0797 00998000
*CHECKFLG:                                                         0798 00999000
*             ;                     /* BRANCH POINT                  */ 01000000
CHECKFLG DS    0H                                                  0799 01001000
*             IF ZCRTPINV=OFF       /* IF INVOKE TPIOS FLAG OFF      */ 01002000
*                 &ZCRTPEX=OFF THEN                                0799 01003000
         TM    ZCRTPINV(ZCRRPTR),B'11000000'                       0799 01004000
         BNZ   @RF00799                                            0799 01005000
*               DO;                 /* & EXIT FLAG OFF               */ 01006000
*                 CALL ISTZFDB;     /* FEED FSB BACK TO REQUESTOR    */ 01007000
         BAL   @14,ISTZFDB                                         0801 01008000
*                 ZCRTPEX=ON;       /* SET TPEXIT INDICATOR          */ 01009000
         OI    ZCRTPEX(ZCRRPTR),B'10000000'                        0802 01010000
*               END;                /* END FEEDBACK PROCESSING       */ 01011000
*             ELSE                                                 0804 01012000
*               ;                   /* NOT FEEDBACK PROCESSING       */ 01013000
@RF00799 DS    0H                                                  0805 01014000
*           END;                    /* END FSB NOT PRESENT           */ 01015000
*       END ISTZTRM;                /* END OF SEGMENT                */ 01016000
*/********************************************************************/ 01017000
*/*      ISTZTRM SEGMENT END                                         */ 01018000
*/********************************************************************/ 01019000
*                                                                  0807 01020000
*   RFY                                                            0807 01021000
*    (R15) RSTD;                    /* RETURN CODE REGISTER          */ 01022000
@RC00564 DS    0H                                                  0807 01023000
@RT00562 DS    0H                                                  0808 01024000
*   IF ZCRTPINV=ON THEN             /* IF INVOKE TPIOS FLAG ON       */ 01025000
         TM    ZCRTPINV(ZCRRPTR),B'01000000'                       0808 01026000
         BNO   @RF00808                                            0808 01027000
*     R15=00;                       /* SET RETURN CODE = 00          */ 01028000
         SLR   R15,R15                                             0809 01029000
*   ELSE                            /* IF INVOKE TPIOS FLAG OFF      */ 01030000
*     IF ZCRRJLCP=ON THEN           /* IF REJECT LCPB INDICATED      */ 01031000
         B     @RC00808                                            0810 01032000
@RF00808 TM    ZCRRJLCP(ZCRRPTR),B'00100000'                       0810 01033000
         BNO   @RF00810                                            0810 01034000
*       R15=08;                     /* SET RETURN CODE = 08          */ 01035000
         LA    R15,8                                               0811 01036000
*     ELSE                          /* IF LCPB REJECT OFF            */ 01037000
*       R15=04;                     /* SET RETURN CODE = 04          */ 01038000
         B     @RC00810                                            0812 01039000
@RF00810 LA    R15,4                                               0812 01040000
*/* TPZRETN                                                          */ 01041000
*                                                                  0813 01042000
*   DO;                                                            0813 01043000
@RC00810 DS    0H                                                  0813 01044000
@RC00808 DS    0H                                                  0814 01045000
*     RFY                                                          0814 01046000
*      (GPR07P,                                                    0814 01047000
*       GPR14P,                                                    0814 01048000
*       GPR15P) RSTD;                                              0814 01049000
*     GPR13P=ZSAZSVBC;                                             0815 01050000
         L     GPR13P,ZSAZSVBC(,GPR13P)                            0815 01051000
*     IF GPR13P=0 THEN                                             0816 01052000
         LTR   GPR13P,GPR13P                                       0816 01053000
         BZ    @RT00816                                            0816 01054000
*       ;                           /* TPDVTS ISTZFARR               */ 01055000
*     ELSE                                                         0818 01056000
*       GPR14P=ZSAZSVRR;                                           0818 01057000
         L     GPR14P,ZSAZSVRR(,GPR13P)                            0818 01058000
*     GEN REFS(ZSAZSVVR)(LM    7,12,ZSAZSVVR+0(13));               0819 01059000
@RT00816 DS    0H                                                  0819 01060000
         LM    7,12,ZSAZSVVR+0(13)                                      01061000
*     GEN REFS(GPR14P) EXIT NOSEQFLOW(BR    GPR14P);               0820 01062000
         BR    GPR14P                                                   01063000
*     RFY                                                          0821 01064000
*      (GPR07P,                                                    0821 01065000
*       GPR14P,                                                    0821 01066000
*       GPR15P) UNRSTD;                                            0821 01067000
*   END;                                                           0822 01068000
*                                                                  0822 01069000
*/* END TPZRETN                                                      */ 01070000
*                                                                  0823 01071000
*                                   /* RETURN LINKAGE                */ 01072000
         EJECT                                                          01073000
*                                                                  0823 01074000
*/********************************************************************/ 01075000
*/*                                                                  */ 01076000
*/* SUBROUTINE-                                                      */ 01077000
*/*      ISTZCLR                                                     */ 01078000
*/*                                                                  */ 01079000
*/* FUNCTION-                                                        */ 01080000
*/*      PROCESS CLEAR (UNBIND,(NOTIFY)) OR NOTIFY ALONE REQUESTS.   */ 01081000
*/*                                                                  */ 01082000
*/*      UPON ENTRY, THE PROCESSING CLEAR FLAG IS SET UNLESS         */ 01083000
*/*      ENTERED TO PROCESS A NOTIFY ALONE.                          */ 01084000
*/*                                                                  */ 01085000
*/*      FOR A CLEAR TYPE REQUEST,                                   */ 01086000
*/*            TRAFFIC IS PROHIBITED ON THE TOP, ATOP, AND CLIP.     */ 01087000
*/*                                                                  */ 01088000
*/*      FOR CLEAR/UNBIND/NOTIFIY AND NOTIFY,                        */ 01089000
*/*            A FSB IS OBTAINED, FORMATTED, AND QUEUED TO THE SIP.  */ 01090000
*/*            FOR NOTIFY,                                           */ 01091000
*/*                THE RPH (ADDRESSED FROM THE LCPB) IS POSTED.      */ 01092000
*/*                                                                  */ 01093000
*/*      FOR MULTIPLE REQUESTS, (CLEAR/UNBIND OR CLEAR/UNBIND/NOTIFY)*/ 01094000
*/*            A TLB IS BUILT AND A POINTER TO IT IS SET IN THE LCPB.*/ 01095000
*/*            DATA TRAFFIC IS PROHIBITED ON THE TOP, ATOP, AND CLIP.*/ 01096000
*/*                                                                  */ 01097000
*/********************************************************************/ 01098000
*                                                                  0823 01099000
*ISTZCLR:                                                          0823 01100000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* ISTZCLR SUBROUTINE            */ 01101000
ISTZCLR  DS    0H                                                  0824 01102000
*   RFY                                                            0824 01103000
*     R14 RSTD;                     /* TO SAVE RETURN POINTER        */ 01104000
*   R14SAVE=R14;                    /* SAVE RETURN POINTER           */ 01105000
         ST    R14,R14SAVE(,ZCRRPTR)                               0825 01106000
*   RFY                                                            0826 01107000
*     R14 UNRSTD;                   /* SAVED                         */ 01108000
*   IF LCCWOP^=LNFY THEN            /* IF NOT NOTIFY OPERATION       */ 01109000
         L     @14,LCCWPTR(,ZCRRPTR)                               0827 01110000
         CLI   LCCWOP(@14),X'F5'                                   0827 01111000
         BE    @RF00827                                            0827 01112000
*     FMCCIPI=ON;                   /* FLAG CLEAR IN PROCESS         */ 01113000
         OI    FMCCIPI(FMCBPTR),B'01000000'                        0828 01114000
*   ELSE                                                           0829 01115000
*     ;                             /* IF NOTIFY OPERATION CODE      */ 01116000
@RF00827 DS    0H                                                  0830 01117000
*   IF LCCWOP=LCLEAR THEN                                          0830 01118000
         L     @14,LCCWPTR(,ZCRRPTR)                               0830 01119000
         CLI   LCCWOP(@14),X'F2'                                   0830 01120000
         BNE   @RF00830                                            0830 01121000
*     DO;                           /* IF HANDLING A CLEAR           */ 01122000
*       ZCRTPINV=ON;                /* SET INVOKE TPIOS BIT ON TO  0832 01123000
*                                      CAUSE RETURN CODE 0           */ 01124000
         OI    ZCRTPINV(ZCRRPTR),B'01000000'                       0832 01125000
*       CALL ISTZPBST;              /* INHIBIT DATA TRAFFIC          */ 01126000
         BAL   @14,ISTZPBST                                        0833 01127000
*     END;                          /* END CLEAR PROCESSING          */ 01128000
*   ELSE                                                           0835 01129000
*     DO;                           /* NOT CLEAR ALONE               */ 01130000
         B     @RC00830                                            0835 01131000
@RF00830 DS    0H                                                  0836 01132000
*       IF LCCWOP=LNFY THEN                                        0836 01133000
         L     @14,LCCWPTR(,ZCRRPTR)                               0836 01134000
         CLI   LCCWOP(@14),X'F5'                                   0836 01135000
         BNE   @RF00836                                            0836 01136000
*         DO;                       /* IF NOTIFY ALONE               */ 01137000
*           CALL NOTIFY;            /* PROCESS NOFIFY        @XM19747*/ 01138000
         BAL   @14,NOTIFY                                          0838 01139000
*/* TPDEQ (LCPBPTR,FMCPAB5) RESET(YES)                               */ 01140000
*                                                                  0839 01141000
*           DO;                     /* BEGIN TPDEQ CODE GENERATION   */ 01142000
*             DCL                                                  0840 01143000
*               FMZCA035 BIT(32) CONSTANT('80000000'X);            0840 01144000
*             DCL                                                  0841 01145000
*               FMZCB035 BIT(32) CONSTANT('7FFFFFFF'X);            0841 01146000
*             RFY                                                  0842 01147000
*              (GPR00P,                                            0842 01148000
*               GPR01P,                                            0842 01149000
*               GPR14P,                                            0842 01150000
*               GPR15P) RSTD;                                      0842 01151000
*             RFY                                                  0843 01152000
*               ISTWKE;                                            0843 01153000
*             GPR15P=ADDR(FMCPAB5); /* ADDRESS QUEUE POINTER         */ 01154000
         LA    GPR15P,FMCPAB5(,FMCBPTR)                            0844 01155000
*             RFY                                                  0845 01156000
*              (GPR02P,                                            0845 01157000
*               GPR03P) RSTD;                                      0845 01158000
*             STM(GPR02P,GPR03P,GPR01P->RPHWORK(5:12));            0846 01159000
         STM   GPR02P,GPR03P,RPHWORK+4(GPR01P)                     0846 01160000
