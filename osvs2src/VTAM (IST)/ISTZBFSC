         TITLE 'ISTZBFSC/ASC-VS1/MSC-VS2-SESSION CONTROL OUTBOUND      *00001000
                        '                                               00002000
ISTZBFSC CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ISTZBFSC  78.052'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
*                                                                  0508 00011000
*/********************************************************************/ 00012000
*/*                                                                  */ 00013000
*/* SECTION-                                                         */ 00014000
*/*      MAINLINE                                                    */ 00015000
*/*                                                                  */ 00016000
*/* FUNCTION-                                                        */ 00017000
*/*      SESSION CONTROL OUTBOUND PROCESSOR.                         */ 00018000
*/*                                                                  */ 00019000
*/*      THIS ROUTINE HANDLES CONTROL ROUTING FOR LCCWS AND          */ 00020000
*/*      OUTBOUND FSBS.                                              */ 00021000
*/*                                                                  */ 00022000
*/*      THE FOLLOWING LCCW OPERATION CODES ARE HANDLED:             */ 00023000
*/*            SEND                                                  */ 00024000
*/*            CLEAR (,UNBIND (,NOTIFY))                             */ 00025000
*/*            NOTIFY                                                */ 00026000
*/*                                                                  */ 00027000
*/*      THIS ROUTINE ALSO HANDLES CONTROL ROUTING FOR A             */ 00028000
*/*      CLEAR OR SESSION TERMINATION FSB.                           */ 00029000
*/*                                                                  */ 00030000
*/********************************************************************/ 00031000
*/* TPZSAVE (7,12,14) AC(BCSC)BASE                                   */ 00032000
*                                                                  0508 00033000
*   DO;                                                            0508 00034000
*     DCL                                                          0509 00035000
*       GPR01F FIXED(31) REG(01);                                  0509 00036000
*     DCL                                                          0510 00037000
*       GPR01P PTR(31) REG(01);                                    0510 00038000
*     DCL                                                          0511 00039000
*       GPR12F FIXED(31) REG(12);                                  0511 00040000
*     DCL                                                          0512 00041000
*       GPR12P PTR(31) REG(12);                                    0512 00042000
*     DCL                                                          0513 00043000
*       GPR13F FIXED(31) REG(13);                                  0513 00044000
*     DCL                                                          0514 00045000
*       GPR13P PTR(31) REG(13);                                    0514 00046000
*     DCL                                                          0515 00047000
*       GPR14F FIXED(31) REG(14);                                  0515 00048000
*     DCL                                                          0516 00049000
*       GPR14P PTR(31) REG(14);                                    0516 00050000
*     RFY                                                          0517 00051000
*      (GPR01P,                                                    0517 00052000
*       GPR12P,                                                    0517 00053000
*       GPR13P,                                                    0517 00054000
*       GPR14P) RSTD;                                              0517 00055000
*     GEN(PRINT OFF);                                              0518 00056000
*                                                                  0518 00057000
         PRINT OFF                                                      00058000
*/* ATBUILD (ISTZSAVE)                                               */ 00059000
*/* END OF ATBUILD                                                   */ 00060000
*                                                                  0519 00061000
*     GEN(PRINT ON);                                               0519 00062000
         PRINT ON                                                       00063000
*     RFY                                                          0520 00064000
*       ISTZSAVE BASED(GPR13P);                                    0520 00065000
*     GEN SETS(ZSAZSVVR)(STM   7,12,ZSAZSVVR+0(13));               0521 00066000
         STM   7,12,ZSAZSVVR+0(13)                                      00067000
*     GEN SETS(GPR12P);                                            0522 00068000
         BALR  GPR12P,0                                                 00069000
@TSTART  DS    0H                                                       00070000
         USING @TSTART,GPR12P                                           00071000
*     GEN(PRINT OFF);                                              0523 00072000
*                                                                  0523 00073000
         PRINT OFF                                                      00074000
*/* ATBUILD (ISTZCRR,ISTRPH,ISTCRA) EJECT(NO)                        */ 00075000
*/* END OF ATBUILD                                                   */ 00076000
*                                                                  0524 00077000
*     GEN(PRINT ON);                                               0524 00078000
         PRINT ON                                                       00079000
*     GPR01P->RPHCRR->CRACRR->ZCREBCSC='1'B;                       0525 00080000
         L     @15,RPHCRR(,GPR01P)                                 0525 00081000
         L     @15,CRACRR(,@15)                                    0525 00082000
         OI    ZCREBCSC(@15),B'00000001'                           0525 00083000
*     ZSAZACTV='1'B;                                               0526 00084000
         OI    ZSAZACTV(GPR13P),B'10000000'                        0526 00085000
*     ZSAZTRAC='BCSC';                                             0527 00086000
         MVC   ZSAZTRAC(4,GPR13P),@CC04064                         0527 00087000
*     ZSAZSVRR=GPR14P;                                             0528 00088000
         ST    GPR14P,ZSAZSVRR(,GPR13P)                            0528 00089000
*     GPR13P=ZSAZSVFC;                                             0529 00090000
         L     GPR13P,ZSAZSVFC(,GPR13P)                            0529 00091000
*     RFY                                                          0530 00092000
*      (GPR01P) UNRSTD;                                            0530 00093000
*     RFY                                                          0531 00094000
*      (GPR14P) UNRSTD;                                            0531 00095000
*   END;                                                           0532 00096000
*                                                                  0532 00097000
*/* END TPZSAVE                                                      */ 00098000
*                                                                  0533 00099000
*                                   /* TPIOS ENTRY LINKAGE           */ 00100000
*   ZCRRPTR=CRACRR;                 /* ADDRESS ZCRR                  */ 00101000
         L     @15,RPHCRR(,RPHPTR)                                 0533 00102000
         L     ZCRRPTR,CRACRR(,@15)                                0533 00103000
*   ZCRSCPSW=0;                     /* CLEAR BYTE OF INTERNAL FLAGS  */ 00104000
         MVI   ZCRSCPSW(ZCRRPTR),X'00'                             0534 00105000
*   IF R7->FSBTYPE=FSTYPE THEN                                     0535 00106000
         CLI   FSBTYPE(R7),X'02'                                   0535 00107000
         BNE   @RF00535                                            0535 00108000
*     DO;                           /* IF ELEMENT IS A FSB           */ 00109000
*       FSBPTR=R7;                  /* INITIALIZE PTR TO FSB         */ 00110000
         ST    R7,FSBPTR(,ZCRRPTR)                                 0537 00111000
*       LCPBPTR=FSBLCPB;            /* INITIALIZE PTR TO LCPB        */ 00112000
         L     @09,FSBPTR(,ZCRRPTR)                                0538 00113000
         L     @09,FSBLCPB(,@09)                                   0538 00114000
         ST    @09,LCPBPTR(,ZCRRPTR)                               0538 00115000
*       ZCRFSBWE=ON;                /* SET SWITCH INDICATING FSB     */ 00116000
         OI    ZCRFSBWE(ZCRRPTR),B'00000100'                       0539 00117000
*     END;                          /* END FSB PROCESSING            */ 00118000
*   ELSE                                                           0541 00119000
*     DO;                           /* NOT FSB, MUST BE LCPB         */ 00120000
         B     @RC00535                                            0541 00121000
@RF00535 DS    0H                                                  0542 00122000
*       LCPBPTR=R7;                 /* INITIALIZE LCPB PTR           */ 00123000
         ST    R7,LCPBPTR(,ZCRRPTR)                                0542 00124000
*       RFY                                                        0543 00125000
*        (R7) UNRSTD;               /* NOT REQUIRED                  */ 00126000
*       LCCWPTR=LCPNXLCW;           /* INITIALIZE LCCW POINTER       */ 00127000
         L     @09,LCPBPTR(,ZCRRPTR)                               0544 00128000
         L     @09,LCPNXLCW(,@09)                                  0544 00129000
         ST    @09,LCCWPTR(,ZCRRPTR)                               0544 00130000
         EJECT                                                          00131000
*                                                                  0545 00132000
*/********************************************************************/ 00133000
*/*                                                                  */ 00134000
*/* SEGMENT-                                                         */ 00135000
*/*      ISTZNSES                                                    */ 00136000
*/*                                                                  */ 00137000
*/* FUNCTION-                                                        */ 00138000
*/*      IF A SESSION REJECT REQUEST IS DETECTED,                    */ 00139000
*/*            ALLOCATE A FSB,                                       */ 00140000
*/*            SET MAJOR AND MINOR RETURN CODES TO                   */ 00141000
*/*              LOGIN ERROR - NO SESSION,                           */ 00142000
*/*            POST THE FSB BACK TO THE REQUESTOR,                   */ 00143000
*/*            SET UP FLAGS FOR EXIT.                                */ 00144000
*/*                                                                  */ 00145000
*/********************************************************************/ 00146000
*                                                                  0545 00147000
*ISTZNSES:                                                         0545 00148000
*       DO;                         /* SEGMENT ISTZNSES              */ 00149000
ISTZNSES DS    0H                                                  0546 00150000
*         IF FMCNSALU=ON THEN                                      0546 00151000
         TM    FMCNSALU(FMCBPTR),B'00000100'                       0546 00152000
         BNO   @RF00546                                            0546 00153000
*           DO;                     /* IF SESSION REJECT REQUEST     */ 00154000
*             ZCRPCOD1=0;           /* ZDFA0 PARAMETER               */ 00155000
         MVI   ZCRPCOD1(ZCRRPTR),X'00'                             0548 00156000
*             ZCRPFSB=ON;           /* ZDFA0 PARAMETER               */ 00157000
         OI    ZCRPFSB(ZCRRPTR),B'00100000'                        0549 00158000
*             CALL ISTZDFA0;        /* BUFFER ALLOCATION UTILITY     */ 00159000
         L     @09,ATCZPSVT(,ATCVTPTR)                             0550 00160000
         L     @15,ZPSZDFA0(,@09)                                  0550 00161000
         BALR  @14,@15                                             0550 00162000
*             FSBPTR=RPHSRPRM;      /* ADDRESS NEWLY OBTAINED FSB    */ 00163000
         L     @09,RPHSRPRM(,RPHPTR)                               0551 00164000
         ST    @09,FSBPTR(,ZCRRPTR)                                0551 00165000
*             FSBMJRCD=FRLOGICE;    /* MAJOR = LOGIC ERROR           */ 00166000
         MVI   FSBMJRCD(@09),X'08'                                 0552 00167000
*             FSBMNRCD=FRMUSELE;    /* MINOR = NO SESSION            */ 00168000
         MVI   FSBMNRCD(@09),X'A3'                                 0553 00169000
*             FSBLCPB=LCPBPTR;      /* SET LCPB POINTER IN FSB       */ 00170000
         L     @15,LCPBPTR(,ZCRRPTR)                               0554 00171000
         ST    @15,FSBLCPB(,@09)                                   0554 00172000
*             FSBLCCWA=LCPNXLCW;    /* SET LCCW POINTER IN FSB       */ 00173000
         L     @00,LCPNXLCW(,@15)                                  0555 00174000
         ST    @00,FSBLCCWA(,@09)                                  0555 00175000
*             CALL ISTZFDB;         /* FEEDBACK FSB TO REQUESTOR     */ 00176000
         BAL   @14,ISTZFDB                                         0556 00177000
*             ZCRTPEX='1'B;         /* SET TPEXIT FLAG FOR CALLER    */ 00178000
         OI    ZCRTPEX(ZCRRPTR),B'10000000'                        0557 00179000
*           END;                    /* END SESSION REJECT            */ 00180000
*         ELSE                                                     0559 00181000
*           ;                       /* NOT SESSION REJECT            */ 00182000
@RF00546 DS    0H                                                  0560 00183000
*       END ISTZNSES;               /* END OF SEGMENT                */ 00184000
*/********************************************************************/ 00185000
*/*      SEGMENT ISTZNSES END                                        */ 00186000
*/********************************************************************/ 00187000
*                                                                  0561 00188000
*     END;                          /* END LCPB PROCESSING   @XM00921*/ 00189000
*   IF ZCRTPEX=ON THEN                                             0562 00190000
@RC00535 TM    ZCRTPEX(ZCRRPTR),B'10000000'                        0562 00191000
         BO    @RT00562                                            0562 00192000
*     ;                             /* IF TPEXIT REQUIRED            */ 00193000
*   ELSE                            /* TPEXIT NOT REQUIRED           */ 00194000
*     IF ZCRFSBWE=OFF               /* IF INPUT WAS NOT FSB  @XM00921*/ 00195000
*         &LCCWOP=LSEND THEN        /* OPERATION CODE = SEND         */ 00196000
         TM    ZCRFSBWE(ZCRRPTR),B'00000100'                       0564 00197000
         BNZ   @RF00564                                            0564 00198000
         L     @09,LCCWPTR(,ZCRRPTR)                               0564 00199000
         CLI   LCCWOP(@09),X'F1'                                   0564 00200000
         BNE   @RF00564                                            0564 00201000
         EJECT                                                          00202000
*                                                                  0565 00203000
*/********************************************************************/ 00204000
*/*                                                                  */ 00205000
*/* SEGMENT-                                                         */ 00206000
*/*      ISTZSND                                                     */ 00207000
*/*                                                                  */ 00208000
*/* FUNCTION-                                                        */ 00209000
*/*      HANDLE SESSION CONTROL SEND LCCW OPERATION CODE.            */ 00210000
*/*                                                                  */ 00211000
*/*      FOR RESPONSES, NO ACTION IS TAKEN BY THIS SEGMENT.          */ 00212000
*/*                                                                  */ 00213000
*/*      SESSION INITIATION REQUESTS, (BIND, ACTPU, OR ACTLU)        */ 00214000
*/*            CAUSE THE PABINHBT FLAG SETTING TO BE MADE            */ 00215000
*/*            ACCORDING TO THE DATA FLOW CONTROL LEVEL SPECIFIED.   */ 00216000
*/*                                                                  */ 00217000
*/*      FOR SET AND TEST SEQUENCE NUMBERS,                          */ 00218000
*/*            THE FMCB SEQUENCE NUMBERS ARE UPDATED.                */ 00219000
*/*                                                                  */ 00220000
*/*      IF THE SESSION IS NOT BOUND,                                */ 00221000
*/*      OR IF IN 'PROCESSING CLEAR' STATE,                          */ 00222000
*/*      OR IF IN 'PROCESSING UNBIND' STATE,                         */ 00223000
*/*            THE REQUEST IS REJECTED.                              */ 00224000
*/*                                                                  */ 00225000
*/********************************************************************/ 00226000
*                                                                  0565 00227000
*ISTZSND:                                                          0565 00228000
*       DO;                         /* SEGMENT ISTZSND               */ 00229000
ISTZSND  DS    0H                                                  0566 00230000
*         IF LCCWIDAT=ON THEN       /* IF IMMEDIATE DATA FLAG ON     */ 00231000
         L     @09,LCCWPTR(,ZCRRPTR)                               0566 00232000
         TM    LCCWIDAT(@09),B'00001000'                           0566 00233000
         BNO   @RF00566                                            0566 00234000
*           RHPTR=ADDR(LCCWDATA);   /* LCCW CONTAINS RH DATA         */ 00235000
         LA    @09,LCCWDATA(,@09)                                  0567 00236000
         ST    @09,RHPTR(,ZCRRPTR)                                 0567 00237000
*         ELSE                      /* IF IMMEDIATE DATA FLAG OFF    */ 00238000
*           RHPTR=LCCWDATA;         /* LCCW ADDRESSES RH DATA        */ 00239000
         B     @RC00566                                            0568 00240000
@RF00566 L     @09,LCCWPTR(,ZCRRPTR)                               0568 00241000
         L     @09,LCCWDATA(,@09)                                  0568 00242000
         ST    @09,RHPTR(,ZCRRPTR)                                 0568 00243000
*         IF LCCWCD=OFF THEN        /* IF NOT DATA CHAINED           */ 00244000
@RC00566 L     @09,LCCWPTR(,ZCRRPTR)                               0569 00245000
         TM    LCCWCD(@09),B'10000000'                             0569 00246000
         BNZ   @RF00569                                            0569 00247000
*           SRUPTR=RHPTR+3;         /* SRU IS IMMEDIATELY ADDRESSABLE*/ 00248000
         LA    @09,3                                               0570 00249000
         AL    @09,RHPTR(,ZCRRPTR)                                 0570 00250000
         ST    @09,SRUPTR(,ZCRRPTR)                                0570 00251000
*         ELSE                                                     0571 00252000
*           DO;                     /* IF DATA CHAINED               */ 00253000
         B     @RC00569                                            0571 00254000
@RF00569 DS    0H                                                  0572 00255000
*             K=1;                  /* START AT BEGIN OF SRUCOPY     */ 00256000
         LA    @09,1                                               0572 00257000
         LR    K,@09                                               0572 00258000
*             SRUPTR=ADDR(SRUCOPY); /* SET RUPOINTER TO POINT TO   0573 00259000
*                                      COPIED RU                     */ 00260000
         LA    @00,SRUCOPY(,ZCRRPTR)                               0573 00261000
         ST    @00,SRUPTR(,ZCRRPTR)                                0573 00262000
*             IF LCCWIDAT=ON        /* IF IMMEDIATE DATA             */ 00263000
*                 &LCCWCNT=4 THEN                                  0574 00264000
         L     @15,LCCWPTR(,ZCRRPTR)                               0574 00265000
         TM    LCCWIDAT(@15),B'00001000'                           0574 00266000
         BNO   @RF00574                                            0574 00267000
         CLC   LCCWCNT(2,@15),@CH00052                             0574 00268000
         BNE   @RF00574                                            0574 00269000
*               DO;                 /* & LENGTH = 4                  */ 00270000
*                 RUPTRC=ADDR(LCCWDATA);/* SET RUPTR = FIRST RU BYTE */ 00271000
         LA    @00,LCCWDATA(,@15)                                  0576 00272000
         ST    @00,RUPTRC(,ZCRRPTR)                                0576 00273000
*                 SRUCOPY(K)=DATALCCW(4);/* COPY 1 BYTE OF RU        */ 00274000
         LR    @15,ZCRRPTR                                         0577 00275000
         ALR   @15,K                                               0577 00276000
         LR    @14,@00                                             0577 00277000
         MVC   SRUCOPY-1(1,@15),DATALCCW+3(@14)                    0577 00278000
*                 K=K+1;            /* UPDATE INDEX INTO SRUCOPY     */ 00279000
         ALR   K,@09                                               0578 00280000
*               END;                /* END IMMEDIATE 4 BYTE DATA     */ 00281000
*             ELSE                  /* NOT IMMEDIATE 4 BYTE DATA     */ 00282000
*               IF LCCWCNT>3 THEN                                  0580 00283000
         B     @RC00574                                            0580 00284000
@RF00574 L     @09,LCCWPTR(,ZCRRPTR)                               0580 00285000
         LH    @15,LCCWCNT(,@09)                                   0580 00286000
         N     @15,@CF04247                                        0580 00287000
         C     @15,@CF00287                                        0580 00288000
         BNH   @RF00580                                            0580 00289000
*                 DO;               /* IF LENGTH .GT. 3              */ 00290000
*                   RUPTRC=ADDR(LCCWDATA)+3;/* POINT TO PART OF RU IN   00291000
*                                      DATA AREA             @XM08890*/ 00292000
         LA    @14,3                                               0582 00293000
         LA    @09,LCCWDATA(,@09)                                  0582 00294000
         ALR   @09,@14                                             0582 00295000
         ST    @09,RUPTRC(,ZCRRPTR)                                0582 00296000
*                   SRUCOPY(1:MIN((LCCWCNT-3),6))=DATALCCW(1:(LCCWCNT-3 00297000
*                       ));         /* COPY LCCW DATA TO SRU         */ 00298000
         SLR   @15,@14                                             0583 00299000
         LA    @14,6                                               0583 00300000
         CR    @14,@15                                             0583 00301000
         BNH   *+6                                                      00302000
         LR    @14,@15                                             0583 00303000
         BCTR  @14,0                                               0583 00304000
         EX    @14,@SM04248                                        0583 00305000
*                   K=K+(LCCWCNT-3);/* DECREMENT DATA COUNTER        */ 00306000
         ALR   K,@15                                               0584 00307000
*                 END;              /* END LENGTH .GT. 3             */ 00308000
*               ELSE                                               0586 00309000
*                 ;                 /* IF LENGTH NOT .GT. 3          */ 00310000
@RF00580 DS    0H                                                  0587 00311000
*             DO WHILE(LCCWPTR^=0&K<7);/* COPY 6 BYTES OF RU DATA    */ 00312000
@RC00574 B     @DE00587                                            0587 00313000
@DL00587 DS    0H                                                  0588 00314000
*               LCCWPTR=LCCWPTR+LENGTH(ISTLCCW);/* INCREMENT TO NEXT    00315000
*                                      LCCW IN CHAIN                 */ 00316000
         LA    @09,8                                               0588 00317000
         AL    @09,LCCWPTR(,ZCRRPTR)                               0588 00318000
         ST    @09,LCCWPTR(,ZCRRPTR)                               0588 00319000
*               IF LCCWOP=LTIC THEN /* IF OPCODE = LTIC              */ 00320000
         CLI   LCCWOP(@09),X'08'                                   0589 00321000
         BNE   @RF00589                                            0589 00322000
*                 LCCWPTR=LCCWDATA; /* GET NEXT LCCW ADDRESS         */ 00323000
         L     @09,LCCWDATA(,@09)                                  0590 00324000
         ST    @09,LCCWPTR(,ZCRRPTR)                               0590 00325000
*               ELSE                                               0591 00326000
*                 ;                 /* OPCODE .NE. LTIC              */ 00327000
@RF00589 DS    0H                                                  0592 00328000
*               IF LCCWIDAT=ON THEN /* IF IMMEDIATE DATA             */ 00329000
         L     @09,LCCWPTR(,ZCRRPTR)                               0592 00330000
         TM    LCCWIDAT(@09),B'00001000'                           0592 00331000
         BNO   @RF00592                                            0592 00332000
*                 RUPTRC=ADDR(LCCWDATA);/* ADDRESS DATA IN LCCW      */ 00333000
         LA    @09,LCCWDATA(,@09)                                  0593 00334000
         ST    @09,RUPTRC(,ZCRRPTR)                                0593 00335000
*               ELSE                /* IF NOT IMMEDIATE DATA         */ 00336000
*                 RUPTRC=LCCWDATA;  /* ADDRESS DATA FROM LCCW        */ 00337000
         B     @RC00592                                            0594 00338000
@RF00592 L     @09,LCCWPTR(,ZCRRPTR)                               0594 00339000
         L     @09,LCCWDATA(,@09)                                  0594 00340000
         ST    @09,RUPTRC(,ZCRRPTR)                                0594 00341000
*               IF LCCWCNT>0        /* IF ANY ASSOCIATED DATA        */ 00342000
*                   &K<7 THEN                                      0595 00343000
@RC00592 L     @09,LCCWPTR(,ZCRRPTR)                               0595 00344000
         LH    @09,LCCWCNT(,@09)                                   0595 00345000
         N     @09,@CF04247                                        0595 00346000
         LTR   @09,@09                                             0595 00347000
         BNP   @RF00595                                            0595 00348000
         C     K,@CF00714                                          0595 00349000
         BNL   @RF00595                                            0595 00350000
*                 DO;               /* & STILL MOVING DATA           */ 00351000
*                   SRUCOPY(K:MIN((K+(LCCWCNT-1)),6))=DATALCCW(1:  0597 00352000
*                       LCCWCNT);   /* COPY NUMBER OF RU BYTES     0597 00353000
*                                      INDICATED BY COUNT            */ 00354000
         LR    @15,ZCRRPTR                                         0597 00355000
         ALR   @15,K                                               0597 00356000
         LR    @14,@09                                             0597 00357000
         BCTR  @14,0                                               0597 00358000
         ALR   @14,K                                               0597 00359000
         LA    @11,6                                               0597 00360000
         CR    @14,@11                                             0597 00361000
         BNH   *+6                                                      00362000
         LR    @14,@11                                             0597 00363000
         SLR   @14,K                                               0597 00364000
         L     @11,RUPTRC(,ZCRRPTR)                                0597 00365000
         EX    @14,@SM04250                                        0597 00366000
*                   K=K+LCCWCNT;    /* UPDATE INDEX INTO COPY FIELD  */ 00367000
         ALR   K,@09                                               0598 00368000
*                 END;              /* END DATA MOVE                 */ 00369000
*               ELSE                                               0600 00370000
*                 ;                 /* IF NOT MOVING DATA            */ 00371000
@RF00595 DS    0H                                                  0601 00372000
*               IF LCCWCD=OFF THEN  /* IF NOT DATA CHAINED           */ 00373000
         L     @09,LCCWPTR(,ZCRRPTR)                               0601 00374000
         TM    LCCWCD(@09),B'10000000'                             0601 00375000
         BNZ   @RF00601                                            0601 00376000
*                 LCCWPTR=0;        /* CLEAR LCCW POINTER            */ 00377000
         SLR   @09,@09                                             0602 00378000
         ST    @09,LCCWPTR(,ZCRRPTR)                               0602 00379000
*               ELSE                                               0603 00380000
*                 ;                 /* IF DATA CHAINED               */ 00381000
@RF00601 DS    0H                                                  0604 00382000
*             END;                  /* END DATA COPY LOOP            */ 00383000
@DE00587 L     @09,LCCWPTR(,ZCRRPTR)                               0604 00384000
         LTR   @09,@09                                             0604 00385000
         BZ    @DC00587                                            0604 00386000
         C     K,@CF00714                                          0604 00387000
         BL    @DL00587                                            0604 00388000
@DC00587 DS    0H                                                  0605 00389000
*             LCCWPTR=LCPNXLCW;     /* RESTORE LCCW POINTER          */ 00390000
         L     @09,LCPBPTR(,ZCRRPTR)                               0605 00391000
         L     @09,LCPNXLCW(,@09)                                  0605 00392000
         ST    @09,LCCWPTR(,ZCRRPTR)                               0605 00393000
*           END;                    /* END DATA CHAINING             */ 00394000
*         RFY                                                      0607 00395000
*           K UNRSTD;                                              0607 00396000
@RC00569 DS    0H                                                  0608 00397000
*         IF RHQS=OFF THEN          /* IF REQUEST                    */ 00398000
         L     @09,RHPTR(,ZCRRPTR)                                 0608 00399000
         TM    RHQS(@09),B'10000000'                               0608 00400000
         BNZ   @RF00608                                            0608 00401000
*           ZCRLREQ=ON;             /* SET REQUEST PROCESS FLAG      */ 00402000
         OI    ZCRLREQ(ZCRRPTR),B'00010000'                        0609 00403000
*         ELSE                                                     0610 00404000
*           ;                       /* IF RESPONSE                   */ 00405000
@RF00608 DS    0H                                                  0611 00406000
*         IF(SRUQSREQ=SRACTPU       /* IF ACTIVATE PHYSICAL          */ 00407000
