*                     FMCNSALU=ON;  /* NO SESSION EXISTS             */ 00923000
         OI    FMCNSALU(FMCBPTR),B'00000100'                       0773 00924000
*                   END;            /* END CLEAR UNBIND NOTIFY       */ 00925000
*                 ELSE                                             0775 00926000
