@RF00684 DS    0H                                                  0694 00675000
*                 ZCRTPINV=ON;      /* SET INVOKE TPIOS FLAG         */ 00676000
         OI    ZCRTPINV(ZCRRPTR),B'01000000'                       0694 00677000
*               END;                /* END NOT INVALID TERMINATION   */ 00678000
*       END ISTZSND;                /* END OF SEGMENT ISTZSND        */ 00679000
*/********************************************************************/ 00680000
*/*      SEGMENT ISTZSND END                                         */ 00681000
*/********************************************************************/ 00682000
*                                                                  0697 00683000
*     ELSE                          /* IF NOT SEND OPERATION CODE    */ 00684000
         EJECT                                                          00685000
*                                                                  0697 00686000
*/********************************************************************/ 00687000
*/*                                                                  */ 00688000
*/* SEGMENT-                                                         */ 00689000
*/*      ISTZTRM                                                     */ 00690000
*/*                                                                  */ 00691000
*/* FUNCTION-                                                        */ 00692000
*/*      THIS SEGMENT HANDLES REQUESTS OTHER THAN NORMAL SENDS.      */ 00693000
*/*                                                                  */ 00694000
*/*      IF THE WORK ELEMENT IS A FSB AND THE OPCODE IS CLEAR,       */ 00695000
*/*            THE FSB IS PASSED TO THE REQUESTOR.                   */ 00696000
*/*                                                                  */ 00697000
*/*            IF IT WAS A MULTIPLE REQUEST,                         */ 00698000
*/*            THE UNBIND FSB IS RETURNED,                           */ 00699000
*/*            NOTIFY IS DRIVEN (IF REQUIRED),               @XM19747*/ 00700000
*/*            AND THE CLEAR FSB IS FREED.                           */ 00701000
*/*                                                                  */ 00702000
*/*            OTHERWISE,                                            */ 00703000
*/*            NOTIFY IS DRIVEN (IF REQUIRED), AND           @XM19747*/ 00704000
*/*            THE CLEAR FSB IS PASSED BACK TO THE REQUESTOR.        */ 00705000
*/*                                                                  */ 00706000
*/*      IF IT WAS NOT A CLEAR FSB,                                  */ 00707000
*/*            A CHECK IS MADE TO SEE IF IT IS A CLSDEST FINISH      */ 00708000
*/*            REQUEST.                                              */ 00709000
*/*            IF IT IS A CLSDEST FINISH REQUEST,                    */ 00710000
*/*            A FSB IS OBTAINED AND FORMATTED BACK TO THE REQUESTOR.*/ 00711000
*/*                                                                  */ 00712000
*/*      IF IT IS NONE OF THE ABOVE CONDITIONS,                      */ 00713000
*/*            IF THE SESSION IS BOUND                               */ 00714000
*/*            AND NOT PROCESSING CLEAR OR UNBIND,                   */ 00715000
*/*            SEGMENT ISTZCLR IS INVOKED.                           */ 00716000
*/*                                                                  */ 00717000
*/********************************************************************/ 00718000
*                                                                  0697 00719000
*ISTZTRM:                                                          0697 00720000
*       DO;                         /* SEGMENT ISTZTRM               */ 00721000
         B     @RC00564                                            0697 00722000
@RF00564 DS    0H                                                  0697 00723000
ISTZTRM  DS    0H                                                  0698 00724000
*         IF ZCRFSBWE=ON THEN                                      0698 00725000
         TM    ZCRFSBWE(ZCRRPTR),B'00000100'                       0698 00726000
         BNO   @RF00698                                            0698 00727000
*           DO;                     /* IF FSB IS PRESENT             */ 00728000
*             IF LCPTLBPI=ON THEN                                  0700 00729000
         L     @09,LCPBPTR(,ZCRRPTR)                               0700 00730000
         TM    LCPTLBPI(@09),B'00010000'                           0700 00731000
         BNO   @RF00700                                            0700 00732000
*               DO;                 /* IF TLB IS PRESENT IT IS A   0701 00733000
*                                      MULTIPLE REQUEST              */ 00734000
*                 ZCRPCOD1=0;       /* ZDFB0 PARAMETER               */ 00735000
         MVI   ZCRPCOD1(ZCRRPTR),X'00'                             0702 00736000
*                 ZCRPBUF=ON;       /* ZDFB0 PARAMETER               */ 00737000
         OI    ZCRPBUF(ZCRRPTR),B'00000100'                        0703 00738000
*                 RFY                                              0704 00739000
*                  (R7) RSTD;       /* ZDFB0 PARAMETER REGISTER      */ 00740000
*                 R7=ADDR(FSBFDB)-FDBOFSET;/* ZDFB0 PARAMETER -    0705 00741000
*                                      ADDRESS                       */ 00742000
         L     @09,FSBPTR(,ZCRRPTR)                                0705 00743000
         LA    R7,FSBFDB(,@09)                                     0705 00744000
         SH    R7,FDBOFSET+32(,@09)                                0705 00745000
*                 R7->ZIBCHAIN=0;   /* ZERO BUFFER CHAIN POINTER     */ 00746000
         SLR   @09,@09                                             0706 00747000
         ST    @09,ZIBCHAIN(,R7)                                   0706 00748000
*                 CALL ISTZDFB0;    /* BUFFER RELEASE UTILITY        */ 00749000
         L     @09,ATCZPSVT(,ATCVTPTR)                             0707 00750000
         L     @15,ZPSZDFB0(,@09)                                  0707 00751000
         BALR  @14,@15                                             0707 00752000
*                 RFY                                              0708 00753000
*                  (R7) UNRSTD;     /* ZDFB0 PARAMETER REGISTER      */ 00754000
*                 TLBPTR=LCPTLBAD;  /* POINT TO BEGINNING OF TLB   0709 00755000
*                                                            @XM00921*/ 00756000
         L     @09,LCPBPTR(,ZCRRPTR)                               0709 00757000
         L     @07,LCPTLBAD(,@09)                                  0709 00758000
         ST    @07,TLBPTR(,ZCRRPTR)                                0709 00759000
*                 LCPNXLCW=TLBLCPA; /* RESTORE LCCW ADDRESS          */ 00760000
         L     @00,TLBLCPA(,@07)                                   0710 00761000
         ST    @00,LCPNXLCW(,@09)                                  0710 00762000
*                 FSBPTR=TLBFSBA;   /* POINT TO UNBIND FSB           */ 00763000
         L     @00,TLBFSBA(,@07)                                   0711 00764000
         ST    @00,FSBPTR(,ZCRRPTR)                                0711 00765000
*                 LCPTLBAD=0;       /* CLEAR TLB POINTER             */ 00766000
         SLR   @00,@00                                             0712 00767000
         ST    @00,LCPTLBAD(,@09)                                  0712 00768000
*                 LCPTLBPI=OFF;     /* CLEAR TLB FLAG                */ 00769000
         NI    LCPTLBPI(@09),B'11101111'                           0713 00770000
*                 TLBPTR=TLBPTR-LENGTH(ISTBFHDR);/* POINT BACK TO  0714 00771000
*                                      BUFFER ORIGIN                 */ 00772000
*                                                                  0714 00773000
         SL    @07,@CF00058                                        0714 00774000
         ST    @07,TLBPTR(,ZCRRPTR)                                0714 00775000
*                 /***************************************************/ 00776000
*                 /*                                                 */ 00777000
*                 /* RELSTORE BUFADDR(TLBPTR)                        */ 00778000
*                 /*                                                 */ 00779000
*                 /***************************************************/ 00780000
*                                                                  0715 00781000
*                 DO;                                              0715 00782000
*                                                                  0715 00783000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00784000
*/* END OF ATBUILD                                                   */ 00785000
*                                                                  0716 00786000
*                   DCL                                            0716 00787000
*                     GPR00F FIXED(31) REG(00);                    0716 00788000
*                   DCL                                            0717 00789000
