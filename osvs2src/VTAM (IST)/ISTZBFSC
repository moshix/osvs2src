@GL00005 TM    ZCRLREQ(ZCRRPTR),B'00010000'                        0611 00415000
         BNO   @RF00611                                            0611 00416000
*           DO;                     /* REQUEST                       */ 00417000
*             FMCOSSN=0;            /* ZERO OUTBOUND SYNCH COUNTER   */ 00418000
         SLR   @09,@09                                             0613 00419000
         STH   @09,FMCOSSN(,FMCBPTR)                               0613 00420000
*             FMCISSN=0;            /* ZERO INBOUND SYNCH COUNTER    */ 00421000
         STH   @09,FMCISSN(,FMCBPTR)                               0614 00422000
