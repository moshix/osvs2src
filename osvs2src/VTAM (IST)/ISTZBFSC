         STH   @09,FMCOASN(,FMCBPTR)                               0615 00424000
*           END;                    /* END ACTPU OR BIND REQUEST     */ 00425000
*         ELSE                                                     0617 00426000
*           ;                       /* NOT ACTPU OR BIND REQUEST     */ 00427000
@RF00611 DS    0H                                                  0618 00428000
*         IF SRUQSREQ=SRBIND        /* IF BIND REQUEST       @XM08711*/ 00429000
*             &ZCRLREQ=ON THEN                                     0618 00430000
         L     @09,SRUPTR(,ZCRRPTR)                                0618 00431000
         CLI   SRUQSREQ(@09),X'31'                                 0618 00432000
