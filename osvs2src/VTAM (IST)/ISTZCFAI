*         END;                                                     0470 00288000
*       IF FMCBHSET=DNCBHSET THEN                                  0471 00289000
@RT00465 CLC   FMCBHSET(1,FMCBREG),DNCBHSET(DNCBREG)               0471 00290000
         BE    @RT00471                                            0471 00291000
*         ;                         /* IF FMCB AND DNCB BLOCK HANDLRS   00292000
*                                      SAME GO RETURN                */ 00293000
*       ELSE                                                       0473 00294000
*         DO;                                                      0473 00295000
*           CNTLPREG=1;             /* INDICATE BHSET MODIFIER       */ 00296000
         LA    CNTLPREG,1                                          0474 00297000
*           CALL CFAI0021;          /* GO BUILD CNTRL CMND-BHSET     */ 00298000
         BAL   @14,CFAI0021                                        0475 00299000
*         END;                                                     0476 00300000
*     END CFAIMDBH;                                                0477 00301000
*                                                                  0477 00302000
@RT00471 DS    0H                                                  0478 00303000
*/* SEGMENT(CFAI020)FINISH                                           */ 00304000
*                                                                  0478 00305000
*   DO;                                                            0478 00306000
@RC00462 DS    0H                                                  0479 00307000
*     GOTO CFAI020#;                                               0479 00308000
         B     CFAI020#                                            0479 00309000
*CFAI020@:                                                         0480 00310000
*   END;                                                           0480 00311000
