*   GEN(EJECT);                                                    0481 00313000
         EJECT                                                          00314000
*                                                                  0482 00315000
*/********************************************************************/ 00316000
*/*                                                                  */ 00317000
*/* CFAI021 -                                                        */ 00318000
*/*        THIS SUBROUTINE BUILDS A PIU TO EITHER SET THE DEVICE     */ 00319000
*/*      MODE OR SET THE BH SET. CNTLPREG IS THE INPUT PARAMETER     */ 00320000
*/*      EXPECTED WHICH SPECIFIES WHICH TO DO.                       */ 00321000
*/*                IF CNTLPREG=0, THE DEVICE MODE IS SET,            */ 00322000
*/*                OTHERWISE THE BH SET IS SET.                      */ 00323000
*/*      THE LSCB ALLOCATED WITH THE BUFFER FOR THE PIU BUILT IS     */ 00324000
*/*      QUEUED ON THE FMCB SCHEDQ AND A DO-NOT-PROCEED RETURN CODE  */ 00325000
*/*      (=4) IS SET FOR THE CALLER OF THIS SUBROUTINE.              */ 00326000
*/*                                                                  */ 00327000
*/********************************************************************/ 00328000
*                                                                  0482 00329000
*CFAI0021:                                                         0482 00330000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0482 00331000
         B     @PB00002                                            0482 00332000
CFAI0021 DS    0H                                                  0483 00333000
*   RFY                                                            0483 00334000
*     RTNREG RSTD;                                                 0483 00335000
*   ZCRSVF6=RTNREG;                                                0484 00336000
         L     @15,RPHCRR(,RPHREG)                                 0484 00337000
         L     @15,CRACRR(,@15)                                    0484 00338000
         ST    RTNREG,ZCRSVF6(,@15)                                0484 00339000
*   RFY                                                            0485 00340000
*     RTNREG UNRSTD;                                               0485 00341000
*   ZCRPCOD1=0;                     /* ZERO PARM FLAGS               */ 00342000
         MVI   ZCRPCOD1(@15),X'00'                                 0486 00343000
*   ZCRPLSBF=ON;                    /* SET PARM= GET LSCB&BUFF       */ 00344000
         OI    ZCRPLSBF(@15),B'00010000'                           0487 00345000
*   CALL ATCZPSVT->ZPSZDFA0;        /* GO GET LSCB WITH BUFF         */ 00346000
         L     @14,ATCZPSVT(,VTCVTREG)                             0488 00347000
         L     @15,ZPSZDFA0(,@14)                                  0488 00348000
         BALR  @14,@15                                             0488 00349000
*   FAISRPRM->LSCLCPBA=ZCRCRLCP;    /* INIT LSCB LCPB ADDR           */ 00350000
         L     @15,FAISRPRM(,RPHREG)                               0489 00351000
         L     @14,RPHCRR(,RPHREG)                                 0489 00352000
         L     @14,CRACRR(,@14)                                    0489 00353000
         L     @14,ZCRCRLCP(,@14)                                  0489 00354000
         ST    @14,LSCLCPBA(,@15)                                  0489 00355000
*   FAISRPRM->LSCLCCWA=ZCRCRLCP->LCPNXLCW;/* INIT LSCB LCCW ADDR     */ 00356000
         L     @14,LCPNXLCW(,@14)                                  0490 00357000
         ST    @14,LSCLCCWA(,@15)                                  0490 00358000
*   RFY                                                            0491 00359000
*     ISTZOBUF BASED(FAISRPRM->LSCFDBA);/* BASE ZOB                  */ 00360000
*   RFY                                                            0492 00361000
*     ISTFDB BASED(ADDR(ZOBFDB));   /* BASE ZOBFDB                   */ 00362000
*   ZOBLNG2=ZOBLNG1;                /* INIT                          */ 00363000
         L     @15,LSCFDBA(,@15)                                   0493 00364000
         LH    @14,ZOBLNG1(,@15)                                   0493 00365000
         N     @14,@CF03389                                        0493 00366000
         STH   @14,ZOBLNG2(,@15)                                   0493 00367000
*   FDBDATAA=ADDR(ZOBBTU);          /* THE ZOB FDB                   */ 00368000
         LA    @14,ZOBBTU(,@15)                                    0494 00369000
         ST    @14,FDBDATAA(,@15)                                  0494 00370000
*   BTUREG=ADDR(ZOBBTU);            /* GET BTU PTR                   */ 00371000
         LR    BTUREG,@14                                          0495 00372000
*   BTUFID=THSET;                   /* INIT TH BYTES 1&2     @XL03HAH*/ 00373000
         MVC   BTUFID(2,BTUREG),@CB03301                           0496 00374000
*   BTURH=0;                        /* INIT REQUST HEADR     @XL03HAH*/ 00375000
         SLR   @14,@14                                             0497 00376000
         STCM  @14,15,BTURH(BTUREG)                                0497 00377000
*   BTUSORCE=FMCSAF;                /* INIT BTU SAF                  */ 00378000
         MVC   BTUSORCE(2,BTUREG),FMCSAF(FMCBREG)                  0498 00379000
*   BTUDEST=NCBDAF;                 /* DAF                           */ 00380000
         MVC   BTUDEST(2,BTUREG),NCBDAF(DNCBREG)                   0499 00381000
*   BTUZRT03=ON;                    /* SET ON LOOK AT FMCB LNE ST    */ 00382000
         OI    BTUZRT03(BTUREG),B'00010000'                        0500 00383000
*   IF PROCFTX=ON THEN                                             0501 00384000
         TM    PROCFTX+48(FMCBREG),B'00000001'                     0501 00385000
         BNO   @RF00501                                            0501 00386000
*     ZOBRT06=ON;                   /* COPY CRIT TEXT FLAG   @XM09248*/ 00387000
         OI    ZOBRT06(@15),B'10000000'                            0502 00388000
*   IF NCBFTRIO=ON THEN                                            0503 00389000
@RF00501 CLI   NCBFTRIO(DNCBREG),1                                 0503 00390000
         BNE   @RF00503                                            0503 00391000
*     ZOBRT08=ON;                   /* COPY I/O TRACE FLAG   @XM09248*/ 00392000
         L     @15,FAISRPRM(,RPHREG)                               0504 00393000
         L     @15,LSCFDBA(,@15)                                   0504 00394000
         OI    ZOBRT08(@15),B'01000000'                            0504 00395000
*   DCL                                                            0505 00396000
*     SEQ1FMC DEFINED FMCSEQ1 CHAR(1);                             0505 00397000
@RF00503 DS    0H                                                  0506 00398000
*   MVN(BTUREQ(2),SEQ1FMC);         /* SET REQTAG IN BTU             */ 00399000
         MVN   BTUREQ+1(1,BTUREG),SEQ1FMC(FMCBREG)                 0506 00400000
*   FMCSEQ1=FMCSEQ1+1;              /* UP FMCB SEQUENCE COUNT        */ 00401000
         LA    @15,1                                               0507 00402000
         SLR   @14,@14                                             0507 00403000
