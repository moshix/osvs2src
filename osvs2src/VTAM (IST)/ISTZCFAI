*   GEN(EJECT);                                                    0458 00237000
         EJECT                                                          00238000
*                                                                  0459 00239000
*/********************************************************************/ 00240000
*/*                                                                  */ 00241000
*/* CFAI020 -                                                        */ 00242000
*/*      THIS SEGMENT COMPARES THE MODE AND BH SET IN THE 370X WITH  */ 00243000
*/*    THOSE SELECTED BY THE APPLICATION AND CALL CFAI021 TO HANDLE  */ 00244000
*/*    THE CASES WHEN EITHER BH SET OR MODE DO NOT AGREE.            */ 00245000
*/*    A DO-NOT-START-I/O RETURN CODE (8) IS SET (AND CFAI021 IS NOT */ 00246000
*/*    CALLED) IF THE DNCB DIALOG SCHEDULING FLAG IS ON OR THE FMCB  */ 00247000
*/*    TOP PABERLOK IS ON.                                           */ 00248000
*/*                                                                  */ 00249000
*/********************************************************************/ 00250000
*/* SEGMENT(CFAI020)START                                            */ 00251000
*                                                                  0459 00252000
*   DO;                                                            0459 00253000
*     GOTO CFAI020@;                                               0460 00254000
         B     CFAI020@                                            0460 00255000
*CFAI020:                                                          0461 00256000
*   END;                                                           0461 00257000
CFAI020  DS    0H                                                  0462 00258000
*   IF DNCZF002=ON|                 /* IF DIAL SCHED OR              */ 00259000
*       ADDR(FMCPAB3)->PABERLOK=ON THEN/* IF ERR LOCK ON THEN        */ 00260000
         TM    DNCZF002(DNCBREG),B'00000010'                       0462 00261000
         BO    @RT00462                                            0462 00262000
         TM    PABERLOK+96(FMCBREG),B'00000100'                    0462 00263000
         BNO   @RF00462                                            0462 00264000
@RT00462 DS    0H                                                  0463 00265000
*     ZCRRCDSV=DONTSIO;             /* SET RC= DON'T START I/O       */ 00266000
         L     @15,RPHCRR(,RPHREG)                                 0463 00267000
         L     @15,CRACRR(,@15)                                    0463 00268000
         MVI   ZCRRCDSV(@15),X'08'                                 0463 00269000
*   ELSE                            /* CHECK MODES                   */ 00270000
*CFAIMDBH:                                                         0464 00271000
*     DO;                           /* & BHSET                       */ 00272000
         B     @RC00462                                            0464 00273000
@RF00462 DS    0H                                                  0464 00274000
CFAIMDBH DS    0H                                                  0465 00275000
*       IF DNCMODE(1:3)=PROSETMS(1:3) THEN                         0465 00276000
         CLC   DNCMODE(3,DNCBREG),PROSETMS+48(FMCBREG)             0465 00277000
         BE    @RT00465                                            0465 00278000
*         ;                         /* IF FMCB AND DNCB HAVE SAME DEV   00279000
*                                      MODES THEN GO CK BHSET      0466 00280000
*                                                            @XM10465*/ 00281000
*       ELSE                        /* MODES NOT SAME                */ 00282000
*         DO;                                                      0467 00283000
*           CNTLPREG=0;             /* INDICATE SET MODE MODIFIER    */ 00284000
