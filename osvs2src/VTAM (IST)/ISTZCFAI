*/*                                                                  */ 00196000
*/* CFAI010 -                                                        */ 00197000
*/*      THIS SEGMENT IS AN ERROR HANDLER. IT TURNS OFF THE DNCB     */ 00198000
*/*    DIALOG SCHEDULING AND LOGICAL ERROR FLAGS, SETS LOGICAL       */ 00199000
*/*    ERROR IN LCPB , CALLS QUE TRANSFER AND RETURNS IN ZCRRCDSV    */ 00200000
*/*    A DO-NOT-PROCEED RETURN CODE (4).                             */ 00201000
*/*                                                                  */ 00202000
*/********************************************************************/ 00203000
*/* SEGMENT(CFAI010)START                                            */ 00204000
*                                                                  0445 00205000
*   DO;                                                            0445 00206000
*     GOTO CFAI010@;                                               0446 00207000
         B     CFAI010@                                            0446 00208000
*CFAI010:                                                          0447 00209000
*   END;                                                           0447 00210000
CFAI010  DS    0H                                                  0448 00211000
*   DNCZF002=OFF;                   /* TURN OFF DIALOG SCHED FLAG    */ 00212000
*   DNCZF003=OFF;                   /* TURN OFF ERR FLAG             */ 00213000
         NI    DNCZF002(DNCBREG),B'11111100'                       0449 00214000
*   LCPZRT00=ON;                    /* SET LOGICAL ERROR     @XM02347*/ 00215000
         OI    LCPZRT00(ELEMREG),B'10000000'                       0450 00216000
*   WKREG=ADDR(FMCPAB3);            /*                       @XM09808*/ 00217000
         LA    WKREG,FMCPAB3(,FMCBREG)                             0451 00218000
*   CNTLPREG=ADDR(FMCSHEDQ);        /*                       @XM09808*/ 00219000
         LA    CNTLPREG,FMCSHEDQ(,FMCBREG)                         0452 00220000
*   CALL ATCZPSVT->ZPSZDFH0;        /*                       @XM09808*/ 00221000
         L     @14,ATCZPSVT(,VTCVTREG)                             0453 00222000
         L     @15,ZPSZDFH0(,@14)                                  0453 00223000
         BALR  @14,@15                                             0453 00224000
*   ZCRRCDSV=DONTPROC;              /* SET RET CODE: DO NOT PROCEED  */ 00225000
         L     @15,RPHCRR(,RPHREG)                                 0454 00226000
         L     @15,CRACRR(,@15)                                    0454 00227000
         MVI   ZCRRCDSV(@15),X'04'                                 0454 00228000
*/* SEGMENT(CFAI010)FINISH                                           */ 00229000
*                                                                  0455 00230000
*   DO;                                                            0455 00231000
*     GOTO CFAI010#;                                               0456 00232000
