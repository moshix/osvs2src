         LA    @14,0(,@14)                                         0370 00296000
         LTR   @14,@14                                             0370 00297000
         BZ    @RT00370                                            0370 00298000
*       ;                           /* IF LSCB HAS NO BUFFER GOTO    */ 00299000
*     ELSE                          /* ELSE                          */ 00300000
*       DO;                         /* DO BUFF CHAINING              */ 00301000
*/**** BUILD BUFFER CHAIN ********************************************/ 00302000
*                                                                  0373 00303000
*         IF CURBUFRG=0 THEN        /* IF THIS IS 1ST BUFF FOUND     */ 00304000
         LTR   CURBUFRG,CURBUFRG                                   0373 00305000
