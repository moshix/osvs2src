*     ;                             /* IF VALCHECK SUCCESSFUL        */ 01067000
*   ELSE                            /* IF VALCHECK FAILED            */ 01068000
*     RTNCD=12;                     /* ERROR RETURN CODE     @XL03HAH*/ 01069000
*                                                                  0785 01070000
         LA    RTNCD,12                                            0785 01071000
*   /*****************************************************************/ 01072000
*   /*                                                               */ 01073000
*   /* SEGMENT(VALIDAT)FINISH                                        */ 01074000
*   /*                                                               */ 01075000
