VALIDAT@ DS    0H                                                  0789 01084000
         EJECT                                                          01085000
*                                                                  0789 01086000
*/********************************************************************/ 01087000
*/*                                                                  */ 01088000
*/* SEGMENT-                                                         */ 01089000
*/*      CHKLCCW                                                     */ 01090000
*/*                                                                  */ 01091000
*/* FUNCTION-                                                        */ 01092000
*/*      LOCATES THE NEXT NON-TIC LCCW.                              */ 01093000
*/*      LSCLCCWA IS SET TO THE ADDRESS OF THE NEXT NON-TIC LCCW.    */ 01094000
*/*                                                                  */ 01095000
*/********************************************************************/ 01096000
*                                                                  0789 01097000
*   /*****************************************************************/ 01098000
*   /*                                                               */ 01099000
*   /* SEGMENT(CHKLCCW)START                                         */ 01100000
*   /*                                                               */ 01101000
*   /*****************************************************************/ 01102000
*                                                                  0789 01103000
*   DO;                                                            0789 01104000
*     GOTO CHKLCCW@;                                               0790 01105000
         B     CHKLCCW@                                            0790 01106000
*CHKLCCW:                                                          0791 01107000
*   END;                            /* CHKLCCW: SUBROUTINE           */ 01108000
CHKLCCW  DS    0H                                                  0792 01109000
*   RFY                                                            0792 01110000
*     LNGREG RSTD;                  /* WORKING REGISTER              */ 01111000
*   RFY                                                            0793 01112000
*     ISTLCPB BASED(LNGREG);        /* USED TO COVER LCPB            */ 01113000
*   LNGREG=LCPBREG;                 /* ADDRESS LSCB/LCPB             */ 01114000
         LR    LNGREG,LCPBREG                                      0794 01115000
*   IF LCPTYPE=LSTYPE THEN          /* IF ELEMENT IS A LSCB          */ 01116000
         CLI   LCPTYPE(LNGREG),X'DF'                               0795 01117000
         BNE   @RF00795                                            0795 01118000
*     LNGREG=LSCLCPBA;              /* ADDRESS LCPB FROM LSCB        */ 01119000
         L     LNGREG,LSCLCPBA(,LCPBREG)                           0796 01120000
*   ELSE                                                           0797 01121000
*     ;                             /* IF ELEMENT IS A LCPB          */ 01122000
@RF00795 DS    0H                                                  0798 01123000
*   LCPLCCWC=LCPLCCWC+1;            /* INCREMENT LCCW COUNT  @XL03HAH*/ 01124000
         LA    @11,1                                               0798 01125000
         SLR   @00,@00                                             0798 01126000
         IC    @00,LCPLCCWC(,LNGREG)                               0798 01127000
         ALR   @00,@11                                             0798 01128000
         STC   @00,LCPLCCWC(,LNGREG)                               0798 01129000
*   LCCWREG=LSCLCCWA+LENGTH(ISTLCCW);/* ADDRESS NEXT LCCW            */ 01130000
         LA    LCCWREG,8                                           0799 01131000
         AL    LCCWREG,LSCLCCWA(,LCPBREG)                          0799 01132000
*   IF LCCWREG->LCCWOP=LTIC THEN                                   0800 01133000
         CLI   LCCWOP(LCCWREG),X'08'                               0800 01134000
         BNE   @RF00800                                            0800 01135000
*     DO;                           /* IF NEXT LCCW IS A TIC         */ 01136000
*       LCPLCCWC=LCPLCCWC+1;        /* UPDATE LCCW COUNT     @XL03HAH*/ 01137000
         ALR   @00,@11                                             0802 01138000
         STC   @00,LCPLCCWC(,LNGREG)                               0802 01139000
*       LCCWREG=LCCWREG->LCCWDATA;  /* ADDRESS NEXT LCCW             */ 01140000
         L     LCCWREG,LCCWDATA(,LCCWREG)                          0803 01141000
*     END;                                                         0804 01142000
*   ELSE                                                           0805 01143000
*     ;                             /* IF NEXT LCCW NOT A TIC        */ 01144000
@RF00800 DS    0H                                                  0806 01145000
*   ZCRTSW1=OFF;                    /* RE-CLEAR TEST FLAG    @XL03HAH*/ 01146000
         NI    ZCRTSW1(ZCRREG),B'01111111'                         0806 01147000
*   LSCLCCWA=LCCWREG;               /* STORE UPDATED LCCWPTR IN    0807 01148000
*                                      LSCB/LCPB                     */ 01149000
         ST    LCCWREG,LSCLCCWA(,LCPBREG)                          0807 01150000
*   RFY                                                            0808 01151000
*     LNGREG UNRSTD;                /* WORK REGISTER                 */ 01152000
*   RFY                                                            0809 01153000
*     ISTLCPB BASED(LCPBREG);       /* RE-BASE LCPB                  */ 01154000
*                                                                  0809 01155000
*   /*****************************************************************/ 01156000
*   /*                                                               */ 01157000
*   /* SEGMENT(CHKLCCW)FINISH                                        */ 01158000
*   /*                                                               */ 01159000
*   /*****************************************************************/ 01160000
*                                                                  0810 01161000
*   DO;                                                            0810 01162000
*     GOTO CHKLCCW#;                                               0811 01163000
         B     CHKLCCW#                                            0811 01164000
*CHKLCCW@:                                                         0812 01165000
*   END;                            /* CHKLCCW: END                  */ 01166000
CHKLCCW@ DS    0H                                                  0813 01167000
         EJECT                                                          01168000
*                                                                  0813 01169000
*/********************************************************************/ 01170000
*/*                                                                  */ 01171000
*/* SEGMENT-                                                         */ 01172000
*/*      ALLOCAT                                                     */ 01173000
*/*                                                                  */ 01174000
*/* FUNCTION-                                                        */ 01175000
*/*      CALCULATE BUFFER SIZE NEEDED TO CONTAIN ALL DATA IN         */ 01176000
*/*      THE LCCW STRING, AND ALLOCATE THE BUFFERS NEEDED            */ 01177000
*/*      TO CONTAIN THE DATA.                                        */ 01178000
*/*                                                                  */ 01179000
*/*      DETERMINES IF ANY LCCWS NEED TO BE VALIDATED, AND           */ 01180000
*/*      SETS INTERNAL FLAGS AS APPROPRIATE.                         */ 01181000
*/*                                                                  */ 01182000
*/********************************************************************/ 01183000
*                                                                  0813 01184000
*   /*****************************************************************/ 01185000
*   /*                                                               */ 01186000
*   /* SEGMENT(ALLOCAT)START                                         */ 01187000
*   /*                                                               */ 01188000
*   /*****************************************************************/ 01189000
*                                                                  0813 01190000
*   DO;                                                            0813 01191000
*     GOTO ALLOCAT@;                                               0814 01192000
         B     ALLOCAT@                                            0814 01193000
*ALLOCAT:                                                          0815 01194000
*   END;                            /* ALLOCAT: SUBROUTINE           */ 01195000
ALLOCAT  DS    0H                                                  0816 01196000
*   RFY                                                            0816 01197000
*     LNGREG RSTD;                  /* WORKING REGISTER      @XL03HAH*/ 01198000
*   TLCCWREG=LSCLCCWA;              /* ADDRESS LCCW          @XL03HAH*/ 01199000
         L     TLCCWREG,LSCLCCWA(,LCPBREG)                         0817 01200000
*   RFY                                                            0818 01201000
*     ISTLCCW BASED(TLCCWREG);      /* ADDRESS LCCW          @XL03HAH*/ 01202000
*   LNGREG=LCCWCNT;                 /* INIT BUFFER LENGTH    @XL03HAH*/ 01203000
         LH    LNGREG,LCCWCNT(,TLCCWREG)                           0819 01204000
         N     LNGREG,@CF04478                                     0819 01205000
*   IF LCCWNTVC=OFF                 /* IF DATA TO BE VALIDATED     0820 01206000
*                                                            @XM10933*/ 01207000
*       &LCCWIDAT=OFF THEN          /* & NOT IMMEDIATE       @XM10933*/ 01208000
         TM    LCCWNTVC(TLCCWREG),B'00011000'                      0820 01209000
         BNZ   @RF00820                                            0820 01210000
*     ZCRTSW2=ON;                   /* SET VALIDATE FLAG     @XM10933*/ 01211000
         OI    ZCRTSW2(ZCRREG),B'01000000'                         0821 01212000
*   ELSE                                                           0822 01213000
*     ;                             /* VALIDATION NOT REQUIRED     0822 01214000
*                                                            @XM10933*/ 01215000
@RF00820 DS    0H                                                  0823 01216000
*   DO WHILE(LCCWCD=ON);            /* LOOP UNTLL LCCW CHAIN IS    0823 01217000
*                                      EXHAUSTED             @XL03HAH*/ 01218000
         B     @DE00823                                            0823 01219000
@DL00823 DS    0H                                                  0824 01220000
*     TLCCWREG=TLCCWREG+8;          /* POINT TO NEXT LCCW    @XL03HAH*/ 01221000
         AL    TLCCWREG,@CF00035                                   0824 01222000
*     IF LCCWOP=LTIC THEN           /* IF TIC LCCW FOUND     @XL03HAH*/ 01223000
         CLI   LCCWOP(TLCCWREG),X'08'                              0825 01224000
         BNE   @RF00825                                            0825 01225000
*       TLCCWREG=LCCWDATA;          /* POINT TO NEXT LCCW    @XL03HAH*/ 01226000
         L     TLCCWREG,LCCWDATA(,TLCCWREG)                        0826 01227000
*     ELSE                                                         0827 01228000
*       ;                           /* IF NOT TIC LCCW       @XL03HAH*/ 01229000
@RF00825 DS    0H                                                  0828 01230000
*     IF LCCWNTVC=OFF               /* IF DATA TO BE VALIDATED     0828 01231000
*                                                            @XM10933*/ 01232000
*         &LCCWIDAT=OFF THEN        /* & NOT IMMEDIATE       @XM10933*/ 01233000
         TM    LCCWNTVC(TLCCWREG),B'00011000'                      0828 01234000
         BNZ   @RF00828                                            0828 01235000
*       ZCRTSW2=ON;                 /* SET VALIDATE FLAG     @XM10933*/ 01236000
         OI    ZCRTSW2(ZCRREG),B'01000000'                         0829 01237000
*     ELSE                                                         0830 01238000
*       ;                           /* VALIDATION NOT REQUIRED     0830 01239000
*                                                            @XM10933*/ 01240000
@RF00828 DS    0H                                                  0831 01241000
*     LNGREG=LNGREG+LCCWCNT;        /* INCREMENT LENGTH      @XL03HAH*/ 01242000
         LH    @00,LCCWCNT(,TLCCWREG)                              0831 01243000
         N     @00,@CF04478                                        0831 01244000
         ALR   LNGREG,@00                                          0831 01245000
*   END;                            /* END LCCW SCAN         @XL03HAH*/ 01246000
@DE00823 TM    LCCWCD(TLCCWREG),B'10000000'                        0832 01247000
         BO    @DL00823                                            0832 01248000
*   IF PTRICN->NCBTYPE=ICTYPE THEN                                 0833 01249000
         CLI   NCBTYPE(PTRICN),X'06'                               0833 01250000
         BNE   @RF00833                                            0833 01251000
*     DO;                           /* IF 370X ATTACHMENT    @XA07953*/ 01252000
*       RFY                                                        0835 01253000
*         WORKREG RSTD;             /* LOCATE LOCAL 370X     @XA07953*/ 01254000
*       WORKREG=PTRICN;             /* ADDRESS ICNCB         @XA07953*/ 01255000
         LR    WORKREG,PTRICN                                      0836 01256000
*       DO WHILE(WORKREG->NCBNSNA^=0);/* LOCATE LOCAL 370X   @XA07953*/ 01257000
         B     @DE00837                                            0837 01258000
@DL00837 DS    0H                                                  0838 01259000
*         WORKREG=WORKREG->NCBNSNA; /* ADDRESS NEXT NODE     @XA07953*/ 01260000
         L     WORKREG,NCBNSNA(,WORKREG)                           0838 01261000
*       END;                        /* LOCAL 370X LOCATED    @XA07953*/ 01262000
@DE00837 L     @11,NCBNSNA(,WORKREG)                               0839 01263000
         LTR   @11,@11                                             0839 01264000
         BNZ   @DL00837                                            0839 01265000
*       IF LNGREG>WORKREG->ICNMSIZE THEN/* IF BUFFER TOO LARGE     0840 01266000
*                                                            @XA07953*/ 01267000
         C     LNGREG,ICNMSIZE(,WORKREG)                           0840 01268000
         BNH   @RF00840                                            0840 01269000
*         RTNCD=SIZERROR;           /* INDICATE SIZE ERR     @XA07953*/ 01270000
         LA    RTNCD,12                                            0841 01271000
*       ELSE                                                       0842 01272000
*         ;                         /* IF NOT TOO LARGE      @XA07953*/ 01273000
@RF00840 DS    0H                                                  0843 01274000
*       RFY                                                        0843 01275000
*         WORKREG UNRSTD;           /* NO LONGER REQUIRED    @XA07953*/ 01276000
*     END;                          /* END 370X PROCESS      @XA07953*/ 01277000
*   ELSE                                                           0845 01278000
*     ;                             /* NOT 370X ATTACHED     @XA07953*/ 01279000
@RF00833 DS    0H                                                  0846 01280000
*   IF RTNCD=SUCCESS THEN                                          0846 01281000
         LTR   RTNCD,RTNCD                                         0846 01282000
         BNZ   @RF00846                                            0846 01283000
*     DO;                           /* IF BUFFER SIZE OK     @XA07953*/ 01284000
*       RFY                                                        0848 01285000
*         ISTFDB BASED(ADDR(ZOBFDB));/* SET FDB BASE         @XL03HAH*/ 01286000
*       LNGREG=LNGREG-((ZOBLNG2+ZOBHDSIZ)-(FDBDATAA+FDBDATCT-BUFFREG)); 01287000
         LH    @00,FDBDATCT(,BUFFREG)                              0849 01288000
         AL    @00,FDBDATAA(,BUFFREG)                              0849 01289000
         SLR   @00,BUFFREG                                         0849 01290000
         LA    @11,28                                              0849 01291000
         LH    @10,ZOBLNG2(,BUFFREG)                               0849 01292000
         N     @10,@CF04478                                        0849 01293000
         ALR   @11,@10                                             0849 01294000
         SLR   @00,@11                                             0849 01295000
         ALR   LNGREG,@00                                          0849 01296000
*                                   /* COMPUTE BUFFER LENGTH @XM09250*/ 01297000
*       IF LNGREG<=0 THEN                                          0850 01298000
         LTR   LNGREG,LNGREG                                       0850 01299000
         BNP   @RT00850                                            0850 01300000
*         ;                         /* IF DATA WILL FIT IN THE SINGLE   01301000
*                                      BUFFER                @XL03HAH*/ 01302000
*       ELSE                                                       0852 01303000
*         DO;                       /* IF NEED MORE BUFFS    @XL03HAH*/ 01304000
*           ZCRPCOD1=0;             /* ZDFA0 PARAMETER               */ 01305000
         MVI   ZCRPCOD1(ZCRREG),X'00'                              0853 01306000
*           ZCRPBUF=ON;             /* ZDFA0 PARAMETER               */ 01307000
*           ZCRPLNG=ON;             /* ZDFA0 PARAMETER       @XL03HAH*/ 01308000
         OI    ZCRPBUF(ZCRREG),B'01000100'                         0855 01309000
*           REG8=LNGREG;            /* ZDFA0 PARAMETER       @XL03HAH*/ 01310000
         LR    REG8,LNGREG                                         0856 01311000
*           CALL ISTZDFA0;          /* BUFFER ALLOCATION     @XL03HAH*/ 01312000
         L     @11,ATCZPSVT(,ATCVTREG)                             0857 01313000
         L     @15,ZPSZDFA0(,@11)                                  0857 01314000
         BALR  @14,@15                                             0857 01315000
*           IF RTNCD=SUCCESS THEN   /* IF BUFF ALLOCATED     @XM10446*/ 01316000
         LTR   RTNCD,RTNCD                                         0858 01317000
         BNZ   @RF00858                                            0858 01318000
*             FDBFDBA=RPHSRPRM;     /* CHAIN NEW BUFFER TO CURRENT 0859 01319000
*                                      BUFFER                @XM10446*/ 01320000
         L     @00,RPHSRPRM(,RPHREG)                               0859 01321000
         ST    @00,FDBFDBA(,BUFFREG)                               0859 01322000
*           ELSE                                                   0860 01323000
*             ;                     /* IF CANNOT ALLOCATE    @XM10446*/ 01324000
@RF00858 DS    0H                                                  0861 01325000
*         END;                      /* LEN NOT POSITIVE      @XL03HAH*/ 01326000
*       RFY                                                        0862 01327000
*         LNGREG UNRSTD;            /* FREE WORK REGISTER    @XL03HAH*/ 01328000
@RT00850 DS    0H                                                  0863 01329000
*     END;                          /* END NCP SIZE ACCEPTED @XA07953*/ 01330000
*   ELSE                                                           0864 01331000
*     ;                             /* IF NCP SIZE REJECT    @XA07953*/ 01332000
*                                                                  0864 01333000
@RF00846 DS    0H                                                  0865 01334000
*   /*****************************************************************/ 01335000
*   /*                                                               */ 01336000
*   /* SEGMENT(ALLOCAT)FINISH                                        */ 01337000
*   /*                                                               */ 01338000
*   /*****************************************************************/ 01339000
*                                                                  0865 01340000
*   DO;                                                            0865 01341000
*     GOTO ALLOCAT#;                                               0866 01342000
         B     ALLOCAT#                                            0866 01343000
*ALLOCAT@:                                                         0867 01344000
*   END;                            /* ALLOCAT: END          @XL03HAH*/ 01345000
ALLOCAT@ DS    0H                                                  0868 01346000
         EJECT                                                          01347000
*                                                                  0868 01348000
*/********************************************************************/ 01349000
*/*                                                                  */ 01350000
*/* SEGMENT-                                                         */ 01351000
*/*      TRACE                                                       */ 01352000
*/*                                                                  */ 01353000
*/* FUNCTION-                                                        */ 01354000
*/*      INTERFACE TO BUFFER TRACE ROUTINE.                          */ 01355000
*/*                                                                  */ 01356000
*/********************************************************************/ 01357000
*                                                                  0868 01358000
*   /*****************************************************************/ 01359000
*   /*                                                               */ 01360000
*   /* SEGMENT(TRACE)START                                           */ 01361000
*   /*                                                               */ 01362000
*   /*****************************************************************/ 01363000
*                                                                  0868 01364000
*   DO;                                                            0868 01365000
*     GOTO TRACE@;                                                 0869 01366000
         B     TRACE@                                              0869 01367000
*TRACE:                                                            0870 01368000
*   END;                            /* TRACE: SUBROUTINE             */ 01369000
TRACE    DS    0H                                                  0871 01370000
*   IF RTNCD^=SUCCESS THEN                                         0871 01371000
         LTR   RTNCD,RTNCD                                         0871 01372000
         BNZ   @RT00871                                            0871 01373000
*     ;                             /* IF ANY ERROR ENCOUNTERED      */ 01374000
*   ELSE                            /* IF NO ERROR FOUND             */ 01375000
*     IF ZCRMDATA=OFF|ZCRSKIPT=ON THEN/* IF NO DATA FOUND    @ZA29724*/ 01376000
         TM    ZCRSKIPT(ZCRREG),B'00000010'                        0873 01377000
         BNZ   @RT00873                                            0873 01378000
         TM    ZCRMDATA(ZCRREG),B'00000001'                        0873 01379000
         BNO   @RT00873                                            0873 01380000
*       ;                           /* OR TRACE NOT REQUIRED         */ 01381000
*     ELSE                                                         0875 01382000
*       DO;                         /* IF TRACE REQUIRED             */ 01383000
*         CALL ATCTPBUF;            /* BUFFER TRACE ROUTINE          */ 01384000
         L     @15,ATCTPBUF(,ATCVTREG)                             0876 01385000
         BALR  @14,@15                                             0876 01386000
*         RTNCD=SUCCESS;            /* RETURN CODE = SUCESS          */ 01387000
         SLR   RTNCD,RTNCD                                         0877 01388000
*       END;                        /* END TRACE REQUIRED            */ 01389000
*                                                                  0878 01390000
*   /*****************************************************************/ 01391000
*   /*                                                               */ 01392000
*   /* SEGMENT(TRACE)FINISH                                          */ 01393000
*   /*                                                               */ 01394000
*   /*****************************************************************/ 01395000
*                                                                  0879 01396000
*   DO;                                                            0879 01397000
@RT00873 DS    0H                                                  0879 01398000
@RT00871 DS    0H                                                  0880 01399000
*     GOTO TRACE#;                                                 0880 01400000
         B     TRACE#                                              0880 01401000
*TRACE@:                                                           0881 01402000
*   END;                            /* END: TRACE                    */ 01403000
TRACE@   DS    0H                                                  0882 01404000
*/* TPEPILOG                                                         */ 01405000
*                                                                  0882 01406000
*   GEN(PRINT OFF);                                                0882 01407000
         PRINT OFF                                                      01408000
*   GEN(PRINT ON);                                                 0883 01409000
*                                                                  0883 01410000
         PRINT ON                                                       01411000
*/* END OF TPEPILOG                                                  */ 01412000
*                                                                  0884 01413000
*   END ISTZDFC0                                                   0884 01414000
*                                                                  0884 01415000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 01416000
*/*%INCLUDE SYSLIB  (IHAPSA  )                                       */ 01417000
*/*%INCLUDE SYSLIB  (IHASRB  )                                       */ 01418000
