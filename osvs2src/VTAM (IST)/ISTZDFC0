*     BC(8,OKAY0046);               /* IF ADDR VALID, DON'T ALTER  0767 01020000
*                                      RETURN CODE VALUE FROM 0      */ 01021000
         BC    8,OKAY0046                                          0767 01022000
*     RTNCD=4;                      /* SET RETURN CODE TO ERROR      */ 01023000
         LA    RTNCD,4                                             0768 01024000
*     GO TO VC30046;                                               0769 01025000
         B     VC30046                                             0769 01026000
*OKAY0046:                                                         0770 01027000
*     RTNCD=0;                      /* PUT RETURN CODE IN RCREG      */ 01028000
OKAY0046 SLR   RTNCD,RTNCD                                         0770 01029000
*VC30046:                                                          0771 01030000
*     GPR02P=GPR06P;                                               0771 01031000
VC30046  LR    GPR02P,GPR06P                                       0771 01032000
*     GEN SETS(2);                                                 0772 01033000
         MODESET KEYADDR=(2),WORKREG=2                                  01034000
*     GPR01F=SAV10046;              /* RESTORE REGS ONE,             */ 01035000
         ICM   GPR01F,15,SAV10046(GPR13P)                          0773 01036000
*     GPR02F=SAV20046;              /* TWO,                          */ 01037000
         ICM   GPR02F,15,SAV20046(GPR13P)                          0774 01038000
*     GPR04F=SAV40046;              /* FOUR,                         */ 01039000
         ICM   GPR04F,15,SAV40046(GPR13P)                          0775 01040000
