         ICM   GPR06F,15,SAV60046(GPR13P)                          0776 01042000
*     GPR14F=SAVE0046;              /* AND FOURTEEN                  */ 01043000
         ICM   GPR14F,15,SAVE0046(GPR13P)                          0777 01044000
*     RFY                                                          0778 01045000
*      (GPR01P,                                                    0778 01046000
*       GPR02P,                                                    0778 01047000
*       GPR04P,                                                    0778 01048000
*       GPR06P,                                                    0778 01049000
*       GPR14P,                                                    0778 01050000
*       GPR15P) UNRSTD;             /* UNRESTRICT REGISTERS          */ 01051000
*   END;                            /* VALCHEK CHECK THAT LCCW DATA     01052000
*                                      IS WITHIN USER'S CORE         */ 01053000
*   GEN(LM    ATCVTREG,PTRICN,ZCRWKARA(ZCRREG))                    0780 01054000
*   REFS(ZCRWKARA,ZCRREG) SETS(ATCVTREG,PTRICN);/* RESTORE EVEN-ODD     01055000
*                                      PAIR @DC038910                */ 01056000
         LM    ATCVTREG,PTRICN,ZCRWKARA(ZCRREG)                         01057000
*   RFY                                                            0781 01058000
*    (ATCVTREG,                                                    0781 01059000
*     PTRICN) RSTD;                 /* RESTRICT EVEN-ODD PAIR      0781 01060000
*                                                            @XA07953*/ 01061000
*   SAVREG=SAVREG-ZSAZSVVA;         /* RESTORE REG 13                */ 01062000
