*         END;                      /* ADDRESS NEXT LCCW             */ 00797000
*       ELSE                        /* IF NOT DATA CHAINED           */ 00798000
*         ZCRTSW1=ON;               /* SET EXIT FLAG                 */ 00799000
         B     @RC00709                                            0713 00800000
@RF00709 OI    ZCRTSW1(ZCRREG),B'10000000'                         0713 00801000
*     ELSE                          /* IF ERROR DETECTED             */ 00802000
*       ZCRTSW1=ON;                 /* SET EXIT FLAG                 */ 00803000
         B     @RC00708                                            0714 00804000
@RF00708 OI    ZCRTSW1(ZCRREG),B'10000000'                         0714 00805000
*   ELSE                            /* EXIT FLAG ALREADY SET @ZA10968*/ 00806000
*     ;                             /* NO MORE TO DO         @ZA10968*/ 00807000
*                                                                  0715 00808000
@RF00707 DS    0H                                                  0716 00809000
*   /*****************************************************************/ 00810000
*   /*                                                               */ 00811000
*   /* SEGMENT(MOVE)FINISH                                           */ 00812000
*   /*                                                               */ 00813000
*   /*****************************************************************/ 00814000
*                                                                  0716 00815000
*   DO;                                                            0716 00816000
@RC00707 DS    0H                                                  0717 00817000
*     GOTO MOVE#;                                                  0717 00818000
         B     MOVE#                                               0717 00819000
*MOVE@:                                                            0718 00820000
*   END;                            /* END OF SUBROUTINE             */ 00821000
MOVE@    DS    0H                                                  0719 00822000
         EJECT                                                          00823000
*                                                                  0719 00824000
*/********************************************************************/ 00825000
*/*                                                                  */ 00826000
*/* TITLE-                                                           */ 00827000
*/*      VALIDAT                                                     */ 00828000
*/*                                                                  */ 00829000
*/* FUNCTION-                                                        */ 00830000
*/*      VALIDATE USER LCCW DATA, TO INSURE THAT IT IS IN FACT       */ 00831000
*/*      WITHIN THE USER'S REGION.                                   */ 00832000
*/*                                                                  */ 00833000
