*     GPR04P=GPR01P->RPHTSKID;      /* PUT ADDR OF PROCESS     SCHED-   00890000
*                                      ULING TABLE IN REG 4          */ 00891000
         L     GPR04P,RPHTSKID(,GPR01P)                            0734 00892000
*/* ATBUILD (ISTPST)                                                 */ 00893000
*                                                                  0735 00894000
*/* ***** PROCESS SCHEDULING TABLE FOR VS/2       ****** */        0735 00895000
*/*  INITIAL LEVEL                                      001 04/09/75 */ 00896000
*DCL  1  ISTPST BDY(DWORD) BASED,                                  0735 00897000
*        2  PSTTYPE  CHAR(1),          /* CONTROL BLOCK TYPE         */ 00898000
*        2  PSTLNGTH PTR(8),           /* LENGTH IN BYTES            */ 00899000
*        2  *        CHAR(2),          /* RESERVED                   */ 00900000
*        2  PSTCHAIN PTR(31),          /* ADDRESS OF NEXT PST        */ 00901000
*        2  PSTRQFGA CHAR(8) BDY(DWORD),  /* READY QUEUE AND FLAGS   */ 00902000
*           3  PSTRQUE PTR(31),        /* READY QUEUE                */ 00903000
*              4  PSTRQFLG BIT(1),     /* READY QUEUE GATE           */ 00904000
*              4  PSTRSV01 BIT(1),     /* REVERVED                   */ 00905000
*              4  PSTRSV02 BIT(1),     /* REVERVED                   */ 00906000
*              4  PSTRSV03 BIT(1),     /* REVERVED                   */ 00907000
*              4  PSTRSV04 BIT(1),     /* REVERVED                   */ 00908000
*              4  PSTRSV05 BIT(1),     /* REVERVED                   */ 00909000
*              4  PSTRSV06 BIT(1),     /* REVERVED                   */ 00910000
*              4  PSTRSV07 BIT(1),     /* REVERVED                   */ 00911000
*              4  PSTRQUEA PTR(24),    /* READY QUEUE                */ 00912000
*           3  PSTFLGA PTR(32),        /* FLAG FIELD                 */ 00913000
*              4  PSTSSRN BIT(1),      /* IRB RUNNING SYSTEM SERVICE    00914000
*                                         TYPE REQUESTS              */ 00915000
*              4  PSTUERN BIT(1),      /* IRB RUNNING USER EXIT TYPE    00916000
*                                         REQUESTS                   */ 00917000
*              4  PSTIRBU1 BIT(1),     /* IRB # 1 IN USE             */ 00918000
*              4  PSTIRBU2 BIT(1),     /* IRB # 2 IN USE             */ 00919000
*              4  PSTSMQR BIT(1),      /* SMS RPH QUEUED FOR STORAGE */ 00920000
*              4  PSTSMCU BIT(1),      /* SMS CRA IN USE             */ 00921000
*              4  PSTAUTH BIT(1),      /* THE TASK THIS PST IS OBTAINED 00922000
*                                         FOR IS AUTHORIZED          */ 00923000
*              4  PSTABINP BIT(1),     /* ABEND IN PROGRESS ON THIS     00924000
*                                         TASK                       */ 00925000
*                 5 PSTABEND BIT(1),   /* TASK ABEND IN PROGRESS     */ 00926000
*              4  PSTDMPC BIT(1),      /* DUMP HAS BEEN REQUESTED    */ 00927000
*              4  PSTRSV08 BIT(23),    /* RESERVED                   */ 00928000
*        2  PSTTCBA PTR(31),           /* TCB POINTER                */ 00929000
*        2  PSTMPSTP PTR(31),          /* MPST POINTER - PST 6TH WORD*/ 00930000
*        2  PSTACBCT FIXED(31),        /* COUNT OF OPEN ACB'S USING     00931000
*                                         THIS PST                   */ 00932000
*        2  PSTPSSQ PTR(31),           /* POSTED SYSTEM SERVICE QUEUE*/ 00933000
*        2  PSTNSSQ PTR(31),           /* NORMAL SYSTEM SERVICE QUEUE*/ 00934000
*        2  PSTUEQ  PTR(31),           /* USER EXIT QUEUE            */ 00935000
*        2  PSTIQE11 PTR(31),          /* INACTIVE IQE FOR FIRST IRB */ 00936000
*        2  PSTIQE12 PTR(31),          /* ACTIVE IQE FOR FIRST IRB   */ 00937000
*        2  PSTIQE21 PTR(31),          /* INACTIVE IQE FOR 2ND IRB   */ 00938000
*        2  PSTIQE22 PTR(31),          /* ACTIVE IQE FOR 2ND IRB     */ 00939000
*        2  PSTCRR1 PTR(31),           /* PREALLOCATED CRA           */ 00940000
*        2  PSTCRRS PTR(31),           /* SMS CRA                    */ 00941000
*        2  PSTSMDYP CHAR(LENGTH(ISTDYPAB)) BDY(DWORD), /* SMS PAB   */ 00942000
*        2  PSTSAVE CHAR(32),          /* SAVE AREA                  */ 00943000
*        2  PSTDEBP  PTR(31),          /* POINTER TO DEB CHAIN       */ 00944000
*                                      /* WHEN TASK ABENDS           */ 00945000
*        2  PSTDMPS  FIXED(31),        /* DUMMY MPST                 */ 00946000
*           3 PSTDGATE BIT(1),         /* DUMMY MPST GATE BIT        */ 00947000
*           3 *        BIT(7),         /* NOT TO BE USED             */ 00948000
*           3 PSTDRDYQ PTR(24);        /* DUMMY MPST READY QUEUE     */ 00949000
*DCL  PSTSSRN0 BIT(32) CONSTANT('7FFFFFFF'X); /* TURN PSTSSRN OFF    */ 00950000
*DCL  PSTSSRN1 BIT(32) CONSTANT('80000000'X); /* TURN PSTSSRN ON     */ 00951000
*DCL  PSTUERN0 BIT(32) CONSTANT('BFFFFFFF'X); /* TURN PSTUERN OFF    */ 00952000
*DCL  PSTUERN1 BIT(32) CONSTANT('40000000'X); /* TURN PSTUERN ON     */ 00953000
*DCL  PSTGATE0 BIT(32) CONSTANT('7FFFFFFF'X); /* TURN PST READY QUEUE   00954000
*                                                GATE OFF            */ 00955000
*DCL  PSTGATE1 BIT(32) CONSTANT('80000000'X); /* TURN PST READY QUEUE   00956000
*                                                GATE ON             */ 00957000
*DCL  PSTIRB10 BIT(32) CONSTANT('DFFFFFFF'X); /* TURN PSTIRBU1 OFF   */ 00958000
*DCL  PSTIRB11 BIT(32) CONSTANT('20000000'X); /* TURN PSTIRBU1 ON    */ 00959000
*DCL  PSTIRB20 BIT(32) CONSTANT('EFFFFFFF'X); /* TURN PSTIRBU2 OFF   */ 00960000
*DCL  PSTIRB21 BIT(32) CONSTANT('10000000'X); /* TURN PSTIRBU2 ON    */ 00961000
*DCL  PSTSMQR0 BIT(32) CONSTANT('F7FFFFFF'X); /* TURN PSTSMQR OFF    */ 00962000
*DCL  PSTSMQR1 BIT(32) CONSTANT('08000000'X); /* TURN PSTSMQR ON     */ 00963000
*DCL  PSTSMCU0 BIT(32) CONSTANT('FBFFFFFF'X); /* TURN PSTSMCU OFF    */ 00964000
*DCL  PSTSMCU1 BIT(32) CONSTANT('04000000'X); /* TURN PSTSMCU ON     */ 00965000
*DCL  PSTABIP1 BIT(32) CONSTANT('01000000'X); /* TURN PSTABINP ON    */ 00966000
*DCL  PSTABIP0 BIT(32) CONSTANT('FEFFFFFF'X); /* TURN OFF PSTABINP   */ 00967000
*DCL  PSTYPE   BIT(8) CONSTANT('61'X);        /* TYPE CODE FOR PST   */ 00968000
*                                                                  0753 00969000
*/* END OF ATBUILD                                                   */ 00970000
*                                                                  0753 00971000
*                                   /* INCLUDE PROCESS SCHEDULING  0753 00972000
*                                      TABLE (ISTPST)                */ 00973000
*     GPR15P=GPR04P->PSTTCBA;       /* PUT TASK CONTROL BLOCK ADDRESS   00974000
*                                      IN REGISTER 15                */ 00975000
         L     GPR15P,PSTTCBA(,GPR04P)                             0753 00976000
*     GPR04F=SAV40046;              /* RESTORE REG 4 INVOKE XREGSET     00977000
*                                      MACRO TO VERIFY USER AREA   0754 00978000
*                                      OPERAND AND PUT ADDR IN REG 1 */ 00979000
