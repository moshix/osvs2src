*                                      PPARM OPERAND                 */ 00986000
         L     @11,LSCLCCWA(,LCPBREG)                              0756 00987000
         L     GPR01P,LCCWDATA(,@11)                               0756 00988000
*     END;                          /* XREGSET INVOKE XREGSET MACRO     00989000
*                                      TO VERIFY LENGTH OPERAND AND     00990000
*                                      PUT VALUE IN REG 14           */ 00991000
*/*XREGSET  PID('LENGTH')PPARM(LCCWCNT)RREG(,GPR14F)OUTPUT(PARM)     */ 00992000
*                                                                  0758 00993000
*     DO;                                                          0758 00994000
*       GPR14F=LCCWCNT;             /* SET RREG SYMBOL TO VALUE OF 0759 00995000
*                                      PPARM OPERAND                 */ 00996000
         LH    GPR14F,LCCWCNT(,@11)                                0759 00997000
         N     GPR14F,@CF04478                                     0759 00998000
*     END;                          /* XREGSET                       */ 00999000
*     GEN SETS(2);                                                 0761 01000000
         MODESET EXTKEY=ZERO,SAVEKEY=(2),WORKREG=2                      01001000
*     GPR06P=GPR02P;                                               0762 01002000
         LR    GPR06P,GPR02P                                       0762 01003000
*     GPR04P=GPR15P;                /* RESTORE TCB ADDR              */ 01004000
         LR    GPR04P,GPR15P                                       0763 01005000
*     GPR02F=GPR01F+GPR14F-1;       /* PUT END ADDRESS IN REG 2      */ 01006000
