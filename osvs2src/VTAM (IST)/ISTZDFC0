*     RFY                                                          0726 00863000
*      (GPR01P,                                                    0726 00864000
*       GPR02P,                                                    0726 00865000
*       GPR04P,                                                    0726 00866000
*       GPR06P,                                                    0726 00867000
*       GPR14P,                                                    0726 00868000
*       GPR15P) RSTD;               /* DECLARE AND RESTRICT REGS 1,     00869000
*                                      2, 4, 6, 13,14, AND 15        */ 00870000
*     RTNCD=0;                      /* SET RETURN CODE TO NO ERROR   */ 00871000
         SLR   RTNCD,RTNCD                                         0727 00872000
*     DCL                                                          0728 00873000
*       1 SAV0046 CHAR(20) BASED(GPR13P),/* ESTABLISH SAVE AREA MAP  */ 00874000
*        2 SAV10046 CHAR(4),        /* TO SAVE REGS 1,2,4,6, AND 14  */ 00875000
*        2 SAV20046 CHAR(4),                                       0728 00876000
*        2 SAV40046 CHAR(4),                                       0728 00877000
*        2 SAV60046 CHAR(4),                                       0728 00878000
*        2 SAVE0046 CHAR(4);                                       0728 00879000
*     SAV10046=GPR01F;              /* SAVE REGS ONE,                */ 00880000
         STCM  GPR01F,15,SAV10046(GPR13P)                          0729 00881000
*     SAV20046=GPR02F;              /* TWO,                          */ 00882000
         STCM  GPR02F,15,SAV20046(GPR13P)                          0730 00883000
