@EF00003 DS    0H                                                  0418 00465000
@ER00003 BR    @14                                                 0418 00466000
*                                                                  0419 00467000
*/********************************************************************/ 00468000
*/*                                                                  */ 00469000
*/*   THE TBUFFCHN SUBRTN IS USED TO ALLOCATE A NEW 'TO BUFF' AND TO */ 00470000
*/*     CHAIN IT TO CURRENT 'TO BUFF' IF ANY, OR TO ZCRBFPTR IF NO   */ 00471000
*/*     CURRENT 'TO BUFF' EXISTS.  FIELDS ZOBLNG2,  FDBDATAA &       */ 00472000
*/*     FDBDATCT ARE INITIALIZED IN THE NEW 'TO BUFF'.  THE LNTH OF  */ 00473000
*/*     THE BUFFER ALLOCATED IS OBTAINED FROM ZCRMLNG                */ 00474000
*/*   INPUT:                                                         */ 00475000
*/*     TCBFPTR = ADDR CURRENT 'TO BUFF' OR 0 IF NONE                */ 00476000
*/*     ZCRPTR  = ADDR ISTZCRR                                       */ 00477000
*/*     REG14   = RETURN ADDR                                        */ 00478000
*/*   OUTPUT:                                                        */ 00479000
*/*     RTNCODE = 0 - SUCCESSFUL COMPLETION                          */ 00480000
*/*             ^=0 - UNSUCCESSFUL ALLOCATION OF 'TO BUF'            */ 00481000
*/*     TCBFPTR = ADDR NEW 'TO BUF' IF RTNCODE = 0                   */ 00482000
*/*             = UNCHANGED OTHERWISE                                */ 00483000
*/********************************************************************/ 00484000
*                                                                  0419 00485000
*TBUFFCHN:                                                         0419 00486000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0419 00487000
TBUFFCHN DS    0H                                                  0420 00488000
*   RFY                                                            0420 00489000
*     ISTZOBUF BASED(TCBFPTR);      /* ADDRESSABILITY TO CURRENT 'TO'   00490000
*                                      BUFF                          */ 00491000
*   RFY                                                            0421 00492000
*     ISTFDB BASED(ADDR(ZOBFDB));                                  0421 00493000
*   RFY                                                            0422 00494000
*     REG14 RSTD;                                                  0422 00495000
*   ZCRSVF19=REG14;                 /* SAVE RETURN REG               */ 00496000
         ST    REG14,ZCRSVF19(,ZCRPTR)                             0423 00497000
*   RFY                                                            0424 00498000
*     REG14 UNRSTD;                 /* LET CMPLR USE IT              */ 00499000
*   ZCRPCOD1=0;                     /* CLEAR FLG FOR ALLOCATION      */ 00500000
         MVI   ZCRPCOD1(ZCRPTR),X'00'                              0425 00501000
*   ZCRPLNG=ON;                     /* INDIC BUFF LNTH SPECIFIED     */ 00502000
*   ZCRPBUF=ON;                     /* INDIC 'GET', NOT 'RELEASE'    */ 00503000
         OI    ZCRPLNG(ZCRPTR),B'01000100'                         0427 00504000
*   IF ZCRPPG2=ON THEN              /* IF 'PAGEABLE BUFFER' FLG      */ 00505000
         TM    ZCRPPG2(ZCRPTR),B'10000000'                         0428 00506000
         BNO   @RF00428                                            0428 00507000
*     ZCRPPG=ON;                    /* IS ON, TELL ALLOCATION        */ 00508000
         OI    ZCRPPG(ZCRPTR),B'10000000'                          0429 00509000
*   ELSE                                                           0430 00510000
*     ;                             /* LEAVE FLG OFF OTHERWISE       */ 00511000
@RF00428 DS    0H                                                  0431 00512000
*   BUFFLNG=ZCRMLNG;                /* LNTH OF BUFFER TO ALLOCATE    */ 00513000
         LH    BUFFLNG,ZCRMLNG(,ZCRPTR)                            0431 00514000
*   IF TCBFPTR=0 THEN                                              0432 00515000
         LTR   TCBFPTR,TCBFPTR                                     0432 00516000
         BZ    @RT00432                                            0432 00517000
*     ;                             /* IS THERE A 'TO' BUFF ?        */ 00518000
*   ELSE                            /* IF THERE IS...                */ 00519000
*     IF FDBFDBA=0 THEN                                            0434 00520000
         L     @14,FDBFDBA(,TCBFPTR)                               0434 00521000
         LTR   @14,@14                                             0434 00522000
         BZ    @RT00434                                            0434 00523000
*       ;                           /* DOES IT HAVE ANOTHER BUFF   0435 00524000
*                                      CHAINED TO IT ?               */ 00525000
*     ELSE                          /* IF SO...                      */ 00526000
*       DO;                                                        0436 00527000
*         TCBFPTR=FDBFDBA;          /* POINT TO NEXT 'TO' BUFF       */ 00528000
         L     TCBFPTR,FDBFDBA(,TCBFPTR)                           0437 00529000
*         ZCRTSW1=ON;               /* SW ON TO BYPASS ALLOCATION    */ 00530000
         OI    ZCRTSW1(ZCRPTR),B'10000000'                         0438 00531000
*       END;                                                       0439 00532000
*   IF ZCRTSW1=ON THEN              /* IF SWITCH IS ON...            */ 00533000
@RT00434 DS    0H                                                  0440 00534000
@RT00432 TM    ZCRTSW1(ZCRPTR),B'10000000'                         0440 00535000
         BNO   @RF00440                                            0440 00536000
*     ZCRTSW1=OFF;                  /* TURN IT OFF. ALLOCATION WILL     00537000
*                                      BE BYPASSED                   */ 00538000
         NI    ZCRTSW1(ZCRPTR),B'01111111'                         0441 00539000
*   ELSE                            /* SWITCH NOT ON                 */ 00540000
*FD0TB1:                                                           0442 00541000
*     DO;                                                          0442 00542000
         B     @RC00440                                            0442 00543000
@RF00440 DS    0H                                                  0442 00544000
FD0TB1   DS    0H                                                  0443 00545000
*       RFY                                                        0443 00546000
*         REG2 RSTD;                                               0443 00547000
*       TLNG=REG2;                  /* SAVE CONTENTS OF REG2         */ 00548000
         LR    TLNG,REG2                                           0444 00549000
*       REG2=ZCRSVWK1;              /* GET ATCVT PTR FROM SAVE       */ 00550000
         L     REG2,ZCRSVWK1(,ZCRPTR)                              0445 00551000
*       RFY                                                        0446 00552000
*         ISTATCVT BASED(REG2);     /* AREA AND NOTIFY COMPILER      */ 00553000
*       CALL ISTZDFA0;              /* CALL BUFFER ALLOCATION        */ 00554000
         L     @14,ATCZPSVT(,REG2)                                 0447 00555000
         L     @15,ZPSZDFA0(,@14)                                  0447 00556000
         BALR  @14,@15                                             0447 00557000
*       ZCRTSW1=OFF;                /* RESET SWITCH                  */ 00558000
         NI    ZCRTSW1(ZCRPTR),B'01111111'                         0448 00559000
*       REG2=TLNG;                  /* RESTORE REG2                  */ 00560000
         LR    REG2,TLNG                                           0449 00561000
*       RFY                                                        0450 00562000
*         REG2 UNRSTD;                                             0450 00563000
*       IF RTNCODE=0 THEN           /* IF ALLOCATION SUCCESSFUL      */ 00564000
         LTR   RTNCODE,RTNCODE                                     0451 00565000
         BNZ   @RF00451                                            0451 00566000
*FD0TB2:                                                           0452 00567000
*         DO;                                                      0452 00568000
FD0TB2   DS    0H                                                  0453 00569000
*           IF TCBFPTR=0 THEN       /* IF THERE WAS NO 'TO BUFF'     */ 00570000
         LTR   TCBFPTR,TCBFPTR                                     0453 00571000
         BNZ   @RF00453                                            0453 00572000
*             ZCRBFPTR=RPHSRPRM;    /* SAVE ADDR NEW 'TO BUFF' IN ZCR   00573000
*                                      FOR CALLER                    */ 00574000
         L     @14,RPHSRPRM(,PTRRPH)                               0454 00575000
         ST    @14,ZCRBFPTR(,ZCRPTR)                               0454 00576000
*           ELSE                    /* IF THERE WAS A 'TO BUFF'      */ 00577000
*             FDBFDBA=RPHSRPRM;     /* LINK NEW 'TO BUFF' TO ORIGINAL   00578000
*                                      'TO BUFF'                     */ 00579000
         B     @RC00453                                            0455 00580000
@RF00453 L     @14,RPHSRPRM(,PTRRPH)                               0455 00581000
         ST    @14,FDBFDBA(,TCBFPTR)                               0455 00582000
*           TCBFPTR=RPHSRPRM;       /* BASE FOR NEW 'TO BUFF'        */ 00583000
@RC00453 L     TCBFPTR,RPHSRPRM(,PTRRPH)                           0456 00584000
*         END FD0TB2;                                              0457 00585000
*       ELSE                                                       0458 00586000
*         ;                         /* ALLOCATION NOT SUCCESSFUL     */ 00587000
@RF00451 DS    0H                                                  0459 00588000
*     END FD0TB1;                                                  0459 00589000
*   IF RTNCODE=0 THEN               /* IF ALLOC. WAS SUCCESSFUL..    */ 00590000
@RC00440 LTR   RTNCODE,RTNCODE                                     0460 00591000
         BNZ   @RF00460                                            0460 00592000
*     DO;                           /* OR NOT REQUIRED...            */ 00593000
*       ZOBLNG2=ZOBLNG1;            /* INIT. BUFFER LNTH FIELD       */ 00594000
         LH    @14,ZOBLNG1(,TCBFPTR)                               0462 00595000
         N     @14,@CF03177                                        0462 00596000
         STH   @14,ZOBLNG2(,TCBFPTR)                               0462 00597000
*       IF ZCROB=ON THEN            /* IF IN OUTBOUND MODE           */ 00598000
         TM    ZCROB(ZCRPTR),B'00010000'                           0463 00599000
         BNO   @RF00463                                            0463 00600000
*         FDBDATAA=TCBFPTR+ZOBHDSIZ;/* INIT. DATA ADDR               */ 00601000
         LA    @14,28                                              0464 00602000
         ALR   @14,TCBFPTR                                         0464 00603000
         ST    @14,FDBDATAA(,TCBFPTR)                              0464 00604000
*       ELSE                        /* IF IN INBOUND MODE            */ 00605000
*         IF ZCRPMFGF=ON THEN       /* IF FIRST BUFFER IN BLOCK      */ 00606000
         B     @RC00463                                            0465 00607000
@RF00463 TM    ZCRPMFGF(ZCRPTR),B'00100000'                        0465 00608000
         BNO   @RF00465                                            0465 00609000
*           DO;                     /* PROCESS FIRST BUFFER IN BLK   */ 00610000
*             RFY                                                  0467 00611000
*               ISTZIBUF BASED(ADDR(L000000));                     0467 00612000
*             FDBDATAA=TCBFPTR+(ADDR(ZIBFSB)-ADDR(ZIBFDBZ));/* SET UP   00613000
*                                      FDB DATA ADDRESS              */ 00614000
         LA    @14,L000000                                         0468 00615000
         LA    @00,ZIBFSB(,@14)                                    0468 00616000
         SLR   @00,@14                                             0468 00617000
         ALR   @00,TCBFPTR                                         0468 00618000
         ST    @00,FDBDATAA(,TCBFPTR)                              0468 00619000
*             ZCRPMFGF=OFF;         /* CLEAR FIRST BUFFER SWITCH     */ 00620000
         NI    ZCRPMFGF(ZCRPTR),B'11011111'                        0469 00621000
*             RFY                                                  0470 00622000
*               ISTZIBUF;                                          0470 00623000
*           END;                    /* END FIRST BUFFER PROCESS      */ 00624000
*         ELSE                      /* IF NOT FIRST BUFFER IN BLK    */ 00625000
*           FDBDATAA=TCBFPTR+ZOBHDSIZ;/* SET UP FDB DATA ADDRESS     */ 00626000
         B     @RC00465                                            0472 00627000
@RF00465 LA    @14,28                                              0472 00628000
         ALR   @14,TCBFPTR                                         0472 00629000
         ST    @14,FDBDATAA(,TCBFPTR)                              0472 00630000
*     END;                                                         0473 00631000
*   ELSE                                                           0474 00632000
*     ;                                                            0474 00633000
@RF00460 DS    0H                                                  0475 00634000
*   RFY                                                            0475 00635000
*     REG14 RSTD;                                                  0475 00636000
