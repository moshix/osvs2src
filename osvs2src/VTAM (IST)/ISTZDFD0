*                                                                  0391 00321000
*                                                                  0391 00322000
*/********************************************************************/ 00323000
*/*                                                                  */ 00324000
*/*  THE FINDFLNG SUBROUTINE IS USED TO CALCULATE THE ADDRESS        */ 00325000
*/*    AND THE LENGTH OF THE 'FROM' DATA IN THE 'FROM' BUFFER.       */ 00326000
*/*    THE FORMER IS RETURNED IN FROMREG AND THE LATTER IN FLNG.     */ 00327000
*/*                                                                  */ 00328000
*/*                                                                  */ 00329000
*/*  INPUT:                                                          */ 00330000
*/*                                                                  */ 00331000
*/*    FCBFPTR = ADDR 'FROMBUFF' HEADER                              */ 00332000
*/*    ZCRPTR  = ADDR ISTZCRR                                        */ 00333000
*/*    REG14   = RETURN ADDR                                         */ 00334000
*/*                                                                  */ 00335000
*/*  OUTPUT:                                                         */ 00336000
*/*                                                                  */ 00337000
*/*    FROMREG = ADDR 'FROM' DATA                                    */ 00338000
*/*    FLNG    = LENGTH OF 'FROM' DATA                               */ 00339000
*/*                                                                  */ 00340000
*/*                                                                  */ 00341000
*/*                                                                  */ 00342000
*/********************************************************************/ 00343000
*                                                                  0391 00344000
*FINDFLNG:                                                         0391 00345000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0391 00346000
FINDFLNG DS    0H                                                  0392 00347000
*   RFY                                                            0392 00348000
*     REG14 RSTD;                   /* RESTRICT REG 14               */ 00349000
*   ZCRSVF19=REG14;                 /* SAVE REG 14                   */ 00350000
         ST    REG14,ZCRSVF19(,ZCRPTR)                             0393 00351000
*   RFY                                                            0394 00352000
*     REG14 UNRSTD;                 /* LET CMPLR USE IT              */ 00353000
*   RFY                                                            0395 00354000
*     ISTZOBUF BASED(FCBFPTR);      /* ADDRESSABILITY TO 'FROMBUF'   */ 00355000
*   RFY                                                            0396 00356000
*     ISTFDB BASED(ADDR(ZOBFDB));                                  0396 00357000
*   FROMREG=FDBDATAA+ZCRDFRDA;      /* ADD DISPLACEMENT TO GET ADDR     00358000
*                                      OF FROM DATA IN FROMBUF       */ 00359000
         LH    FROMREG,ZCRDFRDA(,ZCRPTR)                           0397 00360000
         AL    FROMREG,FDBDATAA(,FCBFPTR)                          0397 00361000
*   FLNG=ZOBLNG2-(FROMREG-FCBFPTR); /*                       @ZA19915   00362000
*                                      LNTH OF 'FROM' DATA = BUFFER     00363000
*                                      LNTH MINUS DISTANCE FROM    0398 00364000
*                                      BEGINNING OF BUFFER TO      0398 00365000
*                                      BEGINNING OF 'FROM' DATA      */ 00366000
         LR    FLNG,FROMREG                                        0398 00367000
         SLR   FLNG,FCBFPTR                                        0398 00368000
         LCR   FLNG,FLNG                                           0398 00369000
         LH    @14,ZOBLNG2(,FCBFPTR)                               0398 00370000
         N     @14,@CF03177                                        0398 00371000
         ALR   FLNG,@14                                            0398 00372000
*   IF ZCROB=ON THEN                /* OUTBOUND ?            @ZA19915*/ 00373000
         TM    ZCROB(ZCRPTR),B'00010000'                           0399 00374000
         BNO   @RF00399                                            0399 00375000
*     FLNG=FLNG+ZOBHDSIZ;           /* INCLUDE HEADER        @ZA19915*/ 00376000
         AL    FLNG,@CF03090                                       0400 00377000
*   ELSE                                                           0401 00378000
*     ;                             /*                       @ZA19915*/ 00379000
@RF00399 DS    0H                                                  0402 00380000
*   RFY                                                            0402 00381000
*     REG14 RSTD;                                                  0402 00382000
*   REG14=ZCRSVF19;                 /* RESTORE RETURN REG            */ 00383000
         L     REG14,ZCRSVF19(,ZCRPTR)                             0403 00384000
*   RFY                                                            0404 00385000
*     REG14 UNRSTD;                 /* LET CMPLR USE IT              */ 00386000
*   END FINDFLNG;                   /* RETURN TO CALLER              */ 00387000
@EL00002 DS    0H                                                  0405 00388000
@EF00002 DS    0H                                                  0405 00389000
@ER00002 BR    @14                                                 0405 00390000
*                                                                  0406 00391000
*/********************************************************************/ 00392000
*/*                                                                  */ 00393000
*/*  THE FINDTLNG SUBROUTINE IS USED TO CALCULATE THE ADDRESS OF     */ 00394000
*/*    THE AVAILABLE SPACE IN THE 'TO BUFF' INTO WHICH DATA CAN      */ 00395000
*/*    BE MOVED AND TO CALCULATE THE LENGTH OF THE AVAILABLE SPACE.  */ 00396000
*/*    THE ADDRESS IS RETURNED IN TOREG AND THE LENGTH IN TLNG.      */ 00397000
*/*                                                                  */ 00398000
*/*                                                                  */ 00399000
*/*  INPUT:                                                          */ 00400000
*/*                                                                  */ 00401000
*/*    TCBFPTR = ADDR 'TO BUFF' HEADER                               */ 00402000
*/*    ZCRPTR  = ADDR ISTZCRR                                        */ 00403000
*/*    REG14   = RETURN ADDR                                         */ 00404000
*/*                                                                  */ 00405000
*/*  OUTPUT:                                                         */ 00406000
*/*                                                                  */ 00407000
*/*    TOREG   = ADDR NEXT AVAIL BYTE IN 'TO BUFF'                   */ 00408000
*/*    TLNG    = LNTH OF AVAIL 'TO BUFF'                             */ 00409000
*/*                                                                  */ 00410000
*/********************************************************************/ 00411000
*                                                                  0406 00412000
*FINDTLNG:                                                         0406 00413000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0406 00414000
FINDTLNG DS    0H                                                  0407 00415000
*   RFY                                                            0407 00416000
*     REG14 RSTD;                                                  0407 00417000
*   ZCRSVF19=REG14;                 /* SAVE RETURN REG               */ 00418000
         ST    REG14,ZCRSVF19(,ZCRPTR)                             0408 00419000
*   RFY                                                            0409 00420000
*     REG14 UNRSTD;                 /* LET CMPLR USE IT              */ 00421000
*   RFY                                                            0410 00422000
*     ISTZOBUF BASED(TCBFPTR);      /* ADDRESSABILITY TO 'TOBUFF'    */ 00423000
*   RFY                                                            0411 00424000
*     ISTFDB BASED(ADDR(ZOBFDB));                                  0411 00425000
*   TOREG=FDBDATAA+FDBDATCT;        /* ADDR OF 'TO' AREA = ADDR OF 0412 00426000
*                                      1ST BYTE IN 'TOBUFF' AFTER  0412 00427000
*                                      DATA ALREADY THERE            */ 00428000
         LH    TOREG,FDBDATCT(,TCBFPTR)                            0412 00429000
         AL    TOREG,FDBDATAA(,TCBFPTR)                            0412 00430000
*   TLNG=ZOBLNG2-(TOREG-TCBFPTR);   /*                       @ZA19915   00431000
*                                      AVAILABLE 'TO LNTH' =NO. OF 0413 00432000
*                                      BYTES LEFT IN 'TOBUFF'        */ 00433000
