*   RFY                                                            0477 00640000
*     REG14 UNRSTD;                                                0477 00641000
*   END TBUFFCHN;                                                  0478 00642000
@EL00004 DS    0H                                                  0478 00643000
@EF00004 DS    0H                                                  0478 00644000
@ER00004 BR    @14                                                 0478 00645000
*                                                                  0479 00646000
*/********************************************************************/ 00647000
*/*   THE MOVELNG SEGMENT IS USED TO MOVE DATA FROM THE LOCATION     */ 00648000
*/*     SPECIFIED BY FROMREG TO THE LOCATION SPECIFIED BY TOREG. THE */ 00649000
*/*     NUMBER OF BYTES MOVED WILL BE THE LESSER OF WKLNG & ZCRMLNG. */ 00650000
*/*                                                                  */ 00651000
*/*   IF THE MOVE INSTRUCTION RETURNS A CONDITION CODE NOT EQUAL TO  */ 00652000
*/*     ZERO, AN ERROR RETURN CODE IS SET.                           */ 00653000
*/*                                                                  */ 00654000
*/*   INPUT:                                                         */ 00655000
*/*     WKLNG = MAX NO. BYTES WHICH CAN BE MOVED THIS PASS           */ 00656000
*/*     ZCRMLNG = TOTAL NO. BYTES TO BE MOVED.                       */ 00657000
*/*     FROMREG = ADDR TO MOVE DATA FROM                             */ 00658000
*/*     TOREG   = ADDR TO MOVE DATA TO                               */ 00659000
*/*     REG0    = WORK REG                                           */ 00660000
*/*     ZCRPTR  = ADDR ISTZCRR                                       */ 00661000
*/*    TCBFPTR  = ADDR TO-BUF HEADER                                 */ 00662000
*/*    REG14    = RETURN ADDRESS                                     */ 00663000
*/*   OUTPUT:                                                        */ 00664000
*/*     WKLNG   = NO. OF BYTES MOVED                                 */ 00665000
*/*     ZCRMLNG = NO. OF BYTES LEFT TO MOVE                          */ 00666000
*/*     FROMREG = ADDR BYTE FOLLOWING LAST CHAR MOVED                */ 00667000
*/*     TOREG   = ADDR NEXT AVAIL BYTE IN 'TO BUF' IF TLNG > 0       */ 00668000
*/*     FLNG    = NO. OF BYTES IF FROM-BUF ELIGIBLE TO MOVE          */ 00669000
*/*     TLNG    = NO. OF BYTES AVAIL IN TO-BUF                       */ 00670000
*/*     ZCRDFRDA = DISPLACEMENT OF BYTE AFTER LAST CHAR MOVED        */ 00671000
*/*     FDBDATCT (TO-BUF) = TOTAL NO. BYTES IN TO-BUF AFTER MOVE     */ 00672000
*/*     RTNCODE = 0 - MOVE SUCCESSFUL                                */ 00673000
*/*             = 8 - NON-ZERO COND. CODE FROM MVCL INSTRUCTION      */ 00674000
*/*                                                                  */ 00675000
*/********************************************************************/ 00676000
*/* SEGMENT(MOVELNG)START                                            */ 00677000
*                                                                  0479 00678000
*   DO;                                                            0479 00679000
*     GOTO MOVELNG@;                                               0480 00680000
*MOVELNG:                                                          0481 00681000
*   END;                                                           0481 00682000
MOVELNG  DS    0H                                                  0482 00683000
*   IF WKLNG<ZCRMLNG THEN                                          0482 00684000
         CH    WKLNG,ZCRMLNG(,ZCRPTR)                              0482 00685000
         BL    @RT00482                                            0482 00686000
*     ;                             /* CAN MOVE AS MANY BYTES AS     */ 00687000
*   ELSE                            /* INDIC BY WKLNG, BUT DON'T     */ 00688000
*     WKLNG=ZCRMLNG;                /* MOVE MORE THAN SPECIFIED BY 0484 00689000
*                                      ZCRMLNG                       */ 00690000
         LH    WKLNG,ZCRMLNG(,ZCRPTR)                              0484 00691000
*   ZCRMLNG=ZCRMLNG-WKLNG;          /* DECR CNTS: TOTAL DATA TO      */ 00692000
@RT00482 LH    @14,ZCRMLNG(,ZCRPTR)                                0485 00693000
         SLR   @14,WKLNG                                           0485 00694000
         STH   @14,ZCRMLNG(,ZCRPTR)                                0485 00695000
*   FLNG=FLNG-WKLNG;                /* MOVE, MOVE DATA IN THIS       */ 00696000
         SLR   FLNG,WKLNG                                          0486 00697000
*   TLNG=TLNG-WKLNG;                /* 'FROMBUF' & SPACE AVAIL IN  0487 00698000
*                                      THIS 'TOBUF' BY # BYTES MOVED    00699000
*                                      THIS PASS                     */ 00700000
         SLR   TLNG,WKLNG                                          0487 00701000
*   RFY                                                            0488 00702000
*     ISTZOBUF BASED(TCBFPTR);      /* ADDRESSABILITY TO 'TOBUF'     */ 00703000
*   RFY                                                            0489 00704000
*     ISTFDB BASED(ADDR(ZOBFDB));                                  0489 00705000
*   FDBDATCT=FDBDATCT+WKLNG;        /* BUMP DATA CNT BY NO. OF BYTES    00706000
*                                      MOVED THIS PASS               */ 00707000
         LR    @14,WKLNG                                           0490 00708000
         AH    @14,FDBDATCT(,TCBFPTR)                              0490 00709000
         STH   @14,FDBDATCT(,TCBFPTR)                              0490 00710000
*   ZCRDFRDA=ZCRDFRDA+WKLNG;        /* BUMP DISPLACEMENT OF MOVE DATA   00711000
*                                      BY SAME CNT                   */ 00712000
         LR    @14,WKLNG                                           0491 00713000
         AH    @14,ZCRDFRDA(,ZCRPTR)                               0491 00714000
         STH   @14,ZCRDFRDA(,ZCRPTR)                               0491 00715000
*   RFY                                                            0492 00716000
*     REG0 RSTD;                                                   0492 00717000
*   REG0=FLNG;                      /* SAVE FLNG IN REG0             */ 00718000
         LR    REG0,FLNG                                           0493 00719000
*   FLNG=WKLNG;                     /* SET UP FLNG FOR MOVE          */ 00720000
         LR    FLNG,WKLNG                                          0494 00721000
*   WKLNG=TLNG;                     /* SAVE TLNG                     */ 00722000
         LR    WKLNG,TLNG                                          0495 00723000
*   TLNG=FLNG;                      /* SET UP TLNG FOR MOVE          */ 00724000
         LR    TLNG,FLNG                                           0496 00725000
*   GEN CODE SETS(FROMREG,FLNG,TOREG,TLNG)                         0497 00726000
*       ( MVCL   TOREG,FROMREG   MOVE DATA FROM FROM-BUF TO TO-BUF);    00727000
          MVCL   TOREG,FROMREG   MOVE DATA FROM FROM-BUF TO TO-BUF      00728000
*   BC(EIGHT,FD0ML1);               /* IF MVCL SUCCESSFUL THEN..     */ 00729000
         BC    8,FD0ML1                                            0498 00730000
*   RTNCODE=EIGHT;                  /* ELSE SET ERROR CODE           */ 00731000
         LA    RTNCODE,8                                           0499 00732000
*FD0ML1:                                                           0500 00733000
*   TOREG=TOREG+TLNG;               /* PTR IN TO BUF         @ZA19915*/ 00734000
FD0ML1   ALR   TOREG,TLNG                                          0500 00735000
*   FLNG=REG0;                      /* RESTORE FLNG                  */ 00736000
         LR    FLNG,REG0                                           0501 00737000
*   RFY                                                            0502 00738000
*     REG0 UNRSTD;                  /* RELEASE REG0 FOR CMPLR USE    */ 00739000
*   TLNG=WKLNG;                     /* RESTORE TLNG                  */ 00740000
*                                                                  0503 00741000
         LR    TLNG,WKLNG                                          0503 00742000
*   /*****************************************************************/ 00743000
*   /*                                                               */ 00744000
*   /* SEGMENT(MOVELNG)FINISH                                        */ 00745000
*   /*                                                               */ 00746000
