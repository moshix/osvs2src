*/* END TPZRETN                                                      */ 00558000
*                                                                  0245 00559000
*                                                                  0245 00560000
*/********************************************************************/ 00561000
*/*                                                                  */ 00562000
*/*     THIS SEGMENT SCANS THE BUFFER POINTED TO BY BEGPTR FOR       */ 00563000
*/*     A LENGTH INDICATED BY REMLNG FOR A CHARACTER MATCHING THE    */ 00564000
*/*     FIELD ZCRPCHAR.  IF THE CHARACTER IS FOUND ITS ADDRESS WILL  */ 00565000
*/*     BE RETURNED IN RTNPTR, THE TOTAL NUMBER OF BYTES SCANNED     */ 00566000
*/*     WILL BE IN RTNLNG AND ZCRTSW1 WILL BE SET ON,  IF THE SEARCH */ 00567000
*/*     CHARACTER IS NOT FND ZCRTSW1 WILL NOT BE CHANGED.            */ 00568000
*/*                                                                  */ 00569000
*/********************************************************************/ 00570000
*/* SEGMENT(SEARCH)START                                             */ 00571000
*                                                                  0245 00572000
*   DO;                                                            0245 00573000
*     GOTO SEARCH@;                                                0246 00574000
*SEARCH:                                                           0247 00575000
*   END;                                                           0247 00576000
SEARCH   DS    0H                                                  0248 00577000
*FI02A2:                            /* START OF DO UNTIL LOOP        */ 00578000
*   XLNG=REMLNG;                    /* SET UP SCAN LENGTH REG        */ 00579000
FI02A2   LR    XLNG,REMLNG                                         0248 00580000
*   REMLNG=0;                       /* REMAINING LNTH = 0            */ 00581000
         SLR   REMLNG,REMLNG                                       0249 00582000
*   IF XLNG>MAX THEN                /* IF SCAN LNTH TOO LONG         */ 00583000
         LA    @14,256                                             0250 00584000
         CR    XLNG,@14                                            0250 00585000
         BNH   @RF00250                                            0250 00586000
*     DO;                           /* FOR 1 TRT INSTRUCTION         */ 00587000
*       REMLNG=XLNG-MAX;            /* THEN INIT. REMAINING LNTH     */ 00588000
         LR    REMLNG,XLNG                                         0252 00589000
         SLR   REMLNG,@14                                          0252 00590000
*       XLNG=MAX;                   /* AND LNTH FOR 1ST TRT SCAN     */ 00591000
         LR    XLNG,@14                                            0253 00592000
*     END;                                                         0254 00593000
*   ELSE                                                           0255 00594000
*     ;                             /* LNTH NOT TOO LONG             */ 00595000
@RF00250 DS    0H                                                  0256 00596000
*   RTNLNG=RTNLNG+XLNG;             /* BUMP TOTAL SCAN LNTH BY LNTH     00597000
*                                      OF THIS SCAN                  */ 00598000
         ALR   RTNLNG,XLNG                                         0256 00599000
*   CHARFND=0;                      /* CLEAR REG2 BEFORE TRT         */ 00600000
         SLR   CHARFND,CHARFND                                     0257 00601000
*   GEN CODE REFS(BEGPTR) SETS(XLNG,CHARPTR,CHARFND);              0258 00602000
        BCTR   XLNG,0                    DECR CNT BY 1 FOR EX           00603000
        EX     XLNG,FI0TRT               EXECUTE TRT SCAN               00604000
        LA     XLNG,1(XLNG)              RESET CNT TO PROPER VALUE      00605000
*   BC(EIGHT,FI0NFND);              /* IF CHAR FOUND IN TBLE THEN    */ 00606000
         BC    8,FI0NFND                                           0259 00607000
*   BC(FOUR,FI0NDONE);              /* DO..IF IT WAS LAST CHAR       */ 00608000
         BC    4,FI0NDONE                                          0260 00609000
*   XLNG=0;                         /* IN BUFFER, THEN ZERO XLNG     */ 00610000
         SLR   XLNG,XLNG                                           0261 00611000
*   GOTO FI0RT1;                    /* ELSE (CHAR WAS NOT            */ 00612000
         B     FI0RT1                                              0262 00613000
*FI0NDONE:                          /* LAST IN BUFFER) DO..          */ 00614000
*   XLNG=XLNG-(CHARPTR-BEGPTR)-1;   /* XLNG=NUMBER OF BYTES NOT    0263 00615000
*                                      SCANNED IN TRT                */ 00616000
FI0NDONE LR    @14,BEGPTR                                          0263 00617000
         SLR   @14,CHARPTR                                         0263 00618000
         ALR   @14,XLNG                                            0263 00619000
         BCTR  @14,0                                               0263 00620000
         LR    XLNG,@14                                            0263 00621000
*   BEGPTR=CHARPTR+1;               /* PNT TO CHAR AFTER ONE FND     */ 00622000
         LA    BEGPTR,1                                            0264 00623000
         ALR   BEGPTR,CHARPTR                                      0264 00624000
*   RTNLNG=RTNLNG-XLNG;             /* SUBTRACT BYTES NOT SCANNED  0265 00625000
*                                      FROM TOTAL SCAN LNTH          */ 00626000
         SLR   RTNLNG,XLNG                                         0265 00627000
*   REMLNG=REMLNG+XLNG;             /* BUMP REMAINING LNTH BY CNT OF    00628000
*                                      BYTES NOT SCANNED IN TRT END..*/ 00629000
         ALR   REMLNG,XLNG                                         0266 00630000
*/*                                                                  */ 00631000
*/*   REGISTER 2, WHICH CONTAINS A FUNCTION BYTE FROM THE TRT TABLE  */ 00632000
*/*   AS A RESULT OF THE TRT INSTRUCTION, WILL BE COMPARED WITH      */ 00633000
*/*   ZCRPCHAR, WHICH IS THE SEARCH CHARACTER BEING SOUGHT.          */ 00634000
*/*                                                                  */ 00635000
*/*   NOTE - IN ORDER FOR THIS TEST TO BE VALID, THE TRT TABLE MUST  */ 00636000
*/*   BE SET UP SO THAT NON-ZERO FUNCTION BYTES IN THE TABLE ARE     */ 00637000
*/*   EQUAL TO THE ARGUMENT BYTES THEY REPRESENT.  THAT IS, THE      */ 00638000
*/*   VALUE OF EACH NON-ZERO FUNCT BYTE MUST EQUAL THE DISPLACEMENT  */ 00639000
*/*   OF THAT FUNCT BYTE FROM THE BEGINNING OF THE TABLE.            */ 00640000
*/*                                                                  */ 00641000
*                                                                  0267 00642000
*FI0RT1:                                                           0267 00643000
*   IF CHARFND=ZCRPCHAR THEN        /* IF CHAR FND WAS THE           */ 00644000
FI0RT1   SLR   @14,@14                                             0267 00645000
         IC    @14,ZCRPCHAR(,ZCRPTR)                               0267 00646000
         CR    CHARFND,@14                                         0267 00647000
         BNE   @RF00267                                            0267 00648000
*     DO;                           /* SRCH CHAR                     */ 00649000
*       RTNPTR=CHARPTR;             /* OUTPUT R15 PTS TO CHAR FND    */ 00650000
         LR    RTNPTR,CHARPTR                                      0269 00651000
*       ZCRTSW1=ON;                 /* SW ON TO END CALLERS LOOP     */ 00652000
         OI    ZCRTSW1(ZCRPTR),B'10000000'                         0270 00653000
*       REMLNG=0;                   /* REMLNG=0 TO END OUR LOOP      */ 00654000
         SLR   REMLNG,REMLNG                                       0271 00655000
*     END;                                                         0272 00656000
*   ELSE                                                           0273 00657000
*     ;                             /* SRCH CHAR NOT FND             */ 00658000
@RF00267 DS    0H                                                  0274 00659000
*   GOTO FI0TSTR;                   /* END..                         */ 00660000
         B     FI0TSTR                                             0274 00661000
*FI0NFND:                           /* ELSE (CHAR NOT FND)           */ 00662000
*   BEGPTR=BEGPTR+XLNG;             /* UPDATE PTR TO NXT CHAR        */ 00663000
FI0NFND  ALR   BEGPTR,XLNG                                         0275 00664000
*   XLNG=0;                         /* COUNT OF BYTES NOT SCANNED IN    00665000
*                                      TRT=ZERO.                     */ 00666000
         SLR   XLNG,XLNG                                           0276 00667000
*FI0TSTR:                                                          0277 00668000
*   IF REMLNG=0 THEN                                               0277 00669000
FI0TSTR  LTR   REMLNG,REMLNG                                       0277 00670000
         BNZ   @RF00277                                            0277 00671000
*     ;                             /* CHECK IF MORE CHARS TO SCAN   */ 00672000
*   ELSE                                                           0279 00673000
*     GOTO FI02A2;                  /* IF SO, CONTINUE DO UNTIL LOOP */ 00674000
*                                                                  0279 00675000
*   /*****************************************************************/ 00676000
*   /*                                                               */ 00677000
*   /* SEGMENT(SEARCH)FINISH                                         */ 00678000
*   /*                                                               */ 00679000
*   /*****************************************************************/ 00680000
*                                                                  0280 00681000
*   DO;                                                            0280 00682000
*     GOTO SEARCH#;                                                0281 00683000
         B     SEARCH#                                             0281 00684000
*SEARCH@:                                                          0282 00685000
*   END;                                                           0282 00686000
SEARCH@  DS    0H                                                  0283 00687000
*   GEN CODE;                                                      0283 00688000
*                                                                  0283 00689000
FI0TRT  TRT   0(0,BEGPTR),ISTZTRTB                                      00690000
ISTZTRTB DC    XL256'0'                 TRT TABLE                       00691000
        ORG    ISTZTRTB+2               OVERLAY BYTE 2                  00692000
        DC     X'02'                    FUNC BYTE FOR SRCH CHAR         00693000
        ORG    ,                        ORG TO END OF TABLE             00694000
*/* TPEPILOG                                                         */ 00695000
*                                                                  0284 00696000
*   GEN(PRINT OFF);                                                0284 00697000
         PRINT OFF                                                      00698000
*   GEN(PRINT ON);                                                 0285 00699000
*                                                                  0285 00700000
         PRINT ON                                                       00701000
*/* END OF TPEPILOG                                                  */ 00702000
