*   /*****************************************************************/ 00324000
*   /*                                                               */ 00325000
*   /* SEGMENT(SEG000G)FINISH                                        */ 00326000
*   /*                                                               */ 00327000
*   /*****************************************************************/ 00328000
*                                                                  0793 00329000
*   DO;                                                            0793 00330000
*     GOTO SEG000G#;                                               0794 00331000
         B     SEG000G#                                            0794 00332000
*SEG000G@:                                                         0795 00333000
*   END;                                                           0795 00334000
SEG000G@ DS    0H                                                  0796 00335000
*   GEN NOSETS NODEFS NOREFS(EJECT);                               0796 00336000
         EJECT                                                          00337000
*                                                                  0797 00338000
*/********************************************************************/ 00339000
*/*                                                                  */ 00340000
*/* SEGMENT-                                                         */ 00341000
*/*      SEG000P                                                     */ 00342000
*/*                                                                  */ 00343000
*/* FUNCTION-                                                        */ 00344000
*/*      FIND ALL REMOTE 370X'S OPENED OFF LOCAL BY SEARCHING DOWN   */ 00345000
*/*      NCBNCBA CHAIN.  GO TO SEGMENT F TO CLEAR SIO QUEUES FOR     */ 00346000
*/*      EACH REMOTE 370X FOUND.                                     */ 00347000
*/*      THE S/D CODE FOR THIS SEGMENT IS                    @XL03HCH*/ 00348000
*/********************************************************************/ 00349000
*                                                                  0797 00350000
*   /*****************************************************************/ 00351000
*   /*                                                               */ 00352000
*   /* SEGMENT(SEG000P)START                                         */ 00353000
*   /*                                                               */ 00354000
*   /*****************************************************************/ 00355000
*                                                                  0797 00356000
*   DO;                                                            0797 00357000
*     GOTO SEG000P@;                                               0798 00358000
         B     SEG000P@                                            0798 00359000
*SEG000P:                                                          0799 00360000
*   END;                                                           0799 00361000
SEG000P  DS    0H                                                  0800 00362000
*   WRKICN=PTRICN->NCBNCBA;         /* GET DAF OF LAST DEVICE OPEND     00363000
*                                      OFF LOCAL 370X                */ 00364000
         LH    WRKICN,NCBNCBA(,PTRICN)                             0800 00365000
         N     WRKICN,@CF05868                                     0800 00366000
*SRCHNCBA:                                                         0801 00367000
*   DO WHILE(WRKICN^=0);            /* CONTIN SEARCH IF DAF EXISTS   */ 00368000
SRCHNCBA B     @DE00801                                            0801 00369000
@DL00801 DS    0H                                                  0802 00370000
*     PTRG02=WRKICN;                /* SET DAF ONLY PARM             */ 00371000
         LR    PTRG02,WRKICN                                       0802 00372000
*     WKREG3=PTRG02;                /* SAVE DAF FOR CIDFINISH        */ 00373000
         LR    WKREG3,PTRG02                                       0803 00374000
*     RFY                                                          0804 00375000
*       PTRG15 RSTD;                                               0804 00376000
*     DO;                           /* CIDCTL(FIND)                  */ 00377000
*       RFY                                                        0806 00378000
*        (EMCR2,                                                   0806 00379000
*         EMCR3) RSTD;                                             0806 00380000
*       GEN SETS(RPHWORK)                                          0807 00381000
*           (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);   0807 00382000
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH              00383000
*       EMCR3='00'X;                /* CLEAR EXTRA BITS              */ 00384000
         SLR   EMCR3,EMCR3                                         0808 00385000
*       EMCR3='01'X;                /* INDICATE SHARED LOCK          */ 00386000
         LA    EMCR3,1                                             0809 00387000
*       CALL ATCLCPTR->ATCCDFND->ISTSDCCF;/* PROCESS REQUEST         */ 00388000
         L     @11,ATCLCPTR                                        0810 00389000
         L     @15,ATCCDFND(,@11)                                  0810 00390000
         BALR  @14,@15                                             0810 00391000
*       RFY                                                        0811 00392000
*        (EMCR2,                                                   0811 00393000
*         EMCR3) UNRSTD;                                           0811 00394000
*     END;                          /* CIDCTL FIND NCB OF DAF        */ 00395000
*     IF PTRG15=0 THEN              /* IF NCB FOUND                  */ 00396000
         LTR   PTRG15,PTRG15                                       0813 00397000
         BNZ   @RF00813                                            0813 00398000
*GOODFIND:                                                         0814 00399000
*       DO;                                                        0814 00400000
GOODFIND DS    0H                                                  0815 00401000
*         RFY                                                      0815 00402000
*           PTRG15 UNRSTD;                                         0815 00403000
*         WRKICN=PTRG00;            /* GET NCB ADDR                  */ 00404000
         LR    WRKICN,PTRG00                                       0816 00405000
*         IF WRKICN->NCBTYPE=ICTYPE THEN/* IF NCB= ICNCB             */ 00406000
         CLI   NCBTYPE(WRKICN),X'06'                               0817 00407000
         BNE   @RF00817                                            0817 00408000
*           DO;                     /* THEN WRKICN=@REMOTE ICNCB     */ 00409000
*             WRKICN->NCBCONF=SET;  /* SET NCB SICK FLAG             */ 00410000
         OI    NCBCONF(WRKICN),B'01000000'                         0819 00411000
*             IF PTRICN->ICNZF00F=SET THEN/* IF LOCAL HARD FAILURE   */ 00412000
         TM    ICNZF00F(PTRICN),B'00100000'                        0820 00413000
         BNO   @RF00820                                            0820 00414000
*               WRKICN->ICNZF00F=SET;/* THEN SET HARD FAILURE IN   0821 00415000
*                                      REMOTE                        */ 00416000
         L     @11,ICNZF00F(,WRKICN)                               0821 00417000
@GL00001 LR    @04,@11                                             0821 00418000
         O     @04,@CF05870                                        0821 00419000
         CS    @11,@04,ICNZF00F(WRKICN)                            0821 00420000
         BNE   @GL00001                                            0821 00421000
*             ELSE                                                 0822 00422000
*               ;                                                  0822 00423000
@RF00820 DS    0H                                                  0823 00424000
*             DO;                   /* LISATCVT                    0823 00425000
*                                      CVTBASE(PAGEABLE,(PTRATC))    */ 00426000
