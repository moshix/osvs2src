         BNZ   @RF00827                                            0827 00762000
*     DO;                           /* NO RPH WORK ELEMENT           */ 00763000
*       ZCRIF0E1='1'B;              /* SET LOOP STOPPER              */ 00764000
         OI    ZCRIF0E1(PTRZCR),B'00001000'                        0829 00765000
*       GO TO CASEND;               /* END OF CASE                   */ 00766000
         B     CASEND                                              0830 00767000
*     END;                          /* NO RPH WORK ELEMENT           */ 00768000
