*     GPR14P->WRD10025=GPR09P;      /* SAVE SAVEAREA ADDR            */ 00371000
         ST    GPR09P,WRD10025(,GPR14P)                            0632 00372000
*     GEN(MODESET KEYADDR=(2));     /* RESTORE KEY FROM REG 2        */ 00373000
         MODESET KEYADDR=(2)                                            00374000
