*   RFY                                                            0807 00899000
*     PTRG14 RSTD;                  /* RESTRICT RETURN REG           */ 00900000
*   ICNZSW3R=PTRG14;                /* SAVE RETURN REG               */ 00901000
         ST    PTRG14,ICNZSW3R(,PTRICN)                            0808 00902000
*   RFY                                                            0809 00903000
*     PTRG14 UNRSTD;                /* UNRESTRICT RETURN REG         */ 00904000
*   IF REGD->ICNSIOQ=0 THEN                                        0810 00905000
         L     @00,ICNSIOQ(,REGD)                                  0810 00906000
         LTR   @00,@00                                             0810 00907000
         BZ    @RT00810                                            0810 00908000
*     ;                             /*                       @Z30APGW   00909000
*                                      IF SIOQ IS EMPTY-NULL THEN    */ 00910000
*   ELSE                            /* IF SIOQ NOT EMPTY             */ 00911000
*EFBBQNOE:                                                         0812 00912000
*     DO;                           /* THEN                          */ 00913000
