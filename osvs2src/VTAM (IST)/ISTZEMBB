*         GEN;                                                     0789 00844000
         MODESET EXTKEY=VTAM,WORKREG=2                                  00845000
*         RCREG=0;                  /* PUT RETURN CODE IN RCREG      */ 00846000
         SLR   RCREG,RCREG                                         0790 00847000
*       END;                        /* VMDESET RESET TO/VTAM KEY   0791 00848000
*                                                             @ZM0403*/ 00849000
*       REGD=PTRICN;                /* SET REGD=@ LOCAL ICNCB        */ 00850000
         LR    REGD,PTRICN                                         0792 00851000
*       CALL SHIPOT6;               /* GO CLEAR ALL SIOQS            */ 00852000
         BAL   @14,SHIPOT6                                         0793 00853000
*       IF ICNZF00D='1'B THEN       /* IF I/O BUSY                   */ 00854000
         TM    ICNZF00D(PTRICN),B'01000000'                        0794 00855000
