*       REGD->ICNLSTBT=0;           /* CLEAR @ FIRST BUFFER OF LAST     00922000
*                                      BTU ON SIOQ                   */ 00923000
         ST    @15,ICNLSTBT(,REGD)                                 0816 00924000
*       ICNZCRIT=OFF;               /* CLEAR CRITICAL TEXT FLAG IN 0817 00925000
*                                      ICNCB                 @Z30APGW*/ 00926000
         L     @00,ICNZCRIT(,PTRICN)                               0817 00927000
@GL00006 LR    @15,@00                                             0817 00928000
         N     @15,@CF04831                                        0817 00929000
         CS    @00,@15,ICNZCRIT(PTRICN)                            0817 00930000
         BNE   @GL00006                                            0817 00931000
*/*  REGA=PTRZOB;                      **  SET UP FOR RELBUF @Z30APGW*/ 00932000
*/*  CALL RELBUF;                      **        FREE BUFFER @Z30APGW*/ 00933000
*/*               *** THE ABOVE TWO LINES DELETED BY ***     @ZA11976*/ 00934000
*/*  RELSTORE BUFADDR(PTRZOB)DISABLE(YES)CLEAR(YES)                  */ 00935000
*                                                                  0818 00936000
*       DO;                                                        0818 00937000
*                                                                  0818 00938000
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */ 00939000
*/* END OF ATBUILD                                                   */ 00940000
*                                                                  0819 00941000
*         RFY                                                      0819 00942000
*          (GPR02P,                                                0819 00943000
