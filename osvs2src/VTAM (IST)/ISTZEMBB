         L     PTRZOB,ICNSIOQ(,REGD)                               0813 00916000
*       REGD->ICNSIOQ=0;            /* ZERO START IO QUEUE           */ 00917000
         SLR   @15,@15                                             0814 00918000
         ST    @15,ICNSIOQ(,REGD)                                  0814 00919000
*       REGD->ICNLAST=0;            /* CLEAR @ LAST BUFFER ON SIOQ   */ 00920000
