               RELATED=(ICNCB,ISTZEMBB(EMBBUNLK))                       00364000
*     GEN REFS(GPR10P,GPR13P,GPR14P,PSACSTK,FRRSCURR,FRRSLAST,FRRS,     00365000
*         FRRSENTR,FRRSPARM,PSALSFCC,FRRSELEN,FRRSFRRA);           0630 00366000
         SETFRR A,FRRAD=(GPR10P),PARMAD=(GPR14P),                      *00367000
