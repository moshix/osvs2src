*     ;                             /* NULL ELSE IF ICNCB NOT SICK   */ 01058000
@RF00850 DS    0H                                                  0855 01059000
*   IF ICNZSW08='1'B THEN           /* IF CONTROL SWITCH ON          */ 01060000
@RC00850 TM    ICNZSW08(PTRICN),B'00000001'                        0855 01061000
         BNO   @RF00855                                            0855 01062000
*EFBBCON:                                                          0856 01063000
*     DO;                           /* THEN                          */ 01064000
EFBBCON  DS    0H                                                  0857 01065000
*       IF REGD=REGC THEN           /* IF REMOTE ICNCB               */ 01066000
         CR    REGD,REGC                                           0857 01067000
         BNE   @RF00857                                            0857 01068000
*EFBBREM5:                                                         0858 01069000
*         DO;                       /* THEN                          */ 01070000
EFBBREM5 DS    0H                                                  0859 01071000
*           CALL DEQUE;             /* REMOVE REMOTE ICNCB   @Z30APGW*/ 01072000
         BAL   @14,DEQUE                                           0859 01073000
*           CALL SHIPOT4;           /* GO CLEAR SIOQ FOR ICNCB       */ 01074000
         BAL   @14,SHIPOT4                                         0860 01075000
*         END EFBBREM5;             /* END DO-REMOTE ENTRY           */ 01076000
*       ELSE                        /* IF NOT REMOTE ICNCB           */ 01077000
*EFBBNREM:                                                         0862 01078000
*         DO;                       /* THEN                          */ 01079000
         B     @RC00857                                            0862 01080000
@RF00857 DS    0H                                                  0862 01081000
EFBBNREM DS    0H                                                  0863 01082000
*           REGD=PTRICN;            /* SET REGD=@ LOCAL ICNCB        */ 01083000
         LR    REGD,PTRICN                                         0863 01084000
*           CALL SHIPOT6;           /* GO CLEAR ALL SIOQS            */ 01085000
         BAL   @14,SHIPOT6                                         0864 01086000
*         END EFBBNREM;             /* END DO-LOCAL ENTRY            */ 01087000
*     END EFBBCON;                  /* END DO-CONTROL SWITCH ON      */ 01088000
*   ELSE                            /* IF CONTROL SWITCH OFF         */ 01089000
*EFBBCOFF:                                                         0867 01090000
*     DO;                           /* THEN                          */ 01091000
         B     @RC00855                                            0867 01092000
@RF00855 DS    0H                                                  0867 01093000
EFBBCOFF DS    0H                                                  0868 01094000
*       IF REGD->ICNZF010='1'B THEN /* IF ICNCB IN SHUTDOWN          */ 01095000
         TM    ICNZF010(REGD),B'00010000'                          0868 01096000
         BNO   @RF00868                                            0868 01097000
*EFBBSHDN:                                                         0869 01098000
*         DO;                       /* THEN                          */ 01099000
EFBBSHDN DS    0H                                                  0870 01100000
*           IF REGD=PTRICN THEN     /* IF LOCAL ICNCB                */ 01101000
         CR    REGD,PTRICN                                         0870 01102000
         BNE   @RF00870                                            0870 01103000
*             IF REGC=0 THEN                                       0871 01104000
         LTR   REGC,REGC                                           0871 01105000
         BZ    @RT00871                                            0871 01106000
*               ;                   /* NULL THEN IF LOCAL ENTRY      */ 01107000
*             ELSE                  /* IF REMOTE ENTRY               */ 01108000
