*   GEN(EJECT);                                                    0754 00726000
         EJECT                                                          00727000
*                                                                  0755 00728000
*/********************************************************************/ 00729000
*/*                                                                  */ 00730000
*/* SEGMENT-                                                         */ 00731000
*/*      SHIPOT3                                                     */ 00732000
*/*                                                                  */ 00733000
*/* FUNCTION-                                                        */ 00734000
*/*      SET UP THE QUIESCE FSB BY MOVING ZOBBTU FIELDS TO THE ZIBFSB*/ 00735000
*/*      IF A REMOTE ENTRY, GO TO SHIPOT4  TO CLEAR THE REMOTE       */ 00736000
*/*          START I/O QUEUE. DEQUEUE THE REMOTE ICNCB FROM LOCAL'S  */ 00737000
*/*          REMOTE START I/O QUEUE AND QUEUE THE FSB TO THE         */ 00738000
*/*          FEEDBACK ROUTER                                         */ 00739000
*/*      IF A LOCAL ENTRY, GO TO SHIPOT6 TO CLEAR ALL START I/O      */ 00740000
*/*          QUEUES (LOCAL AND REMOTE). IF I/O BUSY, PUT THE FSB     */ 00741000
*/*          ON THE SICK FSB QUEUE. IF NOT BUSY, QUEUE THE FSB TO    */ 00742000
*/*          TO THE FEEDBACK ROUTER                                  */ 00743000
*/*                                                                  */ 00744000
*/********************************************************************/ 00745000
*/* SEGMENT(SHIPOT3)START                                            */ 00746000
*                                                                  0755 00747000
*   DO;                                                            0755 00748000
*     GOTO SHIPOT3@;                                               0756 00749000
         B     SHIPOT3@                                            0756 00750000
*SHIPOT3:                                                          0757 00751000
*   END;                                                           0757 00752000
SHIPOT3  DS    0H                                                  0758 00753000
*   RFY                                                            0758 00754000
*     ISTZIBUF BASED(REGA);         /* INPUT BUFFER                  */ 00755000
*   RFY                                                            0759 00756000
*     ISTFSB BASED(ADDR(ZIBFSB));   /* FEEDBACK STATUS BLOCK-INPUT   */ 00757000
*   ZIBBTU=ZOBBTU;                  /* MOVE PIU TO ZIB POSITION    0760 00758000
*                                                            @Z30APGW*/ 00759000
         MVC   ZIBBTU(20,REGA),ZOBBTU(REGA)                        0760 00760000
*   THDAF=BTUSORCE;                 /* SWAP ZIB DAF          @Z30APGW*/ 00761000
         LA    @15,48                                              0761 00762000
         ALR   @15,REGA                                            0761 00763000
         LA    @14,28                                              0761 00764000
         ALR   @14,REGA                                            0761 00765000
         MVC   THDAF(2,@15),BTUSORCE(@14)                          0761 00766000
*   THOAF=BTUDEST;                  /* AND SAF               @Z30APGW*/ 00767000
         MVC   THOAF(2,@15),BTUDEST(@14)                           0762 00768000
*   FSBTYPE=FSTYPE;                 /* SET FSB TYPE CODE             */ 00769000
         LA    @14,16                                              0763 00770000
         ALR   @14,REGA                                            0763 00771000
         MVI   FSBTYPE(@14),X'02'                                  0763 00772000
*   FSBLNGTH=LENGTH(ISTFSB);        /* SET FSB LENGTH                */ 00773000
         MVI   FSBLNGTH(@14),X'2C'                                 0764 00774000
*   FSBRQTAG=THSNF;                 /* SET FSB SEQ NUMBER    @XL03HCH*/ 00775000
         MVC   FSBRQTAG(2,@14),THSNF(@15)                          0765 00776000
*/* SET FSB SAF AND DAF FROM TH DAF AND OAF RESPECTIVELY SINCE DAF   */ 00777000
*/* AND SAF WERE REVERSED IN THE TH AND SHOULD NOT BE IN THE FSB     */ 00778000
*/*                                                          @ZA13638*/ 00779000
*                                                                  0766 00780000
*   FSBSAF=THDAF;                   /* SET FSB SAF = THE ADDRESS OF     00781000
*                                      THE APPLICATION       @ZA13638*/ 00782000
         MVC   FSBSAF(2,@14),THDAF(@15)                            0766 00783000
*   FSBDAF=THOAF;                   /* SET FSB DAF = THE ADDRESS OF     00784000
*                                      THE DEVICE            @ZA13638*/ 00785000
         MVC   FSBDAF(2,@14),THOAF(@15)                            0767 00786000
*   FSBRHFLD=RHFLAGS;               /* SET FSB RH            @XL03HCH*/ 00787000
         LA    @15,58                                              0768 00788000
         ALR   @15,REGA                                            0768 00789000
         MVC   FSBRHFLD(3,@14),RHFLAGS(@15)                        0768 00790000
*   RHFLAGS=RHRESP;                 /* SET RH RESPONSE MASK  @Z30APGW*/ 00791000
         MVC   RHFLAGS(3,@15),@CB03173                             0769 00792000
*   ADDR(RHFLAGS)->RHSCI=ON;        /* FLAG AS               @ZM41308*/ 00793000
*   ADDR(RHFLAGS)->RHTYPE=ON;       /* SESSION CONTROL       @ZM41308*/ 00794000
         OI    RHSCI(@15),B'01100000'                              0771 00795000
*   FSBMJRCD=FRNORM;                /* SET FSB MAJOR RETURN CODE TO     00796000
*                                      NORMAL COMPLETION             */ 00797000
         MVI   FSBMJRCD(@14),X'00'                                 0772 00798000
*   FSBMNRCD=FRMOK;                 /* SET FSB MINOR RETURN CODE TO     00799000
*                                      OK SO FAR                     */ 00800000
         MVI   FSBMNRCD(@14),X'00'                                 0773 00801000
*   ZIBFDBZ=ZIBFDBZ&&ZIBFDBZ;       /* CLEAR TPIOS FDB               */ 00802000
         XC    ZIBFDBZ(12,REGA),ZIBFDBZ(REGA)                      0774 00803000
*   IF REGC^=0 THEN                 /* IF REMOTE ICNCB               */ 00804000
         LTR   REGC,REGC                                           0775 00805000
         BZ    @RF00775                                            0775 00806000
*EFBBREM3:                                                         0776 00807000
*     DO;                           /* THEN                          */ 00808000
EFBBREM3 DS    0H                                                  0777 00809000
*       REGD=REGC;                  /* SET REGD=@ REMOTE ICNCB       */ 00810000
         LR    REGD,REGC                                           0777 00811000
*       CALL SHIPOT4;               /* GO CLEAR SIOQ FOR REMOTE      */ 00812000
         BAL   @14,SHIPOT4                                         0778 00813000
*       CALL DEQUE;                 /* REMOVE REMOTE ICNCB   @Z30APGW*/ 00814000
         BAL   @14,DEQUE                                           0779 00815000
*       CALL QUEUE;                 /* Q FSB TO FEEDBK ROUTR @Z30APGW*/ 00816000
         BAL   @14,QUEUE                                           0780 00817000
*     END EFBBREM3;                 /* END DO-REMOTE ENTRY           */ 00818000
*   ELSE                            /* IF LOCAL ENTRY                */ 00819000
*EFBBLOC1:                                                         0782 00820000
*     DO;                           /* THEN                          */ 00821000
         B     @RC00775                                            0782 00822000
@RF00775 DS    0H                                                  0782 00823000
EFBBLOC1 DS    0H                                                  0783 00824000
*/* RESETING OF UCBATI TO BE DONE ONLY IF LCL RN PER VS41417 @ZM41417*/ 00825000
*                                                                  0783 00826000
*                                                                  0783 00827000
*/*VMDESET  ((RCREG),ZERO)                                           */ 00828000
*                                                                  0783 00829000
*       DO;                         /* INVOKE MODESET MACRO          */ 00830000
*         GEN;                                                     0784 00831000
