         BE    @RF00603                                            0603 00267000
*EFBBREM1:                                                         0604 00268000
*     DO;                           /* THEN                  @Z30APGW*/ 00269000
EFBBREM1 DS    0H                                                  0605 00270000
*       REGC=PTRICN;                /* SAVE REMOTE ICNCB ADDRESS   0605 00271000
*                                                            @Z30APGW*/ 00272000
         LR    REGC,PTRICN                                         0605 00273000
*       PTRICN=ICNLICNA;            /* GET ADDR OF LOCAL     @ZM31978*/ 00274000
         L     PTRICN,ICNLICNA(,PTRICN)                            0606 00275000
*     END EFBBREM1;                 /* END DO=REMOTE ICNCB   @Z30APGW*/ 00276000
*   ELSE                                                           0608 00277000
*     ;                             /*                       @Z30APGW*/ 00278000
@RF00603 DS    0H                                                  0609 00279000
*   RFY                                                            0609 00280000
