         CLR   GPR14P,@10                                          1095 01171000
         BE    @RT01095                                            1095 01172000
*       ;                                                          1096 01173000
*     ELSE                                                         1097 01174000
*       DO;                                                        1097 01175000
*         GPR00P=FMZCA047;          /* INITIALIZE QUIT FLAG          */ 01176000
         L     GPR00P,@CF01209                                     1098 01177000
*         DO WHILE(GPR00P^=GPR14P->WKECHAIN);                      1099 01178000
         B     @DE01099                                            1099 01179000
@DL01099 DS    0H                                                  1100 01180000
*           GPR14P=GPR14P->WKECHAIN;/* SEARCH FOR END OF CHAIN       */ 01181000
         L     GPR14P,WKECHAIN(,GPR14P)                            1100 01182000
*         END;                                                     1101 01183000
@DE01099 C     GPR00P,WKECHAIN(,GPR14P)                            1101 01184000
         BNE   @DL01099                                            1101 01185000
*       END;                                                       1102 01186000
*     GPR14P=GPR14P&FMZCB047;                                      1103 01187000
@RT01095 N     GPR14P,@CF02075                                     1103 01188000
*     PTRLSC=GPR14P;                                               1104 01189000
         LR    PTRLSC,GPR14P                                       1104 01190000
*     RFY                                                          1105 01191000
*      (GPR00P,                                                    1105 01192000
*       GPR01P,                                                    1105 01193000
*       GPR14P,                                                    1105 01194000
*       GPR15P) UNRSTD;                                            1105 01195000
*   END;                            /* END TPFEL GENERATION FIND   1106 01196000
*                                      OLDEST ELEMENT                */ 01197000
*   IF PTRLSC=0 THEN                                               1107 01198000
         LTR   PTRLSC,PTRLSC                                       1107 01199000
         BZ    @RT01107                                            1107 01200000
*     GOTO UNLOCK;                  /* NO WORK TO DO,PUNT    @ZA19516*/ 01201000
*   ELSE                                                           1109 01202000
*     ;                             /*                       @ZA19516*/ 01203000
*   RFY                                                            1110 01204000
*    (PTRG00,                                                      1110 01205000
*     PTRG14,                                                      1110 01206000
*     PTRG15) RSTD;                 /* RESTRICT WORK REGS    @XL03HFZ*/ 01207000
*   PTRG14=FSBSEQ;                  /* GET FSB SEQUENCE COUNTER      */ 01208000
         SLR   PTRG14,PTRG14                                       1111 01209000
         IC    PTRG14,FSBSEQ(,WORKG)                               1111 01210000
*   PTRG14=PTRG14&'0000000F'X;      /* AND OUT ALL BUT LOW 4 BITS    */ 01211000
         LA    @10,15                                              1112 01212000
         NR    PTRG14,@10                                          1112 01213000
*   PTRG15=LSCZMNRC;                /* GET LSCB SEQUENCE             */ 01214000
         SLR   PTRG15,PTRG15                                       1113 01215000
         IC    PTRG15,LSCZMNRC(,PTRLSC)                            1113 01216000
*   PTRG15=PTRG15&'0000000F'X;      /* AND OUT BUT LOW 4 BITS        */ 01217000
         NR    PTRG15,@10                                          1114 01218000
*   IF PTRG14=PTRG15 THEN                                          1115 01219000
         CR    PTRG14,PTRG15                                       1115 01220000
         BNE   @RF01115                                            1115 01221000
*     DO;                           /* IF SEQUENCE NOS. EQUAL        */ 01222000
*       RFY                                                        1117 01223000
*        (PTRG14,                                                  1117 01224000
*         PTRG15) UNRSTD;           /* UNRESTRICT WORK REGS          */ 01225000
*       PTRG00=4;                   /* SET FDBKTST PARAMETER         */ 01226000
         LA    PTRG00,4                                            1118 01227000
*       CALL FDBKTST;               /* TEST FOR EXCEPTION RESPONSE   */ 01228000
         BAL   @14,FDBKTST                                         1119 01229000
*       IF FSBMJRCD=FRCMDRST        /* IF COMMAND RESET      @XL03HFZ*/ 01230000
*           &FSBMNRCD=FRMCDRST THEN                                1120 01231000
         CLI   FSBMJRCD(WORKG),X'18'                               1120 01232000
         BNE   @RF01120                                            1120 01233000
         CLI   FSBMNRCD(WORKG),X'00'                               1120 01234000
         BNE   @RF01120                                            1120 01235000
*         ZCRENDLP='0'B;            /* CLEAR END LOOP SWITCH         */ 01236000
         L     @10,RPHCRR(,PTRRPH)                                 1121 01237000
         NI    ZCRENDLP+312(@10),B'11111011'                       1121 01238000
*       ELSE                                                       1122 01239000
*         ;                         /* OTHERWISE LEAVE IT ALONE      */ 01240000
@RF01120 DS    0H                                                  1123 01241000
*       IF FSBMNELI='1'B THEN       /* IF ERROR INDICATED IN FSB     */ 01242000
         TM    FSBMNELI(WORKG),B'10000000'                         1123 01243000
         BNO   @RF01123                                            1123 01244000
*         IF FSBLCCWA=LSCLCCWA|FMCMODE=FMCRCD|/*             @ZA19516*/ 01245000
*             LSCLCCWA=0 THEN                                      1124 01246000
         L     @10,LSCLCCWA(,PTRLSC)                               1124 01247000
         C     @10,FSBLCCWA(,WORKG)                                1124 01248000
         BE    @RT01124                                            1124 01249000
         CLC   FMCMODE(8,PTRFMC),@CC02939                          1124 01250000
         BE    @RT01124                                            1124 01251000
         LTR   @10,@10                                             1124 01252000
         BZ    @RT01124                                            1124 01253000
*           ;                       /* | LCCW POINTER NULL   @XM06429*/ 01254000
*         ELSE                      /* IF NOT LST LCCW               */ 01255000
*           ZCRDEQSW='0'B;          /* CLEAR DEQUEUE SWITCH          */ 01256000
         L     @10,RPHCRR(,PTRRPH)                                 1126 01257000
         NI    ZCRDEQSW+312(@10),B'11011111'                       1126 01258000
*       ELSE                        /* ERROR NOT INDICATED IN FSB    */ 01259000
*         IF FSBRT04='1'B THEN                                     1127 01260000
         B     @RC01123                                            1127 01261000
@RF01123 TM    FSBRT04(WORKG),B'00001000'                          1127 01262000
         BO    @RT01127                                            1127 01263000
*           ;                       /* IF FEEDBACK REQUIRED          */ 01264000
*         ELSE                      /* IF FEEDBACK NOT REQUIRED      */ 01265000
*           ZCRQUESW='0'B;          /* CLEAR QUEUE SWITCH    @XL03HFZ*/ 01266000
         L     @10,RPHCRR(,PTRRPH)                                 1129 01267000
         NI    ZCRQUESW+312(@10),B'10111111'                       1129 01268000
*     END;                          /* END DO-SEQ NOS EQUAL          */ 01269000
*   ELSE                                                           1131 01270000
*     DO;                           /* IF SEQ NOS NOT EQUAL          */ 01271000
         B     @RC01115                                            1131 01272000
@RF01115 DS    0H                                                  1132 01273000
*       ZCREXSW=OFF;                /* CLEAR EXIT SWITCH             */ 01274000
         L     @10,RPHCRR(,PTRRPH)                                 1132 01275000
         NI    ZCREXSW+312(@10),B'01111111'                        1132 01276000
*       IF LSCZRTL4='1'B THEN                                      1133 01277000
         TM    LSCZRTL4(PTRLSC),B'00001000'                        1133 01278000
         BNO   @RF01133                                            1133 01279000
*         DO;                       /* IF FEEDBACK           @XL03HFZ*/ 01280000
*           PTRG00=0;               /* SET NO-MATCH PARAMETER        */ 01281000
         SLR   PTRG00,PTRG00                                       1135 01282000
*           CALL FDBKTST;           /* TEST FOR EXCEPTION RESPONSE   */ 01283000
         BAL   @14,FDBKTST                                         1136 01284000
*           RFY                                                    1137 01285000
*             PTRG00 UNRSTD;        /* END OF USAGE          @XL03HFZ*/ 01286000
*           IF LSCZRTL4='1'B THEN                                  1138 01287000
         TM    LSCZRTL4(PTRLSC),B'00001000'                        1138 01288000
         BNO   @RF01138                                            1138 01289000
*             DO;                   /* IF FEEDBACK STILL REQUIRED    */ 01290000
*               ZCRPCOD1=0;         /* CLEAR PARAMETER BYTE          */ 01291000
         LA    @10,312                                             1140 01292000
         AL    @10,RPHCRR(,PTRRPH)                                 1140 01293000
         MVI   ZCRPCOD1(@10),X'00'                                 1140 01294000
*               ZCRPFSB=ON;         /* TURN ON BIT TO GET FSB        */ 01295000
         OI    ZCRPFSB(@10),B'00100000'                            1141 01296000
*               CALL ZPSZDFA0;      /* GO GET FSB                    */ 01297000
         L     @10,ATCZPSVT(,PTRATC)                               1142 01298000
         L     @15,ZPSZDFA0(,@10)                                  1142 01299000
         BALR  @14,@15                                             1142 01300000
*               ZCRERR=OFF;         /* CLEAR ERROR INDICATOR         */ 01301000
*               ZCRNOBUF=ON;        /* SET NO BUFFS INDICATOR        */ 01302000
         L     @10,RPHCRR(,PTRRPH)                                 1144 01303000
         OI    ZCRNOBUF+312(@10),B'10000000'                       1144 01304000
         NI    ZCRERR+312(@10),B'10111111'                         1144 01305000
*               WORKG=RPHSRPRM;     /* GET ADDRESS OF THE FSB        */ 01306000
         L     WORKG,RPHSRPRM(,PTRRPH)                             1145 01307000
*               CALL BUILDFSB;      /* GO SET UP FSB                 */ 01308000
         BAL   @14,BUILDFSB                                        1146 01309000
*             END;                  /* END FEEDBACK STILL REQUIRED   */ 01310000
*           ELSE                                                   1148 01311000
*             ;                     /* FEEDBACK NOT REQUIRED NOW     */ 01312000
@RF01138 DS    0H                                                  1149 01313000
*         END;                      /* END DO-FEEDBACK       @XL03HFZ*/ 01314000
*       ELSE                        /* IF NO FEEDBACK                */ 01315000
*         ZCRQUESW=OFF;             /* CLEAR QUEUE SWITCH            */ 01316000
         B     @RC01133                                            1150 01317000
@RF01133 L     @10,RPHCRR(,PTRRPH)                                 1150 01318000
         NI    ZCRQUESW+312(@10),B'10111111'                       1150 01319000
*     END;                          /* END DO-SEQ NOS NOT EQUAL      */ 01320000
@RC01133 DS    0H                                                  1152 01321000
*/* SEGMENT(USRPRC1)REQUEST                                          */ 01322000
*                                                                  1152 01323000
*   DO;                                                            1152 01324000
@RC01115 DS    0H                                                  1153 01325000
*     GOTO USRPRC1;                                                1153 01326000
         B     USRPRC1                                             1153 01327000
*USRPRC1#:                                                         1154 01328000
*   END;                            /* TEST FOR EXIT OR CLEANUP      */ 01329000
USRPRC1# DS    0H                                                  1155 01330000
*   IF ZCREXSW=OFF THEN             /* IF EXIT SWITCH OFF            */ 01331000
         L     @10,RPHCRR(,PTRRPH)                                 1155 01332000
         TM    ZCREXSW+312(@10),B'10000000'                        1155 01333000
         BZ    @RT01155                                            1155 01334000
*     GO TO GF0ALOOP;               /* DO UNTIL SIMULATION           */ 01335000
*   ELSE                                                           1157 01336000
*     ;                             /* IF EXIT SWITCH ON-NULL ELSE   */ 01337000
*/* SEGMENT(USERPRC)FINISH                                           */ 01338000
*                                                                  1158 01339000
*   DO;                                                            1158 01340000
*     GOTO USERPRC#;                                               1159 01341000
         B     USERPRC#                                            1159 01342000
*USERPRC@:                                                         1160 01343000
*   END;                                                           1160 01344000
*                                                                  1161 01345000
*/********************************************************************/ 01346000
*/*                                                                  */ 01347000
*/* SUBROUTINE-                                                      */ 01348000
*/*      BUILDFSB                                                    */ 01349000
*/*                                                                  */ 01350000
*/* FUNCTION-                                                        */ 01351000
*/*      SETS UP FEEDBACK.                                           */ 01352000
*/*      THE FSB RETURN CODE IS SET UP, THE REQUEST TAG, CSW STATUS  */ 01353000
*/*      AND ALL THE OTHER GOOD THINGS A FSB NEEDS IS SET INTO THE   */ 01354000
*/*      FSB.  THE FSB IS NOT QUEUED, THAT IS LEFT TO THE CALLER.    */ 01355000
*/*                                                                  */ 01356000
*/********************************************************************/ 01357000
*                                                                  1161 01358000
*BUILDFSB:                                                         1161 01359000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* BUILDFSB: SUBROUTINE          */ 01360000
         B     @PB00002                                            1161 01361000
BUILDFSB DS    0H                                                  1162 01362000
*   RFY                                                            1162 01363000
*     PTRG14 RSTD;                  /* TO SAVE RETURN ADDRESS        */ 01364000
*   ZCRSVF1=PTRG14;                 /* SAVE RETURN ADDRESS           */ 01365000
         L     @15,RPHCRR(,PTRRPH)                                 1163 01366000
         ST    PTRG14,ZCRSVF1+312(,@15)                            1163 01367000
*   RFY                                                            1164 01368000
*     PTRG14 UNRSTD;                /* SAVED                         */ 01369000
*   FSBSAF=FMCSAF;                  /* SET SOURCE IDENTIFIER         */ 01370000
         MVC   FSBSAF(2,WORKG),FMCSAF(PTRFMC)                      1165 01371000
*   FSBDAF=PTRDNC->NCBDAF;          /* SET DESTINATION ID            */ 01372000
         MVC   FSBDAF(2,WORKG),NCBDAF(PTRDNC)                      1166 01373000
*/* SEGMENT(R3270)REQUEST                                            */ 01374000
*                                                                  1167 01375000
*   DO;                                                            1167 01376000
*     GOTO R3270;                                                  1168 01377000
         B     R3270                                               1168 01378000
*R3270#:                                                           1169 01379000
*   END;                            /* RECORD 3270 CONSIDERATIONS    */ 01380000
R3270#   DS    0H                                                  1170 01381000
*   IF ZCRERR=ON THEN               /* IF ERROR FLAG SET             */ 01382000
         L     @10,RPHCRR(,PTRRPH)                                 1170 01383000
         TM    ZCRERR+312(@10),B'01000000'                         1170 01384000
         BNO   @RF01170                                            1170 01385000
*     CALL ERRORFSB;                /* SET UP ERROR FSB      @XL03HFZ*/ 01386000
         BAL   @14,ERRORFSB                                        1171 01387000
*   ELSE                            /* IF ERROR FLAG NOT SET         */ 01388000
*     IF ZCRNOBUF=ON THEN           /* IF FSB ONLY                   */ 01389000
         B     @RC01170                                            1172 01390000
@RF01170 L     @10,RPHCRR(,PTRRPH)                                 1172 01391000
         TM    ZCRNOBUF+312(@10),B'10000000'                       1172 01392000
         BNO   @RF01172                                            1172 01393000
*GF0AFSBO:                                                         1173 01394000
*       DO;                         /* PROCESS FSB ALONE             */ 01395000
GF0AFSBO DS    0H                                                  1174 01396000
*         IF LSCZRTL6=ON THEN                                      1174 01397000
         TM    LSCZRTL6(PTRLSC),B'00000010'                        1174 01398000
         BO    @RT01174                                            1174 01399000
*           ;                       /* IF WRITE REQUEST              */ 01400000
*         ELSE                      /* IF NOT WRITE                  */ 01401000
*           DO;                     /* FLAG EXTENDED RESPONSE        */ 01402000
*             FSBEBI=ON;            /* FLAG END OF BLOCK             */ 01403000
*             FSBEMI=ON;            /* FLAG END OF MESSAGE           */ 01404000
*             FSBETI=ON;            /* FLAG END OF TRANSMISSION      */ 01405000
*             FSBFBMI=ON;           /* FLAG 1ST BLOCK OF MESSAGE     */ 01406000
         OI    FSBEBI(WORKG),B'00111100'                           1180 01407000
*           END;                    /* END EXTENDED RESPONSE FLAGS   */ 01408000
*         FSBECI=ON;                /* FLAG END OF COMMAND           */ 01409000
@RT01174 OI    FSBECI(WORKG),B'01000000'                           1182 01410000
*         IF LSCTYPE=LSTYPE THEN    /* IF ELEMENT A LSCB     @XM06349*/ 01411000
         CLI   LSCTYPE(PTRLSC),X'DF'                               1183 01412000
         BNE   @RF01183                                            1183 01413000
*           FSBLCPB=LSCLCPBA;       /* SET LCPB ADDRESS IN FSB       */ 01414000
         L     @10,LSCLCPBA(,PTRLSC)                               1184 01415000
         ST    @10,FSBLCPB(,WORKG)                                 1184 01416000
*         ELSE                      /* IF ELEMENT A LCPB     @XM06349*/ 01417000
*           FSBLCPB=PTRLCP;         /* SET LCPB ADDRESS      @XM06349*/ 01418000
         B     @RC01183                                            1185 01419000
@RF01183 ST    PTRLCP,FSBLCPB(,WORKG)                              1185 01420000
*         FSBLCCWA=LSCLCCWA;        /* SET LCCW ADDRESS IN FSB       */ 01421000
@RC01183 L     @10,LSCLCCWA(,PTRLSC)                               1186 01422000
         ST    @10,FSBLCCWA(,WORKG)                                1186 01423000
*         FSBSYSRS=FSBNRMST;        /* INDICATE NORMAL COMPLETION    */ 01424000
         MVI   FSBSYSRS(WORKG),X'0C'                               1187 01425000
*       END GF0AFSBO;               /* END FSB ALONE PROCESS         */ 01426000
*     ELSE                          /* NOT FSB ALONE                 */ 01427000
*GF0ABUFO:                                                         1189 01428000
*       DO;                         /* PROCESS ZLBUF FSB             */ 01429000
         B     @RC01172                                            1189 01430000
@RF01172 DS    0H                                                  1189 01431000
GF0ABUFO DS    0H                                                  1190 01432000
*         IF ZLBRT08=ON THEN        /* IF DEVICE END NOTIFICATION    */ 01433000
         TM    ZLBRT08(PTRZLB),B'10000000'                         1190 01434000
         BNO   @RF01190                                            1190 01435000
*           DO;                     /* PROCESS DEVICE END            */ 01436000
*             FSBMJRCD=FRPHYSC;     /* SET MAJ RC=PHYSICAL ERROR     */ 01437000
         MVI   FSBMJRCD(WORKG),X'0C'                               1192 01438000
*             FSBMNRCD=FRMDENOT;    /* SET DE MINOR STATUS           */ 01439000
         MVI   FSBMNRCD(WORKG),X'01'                               1193 01440000
*             FSBDMGRS=ZLBSSDE;     /* SET SENSE AREA        @XL03HQV*/ 01441000
         MVC   FSBDMGRS(2,WORKG),@CB03819                          1194 01442000
*           END;                    /* END DEVICE END PROCESS        */ 01443000
*         ELSE                      /* NOT DEVICE END NOTIFICATION   */ 01444000
*GF0ANDE:                                                          1196 01445000
*           DO;                     /* INDICATE NORMAL RETURN CODE   */ 01446000
         B     @RC01190                                            1196 01447000
@RF01190 DS    0H                                                  1196 01448000
GF0ANDE  DS    0H                                                  1197 01449000
*             IF ZLBCOMP=ZLBCMDRT THEN/* IF A COMMAND RESET FSB      */ 01450000
         CLI   ZLBCOMP(PTRZLB),X'04'                               1197 01451000
         BNE   @RF01197                                            1197 01452000
*               DO;                 /* THEN                          */ 01453000
*                 FSBMJRCD=FRCMDRST;/* SET MAJ RC=COMMAND RESET      */ 01454000
         MVI   FSBMJRCD(WORKG),X'18'                               1199 01455000
*                 FSBMNRCD=FRMCDRST;/* SET MIN RC=COMMAND RESET      */ 01456000
         MVI   FSBMNRCD(WORKG),X'00'                               1200 01457000
*                 FSBECI=ON;        /* SET END OF COMMAND            */ 01458000
*                 FSBELI=ON;        /* SET END OF LCPB               */ 01459000
         OI    FSBECI(WORKG),B'11000000'                           1202 01460000
*               END;                /* END DO-COMMAND RESET FSB      */ 01461000
*             ELSE                  /* IF NOT COMMAND RESET FSB      */ 01462000
*               DO;                 /* THEN                          */ 01463000
         B     @RC01197                                            1204 01464000
@RF01197 DS    0H                                                  1205 01465000
*                 FSBMNRCD=FRMOK;   /* SET MIN RC=OK SO FAR          */ 01466000
         MVI   FSBMNRCD(WORKG),X'00'                               1205 01467000
*                 FSBMJRCD=FRNORM;  /* SET MAJ RC=NORMAL             */ 01468000
         MVI   FSBMJRCD(WORKG),X'00'                               1206 01469000
*                 IF ZLBCOMP=ZLBTRM THEN/* IF TRM FSB                */ 01470000
         CLI   ZLBCOMP(PTRZLB),X'18'                               1207 01471000
         BNE   @RF01207                                            1207 01472000
*                   FSBMNRCD=ZLBTRM;/* SET MIN RC=TRM                */ 01473000
         MVI   FSBMNRCD(WORKG),X'18'                               1208 01474000
*                 ELSE                                             1209 01475000
*                   ;               /* NULL ELSE-NOT TRM FSB         */ 01476000
@RF01207 DS    0H                                                  1210 01477000
*               END;                /* END DO-NOT RESET FSB          */ 01478000
*             FSBDMGRS=0;           /* CLEAR SENSE DATA AREA         */ 01479000
@RC01197 SLR   @10,@10                                             1211 01480000
         STH   @10,FSBDMGRS(,WORKG)                                1211 01481000
*           END GF0ANDE;            /* END DO-NOT DE NOTIFY          */ 01482000
*       END GF0ABUFO;               /* END DO-NOT FSB ONLY           */ 01483000
@RC01190 DS    0H                                                  1214 01484000
*   IF ZCRNOBUF=ON THEN             /* IF ONLY FSB                   */ 01485000
@RC01172 DS    0H                                                  1214 01486000
@RC01170 L     @10,RPHCRR(,PTRRPH)                                 1214 01487000
         TM    ZCRNOBUF+312(@10),B'10000000'                       1214 01488000
         BNO   @RF01214                                            1214 01489000
*     FSBRQTAG=LSCREQTG;            /* SET FSB REQUEST TAG           */ 01490000
         MVC   FSBRQTAG(2,WORKG),LSCREQTG(PTRLSC)                  1215 01491000
*   ELSE                            /* IF NOT FSB ONLY               */ 01492000
*GF0ABUFX:                                                         1216 01493000
*     DO;                           /* PROCESS ZLBUF FSB             */ 01494000
         B     @RC01214                                            1216 01495000
@RF01214 DS    0H                                                  1216 01496000
GF0ABUFX DS    0H                                                  1217 01497000
*       FSBRQTAG=ZLBRQTG;           /* SET FSB REQUEST TAG           */ 01498000
         MVC   FSBRQTAG(2,WORKG),ZLBRQTG(PTRZLB)                   1217 01499000
*       FSBCSWST=ZLBSTAT;           /* SET FSB CSW STATUS            */ 01500000
         MVC   FSBCSWST(2,WORKG),ZLBSTAT(PTRZLB)                   1218 01501000
*       FSBFSBA=0;                  /* CLEAR FSB CHAIN POINTER       */ 01502000
         SLR   @10,@10                                             1219 01503000
         ST    @10,FSBFSBA(,WORKG)                                 1219 01504000
*       FDBOFSET=Y;                 /* SET SYSTEM FDB OFFSET         */ 01505000
         L     @10,Y                                               1220 01506000
         STH   @10,FDBOFSET(,PTRZLB)                               1220 01507000
*       ZLBFDBS=ZLBFDBZ;            /* MOVE FDB TO SYSTEM LOCATION   */ 01508000
         MVC   ZLBFDBS(12,PTRZLB),ZLBFDBZ(PTRZLB)                  1221 01509000
*     END GF0ABUFX;                 /* END ZLBUF FSB PROCESS         */ 01510000
*   RFY                                                            1223 01511000
*     PTRG14 RSTD;                  /* TO RESTORE RETURN REGISTER    */ 01512000
@RC01214 DS    0H                                                  1224 01513000
*   PTRG14=ZCRSVF1;                 /* RESTORE RETURN REGISTER       */ 01514000
         L     @10,RPHCRR(,PTRRPH)                                 1224 01515000
         L     PTRG14,ZCRSVF1+312(,@10)                            1224 01516000
*   RFY                                                            1225 01517000
*     PTRG14 UNRSTD;                /* RESTORED                      */ 01518000
*                                                                  1226 01519000
*/********************************************************************/ 01520000
*/*                                                          @XL03HFZ*/ 01521000
*/* SEGMENT-                                                 @XL03HFZ*/ 01522000
*/*      R3270                                               @XL03HFZ*/ 01523000
*/*                                                          @XL03HFZ*/ 01524000
*/* FUNCTION-                                                @XL03HFZ*/ 01525000
*/*      SETS UP SESSION INDICATIONS FOR 3270 RECORD.        @XL03HFZ*/ 01526000
*/*                                                          @XL03HFZ*/ 01527000
*/********************************************************************/ 01528000
*                                                                  1226 01529000
*   /*****************************************************************/ 01530000
*   /*                                                               */ 01531000
*   /* SEGMENT(R3270)START                                           */ 01532000
*   /*                                                               */ 01533000
