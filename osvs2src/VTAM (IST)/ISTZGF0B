@PB00003 DS    0H                                                  0900 00849000
*   GEN NOSETS NODEFS NOREFS(EJECT);                               0900 00850000
         EJECT                                                          00851000
*                                                                  0901 00852000
*   /*****************************************************************/ 00853000
*   /*                                                               */ 00854000
*   /* GF0B0CC - SUBROUTINE FOR INOPERATIVE UNIT PROCESSING. THIS    */ 00855000
*   /* ROUTINE CHECKS IF THE INOPERATIVE NCB IS AN ICNCB, AND IF SO, */ 00856000
*   /* SETS THE HARD ERROR FLAG AND RESETS THE SLOWDOWN MODE FLAG.   */ 00857000
*   /* ADDITIONALLY, ANY RPHS ON THE SLOWDOWN PAB ARE DEQUEUED AND   */ 00858000
*   /* TPPOSTED. THE S/D CODE FOR THIS SUBROUTINE IS -->     @XL03HDP*/ 00859000
*   /*                                                               */ 00860000
*   /*****************************************************************/ 00861000
*                                                                  0901 00862000
*GF0B0CC:                                                          0901 00863000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @X50BHDP*/ 00864000
         B     @PB00004                                            0901 00865000
GF0B0CC  DS    0H                                                  0902 00866000
*   RFY                                                            0902 00867000
*     RETNREG RSTD;                 /*                       @X50BHDP*/ 00868000
*   ZCRSVF6=RETNREG;                /*                       @X50BHDP*/ 00869000
         ST    RETNREG,ZCRSVF6(,PTRZCR)                            0903 00870000
*   RFY                                                            0904 00871000
*     RETNREG UNRSTD;               /*                       @X50BHDP*/ 00872000
*   NCBCONF=ON;                     /* SET NCB SICK FLAG ON          */ 00873000
         OI    NCBCONF(ICNCBREG),B'01000000'                       0905 00874000
*   IF NCBTYPE=ICTYPE THEN          /* INOP NCB AN ICNCB ?           */ 00875000
         CLI   NCBTYPE(ICNCBREG),X'06'                             0906 00876000
         BNE   @RF00906                                            0906 00877000
*     DO;                           /* IF SO....                     */ 00878000
*       ICNZF00F=ON;                /* HARD ERROR                    */ 00879000
*       ICNSLOW=OFF;                /* NO MORE SLOWDOWN MODE         */ 00880000
         L     @09,ICNZF00F(,ICNCBREG)                             0909 00881000
@GL00010 LR    @15,@09                                             0909 00882000
         N     @15,@CB05660                                        0909 00883000
         O     @15,@CB05660+4                                      0909 00884000
         CS    @09,@15,ICNZF00F(ICNCBREG)                          0909 00885000
         BNE   @GL00010                                            0909 00886000
*       DO WHILE(ADDR(ICNZPAB1)->PABWEQP^=0);/* SO LONG AS THERE ARE    00887000
*                                      ANY RPHS ON SLOWDOWN PAB..    */ 00888000
         B     @DE00910                                            0910 00889000
@DL00910 DS    0H                                                  0911 00890000
*         RFY                                                      0911 00891000
*           R10 RSTD;               /* COMPILER MUSNT TOUCH          */ 00892000
*/* TPDEQ (,ICNZPAB1) RETPTR(R10)                                    */ 00893000
*                                                                  0912 00894000
