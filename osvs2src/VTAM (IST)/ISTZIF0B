*       FSBLCPB=ELEMREG;            /* SET LCPB ADDRESS IN FSB     0758 00952000
*                                                            @XL03HDP*/ 00953000
         ST    ELEMREG,FSBLCPB(,@14)                               0758 00954000
*     END;                          /*                       @XM03HDP*/ 00955000
*   FSBLCCWA=LCPNXLCW;              /* SET LCCW ADDRESS IN FSB     0760 00956000
*                                                            @XL03HDP*/ 00957000
*                                                                  0760 00958000
@RC00751 L     @14,LCPNXLCW(,ELEMREG)                              0760 00959000
         ST    @14,FSBLCCWA+16(,BFRREG)                            0760 00960000
*   /*****************************************************************/ 00961000
*   /*                                                               */ 00962000
*   /* SEGMENT(IF0B0CA)FINISH                                        */ 00963000
*   /*                                                               */ 00964000
*   /*****************************************************************/ 00965000
*                                                                  0761 00966000
*   DO;                                                            0761 00967000
*     GOTO IF0B0CA#;                                               0762 00968000
         B     IF0B0CA#                                            0762 00969000
*IF0B0CA@:                                                         0763 00970000
*   END;                                                           0763 00971000
IF0B0CA@ DS    0H                                                  0764 00972000
         EJECT                                                          00973000
*                                                                  0764 00974000
*/********************************************************************/ 00975000
*/*                                                                  */ 00976000
*/* SEGMENT-                                                         */ 00977000
*/*      IF0B0CB                                                     */ 00978000
*/*                                                                  */ 00979000
*/* FUNCTION-                                                        */ 00980000
*/*      HANDLE ERROR LCPB ON WAITQ                                  */ 00981000
*/*                                                                  */ 00982000
*/********************************************************************/ 00983000
*                                                                  0764 00984000
*   /*****************************************************************/ 00985000
*   /*                                                               */ 00986000
*   /* SEGMENT(IF0B0CB)START                                         */ 00987000
*   /*                                                               */ 00988000
*   /*****************************************************************/ 00989000
*                                                                  0764 00990000
*   DO;                                                            0764 00991000
*     GOTO IF0B0CB@;                                               0765 00992000
         B     IF0B0CB@                                            0765 00993000
*IF0B0CB:                                                          0766 00994000
*   END;                                                           0766 00995000
IF0B0CB  DS    0H                                                  0767 00996000
*   CALL IF0B000N;                  /* REMOVE ERROR LCPB FROM WAITQ     00997000
*                                                            @XL03HEK*/ 00998000
         BAL   @14,IF0B000N                                        0767 00999000
*   IF FMCDNCBA->NCBTYPE=ICTYPE THEN                               0768 01000000
         L     @14,FMCDNCBA(,FMCBREG)                              0768 01001000
         CLI   NCBTYPE(@14),X'06'                                  0768 01002000
