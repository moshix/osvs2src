*/*      SET UP FOR REQUEST TAG COMPARE                              */ 00772000
*/*                                                                  */ 00773000
*/********************************************************************/ 00774000
*                                                                  0718 00775000
*   /*****************************************************************/ 00776000
*   /*                                                               */ 00777000
*   /* SEGMENT(IF0B00B)START                                         */ 00778000
*   /*                                                               */ 00779000
*   /*****************************************************************/ 00780000
*                                                                  0718 00781000
*   DO;                                                            0718 00782000
*     GOTO IF0B00B@;                                               0719 00783000
         B     IF0B00B@                                            0719 00784000
*IF0B00B:                                                          0720 00785000
*   END;                            /*                       @XL03HEK*/ 00786000
IF0B00B  DS    0H                                                  0721 00787000
*   CALL IF0B000K;                  /* GET ADDR OLDEST WAITQ ELEMENT    00788000
*                                                            @XL03HEK*/ 00789000
         BAL   @14,IF0B000K                                        0721 00790000
*   CMPRREG=LSCREQTG;               /* GET ELEMENT REQUEST TAG       */ 00791000
         LH    CMPRREG,LSCREQTG(,ELEMREG)                          0722 00792000
         N     CMPRREG,@CF03646                                    0722 00793000
*   CMPRREG=CMPRREG&TPGENMSK;       /* CLR LINESTATE ANALYSIS FLAG   */ 00794000
*                                                                  0723 00795000
         N     CMPRREG,@CF03450                                    0723 00796000
*   /*****************************************************************/ 00797000
*   /*                                                               */ 00798000
*   /* SEGMENT(IF0B00B)FINISH                                        */ 00799000
*   /*                                                               */ 00800000
