@DL00836 DS    0H                                                  0837 01245000
*     IF(CMPRREG->LSCCHAI2=EMPTY) THEN/* END OF WAIT QUEUE   @XL03HDP*/ 01246000
         L     @14,LSCCHAI2-1(,CMPRREG)                            0837 01247000
         LA    @14,0(,@14)                                         0837 01248000
         LTR   @14,@14                                             0837 01249000
         BNZ   @RF00837                                            0837 01250000
*       DO;                         /* YES,                          */ 01251000
*         CALL IF0B00DB;            /* DEALLOC LSCBS,SET FSB ADDR    */ 01252000
         BAL   @14,IF0B00DB                                        0839 01253000
*         ZCRSCANQ=OFF;             /* STOP QUEUE SCAN               */ 01254000
         NI    ZCRSCANQ(PTRZCR),B'11011111'                        0840 01255000
*       END;                                                       0841 01256000
*     ELSE                          /* NO (NOT END OF WAITQ)         */ 01257000
*       DO;                                                        0842 01258000
         B     @RC00837                                            0842 01259000
@RF00837 DS    0H                                                  0843 01260000
*         CMPRREG=CMPRREG->LSCCHAIN;/* GET NXT OLDER ELEM ADDR       */ 01261000
