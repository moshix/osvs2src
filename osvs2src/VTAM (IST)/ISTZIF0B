*                                                                  0724 00802000
*   DO;                                                            0724 00803000
*     GOTO IF0B00B#;                                               0725 00804000
         B     IF0B00B#                                            0725 00805000
*IF0B00B@:                                                         0726 00806000
*   END;                            /*                       @XL03HEK*/ 00807000
IF0B00B@ DS    0H                                                  0727 00808000
         EJECT                                                          00809000
*                                                                  0727 00810000
*/********************************************************************/ 00811000
*/*                                                                  */ 00812000
*/* SUBROUTINE-                                                      */ 00813000
*/*      IF0B00C                                                     */ 00814000
*/*                                                                  */ 00815000
*/* FUNCTION-                                                        */ 00816000
*/*      BUILD AN ERROR FSB AND QUEUE IT TO THE CLIP.                */ 00817000
*/*                                                                  */ 00818000
*/********************************************************************/ 00819000
*                                                                  0727 00820000
*   /*****************************************************************/ 00821000
*   /*                                                               */ 00822000
*   /* SEGMENT(IF0B00C)START                                         */ 00823000
*   /*                                                               */ 00824000
*   /*****************************************************************/ 00825000
*                                                                  0727 00826000
*   DO;                                                            0727 00827000
*     GOTO IF0B00C@;                                               0728 00828000
         B     IF0B00C@                                            0728 00829000
*IF0B00C:                                                          0729 00830000
*   END;                            /*                       @XL03HEK*/ 00831000
*                                                                  0729 00832000
IF0B00C  DS    0H                                                  0730 00833000
*   /*****************************************************************/ 00834000
*   /*                                                               */ 00835000
*   /* SEGMENT(IF0B0CA)REQUEST                                       */ 00836000
*   /*                                                               */ 00837000
*   /*****************************************************************/ 00838000
*                                                                  0730 00839000
*   DO;                                                            0730 00840000
*     GOTO IF0B0CA;                                                0731 00841000
         B     IF0B0CA                                             0731 00842000
*IF0B0CA#:                                                         0732 00843000
*   END;                            /* ALLOCATE AND INITIALIZE FSB 0732 00844000
*                                      FOR ELEMENT ERROR@XL03HDP     */ 00845000
*                                                                  0732 00846000
IF0B0CA# DS    0H                                                  0733 00847000
*   /*****************************************************************/ 00848000
*   /*                                                               */ 00849000
*   /* SEGMENT(IF0B0CB)REQUEST                                       */ 00850000
*   /*                                                               */ 00851000
*   /*****************************************************************/ 00852000
*                                                                  0733 00853000
*   DO;                                                            0733 00854000
*     GOTO IF0B0CB;                                                0734 00855000
         B     IF0B0CB                                             0734 00856000
*IF0B0CB#:                                                         0735 00857000
*   END;                            /* QUEUE FSB TO CLIP AND SET LINE   00858000
*                                      STATE                 @XL03HDP*/ 00859000
IF0B0CB# DS    0H                                                  0736 00860000
*   IF ZCR3270R=ON THEN             /* IF RECORD 3270..      @XM03HDP*/ 00861000
         TM    ZCR3270R(PTRZCR),B'00000010'                        0736 00862000
         BNO   @RF00736                                            0736 00863000
*     RCDREG=ZCRSRCHQ;              /* GO SEARCH WAITQ       @XM03HDP*/ 00864000
         LA    RCDREG,16                                           0737 00865000
*   ELSE                            /* NOT 3270 RECORD       @XM03HDP*/ 00866000
*     RCDREG=ZCRNOSCH;              /* SET RTN CODE          @XL03HDP*/ 00867000
*                                                                  0738 00868000
         B     @RC00736                                            0738 00869000
@RF00736 LA    RCDREG,32                                           0738 00870000
*   /*****************************************************************/ 00871000
*   /*                                                               */ 00872000
*   /* SEGMENT(IF0B00C)FINISH                                        */ 00873000
*   /*                                                               */ 00874000
*   /*****************************************************************/ 00875000
*                                                                  0739 00876000
*   DO;                                                            0739 00877000
@RC00736 DS    0H                                                  0740 00878000
*     GOTO IF0B00C#;                                               0740 00879000
         B     IF0B00C#                                            0740 00880000
*IF0B00C@:                                                         0741 00881000
*   END;                            /*                       @XL03HEK*/ 00882000
IF0B00C@ DS    0H                                                  0742 00883000
         EJECT                                                          00884000
*                                                                  0742 00885000
*/********************************************************************/ 00886000
*/*                                                                  */ 00887000
*/* SEGMENT-                                                         */ 00888000
*/*      IF0B0CA                                                     */ 00889000
*/*                                                                  */ 00890000
*/* FUNCTION-                                                        */ 00891000
*/*      CONSTRUCT FSB FROM ERROR WAITQ ELEMENT'S FSB                */ 00892000
*/*                                                                  */ 00893000
