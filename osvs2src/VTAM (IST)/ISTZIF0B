*   CALL ISTZDFB0;                  /* DEALLOCATE LSCB               */ 01447000
         L     @10,ATCZPSVT(,VTCVTREG)                             0902 01448000
         L     @15,ZPSZDFB0(,@10)                                  0902 01449000
         BALR  @14,@15                                             0902 01450000
*   IF(WKREGB->LSCCHAI2=EMPTY) THEN /*                       @XL03HDP*/ 01451000
         L     @14,LSCCHAI2-1(,WKREGB)                             0903 01452000
         LA    @14,0(,@14)                                         0903 01453000
