*                                                                  0742 00901000
*   DO;                                                            0742 00902000
*     GOTO IF0B0CA@;                                               0743 00903000
         B     IF0B0CA@                                            0743 00904000
*IF0B0CA:                                                          0744 00905000
*   END;                                                           0744 00906000
IF0B0CA  DS    0H                                                  0745 00907000
*   FSBMNRCD=LCPMNRCD;              /* MOVE MINOR RETURN CODE FROM 0745 00908000
*                                      ELEMENT TO FSB        @XL03HDP*/ 00909000
         LA    @14,16                                              0745 00910000
         ALR   @14,BFRREG                                          0745 00911000
         MVC   FSBMNRCD(1,@14),LCPMNRCD(ELEMREG)                   0745 00912000
*   IF LCPZRT01=ON THEN             /* CHECK ELEM REQ TAG FOR CONTROL   00913000
*                                      BLOCK ERROR                   */ 00914000
         TM    LCPZRT01(ELEMREG),B'01000000'                       0746 00915000
         BNO   @RF00746                                            0746 00916000
*     FSBMJRCD=FRCBLKE;             /* YES,SET FSB MAJ RETURN CODE TO   00917000
*                                      CONTROL BLK ERROR     @XL03HDP*/ 00918000
         MVI   FSBMJRCD(@14),X'04'                                 0747 00919000
*   ELSE                                                           0748 00920000
*     FSBMJRCD=FRLOGICE;            /* NO,SET FSB MAJOR RETURN CODE     00921000
*                                      TO LOGICAL ERROR      @XL03HDP*/ 00922000
         B     @RC00746                                            0748 00923000
@RF00746 MVI   FSBMJRCD+16(BFRREG),X'08'                           0748 00924000
*   FSBECI=ON;                      /* SET LAST FSB FOR LCCW SW    0749 00925000
*                                                            @XL03HDP*/ 00926000
@RC00746 DS    0H                                                  0750 00927000
*   FSBELI=ON;                      /* SET LAST FSB FOR LCPB SW    0750 00928000
*                                                            @XL03HDP*/ 00929000
         LA    @14,16                                              0750 00930000
         ALR   @14,BFRREG                                          0750 00931000
         OI    FSBECI(@14),B'11000000'                             0750 00932000
*   IF LSCTYPE=LSTYPE THEN          /* DO WE HAVE AN LSCB?   @XM03HDP*/ 00933000
         CLI   LSCTYPE(ELEMREG),X'DF'                              0751 00934000
         BNE   @RF00751                                            0751 00935000
*     DO;                           /* IF SO...              @XM03HDP*/ 00936000
*       FSBLCPB=LSCLCPBA;           /* SET LCPB @ IN FSB     @XM03HDP*/ 00937000
         L     @10,LSCLCPBA(,ELEMREG)                              0753 00938000
         ST    @10,FSBLCPB(,@14)                                   0753 00939000
*       FSBCID=LSCLCPBA->LCPCID;    /* COPY CID TO FSB       @XM03HDP*/ 00940000
         MVC   FSBCID(4,@14),LCPCID(@10)                           0754 00941000
*     END;                          /*                       @XM03HDP*/ 00942000
*   ELSE                            /* MUST BE AN LCPB       @XM03HDP*/ 00943000
*     DO;                           /*                       @XM03HDP*/ 00944000
         B     @RC00751                                            0756 00945000
@RF00751 DS    0H                                                  0757 00946000
*       FSBCID=LCPCID;              /* MOVE CID FROM LCPB TO FSB   0757 00947000
*                                                            @XL03HDP*/ 00948000
         LA    @14,16                                              0757 00949000
         ALR   @14,BFRREG                                          0757 00950000
