*   ELSE                                                           0774 01026000
*     ;                             /*                       @XM09724*/ 01027000
@RF00772 DS    0H                                                  0775 01028000
*   ZIBFDBS=ZIBFDBZ;                /* COPY SYS FDB TO TPIOS FDB   0775 01029000
*                                                            @XL03HDP*/ 01030000
         MVC   ZIBFDBS(12,BFRREG),ZIBFDBZ(BFRREG)                  0775 01031000
*   ADDR(ZIBFDBS)->FDBOFSET=ADDR(ZIBFDBS)-ADDR(ISTZIBUF);/* COMPUTE     01032000
*                                      FDB OFFSET            @XL03HDP*/ 01033000
         LA    @14,ZIBFDBS(,BFRREG)                                0776 01034000
         SLR   @14,BFRREG                                          0776 01035000
         STH   @14,FDBOFSET+48(,BFRREG)                            0776 01036000
*/*  TPQUE (ADDR(ISTFSB),FMCPAB2) NORM                               */ 01037000
*                                                                  0777 01038000
*   DO;                                                            0777 01039000
*     RFY                                                          0778 01040000
*      (GPR00P,                                                    0778 01041000
*       GPR01P,                                                    0778 01042000
*       GPR04P,                                                    0778 01043000
*       GPR05P,                                                    0778 01044000
*       GPR15P) RSTD;                                              0778 01045000
*     GEN REFS(GPR01P) SETS(RPHWORK)(STM GPR02P,GPR08P,RPHWORK(GPR01P)) 01046000
*         ;                                                        0779 01047000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              01048000
*     GPR00P=0;                                                    0780 01049000
         SLR   GPR00P,GPR00P                                       0780 01050000
*     GPR15P=ADDR(FMCPAB2);         /* HOLD PAB PTR                  */ 01051000
         LA    GPR15P,FMCPAB2(,FMCBREG)                            0781 01052000
*     GPR04P=ADDR(ISTFSB);          /* WORK ELE PTR                  */ 01053000
         LA    GPR04P,16                                           0782 01054000
         ALR   GPR04P,BFRREG                                       0782 01055000
*     GPR05P=GPR15P;                /* PASS PAB PTR                  */ 01056000
         LR    GPR05P,GPR15P                                       0783 01057000
*     CALL ATCLCPTR->ATCAP37->ISTAPC37;                            0784 01058000
         L     @10,ATCLCPTR                                        0784 01059000
         L     @15,ATCAP37(,@10)                                   0784 01060000
         BALR  @14,@15                                             0784 01061000
*     GEN(NOPR  0)                                                 0785 01062000
*     NOSETS NOREFS;                                               0785 01063000
         NOPR  0                                                        01064000
*     RFY                                                          0786 01065000
*      (GPR00P,                                                    0786 01066000
*       GPR01P,                                                    0786 01067000
*       GPR04P,                                                    0786 01068000
*       GPR05P,                                                    0786 01069000
*       GPR15P) UNRSTD;                                            0786 01070000
*   END;                            /* *** END OF TPQUE GENERATION 0787 01071000
*                                      PUT FSB ON CLIP, SCHEDULE PAB    01072000
*                                                            @XL03HDP*/ 01073000
*   IF LSCTYPE=LSTYPE THEN          /* IS THIS AN LSCB ?     @XM03HDP*/ 01074000
         CLI   LSCTYPE(ELEMREG),X'DF'                              0788 01075000
         BNE   @RF00788                                            0788 01076000
*     DO;                           /* IF SO...              @XM03HDP*/ 01077000
*       ZCRPCOD1=ZCPLSCB;           /* FREE IT BY CALLING    @XM03HDP*/ 01078000
         MVI   ZCRPCOD1(PTRZCR),X'08'                              0790 01079000
*       CALL ISTZDFB0;              /* LSCB/BUFFER DEALLOC.  @XM03HDP*/ 01080000
         L     @10,ATCZPSVT(,VTCVTREG)                             0791 01081000
         L     @15,ZPSZDFB0(,@10)                                  0791 01082000
         BALR  @14,@15                                             0791 01083000
*     END;                          /*                       @XM03HDP*/ 01084000
*   ELSE                                                           0793 01085000
*     ;                             /* MUST BE AN LCPB       @XM03HDP*/ 01086000
*                                                                  0793 01087000
@RF00788 DS    0H                                                  0794 01088000
*   /*****************************************************************/ 01089000
*   /*                                                               */ 01090000
*   /* SEGMENT(IF0B0CB)FINISH                                        */ 01091000
*   /*                                                               */ 01092000
*   /*****************************************************************/ 01093000
*                                                                  0794 01094000
*   DO;                                                            0794 01095000
*     GOTO IF0B0CB#;                                               0795 01096000
         B     IF0B0CB#                                            0795 01097000
*IF0B0CB@:                                                         0796 01098000
*   END;                                                           0796 01099000
IF0B0CB@ DS    0H                                                  0797 01100000
         EJECT                                                          01101000
*                                                                  0797 01102000
*/********************************************************************/ 01103000
*/*                                                                  */ 01104000
*/* SEGMENT-                                                         */ 01105000
*/*      IF0B00D                                                     */ 01106000
*/*                                                                  */ 01107000
*/* FUNCTION-                                                        */ 01108000
*/*      MATCH FSB WITH WAITQ ELEMENT (USING REQUEST TAG COMPARE)    */ 01109000
*/*                                                                  */ 01110000
*/********************************************************************/ 01111000
*                                                                  0797 01112000
*   /*****************************************************************/ 01113000
*   /*                                                               */ 01114000
*   /* SEGMENT(IF0B00D)START                                         */ 01115000
*   /*                                                               */ 01116000
*   /*****************************************************************/ 01117000
*                                                                  0797 01118000
*   DO;                                                            0797 01119000
*     GOTO IF0B00D@;                                               0798 01120000
         B     IF0B00D@                                            0798 01121000
*IF0B00D:                                                          0799 01122000
*   END;                            /*                       @XL03HEK*/ 01123000
IF0B00D  DS    0H                                                  0800 01124000
*   ZCRMOREL=ON;                    /* SET MORE ELEM ON WAITQ SW     */ 01125000
*   ZCRNXTEL=OFF;                   /* SET USE NEXT WAITQ ELEM SW    */ 01126000
         OI    ZCRMOREL(PTRZCR),B'01000000'                        0801 01127000
         NI    ZCRNXTEL(PTRZCR),B'11101111'                        0801 01128000
*   DO WHILE(ZCRMOREL=ON);          /* PROCESS WHILE MORE WAITQ ELEMS   01129000
*                                      REMAIN OR UNTIL REQUEST TAG 0802 01130000
*                                      MATCH                         */ 01131000
         B     @DE00802                                            0802 01132000
@DL00802 DS    0H                                                  0803 01133000
*     IF ZCRNXTEL=ON THEN           /* ANOTHER ELEM AVAIL ON WAITQ   */ 01134000
         TM    ZCRNXTEL(PTRZCR),B'00010000'                        0803 01135000
         BNO   @RF00803                                            0803 01136000
*       ELEMREG=LSCCHAIN;           /* YES, GET ITS ADDRESS          */ 01137000
         L     ELEMREG,LSCCHAIN(,ELEMREG)                          0804 01138000
*     ELSE                                                         0805 01139000
*       DO;                         /* NO,                           */ 01140000
         B     @RC00803                                            0805 01141000
@RF00803 DS    0H                                                  0806 01142000
*         ELEMREG=FMCLCPBQ;         /* GET ADDR NEWEST WAITQ ELEM    */ 01143000
         L     ELEMREG,FMCLCPBQ(,FMCBREG)                          0806 01144000
*         ZCRNXTEL=ON;              /* SET USE NEXT WAITQ ELEM SW    */ 01145000
         OI    ZCRNXTEL(PTRZCR),B'00010000'                        0807 01146000
*       END;                                                       0808 01147000
*     CMPRREG=LSCREQTG;             /* GET THIS ELEM'S REQUEST TAG   */ 01148000
@RC00803 LH    CMPRREG,LSCREQTG(,ELEMREG)                          0809 01149000
         N     CMPRREG,@CF03646                                    0809 01150000
*     CMPRREG=CMPRREG&TPGENMSK;     /* CLEAR LINE STATE ANLYS FLAG   */ 01151000
         N     CMPRREG,@CF03450                                    0810 01152000
*     IF CMPRREG=FSBRQTAG THEN      /* ELEM REQTAG = FSB REQTAG?     */ 01153000
         LH    @14,FSBRQTAG+16(,BFRREG)                            0811 01154000
         N     @14,@CF03646                                        0811 01155000
         CR    CMPRREG,@14                                         0811 01156000
         BNE   @RF00811                                            0811 01157000
*       DO;                         /* YES,                          */ 01158000
*         CALL IF0B000L;            /* CHAIN FSB ON WAITQ ELEMENT  0813 01159000
*                                                            @XL03HEK*/ 01160000
*                                                                  0813 01161000
         BAL   @14,IF0B000L                                        0813 01162000
*         /***********************************************************/ 01163000
*         /*                                                         */ 01164000
*         /* SEGMENT(IF0B0DA)REQUEST                                 */ 01165000
*         /*                                                         */ 01166000
*         /***********************************************************/ 01167000
*                                                                  0814 01168000
*         DO;                                                      0814 01169000
*           GOTO IF0B0DA;                                          0815 01170000
         B     IF0B0DA                                             0815 01171000
*IF0B0DA#:                                                         0816 01172000
*         END;                      /* SCAN WAITQ FOR LSCB W/STPR  0816 01173000
*                                                            @XL03HEK*/ 01174000
IF0B0DA# DS    0H                                                  0817 01175000
*         ZCRMOREL=OFF;             /* SET NO MORE WAITQ ELEMS       */ 01176000
         NI    ZCRMOREL(PTRZCR),B'10111111'                        0817 01177000
*       END;                                                       0818 01178000
*     ELSE                          /* NO REQ TAG MATCH,             */ 01179000
*       DO;                                                        0819 01180000
         B     @RC00811                                            0819 01181000
@RF00811 DS    0H                                                  0820 01182000
*         IF(LSCCHAI2^=EMPTY) THEN  /* CHAIN EMPTY?          @XL03HDP*/ 01183000
         L     @14,LSCCHAI2-1(,ELEMREG)                            0820 01184000
         LA    @14,0(,@14)                                         0820 01185000
         LTR   @14,@14                                             0820 01186000
         BNZ   @RT00820                                            0820 01187000
*           ;                       /* YES, CONTINUE WAITQ SCAN      */ 01188000
*         ELSE                                                     0822 01189000
*           DO;                     /* NO,                           */ 01190000
*             ELEMREG=EMPTY;        /* NULLIFY FSB PTR               */ 01191000
         SLR   ELEMREG,ELEMREG                                     0823 01192000
*             ZCRMOREL=OFF;         /* SET NO MORE WAITQ ELEMS       */ 01193000
         NI    ZCRMOREL(PTRZCR),B'10111111'                        0824 01194000
*           END;                                                   0825 01195000
*       END;                                                       0826 01196000
@RT00820 DS    0H                                                  0827 01197000
*   END;                                                           0827 01198000
*                                                                  0827 01199000
@RC00811 DS    0H                                                  0827 01200000
