*   ZCRPLSCB=ON;                    /* SET SW FOR LSCB DEALLOC       */ 01439000
         OI    ZCRPLSCB(PTRZCR),B'00001000'                        0899 01440000
*F0BDU010:                                                         0900 01441000
*   ;                               /* LOOP START POINT (NULL)       */ 01442000
