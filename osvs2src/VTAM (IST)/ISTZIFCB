         TM    FSBXFSTS+16(@11),B'10100000'                        0632 00533000
         BNZ   @RF00632                                            0632 00534000
*         WRKREGB->FDBDATCT=WRKREGB->FDBDATCT-1;/* ADJUST LAST HDR 0633 00535000
*                                      DATA COUNT TO IGNORE DLE      */ 00536000
