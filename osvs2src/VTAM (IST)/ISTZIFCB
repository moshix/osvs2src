         L     @11,ZCRTBPTR(,ZCRREG)                               0618 00483000
         STH   @14,FDBDATCT(,@11)                                  0618 00484000
*   ZCRTBPTR->FDBDATAA=0;           /* SET DATA ADDRESS TO ZERO      */ 00485000
         ST    @14,FDBDATAA(,@11)                                  0619 00486000
*   FDBDATAA=ADDR(BTUTEXT);         /* READJUST HDR DATA ADDRESS BACK   00487000
*                                      TO HEADER DATA                */ 00488000
