*/*                                                                  */ 00421000
*                                                                  0602 00422000
*   RFY                                                            0602 00423000
*     ISTZIBUF BASED(ZCRTBPTR);     /* NEW BUFFER                    */ 00424000
*   FSBFBMI=OFF;                    /* SET OFF FIRST BLOCK OF MSG IN    00425000
*                                      HEADER FSB                    */ 00426000
         L     @10,ZCRTBPTR(,ZCRREG)                               0603 00427000
         ALR   @14,@10                                             0603 00428000
         NI    FSBFBMI(@14),B'11111011'                            0603 00429000
*/*                                                                  */ 00430000
*                                                                  0604 00431000
*   RFY                                                            0604 00432000
*     ISTZIBUF BASED(BFRREG);       /* TEXT FSB                      */ 00433000
*   FSBFSBA=ADDR(ZCRTBPTR->ZIBFSB); /* CHAIN HEADER FSB FROM TEXT FSB*/ 00434000
         LA    @14,ZIBFSB(,@10)                                    0605 00435000
         ST    @14,FSBFSBA(,@11)                                   0605 00436000
*   FSBFSBA=FSBFSBA|GATEWRD;        /* TURN ON GATE BIT              */ 00437000
         O     @14,@CF00850                                        0606 00438000
         ST    @14,FSBFSBA(,@11)                                   0606 00439000
*   ZCRFSBSV=ZCRTBPTR;              /* SAVE HEADER BUFFER ADDRESS    */ 00440000
         ST    @10,ZCRFSBSV(,ZCRREG)                               0607 00441000
*   RFY                                                            0608 00442000
*     LNKREG RSTD;                                                 0608 00443000
*   LNKREG=ZCRSVF8;                 /* RESTORE RETURN REG            */ 00444000
         L     LNKREG,ZCRSVF8(,ZCRREG)                             0609 00445000
*   RFY                                                            0610 00446000
*     LNKREG UNRSTD;                                               0610 00447000
*   END IFCB000B;                                                  0611 00448000
@EL00003 DS    0H                                                  0611 00449000
@EF00003 DS    0H                                                  0611 00450000
@ER00003 BR    @14                                                 0611 00451000
*                                                                  0612 00452000
*/*    SUBROUTINE IFCB000C IS CALLED BY THE MAINLINE WHENEVER THE    */ 00453000
*/* STX CHARACTER IS FOUND IN EITHER THE FIRST OR LAST POSITION IN   */ 00454000
*/* THE BUFFER. THE DATA MOVE ROUTINE IS CALLED TO MOVE THE FSB,BTH  */ 00455000
*/* AND BDU.                                                         */ 00456000
*/*    THE BUFFER DATA ADDRESSES AND COUNTS ARE ADJUSTED AND THE NEW */ 00457000
*/* FSB IS CHAINED TO THE FIRST TEXT BUFFER. THE HEADER FSB IS       */ 00458000
*/* CHAINED FROM THE TEXT FSB.                                       */ 00459000
*/*                                                                  */ 00460000
*                                                                  0612 00461000
*IFCB000C:                                                         0612 00462000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0612 00463000
IFCB000C DS    0H                                                  0613 00464000
*   RFY                                                            0613 00465000
*     LNKREG RSTD;                                                 0613 00466000
*   ZCRSVF8=LNKREG;                 /* SAVE RETURN REGISTER          */ 00467000
         ST    LNKREG,ZCRSVF8(,ZCRREG)                             0614 00468000
*   RFY                                                            0615 00469000
*     LNKREG UNRSTD;                                               0615 00470000
