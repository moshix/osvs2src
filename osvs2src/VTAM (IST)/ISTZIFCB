         LR    @10,BFRREG                                          0621 00494000
         ALR   @10,@14                                             0621 00495000
         NI    FSBFBMI(@10),B'11111011'                            0621 00496000
*/*                                                                  */ 00497000
*                                                                  0622 00498000
*   RFY                                                            0622 00499000
*     ISTZIBUF BASED(ZCRTBPTR);     /* TEXT FSB                      */ 00500000
*   FSBSOHI=OFF;                    /* SET OFF SOH FLAG IN TXT FSB   */ 00501000
         ALR   @11,@14                                             0623 00502000
         NI    FSBSOHI(@11),B'11111110'                            0623 00503000
*   IF WRKREGB=ZCRFSBSV->FDBDATAA THEN/* STX IN FIRST POSITION IN  0624 00504000
*                                      BUFFER                        */ 00505000
         L     @14,ZCRFSBSV(,ZCRREG)                               0624 00506000
         C     WRKREGB,FDBDATAA(,@14)                              0624 00507000
         BNE   @RF00624                                            0624 00508000
*     DO;                                                          0625 00509000
*       WRKREGB=BFRREG;             /* ADDRESS FIRST BUFFER          */ 00510000
