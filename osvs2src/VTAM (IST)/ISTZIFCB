*   END;                                                           0559 00261000
*                                                                  0559 00262000
*/* END TPZRETN                                                      */ 00263000
*                                                                  0560 00264000
*                                                                  0560 00265000
*/*    SUBROUTINE IFCB000A IS CALLED BY THE MAINLINE TO SET UP       */ 00266000
*/* PARMS FOR THE DATA MOVE ROUTINE. THE 'FROM'AND 'TO'BUFFER        */ 00267000
*/* ADDRESSES ARE INITIALIZED, THE FDBDATAA FIELD IS SET TO POINT    */ 00268000
*/* TO THE FSB AND THE DATA DISPLACEMENT SET TO ZERO.                */ 00269000
*/* A FIXED 'TO' BUFFER IS REQUESTED.                                */ 00270000
*/*                                                                  */ 00271000
*                                                                  0560 00272000
*IFCB000A:                                                         0560 00273000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0560 00274000
IFCB000A DS    0H                                                  0561 00275000
*   RFY                                                            0561 00276000
*     LNKREG RSTD;                                                 0561 00277000
*   ZCRSVF8=LNKREG;                 /* SAVE RETURN REG               */ 00278000
         ST    LNKREG,ZCRSVF8(,ZCRREG)                             0562 00279000
*   RFY                                                            0563 00280000
*     LNKREG UNRSTD;                                               0563 00281000
*   WRKREGB=RCDREG;                 /* SAVE PTR TO STX               */ 00282000
         LR    WRKREGB,RCDREG                                      0564 00283000
*   ZCRFSBSV=ZCRCBPTR;              /* SAVE PTR TO BUFFER WITH STX   */ 00284000
         L     @14,ZCRCBPTR(,ZCRREG)                               0565 00285000
         ST    @14,ZCRFSBSV(,ZCRREG)                               0565 00286000
*   ZCRRCDSV=ZCRDLFSB;              /* SET DUAL FSB CODE             */ 00287000
         MVI   ZCRRCDSV(ZCRREG),X'1C'                              0566 00288000
*   ZCRTBPTR=0;                     /* SET 'TO' BUFFER PTR TO 0      */ 00289000
         SLR   @14,@14                                             0567 00290000
         ST    @14,ZCRTBPTR(,ZCRREG)                               0567 00291000
*   ZCRFBPTR=BFRREG;                /* FROM BUFFER IS INBOUND BUFFER */ 00292000
         ST    BFRREG,ZCRFBPTR(,ZCRREG)                            0568 00293000
*   FDBDATAA=ADDR(ZIBFSB);          /* START MOVE WITH FSB           */ 00294000
         LA    @11,ZIBFSB(,BFRREG)                                 0569 00295000
         ST    @11,FDBDATAA(,BFRREG)                               0569 00296000
*   ZCRDFRDA=0;                     /* ZERO DISPLACEMENT FROM DATA 0570 00297000
*                                      ADDR                          */ 00298000
         STH   @14,ZCRDFRDA(,ZCRREG)                               0570 00299000
*   ZCRPCOD2=0;                     /* ZERO FLAGS                    */ 00300000
         MVI   ZCRPCOD2(ZCRREG),X'00'                              0571 00301000
*   ZCRPFHDR=ON;                    /* INDICATE FDB EXISTS           */ 00302000
*   ZCRPMFGF=ON;                    /* SET MOVE FIRST BUFF FLAG      */ 00303000
*   ZCRPPG2=OFF;                    /* FIXED BUFFER REQUESTED        */ 00304000
         OI    ZCRPFHDR(ZCRREG),B'01100000'                        0574 00305000
         NI    ZCRPPG2(ZCRREG),B'01111111'                         0574 00306000
*   RFY                                                            0575 00307000
*     LNKREG RSTD;                                                 0575 00308000
*   LNKREG=ZCRSVF8;                 /* RESTORE RETURN REG            */ 00309000
         L     @11,ZCRSVF8(,ZCRREG)                                0576 00310000
         LR    LNKREG,@11                                          0576 00311000
*   RFY                                                            0577 00312000
*     LNKREG UNRSTD;                                               0577 00313000
*   END IFCB000A;                                                  0578 00314000
@EL00002 DS    0H                                                  0578 00315000
@EF00002 DS    0H                                                  0578 00316000
@ER00002 BR    @14                                                 0578 00317000
*                                                                  0579 00318000
*/*    SUBROUTINE IFCB000B IS CALLED BY THE MAINLINE WHENEVER        */ 00319000
*/* THE STX CHARACTER IS FOUND IN THE FIRST BUFFER.                  */ 00320000
*/* THE LENGTH FOR THE DATA MOVE ROUTINE IS SET AND DATA MOVE IS     */ 00321000
*/* CALLED TO MOVE THE FSB AND THE HEADER DATA. THE 'TO' BUFFER      */ 00322000
*/* DATA ADDRESS AND COUNT IS ADJUSTED TO CORRESPOND TO THE HEADER   */ 00323000
*/* DATA.                                                            */ 00324000
*/*    THE 'FROM' BUFFER DATA ADDRESS AND COUNT ARE ALSO ADJUSTED    */ 00325000
*/* TO EXCLUDE THE STX. THE HEADER FSB IS CHAINED FROM THE TEXT FSB  */ 00326000
*/* AND THE SUBROUTINE RETURNS TO THE MAINLINE.                      */ 00327000
*/*                                                                  */ 00328000
*                                                                  0579 00329000
*IFCB000B:                                                         0579 00330000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0579 00331000
IFCB000B DS    0H                                                  0580 00332000
*   RFY                                                            0580 00333000
*     LNKREG RSTD;                                                 0580 00334000
*   ZCRSVF8=LNKREG;                 /* SAVE RETURN REGISTER          */ 00335000
         ST    LNKREG,ZCRSVF8(,ZCRREG)                             0581 00336000
*   RFY                                                            0582 00337000
*     LNKREG UNRSTD;                                               0582 00338000
*   ZCRMLNG=WRKREGB-ADDR(ZIBFSB);   /* SET LENGTH FOR MOVE AND     0583 00339000
*                                      EXCLUDE STX CHAR              */ 00340000
         LA    @14,ZIBFSB(,BFRREG)                                 0583 00341000
         LCR   @14,@14                                             0583 00342000
         ALR   @14,WRKREGB                                         0583 00343000
         STH   @14,ZCRMLNG(,ZCRREG)                                0583 00344000
*   CALL ISTZDFD0;                  /* CALL DATA MOVE TO MOVE FSB AND   00345000
*                                      HEADER DATA                   */ 00346000
         L     @14,ATCZPSVT(,VTCVTREG)                             0584 00347000
         L     @15,ZPSZDFD0(,@14)                                  0584 00348000
         BALR  @14,@15                                             0584 00349000
*   RFY                                                            0585 00350000
*     ISTZIBUF BASED(ZCRTBPTR);     /* NEW HEADER BUFFER             */ 00351000
*   FDBDATAA=ADDR(BTUTEXT);         /* ADJUST HDR BUFFER DATA ADDR TO   00352000
*                                      HDR START                     */ 00353000
         L     @14,ZCRTBPTR(,ZCRREG)                               0586 00354000
         LA    @11,BTUTEXT+48(,@14)                                0586 00355000
         ST    @11,FDBDATAA(,@14)                                  0586 00356000
*   FDBDATCT=FDBDATCT-(LENGTH(ZIBFSB)+LENGTH(BTUBTH)+LENGTH(BTUBDU));   00357000
         LH    @11,FDBDATCT(,@14)                                  0587 00358000
         SL    @11,@CF04629                                        0587 00359000
         STH   @11,FDBDATCT(,@14)                                  0587 00360000
*                                   /* ADJUST HDR DATA COUNT         */ 00361000
*   IF FSBXFSTS=NCPXTTXT THEN       /* TRANSPARENT TEXT              */ 00362000
         TM    FSBXFSTS+16(@14),B'01000000'                        0588 00363000
         BNO   @RF00588                                            0588 00364000
         TM    FSBXFSTS+16(@14),B'10100000'                        0588 00365000
         BNZ   @RF00588                                            0588 00366000
*     FDBDATCT=FDBDATCT-1;          /* ADJUST HDR DATA COUNT TO    0589 00367000
*                                      EXCLUDE DLE                   */ 00368000
         BCTR  @11,0                                               0589 00369000
         STH   @11,FDBDATCT(,@14)                                  0589 00370000
*   ELSE                                                           0590 00371000
*     ;                                                            0590 00372000
*                                                                  0590 00373000
@RF00588 DS    0H                                                  0591 00374000
*/*                                                                  */ 00375000
*                                                                  0591 00376000
*   RFY                                                            0591 00377000
*     ISTZIBUF BASED(BFRREG);       /* FROM BUFFER                   */ 00378000
*   IF WRKREGB=(BFRREG+ZIBLNG2)-1 THEN/* STX IN LAST POSITION OF FROM   00379000
*                                      BUFFER                        */ 00380000
         LR    @14,BFRREG                                          0592 00381000
         LH    @11,ZIBLNG2(,BFRREG)                                0592 00382000
         N     @11,@CF04641                                        0592 00383000
         ALR   @14,@11                                             0592 00384000
         BCTR  @14,0                                               0592 00385000
         CR    WRKREGB,@14                                         0592 00386000
         BNE   @RF00592                                            0592 00387000
*     DO;                                                          0593 00388000
*       FDBDATAA=0;                 /* SET DATA ADDR IN 'FROM' BFR TO   00389000
*                                      ZERO                          */ 00390000
         SLR   @14,@14                                             0594 00391000
         ST    @14,FDBDATAA(,BFRREG)                               0594 00392000
*       FDBDATCT=0;                 /* SET DATA COUNT IN 'FROM' BFR     00393000
*                                      TO ZERO                       */ 00394000
         STH   @14,FDBDATCT(,BFRREG)                               0595 00395000
*     END;                                                         0596 00396000
*   ELSE                                                           0597 00397000
*     DO;                                                          0597 00398000
         B     @RC00592                                            0597 00399000
@RF00592 DS    0H                                                  0598 00400000
*       FDBDATAA=FDBDATAA+ZCRDFRDA+1;/* ADJUST 'FROM' DATA ADDR TO 0598 00401000
*                                      EXCLUDE STX                   */ 00402000
         LH    @14,ZCRDFRDA(,ZCRREG)                               0598 00403000
         L     @11,FDBDATAA(,BFRREG)                               0598 00404000
         ALR   @11,@14                                             0598 00405000
         AL    @11,@CF00092                                        0598 00406000
         ST    @11,FDBDATAA(,BFRREG)                               0598 00407000
*       FDBDATCT=FDBDATCT-((ZCRDFRDA+1)-(LENGTH(ZIBFSB)+LENGTH(BTUBTH)+ 00408000
*           LENGTH(BTUBDU)));       /* ADJUST FROM BUFFER DATA COUNT    00409000
*                                      TO EXCLUDE STX CHAR           */ 00410000
         SL    @14,@CF04630                                        0599 00411000
         LCR   @14,@14                                             0599 00412000
         AH    @14,FDBDATCT(,BFRREG)                               0599 00413000
         STH   @14,FDBDATCT(,BFRREG)                               0599 00414000
*     END;                                                         0600 00415000
*   FSBSOHI=OFF;                    /* SET OFF SOH FLAG IN TEXT FSB  */ 00416000
@RC00592 LA    @14,16                                              0601 00417000
         LR    @11,BFRREG                                          0601 00418000
