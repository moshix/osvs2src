         BCTR  @14,0                                               0678 00474000
         STC   @14,FMCZX001(,FMCBREG)                              0678 00475000
*         IF FMCZX001=ZERO THEN     /* IF NONE IN SESSION@XL03HEN    */ 00476000
         CLI   FMCZX001(FMCBREG),0                                 0679 00477000
         BNE   @RF00679                                            0679 00478000
*           DO;                                                    0680 00479000
*             FMCZFD06=OFF;         /* FLAG NO DISCONNECT OUT        */ 00480000
         NI    FMCZFD06(FMCBREG),B'01111111'                       0681 00481000
*             DNCDISC=OFF;          /*                       @XL03HEN*/ 00482000
         NI    DNCDISC(DNCBREG),B'11110111'                        0682 00483000
*           END;                                                   0683 00484000
*         ELSE                                                     0684 00485000
*           ;                       /* SESS COUNT NOT 0      @XL03HEK*/ 00486000
@RF00679 DS    0H                                                  0685 00487000
*       END;                                                       0685 00488000
*     ELSE                                                         0686 00489000
*       ;                           /* DISCONN NOT OUTSTANDING       */ 00490000
@RF00676 DS    0H                                                  0687 00491000
*   ELSE                            /* ERROR NOT FOUND       @XL03HEK*/ 00492000
*FCIDO106:                                                         0687 00493000
*     DO;                                                          0687 00494000
         B     @RC00675                                            0687 00495000
@RF00675 DS    0H                                                  0687 00496000
FCIDO106 DS    0H                                                  0688 00497000
*       IF FMCZFD06=ON THEN         /* DISCONN OUTSTAND?             */ 00498000
         TM    FMCZFD06(FMCBREG),B'10000000'                       0688 00499000
         BNO   @RF00688                                            0688 00500000
*FCIDO110:                                                         0689 00501000
*         DO;                       /* YES                           */ 00502000
FCIDO110 DS    0H                                                  0690 00503000
*           FMCZX001=FMCZX001-1;    /* DECREMENT SESS CT             */ 00504000
         SLR   @14,@14                                             0690 00505000
         IC    @14,FMCZX001(,FMCBREG)                              0690 00506000
         BCTR  @14,0                                               0690 00507000
         STC   @14,FMCZX001(,FMCBREG)                              0690 00508000
*           IF FMCZX001=ZERO THEN   /* CHECK SESS CT                 */ 00509000
         CLI   FMCZX001(FMCBREG),0                                 0691 00510000
         BNE   @RF00691                                            0691 00511000
*FCIDO120:                                                         0692 00512000
*             DO;                   /* IT IS ZERO                    */ 00513000
FCIDO120 DS    0H                                                  0693 00514000
*               FMCZFD06=OFF;       /* TURN OFF DISCONN              */ 00515000
         NI    FMCZFD06(FMCBREG),B'01111111'                       0693 00516000
*               DNCDISC=OFF;        /* OUTSTANDING FLAGS             */ 00517000
         NI    DNCDISC(DNCBREG),B'11110111'                        0694 00518000
*               CALL ISTZCF2B;      /* DIALOG SCHED-2                */ 00519000
         L     @14,PABDVTA+111(,FMCBREG)                           0695 00520000
         LA    @14,0(,@14)                                         0695 00521000
         L     @15,ZRCZCFCI(,@14)                                  0695 00522000
         BALR  @14,@15                                             0695 00523000
*             END FCIDO120;                                        0696 00524000
*           ELSE                                                   0697 00525000
*             ;                     /* SESS CT NOT ZERO              */ 00526000
@RF00691 DS    0H                                                  0698 00527000
*         END FCIDO110;             /*                       @XL03HCH*/ 00528000
*       ELSE                                                       0699 00529000
*         ;                         /* NO DISCONN OUTSTAN@XL03HCH    */ 00530000
@RF00688 DS    0H                                                  0700 00531000
*     END FCIDO106;                 /*                       @XL03HCH*/ 00532000
*                                                                  0700 00533000
*   /*****************************************************************/ 00534000
*   /*                                                               */ 00535000
*   /* SEGMENT(PPHASE3)FINISH                                        */ 00536000
*   /*                                                               */ 00537000
*   /*****************************************************************/ 00538000
*                                                                  0701 00539000
*   DO;                                                            0701 00540000
@RC00675 DS    0H                                                  0702 00541000
*     GOTO PPHASE3#;                                               0702 00542000
         B     PPHASE3#                                            0702 00543000
*PPHASE3@:                                                         0703 00544000
*   END;                                                           0703 00545000
PPHASE3@ DS    0H                                                  0704 00546000
*   GEN NODEFS NOREFS NOSETS;                                      0704 00547000
*                                                                  0704 00548000
 TITLE 'ISTZIFCI - INPUT COMPLETION FOR GENERAL POLL DEVICE - TRMPROC'  00549000
*/********************************************************************/ 00550000
*/*                                                                  */ 00551000
*/* TRMPROC: THIS ROUTINE BUILDS A TOLTEP INTERFACE ELEMENT LCPB     */ 00552000
*/*          (TIE) AND ENQUEUES IT ON THE TOLTEP PAB.                */ 00553000
*/*                                                                  */ 00554000
*/********************************************************************/ 00555000
*                                                                  0705 00556000
*   /*****************************************************************/ 00557000
*   /*                                                               */ 00558000
*   /* SEGMENT(TRMPROC)START                                         */ 00559000
*   /*                                                               */ 00560000
*   /*****************************************************************/ 00561000
*                                                                  0705 00562000
*   DO;                                                            0705 00563000
*     GOTO TRMPROC@;                                               0706 00564000
         B     TRMPROC@                                            0706 00565000
*TRMPROC:                                                          0707 00566000
*   END;                                                           0707 00567000
TRMPROC  DS    0H                                                  0708 00568000
*   RFY                                                            0708 00569000
*     WKREGB RSTD;                  /*                       @XM09779*/ 00570000
*/* SETTING OF DEVICE OFFLINE FLAG MOVED BELOW               @XM09779*/ 00571000
*                                                                  0709 00572000
*   FMCZFA06=ON;                    /* TURN ON IN SESSION    @ZA29496   00573000
*                                      FLAG                  @ZA29496*/ 00574000
         OI    FMCZFA06(FMCBREG),B'00000100'                       0709 00575000
*   FMCZH001=FMCZHF71;              /* SET LINE STATE TO     @ZA29496   00576000
*                                      PHYSICAL ERROR        @ZA29496*/ 00577000
         MVC   FMCZH001(2,FMCBREG),@CH02841                        0710 00578000
*   RFY                                                            0711 00579000
*     ISTPAB BASED(ADDR(FMCPAB3));  /* SET THE PAB           @ZA29496*/ 00580000
*   PABERLCK=ON;                    /* ERROR LOCK ON         @ZA29496*/ 00581000
         OI    PABERLCK+96(FMCBREG),B'00000100'                    0712 00582000
*   RFY                                                            0713 00583000
*     ISTPAB;                       /*                       @ZA29496*/ 00584000
*   IF FMCZF00D=ON THEN             /* TRM FLAG ON?          @XC36514*/ 00585000
         TM    FMCZF00D(FMCBREG),B'00000100'                       0714 00586000
         BNO   @RF00714                                            0714 00587000
*     IF LSCBPTR=EMPTY|             /* IF UNSOLIT DATA OR RESPNS     */ 00588000
*         DNCBREG->NCBTYPE=HCTYPE THEN/* RECEIVD FROM CLUSTER THEN 0715 00589000
*                                                            @XL03HCH*/ 00590000
         LTR   LSCBPTR,LSCBPTR                                     0715 00591000
         BZ    @RT00715                                            0715 00592000
         CLI   NCBTYPE(DNCBREG),X'14'                              0715 00593000
         BNE   @RF00715                                            0715 00594000
@RT00715 DS    0H                                                  0716 00595000
*FCIDO130:                                                         0716 00596000
*       DO;                         /* YES                           */ 00597000
FCIDO130 DS    0H                                                  0717 00598000
*         ZCRPCOD1=0;               /* SET FLAGS FOR                 */ 00599000
         L     @14,RPHCRR(,RPHREG)                                 0717 00600000
         L     @14,CRACRR(,@14)                                    0717 00601000
         MVI   ZCRPCOD1(@14),X'00'                                 0717 00602000
*         ZCRPBUF=ON;               /* BUFFER DEALLOCATION           */ 00603000
         OI    ZCRPBUF(@14),B'00000100'                            0718 00604000
*         LSCBPTR=ZIBPTR;           /* ADDR OF BUFF TO DEALL         */ 00605000
         LR    LSCBPTR,ZIBPTR                                      0719 00606000
*         CALL ZPSZDFB0;            /* BUFFER DEALLOCATION           */ 00607000
         L     @08,ATCZPSVT(,VTCVTREG)                             0720 00608000
         L     @15,ZPSZDFB0(,@08)                                  0720 00609000
         BALR  @14,@15                                             0720 00610000
*         LSCBPTR=EMPTY;            /* RESET CORRUPTED REG           */ 00611000
         SLR   LSCBPTR,LSCBPTR                                     0721 00612000
*         RCDREG=ZCRNRSCH;          /* SET RC: EXIT AND SCHEDULE NORM*/ 00613000
         LA    RCDREG,36                                           0722 00614000
*       END FCIDO130;                                              0723 00615000
*     ELSE                          /* NOT UNSOLICITED DATA          */ 00616000
*FCIDO140:                                                         0724 00617000
*       DO;                                                        0724 00618000
         B     @RC00715                                            0724 00619000
@RF00715 DS    0H                                                  0724 00620000
FCIDO140 DS    0H                                                  0725 00621000
*         FSBMJRCD=FRCMDRST;        /* SET FSB MAJOR RC: COMMAND   0725 00622000
*                                      RESET                         */ 00623000
         LA    @14,16                                              0725 00624000
         ALR   @14,ZIBPTR                                          0725 00625000
         MVI   FSBMJRCD(@14),X'18'                                 0725 00626000
*         FSBMNRCD=FRMRUTRM;        /* SET FSB MINOR RC: UNSUCCESSFUL   00627000
*                                      TRM                           */ 00628000
         MVI   FSBMNRCD(@14),X'81'                                 0726 00629000
