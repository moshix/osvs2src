*/*  ASSIGN ZERO TASKID TO THE TIE SO THAT TIE IS NOT PREMATURELY  0789 00812000
*    FREED IN THE EVENT OF APPLICATION ABEND.                        */ 00813000
*                                                                  0789 00814000
*       RFY                                                        0789 00815000
*        (DAFREG,                                                  0789 00816000
*         R10) RSTD;                /*                       @X50BHDP*/ 00817000
*       R10=TEMPTIE;                /* HOLD TIE ADDR         @X50BHDP*/ 00818000
         LR    R10,@14                                             0790 00819000
*       STM(VTCVTREG,R10,RPHWORK);  /* SAVE REGS             @X50BHDP*/ 00820000
         STM   VTCVTREG,R10,RPHWORK(RPHREG)                        0791 00821000
*       DAFREG=0;                   /* ASSIGN TSKID 0        @X50BHDP*/ 00822000
         SLR   DAFREG,DAFREG                                       0792 00823000
*       RCDREG=VTCVTREG;            /* HOLD CVT ADDR         @X50BHDP*/ 00824000
         LR    RCDREG,VTCVTREG                                     0793 00825000
*       VTCVTREG=R10;               /* ADDRESS OF TIE        @X50BHDP*/ 00826000
         LR    VTCVTREG,R10                                        0794 00827000
*       CALL RCDREG->ATCSMAID->ISTORAID;/*                   @X50BHDP*/ 00828000
         L     @14,ATCSMAID(,RCDREG)                               0795 00829000
         LR    @15,@14                                             0795 00830000
         BALR  @14,@15                                             0795 00831000
*       LM(VTCVTREG,R10,RPHWORK);   /* RESTORE REGS          @X50BHDP*/ 00832000
         LM    VTCVTREG,R10,RPHWORK(RPHREG)                        0796 00833000
*       TEMPTIE=R10;                /* RESTORE TIE ADDR      @X50BHDP*/ 00834000
         ST    R10,TEMPTIE(,RPHREG)                                0797 00835000
*       RFY                                                        0798 00836000
*        (DAFREG,                                                  0798 00837000
*         R10) UNRSTD;              /*                       @X50BHDP*/ 00838000
*       IF FMCDEBA->ACDRDTE->RPRNAME='ISTOLTEP' THEN/* IF APPLICATION   00839000
*                                      IS TOLTEP             @XM09779*/ 00840000
         L     @14,FMCDEBA(,FMCBREG)                               0799 00841000
         L     @14,ACDRDTE(,@14)                                   0799 00842000
         CLC   RPRNAME(8,@14),@CC04759                             0799 00843000
         BNE   @RF00799                                            0799 00844000
*         WKREGB=ADDR(ATCTCIPB);    /* SET ADDRESS OF TOLTEP       0800 00845000
*                                      PROCESSOR PAB         @XM09779*/ 00846000
         LA    WKREGB,ATCTCIPB(,VTCVTREG)                          0800 00847000
*       ELSE                                                       0801 00848000
*         DO;                       /* IF NOT TOLTEP         @XM09779*/ 00849000
         B     @RC00799                                            0801 00850000
@RF00799 DS    0H                                                  0802 00851000
*           FMCDEVOF=ON;            /* SET DEVICE OFFLINE    @XM09779*/ 00852000
         OI    FMCDEVOF(FMCBREG),B'00010000'                       0802 00853000
*           FMCZF00D=ON;            /* TURN ON TRM RECEIVED FLAG   0803 00854000
*                                                            @XM09779*/ 00855000
         OI    FMCZF00D(FMCBREG),B'00000100'                       0803 00856000
*           WKREGB=ADDR(ATCVTIPB);  /* SET ADDRESS OF TOLTEP INCMS 0804 00857000
*                                      PAB                   @XM09779*/ 00858000
         LA    WKREGB,ATCVTIPB(,VTCVTREG)                          0804 00859000
*         END;                      /* END NOT TOLTEP        @XM09779*/ 00860000
*/*  TPQUE (TEMPTIE,WKREGB->DYPPAB) NORM                             */ 00861000
*                                                                  0806 00862000
*       DO;                                                        0806 00863000
@RC00799 DS    0H                                                  0807 00864000
*         RFY                                                      0807 00865000
*          (GPR00P,                                                0807 00866000
*           GPR01P,                                                0807 00867000
*           GPR04P,                                                0807 00868000
*           GPR05P,                                                0807 00869000
*           GPR15P) RSTD;                                          0807 00870000
*         GEN REFS(GPR01P) SETS(RPHWORK)                           0808 00871000
*             (STM GPR02P,GPR08P,RPHWORK(GPR01P));                 0808 00872000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00873000
*         GPR00P=0;                                                0809 00874000
         SLR   GPR00P,GPR00P                                       0809 00875000
*         GPR15P=ADDR(WKREGB->DYPPAB);/* HOLD PAB PTR                */ 00876000
         LA    GPR15P,DYPPAB(,WKREGB)                              0810 00877000
*         GPR04P=TEMPTIE;           /* WORK ELE PTR                  */ 00878000
         L     GPR04P,TEMPTIE(,RPHREG)                             0811 00879000
*         GPR05P=GPR15P;            /* PASS PAB PTR                  */ 00880000
         LR    GPR05P,GPR15P                                       0812 00881000
*         CALL ATCLCPTR->ATCAP37->ISTAPC37;                        0813 00882000
         L     @08,ATCLCPTR                                        0813 00883000
         L     @15,ATCAP37(,@08)                                   0813 00884000
         BALR  @14,@15                                             0813 00885000
*         GEN(NOPR  0)                                             0814 00886000
*         NOSETS NOREFS;                                           0814 00887000
         NOPR  0                                                        00888000
*         RFY                                                      0815 00889000
*          (GPR00P,                                                0815 00890000
*           GPR01P,                                                0815 00891000
*           GPR04P,                                                0815 00892000
*           GPR05P,                                                0815 00893000
*           GPR15P) UNRSTD;                                        0815 00894000
*       END;                        /* *** END OF TPQUE GENERATION 0816 00895000
*                                      QUEUE TIE TO TOLTEP   @XM09779*/ 00896000
*       IF LSCBPTR=EMPTY|           /* IF DATA UNSOLIT OR            */ 00897000
*           DNCBREG->NCBTYPE=HCTYPE|/* FROM CLUSTER OR               */ 00898000
*           FMCMODE=FMCRCD THEN     /* RECORD MODE?          @XL03HEK*/ 00899000
         LTR   LSCBPTR,LSCBPTR                                     0817 00900000
         BZ    @RT00817                                            0817 00901000
         CLI   NCBTYPE(DNCBREG),X'14'                              0817 00902000
         BE    @RT00817                                            0817 00903000
         CLC   FMCMODE(8,FMCBREG),@CC02851                         0817 00904000
         BNE   @RF00817                                            0817 00905000
@RT00817 DS    0H                                                  0818 00906000
*FCIDO160:                                                         0818 00907000
*         DO;                       /* YES                           */ 00908000
FCIDO160 DS    0H                                                  0819 00909000
*           WKREGB=LSCBPTR;         /* SAVE @ LSCB           @XL03HCH*/ 00910000
         LR    WKREGB,LSCBPTR                                      0819 00911000
*           ZCRPCOD1=0;             /* SET UP FLAGS TO DEALLOCATE    */ 00912000
         L     @14,RPHCRR(,RPHREG)                                 0820 00913000
         L     @14,CRACRR(,@14)                                    0820 00914000
         MVI   ZCRPCOD1(@14),X'00'                                 0820 00915000
*           ZCRPBUF=ON;             /* THE BUFFER                    */ 00916000
         OI    ZCRPBUF(@14),B'00000100'                            0821 00917000
*           LSCBPTR=ZIBPTR;         /* ADDR OF BUFF TO DEALLOCATE    */ 00918000
         LR    LSCBPTR,ZIBPTR                                      0822 00919000
*           CALL ZPSZDFB0;          /* BUFFER DEALLOCATION           */ 00920000
         L     @08,ATCZPSVT(,VTCVTREG)                             0823 00921000
         L     @15,ZPSZDFB0(,@08)                                  0823 00922000
         BALR  @14,@15                                             0823 00923000
*           LSCBPTR=WKREGB;         /* RESTOR @LSCB          @XL03HCH*/ 00924000
         LR    LSCBPTR,WKREGB                                      0824 00925000
*           RCDREG=ZCRNRSCH;        /* SCHED NORM            @XM09824*/ 00926000
         LA    RCDREG,36                                           0825 00927000
*         END FCIDO160;                                            0826 00928000
*       ELSE                        /* NOT UNSOLICITED DATA FSB      */ 00929000
*FCIDO170:                                                         0827 00930000
*         DO;                                                      0827 00931000
         B     @RC00817                                            0827 00932000
@RF00817 DS    0H                                                  0827 00933000
FCIDO170 DS    0H                                                  0828 00934000
*           FSBMJRCD=FRCMDRST;      /* SET FSB MAJOR RC: COMMAND   0828 00935000
*                                      RESET                         */ 00936000
         LA    @14,16                                              0828 00937000
         ALR   @14,ZIBPTR                                          0828 00938000
         MVI   FSBMJRCD(@14),X'18'                                 0828 00939000
*           FSBMNRCD=FRMRUTRM;      /* SET FSB MINOR RC: TRM RECVD 0829 00940000
*                                                            @XL03HEK*/ 00941000
         MVI   FSBMNRCD(@14),X'81'                                 0829 00942000
*           RCDREG=ZERO;            /* SET FEEDBACK RC TO HANDLE WITH   00943000
*                                      SCHED NORM            @XM09824*/ 00944000
         SLR   RCDREG,RCDREG                                       0830 00945000
*         END FCIDO170;             /*                       @XL03HCH*/ 00946000
*     END FCIDO150;                                                0832 00947000
@RC00817 DS    0H                                                  0833 00948000
*   RFY                                                            0833 00949000
*     WKREGB UNRSTD;                /*                       @XM09779*/ 00950000
*                                                                  0833 00951000
@RC00714 DS    0H                                                  0834 00952000
*   /*****************************************************************/ 00953000
*   /*                                                               */ 00954000
*   /* SEGMENT(TRMPROC)FINISH                                        */ 00955000
*   /*                                                               */ 00956000
*   /*****************************************************************/ 00957000
*                                                                  0834 00958000
*   DO;                                                            0834 00959000
*     GOTO TRMPROC#;                                               0835 00960000
         B     TRMPROC#                                            0835 00961000
*TRMPROC@:                                                         0836 00962000
*   END;                                                           0836 00963000
TRMPROC@ DS    0H                                                  0837 00964000
*   GEN NODEFS NOREFS NOSETS;                                      0837 00965000
*                                                                  0837 00966000
 TITLE 'ISTZIFCI - INPUT COMPLETION FOR GENERAL POLL DEVICE - FCIDEND'  00967000
*/********************************************************************/ 00968000
*/*                                                                  */ 00969000
*/* FCIDEND - THIS ROUTINE PROCESSES DEVICE END STATUS.      @XL03HEK*/ 00970000
*/*                                                                  */ 00971000
*/********************************************************************/ 00972000
*/* SEGMENT(FCIDEND)START                                            */ 00973000
*                                                                  0838 00974000
*   DO;                                                            0838 00975000
*     GOTO FCIDEND@;                                               0839 00976000
         B     FCIDEND@                                            0839 00977000
*FCIDEND:                                                          0840 00978000
*   END;                                                           0840 00979000
FCIDEND  DS    0H                                                  0841 00980000
*   FDBDATCT=ZERO;                  /* SET ZERO FDB DATA COUNT       */ 00981000
         SLR   @14,@14                                             0841 00982000
         STH   @14,FDBDATCT(,ZIBPTR)                               0841 00983000
*   FSBMJRCD=FRPHYSC;               /* SET MAJ RC FOR DEVICE END ON     00984000
*                                      POWER ON NOTIFICATION         */ 00985000
         LA    @14,16                                              0842 00986000
         ALR   @14,ZIBPTR                                          0842 00987000
         MVI   FSBMJRCD(@14),X'0C'                                 0842 00988000
*   FSBMNRCD=FRMUSDA;               /* (UNSOLIC SENSE DATA AVAIL)    */ 00989000
         MVI   FSBMNRCD(@14),B'00000001'                           0843 00990000
*   FSBDMGRS=BTUTEXT(6:7);          /* MOVE SENSE DATA TO FSB DEV  0844 00991000
*                                      MGMT RESP FIELD               */ 00992000
         MVC   FSBDMGRS(2,@14),BTUTEXT+53(ZIBPTR)                  0844 00993000
*   IF ZCR3270R=ON THEN             /* DEVICE 3270 IN RECORD MODE  0845 00994000
*                                                            @XM09428*/ 00995000
         L     @14,RPHCRR(,RPHREG)                                 0845 00996000
         L     @14,CRACRR(,@14)                                    0845 00997000
         TM    ZCR3270R(@14),B'00000010'                           0845 00998000
         BNO   @RF00845                                            0845 00999000
*     CALL ZPSZDFXR;                /* YES, XLATE SENSE TO RECORD  0846 01000000
*                                      FORMAT                @XM09428*/ 01001000
         L     @14,ATCZPSVT(,VTCVTREG)                             0846 01002000
         L     @15,ZPSZDFXR(,@14)                                  0846 01003000
         BALR  @14,@15                                             0846 01004000
*   ELSE                                                           0847 01005000
*     ;                             /* NO, (NULL)            @XM09428*/ 01006000
@RF00845 DS    0H                                                  0848 01007000
*/* SEGMENT(FCIDEND)FINISH                                           */ 01008000
*                                                                  0848 01009000
*   DO;                                                            0848 01010000
*     GOTO FCIDEND#;                                               0849 01011000
         B     FCIDEND#                                            0849 01012000
*FCIDEND@:                                                         0850 01013000
*   END;                                                           0850 01014000
FCIDEND@ DS    0H                                                  0851 01015000
*                                                                  0851 01016000
*/* TPEPILOG                                                         */ 01017000
*                                                                  0851 01018000
*   GEN(PRINT OFF);                                                0851 01019000
         PRINT OFF                                                      01020000
*   DCL                                                            0852 01021000
*     ISTAPC34 ENTRY BASED(ATCAP34) VALRG(*);/* RESET ROUTINE        */ 01022000
*   DCL                                                            0853 01023000
