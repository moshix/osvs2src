         LH    DATACNT,FDBDATCT(,PTRZIB)                           0827 01186000
         LA    @15,2                                               0827 01187000
         CR    DATACNT,@15                                         0827 01188000
         BNH   *+6                                                      01189000
         LR    DATACNT,@15                                         0827 01190000
*       RFY                                                        0828 01191000
*         R15 RSTD;                 /* RESTRICT R15 FOR OUR USE      */ 01192000
*       R15=PTRFSB+(ADDR(ZIBBTU)-ADDR(ZIBFSB));/* POINT TO BTU       */ 01193000
         LA    R15,ZIBBTU(,PTRZIB)                                 0829 01194000
         LA    @00,ZIBFSB(,PTRZIB)                                 0829 01195000
         SLR   R15,@00                                             0829 01196000
         ALR   R15,PTRFSB                                          0829 01197000
*       FSBDMGRS=R15->BTUTEXT(1:DATACNT);/* MOVE UP TO 2 BYTES OF  0830 01198000
*                                      TEXT TO FSB SENSE AREA        */ 01199000
         MVI   FSBDMGRS+1(PTRFSB),X'00'                            0830 01200000
         LR    @11,DATACNT                                         0830 01201000
         BCTR  @11,0                                               0830 01202000
         EX    @11,@SM04182                                        0830 01203000
*       RFY                                                        0831 01204000
*         R15 UNRSTD;               /* LET CMPLR USE R15 AGAIN       */ 01205000
*       FDBDATAA=0;                 /* ZERO FDB DATA ADDRESS         */ 01206000
         SLR   @15,@15                                             0832 01207000
         ST    @15,FDBDATAA(,PTRZIB)                               0832 01208000
*       FDBDATCT=0;                 /* ZERO FDB DATA COUNT           */ 01209000
         STH   @15,FDBDATCT(,PTRZIB)                               0833 01210000
*     END FDBDO50;                                                 0834 01211000
*   ELSE                                                           0835 01212000
*     ;                                                            0835 01213000
@RF00824 DS    0H                                                  0836 01214000
*   RFY                                                            0836 01215000
*     RTNREG UNRSTD;                /*                       @XL03HEN*/ 01216000
*   END STS2740;                    /*                       @XL03HEN*/ 01217000
@EL00006 DS    0H                                                  0837 01218000
@EF00006 DS    0H                                                  0837 01219000
@ER00006 BR    @14                                                 0837 01220000
@PB00006 DS    0H                                                  0838 01221000
*                                                                  0838 01222000
*   /*****************************************************************/ 01223000
*   /*                                                               */ 01224000
*   /* SEGMENT(DEALBFR)START                                         */ 01225000
*   /*                                                               */ 01226000
*   /*****************************************************************/ 01227000
*                                                                  0838 01228000
*   DO;                                                            0838 01229000
