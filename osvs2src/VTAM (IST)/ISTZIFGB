         SLR   @11,@11                                             0539 00328000
         ST    @11,WKECHAIN(,GPR14P)                               0539 00329000
*               END;                                               0540 00330000
*             ELSE                                                 0541 00331000
*               DO;                                                0541 00332000
         B     @RC00533                                            0541 00333000
@RF00533 DS    0H                                                  0542 00334000
*                 IF(GPR02P->WKECHAIN)=GPR14P THEN                 0542 00335000
         L     @11,WKECHAIN(,GPR02P)                               0542 00336000
         CR    @11,GPR14P                                          0542 00337000
         BE    @RT00542                                            0542 00338000
*                   GOTO ISTA0028;                                 0543 00339000
*                 ELSE                                             0544 00340000
*                   DO;                                            0544 00341000
*                     GPR15P=ADDR(GPR02P->WKECHAIN);               0545 00342000
