*             ISTWKE;                                              0523 00291000
*           GPR15P=ADDR(LCPFDBA);   /* ADDRESS QUEUE POINTER         */ 00292000
         LA    GPR15P,LCPFDBA(,PTRLCP)                             0524 00293000
*           RFY                                                    0525 00294000
