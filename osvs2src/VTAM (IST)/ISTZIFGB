*           LM(GPR02P,GPR03P,GPR01P->RPHWORK(5:12));               0553 00360000
         LM    GPR02P,GPR03P,RPHWORK+4(GPR01P)                     0553 00361000
*           RFY                                                    0554 00362000
*            (GPR02P,                                              0554 00363000
*             GPR03P) UNRSTD;                                      0554 00364000
*           GPR14P=GPR14P&FMZCB028;                                0555 00365000
         N     GPR14P,@CF00792                                     0555 00366000
*           GPR01P->RPHWEA=GPR14P;                                 0556 00367000
         ST    GPR14P,RPHWEA(,GPR01P)                              0556 00368000
*           RFY                                                    0557 00369000
*            (GPR00P,                                              0557 00370000
*             GPR01P,                                              0557 00371000
*             GPR14P,                                              0557 00372000
*             GPR15P) UNRSTD;                                      0557 00373000
*         END;                      /* END TPDEQ GENERATION          */ 00374000
*         ZIBFDBS=ZIBFDBZ;          /* MOVE 1ST FDB TO SYSTEM FDB  0559 00375000
*                                      LOCATION                      */ 00376000
         MVC   ZIBFDBS(12,PTRZIB),ZIBFDBZ(PTRZIB)                  0559 00377000
*         RFY                                                      0560 00378000
*           ISTFDB BASED(ADDR(ZIBFDBS));/* ADDR SYSTEM FDB           */ 00379000
*         FDBOFSET=ADDR(ZIBFDBS)-ADDR(ZIBFDBZ);/* CALCULATE FDB    0561 00380000
*                                      OFFSET                        */ 00381000
         LA    @15,48                                              0561 00382000
         LR    @14,PTRZIB                                          0561 00383000
         ALR   @14,@15                                             0561 00384000
         STH   @15,FDBOFSET(,@14)                                  0561 00385000
*/* QUEUE FSB TO CTL LAYER FEEDBACK PAB                              */ 00386000
*/*  TPQUE (PTRFSB,FMCPAB2) NORM                                     */ 00387000
*                                                                  0562 00388000
*         DO;                                                      0562 00389000
*           RFY                                                    0563 00390000
*            (GPR00P,                                              0563 00391000
*             GPR01P,                                              0563 00392000
*             GPR04P,                                              0563 00393000
*             GPR05P,                                              0563 00394000
*             GPR15P) RSTD;                                        0563 00395000
*           GEN REFS(GPR01P) SETS(RPHWORK)                         0564 00396000
*               (STM GPR02P,GPR08P,RPHWORK(GPR01P));               0564 00397000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              00398000
