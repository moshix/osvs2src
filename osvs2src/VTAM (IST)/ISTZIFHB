*                                                                  0541 00401000
ERRMSG   DS    0H                                                  0542 00402000
*/********************************************************************/ 00403000
*/* ERRMSG: THIS ROUTINE IS ENTERED WHEN A PHASE1,2,OR 3 ERROR       */ 00404000
*/*         OCCURS. IT CALLS DATA MOVE TO GET A BUFFER AND MOVE      */ 00405000
*/*         THE FSB&BTU TO IT. THE NEW BDU IS SET TO ACTIVATE        */ 00406000
*/*         DEVICE AND FSBMSGID SET = CATASTROPHIC I/O ERROR.        */ 00407000
*/*         THE FSB IS THEN QUEUED TO THE MSG WRITER PAB.            */ 00408000
*/*         THE S/D CODE FOR THIS ROUTINE IS                 @XL03HCH*/ 00409000
*/********************************************************************/ 00410000
*                                                                  0542 00411000
*   ZCRTBPTR=0;                     /* SET PARM FOR DATA MOVE RTN  0542 00412000
*                                      ALLOCTN OF 'TO' BUFF          */ 00413000
         SLR   @15,@15                                             0542 00414000
         ST    @15,ZCRTBPTR(,ZCRPTR)                               0542 00415000
*   ZCRPCOD2=0;                     /* ZERO PARM FLAGS               */ 00416000
         MVI   ZCRPCOD2(ZCRPTR),X'00'                              0543 00417000
*   ZCRPFHDR=ON;                    /* PARM= FDB IN 'FROM' BUFF      */ 00418000
*   ZCRPMFGF=ON;                    /* MOVE STARTS IN 1ST BUFF       */ 00419000
         OI    ZCRPFHDR(ZCRPTR),B'01100000'                        0545 00420000
*   ZCRFBPTR=BUFPTR;                /* SET 'FROM' BUFF ADDR FOR DATA    00421000
*                                      MOVE                          */ 00422000
         ST    BUFPTR,ZCRFBPTR(,ZCRPTR)                            0546 00423000
