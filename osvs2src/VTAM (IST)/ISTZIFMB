         LA    @14,ZIBFDBS(,BFRREG)                                0997 02055000
         SLR   @14,BFRREG                                          0997 02056000
         STH   @14,FDBOFSET+48(,BFRREG)                            0997 02057000
*/*  TPQUE (ADDR(ZIBFSB),FMCPAB2) NORM                               */ 02058000
*                                                                  0998 02059000
*   DO;                                                            0998 02060000
*     RFY                                                          0999 02061000
*      (GPR00P,                                                    0999 02062000
*       GPR01P,                                                    0999 02063000
*       GPR04P,                                                    0999 02064000
*       GPR05P,                                                    0999 02065000
*       GPR15P) RSTD;                                              0999 02066000
*     GEN REFS(GPR01P) SETS(RPHWORK)(STM GPR02P,GPR08P,RPHWORK(GPR01P)) 02067000
*         ;                                                        1000 02068000
         STM GPR02P,GPR08P,RPHWORK(GPR01P)                              02069000
*     GPR00P=0;                                                    1001 02070000
         SLR   GPR00P,GPR00P                                       1001 02071000
*     GPR15P=ADDR(FMCPAB2);         /* HOLD PAB PTR                  */ 02072000
         LA    GPR15P,FMCPAB2(,FMCBREG)                            1002 02073000
*     GPR04P=ADDR(ZIBFSB);          /* WORK ELE PTR                  */ 02074000
         LA    GPR04P,ZIBFSB(,BFRREG)                              1003 02075000
*     GPR05P=GPR15P;                /* PASS PAB PTR                  */ 02076000
         LR    GPR05P,GPR15P                                       1004 02077000
*     CALL ATCLCPTR->ATCAP37->ISTAPC37;                            1005 02078000
         L     @14,ATCLCPTR                                        1005 02079000
         L     @15,ATCAP37(,@14)                                   1005 02080000
         BALR  @14,@15                                             1005 02081000
*     GEN(NOPR  0)                                                 1006 02082000
*     NOSETS NOREFS;                                               1006 02083000
         NOPR  0                                                        02084000
*     RFY                                                          1007 02085000
*      (GPR00P,                                                    1007 02086000
*       GPR01P,                                                    1007 02087000
*       GPR04P,                                                    1007 02088000
*       GPR05P,                                                    1007 02089000
*       GPR15P) UNRSTD;                                            1007 02090000
*   END;                            /* *** END OF TPQUE GENERATION 1008 02091000
*                                      QUEUE FSB TO CLIP             */ 02092000
*   CALL HOUSKEEP;                  /* CHECK IF PROCESSING COMPLET   */ 02093000
         BAL   @14,HOUSKEEP                                        1009 02094000
*   RFY                                                            1010 02095000
*     LNKREG RSTD;                                                 1010 02096000
*   LNKREG=ZCRSVF8;                 /* SAVE RETURN REG               */ 02097000
         L     @11,RPHCRR(,RPHPTR)                                 1011 02098000
         L     @11,CRACRR(,@11)                                    1011 02099000
         L     LNKREG,ZCRSVF8(,@11)                                1011 02100000
*   RFY                                                            1012 02101000
*     LNKREG UNRSTD;                                               1012 02102000
*   END RPOST;                                                     1013 02103000
*                                                                  1013 02104000
@EL00005 DS    0H                                                  1013 02105000
@EF00005 DS    0H                                                  1013 02106000
@ER00005 BR    @14                                                 1013 02107000
@PB00005 DS    0H                                                  1014 02108000
