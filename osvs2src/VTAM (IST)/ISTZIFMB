*   ZCRTBPTR=0;                     /* SET NO 'TO' DATA ADDR- GET NEW   01879000
*                                      BUFFER                        */ 01880000
         SLR   @00,@00                                             0939 01881000
         ST    @00,ZCRTBPTR(,@14)                                  0939 01882000
*   ZCRMLNG=N56;                    /* SET LENGTH TO MOVE    @ZA19929   01883000
*                                      OF FSB PLUS FEDBK     @ZA19929*/ 01884000
