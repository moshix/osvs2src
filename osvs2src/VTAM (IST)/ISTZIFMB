         L     @11,CRACRR(,@11)                                    0930 01849000
         L     LNKREG,ZCRSVF7(,@11)                                0930 01850000
*   RFY                                                            0931 01851000
*     LNKREG UNRSTD;                                               0931 01852000
*                                                                  0932 01853000
*/********************************************************************/ 01854000
*/* MAKAFSB:                                                         */ 01855000
*/* THIS ROUTINE CALLS THE DATA MOVE UTILITY TO COPY THE EXISTING    */ 01856000
*/* REQUEST FSB INTO A NEW BUFFER FOR A RESPONSE FSB                 */ 01857000
*/* THIS ROUTINE CODED BY                                    @XL03HCH*/ 01858000
*/********************************************************************/ 01859000
*/* SEGMENT(MAKAFSB)START                                            */ 01860000
*                                                                  0932 01861000
*   DO;                                                            0932 01862000
*     GOTO MAKAFSB@;                                               0933 01863000
         B     MAKAFSB@                                            0933 01864000
*MAKAFSB:                                                          0934 01865000
*   END;                                                           0934 01866000
MAKAFSB  DS    0H                                                  0935 01867000
*   ZCRPCOD1=0;                     /* ZERO PARMS FLAGS FIELD        */ 01868000
