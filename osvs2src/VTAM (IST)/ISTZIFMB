*   END SNARESP;                                                   0967 01964000
@EL00004 DS    0H                                                  0967 01965000
@EF00004 DS    0H                                                  0967 01966000
@ER00004 BR    @14                                                 0967 01967000
*                                                                  0968 01968000
*/********************************************************************/ 01969000
*/* RPOST:                                                           */ 01970000
*/* THIS ROUTINE CALLS THE FID1 REQUEST FSB FORMATTER ROUTINE, THEN  */ 01971000
*/* FURTHER INITS THE FSB AND QUEUES IT TO THE CLIP                  */ 01972000
*/* THIS ROUTINE CODED BY                                    @XL03HCH*/ 01973000
*/********************************************************************/ 01974000
*                                                                  0968 01975000
*RPOST:                                                            0968 01976000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0968 01977000
RPOST    DS    0H                                                  0969 01978000
*   RFY                                                            0969 01979000
*     LNKREG RSTD;                                                 0969 01980000
*   ZCRSVF8=LNKREG;                 /* SAVE RETURN REG               */ 01981000
         L     @11,RPHCRR(,RPHPTR)                                 0970 01982000
         L     @11,CRACRR(,@11)                                    0970 01983000
         ST    LNKREG,ZCRSVF8(,@11)                                0970 01984000
*   RFY                                                            0971 01985000
*     LNKREG UNRSTD;                                               0971 01986000
*   CALL ISTZDFPR;                  /* FID1 REQUEST FSB FORMATTER    */ 01987000
         L     @14,ATCZPSVT(,VTCVTREG)                             0972 01988000
         L     @15,ZPSZDFPR(,@14)                                  0972 01989000
         BALR  @14,@15                                             0972 01990000
*/* TPDEQ (,LSCFDBA)                                                 */ 01991000
*                                                                  0973 01992000
*   DO;                             /* BEGIN TPDEQ CODE GENERATION   */ 01993000
*     RFY                                                          0974 01994000
*      (GPR00P,                                                    0974 01995000
*       GPR01P,                                                    0974 01996000
*       GPR14P,                                                    0974 01997000
*       GPR15P) RSTD;                                              0974 01998000
*     GPR15P=ADDR(LSCFDBA);         /* ADDRESS QUEUE POINTER         */ 01999000
         LA    GPR15P,LSCFDBA(,ELEMREG)                            0975 02000000
*     GPR14P=GPR15P->PABWEQA|FMZCA027;                             0976 02001000
         L     @11,@CF01884                                        0976 02002000
         L     GPR14P,PABWEQA(,GPR15P)                             0976 02003000
