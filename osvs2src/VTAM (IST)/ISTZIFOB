*/*    XITCODE SETS THE RETURN CODE TO BE PASSED BACK TO REQUEST     */ 01165000
*/* COMPLETION. IF THE REQUEST IS COMPLETE, THE RETURN CODE IS SET   */ 01166000
*/* TO 16, IF THERE IS ANOTHER FSB TO PROCESS. A CODE OF 12 IS SET,  */ 01167000
*/* OTHERWISE,THE CODE IS SET TO 20.                                 */ 01168000
*/*                                                                  */ 01169000
*                                                                  0781 01170000
*XITCODE:                                                          0781 01171000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0781 01172000
XITCODE  DS    0H                                                  0782 01173000
*   RFY                                                            0782 01174000
*     LNKREG RSTD;                  /* RESTRICT REGISTER      @X03HDP*/ 01175000
*   ZCRSVF7=LNKREG;                 /* SAVE RETURN REG               */ 01176000
         L     @11,RPHCRR(,RPHPTR)                                 0783 01177000
         L     @11,CRACRR(,@11)                                    0783 01178000
         ST    LNKREG,ZCRSVF7(,@11)                                0783 01179000
*   RFY                                                            0784 01180000
*     LNKREG UNRSTD;                /* RELEASE REGISTER       @X03HDP*/ 01181000
*   RCDREG=ZCRSRCHQ;                /* SET R.C. TO SEARCH FOR WORK   */ 01182000
         LA    RCDREG,16                                           0785 01183000
*   IF(FSBPTR&REQCOMP)^=0 THEN                                     0786 01184000
         LR    @14,FSBPTR                                          0786 01185000
         N     @14,@CF01971                                        0786 01186000
         LTR   @14,@14                                             0786 01187000
         BNZ   @RT00786                                            0786 01188000
*     ;                             /* NCP COMPLETED                 */ 01189000
*   ELSE                                                           0788 01190000
*     DO;                                                          0788 01191000
*       RCDREG=ZCREXIT;             /* SET R.C. TO EXIT REQ COMPL    */ 01192000
         LA    RCDREG,20                                           0789 01193000
*       IF BFRREG=0 THEN                                           0790 01194000
         LTR   BFRREG,BFRREG                                       0790 01195000
         BZ    @RT00790                                            0790 01196000
*         ;                         /* NO MORE FSBS                  */ 01197000
*       ELSE                                                       0792 01198000
*         RCDREG=ZCRMRFSB;          /* MORE FSBS TO PROCESS          */ 01199000
         LA    RCDREG,12                                           0792 01200000
*     END;                                                         0793 01201000
@RT00790 DS    0H                                                  0794 01202000
*   RFY                                                            0794 01203000
*     LNKREG RSTD;                  /* RESTRICT REGISTER      @X03HDP*/ 01204000
@RT00786 DS    0H                                                  0795 01205000
*   LNKREG=ZCRSVF7;                 /* RESTORE RETURN REG            */ 01206000
         L     @11,RPHCRR(,RPHPTR)                                 0795 01207000
         L     @11,CRACRR(,@11)                                    0795 01208000
         L     LNKREG,ZCRSVF7(,@11)                                0795 01209000
*   RFY                                                            0796 01210000
*     LNKREG UNRSTD;                /* RELEASE REGISTER       @X03HDP*/ 01211000
*   END XITCODE;                                                   0797 01212000
@EL00005 DS    0H                                                  0797 01213000
@EF00005 DS    0H                                                  0797 01214000
@ER00005 BR    @14                                                 0797 01215000
*                                                                  0798 01216000
*/********************************************************************/ 01217000
*/*                                                                  */ 01218000
*/* FSBDEQ: THIS ROUTINE REMOVES THE OLDEST FSB FROM THE WAITQ       */ 01219000
*/*         ELEMENT.                                                 */ 01220000
*/*                                                                  */ 01221000
*/*         THE S/D CODE FOR FSBDEQ IS SHOWN AT RIGHT:       @XL03HEK*/ 01222000
*/*                                                                  */ 01223000
*/********************************************************************/ 01224000
*                                                                  0798 01225000
*FSBDEQ:                                                           0798 01226000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0798 01227000
FSBDEQ   DS    0H                                                  0799 01228000
*   RFY                                                            0799 01229000
*     LNKREG RSTD;                                                 0799 01230000
*   ZCRSVF8=LNKREG;                 /* SAVE RETURN ADDRESS           */ 01231000
         L     @11,RPHCRR(,RPHPTR)                                 0800 01232000
         L     @11,CRACRR(,@11)                                    0800 01233000
         ST    LNKREG,ZCRSVF8(,@11)                                0800 01234000
*   RFY                                                            0801 01235000
*     LNKREG UNRSTD;                                               0801 01236000
*                                                                  0801 01237000
*/* REMOVE OLDEST FSB FROM WAITQ ELEMENT (NO RETURN POINTER NEEDED)  */ 01238000
*/* TPDEQ (,LSCFDBA)                                                 */ 01239000
*                                                                  0802 01240000
*   DO;                             /* BEGIN TPDEQ CODE GENERATION   */ 01241000
*     RFY                                                          0803 01242000
*      (GPR00P,                                                    0803 01243000
*       GPR01P,                                                    0803 01244000
*       GPR14P,                                                    0803 01245000
*       GPR15P) RSTD;                                              0803 01246000
*     GPR15P=ADDR(LSCFDBA);         /* ADDRESS QUEUE POINTER         */ 01247000
         LA    GPR15P,LSCFDBA(,ELEMREG)                            0804 01248000
*     GPR14P=GPR15P->PABWEQA|FMZCA020;                             0805 01249000
         L     @11,@CF01971                                        0805 01250000
         L     GPR14P,PABWEQA(,GPR15P)                             0805 01251000
         OR    GPR14P,@11                                          0805 01252000
*     IF GPR14P=FMZCA020 THEN                                      0806 01253000
         CLR   GPR14P,@11                                          0806 01254000
         BE    @RT00806                                            0806 01255000
*       ;                                                          0807 01256000
*     ELSE                                                         0808 01257000
*       DO;                                                        0808 01258000
*FMZL1044:                                                         0809 01259000
*         ;                                                        0809 01260000
FMZL1044 DS    0H                                                  0810 01261000
*         DO WHILE(GPR14P->WKECHAIN^=FMZCA020);                    0810 01262000
         B     @DE00810                                            0810 01263000
@DL00810 DS    0H                                                  0811 01264000
*           GPR15P=ADDR(GPR14P->WKECHAIN);                         0811 01265000
         LA    GPR15P,WKECHAIN(,GPR14P)                            0811 01266000
*           GPR14P=GPR15P->PABWEQA;                                0812 01267000
         L     GPR14P,PABWEQA(,GPR15P)                             0812 01268000
*         END;                                                     0813 01269000
@DE00810 L     @11,@CF01971                                        0813 01270000
         CL    @11,WKECHAIN(,GPR14P)                               0813 01271000
         BNE   @DL00810                                            0813 01272000
*         GPR00P=FMZCA020;                                         0814 01273000
         LR    GPR00P,@11                                          0814 01274000
*         CS(GPR14P,GPR00P,GPR15P->PABWEQA);                       0815 01275000
         CS    GPR14P,@00,PABWEQA(GPR15P)                          0815 01276000
*         BC(7,FMZL1044);                                          0816 01277000
         BC    7,FMZL1044                                          0816 01278000
*         GPR14P->WKECHAIN=0;                                      0817 01279000
         SLR   @11,@11                                             0817 01280000
         ST    @11,WKECHAIN(,GPR14P)                               0817 01281000
*       END;                                                       0818 01282000
*ISTA0044:                                                         0819 01283000
*     ;                                                            0819 01284000
@RT00806 DS    0H                                                  0819 01285000
ISTA0044 DS    0H                                                  0820 01286000
*     GPR14P=GPR14P&FMZCB020;                                      0820 01287000
         N     GPR14P,@CF01969                                     0820 01288000
*     GPR01P->RPHWEA=GPR14P;                                       0821 01289000
         ST    GPR14P,RPHWEA(,GPR01P)                              0821 01290000
*     RFY                                                          0822 01291000
*      (GPR00P,                                                    0822 01292000
*       GPR01P,                                                    0822 01293000
*       GPR14P,                                                    0822 01294000
*       GPR15P) UNRSTD;                                            0822 01295000
*   END;                            /* END TPDEQ GENERATION          */ 01296000
*   RFY                                                            0824 01297000
*     LNKREG RSTD;                                                 0824 01298000
*   LNKREG=ZCRSVF8;                 /* RESTORE RETURN ADDRESS        */ 01299000
         L     @11,RPHCRR(,RPHPTR)                                 0825 01300000
         L     @11,CRACRR(,@11)                                    0825 01301000
         L     LNKREG,ZCRSVF8(,@11)                                0825 01302000
*   RFY                                                            0826 01303000
*     LNKREG UNRSTD;                                               0826 01304000
*   END FSBDEQ;                                                    0827 01305000
*                                                                  0827 01306000
@EL00006 DS    0H                                                  0827 01307000
@EF00006 DS    0H                                                  0827 01308000
@ER00006 BR    @14                                                 0827 01309000
*/* TPEPILOG                                                         */ 01310000
*                                                                  0828 01311000
*   GEN(PRINT OFF);                                                0828 01312000
         PRINT OFF                                                      01313000
*   DCL                                                            0829 01314000
*     ISTAPC34 ENTRY BASED(ATCAP34) VALRG(*);/* RESET ROUTINE        */ 01315000
*   DCL                                                            0830 01316000
*     ISTAPC37 ENTRY BASED(ATCAP37) VALRG(*);/* TPQUE ROUTINE        */ 01317000
*   GEN(PRINT ON);                                                 0831 01318000
*                                                                  0831 01319000
         PRINT ON                                                       01320000
*/* END OF TPEPILOG                                                  */ 01321000
*                                                                  0832 01322000
*   END ISTZIFOB                                                   0832 01323000
