*MANDIAL@:                                                         0621 00666000
*   END;                                                           0621 00667000
MANDIAL@ DS    0H                                                  0622 00668000
*                                                                  0622 00669000
*/********************************************************************/ 00670000
*/*      MOVESETS - INITIALIZES THE FSBDMGRS FIELD FROM THE          */ 00671000
*/*                 APPROPRIATE FIELDS IN BTUTEXT AS DETERMINED      */ 00672000
*/*                 BY THE DEVICE TYPE.  IN ADDITION, THE SENSE      */ 00673000
*/*                 TRANSLATOR MODULE ISTZDFXR IS CALLED IF THE      */ 00674000
*/*                 DEVICE IS A 3270 - TYPE IN RECORD MODE.          */ 00675000
*/*                                                                  */ 00676000
*/*      THE SD CODE FOR THIS ROUTINE IS -->                 @XL03HDP*/ 00677000
*/********************************************************************/ 00678000
*                                                                  0622 00679000
*   /*****************************************************************/ 00680000
*   /*                                                               */ 00681000
*   /* SEGMENT(MVESETS)START                                         */ 00682000
*   /*                                                               */ 00683000
*   /*****************************************************************/ 00684000
*                                                                  0622 00685000
*   DO;                                                            0622 00686000
*     GOTO MVESETS@;                                               0623 00687000
         B     MVESETS@                                            0623 00688000
*MVESETS:                                                          0624 00689000
*   END;                                                           0624 00690000
MVESETS  DS    0H                                                  0625 00691000
*   IF(DEVTCODE=DEV3275             /* IS THIS ONE                   */ 00692000
*       |DEVTCODE=DEV3277           /* OF THOSE                      */ 00693000
*       |DEVTCODE=DEV3284           /* 3270 - TYPE                   */ 00694000
*       |DEVTCODE=DEV3286) THEN     /* DEVICES ?                     */ 00695000
         LA    @14,36                                              0625 00696000
         ALR   @14,DNCBPTR                                         0625 00697000
         CLI   DEVTCODE(@14),X'1C'                                 0625 00698000
         BE    @RT00625                                            0625 00699000
         CLI   DEVTCODE(@14),X'19'                                 0625 00700000
         BE    @RT00625                                            0625 00701000
         CLI   DEVTCODE(@14),X'1A'                                 0625 00702000
         BE    @RT00625                                            0625 00703000
         CLI   DEVTCODE(@14),X'1B'                                 0625 00704000
         BNE   @RF00625                                            0625 00705000
@RT00625 DS    0H                                                  0626 00706000
*     DO;                           /* IF IT IS..                    */ 00707000
*       FSBDMGRS=BTUTEXT(6:7);      /* COPY DEV MGT RESP             */ 00708000
         MVC   FSBDMGRS(2,FSBPTR),BTUTEXT+53(BFRREG)               0627 00709000
*       IF FMCMODE=FMCRCD THEN      /* IN RECORD MODE ?              */ 00710000
         CLC   FMCMODE(8,FMCBREG),@CC02455                         0628 00711000
         BNE   @RF00628                                            0628 00712000
*         CALL ISTZDFXR;            /* CALL SENSE XLATOR             */ 00713000
         L     @14,ATCZPSVT(,VTCVTREG)                             0629 00714000
         L     @15,ZPSZDFXR(,@14)                                  0629 00715000
         BALR  @14,@15                                             0629 00716000
*       ELSE                                                       0630 00717000
*         ;                         /* NOT IN RECORD MODE            */ 00718000
@RF00628 DS    0H                                                  0631 00719000
*     END;                          /* DONE WITH 3270                */ 00720000
*   ELSE                            /* NOT 3270 DEVICE               */ 00721000
*     FSBDMGRS=BTUTEXT(4:5);        /* DEVICE MGMT RESPONSE          */ 00722000
         B     @RC00625                                            0632 00723000
@RF00625 MVC   FSBDMGRS(2,FSBPTR),BTUTEXT+51(BFRREG)               0632 00724000
*   FDBDATCT=0;                     /* NO DATA                       */ 00725000
@RC00625 SLR   @14,@14                                             0633 00726000
         STH   @14,FDBDATCT(,BFRREG)                               0633 00727000
*   FDBDATAA=0;                     /* NO DATA ADDRESS               */ 00728000
         ST    @14,FDBDATAA(,BFRREG)                               0634 00729000
*   CALL ISTZDFNB;                  /* CALL MSG FORMATTER            */ 00730000
*                                                                  0635 00731000
         L     @14,ATCZPSVT(,VTCVTREG)                             0635 00732000
         L     @15,ZPSZDFNB(,@14)                                  0635 00733000
         BALR  @14,@15                                             0635 00734000
*   /*****************************************************************/ 00735000
*   /*                                                               */ 00736000
*   /* SEGMENT(MVESETS)FINISH                                        */ 00737000
*   /*                                                               */ 00738000
*   /*****************************************************************/ 00739000
*                                                                  0636 00740000
*   DO;                                                            0636 00741000
*     GOTO MVESETS#;                                               0637 00742000
         B     MVESETS#                                            0637 00743000
*MVESETS@:                                                         0638 00744000
*   END;                                                           0638 00745000
*   END NCPSTS;                                                    0639 00746000
@EL00002 DS    0H                                                  0639 00747000
@EF00002 DS    0H                                                  0639 00748000
@ER00002 BR    @14                                                 0639 00749000
*                                                                  0640 00750000
*/*    TPOST IS CALLED TO INITIALIZE THE FSB, REMOVE THE FSB FROM    */ 00751000
*/* THE WAIT QUEUE ELEMENT AND QUEUE THE FSB TO THE CLIP.            */ 00752000
*/* THE TPIOS FDB IS MOVED TO THE SYSTEM FDB LOCATION AND THE        */ 00753000
*/* FDBOFSET IS RECALCULATED.                                        */ 00754000
*/*                                                                  */ 00755000
*                                                                  0640 00756000
*TPOST:                                                            0640 00757000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0640 00758000
TPOST    DS    0H                                                  0641 00759000
*   RFY                                                            0641 00760000
*     LNKREG RSTD;                  /* RESTRICT REGISTER      @X03HDP*/ 00761000
*   ZCRSVF7=LNKREG;                 /* SAVE RETURN REG               */ 00762000
         L     @11,RPHCRR(,RPHPTR)                                 0642 00763000
         L     @11,CRACRR(,@11)                                    0642 00764000
         ST    LNKREG,ZCRSVF7(,@11)                                0642 00765000
*   RFY                                                            0643 00766000
*     LNKREG UNRSTD;                /* RELEASE REGISTER       @X03HDP*/ 00767000
*   IF LSCTYPE=LSTYPE&LSCLCCWA=0 THEN/* DISCONNECT AFTER WRT         */ 00768000
         CLI   LSCTYPE(ELEMREG),X'DF'                              0644 00769000
         BNE   @RF00644                                            0644 00770000
         ICM   @14,15,LSCLCCWA(ELEMREG)                            0644 00771000
         BNZ   @RF00644                                            0644 00772000
*     DO;                           /* PRINTER               @XA07962*/ 00773000
*       CALL FSBDEQ;                /* DEQ BUFFER FROM WAITQ ELEMENT    00774000
*                                                            @XA07962*/ 00775000
         BAL   @14,FSBDEQ                                          0646 00776000
*       ZCRPCOD1=ZCPBUF;            /* SET TO FREE BUFF      @XA07962*/ 00777000
         L     @14,RPHCRR(,RPHPTR)                                 0647 00778000
         L     @14,CRACRR(,@14)                                    0647 00779000
         MVI   ZCRPCOD1(@14),X'04'                                 0647 00780000
*       RFY                                                        0648 00781000
*         CMP1 RSTD;                /*                       @XA07962*/ 00782000
*       CMP1=ELEMREG;               /* SAVE ELEM ADDR        @XA07962*/ 00783000
