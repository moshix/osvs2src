*   RFY                                                            1048 00750000
*     REG14 RSTD;                                                  1048 00751000
*   ZCRSVF2=REG14;                  /* SAVE THE RETURN ADDRESS.      */ 00752000
         ST    REG14,ZCRSVF2(,ZCRRPTR)                             1049 00753000
*   RFY                                                            1050 00754000
*     REG14 UNRSTD;                                                1050 00755000
*   IF FMCCONF=ON|(LCPFDBA^=0&LCPFDBA->NCBCONF=ON)|/* IF DEVICE      */ 00756000
*       FMCDEVOF=ON THEN            /* (PU OR LU) SICK OR SESSION IS    00757000
*                                      DEAD                  @ZA29327*/ 00758000
         TM    FMCCONF(FMCBPTR),B'00000001'                        1051 00759000
         BO    @RT01051                                            1051 00760000
         L     @15,LCPFDBA(,LCPBPTR)                               1051 00761000
         LTR   @15,@15                                             1051 00762000
         BZ    @GL00002                                            1051 00763000
         TM    NCBCONF(@15),B'01000000'                            1051 00764000
         BO    @RT01051                                            1051 00765000
@GL00002 TM    FMCDEVOF(FMCBPTR),B'00010000'                       1051 00766000
         BNO   @RF01051                                            1051 00767000
@RT01051 DS    0H                                                  1052 00768000
*     ZCRTSW1=ON;                   /* INDICATE FSB IS TO BE QUEUED.    00769000
*                                                            @AS33958*/ 00770000
         OI    ZCRTSW1(ZCRRPTR),B'10000000'                        1052 00771000
*   ELSE                            /* IF DEVICE (PU OR LU) NOT SICK */ 00772000
*     IF ICNCBPTR->NCBTYPE^=LDTYPE THEN/*                    @ZA26631   00773000
*                                      IF NOT L/3270         @ZA26631*/ 00774000
         B     @RC01051                                            1053 00775000
@RF01051 CLI   NCBTYPE(ICNCBPTR),X'07'                             1053 00776000
