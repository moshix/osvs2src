*   END;                            /* END TPZENTER GENERATION       */ 00225000
*   DO WHILE(RPHWEA^=0);            /* LOOP WHILE THERE ARE ELEMENTS    00226000
*                                      ON THE PAB FOR ISTZLFAB.      */ 00227000
         B     @DE00893                                            0893 00228000
@DL00893 DS    0H                                                  0894 00229000
*     ZCRTSW1=OFF;                  /* INITIALIZE SWITCH TO INDICATE    00230000
*                                      FSB HAS NOT BEEN QUEUED.    0894 00231000
*                                                            @AS33958*/ 00232000
         NI    ZCRTSW1(ZCRRPTR),B'01111111'                        0894 00233000
