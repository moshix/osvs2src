         L     @15,RPHWEA(,RPHAPTR)                                0895 00235000
         LR    LCPBPTR,@15                                         0895 00236000
*     BFRPTR=RPHWEA;                /* POINT TO THE WORK ELEMENT.    */ 00237000
         LR    BFRPTR,@15                                          0896 00238000
*     IF LCPTYPE=NMLPBTYP THEN      /* IF WORK ELEMENT IS AN LCPB    */ 00239000
         CLI   LCPTYPE(LCPBPTR),X'27'                              0897 00240000
         BNE   @RF00897                                            0897 00241000
