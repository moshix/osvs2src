*                                                                  0491 01706000
*   R14=IOSBYPAS;                   /* BYPASS - DISPLACEMENT = 8     */ 01707000
BYPASIOS LA    R14,8                                               0491 01708000
*   GO TO IOSRET;                   /* RETURN TO IOS                 */ 01709000
         B     IOSRET                                              0492 01710000
*POSTIOS:                                                          0493 01711000
*                                                                  0493 01712000
*   /*****************************************************************/ 01713000
*   /*                                                               */ 01714000
*   /* IF BIT 2 OF IOBECBCC IS OFF THEN TURN ON THE ERROR COMPLETION */ 01715000
*   /* FLAG IN DCBIFLGS(DCBIFEC) (ONLY FOR RELATED REQUEST)  @YM03854*/ 01716000
*   /*                                                               */ 01717000
*   /*****************************************************************/ 01718000
*                                                                  0493 01719000
*   IF ERRESETF=RESET&IOBUNREL=RESET THEN                          0493 01720000
POSTIOS  TM    ERRESETF(IOBREG),B'00100000'                        0493 01721000
         BNZ   @RF00493                                            0493 01722000
         TM    IOBUNREL(IOBREG),B'00000010'                        0493 01723000
         BNZ   @RF00493                                            0493 01724000
*     DCBIFEC=IOSCMPLF;             /* SET ERROR FLAG IN DCBIFLGS  0494 01725000
*                                                            @ZA00616*/ 01726000
*                                                                  0494 01727000
         L     @15,DEBDCBAD(,DEBREG)                               0494 01728000
         OI    DCBIFEC+40(@15),B'11000000'                         0494 01729000
*   /*****************************************************************/ 01730000
*   /*                                                               */ 01731000
*   /* IF THE ECB IS NOT BEING WAITED UPON THEN POST THE ECB COMPLETE*/ 01732000
*   /* - SINCE THE LOCAL LOCK IS HELD AND ALL I/O IS COMPLETED BEFORE*/ 01733000
*   /* CONTROL IS PASSED TO THE USER THE COMPLETION CODE CAN BE MOVED*/ 01734000
*   /* INTO THE ECB WITHOUT USING THE POST INTERFACE. THE SKIP RETURN*/ 01735000
*   /* IS TAKEN SINCE THE ECB IS POSTED.                             */ 01736000
*   /*                                                               */ 01737000
*   /*****************************************************************/ 01738000
*                                                                  0495 01739000
*   IF ECB<WAITED THEN                                             0495 01740000
@RF00493 L     @15,IOBECBPT(,IOBREG)                               0495 01741000
         CLI   ECB(@15),X'80'                                      0495 01742000
         BNL   @RF00495                                            0495 01743000
*     DO;                           /* ECB NOT BEING WAITED UPON   0496 01744000
*                                                            @YM03828   01745000
*                                      THEN POST IS DONE HERE      0496 01746000
*                                                            @YM03854*/ 01747000
*       SPKA(RQEPRT);               /* PROTECT AGAINST MALICIOUS   0497 01748000
*                                      TAMPERING BY USER OF ECB    0497 01749000
*                                      ADDRESS                       */ 01750000
         SLR   @12,@12                                             0497 01751000
         IC    @12,RQEPRT(,RQEREG)                                 0497 01752000
         SPKA  0(@12)                                              0497 01753000
*       ECB=IOBECBCC;               /* POST ECB              @YM03854*/ 01754000
         MVC   ECB(1,@15),IOBECBCC(IOBREG)                         0498 01755000
*       SPKA(ZERO);                 /* RESTORE KEY AFTER POST        */ 01756000
         SLR   @15,@15                                             0499 01757000
         SPKA  0(@15)                                              0499 01758000
*       GO TO SKIPIOS;              /* BYPASS EXCPCI POST    @YM03854*/ 01759000
         B     SKIPIOS                                             0500 01760000
*     END;                          /*                       @YM03828*/ 01761000
*                                                                  0501 01762000
*   /*****************************************************************/ 01763000
*   /*                                                               */ 01764000
*   /* WAIT OUTSTANDING. USE EXCP CI POST FACILITY TO POST           */ 01765000
*   /*                                                               */ 01766000
*   /*****************************************************************/ 01767000
*                                                                  0502 01768000
*   R14=IOSPOST;                    /*                       @YM03828*/ 01769000
@RF00495 SLR   R14,R14                                             0502 01770000
*   GO TO IOSRET;                   /* POST IT               @YM03828*/ 01771000
         B     IOSRET                                              0503 01772000
*SKIPIOS:                                                          0504 01773000
*                                                                  0504 01774000
*   /*****************************************************************/ 01775000
*   /*                                                               */ 01776000
*   /* SKIP POST ENTRY                                               */ 01777000
*   /*                                                               */ 01778000
*   /*****************************************************************/ 01779000
*                                                                  0504 01780000
*   R14=IOSSKIP;                    /* SKIP - DISPLACEMENT = 4       */ 01781000
SKIPIOS  LA    R14,4                                               0504 01782000
*                                                                  0505 01783000
*   /*****************************************************************/ 01784000
*   /*                                                               */ 01785000
*   /* THIS ROUTINE RESTORES IOS REGISTERS AND RETURNS               */ 01786000
*   /*                                                               */ 01787000
*   /*****************************************************************/ 01788000
*                                                                  0505 01789000
*IOSRET:                                                           0505 01790000
*   RESPECIFY                                                      0505 01791000
*    (R5) RSTD;                                                    0505 01792000
*                                                                  0505 01793000
IOSRET   DS    0H                                                  0506 01794000
*   /*****************************************************************/ 01795000
*   /*                                                               */ 01796000
*   /* RESTORE IOS REGISTERS                                         */ 01797000
*   /*                                                               */ 01798000
*   /*****************************************************************/ 01799000
*                                                                  0506 01800000
*   SAVERG=RQENRQE;                 /* SAVE AREA                     */ 01801000
         L     SAVERG,RQENRQE(,RQEREG)                             0506 01802000
*   R14=R14+EIREG14;                /* CALCULATE RETURN ADDRESS - R14   01803000
*                                      + 4 SKIP POST, R14 + 8 BYPASS    01804000
*                                      POST                          */ 01805000
         L     @15,RQESRB(,RQEREG)                                 0507 01806000
         AL    R14,EIREG14(,@15)                                   0507 01807000
*   R5=EIREG5;                      /* RESTORE FOR IOS               */ 01808000
*                                                                  0508 01809000
         L     R5,EIREG5(,@15)                                     0508 01810000
*   /*****************************************************************/ 01811000
*   /*                                                               */ 01812000
*   /* DELETE RECOVERY ENVIRENMENT                                   */ 01813000
*   /*                                                               */ 01814000
*   /*****************************************************************/ 01815000
*                                                                  0509 01816000
*   RESPECIFY                                                      0509 01817000
*    (R10,                                                         0509 01818000
*     R11) RSTD;                                                   0509 01819000
*   GEN SETS(R10,R11,FRRSCURR) REFS(PSACSTK,PSA,FRRSCURR,FRRS,FRRSEMP,  01820000
*       FRRSELEN)                                                  0510 01821000
*       (SETFRR D,WRKREGS=(R10,R11)       RELEASE RECOVERY ENVIRONMENT) 01822000
*       ;                                                          0510 01823000
         SETFRR D,WRKREGS=(R10,R11)       RELEASE RECOVERY ENVIRONMENT  01824000
*   RESPECIFY                                                      0511 01825000
*    (R10,                                                         0511 01826000
*     R11) UNRSTD;                                                 0511 01827000
*                                                                  0511 01828000
*   /*****************************************************************/ 01829000
*   /*                                                               */ 01830000
*   /* RETURN TO IOS                                                 */ 01831000
*   /*                                                               */ 01832000
*   /*****************************************************************/ 01833000
*                                                                  0512 01834000
*   END IDDWIAPP                                                   0512 01835000
*                                                                  0512 01836000
