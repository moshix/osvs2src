         SLA   @12,20                                              0237 00408000
         OR    GPR03F,@12                                          0237 00409000
*         CALL CVTCRMN;             /* BRANCH ENTRY POINT            */ 00410000
         L     @12,CVTPTR                                          0238 00411000
         L     @15,CVTCRMN(,@12)                                   0238 00412000
         BALR  @14,@15                                             0238 00413000
*         RESPECIFY                                                0239 00414000
*          (GPR01F,                                                0239 00415000
*           GPR03F,                                                0239 00416000
*           GPR00F) UNRESTRICTED;                                  0239 00417000
*       END;                        /* GETMAIN (RU) LV(LENGTH(WICB))    00418000
