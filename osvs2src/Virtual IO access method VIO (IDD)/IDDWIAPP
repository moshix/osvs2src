         TITLE 'IDDWIAPP - EXCP INTERCEPT APPENDAGE SIMULATOR          *00001000
                        '                                               00002000
IDDWIAPP CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
         USING *,R15                    ADDRESSABILITY FOR BRANCH TABLE 00005000
         B     WIEXCP                   WI EXCP ENTRY POINT             00006000
         B     WICLOSE                  WI CLOSE ENTRY POINT            00007000
         DS    0H                       WI JOURNAL ENTRY POINT          00008000
         L     R15,ADWIJRN              POINTER TO JOURNAL MODULE       00009000
         BR    R15                      GO TO IDDWIJRN                  00010000
         DC    C'IDDWIAPP'             MODULE ID              @YM01137  00011000
WICLOSE  L     R15,ADWICLS              POINTER TO CLOSE MODULE         00012000
         BR    R15                      GO TO IDDWICLS                  00013000
WIEXCP   EQU   *                        *                               00014000
         BALR  R8,0                     ADDRESSABILITY IDDWIEXCP        00015000
         USING *,R8                     *                               00016000
*   R9=ADDR(IDDWIFRR);              /* RECOVERY POINT                */ 00017000
         L     R9,@CV00557                                         0146 00018000
*   GEN SETS(R9,R10,R11,FRRSCURR,FRRSFRRA,FRRSPARM) REFS(R9,PSA,PSACSTK 00019000
*       ,FRRSCURR,FRRS,FRRSLAST,FRRSELEN,FRRSPARM,PSALSFCC)        0147 00020000
*       (SETFRR A,FRRAD=(R9),PARMAD=(R9),WRKREGS=(R10,R11) );/*    0147 00021000
*                                                            @YM03030*/ 00022000
         SETFRR A,FRRAD=(R9),PARMAD=(R9),WRKREGS=(R10,R11)              00023000
*   RQESRB=R9;                      /* SAVE RECOVERY PARAMETER LIST     00024000
*                                                            @YM03196*/ 00025000
         ST    R9,RQESRB(,RQEREG)                                  0148 00026000
*   RESPECIFY                                                      0149 00027000
*     EIFRRPRM BASED(RQESRB);       /*                       @YM03196*/ 00028000
*                                                                  0149 00029000
*   /*****************************************************************/ 00030000
*   /*                                                               */ 00031000
*   /* SAVE IOS REGISTER FOR SUBSEQUENT RETURN IN THE RQE            */ 00032000
*   /*                                                               */ 00033000
*   /*****************************************************************/ 00034000
*                                                                  0150 00035000
*   RQENRQE=SAVERG;                 /* SAVE AREA PASSED              */ 00036000
         ST    SAVERG,RQENRQE(,RQEREG)                             0150 00037000
*   EIRQEPTR=RQEREG;                /* SAVE RQE POINTER              */ 00038000
         L     @15,RQESRB(,RQEREG)                                 0151 00039000
         ST    RQEREG,EIRQEPTR(,@15)                               0151 00040000
*   EIREG14=R14;                    /* SAVE RETURN VECTOR            */ 00041000
         ST    R14,EIREG14(,@15)                                   0152 00042000
*   EIREG5=R5;                      /* SAVE IOS BASE REGISTER        */ 00043000
         ST    R5,EIREG5(,@15)                                     0153 00044000
*   RESPECIFY                                                      0154 00045000
*    (R5,                                                          0154 00046000
*     R14,                                                         0154 00047000
*     R9,                                                          0154 00048000
*     R10,                                                         0154 00049000
*     R11) UNRSTD;                  /* RELEASE REGISTERS             */ 00050000
*                                                                  0155 00051000
*/********************************************************************/ 00052000
*/*                                                                  */ 00053000
*/* FUNCTION - SETS UP REGISTERS AND ADDRESSABILITY FOR MODULE       */ 00054000
*/*                                                                  */ 00055000
*/********************************************************************/ 00056000
*                                                                  0155 00057000
*   RESPECIFY                                                      0155 00058000
*     VDSCB BASED(VDSCBREG);        /* VDSCB AND UCB ADDRESSABILITY  */ 00059000
*   RESPECIFY                                                      0156 00060000
*     IOBSTDRD BASED(IOBREG);       /* IOB ADDRESSABILITY            */ 00061000
*   RESPECIFY                                                      0157 00062000
*     DEBBASIC BASED(RQEDEB);       /* DEB ADDRESSABILITY            */ 00063000
*                                                                  0157 00064000
*   /*****************************************************************/ 00065000
*   /*                                                               */ 00066000
*   /* WICB AND WICSEEKA(APPENDAGES) ADDRESSABILITY                  */ 00067000
*   /*                                                               */ 00068000
*   /*****************************************************************/ 00069000
*                                                                  0158 00070000
*   WICBREG=VDSWICB;                                               0158 00071000
         L     WICBREG,VDSWICB(,VDSCBREG)                          0158 00072000
*   RESPECIFY                                                      0159 00073000
*     WICB BASED(WICBREG);                                         0159 00074000
*                                                                  0160 00075000
*/********************************************************************/ 00076000
*/*                                                                  */ 00077000
*/* FUNCTION - INSURES THAT THE WINDOW, WICB AND VBPPL EXIST FOR THE */ 00078000
*/*      EXIST FOR THE APPENDAGE INTERCEPT AND THE TRACK MANAGER.    */ 00079000
*/*      IF THE KEY OF THE USER HAS CHANGED IT FREES THE CURRENT     */ 00080000
*/*      WINDOW AND WICB AND REACQUIRES THEM IN THE NEW USER KEY.    */ 00081000
*/*      ALSO FREE WICB AND WINDOW IF PREVIOUSLY ACQUIRED BUT A      */ 00082000
*/*      SUCCESSFUL VOPEN WAS NEVER COMPLETED AS EVIDENCED BY A      */ 00083000
*/*      NON ZERO WICB POINTER AND A ZERO DSPCT POINTER.     @ZA00121*/ 00084000
*/*                                                                  */ 00085000
*/********************************************************************/ 00086000
*                                                                  0160 00087000
*   IF((WICBREG^=0)&(VDSDSPCT=0))|(((WICPRT&'F0'X)^=/*       @ZA27906*/ 00088000
*       (RQEPRT&'F0'X))&(WICBREG^=0)) THEN                         0160 00089000
         SLR   @12,@12                                             0160 00090000
         CR    WICBREG,@12                                         0160 00091000
         BE    @GL00001                                            0160 00092000
         C     @12,VDSDSPCT(,VDSCBREG)                             0160 00093000
         BE    @RT00160                                            0160 00094000
@GL00001 LA    @12,240                                             0160 00095000
         SLR   @09,@09                                             0160 00096000
         IC    @09,WICPRT(,WICBREG)                                0160 00097000
         NR    @09,@12                                             0160 00098000
         SLR   @05,@05                                             0160 00099000
         IC    @05,RQEPRT(,RQEREG)                                 0160 00100000
         NR    @12,@05                                             0160 00101000
         CR    @09,@12                                             0160 00102000
         BE    @RF00160                                            0160 00103000
         LTR   WICBREG,WICBREG                                     0160 00104000
         BZ    @RF00160                                            0160 00105000
@RT00160 DS    0H                                                  0161 00106000
*     DO;                           /*                       @ZA27906   00107000
*                                      KEY OF USER HAS CHANGED - OR     00108000
*                                      UNSUCESSFUL VOPEN FREE WICB 0161 00109000
*                                      AND WINDOW SO THEY CAN BE   0161 00110000
*                                      ACQUIRED IN THE NEW KEY.    0161 00111000
*                                                            @ZA00121*/ 00112000
*       RESPECIFY                                                  0162 00113000
*        (R9,                                                      0162 00114000
*         R10) RSTD;                /* SAVE THE RQE AND VDSCB      0162 00115000
*                                      POINTERS                      */ 00116000
*       R9=VDSCBREG;                /* VDSCB ADDRESSABILITY          */ 00117000
         LR    R9,VDSCBREG                                         0163 00118000
*       RESPECIFY                                                  0164 00119000
*         VDSCB BASED(R9);                                         0164 00120000
*       RESPECIFY                                                  0165 00121000
*         UCBOB BASED(R9);          /* UCB ADDRESSABILITY            */ 00122000
*       R10=RQEREG;                 /* RQE ADDRESSABILITY            */ 00123000
         LR    R10,RQEREG                                          0166 00124000
*       RESPECIFY                                                  0167 00125000
*         RQE BASED(R10);                                          0167 00126000
*                                                                  0167 00127000
*       /*************************************************************/ 00128000
*       /*                                                           */ 00129000
*       /* PURGE THE WINDOW - CALL THE TRACK MANAGER TO VWRITE THE   */ 00130000
*       /* WINDOW SO IT CAN BE FREED                                 */ 00131000
*       /*                                                           */ 00132000
*       /*************************************************************/ 00133000
*                                                                  0168 00134000
*       RESPECIFY                                                  0168 00135000
*        (R1) RSTD;                                                0168 00136000
*       IF VDSDSPCT^=0 THEN                                        0169 00137000
         L     @12,VDSDSPCT(,R9)                                   0169 00138000
         LTR   @12,@12                                             0169 00139000
         BZ    @RF00169                                            0169 00140000
*         DO;                       /* MAKE SURE THAT VOPEN HAS    0170 00141000
*                                      COMPLETED SUCCESSFULLY      0170 00142000
*                                                            @ZA00121*/ 00143000
*           R1=0;                   /* RQE = 0 FORCES WINDOW PURGE   */ 00144000
         SLR   R1,R1                                               0171 00145000
*           CALL IDDWITRM;          /* VWRITE THE WINDOW             */ 00146000
         L     @15,@CV00548                                        0172 00147000
         BALR  @14,@15                                             0172 00148000
*         END;                      /* END OF FLUSH WINDOW   @ZA00121*/ 00149000
*                                                                  0173 00150000
*       /*************************************************************/ 00151000
*       /*                                                           */ 00152000
*       /* PREPARE TO FREE THE WINDOW AND THE WICB                   */ 00153000
*       /*                                                           */ 00154000
*       /*************************************************************/ 00155000
*                                                                  0174 00156000
*       R7=ASCBPTR;                 /* ASCB ADDRESSABILITY FOR     0174 00157000
*                                      FREEMAIN                      */ 00158000
@RF00169 L     @12,CVTPTR                                          0174 00159000
         L     @11,CVTTCBP(,@12)                                   0174 00160000
         L     R7,ASCBPTR(,@11)                                    0174 00161000
*       R4=WICTCB;                  /* FREEMAIN UNDER THE SAME TCB AS   00162000
*                                      GETMAIN               @VS05944*/ 00163000
*                                                                  0175 00164000
         L     R4,WICTCB(,WICBREG)                                 0175 00165000
*       /*************************************************************/ 00166000
*       /*                                                           */ 00167000
*       /* FREE THE WINDOW                                           */ 00168000
*       /*                                                           */ 00169000
*       /*************************************************************/ 00170000
*                                                                  0176 00171000
*       DO;                         /* FREEMAIN(RU) LV(VDSWINSI)A(VDS   00172000
*                                      WINDW->VTRACK) SP(SP229)    0176 00173000
*                                      KEY(WICPRT/16) BRANCH         */ 00174000
*         RESPECIFY                                                0177 00175000
*          (GPR01F,                                                0177 00176000
*           GPR03F,                                                0177 00177000
*           GPR00F) RESTRICTED;                                    0177 00178000
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */ 00179000
         SLR   GPR01F,GPR01F                                       0178 00180000
*         GPR03F=3;                 /* SET TYPE                      */ 00181000
         LA    GPR03F,3                                            0179 00182000
*         GPR00F=VDSWINSI;          /* LENGTH REQUESTED              */ 00183000
         LH    GPR00F,VDSWINSI(,R9)                                0180 00184000
*         GPR01F=ADDR(VDSWINDW->VTRACK);/* SET REGISTER ONE          */ 00185000
         L     GPR01F,VDSWINDW(,R9)                                0181 00186000
*         GPR03F=GPR03F+((SP229)*256);/* SUBPOOL VALUE               */ 00187000
         AL    GPR03F,@CF03313                                     0182 00188000
*         GPR03F=GPR03F|((WICPRT/16)*1048576);/* KEY IN BYTE ONE     */ 00189000
         SLR   @11,@11                                             0183 00190000
         IC    @11,WICPRT(,WICBREG)                                0183 00191000
         SRL   @11,4                                               0183 00192000
         SLA   @11,20                                              0183 00193000
         OR    GPR03F,@11                                          0183 00194000
*         CALL CVTCRMN;             /* BRANCH ENTRY POINT            */ 00195000
         L     @15,CVTCRMN(,@12)                                   0184 00196000
         BALR  @14,@15                                             0184 00197000
*         RESPECIFY                                                0185 00198000
*          (GPR01F,                                                0185 00199000
*           GPR03F,                                                0185 00200000
*           GPR00F) UNRESTRICTED;                                  0185 00201000
*       END;                        /* FREEMAIN(RU) LV(VDSWINSI)A(VDS   00202000
*                                      WINDW->VTRACK) SP(SP229)    0186 00203000
*                                      KEY(WICPRT/16) BRANCH @YM01169*/ 00204000
*                                                                  0186 00205000
*       /*************************************************************/ 00206000
*       /*                                                           */ 00207000
*       /* FREE THE WICB                                             */ 00208000
*       /*                                                           */ 00209000
*       /*************************************************************/ 00210000
*                                                                  0187 00211000
*       DO;                         /* FREEMAIN(RU)                0187 00212000
*                                      LV(LENGTH(WICB))A(WICB)     0187 00213000
*                                      SP(230) KEY(WICPRT/16) BRANCH */ 00214000
*         RESPECIFY                                                0188 00215000
*          (GPR01F,                                                0188 00216000
*           GPR03F,                                                0188 00217000
*           GPR00F) RESTRICTED;                                    0188 00218000
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */ 00219000
         SLR   GPR01F,GPR01F                                       0189 00220000
*         GPR03F=3;                 /* SET TYPE                      */ 00221000
         LA    GPR03F,3                                            0190 00222000
*         GPR00F=LENGTH(WICB);      /* LENGTH REQUESTED              */ 00223000
         LA    GPR00F,124                                          0191 00224000
*         GPR01F=ADDR(WICB);        /* SET REGISTER ONE              */ 00225000
         LR    GPR01F,WICBREG                                      0192 00226000
*         GPR03F=GPR03F+((230)*256);/* SUBPOOL VALUE                 */ 00227000
         AL    GPR03F,@CF03315                                     0193 00228000
*         GPR03F=GPR03F|((WICPRT/16)*1048576);/* KEY IN BYTE ONE     */ 00229000
         SLR   @12,@12                                             0194 00230000
         IC    @12,WICPRT(,WICBREG)                                0194 00231000
         SRL   @12,4                                               0194 00232000
         SLA   @12,20                                              0194 00233000
         OR    GPR03F,@12                                          0194 00234000
*         CALL CVTCRMN;             /* BRANCH ENTRY POINT            */ 00235000
         L     @12,CVTPTR                                          0195 00236000
         L     @15,CVTCRMN(,@12)                                   0195 00237000
         BALR  @14,@15                                             0195 00238000
*         RESPECIFY                                                0196 00239000
*          (GPR01F,                                                0196 00240000
*           GPR03F,                                                0196 00241000
*           GPR00F) UNRESTRICTED;                                  0196 00242000
*       END;                        /* FREEMAIN(RU)                0197 00243000
*                                      LV(LENGTH(WICB))A(WICB)     0197 00244000
*                                      SP(230) KEY(WICPRT/16) BRANCH */ 00245000
*                                                                  0198 00246000
*       /*************************************************************/ 00247000
*       /*                                                           */ 00248000
*       /* DISCONNECT OLD WICB AND WINDOW FROM THE VDSCB             */ 00249000
*       /*                                                           */ 00250000
*       /*************************************************************/ 00251000
*                                                                  0198 00252000
*       WICBREG=0;                  /* NO WICB                       */ 00253000
         SLR   WICBREG,WICBREG                                     0198 00254000
*       VDSWICB=0;                  /* NO WICB                       */ 00255000
         SLR   @12,@12                                             0199 00256000
         ST    @12,VDSWICB(,R9)                                    0199 00257000
*       VDSWINDW=0;                 /* NO WINDOW                     */ 00258000
*                                                                  0200 00259000
         ST    @12,VDSWINDW(,R9)                                   0200 00260000
*       /*************************************************************/ 00261000
*       /*                                                           */ 00262000
*       /* REESTABLISH VDSCB AND RQE ADDRESSABILITY                  */ 00263000
*       /*                                                           */ 00264000
*       /*************************************************************/ 00265000
*                                                                  0201 00266000
*       VDSCBREG=R9;                /* VDSCB                         */ 00267000
         LR    VDSCBREG,R9                                         0201 00268000
*       RQEREG=R10;                 /* RQE                           */ 00269000
         LR    RQEREG,R10                                          0202 00270000
*     END;                                                         0203 00271000
*   IF WICBREG=0 THEN                                              0204 00272000
@RF00160 SLR   @12,@12                                             0204 00273000
         CR    WICBREG,@12                                         0204 00274000
         BNE   @RF00204                                            0204 00275000
*     DO;                           /* ACQUIRE THE WICB AND WINDOW 0205 00276000
*                                      AND IF NECESSARY THE VBPPL. 0205 00277000
*                                      FILL IN THE WICB              */ 00278000
*                                                                  0205 00279000
*       /*************************************************************/ 00280000
*       /*                                                           */ 00281000
*       /* SAVE THE RQE AND VDSCB POINTERS                   @ZA00139*/ 00282000
*       /*                                                           */ 00283000
*       /*************************************************************/ 00284000
*                                                                  0206 00285000
*       R9=VDSCBREG;                /* VDSCB ADDRESSABILITY  @ZA00139*/ 00286000
         LR    R9,VDSCBREG                                         0206 00287000
*       R10=RQEREG;                 /* RQE ADDRESSABILITY    @ZA00139*/ 00288000
*                                                                  0207 00289000
         LR    R10,RQEREG                                          0207 00290000
*       /*************************************************************/ 00291000
*       /*                                                           */ 00292000
*       /* ABEND IF THE DATA SET HAS BEEN SCRATCHED. SCRATCH SETS    */ 00293000
*       /* RELATIVE TRACK TO -1 WHEN IT SCRATCHES VIRTUAL DATA SETS. */ 00294000
*       /* ALSO MERGE MODULE IDAVBPJ2 SETS VBPHSCR ON WHEN IT DOES AN*/ 00295000
*       /* ACTIVATE ON A LOGICAL GROUP BUT THE LOGICAL GROUP HAS BEEN*/ 00296000
*       /* DELETED.                                          @ZA00139*/ 00297000
*       /*                                                           */ 00298000
*       /*************************************************************/ 00299000
*                                                                  0208 00300000
*       IF VDSABSTT=-1|((VDSDSPCT^=0)&(VBPHDEL=SET)) THEN          0208 00301000
         CLC   VDSABSTT(2,R9),@CH03240                             0208 00302000
         BE    @RT00208                                            0208 00303000
         L     @11,VDSDSPCT(,R9)                                   0208 00304000
         CR    @11,@12                                             0208 00305000
         BE    @RF00208                                            0208 00306000
         TM    VBPHDEL(@11),B'00001000'                            0208 00307000
         BNO   @RF00208                                            0208 00308000
@RT00208 DS    0H                                                  0209 00309000
*         DO;                       /*                       @ZA00139*/ 00310000
*           RESPECIFY                                              0210 00311000
*             R15 RSTD;                                            0210 00312000
*           R15=24;                 /* INDICATE REASON FOR ABEND     */ 00313000
         LA    R15,24                                              0211 00314000
*           GEN(ABEND X'0E6',DUMP,,SYSTEM)                         0212 00315000
*           EXIT NOSEQFLOW;                                        0212 00316000
         ABEND X'0E6',DUMP,,SYSTEM                                      00317000
*           RESPECIFY                                              0213 00318000
*             R15 UNRSTD;                                          0213 00319000
*         END;                                                     0214 00320000
*                                                                  0215 00321000
*       /*************************************************************/ 00322000
*       /*                                                           */ 00323000
*       /* PREPARE TO ACQUIRE CONTROL BLOCKS                         */ 00324000
*       /*                                                           */ 00325000
*       /*************************************************************/ 00326000
*                                                                  0215 00327000
*       R7=ASCBPTR;                 /* ASCB ADDRESSABILITY FOR     0215 00328000
*                                      GETMAIN                       */ 00329000
@RF00208 L     @12,CVTPTR                                          0215 00330000
         L     @11,CVTTCBP(,@12)                                   0215 00331000
         L     R7,ASCBPTR(,@11)                                    0215 00332000
*       R4=TCBJSTCB;                /* JOB STEP TCB ADDRESSABILITY 0216 00333000
*                                      FOR GETMAIN                   */ 00334000
*                                                                  0216 00335000
         L     @11,TCBPTR(,@11)                                    0216 00336000
         L     R4,TCBJSTCB(,@11)                                   0216 00337000
*       /*************************************************************/ 00338000
*       /*                                                           */ 00339000
*       /* ACQUIRE VBPPL IF NECESSARY                                */ 00340000
*       /*                                                           */ 00341000
*       /*************************************************************/ 00342000
*                                                                  0217 00343000
*       IF VDSVBPPL=0 THEN                                         0217 00344000
         L     @11,VDSVBPPL(,R9)                                   0217 00345000
         LTR   @11,@11                                             0217 00346000
         BNZ   @RF00217                                            0217 00347000
*         DO;                       /* NO VBPPL ACQUIRE ONE          */ 00348000
*           DO;                     /* GETMAIN (RU) LV(LENGTH(VBPPL))   00349000
*                                      SP(SP230) KEY(KEYONE) BRANCH  */ 00350000
*             RESPECIFY                                            0220 00351000
*              (GPR01F,                                            0220 00352000
*               GPR03F,                                            0220 00353000
*               GPR00F) RESTRICTED;                                0220 00354000
*             GPR01F=0;             /* REG 1 MUST BE ZERO            */ 00355000
         SLR   GPR01F,GPR01F                                       0221 00356000
*             GPR03F=2;             /* RU-TYPE GETMAIN               */ 00357000
         LA    GPR03F,2                                            0222 00358000
*             GPR00F=LENGTH(VBPPL); /* LENGTH REQUESTED              */ 00359000
         LA    GPR00F,184                                          0223 00360000
*             GPR03F=GPR03F|((SP230)*256);/* SP IN BYTE 2            */ 00361000
         O     GPR03F,@CF03315                                     0224 00362000
*             GPR03F=GPR03F|((KEYONE)*65536*16);/* KEY IN BYTE 1     */ 00363000
         SLR   @11,@11                                             0225 00364000
         IC    @11,KEYONE                                          0225 00365000
         SLA   @11,20                                              0225 00366000
