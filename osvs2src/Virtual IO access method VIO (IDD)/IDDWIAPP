*             CALL CVTCRMN;         /* BRANCH ENTRY POINT            */ 00368000
         L     @15,CVTCRMN(,@12)                                   0226 00369000
         BALR  @14,@15                                             0226 00370000
*             RESPECIFY                                            0227 00371000
*              (GPR01F,                                            0227 00372000
*               GPR03F,                                            0227 00373000
*               GPR00F) UNRESTRICTED;                              0227 00374000
*           END;                    /* GETMAIN (RU) LV(LENGTH(VBPPL))   00375000
*                                      SP(SP230) KEY(KEYONE) BRANCH  */ 00376000
*           VDSVBPPL=R1;            /* CONNECT VBPPL TO VDSCB        */ 00377000
         ST    R1,VDSVBPPL(,R9)                                    0229 00378000
*         END;                                                     0230 00379000
*                                                                  0231 00380000
*       /*************************************************************/ 00381000
*       /*                                                           */ 00382000
*       /* ACQUIRE WICB IN USER KEY LSQA. WORK AREA FOR EXCP         */ 00383000
*       /* INTERCEPT                                                 */ 00384000
*       /*                                                           */ 00385000
*       /*************************************************************/ 00386000
*                                                                  0231 00387000
*       DO;                         /* GETMAIN (RU) LV(LENGTH(WICB))    00388000
*                                      SP(SP230) KEY(RQEPRT/16)    0231 00389000
*                                      BRANCH                        */ 00390000
@RF00217 DS    0H                                                  0232 00391000
*         RESPECIFY                                                0232 00392000
*          (GPR01F,                                                0232 00393000
*           GPR03F,                                                0232 00394000
