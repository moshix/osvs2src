         TITLE 'IDDWIFRR - FUNCTIONAL RECOVERY ROUTINES                *00001000
                        '                                               00002000
IDDWIFRR CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IDDWIFRR  76.094'                                 0001 00009000
IDDFRRCL DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00178-IDDFRRCL)                                   00013000
         ENTRY IDDFRRCL                                                 00014000
IDDESTJR DS    0H                                                  0001 00015000
         USING *,@15                                               0001 00016000
         B     @PROLOG                                             0001 00017000
         DC    AL2(@EP00241-IDDESTJR)                                   00018000
         ENTRY IDDESTJR                                                 00019000
         DROP  @15                                                      00020000
@PROLOG  BALR  @12,0                                               0001 00021000
@PSTART  DS    0H                                                  0001 00022000
         USING @PSTART,@12                                         0001 00023000
         AH    @15,4(,@15)                                         0001 00024000
         BR    @15                                                 0001 00025000
@EP00001 DS    0H                                                  0002 00026000
JSCBDISP EQU 188                                                        00027000
JSCBAOS1 EQU  72                                                        00028000
JSCBAOS2 EQU 192                                                        00029000
*   R2=R0;                          /* SAVE WORK AREA ADDR   @YM03980*/ 00030000
         LR    R2,R0                                               0144 00031000
*   R13=R0+ONE00;                   /* SET R13 TO POINT TO SAVE AREA    00032000
