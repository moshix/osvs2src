*   /*****************************************************************/ 00870000
*   /*                                                               */ 00871000
*   /* PREVENT RECURSIVE ENTRY                                       */ 00872000
*   /*                                                               */ 00873000
*   /*****************************************************************/ 00874000
*                                                                  0329 00875000
*   ESJBASE=ZERO;                                                  0329 00876000
         SLR   @11,@11                                             0329 00877000
         ST    @11,ESJBASE(,@03)                                   0329 00878000
*                                                                  0330 00879000
*   /*****************************************************************/ 00880000
*   /*                                                               */ 00881000
*   /* SCHEDULE RETRY ROUTINE                                        */ 00882000
*   /*                                                               */ 00883000
*   /*****************************************************************/ 00884000
*                                                                  0330 00885000
*   DO;                             /* SETRP RC(4)RETREGS(YES)FRESDWA   00886000
