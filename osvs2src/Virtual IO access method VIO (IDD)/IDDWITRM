         MVC   WICAUDIT(3,RWICB),@CC01004                          0106 00272000
*         GEN(LM  R3,R11,VBPPLSV2(RVBPPLSV))                       0107 00273000
*         SETS(R3,R4,R5,R6,R7,R8,R9,R10,R11) REFS(RVBPPLSV,VBPPLSV2);   00274000
         LM  R3,R11,VBPPLSV2(RVBPPLSV)                                  00275000
*                                   /* RESTORE THE CHANNEL PGM     0108 00276000
*                                      INTERPRETER'S REG 3-11 AS THEY   00277000
*                                      WERE SAVED. THESE REGS ARE  0108 00278000
*                                      MEANINGLESS IF THIS IS NOT A     00279000
*                                      RESTART FROM A PCI REQUEST OR    00280000
*                                      REENTRY AFTER A NEW TRACK HAS    00281000
*                                      BEEN REQUESTED BY THE CPI     */ 00282000
*         CALL R15->CPI;            /* GO INTERPRET THE CHANNEL    0108 00283000
*                                      PROGRAM                       */ 00284000
*                                                                  0108 00285000
         BALR  @14,@15                                             0108 00286000
*         /***********************************************************/ 00287000
*         /*                                                         */ 00288000
*         /* RETURN FROM THE CHANNEL PROGRAM INTERPRETER             */ 00289000
*         /*                                                         */ 00290000
*         /***********************************************************/ 00291000
*                                                                  0109 00292000
*TRMCPRET:                                                         0109 00293000
*         SPKA(KEYZERO);            /* SET THE PROTECTION KEY BACK TO   00294000
*                                      ZERO IN ORDER TO SAVE REGS IN    00295000
*                                      PROTECTED CORE                */ 00296000
TRMCPRET SLR   @14,@14                                             0109 00297000
         SPKA  0(@14)                                              0109 00298000
*         GEN(STM  R3,R11,VBPPLSV2(RVBPPLSV))                      0110 00299000
*         REFS(R3,R4,R5,R6,R7,R8,R9,R10,R11,RVBPPLSV) SETS(VBPPLSV2);   00300000
         STM  R3,R11,VBPPLSV2(RVBPPLSV)                                 00301000
*                                   /* SAVE THE CHANNEL PGM        0111 00302000
*                                      INTERPRETERS REGS 3-11 IN THE    00303000
*                                      SECOND SAVE AREA PREFIXING THE   00304000
*                                      VBP PARM LIST                 */ 00305000
*         GEN(LM  R3,R11,TRM3TO11(RVBPPLSV))                       0111 00306000
*         SETS(R3,R4,R5,R6,R7,R8,R9,R10,R11) REFS(RVBPPLSV,TRM3TO11);   00307000
         LM  R3,R11,TRM3TO11(RVBPPLSV)                                  00308000
*                                   /* RESTORE OWN REGISTERS FROM THE   00309000
*                                      FIRST SAVE AREA PREFIXING THE    00310000
*                                      VBP PARM LIST                 */ 00311000
*         WICAUDIT='TRM';           /* TRACK MANAGER BACK IN CONTROL */ 00312000
         MVC   WICAUDIT(3,RWICB),@CC01006                          0112 00313000
*         RCNTRL=R15;               /* GET THE RETURN CODE PASSED  0113 00314000
*                                      BACK FROM THE CHANNEL PGM   0113 00315000
*                                      INTERPRETER WHICH IS ZERO IF     00316000
*                                      NOT A REQUEST FOR A NEW TRACK    00317000
*                                      AND FOUR IF IT IS A REQUEST 0113 00318000
*                                      FOR A NEW TRACK               */ 00319000
         LR    RCNTRL,R15                                          0113 00320000
*         RFY                                                      0114 00321000
*          (R0,                                                    0114 00322000
*           R1,                                                    0114 00323000
*           R12,                                                   0114 00324000
*           R15) UNRSTD;                                           0114 00325000
*         IF RCNTRL=TRACKREQ THEN   /* IF A NEW TRACK IS REQUESTED   */ 00326000
         C     RCNTRL,@CF00083                                     0115 00327000
         BNE   @RF00115                                            0115 00328000
*           CALL TRMSETU1;          /* GO SET UP REQUEST BY        0116 00329000
*                                      COMPLETING THE PARM LIST FOR     00330000
*                                      VBP                           */ 00331000
         BAL   @14,TRMSETU1                                        0116 00332000
*       END;                        /* END OF DO WHILE LOOP          */ 00333000
@RF00115 DS    0H                                                  0117 00334000
@DE00096 C     RCNTRL,@CF00083                                     0117 00335000
         BE    @DL00096                                            0117 00336000
*     END;                          /* END OF INTERCEPT PROCESSING   */ 00337000
*                                                                  0119 00338000
*   /*****************************************************************/ 00339000
*   /*                                                               */ 00340000
*   /* FOR CLOSE AND JOURNAL                                         */ 00341000
*   /*                                                               */ 00342000
*   /*****************************************************************/ 00343000
*                                                                  0119 00344000
*   ELSE                                                           0119 00345000
*     DO;                                                          0119 00346000
*                                                                  0119 00347000
         B     @RC00078                                            0119 00348000
@RF00078 DS    0H                                                  0120 00349000
*       /*************************************************************/ 00350000
*       /*                                                           */ 00351000
*       /* CHECK TO SEE IF THE TRACK BUFFER IS EMPTY OR IF A PREVIOUS*/ 00352000
*       /* READ HAS BEEN ISSUED. THE EXTENT NUMBER OF THE WINDOW     */ 00353000
*       /* CONTENTS IS SET TO X'FF' UNTIL THE FIRST READ HAS BEEN    */ 00354000
*       /* ISSUED.                                                   */ 00355000
*       /*                                                           */ 00356000
*       /*************************************************************/ 00357000
*                                                                  0120 00358000
*       IF VDSSEEKA(M)^=EMPTY THEN                                 0120 00359000
*                                                                  0120 00360000
         CLI   VDSSEEKA(RVDSCB),X'FF'                              0120 00361000
         BE    @RF00120                                            0120 00362000
*         /***********************************************************/ 00363000
*         /*                                                         */ 00364000
*         /* TRACK BUFFER IS NOT EMPTY. HAS THE CURRENT CONTENTS BEEN*/ 00365000
*         /* UPDATED.                                                */ 00366000
*         /*                                                         */ 00367000
*         /***********************************************************/ 00368000
*                                                                  0121 00369000
*         IF VTUPDATE=SWON THEN                                    0121 00370000
*                                                                  0121 00371000
         TM    VTUPDATE(RTRKBFR),B'00100000'                       0121 00372000
         BNO   @RF00121                                            0121 00373000
*           /*********************************************************/ 00374000
*           /*                                                       */ 00375000
*           /* CURRENT CONTENTS UPDATED                              */ 00376000
*           /*                                                       */ 00377000
*           /*********************************************************/ 00378000
*                                                                  0122 00379000
*           DO;                                                    0122 00380000
*             VTUPDATE=SWOFF;       /* SET THE TRACK UPDATE FLAG OFF */ 00381000
         NI    VTUPDATE(RTRKBFR),B'11011111'                       0123 00382000
*             BUFCMW=SWON;          /* SET THE WRITE REQUEST OPTION     00383000
*                                      ON IN THE VBP PARAMETER LIST  */ 00384000
         OI    BUFCMW(RVBPPL),B'10000000'                          0124 00385000
*             BUFCORBA=BUFCDDDD;    /* USE THE RBA CALCULATED FOR THE   00386000
*                                      READ WHICH FILLED THE BUFFER     00387000
*                                      AS THE WRITE RBA              */ 00388000
         L     @12,BUFCDDDD(,RVBPPL)                               0125 00389000
         ST    @12,BUFCORBA(,RVBPPL)                               0125 00390000
*             BUFCRRD=SWOFF;        /* TURN OFF THE READ REQUEST   0126 00391000
*                                      OPTION IN THE PARM LIST       */ 00392000
         NI    BUFCRRD(RVBPPL),B'11011111'                         0126 00393000
*             BUFCWLEN=VTDATEND;    /* PASS TO VBP THE ACTUAL NUMBER    00394000
*                                      OF BYTES WHICH OCCUPY THE   0127 00395000
*                                      TRACK BUFFER                  */ 00396000
         L     @12,VTDATEND(,RTRKBFR)                              0127 00397000
         STH   @12,BUFCWLEN(,RVBPPL)                               0127 00398000
*             DO;                                                  0128 00399000
*               RFY                                                0129 00400000
*                 GPR01P RSTD;                                     0129 00401000
*               GPR01P=RVBPPL;                                     0130 00402000
         LR    GPR01P,RVBPPL                                       0130 00403000
*               CALL VREADWR;       /* ENTER VREAD/VWRITE ROUTINE    */ 00404000
         L     @12,CVTPTR                                          0131 00405000
         L     @12,CVTPVBP(,@12)                                   0131 00406000
         L     @15,VBPMOD+4(,@12)                                  0131 00407000
         BALR  @14,@15                                             0131 00408000
*               RFY                                                0132 00409000
*                 GPR01P UNRSTD;                                   0132 00410000
*             END;                  /* CALL VBP                      */ 00411000
*           END;                    /* END OF WRITE FOR UPDATED TRACK*/ 00412000
*       VDSSEEKA(M)=EMPTY;          /* MARK THE TRACK BUFFER AS NO 0135 00413000
*                                      CONTENTS AND THAT A READ MUST    00414000
*                                      BE ISSUED FOR NEXT REQUEST    */ 00415000
@RF00121 DS    0H                                                  0135 00416000
@RF00120 MVI   VDSSEEKA(RVDSCB),X'FF'                              0135 00417000
*     END;                          /* END OF PROCESSING FOR CLOSE 0136 00418000
*                                      AND JOURNAL                   */ 00419000
*                                                                  0137 00420000
*   /*****************************************************************/ 00421000
*   /*                                                               */ 00422000
*   /* RETURN TO THE INVOKER OF THIS MODULE                          */ 00423000
*   /*                                                               */ 00424000
*   /*****************************************************************/ 00425000
*                                                                  0137 00426000
*   R13=RCALLSAV;                   /* RESTORE THE SAVE AREA POINTER    00427000
*                                      PASSED TO THIS MODULE         */ 00428000
@RC00078 LR    R13,RCALLSAV                                        0137 00429000
*                                                                  0138 00430000
*   /*****************************************************************/ 00431000
*   /*                                                               */ 00432000
*   /* THIS SUBROUTINE IS USED TO SET UP THE PARAMETER LIST FOR VBP. */ 00433000
*   /* IT IS CALLED WHEN THE CURRENT TRACK IS NOT THE SAME AS THE    */ 00434000
*   /* DESIRED TRACK. A SECONDARY ENTRY POINT IS PROVIDED FOR THE    */ 00435000
*   /* CASE WHEN THE TRACK BUFFER IS EMPTY. FOR THIS CASE, ONLY A    */ 00436000
*   /* READ IS DESIRED. THE ENTRY FROM CLOSE AND JOURNAL DO NOT USE  */ 00437000
*   /* THIS SUBROUTINE AS THE CODE WHICH HANDLES THOSE ENTRIES SET UP*/ 00438000
*   /* THE PARAMETER LIST FOR A WRITE ONLY                           */ 00439000
*   /*                                                               */ 00440000
*   /*****************************************************************/ 00441000
*                                                                  0138 00442000
*TRMSETU1:                                                         0138 00443000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0138 00444000
@EL00001 DS    0H                                                  0138 00445000
@EF00001 DS    0H                                                  0138 00446000
@ER00001 LM    @14,@12,12(@13)                                     0138 00447000
         BR    @14                                                 0138 00448000
TRMSETU1 DS    0H                                                  0139 00449000
*   RFY                                                            0139 00450000
*     R14 RSTD;                                                    0139 00451000
*                                                                  0139 00452000
*   /*****************************************************************/ 00453000
*   /*                                                               */ 00454000
*   /* HAS THE CURRENT TRACK BEEN UPDATED                            */ 00455000
*   /*                                                               */ 00456000
*   /*****************************************************************/ 00457000
*                                                                  0140 00458000
*   IF VTUPDATE=SWON THEN                                          0140 00459000
*                                                                  0140 00460000
         TM    VTUPDATE(RTRKBFR),B'00100000'                       0140 00461000
         BNO   @RF00140                                            0140 00462000
*     /***************************************************************/ 00463000
*     /*                                                             */ 00464000
*     /* CURRENT TRACK UPDATED. SET UP THE PARAMETER LIST FOR A      */ 00465000
*     /* WRITE.                                                      */ 00466000
*     /*                                                             */ 00467000
*     /***************************************************************/ 00468000
*                                                                  0141 00469000
*     DO;                                                          0141 00470000
*       BUFCMW=SWON;                /* SET WRITE OPTION ON           */ 00471000
         OI    BUFCMW(RVBPPL),B'10000000'                          0142 00472000
*       BUFCORBA=BUFCDDDD;          /* SET OUTPUT RBA TO THE OLD READ   00473000
*                                      RBA                           */ 00474000
         L     @15,BUFCDDDD(,RVBPPL)                               0143 00475000
         ST    @15,BUFCORBA(,RVBPPL)                               0143 00476000
*       VTUPDATE=SWOFF;             /* TURN OFF UPDATE FLAG IN THE 0144 00477000
*                                      STATUS BYTE OF THE TRACK      */ 00478000
         NI    VTUPDATE(RTRKBFR),B'11011111'                       0144 00479000
*       BUFCWLEN=VTDATEND;          /* INDICATE THE NUMBER OF BYTES     00480000
*                                      OF ACTUAL DATA ON THIS TRACK  */ 00481000
         L     @15,VTDATEND(,RTRKBFR)                              0145 00482000
         STH   @15,BUFCWLEN(,RVBPPL)                               0145 00483000
*     END;                          /* END WRITE OPTION SET UP       */ 00484000
*                                                                  0146 00485000
*   /*****************************************************************/ 00486000
*   /*                                                               */ 00487000
*   /* CURRENT TRACK NOT UPDATED                                     */ 00488000
*   /*                                                               */ 00489000
*   /*****************************************************************/ 00490000
*                                                                  0147 00491000
*   ELSE                                                           0147 00492000
*     BUFCMW=SWOFF;                 /* TURN OFF WRITE OPTION REQUEST */ 00493000
         B     @RC00140                                            0147 00494000
@RF00140 NI    BUFCMW(RVBPPL),B'01111111'                          0147 00495000
*                                                                  0148 00496000
*   /*****************************************************************/ 00497000
*   /*                                                               */ 00498000
*   /* SECONDARY ENTRY POINT TO SUBROUTINE IS PROVIDED FOR THE CASE  */ 00499000
*   /* WHERE THE TRACK BUFFER IS EMPTY AND ONLY A READ IS DESIRED.   */ 00500000
*   /*                                                               */ 00501000
*   /*****************************************************************/ 00502000
*                                                                  0148 00503000
*TRMSETU2:                                                         0148 00504000
*   ENTRY;                                                         0148 00505000
         B     @EC00148                                            0148 00506000
TRMSETU2 DS    0H                                                  0148 00507000
@EC00148 DS    0H                                                  0149 00508000
*   RFY                                                            0149 00509000
*     R11 RSTD;                                                    0149 00510000
*   R11=R14;                        /* SAVE RETURN ADDRESS           */ 00511000
         LR    R11,R14                                             0150 00512000
*   RFY                                                            0151 00513000
*     R14 UNRSTD;                                                  0151 00514000
*   BUFCRRD=SWON;                   /* SET READ REQUEST OPTION       */ 00515000
*                                                                  0152 00516000
         OI    BUFCRRD(RVBPPL),B'00100000'                         0152 00517000
*   /*****************************************************************/ 00518000
*   /*                                                               */ 00519000
*   /* CALCULATE THE RELATIVE BYTE ADDRESS FOR THE READ.             */ 00520000
*   /*                                                               */ 00521000
*   /*****************************************************************/ 00522000
*                                                                  0153 00523000
*   BUFCDDDD=((WICSEEKA(C1:C2)*WICMAXHH)+WICSEEKA(H1:H2)-VDSABSTT)*     00524000
*       VDSWINSI;                                                  0153 00525000
         SLR   @01,@01                                             0153 00526000
         ICM   @01,3,WICSEEKA+3(RWICB)                             0153 00527000
         MH    @01,WICMAXHH(,RWICB)                                0153 00528000
         SLR   @12,@12                                             0153 00529000
         ICM   @12,3,WICSEEKA+5(RWICB)                             0153 00530000
         ALR   @01,@12                                             0153 00531000
         SH    @01,VDSABSTT(,RVDSCB)                               0153 00532000
         MH    @01,VDSWINSI(,RVDSCB)                               0153 00533000
         ST    @01,BUFCDDDD(,RVBPPL)                               0153 00534000
*   DO;                                                            0154 00535000
*     RFY                                                          0155 00536000
*       GPR01P RSTD;                                               0155 00537000
*     GPR01P=RVBPPL;                                               0156 00538000
         LR    GPR01P,RVBPPL                                       0156 00539000
*     CALL VREADWR;                 /* ENTER VREAD/VWRITE ROUTINE    */ 00540000
         L     @12,CVTPTR                                          0157 00541000
         L     @12,CVTPVBP(,@12)                                   0157 00542000
         L     @15,VBPMOD+4(,@12)                                  0157 00543000
         BALR  @14,@15                                             0157 00544000
*     RFY                                                          0158 00545000
*       GPR01P UNRSTD;                                             0158 00546000
*   END;                            /* CALL VBP                      */ 00547000
*                                                                  0159 00548000
*   /*****************************************************************/ 00549000
*   /*                                                               */ 00550000
*   /* DETERMINE IF THIS TRACK HAS BEEN REFERENCED BEFORE. THIS IS   */ 00551000
*   /* DONE BY TESTING A BIT SET BY VBP WHEN IT DETECTS AN RBA WHICH */ 00552000
*   /* HAS NOT BEEN REFERENCED BEFORE.                               */ 00553000
*   /*                                                               */ 00554000
*   /*****************************************************************/ 00555000
*                                                                  0160 00556000
*   IF BUFCNLAS=SWON THEN                                          0160 00557000
*                                                                  0160 00558000
         TM    BUFCNLAS(RVBPPL),B'01000000'                        0160 00559000
         BNO   @RF00160                                            0160 00560000
*     /***************************************************************/ 00561000
*     /*                                                             */ 00562000
*     /* NEW TRACK INITIALIZATION                                    */ 00563000
*     /*                                                             */ 00564000
*     /***************************************************************/ 00565000
*                                                                  0161 00566000
*     DO;                                                          0161 00567000
*       VTRACK=VTRACK&&VTRACK;      /* CLEAR THE HEADER PORTION OF 0162 00568000
*                                      THE TRACK                     */ 00569000
         XC    VTRACK(31,RTRKBFR),VTRACK(RTRKBFR)                  0162 00570000
*       VTRKBAL=WICTRKCP;           /* SET THE TRACK BALANCE EQUAL TO   00571000
*                                      THE TRACK CAPACITY WHICH IS 0163 00572000
*                                      SPECIFIED IN THE DEVICE     0163 00573000
*                                      CHARACTERISTICS TABLE. THIS 0163 00574000
*                                      VALUE WILL BE DECREMENTED AS     00575000
*                                      RECORDS ARE ADDED TO THE    0163 00576000
*                                      TRACK.                        */ 00577000
         LH    @12,WICTRKCP(,RWICB)                                0163 00578000
         ST    @12,VTRKBAL(,RTRKBFR)                               0163 00579000
*       VTDATEND=LENGTH(VTRACK)-LENGTH(VTR1);/* SET THE DISPLACEMENT    00580000
*                                      FROM THE BEGINNING OF THE   0164 00581000
*                                      TRACK BUFFER EQUAL TO THE   0164 00582000
*                                      ADDRESS OF THE FIRST RECORD 0164 00583000
*                                      WHICH FOLLOWS RECORD ZERO     */ 00584000
         MVC   VTDATEND(4,RTRKBFR),@CF01014                        0164 00585000
*       VTR0CNT(CNTIDC1:CNTIDH2)=WICSEEKA(C1:H2);/* SET THE FIRST  0165 00586000
*                                      FOUR BYTES OF RECORD ZERO ID     00587000
*                                      EQUAL TO THE CYLINDER AND HEAD   00588000
*                                      ADDRESS OF THIS TRACK         */ 00589000
         MVC   VTR0CNT(4,RTRKBFR),WICSEEKA+3(RWICB)                0165 00590000
*       VTR0CNT(CNTDL2)=LENGTH(VTR0DATA);/* SET THE DATA LENGTH OF 0166 00591000
*                                      RECORD ZERO EQUAL TO EIGHT    */ 00592000
         MVI   VTR0CNT+7(RTRKBFR),X'08'                            0166 00593000
*       VTHA(HAC1:HAH2)=WICSEEKA(C1:H2);/* SET THE HOME OF THIS TRACK   00594000
*                                      EQUAL TO THE CYLINDER AND HEAD   00595000
*                                      VALUES REQUESTED IN THE SEEK     00596000
*                                      ADDR                          */ 00597000
*                                                                  0167 00598000
         MVC   VTHA+1(4,RTRKBFR),WICSEEKA+3(RWICB)                 0167 00599000
*       /*************************************************************/ 00600000
*       /*                                                           */ 00601000
*       /* SET INDICATOR IN THE TRACK TO WRITE THIS TRACK OUT BEFORE */ 00602000
*       /* SWITCHING THE TRACK                               @YM03861*/ 00603000
*       /*                                                           */ 00604000
*       /*************************************************************/ 00605000
*                                                                  0168 00606000
*       VTUPDATE=SWON;              /*                       @YM03861*/ 00607000
         OI    VTUPDATE(RTRKBFR),B'00100000'                       0168 00608000
*     END;                          /* END OF TRACK INITIALIZATION   */ 00609000
*                                                                  0169 00610000
*   /*****************************************************************/ 00611000
*   /*                                                               */ 00612000
*   /* INITIALIZE FIELDS IN THE INTERCEPT CONTROL BLOCK              */ 00613000
*   /*                                                               */ 00614000
*   /*****************************************************************/ 00615000
*                                                                  0170 00616000
*   WICDATND=ADDR(VTRACK)+VTDATEND; /* SET THE END OF DATA POINTER 0170 00617000
*                                      WHICH REPRESENTS THE END OF 0170 00618000
*                                      TRACK                         */ 00619000
@RF00160 LR    @12,RTRKBFR                                         0170 00620000
         AL    @12,VTDATEND(,RTRKBFR)                              0170 00621000
         ST    @12,WICDATND(,RWICB)                                0170 00622000
*   WICCAM=ADDR(VTR0);              /* SET THE CURRENT RECORD ADDRESS   00623000
