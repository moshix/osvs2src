         TITLE 'IDDWITRM   EXCP INTERCEPT PROCESSOR-TRACK MANAGER      *00001000
                        '                                               00002000
IDDWITRM CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(8)                                              0001 00007000
         DC    C'IDDWITRM'                                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @09,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@09                                         0001 00013000
*                                                                  0071 00014000
*   /*****************************************************************/ 00015000
*   /*                                                               */ 00016000
*   /* PERFORM INITIALIZATION FUNCTIONS OF SETTING UP POINTERS FOR   */ 00017000
*   /* BASED VARIABLES.                                              */ 00018000
*   /*                                                               */ 00019000
*   /*****************************************************************/ 00020000
*                                                                  0071 00021000
*   RCALLSAV=R13;                   /* SAVE THE ADDRESS OF THE     0071 00022000
*                                      CALLERS SAVE AREA             */ 00023000
         LR    RCALLSAV,R13                                        0071 00024000
*   RVBPPLSV=VDSVBPPL;              /* THE ADDRESS CONTAINED IN THE     00025000
*                                      VDSCB IS A POINTER TO THE VBP    00026000
*                                      PARAMETER LIST PREFIX WHICH IS   00027000
*                                      IN FACT TWO SAVE AREAS TO BE     00028000
*                                      USED                          */ 00029000
         L     RVBPPLSV,VDSVBPPL(,RVDSCB)                          0072 00030000
*   RVBPPL=ADDR(VBPRWPRM);          /* POINT TO THE ACTUAL VBP     0073 00031000
*                                      READ-WRITE PARM LIST TO BE  0073 00032000
*                                      USED.                         */ 00033000
         LA    RVBPPL,VBPRWPRM(,RVBPPLSV)                          0073 00034000
*   RTRKBFR=VDSWINDW;               /* ADDRESS OF THE TRACK BUFFER   */ 00035000
         L     RTRKBFR,VDSWINDW(,RVDSCB)                           0074 00036000
*   RCNTRL=TRACKREQ;                /* SET THE CONTROL REG SO THAT 0075 00037000
*                                      THE CPI WILL BE CALLED IF THIS   00038000
*                                      IS NOT AN ENTRY FROM CLOSE OR    00039000
*                                      JOURNAL.                      */ 00040000
         LA    RCNTRL,4                                            0075 00041000
*   RRQE=R1;                        /* SAVE THE ADDRESS OF THE RQE 0076 00042000
*                                      FOR SETTING THE KEY TO THE KEY   00043000
*                                      OF THE USER                   */ 00044000
         LR    RRQE,R1                                             0076 00045000
*   RFY                                                            0077 00046000
*     R1 UNRSTD;                                                   0077 00047000
*                                                                  0077 00048000
*   /*****************************************************************/ 00049000
*   /*                                                               */ 00050000
*   /* DETERMINE IF THIS IS A CALL FROM MODULE IDDWICLS OR IDDWIJRN  */ 00051000
*   /* AS OPPOSED TO IDDWIAPP. A NON-ZERO VALUE IN REGISTER ONE      */ 00052000
*   /* INDICATES THAT AN RQE IS BEING PASSED AND THERFORE INVOKED BY */ 00053000
*   /* MODULE IDDWIAPP                                               */ 00054000
*   /*                                                               */ 00055000
*   /*****************************************************************/ 00056000
*                                                                  0078 00057000
*   IF RRQE^=CLOSJOUR THEN                                         0078 00058000
*                                                                  0078 00059000
         LTR   RRQE,RRQE                                           0078 00060000
         BZ    @RF00078                                            0078 00061000
*     /***************************************************************/ 00062000
*     /*                                                             */ 00063000
*     /* CALLED BY MODULE IDDWIAPP                                   */ 00064000
*     /*                                                             */ 00065000
*     /***************************************************************/ 00066000
*                                                                  0079 00067000
*     DO;                                                          0079 00068000
*                                                                  0079 00069000
*       /*************************************************************/ 00070000
*       /*                                                           */ 00071000
*       /* DETERMINE IF THIS IS A PCI RESTART SITUATION              */ 00072000
*       /*                                                           */ 00073000
*       /*************************************************************/ 00074000
*                                                                  0080 00075000
*       IF WICPCIRS=SWOFF THEN                                     0080 00076000
*                                                                  0080 00077000
         TM    WICPCIRS(RWICB),B'10000000'                         0080 00078000
         BNZ   @RF00080                                            0080 00079000
*         /***********************************************************/ 00080000
*         /*                                                         */ 00081000
*         /* NOT A PCI RESTART SITUATION AND THEREFORE IT MUST BE    */ 00082000
*         /* DETERMINED IF THE DESIRED TRACK IS CURRENTLY AVAILABLE  */ 00083000
*         /* OR A NEW TRACK MUST BE REQUESTED. FIRST DETERMINE IF THE*/ 00084000
*         /* SEEK ADDRESS SUPPLIED IS A VALID SEEK ADDRESS. THE      */ 00085000
*         /* EXTENT CHECKING DONE BY THE APPENDAGE INTERFACE HANDLER */ 00086000
*         /* HAS CHECKED THE CYLINDER VALUES AND DETERMINED THAT THEY*/ 00087000
*         /* ARE WITHIN THE ALLOCATED EXTENT. HOWEVER THE CHECK DOES */ 00088000
*         /* NOT INSURE THAT THE HEAD VALUE IS LEGAL. IS THE         */ 00089000
*         /* REQUESTED SEEK ADDRESS A VALID ONE. ONLY THE HEAD       */ 00090000
*         /* ADDRESS MUST BE CHECKED ANS THE CYL ADDRESS HAS BEEN    */ 00091000
*         /* CHECKED DURING EXTENT CHECKING                          */ 00092000
*         /*                                                         */ 00093000
*         /***********************************************************/ 00094000
*                                                                  0081 00095000
*         IF WICSEEKA(H1:H2)>=WICMAXHH THEN                        0081 00096000
*                                                                  0081 00097000
         SLR   @12,@12                                             0081 00098000
         ICM   @12,3,WICSEEKA+5(RWICB)                             0081 00099000
         CH    @12,WICMAXHH(,RWICB)                                0081 00100000
         BL    @RF00081                                            0081 00101000
*           /*********************************************************/ 00102000
*           /*                                                       */ 00103000
*           /* AN INVALID SEEK ADDRESS HAS BEEN DETECTED             */ 00104000
*           /*                                                       */ 00105000
*           /*********************************************************/ 00106000
*                                                                  0082 00107000
*           DO;                                                    0082 00108000
*             WICRSTRT=SWON;        /* INDICATE TO THE CPI THAT THIS    00109000
*                                      SITUATION IS TO BE HANDLED AS    00110000
*                                      A RESTART                     */ 00111000
*             WICTRMSK=SWON;        /* AND INDICATE THE TYPE AS AN 0084 00112000
*                                      ERROR DETECTED IN THE SEEK  0084 00113000
*                                      ADDRESS                       */ 00114000
         OI    WICRSTRT(RWICB),B'00001001'                         0084 00115000
*           END;                    /* GO ENTER LOOP FOR INVOKING THE   00116000
*                                      CPI WITHOUT FURTHER PROCESSING*/ 00117000
*         ELSE                      /* VALID ADDRESS PASSED          */ 00118000
*                                                                  0086 00119000
*           /*********************************************************/ 00120000
*           /*                                                       */ 00121000
*           /* DETERMINE IF THE TRACK BUFFER IS EMPTY                */ 00122000
*           /*                                                       */ 00123000
*           /*********************************************************/ 00124000
*                                                                  0086 00125000
*           IF VDSSEEKA(M)^=EMPTY THEN                             0086 00126000
*                                                                  0086 00127000
         B     @RC00081                                            0086 00128000
@RF00081 CLI   VDSSEEKA(RVDSCB),X'FF'                              0086 00129000
         BE    @RF00086                                            0086 00130000
*             /*******************************************************/ 00131000
*             /*                                                     */ 00132000
*             /* THERE IS A TRACK CURRENTLY IN THE BUFFER. DETERMINE */ 00133000
*             /* IF THE DESIRED TRACK IS THE SAME AS THE ONE         */ 00134000
*             /* CURRENTLY IN THE BUFFER.                            */ 00135000
*             /*                                                     */ 00136000
*             /*******************************************************/ 00137000
*                                                                  0087 00138000
*             IF VDSSEEKA(C1:H2)^=WICSEEKA(C1:H2) THEN             0087 00139000
*                                                                  0087 00140000
         CLC   VDSSEEKA+3(4,RVDSCB),WICSEEKA+3(RWICB)              0087 00141000
         BE    @RF00087                                            0087 00142000
*               /*****************************************************/ 00143000
*               /*                                                   */ 00144000
*               /* DESIRED TRACK IS NOT IN TRACK BUFFER. GO SET UP   */ 00145000
*               /* PARM LIST FOR VBP AND CALL VBP FROM THE SETUP     */ 00146000
*               /* ROUTINE. THE SETUP ROUTINE WILL REQUEST A WRITE ON*/ 00147000
*               /* THE CURRENT TRACK IF THE TRACK HAS BEEN UPDATED.  */ 00148000
*               /*                                                   */ 00149000
*               /*****************************************************/ 00150000
*                                                                  0088 00151000
*               CALL TRMSETU1;      /* DESIRED TRACK AND THE BUFFER     00152000
*                                      CONTENTS ARE THE SAME         */ 00153000
         BAL   @14,TRMSETU1                                        0088 00154000
*             ELSE                                                 0089 00155000
*                                                                  0089 00156000
*               /*****************************************************/ 00157000
*               /*                                                   */ 00158000
*               /* SET THE RELATIVE POSITION POINTER BEFORE RECORD   */ 00159000
*               /* ZERO IF THE CURRENT RECORD ADDRESS POINTS TO R0.  */ 00160000
*               /* OTHERWISE SET THE RELATIVE POSITION POINTER BEFORE*/ 00161000
*               /* THE COUNT OF THE CURRENT RECORD.                  */ 00162000
*               /*                                                   */ 00163000
*               /*****************************************************/ 00164000
*                                                                  0089 00165000
*               IF WICCAM=ADDR(VTR0) THEN                          0089 00166000
         B     @RC00087                                            0089 00167000
@RF00087 LA    @12,VTR0(,RTRKBFR)                                  0089 00168000
         C     @12,WICCAM(,RWICB)                                  0089 00169000
         BNE   @RF00089                                            0089 00170000
*                 WICRELTP=BEFORER0;/* RECORD ZERO                   */ 00171000
         MVC   WICRELTP(2,RWICB),@CH00087                          0090 00172000
*               ELSE                                               0091 00173000
*                                                                  0091 00174000
*                 /***************************************************/ 00175000
*                 /*                                                 */ 00176000
*                 /* CURRENT RECORD NOT R0                           */ 00177000
*                 /*                                                 */ 00178000
*                 /***************************************************/ 00179000
*                                                                  0091 00180000
*                 WICRELTP=BEFORCNT;/* ALL OTHER RECORDS             */ 00181000
         B     @RC00089                                            0091 00182000
@RF00089 MVC   WICRELTP(2,RWICB),@CH00081                          0091 00183000
*           ELSE                    /* THE TRACK BUFFER IS EMPTY.  0092 00184000
*                                      THEREFORE ONLY A READ IS    0092 00185000
*                                      REQUIRED OF VBP.              */ 00186000
*             DO;                                                  0092 00187000
         B     @RC00086                                            0092 00188000
@RF00086 DS    0H                                                  0093 00189000
*               BUFCMW=SWOFF;       /* INDICATE NO WRITE IS TO TAKE     00190000
*                                      PLACE                         */ 00191000
*                                                                  0093 00192000
         NI    BUFCMW(RVBPPL),B'01111111'                          0093 00193000
*               /*****************************************************/ 00194000
*               /*                                                   */ 00195000
*               /* CALL THE SETUP ROUTINE WHICH WILL CALCULTE THE RBA*/ 00196000
*               /* FOR THE READ REQUEST AND WILL CALL VBP TO READ IN */ 00197000
*               /* THE DESIRED TRACK.                                */ 00198000
*               /*                                                   */ 00199000
*               /*****************************************************/ 00200000
*                                                                  0094 00201000
*               CALL TRMSETU2;      /* NO READ TO BE ISSUED          */ 00202000
         BAL   @14,TRMSETU2                                        0094 00203000
*             END;                  /* END OF PROCESSING FOR THE   0095 00204000
*                                      SITUATION WHERE THE TRACK   0095 00205000
*                                      BUFFER IS EMPTY. ENTRY IS NOW    00206000
*                                      MADE TO THE LOOP FOR INVOKING    00207000
*                                      THE CHANNEL PGM INTERPRETER AS   00208000
*                                      THE TRACK BUFFER HAS HAD THE     00209000
*                                      APPROPRIATE TRACK READ IN     */ 00210000
*                                                                  0096 00211000
*       /*************************************************************/ 00212000
*       /*                                                           */ 00213000
*       /* THE FOLLOWING LOOP IS EXECUTED AS LONG AS THE CHANNEL     */ 00214000
*       /* PROGRAM INTERPRETER RETURNS WITH A REQUEST FOR A NEW      */ 00215000
*       /* TRACK. EXIT FROM THE LOOP OCCURS WHEN THE CHANNEL PROGRAM */ 00216000
*       /* HAS BEEN TERMINATED EITHER NORMALLY OR ABNORMALLY OR THE  */ 00217000
*       /* CHANNEL PGM INTERPRETER HAS REQUESTED A PCI EXIT TO BE    */ 00218000
*       /* TAKEN. AT THIS TIME CONTROL IS RETURNED TO THE APPENDAGE  */ 00219000
*       /* INTERFACE HANDLER.                                        */ 00220000
*       /*                                                           */ 00221000
*       /*************************************************************/ 00222000
*                                                                  0096 00223000
*       DO WHILE(RCNTRL=TRACKREQ);                                 0096 00224000
@RC00086 DS    0H                                                  0096 00225000
@RC00081 DS    0H                                                  0096 00226000
@RF00080 B     @DE00096                                            0096 00227000
@DL00096 DS    0H                                                  0097 00228000
*         TRMRETSV=ADDR(TRMCPRET);  /* SAVE THE RETURN ADDRESS TO  0097 00229000
*                                      THIS MODULE IN THE REG SAVE 0097 00230000
*                                      AREA PREFIXING THE VBP PARM 0097 00231000
*                                      LIST. THE CPI WILL LOCATE THE    00232000
*                                      RETURN ADDRESS FROM THIS SAVE    00233000
*                                      AREA AS THE CPI DOES NOT    0097 00234000
*                                      MAINTAIN THE RETURN ADDRESS IN   00235000
*                                      A REG                         */ 00236000
*                                                                  0097 00237000
         LA    @12,TRMCPRET                                        0097 00238000
         ST    @12,TRMRETSV(,RVBPPLSV)                             0097 00239000
*         /***********************************************************/ 00240000
*         /*                                                         */ 00241000
*         /* SAVE THIS MODULES REG 3-11 CONTENTS IN THE FIRST SAVE   */ 00242000
*         /* AREA PREFIXING THE VBP PARM LIST                        */ 00243000
*         /*                                                         */ 00244000
*         /***********************************************************/ 00245000
*                                                                  0098 00246000
*         GEN(STM R3,R11,TRM3TO11(RVBPPLSV))                       0098 00247000
*         REFS(R3,R4,R5,R6,R7,R8,R9,R10,R11,RVBPPLSV) SETS(TRM3TO11);   00248000
         STM R3,R11,TRM3TO11(RVBPPLSV)                                  00249000
*         RFY                                                      0099 00250000
*          (R0,                                                    0099 00251000
*           R1,                                                    0099 00252000
*           R12) RSTD;                                             0099 00253000
